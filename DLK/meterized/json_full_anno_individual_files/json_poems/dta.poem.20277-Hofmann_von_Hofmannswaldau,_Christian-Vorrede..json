{"dta.poem.20277": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Vorrede.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.85", "no:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ist ferner di\u00df so gut/ ein starckes lob erlangen/", "tokens": ["Ist", "fer\u00b7ner", "di\u00df", "so", "gut", "/", "ein", "star\u00b7ckes", "lob", "er\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PDS", "ADV", "ADJD", "$(", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bekandt seyn weit und breit/ mit grossen titel prangen/", "tokens": ["Be\u00b7kandt", "seyn", "weit", "und", "breit", "/", "mit", "gros\u00b7sen", "ti\u00b7tel", "pran\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAINF", "ADJD", "KON", "ADJD", "$(", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der kaum kan auff den brief der edlen ahnen zahl", "tokens": ["Der", "kaum", "kan", "auff", "den", "brief", "der", "ed\u00b7len", "ah\u00b7nen", "zahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "VMFIN", "APPR", "ART", "VVFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zerst\u00fcmmelt und zerhackt um einen gantzen saal", "tokens": ["Zer\u00b7st\u00fcm\u00b7melt", "und", "zer\u00b7hackt", "um", "ei\u00b7nen", "gant\u00b7zen", "saal"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mit wappen und panier in ihrer ordnung weisen?", "tokens": ["Mit", "wap\u00b7pen", "und", "pa\u00b7nier", "in", "ih\u00b7rer", "ord\u00b7nung", "wei\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich ehre deinen stand; Doch soll ich dich auch preisen/", "tokens": ["Ich", "eh\u00b7re", "dei\u00b7nen", "stand", ";", "Doch", "soll", "ich", "dich", "auch", "prei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "VVFIN", "$.", "KON", "VMFIN", "PPER", "PRF", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So lebe ritterlich/ und la\u00df mich unverlacht/", "tokens": ["So", "le\u00b7be", "rit\u00b7ter\u00b7lich", "/", "und", "la\u00df", "mich", "un\u00b7ver\u00b7lacht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "$(", "KON", "VVIMP", "PPER", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ob du gleich edel bist gebohren/ ich gemacht.", "tokens": ["Ob", "du", "gleich", "e\u00b7del", "bist", "ge\u00b7boh\u00b7ren", "/", "ich", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "VVPP", "$(", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wenn schon ein gutes pferd aus Barbarey nicht kommen/", "tokens": ["Wenn", "schon", "ein", "gu\u00b7tes", "pferd", "aus", "Bar\u00b7ba\u00b7rey", "nicht", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "APPR", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wenn seine schlacht schon nicht von Napels ist genommen/", "tokens": ["Wenn", "sei\u00b7ne", "schlacht", "schon", "nicht", "von", "Na\u00b7pels", "ist", "ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "VVFIN", "ADV", "PTKNEG", "APPR", "NE", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+++-+-", "measure": "unknown.measure.septa"}, "line.11": {"text": "Das sonst nur edel ist/ und erstlich trifft das ziel/", "tokens": ["Das", "sonst", "nur", "e\u00b7del", "ist", "/", "und", "erst\u00b7lich", "trifft", "das", "ziel", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "ADJD", "VAFIN", "$(", "KON", "ADJD", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Es habe gleich sein gra\u00df gefressen wo es will/", "tokens": ["Es", "ha\u00b7be", "gleich", "sein", "gra\u00df", "ge\u00b7fres\u00b7sen", "wo", "es", "will", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "PWAV", "PPER", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So kriegt es doch den prei\u00df. Die bilder/ die hier stehen/", "tokens": ["So", "kriegt", "es", "doch", "den", "prei\u00df", ".", "Die", "bil\u00b7der", "/", "die", "hier", "ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$.", "ART", "NN", "$(", "ART", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Von welcher wegen du pflegst oben anzugehen/", "tokens": ["Von", "wel\u00b7cher", "we\u00b7gen", "du", "pflegst", "o\u00b7ben", "an\u00b7zu\u00b7ge\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "APPR", "PPER", "VVFIN", "ADV", "VVIZU", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die ruffen auff dich zu/ und schauen/ was du thust.", "tokens": ["Die", "ruf\u00b7fen", "auff", "dich", "zu", "/", "und", "schau\u00b7en", "/", "was", "du", "thust", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPER", "PTKZU", "$(", "KON", "VVINF", "$(", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Folg ihrer tugend nach/ hastu zum lobe lust.", "tokens": ["Folg", "ih\u00b7rer", "tu\u00b7gend", "nach", "/", "has\u00b7tu", "zum", "lo\u00b7be", "lust", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "$(", "VAFIN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}