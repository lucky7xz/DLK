{"dta.poem.188": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Auf ihre Augen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Kein Demant spielt so sch\u00f6n als deiner Augen-Licht/", "tokens": ["Kein", "De\u00b7mant", "spielt", "so", "sch\u00f6n", "als", "dei\u00b7ner", "Au\u00b7gen\u00b7Licht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "ADJD", "KOKOM", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wann ein beliebter Blick mit holden Flam\u0303en strahlet/", "tokens": ["Wann", "ein", "be\u00b7lieb\u00b7ter", "Blick", "mit", "hol\u00b7den", "Flam\u0303en", "strah\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.3": {"text": "Und wenns der Sonnen gleich mit reinem Feuer prahlet/", "tokens": ["Und", "wenns", "der", "Son\u00b7nen", "gleich", "mit", "rei\u00b7nem", "Feu\u00b7er", "prah\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Sternen Silber gleicht der g\u00fclden Sonnen nicht.", "tokens": ["Der", "Ster\u00b7nen", "Sil\u00b7ber", "gleicht", "der", "g\u00fcl\u00b7den", "Son\u00b7nen", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und vor ", "tokens": ["Und", "vor"], "token_info": ["word", "word"], "pos": ["KON", "APPR"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Jhr Augen aber doch weit besser Anmuth zahlet/", "tokens": ["Ihr", "Au\u00b7gen", "a\u00b7ber", "doch", "weit", "bes\u00b7ser", "An\u00b7muth", "zah\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADV", "ADJD", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Der Himmel hat in euch sein Wesen abgemahlet/", "tokens": ["Der", "Him\u00b7mel", "hat", "in", "euch", "sein", "We\u00b7sen", "ab\u00b7ge\u00b7mah\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Das gleich als wie der Blitz durch Marck und Adern bricht.", "tokens": ["Das", "gleich", "als", "wie", "der", "Blitz", "durch", "Marck", "und", "A\u00b7dern", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "KOUS", "KOKOM", "ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Jhr Augen eure Pracht ist mehr denn wunder-sch\u00f6n.", "tokens": ["Ihr", "Au\u00b7gen", "eu\u00b7re", "Pracht", "ist", "mehr", "denn", "wun\u00b7der\u00b7sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN", "VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Zwey Sonnen sieht man da so noch beysammen stehn.", "tokens": ["Zwey", "Son\u00b7nen", "sieht", "man", "da", "so", "noch", "bey\u00b7sam\u00b7men", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PIS", "ADV", "ADV", "ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Jhr seyd der Liebe-Quell/ der Sch\u00f6nheit bestes Wesen", "tokens": ["Ihr", "seyd", "der", "Lie\u00b7be\u00b7Quell", "/", "der", "Sch\u00f6n\u00b7heit", "bes\u00b7tes", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$(", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der Ort/ wo Freundlichkeit den Wohn-Platz auserlesen/", "tokens": ["Der", "Ort", "/", "wo", "Freund\u00b7lich\u00b7keit", "den", "Wohn\u00b7Platz", "au\u00b7ser\u00b7le\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PWAV", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ein Spiegel holder Lust/ ein Brenn-Glas voller Gluth/", "tokens": ["Ein", "Spie\u00b7gel", "hol\u00b7der", "Lust", "/", "ein", "Brenn\u00b7Glas", "vol\u00b7ler", "Gluth", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "So in geschwinder Eyl die gr\u00f6\u00dfte Wirckung thut.", "tokens": ["So", "in", "ge\u00b7schwin\u00b7der", "Eyl", "die", "gr\u00f6\u00df\u00b7te", "Wir\u00b7ckung", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}