{"dta.poem.6541": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Farben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sobald der Horizont Auroren l\u00e4chlen siehet,", "tokens": ["So\u00b7bald", "der", "Ho\u00b7ri\u00b7zont", "Au\u00b7ro\u00b7ren", "l\u00e4ch\u00b7len", "sie\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NE", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So gl\u00e4ntzt aufs neu und gl\u00fchet", "tokens": ["So", "gl\u00e4ntzt", "aufs", "neu", "und", "gl\u00fc\u00b7het"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der W\u00e4ld-und Felder Schmeltz: so sch\u00f6ne Schildereyen", "tokens": ["Der", "W\u00e4ld\u00b7\u00b7und", "Fel\u00b7der", "Sch\u00b7meltz", ":", "so", "sch\u00f6\u00b7ne", "Schil\u00b7de\u00b7re\u00b7yen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "NN", "$.", "ADV", "ADJA", "NN"], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Erzeigen sich von neuen.", "tokens": ["Er\u00b7zei\u00b7gen", "sich", "von", "neu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So bald hingegen auch die Nacht", "tokens": ["So", "bald", "hin\u00b7ge\u00b7gen", "auch", "die", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den hellen Tag verdrungen;", "tokens": ["Den", "hel\u00b7len", "Tag", "ver\u00b7drun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ist aller Farben bunte Pracht", "tokens": ["Ist", "al\u00b7ler", "Far\u00b7ben", "bun\u00b7te", "Pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dahin; Es haben sie die Schatten eingeschlungen,", "tokens": ["Da\u00b7hin", ";", "Es", "ha\u00b7ben", "sie", "die", "Schat\u00b7ten", "ein\u00b7ge\u00b7schlun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$.", "PPER", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und ihr so lieblicher und sch\u00f6ner Glantz", "tokens": ["Und", "ihr", "so", "lieb\u00b7li\u00b7cher", "und", "sch\u00f6\u00b7ner", "Glantz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "ADJD", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Verliert sich gantz.", "tokens": ["Ver\u00b7liert", "sich", "gantz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}