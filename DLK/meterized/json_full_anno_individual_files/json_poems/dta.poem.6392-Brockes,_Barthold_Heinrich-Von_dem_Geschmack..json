{"dta.poem.6392": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Geschmack.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von C\u00f6rpern, welche schmackbar sind,", "tokens": ["Von", "C\u00f6r\u00b7pern", ",", "wel\u00b7che", "schmack\u00b7bar", "sind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "PRELS", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mu\u00df man sich vorzustellen wissen,", "tokens": ["Mu\u00df", "man", "sich", "vor\u00b7zu\u00b7stel\u00b7len", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PRF", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df sich vom Gantzen abgerissen", "tokens": ["Da\u00df", "sich", "vom", "Gant\u00b7zen", "ab\u00b7ge\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Viel kleine C\u00f6rper sondern mussen,", "tokens": ["Viel", "klei\u00b7ne", "C\u00f6r\u00b7per", "son\u00b7dern", "mus\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die aufs bewegliche Geweb\u2019, (aus den die Zungen", "tokens": ["Die", "aufs", "be\u00b7weg\u00b7li\u00b7che", "Ge\u00b7web'", ",", "(", "aus", "den", "die", "Zun\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["ART", "APPRART", "ADJA", "NN", "$,", "$(", "APPR", "ART", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Verwunderlich zusammen sind geschlungen;)", "tokens": ["Ver\u00b7wun\u00b7der\u00b7lich", "zu\u00b7sam\u00b7men", "sind", "ge\u00b7schlun\u00b7gen", ";)"], "token_info": ["word", "word", "word", "word", "emoticon"], "pos": ["ADJD", "VVPP", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Zu w\u00fcrcken f\u00e4hig sind. Die C\u00f6rper, wie wir sehn,", "tokens": ["Zu", "w\u00fcr\u00b7cken", "f\u00e4\u00b7hig", "sind", ".", "Die", "C\u00f6r\u00b7per", ",", "wie", "wir", "sehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADJD", "VAFIN", "$.", "ART", "NN", "$,", "PWAV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Vergn\u00fcgen unsre Zung\u2019 auf tausend Art,", "tokens": ["Ver\u00b7gn\u00fc\u00b7gen", "uns\u00b7re", "Zung'", "auf", "tau\u00b7send", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Durch Theilchen, die beweglich, rund und zart.", "tokens": ["Durch", "Theil\u00b7chen", ",", "die", "be\u00b7weg\u00b7lich", ",", "rund", "und", "zart", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Durch andere, die lang und spitzig seyn,", "tokens": ["Durch", "an\u00b7de\u00b7re", ",", "die", "lang", "und", "spit\u00b7zig", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "ADJD", "KON", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Druckt ihre Krafft sich ins Gehirn hinein,", "tokens": ["Druckt", "ih\u00b7re", "Krafft", "sich", "ins", "Ge\u00b7hirn", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PRF", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Die kr\u00e4fftig in sie dringt und gehet:", "tokens": ["Die", "kr\u00e4ff\u00b7tig", "in", "sie", "dringt", "und", "ge\u00b7het", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Worinn denn eigentlich des Schmeckens Krafft bestehet.", "tokens": ["Wo\u00b7rinn", "denn", "ei\u00b7gent\u00b7lich", "des", "Schme\u00b7ckens", "Krafft", "be\u00b7ste\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}