{"dta.poem.17762": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "7.  Der verdambte Ubelth\u00e4ter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Ach weh! wo bin ich nu? bey lauter h\u00f6llschen Mohren/", "tokens": ["Ach", "weh", "!", "wo", "bin", "ich", "nu", "?", "bey", "lau\u00b7ter", "h\u00f6ll\u00b7schen", "Moh\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$.", "PWAV", "VAFIN", "PPER", "ADV", "$.", "APPR", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bey teufflischem Gesind: in ", "tokens": ["Bey", "teuf\u00b7fli\u00b7schem", "Ge\u00b7sind", ":", "in"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "ADJA", "NN", "$.", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In einem feurgen Pful/ der ohne Ma\u00df und grund!", "tokens": ["In", "ei\u00b7nem", "feur\u00b7gen", "Pful", "/", "der", "oh\u00b7ne", "Ma\u00df", "und", "grund", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$(", "ART", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ach weh! verfluchter Tag in dem ich bin gebohren!", "tokens": ["Ach", "weh", "!", "ver\u00b7fluch\u00b7ter", "Tag", "in", "dem", "ich", "bin", "ge\u00b7boh\u00b7ren", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$.", "ADJA", "NN", "APPR", "PRELS", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich war zur Seeligkeit ersehen und erkohren;", "tokens": ["Ich", "war", "zur", "See\u00b7lig\u00b7keit", "er\u00b7se\u00b7hen", "und", "er\u00b7koh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "VVINF", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Himmel stund mir frey; ich wuste kurtz und rund", "tokens": ["Der", "Him\u00b7mel", "stund", "mir", "frey", ";", "ich", "wus\u00b7te", "kurtz", "und", "rund"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$.", "PPER", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Was GOttes wille war: und hilt doch nicht den Bund!", "tokens": ["Was", "Got\u00b7tes", "wil\u00b7le", "war", ":", "und", "hilt", "doch", "nicht", "den", "Bund", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "NN", "VAFIN", "$.", "KON", "VVFIN", "ADV", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Nu mu\u00df ewig sein verstossen und verlohren!", "tokens": ["Nu", "mu\u00df", "e\u00b7wig", "sein", "ver\u00b7stos\u00b7sen", "und", "ver\u00b7loh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADJD", "VAINF", "VVINF", "KON", "VVPP", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.9": {"text": "O du verfluchter Leib/ zu was hastu mich bracht!", "tokens": ["O", "du", "ver\u00b7fluch\u00b7ter", "Leib", "/", "zu", "was", "has\u00b7tu", "mich", "bracht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "$(", "APPR", "PWS", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "O du verfluchte Seel was hastu mir gemacht!", "tokens": ["O", "du", "ver\u00b7fluch\u00b7te", "Seel", "was", "has\u00b7tu", "mir", "ge\u00b7macht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "PWS", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ach tausend Ach und Weh! Was hilfft mich nu mein", "tokens": ["Ach", "tau\u00b7send", "Ach", "und", "Weh", "!", "Was", "hilfft", "mich", "nu", "mein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "CARD", "NN", "KON", "NN", "$.", "PWS", "VVFIN", "PPER", "ADV", "PPOSAT"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.12": {"text": "Prangen/", "tokens": ["Pran\u00b7gen", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Mein Geitz und b\u00f6se Lust! Ach h\u00e4tt ich guts gethan!", "tokens": ["Mein", "Geitz", "und", "b\u00f6\u00b7se", "Lust", "!", "Ach", "h\u00e4tt", "ich", "guts", "ge\u00b7than", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "ADJA", "NN", "$.", "NN", "VAFIN", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Nu ist die Reu zu sp\u00e4t/ Gott nimbt sie nicht mehr an:", "tokens": ["Nu", "ist", "die", "Reu", "zu", "sp\u00e4t", "/", "Gott", "nimbt", "sie", "nicht", "mehr", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NE", "PTKA", "ADJD", "$(", "NN", "VVFIN", "PPER", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ich bleib in Ewigkeit mit h\u00f6llscher Qual umbfangen.", "tokens": ["Ich", "bleib", "in", "E\u00b7wig\u00b7keit", "mit", "h\u00f6ll\u00b7scher", "Qual", "umb\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}