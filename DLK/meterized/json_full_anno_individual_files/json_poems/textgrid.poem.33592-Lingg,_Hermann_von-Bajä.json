{"textgrid.poem.33592": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Baj\u00e4", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit Purpursegeln fliegt nach der K\u00fcste zu", "tokens": ["Mit", "Pur\u00b7pur\u00b7se\u00b7geln", "fliegt", "nach", "der", "K\u00fcs\u00b7te", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "APPR", "ART", "NN", "PTKZU"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ein reizend Prachtschiff. Ist es ein Geisterboot", "tokens": ["Ein", "rei\u00b7zend", "Pracht\u00b7schiff", ".", "Ist", "es", "ein", "Geis\u00b7ter\u00b7boot"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "$.", "VAFIN", "PPER", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Aus einer alten Heideninsel,", "tokens": ["Aus", "ei\u00b7ner", "al\u00b7ten", "Hei\u00b7de\u00b7nin\u00b7sel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Eine der goldenen Gondeln Nero's?", "tokens": ["Ei\u00b7ne", "der", "gol\u00b7de\u00b7nen", "Gon\u00b7deln", "Nero's", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "NE", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Nach seiner marmorstrahlenden Villa f\u00e4hrt", "tokens": ["Nach", "sei\u00b7ner", "mar\u00b7mor\u00b7strah\u00b7len\u00b7den", "Vil\u00b7la", "f\u00e4hrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Herrscher Roms und kost der Geliebten Haupt", "tokens": ["Der", "Herr\u00b7scher", "Roms", "und", "kost", "der", "Ge\u00b7lieb\u00b7ten", "Haupt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "KON", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und fl\u00fcstert z\u00e4rtlich: Nimm die Lyra,", "tokens": ["Und", "fl\u00fcs\u00b7tert", "z\u00e4rt\u00b7lich", ":", "Nimm", "die", "Ly\u00b7ra", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$.", "NE", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "R\u00fchre die Saiten, geliebte Cypris!", "tokens": ["R\u00fch\u00b7re", "die", "Sai\u00b7ten", ",", "ge\u00b7lieb\u00b7te", "Cyp\u00b7ris", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADJA", "NE", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Horch, voll die Lyra klang, und es sang das Kind:", "tokens": ["Horch", ",", "voll", "die", "Ly\u00b7ra", "klang", ",", "und", "es", "sang", "das", "Kind", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "ART", "NN", "VVFIN", "$,", "KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Als jene Glutnacht w\u00fctend um Rom sich schlang,", "tokens": ["Als", "je\u00b7ne", "Glut\u00b7nacht", "w\u00fc\u00b7tend", "um", "Rom", "sich", "schlang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "ADJD", "APPR", "NE", "PRF", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da warf das Feuer vor dich nieder", "tokens": ["Da", "warf", "das", "Feu\u00b7er", "vor", "dich", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPER", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Einen verbrennenden Zweig vom Lorbeer.", "tokens": ["Ei\u00b7nen", "ver\u00b7bren\u00b7nen\u00b7den", "Zweig", "vom", "Lor\u00b7beer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Ich sah auf dich, Herr! Ruhig erhobst du dich,", "tokens": ["Ich", "sah", "auf", "dich", ",", "Herr", "!", "Ru\u00b7hig", "er\u00b7hobst", "du", "dich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "NN", "$.", "NE", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+--++--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Schlugst deine weltm\u00fcd-trunkenen Augen auf,", "tokens": ["Schlugst", "dei\u00b7ne", "welt\u00b7m\u00fcd\u00b7trun\u00b7ke\u00b7nen", "Au\u00b7gen", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und l\u00e4chelnd sprachest du die Worte:", "tokens": ["Und", "l\u00e4\u00b7chelnd", "spra\u00b7chest", "du", "die", "Wor\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbilions Flammen verdunkelt ", "tokens": ["\u00bb", "i\u00b7li\u00b7ons", "Flam\u00b7men", "ver\u00b7dun\u00b7kelt"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "APPRART", "NN", "VVPP"], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "So m\u00f6cht' auch ich von liebender Glut verzehrt", "tokens": ["So", "m\u00f6cht'", "auch", "ich", "von", "lie\u00b7ben\u00b7der", "Glut", "ver\u00b7zehrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "PPER", "APPR", "ADJA", "NN", "VVPP"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zu deinen F\u00fc\u00dfen sterben und sterbend noch", "tokens": ["Zu", "dei\u00b7nen", "F\u00fc\u00b7\u00dfen", "ster\u00b7ben", "und", "ster\u00b7bend", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "KON", "ADJD", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dich k\u00fcssen! Siehe, deine Sklavin", "tokens": ["Dich", "k\u00fcs\u00b7sen", "!", "Sie\u00b7he", ",", "dei\u00b7ne", "Skla\u00b7vin"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "VVINF", "$.", "VVIMP", "$,", "PPOSAT", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Bietet dir Persephoneia's \u00c4pfel. \u2013", "tokens": ["Bie\u00b7tet", "dir", "Per\u00b7se\u00b7pho\u00b7nei\u00b7a's", "\u00c4p\u00b7fel", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "NE", "NN", "$.", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Die sch\u00f6ne Nymphe sang es, und Nero sprach:", "tokens": ["Die", "sch\u00f6\u00b7ne", "Nym\u00b7phe", "sang", "es", ",", "und", "Ne\u00b7ro", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$,", "KON", "NE", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta