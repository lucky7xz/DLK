, ",", "von", "Au\u00b7ro\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$,", "APPR", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Angeschienen leis' und fern,", "tokens": ["An\u00b7ge\u00b7schie\u00b7nen", "leis'", "und", "fern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Haucht so aus dem starren Kern", "tokens": ["Haucht", "so", "aus", "dem", "star\u00b7ren", "Kern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ihre Sehnsucht aus in Liedern,", "tokens": ["Ih\u00b7re", "Sehn\u00b7sucht", "aus", "in", "Lie\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und der Mutter Gru\u00df erwiedern", "tokens": ["Und", "der", "Mut\u00b7ter", "Gru\u00df", "er\u00b7wie\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Musik ist die Kunst der Liebe", "tokens": ["Mu\u00b7sik", "ist", "die", "Kunst", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der tiefsten Seel' empfangen,", "tokens": ["In", "der", "tiefs\u00b7ten", "Seel'", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus entflammendem Verlangen", "tokens": ["Aus", "ent\u00b7flam\u00b7men\u00b7dem", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit der Demuth heil'gem Triebe.", "tokens": ["Mit", "der", "De\u00b7muth", "heil'\u00b7gem", "Trie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Da\u00df die Liebe selbst sie liebe,", "tokens": ["Da\u00df", "die", "Lie\u00b7be", "selbst", "sie", "lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zorn und Ha\u00df sich ihr vers\u00f6hnen,", "tokens": ["Zorn", "und", "Ha\u00df", "sich", "ihr", "ver\u00b7s\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PRF", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Mag sie nicht in raschen T\u00f6nen", "tokens": ["Mag", "sie", "nicht", "in", "ra\u00b7schen", "T\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PTKNEG", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Blo\u00df um Lust und Jugend scherzen:", "tokens": ["Blo\u00df", "um", "Lust", "und", "Ju\u00b7gend", "scher\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Sie kann Trauer, Tod und Schmerzen,", "tokens": ["Sie", "kann", "Trau\u00b7er", ",", "Tod", "und", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "wor