{"textgrid.poem.46513": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[manches ist mir doch beschieden]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Manches ist mir doch beschieden,", "tokens": ["Man\u00b7ches", "ist", "mir", "doch", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ich wohl zufrieden", "tokens": ["Da\u00df", "ich", "wohl", "zu\u00b7frie\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "D\u00fcrfte sein, so viele Gaben,", "tokens": ["D\u00fcrf\u00b7te", "sein", ",", "so", "vie\u00b7le", "Ga\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "VAINF", "$,", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die nicht Viele haben,", "tokens": ["Die", "nicht", "Vie\u00b7le", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "NN", "VAFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Unersch\u00f6pflich reiche Fl\u00fcsse", "tokens": ["Un\u00b7er\u00b7sch\u00f6pf\u00b7lich", "rei\u00b7che", "Fl\u00fcs\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eigenster Gen\u00fcsse,", "tokens": ["Ei\u00b7gens\u00b7ter", "Ge\u00b7n\u00fcs\u00b7se", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+---+-", "measure": "dactylic.init"}, "line.7": {"text": "Und nicht minder solche Leiden,", "tokens": ["Und", "nicht", "min\u00b7der", "sol\u00b7che", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die mir w\u00fcrde neiden", "tokens": ["Die", "mir", "w\u00fcr\u00b7de", "nei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "VAFIN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Wer, wie sie herzlieblich brennen,", "tokens": ["Wer", ",", "wie", "sie", "herz\u00b7lieb\u00b7lich", "bren\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "PWAV", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "K\u00f6nnte recht erkennen,", "tokens": ["K\u00f6nn\u00b7te", "recht", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "Und wie sanft, wer's k\u00f6nnte f\u00fchlen,", "tokens": ["Und", "wie", "sanft", ",", "wer's", "k\u00f6nn\u00b7te", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADJD", "$,", "PWS", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Sie sich selber k\u00fchlen;", "tokens": ["Sie", "sich", "sel\u00b7ber", "k\u00fch\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.13": {"text": "Wie der Speer die Wunde heilet,", "tokens": ["Wie", "der", "Speer", "die", "Wun\u00b7de", "hei\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN