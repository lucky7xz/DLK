{"dta.poem.7508": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie Blasen in dem Strom auftauchen und zergehn,", "tokens": ["Wie", "Bla\u00b7sen", "in", "dem", "Strom", "auf\u00b7tau\u00b7chen", "und", "zer\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So sah die Fantasie G\u00f6tter aus Gott entstehn.", "tokens": ["So", "sah", "die", "Fan\u00b7ta\u00b7sie", "G\u00f6t\u00b7ter", "aus", "Gott", "ent\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Kunst, das wirre Spiel der Fantasie zu mildern,", "tokens": ["Die", "Kunst", ",", "das", "wir\u00b7re", "Spiel", "der", "Fan\u00b7ta\u00b7sie", "zu", "mil\u00b7dern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "ART", "NN", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bezaubernd bannte sie den Geist in Marmorbildern.", "tokens": ["Be\u00b7zau\u00b7bernd", "bann\u00b7te", "sie", "den", "Geist", "in", "Mar\u00b7mor\u00b7bil\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Des Sinnbilds Misgestalt will nichts sein, nur bedeuten;", "tokens": ["Des", "Sinn\u00b7bilds", "Mis\u00b7ge\u00b7stalt", "will", "nichts", "sein", ",", "nur", "be\u00b7deu\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "PIS", "VAINF", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Wohlgestalt geb\u00fchrts, Anbetung zu erbeuten.", "tokens": ["Der", "Wohl\u00b7ge\u00b7stalt", "ge\u00b7b\u00fchrts", ",", "An\u00b7be\u00b7tung", "zu", "er\u00b7beu\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Doch soll der Allgeist nicht im engen Haus verk\u00fcmmern,", "tokens": ["Doch", "soll", "der", "All\u00b7geist", "nicht", "im", "en\u00b7gen", "Haus", "ver\u00b7k\u00fcm\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "PTKNEG", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mu\u00df mit dem falschen Schein die Sch\u00f6nheit selbst zertr\u00fcmmern.", "tokens": ["Mu\u00df", "mit", "dem", "fal\u00b7schen", "Schein", "die", "Sch\u00f6n\u00b7heit", "selbst", "zer\u00b7tr\u00fcm\u00b7mern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wenn der vers\u00f6hnte Geist frei mit unschuld'gem Spiel", "tokens": ["Wenn", "der", "ver\u00b7s\u00f6hn\u00b7te", "Geist", "frei", "mit", "un\u00b7schuld'\u00b7gem", "Spiel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADJD", "APPR", "ADJA", "NN"], "meter": "+--+-++-+--+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Beg\u00f6ttert die Natur, dann ist die Kunst am Ziel.", "tokens": ["Be\u00b7g\u00f6t\u00b7tert", "die", "Na\u00b7tur", ",", "dann", "ist", "die", "Kunst", "am", "Ziel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADV", "VAFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}