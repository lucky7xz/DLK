{"dta.poem.876": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "LxIII.  Auff den Sontag de\u00df zum letzten Ge-  \n richts Tage pr\u00e4chtig vnd m\u00e4chtig hereindrin-  \n genden Menschen Sohns/ oder am 26. Sontag nach  \n dem Fest der H. Dreyeinigkeit.  Matth.  25.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Was fol das Krachen seyn? Was sol das Donner blitzen? ", "tokens": ["Was", "fol", "das", "Kra\u00b7chen", "seyn", "?", "Was", "sol", "das", "Don\u00b7ner", "blit\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VAINF", "$.", "PWS", "VMFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Fewr/ Hagel/ Rauch v\u00f1 D\u00e3pf quer durchein \u00e3der gehn/", "tokens": ["Fewr", "/", "Ha\u00b7gel", "/", "Rauch", "v\u00f1", "D\u00e3pf", "quer", "durc\u00b7hein", "\u00e3der", "gehn", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NN", "FM.it", "FM.it", "FM.it", "FM.it", "FM.it", "VVINF", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Himmel f\u00e4llt mir ein/ die Erde wil nicht stehn/", "tokens": ["Der", "Him\u00b7mel", "f\u00e4llt", "mir", "ein", "/", "die", "Er\u00b7de", "wil", "nicht", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "$(", "ART", "NN", "VMFIN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Besondern thut sich auff/ vnd krieget grosse Ritzen.", "tokens": ["Be\u00b7son\u00b7dern", "thut", "sich", "auff", "/", "vnd", "krie\u00b7get", "gros\u00b7se", "Rit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPR", "$(", "KON", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "O! wo soll ich hinau\u00df? wer wil? wer wird mich sch\u00fctzen?", "tokens": ["O", "!", "wo", "soll", "ich", "hin\u00b7au\u00df", "?", "wer", "wil", "?", "wer", "wird", "mich", "sch\u00fct\u00b7zen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "VMFIN", "PPER", "PTKVZ", "$.", "PWS", "VMFIN", "$.", "PWS", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ach! Jesu/ kan ich dich dann nicht anietzo sehn?", "tokens": ["Ach", "!", "Je\u00b7su", "/", "kan", "ich", "dich", "dann", "nicht", "an\u00b7iet\u00b7zo", "sehn", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "NE", "$(", "VMFIN", "PPER", "PRF", "ADV", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Komm/ bitt ich/ sch\u00fctze mich/ damit mich Niemand h\u00f6hn.", "tokens": ["Komm", "/", "bitt", "ich", "/", "sch\u00fct\u00b7ze", "mich", "/", "da\u00b7mit", "mich", "Nie\u00b7mand", "h\u00f6hn", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "VVFIN", "PPER", "$(", "VVFIN", "PPER", "$(", "KOUS", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "O wohl! du kommest an/ Ich sehe dich schon sitzen/", "tokens": ["O", "wohl", "!", "du", "kom\u00b7mest", "an", "/", "Ich", "se\u00b7he", "dich", "schon", "sit\u00b7zen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$.", "PPER", "VVFIN", "PTKVZ", "$(", "PPER", "VVFIN", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Auff deinem Wolcken-Thron. Nun hat es nicht Gefahr/", "tokens": ["Auff", "dei\u00b7nem", "Wol\u00b7cken\u00b7Thron", ".", "Nun", "hat", "es", "nicht", "Ge\u00b7fahr", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "ADV", "VAFIN", "PPER", "PTKNEG", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil du bald brichst herein mit deiner Engel Schaar.", "tokens": ["Weil", "du", "bald", "brichst", "her\u00b7ein", "mit", "dei\u00b7ner", "En\u00b7gel", "Schaar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "PTKVZ", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gieb nur ein reines Hertz/ damit Ich nicht zur Lincken/", "tokens": ["Gieb", "nur", "ein", "rei\u00b7nes", "Hertz", "/", "da\u00b7mit", "Ich", "nicht", "zur", "Lin\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "ADJA", "NN", "$(", "KOUS", "PPER", "PTKNEG", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der b\u00f6sen-B\u00f6cke-Rott m\u00f6g werden zugesellt/", "tokens": ["Der", "b\u00f6\u00b7sen\u00b7B\u00f6\u00b7cke\u00b7Rott", "m\u00f6g", "wer\u00b7den", "zu\u00b7ge\u00b7sellt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hilff/ gn\u00e4dig/ da\u00df ich dir zur Rechten werd gestellt.", "tokens": ["Hilff", "/", "gn\u00e4\u00b7dig", "/", "da\u00df", "ich", "dir", "zur", "Rech\u00b7ten", "werd", "ge\u00b7stellt", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ADJD", "$(", "KOUS", "PPER", "PPER", "APPRART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Komm! nim mich Himmel-ein. Gib Engel-wein zutrincken.", "tokens": ["Komm", "!", "nim", "mich", "Him\u00b7mel\u00b7ein", ".", "Gib", "En\u00b7gel\u00b7wein", "zu\u00b7trin\u00b7cken", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "VVIMP", "PPER", "NN", "$.", "VVIMP", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}