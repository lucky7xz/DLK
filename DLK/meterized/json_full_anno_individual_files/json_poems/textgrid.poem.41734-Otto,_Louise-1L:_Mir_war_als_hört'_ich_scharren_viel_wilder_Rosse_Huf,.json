{"textgrid.poem.41734": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mir war als h\u00f6rt' ich scharren viel wilder Rosse Huf,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mir war als h\u00f6rt' ich scharren viel wilder Rosse Huf,", "tokens": ["Mir", "war", "als", "h\u00f6rt'", "ich", "schar\u00b7ren", "viel", "wil\u00b7der", "Ros\u00b7se", "Huf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "VVFIN", "PPER", "VVFIN", "PIAT", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Es rasselte Schild und Barren, es t\u00f6nte H\u00f6rnerruf,", "tokens": ["Es", "ras\u00b7sel\u00b7te", "Schild", "und", "Bar\u00b7ren", ",", "es", "t\u00f6n\u00b7te", "H\u00f6r\u00b7ner\u00b7ruf", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,", "PPER", "VVFIN", "NN", "$,"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und hei! nach alter Weise kam ich herbei geschwind,", "tokens": ["Und", "hei", "!", "nach", "al\u00b7ter", "Wei\u00b7se", "kam", "ich", "her\u00b7bei", "ge\u00b7schwind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "$.", "APPR", "ADJA", "NN", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Es gilt ein Lied zum Preise der Siegelinde Kind.", "tokens": ["Es", "gilt", "ein", "Lied", "zum", "Prei\u00b7se", "der", "Sie\u00b7ge\u00b7lin\u00b7de", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Die reichen K\u00f6nigshelden, Schilbung und Nibelung,", "tokens": ["Die", "rei\u00b7chen", "K\u00f6\u00b7nigs\u00b7hel\u00b7den", ",", "Schil\u00b7bung", "und", "Ni\u00b7be\u00b7lung", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "So thut die M\u00e4re melden, schlug Siegfried k\u00fchn und jung,", "tokens": ["So", "thut", "die", "M\u00e4\u00b7re", "mel\u00b7den", ",", "schlug", "Sieg\u00b7fried", "k\u00fchn", "und", "jung", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVINF", "$,", "VVFIN", "NE", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Den Hort der Nibelungen erwarb sich seine Hand,", "tokens": ["Den", "Hort", "der", "Ni\u00b7be\u00b7lun\u00b7gen", "er\u00b7warb", "sich", "sei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PRF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Schwert Balmung hat errungen der Held aus Niederland.", "tokens": ["Schwert", "Bal\u00b7mung", "hat", "er\u00b7run\u00b7gen", "der", "Held", "aus", "Nie\u00b7der\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "VVPP", "ART", "NN", "APPR", "NE", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Aus einem hohlen Berge holt er hervor den Hort,", "tokens": [