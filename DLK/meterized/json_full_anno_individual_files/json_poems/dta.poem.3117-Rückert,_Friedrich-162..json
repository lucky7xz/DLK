{"dta.poem.3117": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "162.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn alles Menschenthuns ist Wurzel Eigennutz,", "tokens": ["Wenn", "al\u00b7les", "Men\u00b7schen\u00b7thuns", "ist", "Wur\u00b7zel", "Ei\u00b7gen\u00b7nutz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "NN", "VAFIN", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Komm, la\u00df uns reinigen die Wurzel von dem Schmutz!", "tokens": ["Komm", ",", "la\u00df", "uns", "rei\u00b7ni\u00b7gen", "die", "Wur\u00b7zel", "von", "dem", "Schmutz", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVIMP", "PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Auf diesem Grunde la\u00df uns stehn nur und erkl\u00e4ren,", "tokens": ["Auf", "die\u00b7sem", "Grun\u00b7de", "la\u00df", "uns", "stehn", "nur", "und", "er\u00b7kl\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVIMP", "PPER", "VVFIN", "ADV", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie jene Wurzel selbst das H\u00f6chste mu\u00df geb\u00e4ren.", "tokens": ["Wie", "je\u00b7ne", "Wur\u00b7zel", "selbst", "das", "H\u00f6chs\u00b7te", "mu\u00df", "ge\u00b7b\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "ADV", "ART", "ADJA", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ein jedes Wesen eingepflanzt hat von Natur", "tokens": ["Ein", "je\u00b7des", "We\u00b7sen", "ein\u00b7ge\u00b7pflanzt", "hat", "von", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VVPP", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Grundtrieb: wie es ist, sich zu erhalten nur.", "tokens": ["Den", "Grund\u00b7trieb", ":", "wie", "es", "ist", ",", "sich", "zu", "er\u00b7hal\u00b7ten", "nur", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PWAV", "PPER", "VAFIN", "$,", "PRF", "PTKZU", "VVINF", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}