{"dta.poem.861": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XlIIX.  Auff den Sontag de\u00df gerechtmachen-  \n den Heylandes/ oder  XI.  Sontag nach dem  \n Fest der H. Dreyeinigkeit. Luc. 18.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bins! Gott ach ich bins! d&#7869; keine Schuld noch Schande ", "tokens": ["Ich", "bins", "!", "Gott", "ach", "ich", "bins", "!", "d", "&#7869;", "kei\u00b7ne", "Schuld", "noch", "Schan\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "XML_entity", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$.", "NN", "XY", "PPER", "VAFIN", "$.", "XY", "$(", "PIAT", "NN", "ADV", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat je zu grob gedacht: der rasend eh\u2019 vnd je", "tokens": ["Hat", "je", "zu", "grob", "ge\u00b7dacht", ":", "der", "ra\u00b7send", "eh'", "vnd", "je"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PTKA", "ADJD", "VVPP", "$.", "ART", "ADJD", "VVFIN", "KON", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In Lastern sich gew\u00e4ltzt: al\u00df ein vsinnnig Vieh\u2019", "tokens": ["In", "Las\u00b7tern", "sich", "ge\u00b7w\u00e4ltzt", ":", "al\u00df", "ein", "vsinn\u00b7nig", "Vieh'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PRF", "VVPP", "$.", "KOUS", "ART", "ADJD", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "HeRR! meiner S\u00fcnd ist mehr als Sand\u2019 ans Meeres", "tokens": ["HeRR", "!", "mei\u00b7ner", "S\u00fcnd", "ist", "mehr", "als", "Sand'", "ans", "Mee\u00b7res"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "PPOSAT", "NN", "VAFIN", "ADV", "KOUS", "NN", "APPRART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Strande. ", "tokens": ["Stran\u00b7de", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.2": {"line.1": {"text": "Mich f\u00fchrt der Teufel schon in Demand festem Bande:", "tokens": ["Mich", "f\u00fchrt", "der", "Teu\u00b7fel", "schon", "in", "De\u00b7mand", "fes\u00b7tem", "Ban\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein Vater: schaw doch schaw\u2019/ vnd denck auff dessen", "tokens": ["Mein", "Va\u00b7ter", ":", "schaw", "doch", "scha\u00b7w'", "/", "vnd", "denck", "auff", "des\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "ADV", "VVFIN", "$(", "KON", "VVIMP", "APPR", "PDS"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "m\u00fch. ", "tokens": ["m\u00fch", "."], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Auff dessen milde gunst/ der sterbend mir verzih\u2019", "tokens": ["Auff", "des\u00b7sen", "mil\u00b7de", "gunst", "/", "der", "ster\u00b7bend", "mir", "ver\u00b7zih'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "ADJA", "NN", "$(", "ART", "ADJD", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nimb seine Todes angst vnd tewers Blutt zu pfande.", "tokens": ["Nimb", "sei\u00b7ne", "To\u00b7des", "angst", "vnd", "te\u00b7wers", "Blutt", "zu", "pfan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "KON", "NN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Zum Pfand vnd L\u00f6segeldt! Ich darff mein Augen nicht", "tokens": ["Zum", "Pfand", "vnd", "L\u00f6\u00b7se\u00b7geldt", "!", "Ich", "darff", "mein", "Au\u00b7gen", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "NN", "$.", "PPER", "VMFIN", "PPOSAT", "NN", "PTKNEG"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erheben Himmel an\u2019 ach neige dein Gesicht", "tokens": ["Er\u00b7he\u00b7ben", "Him\u00b7mel", "an'", "ach", "nei\u00b7ge", "dein", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "VVFIN", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Herunter \u00fcber mich/ je mehr du wirst verzeihen.", "tokens": ["Her\u00b7un\u00b7ter", "\u00fc\u00b7ber", "mich", "/", "je", "mehr", "du", "wirst", "ver\u00b7zei\u00b7hen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$(", "ADV", "ADV", "PPER", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Je gr\u00f6sser wird dein Lob/ ist meiner Laster viel:", "tokens": ["Je", "gr\u00f6s\u00b7ser", "wird", "dein", "Lob", "/", "ist", "mei\u00b7ner", "Las\u00b7ter", "viel", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$(", "VAFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So hat doch deine Lieb vnd grosse gunst kein ziel", "tokens": ["So", "hat", "doch", "dei\u00b7ne", "Lieb", "vnd", "gros\u00b7se", "gunst", "kein", "ziel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "NN", "KON", "ADJA", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die \u00f6ffter al\u00df ein Mensch wird fehlen/ kan befreyen.", "tokens": ["Die", "\u00f6ff\u00b7ter", "al\u00df", "ein", "Mensch", "wird", "feh\u00b7len", "/", "kan", "be\u00b7fre\u00b7yen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "KOKOM", "ART", "NN", "VAFIN", "VVINF", "$(", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}