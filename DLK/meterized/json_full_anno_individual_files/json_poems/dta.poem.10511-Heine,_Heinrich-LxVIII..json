{"dta.poem.10511": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "LxVIII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Von schonen Lippen fortgedr\u00e4ngt, getrieben", "tokens": ["Von", "scho\u00b7nen", "Lip\u00b7pen", "fort\u00b7ge\u00b7dr\u00e4ngt", ",", "ge\u00b7trie\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus sch\u00f6nen Armen, die uns fest umschlossen!", "tokens": ["Aus", "sch\u00f6\u00b7nen", "Ar\u00b7men", ",", "die", "uns", "fest", "um\u00b7schlos\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich w\u00e4re gern noch einen Tag geblieben.", "tokens": ["Ich", "w\u00e4\u00b7re", "gern", "noch", "ei\u00b7nen", "Tag", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch kam der Schwager schon mit seinen Rossen.", "tokens": ["Doch", "kam", "der", "Schwa\u00b7ger", "schon", "mit", "sei\u00b7nen", "Ros\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das ist das Leben, Kind, ein ewig Jammern,", "tokens": ["Das", "ist", "das", "Le\u00b7ben", ",", "Kind", ",", "ein", "e\u00b7wig", "Jam\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "NN", "$,", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein ewig Abschiednehmen, ew'ges Trennen!", "tokens": ["Ein", "e\u00b7wig", "Ab\u00b7schied\u00b7neh\u00b7men", ",", "ew'\u00b7ges", "Tren\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Konnt' denn dein Herz das mein'ge nicht umklammern?", "tokens": ["Konnt'", "denn", "dein", "Herz", "das", "mein'\u00b7ge", "nicht", "um\u00b7klam\u00b7mern", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "ART", "PPOSS", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hat selbst dein Auge mich nicht halten k\u00f6nnen?", "tokens": ["Hat", "selbst", "dein", "Au\u00b7ge", "mich", "nicht", "hal\u00b7ten", "k\u00f6n\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "PPER", "PTKNEG", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}