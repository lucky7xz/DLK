{"dta.poem.1323": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Himmel-Schl\u00fcssel  \n oder  \n Geistliche Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "O JEsu kleines Kind/ doch ewig grosser Held/", "tokens": ["O", "Je\u00b7su", "klei\u00b7nes", "Kind", "/", "doch", "e\u00b7wig", "gros\u00b7ser", "Held", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "$(", "ADV", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein F\u00fcrst in deinem Reich/ und Gast auff deiner Welt/", "tokens": ["Ein", "F\u00fcrst", "in", "dei\u00b7nem", "Reich", "/", "und", "Gast", "auff", "dei\u00b7ner", "Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$(", "KON", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mensch/ Bruder/ GOtt mein Freund/ du Trost und Licht der", "tokens": ["Mensch", "/", "Bru\u00b7der", "/", "Gott", "mein", "Freund", "/", "du", "Trost", "und", "Licht", "der"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$(", "NN", "$(", "NN", "PPOSAT", "NN", "$(", "PPER", "NN", "KON", "NN", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Izt k\u00f6mmt der werthe Tag/ der dich zu uns gebracht/", "tokens": ["Izt", "k\u00f6mmt", "der", "wert\u00b7he", "Tag", "/", "der", "dich", "zu", "uns", "ge\u00b7bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$(", "PRELS", "PRF", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Christen frohe Schaar begehet ihn mit Freuden/", "tokens": ["Der", "Chris\u00b7ten", "fro\u00b7he", "Schaar", "be\u00b7ge\u00b7het", "ihn", "mit", "Freu\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "PPER", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und bringt mit Andacht zu die Wunder-volle Nacht.", "tokens": ["Und", "bringt", "mit", "An\u00b7dacht", "zu", "die", "Wun\u00b7der\u00b7vol\u00b7le", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Man h\u00f6ret wie dein Lob in allen Kirchen klingt/", "tokens": ["Man", "h\u00f6\u00b7ret", "wie", "dein", "Lob", "in", "al\u00b7len", "Kir\u00b7chen", "klingt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KOKOM", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von dem ein Engel selbst den frommen Hirten singt/", "tokens": ["Von", "dem", "ein", "En\u00b7gel", "selbst", "den", "from\u00b7men", "Hir\u00b7ten", "singt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "ADV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jedwedes gl\u00e4ubigs Hertz will seinen Eyfer zeigen/", "tokens": ["Jed\u00b7we\u00b7des", "gl\u00e4u\u00b7bigs", "Hertz", "will", "sei\u00b7nen", "Ey\u00b7fer", "zei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "W\u00fcnscht/ betet/ lobt und danckt so viel es immer kau/", "tokens": ["W\u00fcnscht", "/", "be\u00b7tet", "/", "lobt", "und", "danckt", "so", "viel", "es", "im\u00b7mer", "kau", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "VVFIN", "$(", "VVFIN", "KON", "VVFIN", "ADV", "ADV", "PPER", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie solt ich denn allein mit stummen Munde schweigen/", "tokens": ["Wie", "solt", "ich", "denn", "al\u00b7lein", "mit", "stum\u00b7men", "Mun\u00b7de", "schwei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nimm gn\u00e4dig auch von mir die Hand voll Weyrauch an.", "tokens": ["Nimm", "gn\u00e4\u00b7dig", "auch", "von", "mir", "die", "Hand", "voll", "Wey\u00b7rauch", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ADV", "APPR", "PPER", "ART", "NN", "ADJD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wie billich sing ich dich/ du s\u00fcsses Wunder-Kind/", "tokens": ["Wie", "bil\u00b7lich", "sing", "ich", "dich", "/", "du", "s\u00fcs\u00b7ses", "Wun\u00b7der\u00b7Kind", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "PRF", "$(", "PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch dessen kr\u00e4fftig Wort der Welt-Krei\u00df ward gegr\u00fcndt/", "tokens": ["Durch", "des\u00b7sen", "kr\u00e4ff\u00b7tig", "Wort", "der", "Welt\u00b7Krei\u00df", "ward", "ge\u00b7gr\u00fcndt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "ADJD", "NN", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-++--+", "measure": "iambic.hexa.chol"}, "line.3": {"text": "Die Finsterni\u00df erleucht/ der Himmel ausgebreitet/", "tokens": ["Die", "Fins\u00b7ter\u00b7ni\u00df", "er\u00b7leucht", "/", "der", "Him\u00b7mel", "aus\u00b7ge\u00b7brei\u00b7tet", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$(", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das Wasser abgetheilt/ die Sternen angebrennt/", "tokens": ["Das", "Was\u00b7ser", "ab\u00b7ge\u00b7theilt", "/", "die", "Ster\u00b7nen", "an\u00b7ge\u00b7brennt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$(", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Erde mir zum Schlo\u00df und Garten zubereitet/", "tokens": ["Die", "Er\u00b7de", "mir", "zum", "Schlo\u00df", "und", "Gar\u00b7ten", "zu\u00b7be\u00b7rei\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPRART", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mit Thieren ieder Art besezt ihr Element.", "tokens": ["Mit", "Thie\u00b7ren", "ie\u00b7der", "Art", "be\u00b7sezt", "ihr", "E\u00b7le\u00b7ment", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Du bauest nicht vor dich/ wie gro\u00df die Welt mag seyn/", "tokens": ["Du", "bau\u00b7est", "nicht", "vor", "dich", "/", "wie", "gro\u00df", "die", "Welt", "mag", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "PPER", "$(", "PWAV", "ADJD", "ART", "NN", "VMFIN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So w\u00e4re sie dir doch zur Wohnung allzu klein/", "tokens": ["So", "w\u00e4\u00b7re", "sie", "dir", "doch", "zur", "Woh\u00b7nung", "all\u00b7zu", "klein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADV", "APPRART", "NN", "PTKA", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Viel h\u00f6her steigen noch die Wercke deiner Liebe;", "tokens": ["Viel", "h\u00f6\u00b7her", "stei\u00b7gen", "noch", "die", "Wer\u00b7cke", "dei\u00b7ner", "Lie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du schaffest/ da\u00df sie solln der Erde Meister seyn/", "tokens": ["Du", "schaf\u00b7fest", "/", "da\u00df", "sie", "solln", "der", "Er\u00b7de", "Meis\u00b7ter", "seyn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPER", "VMFIN", "ART", "NN", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Den Mann vom Erden-Klo\u00df/ das Weib von seiner Riebe/", "tokens": ["Den", "Mann", "vom", "Er\u00b7den\u00b7Klo\u00df", "/", "das", "Weib", "von", "sei\u00b7ner", "Rie\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$(", "ART", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Du pr\u00e4gest in den Thon dein edles Bildni\u00df ein.", "tokens": ["Du", "pr\u00e4\u00b7gest", "in", "den", "Thon", "dein", "ed\u00b7les", "Bild\u00b7ni\u00df", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ach leider! dieser Thon nimmt fremde Zeichen an/", "tokens": ["Ach", "lei\u00b7der", "!", "die\u00b7ser", "Thon", "nimmt", "frem\u00b7de", "Zei\u00b7chen", "an", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$.", "PDAT", "NN", "VVFIN", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Schlangen List verf\u00fchrt das Weib/ und sie den Mann/", "tokens": ["Der", "Schlan\u00b7gen", "List", "ver\u00b7f\u00fchrt", "das", "Weib", "/", "und", "sie", "den", "Mann", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "ART", "NN", "$(", "KON", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der rein-geschaffne Mensch wird zum befleckten S\u00fcnder/", "tokens": ["Der", "rein\u00b7ge\u00b7schaff\u00b7ne", "Mensch", "wird", "zum", "be\u00b7fleck\u00b7ten", "S\u00fcn\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durch einen Apffel-Bi\u00df verleurt er GOttes Reich:", "tokens": ["Durch", "ei\u00b7nen", "Apf\u00b7fel\u00b7Bi\u00df", "ver\u00b7leurt", "er", "Got\u00b7tes", "Reich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der b\u00f6sen Eltern Schuld erbt auff die b\u00f6sen Kinder/", "tokens": ["Der", "b\u00f6\u00b7sen", "El\u00b7tern", "Schuld", "erbt", "auff", "die", "b\u00f6\u00b7sen", "Kin\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und gleiche Missethat macht ihre Straffe gleich.", "tokens": ["Und", "glei\u00b7che", "Mis\u00b7se\u00b7that", "macht", "ih\u00b7re", "Straf\u00b7fe", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "O s\u00fcsses Wunder-Kind/ wie kr\u00e4fftig liebest du?", "tokens": ["O", "s\u00fcs\u00b7ses", "Wun\u00b7der\u00b7Kind", "/", "wie", "kr\u00e4ff\u00b7tig", "lie\u00b7best", "du", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$(", "PWAV", "ADJD", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du schleussest dennoch nicht dein Vater-Hertze zu.", "tokens": ["Du", "schleus\u00b7sest", "den\u00b7noch", "nicht", "dein", "Va\u00b7ter\u00b7Hert\u00b7ze", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was kein gefallner Geist in Ewigkeit kan hoffen/", "tokens": ["Was", "kein", "ge\u00b7fall\u00b7ner", "Geist", "in", "E\u00b7wig\u00b7keit", "kan", "hof\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "ADJA", "NN", "APPR", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ist uns durch dich erlangt/ ein ausges\u00f6hnter GOtt;", "tokens": ["Ist", "uns", "durch", "dich", "er\u00b7langt", "/", "ein", "aus\u00b7ge\u00b7s\u00f6hn\u00b7ter", "Gott", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "VVPP", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Himmel stehet uns durch deine Wohlthat offen/", "tokens": ["Der", "Him\u00b7mel", "ste\u00b7het", "uns", "durch", "dei\u00b7ne", "Wohlt\u00b7hat", "of\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Segen vor den Fluch/ das Leben vor den Tod.", "tokens": ["Der", "Se\u00b7gen", "vor", "den", "Fluch", "/", "das", "Le\u00b7ben", "vor", "den", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$(", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Du/ deinem Vater gleich/ an Alter und Gewalt/", "tokens": ["Du", "/", "dei\u00b7nem", "Va\u00b7ter", "gleich", "/", "an", "Al\u00b7ter", "und", "Ge\u00b7walt", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "PPOSAT", "NN", "ADV", "$(", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verleugnest uns zu gutt die g\u00f6ttliche Gestalt/", "tokens": ["Ver\u00b7leug\u00b7nest", "uns", "zu", "gutt", "die", "g\u00f6tt\u00b7li\u00b7che", "Ge\u00b7stalt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKA", "ADJD", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und l\u00e4st dich Pflege-Sohn des armen Josephs nennen:", "tokens": ["Und", "l\u00e4st", "dich", "Pfle\u00b7ge\u00b7Sohn", "des", "ar\u00b7men", "Jo\u00b7se\u00b7phs", "nen\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "ART", "ADJA", "NE", "VVINF", "$."], "meter": "-+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Du grosses F\u00fcrsten-Kind wirst an die Brust gelegt/", "tokens": ["Du", "gros\u00b7ses", "F\u00fcrs\u00b7ten\u00b7Kind", "wirst", "an", "die", "Brust", "ge\u00b7legt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5":