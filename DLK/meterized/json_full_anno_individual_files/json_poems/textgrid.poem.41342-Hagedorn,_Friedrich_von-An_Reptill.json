{"textgrid.poem.41342": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An Reptill", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rebuff verfolget mich; ihn darfst du nicht erbittern:", "tokens": ["Re\u00b7buff", "ver\u00b7fol\u00b7get", "mich", ";", "ihn", "darfst", "du", "nicht", "er\u00b7bit\u00b7tern", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Arbas; doch auch er ist dir ein Matador:", "tokens": ["Und", "Ar\u00b7bas", ";", "doch", "auch", "er", "ist", "dir", "ein", "Ma\u00b7ta\u00b7dor", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$.", "ADV", "ADV", "PPER", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Selbst Struma; \u00bbStruma selbst?\u00ab Du widersprichst nicht Rittern,", "tokens": ["Selbst", "Stru\u00b7ma", ";", "\u00bb", "Stru\u00b7ma", "selbst", "?", "\u00ab", "Du", "wi\u00b7der\u00b7sprichst", "nicht", "Rit\u00b7tern", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "$.", "$(", "NE", "ADV", "$.", "$(", "PPER", "VVFIN", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und wie schwingt Struma sich aus Staub und Nacht empor!", "tokens": ["Und", "wie", "schwingt", "Stru\u00b7ma", "sich", "aus", "Staub", "und", "Nacht", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "NE", "PRF", "APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Urgande will sich mehr, als alle die, erk\u00fchnen:", "tokens": ["Ur\u00b7gan\u00b7de", "will", "sich", "mehr", ",", "als", "al\u00b7le", "die", ",", "er\u00b7k\u00fch\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VMFIN", "PRF", "ADV", "$,", "KOUS", "PIS", "ART", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Du bist ein Wittwenfreund, und sie ist reich, Reptill.", "tokens": ["Du", "bist", "ein", "Witt\u00b7wen\u00b7freund", ",", "und", "sie", "ist", "reich", ",", "Rep\u00b7till", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "KON", "PPER", "VAFIN", "ADJD", "$,", "NE", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Mein G\u00f6nner, lebe wohl! Nicht Sklaven mag ich dienen:", "tokens": ["Mein", "G\u00f6n\u00b7ner", ",", "le\u00b7be", "wohl", "!", "Nicht", "Skla\u00b7ven", "mag", "ich", "die\u00b7nen", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "ADV", "$.", "PTKNEG", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Frei mu\u00df der Stolze sein, der mir gebieten will.", "tokens": ["Frei", "mu\u00df", "der", "Stol\u00b7ze", "sein", ",", "der", "mir", "ge\u00b7bie\u00b7ten", "will", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "ADJA", "VAINF", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$."],