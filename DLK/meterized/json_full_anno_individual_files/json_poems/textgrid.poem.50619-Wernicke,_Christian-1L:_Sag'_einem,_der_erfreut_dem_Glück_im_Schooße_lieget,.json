{"textgrid.poem.50619": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sag' einem, der erfreut dem Gl\u00fcck im Schoo\u00dfe lieget,", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sag' einem, der erfreut dem Gl\u00fcck im Schoo\u00dfe lieget,", "tokens": ["Sag'", "ei\u00b7nem", ",", "der", "er\u00b7freut", "dem", "Gl\u00fcck", "im", "Schoo\u00b7\u00dfe", "lie\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "$,", "PRELS", "VVFIN", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df dessen Stille stets die Sicherheit betr\u00fcget,", "tokens": ["Da\u00df", "des\u00b7sen", "Stil\u00b7le", "stets", "die", "Si\u00b7cher\u00b7heit", "be\u00b7tr\u00fc\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRELAT", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df es uns, ehe wir es recht erkannt, verl\u00e4\u00dft;", "tokens": ["Da\u00df", "es", "uns", ",", "e\u00b7he", "wir", "es", "recht", "er\u00b7kannt", ",", "ver\u00b7l\u00e4\u00dft", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "$,", "KOUS", "PPER", "PPER", "ADJD", "VVPP", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er h\u00f6ret dich nicht mehr, denn junge Hochzeitg\u00e4st'", "tokens": ["Er", "h\u00f6\u00b7ret", "dich", "nicht", "mehr", ",", "denn", "jun\u00b7ge", "Hoch\u00b7zeit\u00b7g\u00e4st'"], "token_info": ["word", "word", "word", "word