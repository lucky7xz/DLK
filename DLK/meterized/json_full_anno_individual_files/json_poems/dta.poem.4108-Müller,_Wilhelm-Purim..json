{"dta.poem.4108": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Purim.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "An diesem freudenreichen Tag,", "tokens": ["An", "die\u00b7sem", "freu\u00b7den\u00b7rei\u00b7chen", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo Alles gern in Sammt und Seide,", "tokens": ["Wo", "Al\u00b7les", "gern", "in", "Sammt", "und", "Sei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "APPR", "NN", "KON", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In Gold und Steinen prangen mag?", "tokens": ["In", "Gold", "und", "Stei\u00b7nen", "pran\u00b7gen", "mag", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es schwimmt das festlich bunte Zimmer", "tokens": ["Es", "schwimmt", "das", "fest\u00b7lich", "bun\u00b7te", "Zim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In hoher Kerzen Duft und Schein:", "tokens": ["In", "ho\u00b7her", "Ker\u00b7zen", "Duft", "und", "Schein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie schleicht sich aus der Freude Schimmer,", "tokens": ["Sie", "schleicht", "sich", "aus", "der", "Freu\u00b7de", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und steht am Fenster ganz allein.", "tokens": ["Und", "steht", "am", "Fens\u00b7ter", "ganz", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da legt sich, wie ein wei\u00dfer Schleier,", "tokens": ["Da", "legt", "sich", ",", "wie", "ein", "wei\u00b7\u00dfer", "Schlei\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Mondes Strahl um ihr Gesicht,", "tokens": ["Des", "Mon\u00b7des", "Strahl", "um", "ihr", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und eine stille, tiefe Feier", "tokens": ["Und", "ei\u00b7ne", "stil\u00b7le", ",", "tie\u00b7fe", "Fei\u00b7er"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus ihren sel'gen Augen spricht.", "tokens": ["Aus", "ih\u00b7ren", "sel'\u00b7gen", "Au\u00b7gen", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O w\u00e4r' ich aus den Truggestalten", "tokens": ["O", "w\u00e4r'", "ich", "aus", "den", "Trug\u00b7ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der wilden, blinden Maskenlust,", "tokens": ["Der", "wil\u00b7den", ",", "blin\u00b7den", "Mas\u00b7ken\u00b7lust", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und d\u00fcrfte meine H\u00e4nde falten", "tokens": ["Und", "d\u00fcrf\u00b7te", "mei\u00b7ne", "H\u00e4n\u00b7de", "fal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Entlarvt im Tempel ihrer Brust!", "tokens": ["Ent\u00b7larvt", "im", "Tem\u00b7pel", "ih\u00b7rer", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}