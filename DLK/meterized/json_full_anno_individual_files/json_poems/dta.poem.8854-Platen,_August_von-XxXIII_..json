{"dta.poem.8854": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XxXIII .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Und ein stiller Gram vergiftet meine Tage lange schon:", "tokens": ["Und", "ein", "stil\u00b7ler", "Gram", "ver\u00b7gif\u00b7tet", "mei\u00b7ne", "Ta\u00b7ge", "lan\u00b7ge", "schon", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "ADV", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Seufzer floh'n und Thr\u00e4nen flossen, was noch heischt die", "tokens": ["Seuf\u00b7zer", "floh'n", "und", "Thr\u00e4\u00b7nen", "flos\u00b7sen", ",", "was", "noch", "heischt", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "KON", "NN", "VVFIN", "$,", "PRELS", "ADV", "VVFIN", "ART"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Zeugni\u00df gab von meinem Leben meine Klage lange schon.", "tokens": ["Zeug\u00b7ni\u00df", "gab", "von", "mei\u00b7nem", "Le\u00b7ben", "mei\u00b7ne", "Kla\u00b7ge", "lan\u00b7ge", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "PPOSAT", "NN", "ADV", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Nicht das kleinste Liebeszeichen gabst du mir, ich lausch'", "tokens": ["Nicht", "das", "kleins\u00b7te", "Lie\u00b7bes\u00b7zei\u00b7chen", "gabst", "du", "mir", ",", "ich", "lausch'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "VVFIN", "PPER", "PPER", "$,", "PPER", "VVFIN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.5": {"text": "Lese dir umsonst im Auge, forsch' und frage lange schon!", "tokens": ["Le\u00b7se", "dir", "um\u00b7sonst", "im", "Au\u00b7ge", ",", "for\u00b7sch'", "und", "fra\u00b7ge", "lan\u00b7ge", "schon", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPRART", "NN", "$,", "PTKVZ", "KON", "VVFIN", "ADV", "ADV", "$."], "meter": "+-+-+-+--+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.6": {"text": "Aber nein! Ein leises Etwas, nenn' ich Wink es oder", "tokens": ["A\u00b7ber", "nein", "!", "Ein", "lei\u00b7ses", "Et\u00b7was", ",", "nenn'", "ich", "Wink", "es", "o\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKANT", "$.", "ART", "ADJA", "ADV", "$,", "VVFIN", "PPER", "NN", "PPER", "KON"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.7": {"text": "Weht von dir zu mir und lindert unsre Plage lange", "tokens": ["Weht", "von", "dir", "zu", "mir", "und", "lin\u00b7dert", "uns\u00b7re", "Pla\u00b7ge", "lan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPER", "APPR", "PPER", "KON", "VVFIN", "PPOSAT", "NN", "ADV"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.8": {"text": "Doch was frommt's? Es trennt uns Alles, Sprach' und", "tokens": ["Doch", "was", "frommt's", "?", "Es", "trennt", "uns", "Al\u00b7les", ",", "Sprach'", "und"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PWS", "ADJA", "$.", "PPER", "VVFIN", "PPER", "PIS", "$,", "NN", "KON"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Wandern in die Ferne mu\u00df ich, und ich zage lange", "tokens": ["Wan\u00b7dern", "in", "die", "Fer\u00b7ne", "mu\u00df", "ich", ",", "und", "ich", "za\u00b7ge", "lan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "VMFIN", "PPER", "$,", "KON", "PPER", "VVFIN", "ADV"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}}}}