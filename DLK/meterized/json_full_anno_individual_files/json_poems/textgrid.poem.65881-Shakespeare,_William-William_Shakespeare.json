{"textgrid.poem.65881": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "William Shakespeare", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei der Vogel hellsten Lautes,", "tokens": ["Sei", "der", "Vo\u00b7gel", "hells\u00b7ten", "Lau\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Horstend in Arabiens \u00d6de,", "tokens": ["Hors\u00b7tend", "in", "A\u00b7ra\u00b7biens", "\u00d6\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NE", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Banger Herold und Trommete!", "tokens": ["Ban\u00b7ger", "He\u00b7rold", "und", "Trom\u00b7me\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.4": {"text": "Fromm Gefl\u00fcgel, auf ihn schaut es.", "tokens": ["Fromm", "Ge\u00b7fl\u00fc\u00b7gel", ",", "auf", "ihn", "schaut", "es", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "APPR", "PPER", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch du kreischender Gef\u00e4hrte,", "tokens": ["Doch", "du", "krei\u00b7schen\u00b7der", "Ge\u00b7f\u00e4hr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJA", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "B\u00f6sen Feindes Eingeweihter,", "tokens": ["B\u00f6\u00b7sen", "Fein\u00b7des", "Ein\u00b7ge\u00b7weih\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Fieber-Endes arger Deuter,", "tokens": ["Fie\u00b7ber\u00b7En\u00b7des", "ar\u00b7ger", "Deu\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bleibe fern von dieser Herde!", "tokens": ["Blei\u00b7be", "fern", "von", "die\u00b7ser", "Her\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sei aus unserm Rat verwiesen", "tokens": ["Sei", "aus", "un\u00b7serm", "Rat", "ver\u00b7wie\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aller Vogel rauh und arg;", "tokens": ["Al\u00b7ler", "Vo\u00b7gel", "rauh", "und", "arg", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur der Aar, des Flugs Monarch,", "tokens": ["Nur", "der", "Aar", ",", "des", "Flugs", "Mon\u00b7arch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00fcsse dies Beg\u00e4ngnis gr\u00fc\u00dfen.", "tokens": ["M\u00fcs\u00b7se", "dies", "Be\u00b7g\u00e4ng\u00b7nis", "gr\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PDS", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Priester sei, wei\u00df infuliert,", "tokens": ["Pries\u00b7ter", "sei", ",", "wei\u00df", "in\u00b7fu\u00b7liert", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "VVFIN", "ADJD", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Der die Sterbelieder kann,", "tokens": ["Der", "die", "Ster\u00b7be\u00b7lie\u00b7der", "kann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Todesprophezeier Schwan,", "tokens": ["To\u00b7des\u00b7pro\u00b7phe\u00b7zei\u00b7er", "Schwan", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie dem Requiem geb\u00fchrt.", "tokens": ["Wie", "dem", "Re\u00b7qui\u00b7em", "ge\u00b7b\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "$."], "meter": "+-+---+", "measure": "unknown.measure.tri"}}, "stanza.5": {"line.1": {"text": "Und du, Kr\u00e4he, dreifach alte,", "tokens": ["Und", "du", ",", "Kr\u00e4\u00b7he", ",", "drei\u00b7fach", "al\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "NN", "$,", "PAV", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die ihr schwarz Geschlecht erzielt", "tokens": ["Die", "ihr", "schwarz", "Ge\u00b7schlecht", "er\u00b7zielt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADJD", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit Odem, den sie gibt und stiehlt,", "tokens": ["Mit", "O\u00b7dem", ",", "den", "sie", "gibt", "und", "stiehlt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu dem Trauerzug dich halte!", "tokens": ["Zu", "dem", "Trau\u00b7er\u00b7zug", "dich", "hal\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Hier beginnt der Chor. \u2013 Zusammen", "tokens": ["Hier", "be\u00b7ginnt", "der", "Chor", ".", "\u2013", "Zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "$(", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "T\u00f6net's: Lieb' und Treu ist hin;", "tokens": ["T\u00f6\u00b7net's", ":", "Lieb'", "und", "Treu", "ist", "hin", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "KON", "NN", "VAFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Turteltaub' und Ph\u00f6nix fliehn", "tokens": ["Tur\u00b7tel\u00b7taub'", "und", "Ph\u00f6\u00b7nix", "fliehn"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus der Welt in Wechselflammen.", "tokens": ["Aus", "der", "Welt", "in", "Wech\u00b7sel\u00b7flam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Liebten sich, wie wenn, verdichtet", "tokens": ["Lieb\u00b7ten", "sich", ",", "wie", "wenn", ",", "ver\u00b7dich\u00b7tet"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VVFIN", "PRF", "$,", "KOKOM", "KOUS", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lieb' in Zwei'n zu einem Wesen,", "tokens": ["Lieb'", "in", "Zwei'n", "zu", "ei\u00b7nem", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Trennungslos geteilt gewesen.", "tokens": ["Tren\u00b7nungs\u00b7los", "ge\u00b7teilt", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VAPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da hat Liebe Zahl vernichtet.", "tokens": ["Da", "hat", "Lie\u00b7be", "Zahl", "ver\u00b7nich\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Herzen nah im Weiten schienen;", "tokens": ["Her\u00b7zen", "nah", "im", "Wei\u00b7ten", "schie\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn nicht Raum war, und doch Ferne", "tokens": ["Denn", "nicht", "Raum", "war", ",", "und", "doch", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PTKNEG", "NN", "VAFIN", "$,", "KON", "ADV", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Zwischen Taub' und ihrem Sterne.", "tokens": ["Zwi\u00b7schen", "Taub'", "und", "ih\u00b7rem", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Allen Wunder, au\u00dfer ihnen.", "tokens": ["Al\u00b7len", "Wun\u00b7der", ",", "au\u00b7\u00dfer", "ih\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "APPR", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Liebesstrahl durchzuckte so sie,", "tokens": ["Lie\u00b7bes\u00b7strahl", "durch\u00b7zuck\u00b7te", "so", "sie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PPER", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Da\u00df der Ph\u00f6nix all sein Gl\u00fcck", "tokens": ["Da\u00df", "der", "Ph\u00f6\u00b7nix", "all", "sein", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PIAT", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Flammen sah im Turtelblick;", "tokens": ["Flam\u00b7men", "sah", "im", "Tur\u00b7tel\u00b7blick", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jedes jedem ein Potosi.", "tokens": ["Je\u00b7des", "je\u00b7dem", "ein", "Po\u00b7to\u00b7si", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PIS", "ART", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Eigentum sich so verlie\u00df,", "tokens": ["Ei\u00b7gen\u00b7tum", "sich", "so", "ver\u00b7lie\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df im Selbst das Selbst verschwand,", "tokens": ["Da\u00df", "im", "Selbst", "das", "Selbst", "ver\u00b7schwand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Einzelwesen, zwiebenannt,", "tokens": ["Ein\u00b7zel\u00b7we\u00b7sen", ",", "zwie\u00b7be\u00b7nannt", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Weder zwei noch eines hie\u00df.", "tokens": ["We\u00b7der", "zwei", "noch", "ei\u00b7nes", "hie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "ADV", "PIS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Selbst Vernunft, sie schien bedr\u00e4ngt,", "tokens": ["Selbst", "Ver\u00b7nunft", ",", "sie", "schien", "be\u00b7dr\u00e4ngt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "PPER", "VVFIN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sah Getrenntes sich vereinen,", "tokens": ["Sah", "Ge\u00b7trenn\u00b7tes", "sich", "ver\u00b7ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "An sich selbst wie nichts erscheinen:", "tokens": ["An", "sich", "selbst", "wie", "nichts", "er\u00b7schei\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ADV", "KOKOM", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schlichtes war so wohl vermengt,", "tokens": ["Schlich\u00b7tes", "war", "so", "wohl", "ver\u00b7mengt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Da\u00df sie rief: wie treugepaart", "tokens": ["Da\u00df", "sie", "rief", ":", "wie", "treu\u00b7ge\u00b7paart"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$.", "PWAV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hier in Eintracht ein Geschlecht!", "tokens": ["Hier", "in", "Ein\u00b7tracht", "ein", "Ge\u00b7schlecht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Recht hat Liebe nun, nicht Recht,", "tokens": ["Recht", "hat", "Lie\u00b7be", "nun", ",", "nicht", "Recht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "ADV", "$,", "PTKNEG", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo Entfernung so beharrt.", "tokens": ["Wo", "Ent\u00b7fer\u00b7nung", "so", "be\u00b7harrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Und erhub dann diese Klage", "tokens": ["Und", "er\u00b7hub", "dann", "die\u00b7se", "Kla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um der Liebe Stern und Helden,", "tokens": ["Um", "der", "Lie\u00b7be", "Stern", "und", "Hel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Taub' und Ph\u00f6nix die Entseelten,", "tokens": ["Taub'", "und", "Ph\u00f6\u00b7nix", "die", "Ent\u00b7seel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als Choral am Sarkophage:", "tokens": ["Als", "Cho\u00b7ral", "am", "Sar\u00b7ko\u00b7pha\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPRART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.14": {"line.1": {"text": "Sei der Vogel hellsten Lautes,", "tokens": ["Sei", "der", "Vo\u00b7gel", "hells\u00b7ten", "Lau\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Horstend in Arabiens \u00d6de,", "tokens": ["Hors\u00b7tend", "in", "A\u00b7ra\u00b7biens", "\u00d6\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NE", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Banger Herold und Trommete!", "tokens": ["Ban\u00b7ger", "He\u00b7rold", "und", "Trom\u00b7me\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.4": {"text": "Fromm Gefl\u00fcgel, auf ihn schaut es.", "tokens": ["Fromm", "Ge\u00b7fl\u00fc\u00b7gel", ",", "auf", "ihn", "schaut", "es", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "APPR", "PPER", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Doch du kreischender Gef\u00e4hrte,", "tokens": ["Doch", "du", "krei\u00b7schen\u00b7der", "Ge\u00b7f\u00e4hr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJA", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "B\u00f6sen Feindes Eingeweihter,", "tokens": ["B\u00f6\u00b7sen", "Fein\u00b7des", "Ein\u00b7ge\u00b7weih\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Fieber-Endes arger Deuter,