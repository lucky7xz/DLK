{"textgrid.poem.59826": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo man fr\u00f6hlich in traulicher Runde ist,", "tokens": ["Wo", "man", "fr\u00f6h\u00b7lich", "in", "trau\u00b7li\u00b7cher", "Run\u00b7de", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADJD", "APPR", "ADJA", "NN", "VAFIN", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Ohne zu achten, ob's fr\u00fch oder sp\u00e4t an der Stunde ist,", "tokens": ["Oh\u00b7ne", "zu", "ach\u00b7ten", ",", "ob's", "fr\u00fch", "o\u00b7der", "sp\u00e4t", "an", "der", "Stun\u00b7de", "ist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PTKZU", "VVINF", "$,", "KOUS", "ADJD", "KON", "ADJD", "APPR", "ART", "NN", "VAFIN", "$,"], "meter": "+--+--+-+-+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Wo der Becher von Wein \u00fcberflie\u00dft und die Lippe von Witz", "tokens": ["Wo", "der", "Be\u00b7cher", "von", "Wein", "\u00fc\u00b7berf\u00b7lie\u00dft", "und", "die", "Lip\u00b7pe", "von", "Witz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "VVFIN", "KON", "ART", "NN", "APPR", "NN"], "meter": "--+--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Und ein rosiges Kind mit den Zechern im Bunde ist:", "tokens": ["Und", "ein", "ro\u00b7si\u00b7ges", "Kind", "mit", "den", "Ze\u00b7chern", "im", "Bun\u00b7de", "ist", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA"