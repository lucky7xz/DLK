{"textgrid.poem.46815": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[in dem Zimmer, dessen Fenster]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In dem Zimmer, dessen Fenster", "tokens": ["In", "dem", "Zim\u00b7mer", ",", "des\u00b7sen", "Fens\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gegen Ost und Mittag sehn,", "tokens": ["Ge\u00b7gen", "Ost", "und", "Mit\u00b7tag", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn darin Gespenster gehn,", "tokens": ["Wenn", "da\u00b7rin", "Ge\u00b7spens\u00b7ter", "gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PAV", "NN", "VVINF", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Sind es sonnige Gespenster.", "tokens": ["Sind", "es", "son\u00b7ni\u00b7ge", "Ge\u00b7spens\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dort wo unsre beiden Leichen,", "tokens": ["Dort", "wo", "uns\u00b7re", "bei\u00b7den", "Lei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "PPOSAT", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eine nach der andern lagen,", "tokens": ["Ei\u00b7ne", "nach", "der", "an\u00b7dern", "la\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird an allerhellsten Tagen", "tokens": ["Wird", "an", "al\u00b7ler\u00b7hells\u00b7ten", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht ein sanfter Schauer weichen.", "tokens": ["Nicht", "ein", "sanf\u00b7ter", "Schau\u00b7er", "wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "La\u00df herein die Lichter k\u00e4mpfen", "tokens": ["La\u00df", "her\u00b7ein", "die", "Lich\u00b7ter", "k\u00e4mp\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PTKVZ", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch die Fenster, wehr nur ihnen", "tokens": ["Durch", "die", "Fens\u00b7ter", ",", "wehr", "nur", "ih\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "ADV", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht mit Laden und Gardinen!", "tokens": ["Nicht", "mit", "La\u00b7den", "und", "Gar\u00b7di\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieser Schauer wird sie d\u00e4mpfen.", "tokens": ["Die\u00b7ser", "Schau\u00b7er", "wird", "sie", "d\u00e4mp\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Hier am N\u00e4h- und Schreibetische", "tokens": ["Hier", "am", "