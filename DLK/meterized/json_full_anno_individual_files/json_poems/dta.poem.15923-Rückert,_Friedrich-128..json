{"dta.poem.15923": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "128.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mit Unvollkommenheit zu ringen, ist das Loo\u00df", "tokens": ["Mit", "Un\u00b7voll\u00b7kom\u00b7men\u00b7heit", "zu", "rin\u00b7gen", ",", "ist", "das", "Loo\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "$,", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Menschen, ist sein Werth, und nicht sein Mangel blo\u00df.", "tokens": ["Des", "Men\u00b7schen", ",", "ist", "sein", "Werth", ",", "und", "nicht", "sein", "Man\u00b7gel", "blo\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VAFIN", "PPOSAT", "NN", "$,", "KON", "PTKNEG", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was unvollkommen ist, das soll vollkommen werden;", "tokens": ["Was", "un\u00b7voll\u00b7kom\u00b7men", "ist", ",", "das", "soll", "voll\u00b7kom\u00b7men", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "PDS", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn nur zum Werden, nicht zum Seyn, sind wir auf Erden.", "tokens": ["Denn", "nur", "zum", "Wer\u00b7den", ",", "nicht", "zum", "Seyn", ",", "sind", "wir", "auf", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "$,", "PTKNEG", "APPRART", "NN", "$,", "VAFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}