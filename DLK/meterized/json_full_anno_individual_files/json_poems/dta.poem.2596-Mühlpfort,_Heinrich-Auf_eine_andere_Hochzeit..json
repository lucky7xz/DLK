{"dta.poem.2596": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf eine andere Hochzeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich liesse meine Pflicht in sch\u00f6nen Reimen sp\u00fcren/", "tokens": ["Ich", "lies\u00b7se", "mei\u00b7ne", "Pflicht", "in", "sch\u00f6\u00b7nen", "Rei\u00b7men", "sp\u00fc\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wann mir die Blumen in dem Helicon bekandt;", "tokens": ["Wann", "mir", "die", "Blu\u00b7men", "in", "dem", "He\u00b7li\u00b7con", "be\u00b7kandt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So wil Apollo nicht die gr\u00fcnen Geister r\u00fchren/", "tokens": ["So", "wil", "A\u00b7pol\u00b7lo", "nicht", "die", "gr\u00fc\u00b7nen", "Geis\u00b7ter", "r\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NE", "PTKNEG", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und was ich schreiben soll geschicht mit schwacher", "tokens": ["Und", "was", "ich", "schrei\u00b7ben", "soll", "ge\u00b7schicht", "mit", "schwa\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPER", "VVINF", "VMFIN", "VVPP", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Jedoch/ hochwerther Herr/ es wird ihm nicht mi\u00dffallen/", "tokens": ["Je\u00b7doch", "/", "hoch\u00b7wert\u00b7her", "Herr", "/", "es", "wird", "ihm", "nicht", "mi\u00df\u00b7fal\u00b7len", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "ADJA", "NN", "$(", "PPER", "VAFIN", "PPER", "PTKNEG", "VVINF", "$("], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Schl\u00e4gt doch der Jupiter kein Mehl zum Opffer aus.", "tokens": ["Schl\u00e4gt", "doch", "der", "Ju\u00b7pi\u00b7ter", "kein", "Mehl", "zum", "Opf\u00b7fer", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PIAT", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mein unge\u00fcbter Mund kan freylich nichts als lallen;", "tokens": ["Mein", "un\u00b7ge\u00b7\u00fcb\u00b7ter", "Mund", "kan", "frey\u00b7lich", "nichts", "als", "lal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "ADV", "PIS", "KOKOM", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der Ph\u00f6bus hat mir nie geschenckt den Lorbeerstraus.", "tokens": ["Der", "Ph\u00f6\u00b7bus", "hat", "mir", "nie", "ge\u00b7schenckt", "den", "Lor\u00b7beer\u00b7straus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich ehre diesen Tag mit tieff verbundnem Hertzen/", "tokens": ["Ich", "eh\u00b7re", "die\u00b7sen", "Tag", "mit", "tieff", "ver\u00b7bund\u00b7nem", "Hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "APPR", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da wieder eine Braut des Himmels-Gunst ihm schenckt/", "tokens": ["Da", "wie\u00b7der", "ei\u00b7ne", "Braut", "des", "Him\u00b7mels\u00b7Gunst", "ihm", "schenckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ART", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und wil da\u00df er bey Lust und Anmuts reichen schertzen/", "tokens": ["Und", "wil", "da\u00df", "er", "bey", "Lust", "und", "An\u00b7muts", "rei\u00b7chen", "schert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "KOUS", "PPER", "APPR", "NN", "KON", "NN", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Vergrabe diese Pein so ihn bi\u00dfher gekr\u00e4nckt.", "tokens": ["Ver\u00b7gra\u00b7be", "die\u00b7se", "Pein", "so", "ihn", "bi\u00df\u00b7her", "ge\u00b7kr\u00e4nckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "ADV", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der H\u00f6chste gebe Gl\u00fcck und Segen zu dem Stande!", "tokens": ["Der", "H\u00f6chs\u00b7te", "ge\u00b7be", "Gl\u00fcck", "und", "Se\u00b7gen", "zu", "dem", "Stan\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "KON", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Lebt hochgeliebte Zwey in g\u00f6ldner Einigkeit!", "tokens": ["Lebt", "hoch\u00b7ge\u00b7lieb\u00b7te", "Zwey", "in", "g\u00f6ld\u00b7ner", "Ei\u00b7nig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die Flamme so da scheint von beyder Liebes-Brande/", "tokens": ["Die", "Flam\u00b7me", "so", "da", "scheint", "von", "bey\u00b7der", "Lie\u00b7bes\u00b7Bran\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "VVFIN", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Heg ewigliche Glut/ die Funcken von sich streit!", "tokens": ["Heg", "e\u00b7wig\u00b7li\u00b7che", "Glut", "/", "die", "Fun\u00b7cken", "von", "sich", "streit", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$(", "ART", "NN", "APPR", "PRF", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die Parcen spinnen euch von Gold des Lebens Netze!", "tokens": ["Die", "Par\u00b7cen", "spin\u00b7nen", "euch", "von", "Gold", "des", "Le\u00b7bens", "Net\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Der Segen aus der H\u00f6h kr\u00f6n euer Bett und Stamm!", "tokens": ["Der", "Se\u00b7gen", "aus", "der", "H\u00f6h", "kr\u00f6n", "eu\u00b7er", "Bett", "und", "Stamm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Seht Neff und Niefftelin/ der Ehe wehrtste Sch\u00e4tze/", "tokens": ["Seht", "Neff", "und", "Nieff\u00b7te\u00b7lin", "/", "der", "E\u00b7he", "wehrts\u00b7te", "Sch\u00e4t\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NE", "$(", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Es lieb und lebe wohl stets Braut und Br\u00e4utigam!", "tokens": ["Es", "lieb", "und", "le\u00b7be", "wohl", "stets", "Braut", "und", "Br\u00e4u\u00b7ti\u00b7gam", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "VVFIN", "ADV", "ADV", "NN", "KON", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}