{"textgrid.poem.44386": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Epilog zur ersten Abteilung der Trilogie \u00bbDas goldene Vlie\u00df\u00ab, aufgef\u00fchrt als freie Einnahme der Regisseurs des Hofurgtheaters", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der \u00e4ltste einer altbekannten Schar,", "tokens": ["Der", "\u00e4lts\u00b7te", "ei\u00b7ner", "alt\u00b7be\u00b7kann\u00b7ten", "Schar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zu eurem und der Musen Dienst vereinigt,", "tokens": ["Zu", "eu\u00b7rem", "und", "der", "Mu\u00b7sen", "Dienst", "ver\u00b7ei\u00b7nigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "KON", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nah ich, von meinen Br\u00fcdern abgesandt \u2013", "tokens": ["Nah", "ich", ",", "von", "mei\u00b7nen", "Br\u00fc\u00b7dern", "ab\u00b7ge\u00b7sandt", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der eine, euch den Dank von allen bringend.", "tokens": ["Der", "ei\u00b7ne", ",", "euch", "den", "Dank", "von", "al\u00b7len", "brin\u00b7gend", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "$,", "PPER", "ART", "NN", "APPR", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Den Dienstbefli\u00dfnen, immerdar bem\u00fcht,", "tokens": ["Den", "Dienst\u00b7be\u00b7fli\u00df\u00b7nen", ",", "im\u00b7mer\u00b7dar", "be\u00b7m\u00fcht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "An fremdem Herd f\u00fcr fremden Gaum zu sorgen,", "tokens": ["An", "frem\u00b7dem", "Herd", "f\u00fcr", "frem\u00b7den", "Gaum", "zu", "sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Uns ist ein einzigmal im Jahr verg\u00f6nnt,", "tokens": ["Uns", "ist", "ein", "ein\u00b7zig\u00b7mal", "im", "Jahr", "ver\u00b7g\u00f6nnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Den Hausherrn selbst zu machen, werte G\u00e4ste", "tokens": ["Den", "Haus\u00b7herrn", "selbst", "zu", "ma\u00b7chen", ",", "wer\u00b7te", "G\u00e4s\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADV", "PTKZU", "VVINF", "$,", "VVFIN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Am kleinen Tische freundlich zu empfangen", "tokens": ["Am", "klei\u00b7nen", "Ti\u00b7sche", "freund\u00b7lich", "zu", "emp\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und zu bewirten, wie das Haus es gibt.", "tokens": ["Und", "zu", "be\u00b7wir\u00b7ten", ",", "wie", "das", "Haus", "es", "gibt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKZU", "VVINF", "$,", "PWAV", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Da ist nun unser Streben drauf gewendet,", "tokens": ["Da", "ist", "nun", "un\u00b7ser", "Stre\u00b7ben", "drauf", "ge\u00b7wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "NN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Da\u00df des Vertrauens wir auch wert uns zeigen,", "tokens": ["Da\u00df", "des", "Ver\u00b7trau\u00b7ens", "wir", "auch", "wert", "uns", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "ADV", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Den Vielwillkommnen sich Willkommnes biete,", "tokens": ["Den", "Viel\u00b7will\u00b7komm\u00b7nen", "sich", "Will\u00b7komm\u00b7nes", "bie\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Und Tisch und Becher gl\u00e4nze bei dem Mahle.", "tokens": ["Und", "Tisch", "und", "Be\u00b7cher", "gl\u00e4n\u00b7ze", "bei", "dem", "Mah\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Das ist so leicht nicht in so karger Zeit!", "tokens": ["Das", "ist", "so", "leicht", "nicht", "in", "so", "kar\u00b7ger", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "PTKNEG", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Die alten Meister sind dahingegangen,", "tokens": ["Die", "al\u00b7ten", "Meis\u00b7ter", "sind", "da\u00b7hin\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PAV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Kaum lebet einer, widerwillig, noch;", "tokens": ["Kaum", "le\u00b7bet", "ei\u00b7ner", ",", "wi\u00b7der\u00b7wil\u00b7lig", ",", "noch", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "ADJD", "$,", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Die jungen aber, sie und sich verkennend,", "tokens": ["Die", "jun\u00b7gen", "a\u00b7ber", ",", "sie", "und", "sich", "ver\u00b7ken\u00b7nend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "$,", "PPER", "KON", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Erweitrung der Gedanken und der Kraft", "tokens": ["Er\u00b7weit\u00b7rung", "der", "Ge\u00b7dan\u00b7ken", "und", "der", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "F\u00fcr eines haltend, sehn auf jener Ziel \u2013", "tokens": ["F\u00fcr", "ei\u00b7nes", "hal\u00b7tend", ",", "sehn", "auf", "je\u00b7ner", "Ziel", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "$,", "VVFIN", "APPR", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Als h\u00e4tts ihr Fu\u00df erreicht, weil es ihr Auge \u2013", "tokens": ["Als", "h\u00e4tts", "ihr", "Fu\u00df", "er\u00b7reicht", ",", "weil", "es", "ihr", "Au\u00b7ge", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPOSAT", "NN", "VVPP", "$,", "KOUS", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Mit vornehm stolzem Mitleid tief herab,", "tokens": ["Mit", "vor\u00b7nehm", "stol\u00b7zem", "Mit\u00b7leid", "tief", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Nur weiter! weiter! geht das rasche Streben.", "tokens": ["Nur", "wei\u00b7ter", "!", "wei\u00b7ter", "!", "geht", "das", "ra\u00b7sche", "Stre\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$.", "ADV", "$.", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Das H\u00f6chste will nur jeder und verfehlt", "tokens": ["Das", "H\u00f6chs\u00b7te", "will", "nur", "je\u00b7der", "und", "ver\u00b7fehlt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "ADV", "PIAT", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Das N\u00e4chste dr\u00fcber kindisch schwachen Flugs.", "tokens": ["Das", "N\u00e4chs\u00b7te", "dr\u00fc\u00b7ber", "kin\u00b7disch", "schwa\u00b7chen", "Flugs", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Wohl schwere Wahl in also karger Zeit.", "tokens": ["Wohl", "schwe\u00b7re", "Wahl", "in", "al\u00b7so", "kar\u00b7ger", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da ist das Werk in unsre Hand geraten,", "tokens": ["Da", "ist", "das", "Werk", "in", "uns\u00b7re", "Hand", "ge\u00b7ra\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das ihr zum Teil gesehn, zum Teil noch nicht.", "tokens": ["Das", "ihr", "zum", "Teil", "ge\u00b7sehn", ",", "zum", "Teil", "noch", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPRART", "NN", "VVPP", "$,", "APPRART", "NN", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wir merkten drin, ob gut sonst oder schlimm,", "tokens": ["Wir", "merk\u00b7ten", "drin", ",", "ob", "gut", "sonst", "o\u00b7der", "schlimm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "KOUS", "ADJD", "ADV", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gar manchen Anklang aus entschwundner Zeit,", "tokens": ["Gar", "man\u00b7chen", "An\u00b7klang", "aus", "ent\u00b7schwund\u00b7ner", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und so, vertrauend, bringen wir euchs dar.", "tokens": ["Und", "so", ",", "ver\u00b7trau\u00b7end", ",", "brin\u00b7gen", "wir", "euchs", "dar", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "VVPP", "$,", "VVFIN", "PPER", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Verfa\u00dft hats einer, der sich euer nennt,", "tokens": ["Ver\u00b7fa\u00dft", "hats", "ei\u00b7ner", ",", "der", "sich", "eu\u00b7er", "nennt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PIS", "$,", "PRELS", "PRF", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Als unter euch geboren, euch verwandt", "tokens": ["Als", "un\u00b7ter", "euch", "ge\u00b7bo\u00b7ren", ",", "euch", "ver\u00b7wandt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "APPR", "PPER", "VVPP", "$,", "PPER", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Durch das, was dieses Landes Beste bindet,", "tokens": ["Durch", "das", ",", "was", "die\u00b7ses", "Lan\u00b7des", "Bes\u00b7te", "bin\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "$,", "PWS", "PDAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ein offnes Herz und einen schlichten Sinn.", "tokens": ["Ein", "off\u00b7nes", "Herz", "und", "ei\u00b7nen", "schlich\u00b7ten", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und solchen Sinns hat er sein Werk vollbracht.", "tokens": ["Und", "sol\u00b7chen", "Sinns", "hat", "er", "sein", "Werk", "voll\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Nicht \u00fcberbieten soll es, was schon da,", "tokens": ["Nicht", "\u00fc\u00b7berb\u00b7ie\u00b7ten", "soll", "es", ",", "was", "schon", "da", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "PPER", "$,", "PRELS", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Durch die Verl\u00e4ngrung altgewohnten Ma\u00dfes,", "tokens": ["Durch", "die", "Ver\u00b7l\u00e4n\u00b7grung", "alt\u00b7ge\u00b7wohn\u00b7ten", "Ma\u00b7\u00dfes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.13": {"text": "Aus dreien St\u00fccken k\u00fcnstlich eines flechtend,", "tokens": ["Aus", "drei\u00b7en", "St\u00fc\u00b7cken", "k\u00fcnst\u00b7lich", "ei\u00b7nes", "flech\u00b7tend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "ADJD", "ART", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Dem Geiste folgend, der ihm so gebot,", "tokens": ["Dem", "Geis\u00b7te", "fol\u00b7gend", ",", "der", "ihm", "so", "ge\u00b7bot", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Fast wider Willen folgend, tat er so.", "tokens": ["Fast", "wi\u00b7der", "Wil\u00b7len", "fol\u00b7gend", ",", "tat", "er", "so", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ADJD", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Was ihr nun heut gesehn, der Eingang ists,", "tokens": ["Was", "ihr", "nun", "heut", "ge\u00b7sehn", ",", "der", "Ein\u00b7gang", "ists", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "VVPP", "$,", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Schwelle jenes tiefverschlungnen Baus,", "tokens": ["Die", "Schwel\u00b7le", "je\u00b7nes", "tief\u00b7ver\u00b7schlung\u00b7nen", "Baus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In den er euch, den Faden reichend, f\u00fchrt.", "tokens": ["In", "den", "er", "euch", ",", "den", "Fa\u00b7den", "rei\u00b7chend", ",", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PPER", "$,", "ART", "NN", "VVPP", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Von wo der Mensch beginnt, womit er endet,", "tokens": ["Von", "wo", "der", "Mensch", "be\u00b7ginnt", ",", "wo\u00b7mit", "er", "en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PWAV", "ART", "NN", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Was er f\u00fcr M\u00e4chte in der Brust verbirgt,", "tokens": ["Was", "er", "f\u00fcr", "M\u00e4ch\u00b7te", "in", "der", "Brust", "ver\u00b7birgt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und was f\u00fcr M\u00e4chte seine Brust ihm bergen.", "tokens": ["Und", "was", "f\u00fcr", "M\u00e4ch\u00b7te", "sei\u00b7ne", "Brust", "ihm", "ber\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "NN", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Das ist der Inhalt unsers ernsten Spiels.", "tokens": ["Das", "ist", "der", "In\u00b7halt", "un\u00b7sers", "erns\u00b7ten", "Spiels", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Was heut begonnen, morgen wirds vollendet;", "tokens": ["Was", "heut", "be\u00b7gon\u00b7nen", ",", "mor\u00b7gen", "wirds", "voll\u00b7en\u00b7det", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVPP", "$,", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Drum folget g\u00fcnstig unsern Schritten nach", "tokens": ["Drum", "fol\u00b7get", "g\u00fcns\u00b7tig", "un\u00b7sern", "Schrit\u00b7ten", "nach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ADJD", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und schiebt f\u00fcr heute noch das Urteil auf,", "tokens": ["Und", "schiebt", "f\u00fcr", "heu\u00b7te", "noch", "das", "Ur\u00b7teil", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADV", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Bis sich das Ganze als ein Ganzes zeiget.", "tokens": ["Bis", "sich", "das", "Gan\u00b7ze", "als", "ein", "Gan\u00b7zes", "zei\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "NN", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und so ist meines Kommens Zweck erf\u00fcllt,", "tokens": ["Und", "so", "ist", "mei\u00b7nes", "Kom\u00b7mens", "Zweck", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPOSAT", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Dank gebracht, das N\u00f6tige verk\u00fcndet", "tokens": ["Der", "Dank", "ge\u00b7bracht", ",", "das", "N\u00f6\u00b7ti\u00b7ge", "ver\u00b7k\u00fcn\u00b7det"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Abschied nehmen gilts. So lebt denn wohl!", "tokens": ["Und", "Ab\u00b7schied", "neh\u00b7men", "gilts", ".", "So", "lebt", "denn", "wohl", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "VVFIN", "$.", "ADV", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch ists auf lange nicht, wir hoffens alle", "tokens": ["Doch", "ists", "auf", "lan\u00b7ge", "nicht", ",", "wir", "hof\u00b7fens", "al\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "ADV", "PTKNEG", "$,", "PPER", "ADV", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und \u00fcbers Jahr sieht wohl die Kunst beisammen \u2013", "tokens": ["Und", "\u00fc\u00b7bers", "Jahr", "sieht", "wohl", "die", "Kunst", "bei\u00b7sam\u00b7men", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Denselben Tempel und dieselben Priester,", "tokens": ["Den\u00b7sel\u00b7ben", "Tem\u00b7pel", "und", "die\u00b7sel\u00b7ben", "Pries\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "KON", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Dieselben Freunde, und vielleicht \u2013 wer wei\u00df \u2013", "tokens": ["Die\u00b7sel\u00b7ben", "Freun\u00b7de", ",", "und", "viel\u00b7leicht", "\u2013", "wer", "wei\u00df", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "KON", "ADV", "$(", "PWS", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Denselben Dichter auch, geliebt es Gott und euch.", "tokens": ["Den\u00b7sel\u00b7ben", "Dich\u00b7ter", "auch", ",", "ge\u00b7liebt", "es", "Gott", "und", "euch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADV", "$,", "VVFIN", "PPER", "NN", "KON", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Der \u00e4ltste einer altbekannten Schar,", "tokens": ["Der", "\u00e4lts\u00b7te", "ei\u00b7ner", "alt\u00b7be\u00b7kann\u00b7ten", "Schar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zu eurem und der Musen Dienst vereinigt,", "tokens": ["Zu", "eu\u00b7rem", "und", "der", "Mu\u00b7sen", "Dienst", "ver\u00b7ei\u00b7nigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "KON", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nah ich, von meinen Br\u00fcdern abgesandt \u2013", "tokens": ["Nah", "ich", ",", "von", "mei\u00b7nen", "Br\u00fc\u00b7dern", "ab\u00b7ge\u00b7sandt", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der eine, euch den Dank von allen bringend.", "tokens": ["Der", "ei\u00b7ne", ",", "euch", "den", "Dank", "von", "al\u00b7len", "brin\u00b7gend", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "$,", "PPER", "ART", "NN", "APPR", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Den Dienstbefli\u00dfnen, immerdar bem\u00fcht,", "tokens": ["Den", "Dienst\u00b7be\u00b7fli\u00df\u00b7nen", ",", "im\u00b7mer\u00b7dar", "be\u00b7m\u00fcht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "An fremdem Herd f\u00fcr fremden Gaum zu sorgen,", "tokens": ["An", "frem\u00b7dem", "Herd", "f\u00fcr", "frem\u00b7den", "Gaum", "zu", "sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Uns ist ein einzigmal im Jahr verg\u00f6nnt,", "tokens": ["Uns", "ist", "ein", "ein\u00b7zig\u00b7mal", "im", "Jahr", "ver\u00b7g\u00f6nnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Den Hausherrn selbst zu machen, werte G\u00e4ste", "tokens": ["Den", "Haus\u00b7herrn", "selbst", "zu", "ma\u00b7chen", ",", "wer\u00b7te", "G\u00e4s\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADV", "PTKZU", "VVINF", "$,", "VVFIN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Am kleinen Tische freundlich zu empfangen", "tokens": ["Am", "klei\u00b7nen", "Ti\u00b7sche", "freund\u00b7lich", "zu", "emp\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und zu bewirten, wie das Haus es gibt.", "tokens": ["Und", "zu", "be\u00b7wir\u00b7ten", ",", "wie", "das", "Haus", "es", "gibt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKZU", "VVINF", "$,", "PWAV", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Da ist nun unser Streben drauf gewendet,", "tokens": ["Da", "ist", "nun", "un\u00b7ser", "Stre\u00b7ben", "drauf", "ge\u00b7wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "NN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Da\u00df des Vertrauens wir auch wert uns zeigen,", "tokens": ["Da\u00df", "des", "Ver\u00b7trau\u00b7ens", "wir", "auch", "wert", "uns", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "ADV", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Den Vielwillkommnen sich Willkommnes biete,", "tokens": ["Den", "Viel\u00b7will\u00b7komm\u00b7nen", "sich", "Will\u00b7komm\u00b7nes", "bie\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Und Tisch und Becher gl\u00e4nze bei dem Mahle.", "tokens": ["Und", "Tisch", "und", "Be\u00b7cher", "gl\u00e4n\u00b7ze", "bei", "dem", "Mah\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Das ist so leicht nicht in so karger Zeit!", "tokens": ["Das", "ist", "so", "leicht", "nicht", "in", "so", "kar\u00b7ger", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "PTKNEG", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Die alten Meister sind dahingegangen,", "tokens": ["Die", "al\u00b7ten", "Meis\u00b7ter", "sind", "da\u00b7hin\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PAV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Kaum lebet einer, widerwillig, noch;", "tokens": ["Kaum", "le\u00b7bet", "ei\u00b7ner", ",", "wi\u00b7der\u00b7wil\u00b7lig", ",", "noch", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "ADJD", "$,", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Die jungen aber, sie und sich verkennend,", "tokens": ["Die", "jun\u00b7gen", "a\u00b7ber", ",", "sie", "und", "sich", "ver\u00b7ken\u00b7nend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "$,", "PPER", "KON", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Erweitrung der Gedanken und der Kraft", "tokens": ["Er\u00b7weit\u00b7rung", "der", "Ge\u00b7dan\u00b7ken", "und", "der", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "F\u00fcr eines haltend, sehn auf jener Ziel \u2013", "tokens": ["F\u00fcr", "ei\u00b7nes", "hal\u00b7tend", ",", "sehn", "auf", "je\u00b7ner", "Ziel", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "$,", "VVFIN", "APPR", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Als h\u00e4tts ihr Fu\u00df erreicht, weil es ihr Auge \u2013", "tokens": ["Als", "h\u00e4tts", "ihr", "Fu\u00df", "er\u00b7reicht", ",", "weil", "es", "ihr", "Au\u00b7ge", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPOSAT", "NN", "VVPP", "$,", "KOUS", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Mit vornehm stolzem Mitleid tief herab,", "tokens": ["Mit", "vor\u00b7nehm", "stol\u00b7zem", "Mit\u00b7leid", "tief", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Nur weiter! weiter! geht das rasche Streben.", "tokens": ["Nur", "wei\u00b7ter", "!", "wei\u00b7ter", "!", "geht", "das", "ra\u00b7sche", "Stre\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$.", "ADV", "$.", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Das H\u00f6chste will nur jeder und verfehlt", "tokens": ["Das", "H\u00f6chs\u00b7te", "will", "nur", "je\u00b7der", "und", "ver\u00b7fehlt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "ADV", "PIAT", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Das N\u00e4chste dr\u00fcber kindisch schwachen Flugs.", "tokens": ["Das", "N\u00e4chs\u00b7te", "dr\u00fc\u00b7ber", "kin\u00b7disch", "schwa\u00b7chen", "Flugs", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Wohl schwere Wahl in also karger Zeit.", "tokens": ["Wohl", "schwe\u00b7re", "Wahl", "in", "al\u00b7so", "kar\u00b7ger", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Da ist das Werk in unsre Hand geraten,", "tokens": ["Da", "ist", "das", "Werk", "in", "uns\u00b7re", "Hand", "ge\u00b7ra\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das ihr zum Teil gesehn, zum Teil noch nicht.", "tokens": ["Das", "ihr", "zum", "Teil", "ge\u00b7sehn", ",", "zum", "Teil", "noch", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPRART", "NN", "VVPP", "$,", "APPRART", "NN", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wir merkten drin, ob gut sonst oder schlimm,", "tokens": ["Wir", "merk\u00b7ten", "drin", ",", "ob", "gut", "sonst", "o\u00b7der", "schlimm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "KOUS", "ADJD", "ADV", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gar manchen Anklang aus entschwundner Zeit,", "tokens": ["Gar", "man\u00b7chen", "An\u00b7klang", "aus", "ent\u00b7schwund\u00b7ner", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und so, vertrauend, bringen wir euchs dar.", "tokens": ["Und", "so", ",", "ver\u00b7trau\u00b7end", ",", "brin\u00b7gen", "wir", "euchs", "dar", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "VVPP", "$,", "VVFIN", "PPER", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Verfa\u00dft hats einer, der sich euer nennt,", "tokens": ["Ver\u00b7fa\u00dft", "hats", "ei\u00b7ner", ",", "der", "sich", "eu\u00b7er", "nennt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PIS", "$,", "PRELS", "PRF", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Als unter euch geboren, euch verwandt", "tokens": ["Als", "un\u00b7ter", "euch", "ge\u00b7bo\u00b7ren", ",", "euch", "ver\u00b7wandt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "APPR", "PPER", "VVPP", "$,", "PPER", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Durch das, was dieses Landes Beste bindet,", "tokens": ["Durch", "das", ",", "was", "die\u00b7ses", "Lan\u00b7des", "Bes\u00b7te", "bin\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "$,", "PWS", "PDAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ein offnes Herz und einen schlichten Sinn.", "tokens": ["Ein", "off\u00b7nes", "Herz", "und", "ei\u00b7nen", "schlich\u00b7ten", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und solchen Sinns hat er sein Werk vollbracht.", "tokens": ["Und", "sol\u00b7chen", "Sinns", "hat", "er", "sein", "Werk", "voll\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Nicht \u00fcberbieten soll es, was schon da,", "tokens": ["Nicht", "\u00fc\u00b7berb\u00b7ie\u00b7ten", "soll", "es", ",", "was", "schon", "da", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "PPER", "$,", "PRELS", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Durch die Verl\u00e4ngrung altgewohnten Ma\u00dfes,", "tokens": ["Durch", "die", "Ver\u00b7l\u00e4n\u00b7grung", "alt\u00b7ge\u00b7wohn\u00b7ten", "Ma\u00b7\u00dfes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.13": {"text": "Aus dreien St\u00fccken k\u00fcnstlich eines flechtend,", "tokens": ["Aus", "drei\u00b7en", "St\u00fc\u00b7cken", "k\u00fcnst\u00b7lich", "ei\u00b7nes", "flech\u00b7tend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "ADJD", "ART", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Dem Geiste folgend, der ihm so gebot,", "tokens": ["Dem", "Geis\u00b7te", "fol\u00b7gend", ",", "der", "ihm", "so", "ge\u00b7bot", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Fast wider Willen folgend, tat er so.", "tokens": ["Fast", "wi\u00b7der", "Wil\u00b7len", "fol\u00b7gend", ",", "tat", "er", "so", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ADJD", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Was ihr nun heut gesehn, der Eingang ists,", "tokens": ["Was", "ihr", "nun", "heut", "ge\u00b7sehn", ",", "der", "Ein\u00b7gang", "ists", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "VVPP", "$,", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Schwelle jenes tiefverschlungnen Baus,", "tokens": ["Die", "Schwel\u00b7le", "je\u00b7nes", "tief\u00b7ver\u00b7schlung\u00b7nen", "Baus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In den er euch, den Faden reichend, f\u00fchrt.", "tokens": ["In", "den", "er", "euch", ",", "den", "Fa\u00b7den", "rei\u00b7chend", ",", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PPER", "$,", "ART", "NN", "VVPP", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Von wo der Mensch beginnt, womit er endet,", "tokens": ["Von", "wo", "der", "Mensch", "be\u00b7ginnt", ",", "wo\u00b7mit", "er", "en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PWAV", "ART", "NN", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Was er f\u00fcr M\u00e4chte in der Brust verbirgt,", "tokens": ["Was", "er", "f\u00fcr", "M\u00e4ch\u00b7te", "in", "der", "Brust", "ver\u00b7birgt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und was f\u00fcr M\u00e4chte seine Brust ihm bergen.", "tokens": ["Und", "was", "f\u00fcr", "M\u00e4ch\u00b7te", "sei\u00b7ne", "Brust", "ihm", "ber\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "NN", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Das ist der Inhalt unsers ernsten Spiels.", "tokens": ["Das", "ist", "der", "In\u00b7halt", "un\u00b7sers", "erns\u00b7ten", "Spiels", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Was heut begonnen, morgen wirds vollendet;", "tokens": ["Was", "heut", "be\u00b7gon\u00b7nen", ",", "mor\u00b7gen", "wirds", "voll\u00b7en\u00b7det", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVPP", "$,", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Drum folget g\u00fcnstig unsern Schritten nach", "tokens": ["Drum", "fol\u00b7get", "g\u00fcns\u00b7tig", "un\u00b7sern", "Schrit\u00b7ten", "nach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ADJD", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und schiebt f\u00fcr heute noch das Urteil auf,", "tokens": ["Und", "schiebt", "f\u00fcr", "heu\u00b7te", "noch", "das", "Ur\u00b7teil", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADV", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Bis sich das Ganze als ein Ganzes zeiget.", "tokens": ["Bis", "sich", "das", "Gan\u00b7ze", "als", "ein", "Gan\u00b7zes", "zei\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "NN", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Und so ist meines Kommens Zweck erf\u00fcllt,", "tokens": ["Und", "so", "ist", "mei\u00b7nes", "Kom\u00b7mens", "Zweck", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPOSAT", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Dank gebracht, das N\u00f6tige verk\u00fcndet", "tokens": ["Der", "Dank", "ge\u00b7bracht", ",", "das", "N\u00f6\u00b7ti\u00b7ge", "ver\u00b7k\u00fcn\u00b7det"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Abschied nehmen gilts. So lebt denn wohl!", "tokens": ["Und", "Ab\u00b7schied", "neh\u00b7men", "gilts", ".", "So", "lebt", "denn", "wohl", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "VVFIN", "$.", "ADV", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch ists auf lange nicht, wir hoffens alle", "tokens": ["Doch", "ists", "auf", "lan\u00b7ge", "nicht", ",", "wir", "hof\u00b7fens", "al\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "ADV", "PTKNEG", "$,", "PPER", "ADV", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und \u00fcbers Jahr sieht wohl die Kunst beisammen \u2013", "tokens": ["Und", "\u00fc\u00b7bers", "Jahr", "sieht", "wohl", "die", "Kunst", "bei\u00b7sam\u00b7men", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Denselben Tempel und dieselben Priester,", "tokens": ["Den\u00b7sel\u00b7ben", "Tem\u00b7pel", "und", "die\u00b7sel\u00b7ben", "Pries\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "KON", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Dieselben Freunde, und vielleicht \u2013 wer wei\u00df \u2013", "tokens": ["Die\u00b7sel\u00b7ben", "Freun\u00b7de", ",", "und", "viel\u00b7leicht", "\u2013", "wer", "wei\u00df", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "KON", "ADV", "$(", "PWS", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Denselben Dichter auch, geliebt es Gott und euch.", "tokens": ["Den\u00b7sel\u00b7ben", "Dich\u00b7ter", "auch", ",", "ge\u00b7liebt", "es", "Gott", "und", "euch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADV", "$,", "VVFIN", "PPER", "NN", "KON", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}