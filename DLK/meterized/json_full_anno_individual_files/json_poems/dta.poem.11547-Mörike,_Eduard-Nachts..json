{"dta.poem.11547": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Nachts.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Und klingend jezt den jungen Hain durchl\u00e4uft!", "tokens": ["Und", "klin\u00b7gend", "jezt", "den", "jun\u00b7gen", "Hain", "durch\u00b7l\u00e4uft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da noch der freche Tag verstummt,", "tokens": ["Da", "noch", "der", "fre\u00b7che", "Tag", "ver\u00b7stummt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "H\u00f6rt man der Erdenkr\u00e4fte fl\u00fcsterndes Gedr\u00e4nge,", "tokens": ["H\u00f6rt", "man", "der", "Er\u00b7den\u00b7kr\u00e4f\u00b7te", "fl\u00fcs\u00b7tern\u00b7des", "Ge\u00b7dr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Das aufw\u00e4rts in die z\u00e4rtlichen Ges\u00e4nge", "tokens": ["Das", "auf\u00b7w\u00e4rts", "in", "die", "z\u00e4rt\u00b7li\u00b7chen", "Ge\u00b7s\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der reingestimmten L\u00fcfte summt.", "tokens": ["Der", "rein\u00b7ge\u00b7stimm\u00b7ten", "L\u00fcf\u00b7te", "summt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie ein Gewebe zuckt die Luft manchmal,", "tokens": ["Wie", "ein", "Ge\u00b7we\u00b7be", "zuckt", "die", "Luft", "manch\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Durchsicht'ger stets und leichter aufzuwehen,", "tokens": ["Durch\u00b7sicht'\u00b7ger", "stets", "und", "leich\u00b7ter", "auf\u00b7zu\u00b7we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dazwischen h\u00f6rt man weiche T\u00f6ne gehen,", "tokens": ["Da\u00b7zwi\u00b7schen", "h\u00f6rt", "man", "wei\u00b7che", "T\u00f6\u00b7ne", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIS", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von sel'gen Feen, die im Sternensaa!", "tokens": ["Von", "sel'\u00b7gen", "Feen", ",", "die", "im", "Ster\u00b7nen\u00b7saa", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "APPRART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Beim Sph\u00e4renklang,", "tokens": ["Beim", "Sph\u00e4\u00b7ren\u00b7klang", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Und flei\u00dfig mit Gesang", "tokens": ["Und", "flei\u00b7\u00dfig", "mit", "Ge\u00b7sang"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die goldnen Spindeln hin und wieder drehen.", "tokens": ["Die", "gold\u00b7nen", "Spin\u00b7deln", "hin", "und", "wie\u00b7der", "dre\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "KON", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}