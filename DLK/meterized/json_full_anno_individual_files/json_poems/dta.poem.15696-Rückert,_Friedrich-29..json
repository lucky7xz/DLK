{"dta.poem.15696": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "29.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du stehest \u00fcberall an der Gedanken Gr\u00e4nze,", "tokens": ["Du", "ste\u00b7hest", "\u00fc\u00b7be\u00b7rall", "an", "der", "Ge\u00b7dan\u00b7ken", "Gr\u00e4n\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und halb ist alles, was ich nicht durch dich erg\u00e4nze,", "tokens": ["Und", "halb", "ist", "al\u00b7les", ",", "was", "ich", "nicht", "durch", "dich", "er\u00b7g\u00e4n\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PIS", "$,", "PWS", "PPER", "PTKNEG", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du Anfang nicht allein und Ende meines Seyns,", "tokens": ["Du", "An\u00b7fang", "nicht", "al\u00b7lein", "und", "En\u00b7de", "mei\u00b7nes", "Seyns", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PTKNEG", "ADV", "KON", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auch Mitte du, darin ich mit mir selbst bin eins.", "tokens": ["Auch", "Mit\u00b7te", "du", ",", "da\u00b7rin", "ich", "mit", "mir", "selbst", "bin", "eins", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PPER", "$,", "PAV", "PPER", "APPR", "PPER", "ADV", "VAFIN", "PIS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich mit mir selbst bin eins, wo ich mich find' in dir;", "tokens": ["Ich", "mit", "mir", "selbst", "bin", "eins", ",", "wo", "ich", "mich", "find'", "in", "dir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "ADV", "VAFIN", "PIS", "$,", "PWAV", "PPER", "PRF", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wo ich dich verlor, kam ich abhanden mir.", "tokens": ["Und", "wo", "ich", "dich", "ver\u00b7lor", ",", "kam", "ich", "ab\u00b7han\u00b7den", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PRF", "VVFIN", "$,", "VVFIN", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}