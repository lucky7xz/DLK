{"dta.poem.23101": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "181. Da\u00df allergeitzigste.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Wie Geitzig ist ein Hertz! wen\u0303 tausend Welten w\u00e4re\u0303/", "tokens": ["Wie", "Geit\u00b7zig", "ist", "ein", "Hertz", "!", "we\u00f1", "tau\u00b7send", "Wel\u00b7ten", "w\u00e4r\u1ebd", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "$.", "XY", "CARD", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es w\u00fcrde sie gesambt/ und mehr darzu begehren.", "tokens": ["Es", "w\u00fcr\u00b7de", "sie", "ge\u00b7sambt", "/", "und", "mehr", "dar\u00b7zu", "be\u00b7geh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$(", "KON", "ADV", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}