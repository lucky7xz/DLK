{"dta.poem.14707": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der K\u00e4mpe .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "\u201estill den Garten sch\u00f6n' thust warten", "tokens": ["\u201e", "still", "den", "Gar\u00b7ten", "sch\u00f6n'", "thust", "war\u00b7ten"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADJD", "ART", "NN", "VVFIN", "VVFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bleibst am Fenster ofte steh'n,", "tokens": ["Bleibst", "am", "Fens\u00b7ter", "of\u00b7te", "steh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "NE", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ruhig scheinst Du, heimlich weinst Du,", "tokens": ["Ru\u00b7hig", "scheinst", "Du", ",", "heim\u00b7lich", "weinst", "Du", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "ADJD", "VVFIN", "PPER", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Wie die Schiffe unten geh'n.\u201c", "tokens": ["Wie", "die", "Schif\u00b7fe", "un\u00b7ten", "geh'", "n.", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "abbreviation", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VVFIN", "NE", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}