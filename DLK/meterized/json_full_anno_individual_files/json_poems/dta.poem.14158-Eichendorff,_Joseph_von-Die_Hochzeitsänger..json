{"dta.poem.14158": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Hochzeits\u00e4nger.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wollt' sie doch keinen andern haben,", "tokens": ["Wollt'", "sie", "doch", "kei\u00b7nen", "an\u00b7dern", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PIAT", "ADJA", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ging mit mir durch Wald und Feld,", "tokens": ["Ging", "mit", "mir", "durch", "Wald", "und", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Pr\u00e4chtig in den blauen Tagen", "tokens": ["Pr\u00e4ch\u00b7tig", "in", "den", "blau\u00b7en", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schien die Sonne auf die Welt.", "tokens": ["Schien", "die", "Son\u00b7ne", "auf", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hei\u00dfa: lustig Dreh'n und Ringen,", "tokens": ["Hei\u00b7\u00dfa", ":", "lus\u00b7tig", "Dreh'n", "und", "Rin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADJD", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Jeder h\u00e4lt sein Liebchen warm,", "tokens": ["Je\u00b7der", "h\u00e4lt", "sein", "Lieb\u00b7chen", "warm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und wir Musikanten singen", "tokens": ["Und", "wir", "Mu\u00b7si\u00b7kan\u00b7ten", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Lustig so, da\u00df Gott erbarm.", "tokens": ["Lus\u00b7tig", "so", ",", "da\u00df", "Gott", "er\u00b7barm", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "KOUS", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}