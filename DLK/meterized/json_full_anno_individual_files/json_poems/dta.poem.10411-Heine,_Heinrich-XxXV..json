{"dta.poem.10411": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XxXV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Seit die Liebste war entfernt,", "tokens": ["Seit", "die", "Liebs\u00b7te", "war", "ent\u00b7fernt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hatt' ich's Lachen ganz verlernt.", "tokens": ["Hatt'", "ich's", "La\u00b7chen", "ganz", "ver\u00b7lernt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schlechten Witz ri\u00df mancher Wicht,", "tokens": ["Schlech\u00b7ten", "Witz", "ri\u00df", "man\u00b7cher", "Wicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+---+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Aber lachen konnt' ich nicht.", "tokens": ["A\u00b7ber", "la\u00b7chen", "konnt'", "ich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Seit ich sie verloren hab',", "tokens": ["Seit", "ich", "sie", "ver\u00b7lo\u00b7ren", "hab'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schafft' ich auch das Weinen ab,", "tokens": ["Schafft'", "ich", "auch", "das", "Wei\u00b7nen", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fast vor Weh' das Herz mir bricht,", "tokens": ["Fast", "vor", "Weh'", "das", "Herz", "mir", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber weinen kann ich nicht.", "tokens": ["A\u00b7ber", "wei\u00b7nen", "kann", "ich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}