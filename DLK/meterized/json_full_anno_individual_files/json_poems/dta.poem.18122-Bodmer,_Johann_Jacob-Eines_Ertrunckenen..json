{"dta.poem.18122": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Eines Ertrunckenen.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198432", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hjer hat der Tod gef\u00fchrt vor seinen Urtheltisch", "tokens": ["Hjer", "hat", "der", "Tod", "ge\u00b7f\u00fchrt", "vor", "sei\u00b7nen", "Ur\u00b7thel\u00b7tisch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "VVPP", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den, welcher in der Flut ist j\u00e4mmerlich versuncken,", "tokens": ["Den", ",", "wel\u00b7cher", "in", "der", "Flut", "ist", "j\u00e4m\u00b7mer\u00b7lich", "ver\u00b7sun\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "APPR", "ART", "NN", "VAFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und hat ohn allen Durst zu tode sich getruncken,", "tokens": ["Und", "hat", "ohn", "al\u00b7len", "Durst", "zu", "to\u00b7de", "sich", "ge\u00b7trun\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PIAT", "NN", "APPR", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Ursach, halt ich, sey, er schwamm, und war kein Fisch.", "tokens": ["Die", "Ur\u00b7sach", ",", "halt", "ich", ",", "sey", ",", "er", "schwamm", ",", "und", "war", "kein", "Fisch", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "$,", "VAFIN", "$,", "PPER", "VVFIN", "$,", "KON", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}