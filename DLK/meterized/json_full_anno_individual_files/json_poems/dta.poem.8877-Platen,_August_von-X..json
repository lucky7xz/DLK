{"dta.poem.8877": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "X.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Was w\u00e4r' ich, tr\u00fcg' ich hei\u00dferes Verlangen?", "tokens": ["Was", "w\u00e4r'", "ich", ",", "tr\u00fcg'", "ich", "hei\u00b7\u00dfe\u00b7res", "Ver\u00b7lan\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In welche Netze w\u00fcrd' ich, wenn ich hangen", "tokens": ["In", "wel\u00b7che", "Net\u00b7ze", "w\u00fcrd'", "ich", ",", "wenn", "ich", "han\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "VAFIN", "PPER", "$,", "KOUS", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "An deinem Auge bliebe, mich verstricken!", "tokens": ["An", "dei\u00b7nem", "Au\u00b7ge", "blie\u00b7be", ",", "mich", "ver\u00b7stri\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Was will ich mehr noch, als ein eilig Nicken?", "tokens": ["Was", "will", "ich", "mehr", "noch", ",", "als", "ein", "ei\u00b7lig", "Ni\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "ADV", "$,", "KOUS", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es w\u00fcrden deine Worte mich befangen:", "tokens": ["Es", "w\u00fcr\u00b7den", "dei\u00b7ne", "Wor\u00b7te", "mich", "be\u00b7fan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vom Sch\u00fctzen wird ein Vogel rasch umgangen,", "tokens": ["Vom", "Sch\u00fct\u00b7zen", "wird", "ein", "Vo\u00b7gel", "rasch", "um\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn mehr er will als an der Kirsche picken.", "tokens": ["Wenn", "mehr", "er", "will", "als", "an", "der", "Kir\u00b7sche", "pi\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VMFIN", "KOKOM", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wohl m\u00f6gen Reize, die so ganz dein eigen,", "tokens": ["Wohl", "m\u00f6\u00b7gen", "Rei\u00b7ze", ",", "die", "so", "ganz", "dein", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "PRELS", "ADV", "ADV", "PPOSAT", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Wunsch der Sehnsucht in den Andern wecken,", "tokens": ["Den", "Wunsch", "der", "Sehn\u00b7sucht", "in", "den", "An\u00b7dern", "we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sich dir zu nah'n und dir ein Herz zu zeigen.", "tokens": ["Sich", "dir", "zu", "nah'n", "und", "dir", "ein", "Herz", "zu", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPER", "APPR", "NN", "KON", "PPER", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich werde nur, wenn Jene sich entdecken,", "tokens": ["Ich", "wer\u00b7de", "nur", ",", "wenn", "Je\u00b7ne", "sich", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "KOUS", "PDS", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vor deiner Sch\u00f6nheit huldigend mich neigen,", "tokens": ["Vor", "dei\u00b7ner", "Sch\u00f6n\u00b7heit", "hul\u00b7di\u00b7gend", "mich", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "PPER", "VVFIN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Nicht eine Sylbe soll dein Ohr erschrecken!", "tokens": ["Nicht", "ei\u00b7ne", "Syl\u00b7be", "soll", "dein", "Ohr", "er\u00b7schre\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}