{"textgrid.poem.39332": {"metadata": {"author": {"name": "Lavater, Johann Kaspar", "birth": "N.A.", "death": "N.A."}, "title": "11. F\u00fcrbitte f\u00fcr ein fehlbares Kind", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach! tiefgebeugt erscheinen", "tokens": ["Ach", "!", "tief\u00b7ge\u00b7beugt", "er\u00b7schei\u00b7nen"], "token_info": ["word", "punct", "word", "word"], "pos": ["ITJ", "$.", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wir, Herr, vor dir und weynen", "tokens": ["Wir", ",", "Herr", ",", "vor", "dir", "und", "wey\u00b7nen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "NN", "$,", "APPR", "PPER", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Empor zu deiner Vater-Huld:", "tokens": ["Em\u00b7por", "zu", "dei\u00b7ner", "Va\u00b7ter\u00b7Huld", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Trag mit dem Fehlenden Geduld!", "tokens": ["Trag", "mit", "dem", "Feh\u00b7len\u00b7den", "Ge\u00b7duld", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Gieb, Vater, seine S\u00fcnden", "tokens": ["Gieb", ",", "Va\u00b7ter", ",", "sei\u00b7ne", "S\u00fcn\u00b7den"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["VVIMP", "$,", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihm lebhaft zu empfinden!", "tokens": ["Ihm", "leb\u00b7haft", "zu", "emp\u00b7fin\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gieb seinem Herzen Schaam und Reu", "tokens": ["Gieb", "sei\u00b7nem", "Her\u00b7zen", "Schaam", "und", "Reu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "NN", "KON", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Abscheu vor der Heucheley!", "tokens": ["Und", "Ab\u00b7scheu", "vor", "der", "Heu\u00b7che\u00b7ley", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sprich laut in dem Gewissen!", "tokens": ["Sprich", "laut", "in", "dem", "Ge\u00b7wis\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "La\u00df reine Thr\u00e4nen flie\u00dfen,", "tokens": ["La\u00df", "rei\u00b7ne", "Thr\u00e4\u00b7nen", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und schreckt ihn, Richter, dein Gericht,", "tokens": ["Und", "schreckt", "ihn", ",", "Rich\u00b7ter", ",", "dein", "Ge\u00b7richt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So schenk' ihm wieder Zuversicht!", "tokens": ["So", "schenk'", "ihm", "wie\u00b7der", "Zu\u00b7ver\u00b7sicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und hast du ihm ve