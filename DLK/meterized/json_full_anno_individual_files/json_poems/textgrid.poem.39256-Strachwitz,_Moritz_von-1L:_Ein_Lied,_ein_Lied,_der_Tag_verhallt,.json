{"textgrid.poem.39256": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Lied, ein Lied, der Tag verhallt,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Lied, ein Lied, der Tag verhallt,", "tokens": ["Ein", "Lied", ",", "ein", "Lied", ",", "der", "Tag", "ver\u00b7hallt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die W\u00e4lder atmen sacht,", "tokens": ["Die", "W\u00e4l\u00b7der", "at\u00b7men", "sacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und \u00fcber die Tale wogt und wallt", "tokens": ["Und", "\u00fc\u00b7ber", "die", "Ta\u00b7le", "wogt", "und", "wallt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das Ambrahaar der Nacht.", "tokens": ["Das", "Am\u00b7bra\u00b7haar", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Erde wie tief, und um mich her", "tokens": ["Die", "Er\u00b7de", "wie", "tief", ",", "und", "um", "mich", "her"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "KOKOM", "ADJD", "$,", "KON", "APPR", "PRF", "APZR"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Eine F\u00fclle edler Gestalten,", "tokens": ["Ei\u00b7ne", "F\u00fcl\u00b7le", "ed\u00b7ler", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Tief in der Brust ein flutend Meer", "tokens": ["Tief", "in", "der", "Brust", "ein", "flu\u00b7tend", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "ADJD", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Vollt\u00f6nender Gewalten;", "tokens": ["Voll\u00b7t\u00f6\u00b7nen\u00b7der", "Ge\u00b7wal\u00b7ten", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Und hast du lange versteckt gewohnt,", "tokens": ["Und", "hast", "du", "lan\u00b7ge", "ver\u00b7steckt", "ge\u00b7wohnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "O, freudige Kraft der Lieder,", "tokens": ["O", ",", "freu\u00b7di\u00b7ge", "Kraft", "der", "Lie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "So sch\u00fcttle heut' in den silbernen Mond", "tokens": ["So", "sch\u00fctt\u00b7le", "heut'", "in", "den", "sil\u00b7ber\u00b7nen", "Mond"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Dein silbernes Gefieder! \u2013", "tokens": ["Dein", "sil\u00b7ber\u00b7nes", "Ge\u00b7fie\u00b7der", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Herr Walter war ein Ritter jung.", "tokens": ["Herr", "Wal\u00b7ter", "war", "ein", "Rit\u00b7ter", "jung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er hatte lang gestritten,", "tokens": ["Er", "hat\u00b7te", "lang", "ge\u00b7strit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bis ihm ein scharfer Schwertesschwung", "tokens": ["Bis", "ihm", "ein", "schar\u00b7fer", "Schwer\u00b7tes\u00b7schwung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ins freudige Herz geschnitten.", "tokens": ["Ins", "freu\u00b7di\u00b7ge", "Herz", "ge\u00b7schnit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Herr Walter glitt in den blutigen Sand,", "tokens": ["Herr", "Wal\u00b7ter", "glitt", "in", "den", "blu\u00b7ti\u00b7gen", "Sand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sein Hengst stob in die Winde,", "tokens": ["Sein", "Hengst", "stob", "in", "die", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie trugen ihn aus dem Sonnenbrand", "tokens": ["Sie", "tru\u00b7gen", "ihn", "aus", "dem", "Son\u00b7nen\u00b7brand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Unter die breite Linde.", "tokens": ["Un\u00b7ter", "die", "brei\u00b7te", "Lin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NE", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Sie rissen entzwei den Fahnensaum,", "tokens": ["Sie", "ris\u00b7sen", "ent\u00b7zwei", "den", "Fah\u00b7nen\u00b7saum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zu stillen das Blut dem Degen;", "tokens": ["Zu", "stil\u00b7len", "das", "Blut", "dem", "De\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Auf den Sterbenden vom Lindenbaum", "tokens": ["Auf", "den", "Ster\u00b7ben\u00b7den", "vom", "Lin\u00b7den\u00b7baum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Fiel reicher Bl\u00fctenregen.", "tokens": ["Fiel", "rei\u00b7cher", "Bl\u00fc\u00b7ten\u00b7re\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Das war des K\u00f6nigs T\u00f6chterlein,", "tokens": ["Das", "war", "des", "K\u00f6\u00b7nigs", "T\u00f6ch\u00b7ter\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Aug' in Tr\u00e4nen gl\u00fchte,", "tokens": ["Ihr", "Aug'", "in", "Tr\u00e4\u00b7nen", "gl\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie hielt ihm einen Becher Wein", "tokens": ["Sie", "hielt", "ihm", "ei\u00b7nen", "Be\u00b7cher", "Wein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An des Mundes welkende Bl\u00fcte.", "tokens": ["An", "des", "Mun\u00b7des", "wel\u00b7ken\u00b7de", "Bl\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Das war des K\u00f6nigs T\u00f6chterlein,", "tokens": ["Das", "war", "des", "K\u00f6\u00b7nigs", "T\u00f6ch\u00b7ter\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie kniete zu ihm nieder,", "tokens": ["Sie", "knie\u00b7te", "zu", "ihm", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da drang ein schneller Rosenschein", "tokens": ["Da", "drang", "ein", "schnel\u00b7ler", "Ro\u00b7sen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch die sinkenden Augenlider.", "tokens": ["Durch", "die", "sin\u00b7ken\u00b7den", "Au\u00b7gen\u00b7li\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Es ging ein Schauer durch sein Mark,", "tokens": ["Es", "ging", "ein", "Schau\u00b7er", "durch", "sein", "Mark", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Schauer j\u00e4her Wonne,", "tokens": ["Ein", "Schau\u00b7er", "j\u00e4\u00b7her", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er sah sie an, so voll und stark,", "tokens": ["Er", "sah", "sie", "an", ",", "so", "voll", "und", "stark", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie der sterbende Aar die Sonne.", "tokens": ["Wie", "der", "ster\u00b7ben\u00b7de", "Aar", "die", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Die Binden ri\u00df er, die er trug:", "tokens": ["Die", "Bin\u00b7den", "ri\u00df", "er", ",", "die", "er", "trug", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbnun rinne, mein Blut, o rinne!\u00ab", "tokens": ["\u00bb", "nun", "rin\u00b7ne", ",", "mein", "Blut", ",", "o", "rin\u00b7ne", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VVFIN", "$,", "PPOSAT", "NN", "$,", "FM", "VVFIN", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Er trank den Becher auf ", "tokens": ["Er", "trank", "den", "Be\u00b7cher", "auf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "\u00bbnun gr\u00fc\u00dfe dich Gott, Frau Minne!\u00ab", "tokens": ["\u00bb", "nun", "gr\u00fc\u00b7\u00dfe", "dich", "Gott", ",", "Frau", "Min\u00b7ne", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "NN", "$,", "NN", "NN", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "In der Nacht, in der seligen Sommernacht,", "tokens": ["In", "der", "Nacht", ",", "in", "der", "se\u00b7li\u00b7gen", "Som\u00b7mer\u00b7nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Wo niemand traurig bliebe,", "tokens": ["Wo", "nie\u00b7mand", "trau\u00b7rig", "blie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da hab' ich Euch dennoch ein Lied gebracht,", "tokens": ["Da", "hab'", "ich", "Euch", "den\u00b7noch", "ein", "Lied", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Lied von blutender Liebe.", "tokens": ["Ein", "Lied", "von", "blu\u00b7ten\u00b7der", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Verzeiht, es ist das alte Lied", "tokens": ["Ver\u00b7zeiht", ",", "es", "ist", "das", "al\u00b7te", "Lied"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von Seligkeit und Verderben.", "tokens": ["Von", "Se\u00b7lig\u00b7keit", "und", "Ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Wenn der Dichter dem Himmel ins Auge sieht,", "tokens": ["Wenn", "der", "Dich\u00b7ter", "dem", "Him\u00b7mel", "ins", "Au\u00b7ge", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Dann mu\u00df er jubelnd sterben.", "tokens": ["Dann", "mu\u00df", "er", "ju\u00b7belnd", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Der Himmel ist fern und hoch und hehr,", "tokens": ["Der", "Him\u00b7mel", "ist", "fern", "und", "hoch", "und", "hehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Nun rinne, mein Blut, o rinne!", "tokens": ["Nun", "rin\u00b7ne", ",", "mein", "Blut", ",", "o", "rin\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PPOSAT", "NN", "$,", "FM", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Die Wunden brechen, der Becher ist leer,", "tokens": ["Die", "Wun\u00b7den", "bre\u00b7chen", ",", "der", "Be\u00b7cher", "ist", "leer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Nun gr\u00fc\u00dfe dich Gott, Frau Minne!", "tokens": ["Nun", "gr\u00fc\u00b7\u00dfe", "dich", "Gott", ",", "Frau", "Min\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "NN", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "Ein Lied, ein Lied, der Tag verhallt,", "tokens": ["Ein", "Lied", ",", "ein", "Lied", ",", "der", "Tag", "ver\u00b7hallt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die W\u00e4lder atmen sacht,", "tokens": ["Die", "W\u00e4l\u00b7der", "at\u00b7men", "sacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und \u00fcber die Tale wogt und wallt", "tokens": ["Und", "\u00fc\u00b7ber", "die", "Ta\u00b7le", "wogt", "und", "wallt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON",