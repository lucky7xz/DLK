{"textgrid.poem.58670": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "102.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein d\u00fcrrer Berg wird meine Seel', wenn Gott abwendet sein Gesichte,", "tokens": ["Ein", "d\u00fcr\u00b7rer", "Berg", "wird", "mei\u00b7ne", "Seel'", ",", "wenn", "Gott", "ab\u00b7wen\u00b7det", "sein", "Ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$,", "KOUS", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.2": {"text": "Wenn er sie wieder siehet an, bald gr\u00fcnet sie und tr\u00e4get Fr\u00fcchte;", "tokens": ["Wenn", "er", "sie", "wie\u00b7der", "sie\u00b7het", "an", ",", "bald", "gr\u00fc\u00b7net", "sie", "und", "tr\u00e4\u00b7get", "Fr\u00fcch\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVFIN", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "KON", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.3": {"text": "Die D\u00fcrre kommt von meiner Schuld,", "tokens": ["Die", "D\u00fcr\u00b7re", "kommt", "von", "mei\u00b7ner", "Schuld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Saft und Kraft von Gottes Huld.", "tokens": ["Der", "Saft", "und", "Kraft", "von", "Got\u00b7tes", "Huld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "APPR", 