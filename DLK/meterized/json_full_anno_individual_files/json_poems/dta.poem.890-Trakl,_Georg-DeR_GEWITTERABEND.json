{"dta.poem.890": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "DeR GEWITTERABEND", "genre": "Lyrik", "period": "N.A.", "pub_year": "1913", "urn": "urn:nbn:de:kobv:b4-30357-9", "language": ["de:0.99"], "booktitle": "Trakl, Georg: Gedichte. Leipzig, 1913."}, "poem": {"stanza.1": {"line.1": {"text": "O die roten Abendstunden!", "tokens": ["O", "die", "ro\u00b7ten", "A\u00b7bends\u00b7tun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Flimmernd schwankt am offenen Fenster", "tokens": ["Flim\u00b7mernd", "schwankt", "am", "of\u00b7fe\u00b7nen", "Fens\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Weinlaub wirr ins Blau gewunden,", "tokens": ["Wein\u00b7laub", "wirr", "ins", "Blau", "ge\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "NN", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Drinnen nisten Angstgespenster.", "tokens": ["Drin\u00b7nen", "nis\u00b7ten", "Angst\u00b7ge\u00b7spens\u00b7ter", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Staub tanzt im Gestank der Gossen.", "tokens": ["Staub", "tanzt", "im", "Ge\u00b7stank", "der", "Gos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Klirrend st\u00f6\u00dft der Wind in Scheiben.", "tokens": ["Klir\u00b7rend", "st\u00f6\u00dft", "der", "Wind", "in", "Schei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Einen Zug von wilden Rossen", "tokens": ["Ei\u00b7nen", "Zug", "von", "wil\u00b7den", "Ros\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Blitze grelle Wolken treiben.", "tokens": ["Blit\u00b7ze", "grel\u00b7le", "Wol\u00b7ken", "trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Laut zerspringt der Weiherspiegel.", "tokens": ["Laut", "zer\u00b7springt", "der", "Wei\u00b7her\u00b7spie\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00f6ven schrein am Fensterrahmen.", "tokens": ["M\u00f6\u00b7ven", "schrein", "am", "Fens\u00b7ter\u00b7rah\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Feuerreiter sprengt vom H\u00fcgel", "tokens": ["Feu\u00b7er\u00b7rei\u00b7ter", "sprengt", "vom", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und zerschellt im Tann zu Flammen.", "tokens": ["Und", "zer\u00b7schellt", "im", "Tann", "zu", "Flam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Kranke kreischen im Spitale.", "tokens": ["Kran\u00b7ke", "krei\u00b7schen", "im", "Spi\u00b7ta\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bl\u00e4ulich schwirrt der Nacht Gefieder.", "tokens": ["Bl\u00e4u\u00b7lich", "schwirrt", "der", "Nacht", "Ge\u00b7fie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Glitzernd braust mit einem Male", "tokens": ["Glit\u00b7zernd", "braust", "mit", "ei\u00b7nem", "Ma\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Regen auf die D\u00e4cher nieder.", "tokens": ["Re\u00b7gen", "auf", "die", "D\u00e4\u00b7cher", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}