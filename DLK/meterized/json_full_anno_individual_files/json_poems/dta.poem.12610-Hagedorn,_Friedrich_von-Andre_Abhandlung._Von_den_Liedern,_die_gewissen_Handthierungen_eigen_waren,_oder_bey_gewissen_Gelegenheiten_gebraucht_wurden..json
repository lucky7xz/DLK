{"dta.poem.12610": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Andre Abhandlung.  \n Von den Liedern, die gewissen Handthierungen eigen waren,  \n oder bey gewissen Gelegenheiten gebraucht wurden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905199128", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die du Korn und Aehren mehrest,", "tokens": ["Die", "du", "Korn", "und", "A\u00b7eh\u00b7ren", "meh\u00b7rest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ceres, la\u00df doch diese Erndte", "tokens": ["Ce\u00b7res", ",", "la\u00df", "doch", "die\u00b7se", "Ernd\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVIMP", "ADV", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ja recht reich und fruchtbar seyn.", "tokens": ["Ja", "recht", "reich", "und", "frucht\u00b7bar", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADJD", "ADJD", "KON", "ADJD", "VAINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "H\u00f6rt, die ihr die Garben sammelt,", "tokens": ["H\u00f6rt", ",", "die", "ihr", "die", "Gar\u00b7ben", "sam\u00b7melt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bindet ja dieselben gut,", "tokens": ["Bin\u00b7det", "ja", "die\u00b7sel\u00b7ben", "gut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PDAT", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df der, so vor\u00fcber gehet,", "tokens": ["Da\u00df", "der", ",", "so", "vor\u00b7\u00fc\u00b7ber", "ge\u00b7het", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "$,", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und euch sieht, nicht sagen m\u00f6ge:", "tokens": ["Und", "euch", "sieht", ",", "nicht", "sa\u00b7gen", "m\u00f6\u00b7ge", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Liederliche Tagel\u00f6hner!", "tokens": ["Lie\u00b7der\u00b7li\u00b7che", "Ta\u00b7ge\u00b7l\u00f6h\u00b7ner", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Das hei\u00dft, Lohn umsonst gegeben.", "tokens": ["Das", "hei\u00dft", ",", "Lohn", "um\u00b7sonst", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "NN", "ADV", "VVPP", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Stellet eurer Garben Spitze", "tokens": ["Stel\u00b7let", "eu\u00b7rer", "Gar\u00b7ben", "Spit\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gegen Norden oder Westen;", "tokens": ["Ge\u00b7gen", "Nor\u00b7den", "o\u00b7der", "Wes\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hiedurch schwellt das Korn am besten.", "tokens": ["Hie\u00b7durch", "schwellt", "das", "Korn", "am", "bes\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "PTKA", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Jhr, die ihr dreschet, schlafet nie,", "tokens": ["Ihr", ",", "die", "ihr", "dre\u00b7schet", ",", "schla\u00b7fet", "nie", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn euch der Mittag brennt,", "tokens": ["Wenn", "euch", "der", "Mit\u00b7tag", "brennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weil ihr alsdenn mit leichter M\u00fch", "tokens": ["Weil", "ihr", "als\u00b7denn", "mit", "leich\u00b7ter", "M\u00fch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Korn von seinen H\u00fclsen trennt.", "tokens": ["Das", "Korn", "von", "sei\u00b7nen", "H\u00fcl\u00b7sen", "trennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "La\u00dft euch ja im Felde sehen,", "tokens": ["La\u00dft", "euch", "ja", "im", "Fel\u00b7de", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schnitter, wenn die Lerch\u2019 erwacht.", "tokens": ["Schnit\u00b7ter", ",", "wenn", "die", "Ler\u00b7ch'", "er\u00b7wacht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "VVPP", "$."], "meter": "+---+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Mit ihr m\u00fc\u00dft ihr schlafen gehen;", "tokens": ["Mit", "ihr", "m\u00fc\u00dft", "ihr", "schla\u00b7fen", "ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "PPER", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Mittags-Hitze Macht", "tokens": ["Und", "der", "Mit\u00b7tags\u00b7Hit\u00b7ze", "Macht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Unempfindlich \u00fcberstehen.", "tokens": ["Un\u00b7emp\u00b7find\u00b7lich", "\u00fc\u00b7bers\u00b7te\u00b7hen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Jhr Kinder, die Bequemlichkeit,", "tokens": ["Ihr", "Kin\u00b7der", ",", "die", "Be\u00b7quem\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Ruh, die jenen Frosch erfreut,", "tokens": ["Die", "Ruh", ",", "die", "je\u00b7nen", "Frosch", "er\u00b7freut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verdienet unsern Wunsch und Neid.", "tokens": ["Ver\u00b7die\u00b7net", "un\u00b7sern", "Wunsch", "und", "Neid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jhm fehlet kein verlangter Trunk,", "tokens": ["Jhm", "feh\u00b7let", "kein", "ver\u00b7lang\u00b7ter", "Trunk", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er suchet keinen, der ihm schenket,", "tokens": ["Er", "su\u00b7chet", "kei\u00b7nen", ",", "der", "ihm", "schen\u00b7ket", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Er trinket, durch sich selbst getr\u00e4nket,", "tokens": ["Er", "trin\u00b7ket", ",", "durch", "sich", "selbst", "ge\u00b7tr\u00e4n\u00b7ket", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und hat zu trinken gnung.", "tokens": ["Und", "hat", "zu", "trin\u00b7ken", "gnung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKZU", "VVINF", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "So! karger Filz, nichts steht dir sch\u00f6ner,", "tokens": ["So", "!", "kar\u00b7ger", "Filz", ",", "nichts", "steht", "dir", "sch\u00f6\u00b7ner", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ADJA", "NN", "$,", "PIS", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als da\u00df du deine Tagel\u00f6hner", "tokens": ["Als", "da\u00df", "du", "dei\u00b7ne", "Ta\u00b7ge\u00b7l\u00f6h\u00b7ner"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit schlechten Linsen weidst.", "tokens": ["Mit", "schlech\u00b7ten", "Lin\u00b7sen", "weidst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Verwunde dir nur nicht die H\u00e4nde,", "tokens": ["Ver\u00b7wun\u00b7de", "dir", "nur", "nicht", "die", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn du einmal zu diesem Ende", "tokens": ["Wenn", "du", "ein\u00b7mal", "zu", "die\u00b7sem", "En\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein K\u00fcmmel-Korn zerschneidst.", "tokens": ["Ein", "K\u00fcm\u00b7mel\u00b7Korn", "zer\u00b7schneidst", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}