{"textgrid.poem.31576": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du Dreygeeinter Gott / du Selbstheit alles guten /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Dreygeeinter Gott / du Selbstheit alles guten /", "tokens": ["Du", "Drey\u00b7ge\u00b7e\u00b7in\u00b7ter", "Gott", "/", "du", "Selbst\u00b7heit", "al\u00b7les", "gu\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$(", "PPER", "NN", "PIAT", "ADJA", "$("], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "du Allerforschende Haubt-Vnerforschlichkeit /", "tokens": ["du", "Al\u00b7ler\u00b7for\u00b7schen\u00b7de", "Haub\u00b7t\u00b7V\u00b7ner\u00b7forschlich\u00b7keit", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$("], "meter": "-+-+--++--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "gewest und bleibender vor-in-und nach der Zeit!", "tokens": ["ge\u00b7west", "und", "blei\u00b7ben\u00b7der", "vor\u00b7in\u00b7\u00b7und", "nach", "der", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich schreib itzund von dir / durch deines Geists Anmuten.", "tokens": ["Ich", "schreib", "it\u00b7zund", "von", "dir", "/", "durch", "dei\u00b7nes", "Geists", "An\u00b7mu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "$(", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"tex