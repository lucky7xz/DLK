{"textgrid.poem.48091": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Frieden", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich m\u00f6chte still durch einen Tannenwald", "tokens": ["Ich", "m\u00f6ch\u00b7te", "still", "durch", "ei\u00b7nen", "Tan\u00b7nen\u00b7wald"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "mit dir im roten Abendfrieden schreiten,", "tokens": ["mit", "dir", "im", "ro\u00b7ten", "A\u00b7bend\u00b7frie\u00b7den", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "wenn ganz von fern das Avel\u00e4uten hallt", "tokens": ["wenn", "ganz", "von", "fern", "das", "A\u00b7ve\u00b7l\u00e4u\u00b7ten", "hallt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "ADJD", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "und lichtges\u00e4ttigt sich die Zweige breiten.", "tokens": ["und", "licht\u00b7ge\u00b7s\u00e4t\u00b7tigt", "sich", "die", "Zwei\u00b7ge", "brei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dann legtest du die Hand auf meine Brust", "tokens": ["Dann", "leg\u00b7test", "du", "die", "Hand", "auf", "mei\u00b7ne", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und f\u00fchltest, wie die hei\u00dfen roten Wellen", "tokens": ["und", "f\u00fchl\u00b7test", ",", "wie", "die", "hei\u00b7\u00dfen", "ro\u00b7ten", "Wel\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "beruhigt gleiten und in sanfter Lust", "tokens": ["be\u00b7ru\u00b7higt", "glei\u00b7ten", "und", "in", "sanf\u00b7ter", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVINF", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "nur unterm Drucke deiner Finger schwellen.", "tokens": ["nur", "un\u00b7term", "Dru\u00b7cke", "dei\u00b7ner", "Fin\u00b7ger", "schwel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich m\u00f6chte still durch einen Tannenwald", "tokens": ["Ich", "m\u00f6ch\u00b7te", "still", "durch", "ei\u00b7nen", "Tan\u00b7nen\u00b7wald"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "mit dir im roten Abendfrieden schreiten,", "tokens": ["mit", "dir", "im", "ro\u00b7ten", "A\u00b7bend\u00b7frie\u00b7den", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "wenn ganz von fern das Avel\u00e4uten hallt", "tokens": ["wenn", "ganz", "von", "fern", "das", "A\u00b7ve\u00b7l\u00e4u\u00b7ten", "ha