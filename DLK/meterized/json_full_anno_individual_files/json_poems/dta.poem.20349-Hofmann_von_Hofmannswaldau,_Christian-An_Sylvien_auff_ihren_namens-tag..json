{"dta.poem.20349": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "An Sylvien auff ihren namens-tag.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dein edler namens-tag zeigt heute seinen schein;", "tokens": ["Dein", "ed\u00b7ler", "na\u00b7mens\u00b7tag", "zeigt", "heu\u00b7te", "sei\u00b7nen", "schein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "PPOSAT", "ADJD", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mir aber meine pflicht/ dich w\u00fcrdig anzubinden:", "tokens": ["Mir", "a\u00b7ber", "mei\u00b7ne", "pflicht", "/", "dich", "w\u00fcr\u00b7dig", "an\u00b7zu\u00b7bin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PPOSAT", "NN", "$(", "PPER", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Allein wo soll ich was bey meiner armuth finden/", "tokens": ["Al\u00b7lein", "wo", "soll", "ich", "was", "bey", "mei\u00b7ner", "ar\u00b7muth", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "VMFIN", "PPER", "PIS", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil federn und pappier f\u00fcr dich zu wenig seyn?", "tokens": ["Weil", "fe\u00b7dern", "und", "pap\u00b7pier", "f\u00fcr", "dich", "zu", "we\u00b7nig", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVINF", "KON", "NN", "APPR", "PPER", "PTKA", "PIS", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mein wollen ist zwar gro\u00df/ doch mein verm\u00f6gen klein/", "tokens": ["Mein", "wol\u00b7len", "ist", "zwar", "gro\u00df", "/", "doch", "mein", "ver\u00b7m\u00f6\u00b7gen", "klein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VMFIN", "VAFIN", "ADV", "ADJD", "$(", "ADV", "PPOSAT", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Drum la\u00df mich/ was ich kan/ dir zum geschencke winden:", "tokens": ["Drum", "la\u00df", "mich", "/", "was", "ich", "kan", "/", "dir", "zum", "ge\u00b7schen\u00b7cke", "win\u00b7den", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$(", "PWS", "PPER", "VMFIN", "$(", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Denn wer die liebe will auff gold und reimen gr\u00fcnden/", "tokens": ["Denn", "wer", "die", "lie\u00b7be", "will", "auff", "gold", "und", "rei\u00b7men", "gr\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "ADJA", "VMFIN", "APPR", "NN", "KON", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Schleust nur viel prahlerey und wenig freundschafft ein.", "tokens": ["Schleust", "nur", "viel", "prah\u00b7le\u00b7rey", "und", "we\u00b7nig", "freund\u00b7schafft", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PIAT", "NN", "KON", "PIS", "VVFIN", "PTKVZ", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.9": {"text": "Ich biete dir mein hertz zum opffer selber an/", "tokens": ["Ich", "bie\u00b7te", "dir", "mein", "hertz", "zum", "opf\u00b7fer", "sel\u00b7ber", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "APPRART", "NN", "ADV", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mein hertze/ da\u00df dich zwar nicht sch\u00f6nste binden kan;", "tokens": ["Mein", "hert\u00b7ze", "/", "da\u00df", "dich", "zwar", "nicht", "sch\u00f6ns\u00b7te", "bin\u00b7den", "kan", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "KOUS", "PPER", "ADV", "PTKNEG", "ADJA", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Weil du es schon vorl\u00e4ngst mit ketten angebunden/", "tokens": ["Weil", "du", "es", "schon", "vor\u00b7l\u00e4ngst", "mit", "ket\u00b7ten", "an\u00b7ge\u00b7bun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADV", "APPR", "ADJA", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Legst du es aber nur zu deinen f\u00fcssen hin/", "tokens": ["Legst", "du", "es", "a\u00b7ber", "nur", "zu", "dei\u00b7nen", "f\u00fcs\u00b7sen", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So glaube/ da\u00df ich auch in ketten lustig bin;", "tokens": ["So", "glau\u00b7be", "/", "da\u00df", "ich", "auch", "in", "ket\u00b7ten", "lus\u00b7tig", "bin", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "KOUS", "PPER", "ADV", "APPR", "VVFIN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Weil es nach so viel angst doch einen ruh-platz funden.", "tokens": ["Weil", "es", "nach", "so", "viel", "angst", "doch", "ei\u00b7nen", "ruh\u00b7platz", "fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADV", "ADV", "VVFIN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}}}}}