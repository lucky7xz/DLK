{"dta.poem.3916": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "\u00a7. 12.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Ist erstlich Hochzeit da, so wird dem keuschen Liebe\u0303", "tokens": ["Ist", "erst\u00b7lich", "Hoch\u00b7zeit", "da", ",", "so", "wird", "dem", "keu\u00b7schen", "Lie\u00b7b\u1ebd"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "NN", "PTKVZ", "$,", "ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kein Ordnung, kein Gesetz zur Haltung vorge-", "tokens": ["Kein", "Ord\u00b7nung", ",", "kein", "Ge\u00b7setz", "zur", "Hal\u00b7tung", "vor\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "APPRART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "schrieben.", "tokens": ["schrie\u00b7ben", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Wenn nur das neue Paar einander liebt und ehrt,", "tokens": ["Wenn", "nur", "das", "neu\u00b7e", "Paar", "ein\u00b7an\u00b7der", "liebt", "und", "ehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "PRF", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und der Natur gem\u00e4\u00df sich in der Welt vermehrt.", "tokens": ["Und", "der", "Na\u00b7tur", "ge\u00b7m\u00e4\u00df", "sich", "in", "der", "Welt", "ver\u00b7mehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "PRF", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}