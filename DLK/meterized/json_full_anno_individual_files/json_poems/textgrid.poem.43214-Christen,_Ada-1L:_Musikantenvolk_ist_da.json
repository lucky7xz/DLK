{"textgrid.poem.43214": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Musikantenvolk ist da", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Musikantenvolk ist da", "tokens": ["Mu\u00b7si\u00b7kan\u00b7ten\u00b7volk", "ist", "da"], "token_info": ["word", "word", "word"], "pos": ["NN", "VAFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit der Harf' und Fiedel,", "tokens": ["Mit", "der", "Ha\u00b7rf'", "und", "Fie\u00b7del", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "$,"], "meter": "--++-+-", "measure": "anapaest.init"}, "line.3": {"text": "Und das kleine M\u00e4del singt", "tokens": ["Und", "das", "klei\u00b7ne", "M\u00e4\u00b7del", "singt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00fcstelnd noch ein Liedel.", "tokens": ["H\u00fcs\u00b7telnd", "noch", "ein", "Lie\u00b7del", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Kamen weit vom S\u00fcden her,", "tokens": ["Ka\u00b7men", "weit", "vom", "S\u00fc\u00b7den", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Eine ganze Bande,", "tokens": ["Ei\u00b7ne", "gan\u00b7ze", "Ban\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Starben alle, bis auf drei,", "tokens": ["Star\u00b7ben", "al\u00b7le", ",", "bis", "auf", "drei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "KOUS", "APPR", "CARD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "In dem kalten Lande...", "tokens": ["In", "dem", "kal\u00b7ten", "Lan\u00b7de", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Spielen in der Schenke auf", "tokens": ["Spie\u00b7len", "in", "der", "Schen\u00b7ke", "auf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Heut' vor gro\u00dfen Herren,", "tokens": ["Heut'", "vor", "gro\u00b7\u00dfen", "Her\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "Die vom Musikantenvolk", "tokens": ["Die", "vom", "Mu\u00b7si\u00b7kan\u00b7ten\u00b7volk"], "token_info": ["word", "word", "word"], "pos": ["ART", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Lied um Lied begehren.", "tokens": ["Lied", "um", "Lied", "be\u00b7geh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.13": {"text": "Manchem Zecher naht das Kind,", "tokens": ["Man\u00b7chem", "Ze\u00b7cher", "naht", "das", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Der da l\u00e4rmt und kreischet,", "tokens": ["Der", "da", "l\u00e4rmt", "und", "krei\u00b7schet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.15": {"text": "Rauh giebt er den kargen Lohn,", "tokens": ["Rauh", "giebt", "er", "den", "kar\u00b7gen", "Lohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Den es sch\u00fcchtern heischet.", "tokens": ["Den", "es", "sch\u00fcch\u00b7tern", "hei\u00b7schet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.17": {"text": "Und im Winkel sitzt es nun,", "tokens": ["Und", "im", "Win\u00b7kel", "sitzt", "es", "nun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Ueberz\u00e4hlt die Gabe,", "tokens": ["Ue\u00b7ber\u00b7z\u00e4hlt", "die", "Ga\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.19": {"text": "Grollt und weint in sich hinein:", "tokens": ["Grollt", "und", "weint", "in", "sich", "hin\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "APPR", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "\u00bbl\u00e4g' auch ich im Grabe.\u00ab ...", "tokens": ["\u00bb", "l\u00e4g'", "auch", "ich", "im", "Gra\u00b7be", ".", "\u00ab", "..."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "VVFIN", "ADV", "PPER", "APPRART", "NN", "$.", "$(", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Musikantenvolk ist da", "tokens": ["Mu\u00b7si\u00b7kan\