{"dta.poem.1234": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Seestrand bey  Terracina.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Hier/ wo die wilde Flutt mit stoltzen Wellen spielet/", "tokens": ["Hier", "/", "wo", "die", "wil\u00b7de", "Flutt", "mit", "stolt\u00b7zen", "Wel\u00b7len", "spie\u00b7let", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "PWAV", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Eurus seinen Grimm am nassen Ufer k\u00fchlet/", "tokens": ["Und", "Eu\u00b7rus", "sei\u00b7nen", "Grimm", "am", "nas\u00b7sen", "U\u00b7fer", "k\u00fch\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "PPOSAT", "NE", "APPRART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo Einsamkeit ist Wirth und Gast ein Wandersmann/", "tokens": ["Wo", "Ein\u00b7sam\u00b7keit", "ist", "Wirth", "und", "Gast", "ein", "Wan\u00b7ders\u00b7mann", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VAFIN", "NN", "KON", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der voller Furcht betritt die Schrecken-reiche Bahn/", "tokens": ["Der", "vol\u00b7ler", "Furcht", "be\u00b7tritt", "die", "Schre\u00b7cken\u00b7rei\u00b7che", "Bahn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Schneidt seine treue Faust in Stein", "tokens": ["Schneidt", "sei\u00b7ne", "treu\u00b7e", "Faust", "in", "Stein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Nahmen meiner Liebsten ein.", "tokens": ["Den", "Nah\u00b7men", "mei\u00b7ner", "Liebs\u00b7ten", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es darff den Demant nicht der V\u00f6cke Blutt umschl\u00fcssen/", "tokens": ["Es", "darff", "den", "De\u00b7mant", "nicht", "der", "V\u00f6\u00b7cke", "Blutt", "um\u00b7schl\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "ART", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch scharff-ges\u00e4urter Wein den harten Fels begiessen/", "tokens": ["Noch", "scha\u00b7rff\u00b7ge\u00b7s\u00e4ur\u00b7ter", "Wein", "den", "har\u00b7ten", "Fels", "be\u00b7gies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Kein zugespizter Stahl/ kein Hammer schwer von Last/", "tokens": ["Kein", "zu\u00b7ge\u00b7spiz\u00b7ter", "Stahl", "/", "kein", "Ham\u00b7mer", "schwer", "von", "Last", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$(", "PIAT", "NN", "ADJD", "APPR", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kein Eisen Mulcibers wird in die Hand gefast/", "tokens": ["Kein", "Ei\u00b7sen", "Mul\u00b7ci\u00b7bers", "wird", "in", "die", "Hand", "ge\u00b7fast", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NE", "VAFIN", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.5": {"text": "Wo Amor einen Bau giebt an/", "tokens": ["Wo", "A\u00b7mor", "ei\u00b7nen", "Bau", "giebt", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ART", "NN", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Neid und Zeit besitzen kan.", "tokens": ["Der", "Neid", "und", "Zeit", "be\u00b7sit\u00b7zen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Getreuer Hertzen Blutt/ die Thr\u00e4nen reiner Seelen", "tokens": ["Ge\u00b7treu\u00b7er", "Hert\u00b7zen", "Blutt", "/", "die", "Thr\u00e4\u00b7nen", "rei\u00b7ner", "See\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NN", "ADJD", "$(", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sind m\u00e4chtig ieden Stein und Felsen auszuh\u00f6len.", "tokens": ["Sind", "m\u00e4ch\u00b7tig", "ie\u00b7den", "Stein", "und", "Fel\u00b7sen", "aus\u00b7zu\u00b7h\u00f6\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PIAT", "NN", "KON", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was dieser E\u00dfig-Safft/ di\u00df Scheide-Wasser nezt/", "tokens": ["Was", "die\u00b7ser", "E\u00b7\u00dfig\u00b7\u00b7S\u00b7afft", "/", "di\u00df", "Schei\u00b7de\u00b7Was\u00b7ser", "nezt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "$(", "PDS", "NN", "VVFIN", "$("], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wird durch Cupidens Pfeil/ als Mei\u00dfel/ ausge\u00e4zt:", "tokens": ["Wird", "durch", "Cu\u00b7pi\u00b7dens", "Pfeil", "/", "als", "Mei\u00b7\u00dfel", "/", "aus\u00b7ge\u00e4zt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "APPR", "NE", "NN", "$(", "KOUS", "NN", "$(", "VVPP", "$."], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Mit solchem schreibet meine Hand", "tokens": ["Mit", "sol\u00b7chem", "schrei\u00b7bet", "mei\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Di\u00df edle Zeichen an den Strand.", "tokens": ["Di\u00df", "ed\u00b7le", "Zei\u00b7chen", "an", "den", "Strand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du/ den der Reisen Lauff in diese Gegend f\u00fchret/", "tokens": ["Du", "/", "den", "der", "Rei\u00b7sen", "Lauff", "in", "die\u00b7se", "Ge\u00b7gend", "f\u00fch\u00b7ret", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "ART", "ART", "NN", "NN", "APPR", "PDAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verehre solche Schrifft/ wie deiner Pflicht geb\u00fchret/", "tokens": ["Ver\u00b7eh\u00b7re", "sol\u00b7che", "Schrifft", "/", "wie", "dei\u00b7ner", "Pflicht", "ge\u00b7b\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$(", "KOKOM", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und/ hast du anders was aus reinem Hertzen lieb/", "tokens": ["Und", "/", "hast", "du", "an\u00b7ders", "was", "aus", "rei\u00b7nem", "Hert\u00b7zen", "lieb", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "VAFIN", "PPER", "ADV", "PWS", "APPR", "ADJA", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So w\u00fcnsche/ da\u00df der Hand/ die diese Worte schrieb:", "tokens": ["So", "w\u00fcn\u00b7sche", "/", "da\u00df", "der", "Hand", "/", "die", "die\u00b7se", "Wor\u00b7te", "schrieb", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "KOUS", "ART", "NN", "$(", "ART", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Lisille m\u00f6ge linder seyn", "tokens": ["Li\u00b7sil\u00b7le", "m\u00f6\u00b7ge", "lin\u00b7der", "seyn"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VMFIN", "ADJD", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als dieser rauhe Felsen-Stein.", "tokens": ["Als", "die\u00b7ser", "rau\u00b7he", "Fel\u00b7sen\u00b7Stein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}