, "tokens": ["Den", "man", "mit", "Recht", "er\u00b7war\u00b7ten", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.30": {"line.1": {"text": "So geht es mit Tabak und Rum:", "tokens": ["So", "geht", "es", "mit", "Ta\u00b7bak", "und", "Rum", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "KON", "NE", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Erst bist du froh, dann f\u00e4llst du um.", "tokens": ["Erst", "bist", "du", "froh", ",", "dann", "f\u00e4llst", "du", "um", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.31": {"line.1": {"text": "Hier ruhn die Schl\u00e4fer sch\u00f6n vereint,", "tokens": ["Hier", "ruhn", "die", "Schl\u00e4\u00b7fer", "sch\u00f6n", "ver\u00b7eint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bis da\u00df die Morgensonne scheint.", "tokens": ["Bis", "da\u00df", "die", "Mor\u00b7gen\u00b7son\u00b7ne", "scheint", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.32": {"line.1": {"text": "Im Kopf ert\u00f6nt ein schmerzlich Summen.", "tokens": ["Im", "Kopf", "er\u00b7t\u00f6nt", "ein", "schmerz\u00b7lich", "Sum\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-", "me