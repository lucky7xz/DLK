{"dta.poem.3409": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Elegie .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193292", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ida, wie weh ist mir, wie tr&#252;be deinem Ge-", "tokens": ["I\u00b7da", ",", "wie", "weh", "ist", "mir", ",", "wie", "tr", "&#252;", "be", "dei\u00b7nem", "Ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "XML_entity", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "ADJD", "VAFIN", "PPER", "$,", "PWAV", "ADJD", "$(", "VVFIN", "PPOSAT", "TRUNC"], "meter": "+--+-+-+-+--", "measure": "iambic.penta.invert"}, "line.2": {"text": "treuen!", "tokens": ["treu\u00b7en", "!"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Sieben Tage flohn; aber ich sahe dich", "tokens": ["Sie\u00b7ben", "Ta\u00b7ge", "flohn", ";", "a\u00b7ber", "ich", "sa\u00b7he", "dich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "$.", "KON", "PPER", "VVFIN", "PPER"], "meter": "+-+-++--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "nicht.", "tokens": ["nicht", "."], "token_info": ["word", "punct"], "pos": ["PTKNEG", "$."], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Sieben Tage verschlichen. Ich habe sie einsam ver-", "tokens": ["Sie\u00b7ben", "Ta\u00b7ge", "ver\u00b7schli\u00b7chen", ".", "Ich", "ha\u00b7be", "sie", "ein\u00b7sam", "ver"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVINF", "$.", "PPER", "VAFIN", "PPER", "ADJD", "TRUNC"], "meter": "+-+--+--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "trauert,", "tokens": ["trau\u00b7ert", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Denkend und dichtend an dich, sehnend", "tokens": ["Den\u00b7kend", "und", "dich\u00b7tend", "an", "dich", ",", "seh\u00b7nend"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVPP", "KON", "ADJD", "APPR", "PPER", "$,", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "und durstend nach dir.", "tokens": ["und", "durs\u00b7tend", "nach", "dir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "APPR", "PPER", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Sieben noch dunklere N\u00e4chte verwacht' ich in z\u00e4rt-", "tokens": ["Sie\u00b7ben", "noch", "dunk\u00b7le\u00b7re", "N\u00e4ch\u00b7te", "ver\u00b7wacht'", "ich", "in", "z\u00e4r\u00b7t"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["CARD", "ADV", "ADJA", "NN", "VVFIN", "PPER", "APPR", "TRUNC"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.10": {"text": "lichem Kummer,", "tokens": ["li\u00b7chem", "Kum\u00b7mer", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Traurend, Traute, um dich, rastlos ver-", "tokens": ["Trau\u00b7rend", ",", "Trau\u00b7te", ",", "um", "dich", ",", "rast\u00b7los", "ver"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "$,", "NN", "$,", "KOUI", "PPER", "$,", "ADJD", "TRUNC"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.12": {"text": "schmachtend nach dir.", "tokens": ["schmach\u00b7tend", "nach", "dir", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.13": {"text": "Warum verbirgest du dich? Mir ist um die Seele", "tokens": ["Wa\u00b7rum", "ver\u00b7bir\u00b7gest", "du", "dich", "?", "Mir", "ist", "um", "die", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "$.", "PPER", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "so d\u00fcster,", "tokens": ["so", "d\u00fcs\u00b7ter", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "Und um die Brust so bang. Ida, verbirg", "tokens": ["Und", "um", "die", "Brust", "so", "bang", ".", "I\u00b7da", ",", "ver\u00b7birg"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "ADJD", "$.", "NE", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "dich mir nicht!", "tokens": ["dich", "mir", "nicht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PPER", "PTKNEG", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.17": {"text": "Siehe die Aue draussen. Ihr ist die freundliche", "tokens": ["Sie\u00b7he", "die", "Au\u00b7e", "draus\u00b7sen", ".", "Ihr", "ist", "die", "freund\u00b7li\u00b7che"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "VVINF", "$.", "PPER", "VAFIN", "ART", "ADJA"], "meter": "-+-+-+--+-+--", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Sonne", "tokens": ["Son\u00b7ne"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.19": {"text": "Untergegangen; ihr birgt Nebel den freund-", "tokens": ["Un\u00b7ter\u00b7ge\u00b7gan\u00b7gen", ";", "ihr", "birgt", "Ne\u00b7bel", "den", "freun\u00b7d"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "PPER", "VVFIN", "NN", "ART", "TRUNC"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.20": {"text": "lichen Strahl.", "tokens": ["li\u00b7chen", "Strahl", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "--+", "measure": "anapaest.init"}, "line.21": {"text": "Und nun schmachtet, nun trauert, nun klagt die", "tokens": ["Und", "nun", "schmach\u00b7tet", ",", "nun", "trau\u00b7ert", ",", "nun", "klagt", "die"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "$,", "ADV", "VVFIN", "$,", "ADV", "VVFIN", "ART"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.22": {"text": "Verlassne. So klag' ich,", "tokens": ["Ver\u00b7lass\u00b7ne", ".", "So", "klag'", "ich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.23": {"text": "Seit ich dein sonnig Gesicht, freundliche", "tokens": ["Seit", "ich", "dein", "son\u00b7nig", "Ge\u00b7sicht", ",", "freund\u00b7li\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJD", "NN", "$,", "ADJA"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Ida, nicht sah.", "tokens": ["I\u00b7da", ",", "nicht", "sah", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PTKNEG", "VVFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.25": {"text": "O, so strahle denn wieder hervor aus dem h\u00fcllenden", "tokens": ["O", ",", "so", "strah\u00b7le", "denn", "wie\u00b7der", "her\u00b7vor", "aus", "dem", "h\u00fcl\u00b7len\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADV", "VVFIN", "ADV", "ADV", "PTKVZ", "APPR", "ART", "ADJA"], "meter": "+-+--+---+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.26": {"text": "Dunkel,", "tokens": ["Dun\u00b7kel", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.27": {"text": "Morgenr\u00f6thliches Licht, kl\u00e4re die Seele", "tokens": ["Mor\u00b7gen\u00b7r\u00f6th\u00b7li\u00b7ches", "Licht", ",", "kl\u00e4\u00b7re", "die", "See\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "+-+--+---+-", "measure": "trochaic.tetra.relaxed"}, "line.28": {"text": "mir auf.", "tokens": ["mir", "auf", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.29": {"text": "Tritt hervor, o siegende Sonn', in schimmernder", "tokens": ["Tritt", "her\u00b7vor", ",", "o", "sie\u00b7gen\u00b7de", "Sonn'", ",", "in", "schim\u00b7mern\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$,", "FM", "ADJA", "NN", "$,", "APPR", "ADJA"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.30": {"text": "Sch\u00f6nheit,", "tokens": ["Sch\u00f6n\u00b7heit", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.31": {"text": "Blinke mir Leben und Kraft tief in die", "tokens": ["Blin\u00b7ke", "mir", "Le\u00b7ben", "und", "Kraft", "tief", "in", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "ADJD", "APPR", "ART"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.32": {"text": "Seele hinein.", "tokens": ["See\u00b7le", "hin\u00b7ein", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.33": {"text": "Komm du, die ich liebe, in deiner unschuldigen", "tokens": ["Komm", "du", ",", "die", "ich", "lie\u00b7be", ",", "in", "dei\u00b7ner", "un\u00b7schul\u00b7di\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PRELS", "PPER", "VVFIN", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.34": {"text": "Sch\u00f6ne,", "tokens": ["Sch\u00f6\u00b7ne", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.35": {"text": "Deiner r\u00fchrenden Huld, deinem gewinnen-", "tokens": ["Dei\u00b7ner", "r\u00fch\u00b7ren\u00b7den", "Huld", ",", "dei\u00b7nem", "ge\u00b7win\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "TRUNC"], "meter": "+-+--++--+-", "measure": "trochaic.penta.relaxed"}, "line.36": {"text": "den Reiz.", "tokens": ["den", "Reiz", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.37": {"text": "Komm, o Inniggeliebte, an meinen klopfenden", "tokens": ["Komm", ",", "o", "In\u00b7nig\u00b7ge\u00b7lieb\u00b7te", ",", "an", "mei\u00b7nen", "klop\u00b7fen\u00b7den"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "FM", "NN", "$,", "APPR", "PPOSAT", "ADJA"], "meter": "+-+--+--+-+--", "measure": "trochaic.penta.relaxed"}, "line.38": {"text": "Busen,", "tokens": ["Bu\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.39": {"text": "Stille das schlagende Herz, letze den lech-", "tokens": ["Stil\u00b7le", "das", "schla\u00b7gen\u00b7de", "Herz", ",", "let\u00b7ze", "den", "lech"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "VVFIN", "ART", "TRUNC"], "meter": "+--+--+---+", "measure": "dactylic.tri.plus"}, "line.40": {"text": "zenden Durst.", "tokens": ["zen\u00b7den", "Durst", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "--+", "measure": "anapaest.init"}, "line.41": {"text": "Meine Seele schmachtet nach dir, wie die welkende", "tokens": ["Mei\u00b7ne", "See\u00b7le", "schmach\u00b7tet", "nach", "dir", ",", "wie", "die", "wel\u00b7ken\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPER", "$,", "PWAV", "ART", "NN"], "meter": "+-+-+---+-+--", "measure": "unknown.measure.penta"}, "line.42": {"text": "Blume", "tokens": ["Blu\u00b7me"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.43": {"text": "In der Schw\u00fcle des Tags schmachtet nach", "tokens": ["In", "der", "Schw\u00fc\u00b7le", "des", "Tags", "schmach\u00b7tet", "nach"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "APPR"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.44": {"text": "Abend und Thau.", "tokens": ["A\u00b7bend", "und", "Thau", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.45": {"text": "Meine Seele liebet dich mehr als Jugend und", "tokens": ["Mei\u00b7ne", "See\u00b7le", "lie\u00b7bet", "dich", "mehr", "als", "Ju\u00b7gend", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "KOUS", "NN", "KON"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.46": {"text": "Freiheit,", "tokens": ["Frei\u00b7heit", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.47": {"text": "Mehr als Harf' und Gesang und der Be-", "tokens": ["Mehr", "als", "Ha\u00b7rf'", "und", "Ge\u00b7sang", "und", "der", "Be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "KOKOM", "NE", "KON", "NN", "KON", "ART", "TRUNC"], "meter": "+-++--+-+-", "measure": "trochaic.penta.relaxed"}, "line.48": {"text": "geisterung Kuss.", "tokens": ["geis\u00b7te\u00b7rung", "Kuss", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.49": {"text": "S\u00fcsser klingt mir dein L\u00e4cheln, als meiner Harfe", "tokens": ["S\u00fcs\u00b7ser", "klingt", "mir", "dein", "L\u00e4\u00b7cheln", ",", "als", "mei\u00b7ner", "Har\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.50": {"text": "Gelispel;", "tokens": ["Ge\u00b7lis\u00b7pel", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.51": {"text": "Hehrer blickt mir dein Aug', als die gestir-", "tokens": ["Heh\u00b7rer", "blickt", "mir", "dein", "Aug'", ",", "als", "die", "ge\u00b7stir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "KOUS", "ART", "TRUNC"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.52": {"text": "nete Nacht.", "tokens": ["ne\u00b7te", "Nacht", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "--+", "measure": "anapaest.init"}, "line.53": {"text": "Seliger ruht es sich, Edle, an deinem steigenden", "tokens": ["Se\u00b7li\u00b7ger", "ruht", "es", "sich", ",", "Ed\u00b7le", ",", "an", "dei\u00b7nem", "stei\u00b7gen\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PRF", "$,", "ADJA", "$,", "APPR", "PPOSAT", "ADJA"], "meter": "+--+--+--+-+--", "measure": "dactylic.tri.plus"}, "line.54": {"text": "Busen,", "tokens": ["Bu\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.55": {"text": "Als in der Mutter Natur weichem und offe-", "tokens": ["Als", "in", "der", "Mut\u00b7ter", "Na\u00b7tur", "wei\u00b7chem", "und", "of\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "NN", "ADJA", "KON", "TRUNC"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.56": {"text": "nem Schooss.", "tokens": ["nem", "Scho\u00b7oss", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.57": {"text": "Gl\u00fccklicher bin ich, umweht von deinen goldenen", "tokens": ["Gl\u00fcck\u00b7li\u00b7cher", "bin", "ich", ",", "um\u00b7weht", "von", "dei\u00b7nen", "gol\u00b7de\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "$,", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.58": {"text": "Locken,", "tokens": ["Lo\u00b7cken", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.59": {"text": "Als von des r\u00f6thlichen May's schimmernden", "tokens": ["Als", "von", "des", "r\u00f6th\u00b7li\u00b7chen", "May's", "schim\u00b7mern\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "VVINF"], "meter": "+--+--++--", "measure": "dactylic.di.plus"}, "line.60": {"text": "Bl\u00fcthen beschneyt.", "tokens": ["Bl\u00fc\u00b7then", "be\u00b7schneyt", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.61": {"text": "Tausend sind der Bl\u00fcthen des May's, und tausend", "tokens": 