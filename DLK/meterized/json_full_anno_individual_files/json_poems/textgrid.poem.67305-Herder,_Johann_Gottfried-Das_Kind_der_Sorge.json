{"textgrid.poem.67305": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Das Kind der Sorge", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einst sa\u00df am murmelnden Strome", "tokens": ["Einst", "sa\u00df", "am", "mur\u00b7meln\u00b7den", "Stro\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Sorge nieder und sann;", "tokens": ["Die", "Sor\u00b7ge", "nie\u00b7der", "und", "sann", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "KON", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Da bildet' im Traum der Gedanken", "tokens": ["Da", "bil\u00b7det'", "im", "Traum", "der", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ihr Finger ein leimernes Bild.", "tokens": ["Ihr", "Fin\u00b7ger", "ein", "lei\u00b7mer\u00b7nes", "Bild", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "\u00bbwas hast Du, sinnende G\u00f6ttin?\u00ab", "tokens": ["\u00bb", "was", "hast", "Du", ",", "sin\u00b7nen\u00b7de", "G\u00f6t\u00b7tin", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VAFIN", "PPER", "$,", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Spricht Zeus, der eben ihr naht.", "tokens": ["Spricht", "Zeus", ",", "der", "e\u00b7ben", "ihr", "naht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$,", "PRELS", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "\u00bbein Bild, von Thone gebildet;", "tokens": ["\u00bb", "ein", "Bild", ",", "von", "Tho\u00b7ne", "ge\u00b7bil\u00b7det", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Beleb's! ich bitte Dich, Gott.\u00ab", "tokens": ["Be\u00b7le\u00b7b's", "!", "ich", "bit\u00b7te", "Dich", ",", "Gott", ".", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["NE", "$.", "PPER", "VVFIN", "PPER", "$,", "NN", "$.", "$("], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "\u00bbwolan denn! lebe! \u2013 Es lebet!", "tokens": ["\u00bb", "wo\u00b7lan", "denn", "!", "le\u00b7be", "!", "\u2013", "Es", "le\u00b7bet", "!"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "$.", "VVFIN", "$.", "$(", "PPER", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und mein sei dieses Gesch\u00f6pf!\u00ab", "tokens": ["Und", "mein", "sei", "die\u00b7ses", "Ge\u00b7sch\u00f6pf", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "VAFIN", "PDAT", "NN", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Dagegen redet die Sorge:", "tokens": ["Da\u00b7ge\u00b7gen", "re\u00b7det", "die", "Sor\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "\u00bbnein, la\u00df es, la\u00df es mir, Herr!", "tokens": ["\u00bb", "nein", ",", "la\u00df", "es", ",", "la\u00df", "es", "mir", ",", "Herr", "!"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PTKANT", "$,", "VVIMP", "PPER", "$,", "VVIMP", "PPER", "PPER", "$,", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Mein Finger hat es gebildet.\u00ab", "tokens": ["Mein", "Fin\u00b7ger", "hat", "es", "ge\u00b7bil\u00b7det", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "\u00bbund ich gab Leben dem Thon,\u00ab", "tokens": ["\u00bb", "und", "ich", "gab", "Le\u00b7ben", "dem", "Thon", ",", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "PPER", "VVFIN", "NN", "ART", "NN", "$,", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sprach Jupiter. Als sie so sprachen,", "tokens": ["Sprach", "Ju\u00b7pi\u00b7ter", ".", "Als", "sie", "so", "spra\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da trat auch Tellus hinan.", "tokens": ["Da", "trat", "auch", "Tel\u00b7lus", "hi\u00b7nan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NE", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "\u00bbmein ist's! Sie hat mir genommen", "tokens": ["\u00bb", "mein", "ist's", "!", "Sie", "hat", "mir", "ge\u00b7nom\u00b7men"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PPOSAT", "NE", "$.", "PPER", "VAFIN", "PPER", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Von meinem Schoo\u00dfe das Kind.\u00ab", "tokens": ["Von", "mei\u00b7nem", "Schoo\u00b7\u00dfe", "das", "Kind", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "\u00bbwolan!\u00ab sprach Jupiter, \u00bbwartet!", "tokens": ["\u00bb", "wo\u00b7lan", "!", "\u00ab", "sprach", "Ju\u00b7pi\u00b7ter", ",", "\u00bb", "war\u00b7tet", "!"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "punct"], "pos": ["$(", "ADV", "$.", "$(", "VVFIN", "NE", "$,", "$(", "VVFIN", "$."], "meter": "-+----+-", "measure": "dactylic.init"}, "line.4": {"text": "Dort kommt ein Entscheider, Saturn.\u00ab", "tokens": ["Dort", "kommt", "ein", "Ent\u00b7schei\u00b7der", ",", "Sa\u00b7turn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "NN", "$.", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.6": {"line.1": {"text": "Saturn sprach: \u00bbHabet es Alle!", "tokens": ["Sa\u00b7turn", "sprach", ":", "\u00bb", "Ha\u00b7bet", "es", "Al\u00b7le", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "$(", "VAFIN", "PPER", "PIAT", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "So will's das hohe Geschick.", "tokens": ["So", "will's", "das", "ho\u00b7he", "Ge\u00b7schick", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Du, der das Leben ihm schenkte,", "tokens": ["Du", ",", "der", "das", "Le\u00b7ben", "ihm", "schenk\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Nimm, wenn es stirbet, den Geist;", "tokens": ["Nimm", ",", "wenn", "es", "stir\u00b7bet", ",", "den", "Geist", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "VVFIN", "$,", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.7": {"line.1": {"text": "Du, Tellus, seine Gebeine;", "tokens": ["Du", ",", "Tel\u00b7lus", ",", "sei\u00b7ne", "Ge\u00b7bei\u00b7ne", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "NE", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Denn mehr geh\u00f6ret Dir nicht.", "tokens": ["Denn", "mehr", "ge\u00b7h\u00f6\u00b7ret", "Dir", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Dir, seiner Mutter, o Sorge,", "tokens": ["Dir", ",", "sei\u00b7ner", "Mut\u00b7ter", ",", "o", "Sor\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wird es im Leben geschenkt.", "tokens": ["Wird", "es", "im", "Le\u00b7ben", "ge\u00b7schenkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.8": {"line.1": {"text": "Du wirst, so lang' es nur athmet,", "tokens": ["Du", "wirst", ",", "so", "lang'", "es", "nur", "ath\u00b7met", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "ADV", "ADV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Es nie verlassen, Dein Kind.", "tokens": ["Es", "nie", "ver\u00b7las\u00b7sen", ",", "Dein", "Kind", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Dir \u00e4hnlich, wird es von Tage", "tokens": ["Dir", "\u00e4hn\u00b7lich", ",", "wird", "es", "von", "Ta\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "$,", "VAFIN", "PPER", "APPR", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Zu Tage sich m\u00fchen ins Grab.\u00ab", "tokens": ["Zu", "Ta\u00b7ge", "sich", "m\u00fc\u00b7hen", "ins", "Grab", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "PRF", "ADJD", "APPRART", "NN", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.9": {"line.1": {"text": "Des Schicksals Spruch ist erf\u00fcllet,", "tokens": ["Des", "Schick\u00b7sals", "Spruch", "ist", "er\u00b7f\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Im Leben geh\u00f6rt es der Sorge,", "tokens": ["Im", "Le\u00b7ben", "ge\u00b7h\u00f6rt", "es", "der", "Sor\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+