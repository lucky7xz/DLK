{"dta.poem.9093": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1898", "urn": "urn:nbn:de:kobv:b4-200905192685", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich m\u00f6chte alle Geheimnisse wissen!", "tokens": ["Ich", "m\u00f6ch\u00b7te", "al\u00b7le", "Ge\u00b7heim\u00b7nis\u00b7se", "wis\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Alle Sterne, \u00fcber die Meere rollen, sch\u00f6pf ich mit meiner Hand.", "tokens": ["Al\u00b7le", "Ster\u00b7ne", ",", "\u00fc\u00b7ber", "die", "Mee\u00b7re", "rol\u00b7len", ",", "sch\u00f6pf", "ich", "mit", "mei\u00b7ner", "Hand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "APPR", "ART", "NN", "VVINF", "$,", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "--+-+--+-+-+--+-+", "measure": "iambic.septa.relaxed"}}, "stanza.3": {"line.1": {"text": "In meine Tr\u00e4ume", "tokens": ["In", "mei\u00b7ne", "Tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "drehn sich Welten,", "tokens": ["drehn", "sich", "Wel\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "PRF", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "und mich entz\u00fcckt das kleinste Nest,", "tokens": ["und", "mich", "ent\u00b7z\u00fcckt", "das", "kleins\u00b7te", "Nest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "das im Sommer ein Schwalbenpaar", "tokens": ["das", "im", "Som\u00b7mer", "ein", "Schwal\u00b7ben\u00b7paar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "APPRART", "NN", "ART", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "an meinem Giebel baut.", "tokens": ["an", "mei\u00b7nem", "Gie\u00b7bel", "baut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Das leiseste Zwitschern draus", "tokens": ["Das", "lei\u00b7ses\u00b7te", "Zwit\u00b7schern", "draus"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PAV"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "r\u00fchrt an mein Herz!", "tokens": ["r\u00fchrt", "an", "mein", "Herz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}