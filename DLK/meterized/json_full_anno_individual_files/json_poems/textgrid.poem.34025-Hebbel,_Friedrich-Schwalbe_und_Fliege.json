{"textgrid.poem.34025": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Schwalbe und Fliege", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An dem heitersten Morgen entst\u00fcrzte die fr\u00f6hlichste Schwalbe", "tokens": ["An", "dem", "hei\u00b7ters\u00b7ten", "Mor\u00b7gen", "ent\u00b7st\u00fcrz\u00b7te", "die", "fr\u00f6h\u00b7lichs\u00b7te", "Schwal\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Pl\u00f6tzlich dem Himmel und sank todt zu den F\u00fc\u00dfen mir hin.", "tokens": ["Pl\u00f6tz\u00b7lich", "dem", "Him\u00b7mel", "und", "sank", "todt", "zu", "den", "F\u00fc\u00b7\u00dfen", "mir", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "KON", "VVFIN", "ADJD", "APPR", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Mittags, der l\u00e4ngst Erstarrten den Schnabel \u00f6ffnend, ersp\u00e4ht' ich", "tokens": ["Mit\u00b7tags", ",", "der", "l\u00e4ngst", "Er\u00b7starr\u00b7ten", "den", "Schna\u00b7bel", "\u00f6ff\u00b7nend", ",", "er\u00b7sp\u00e4ht'", "ich"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "PRELS", "ADV", "VVFIN", "ART", "NN", "VVPP", "$,", "VVFIN", "PPER"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Eine Fliege im Schlund, welche sie halb nur verschluckt.", "tokens": ["Ei\u00b7ne", "Flie\u00b7ge", "im", "Schlund", ",", "wel\u00b7che", "sie", "halb", "nur", "ver\u00b7schluckt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "PRELS", "PPER", "ADJD", "ADV", "VVPP", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Diese zappelte noch, ich zog sie hervor, und, die Fl\u00fcgel", "tokens": ["Die\u00b7se", "zap\u00b7pel\u00b7te", "noch", ",", "ich", "zog", "sie", "her\u00b7vor", ",", "und", ",", "die", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "$,", "ART", "NN"], "meter": "+-+-++-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Trocknend im Sonnenstral, schwirrte sie bald mir davon.", "tokens": ["Trock\u00b7nend", "im", "Son\u00b7nen\u00b7stral", ",", "schwirr\u00b7te", "sie", "bald", "mir", "da\u00b7von", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$,", "VVFIN", "PPER", "ADV", "PPER", "PAV", "$."], "meter": "+--+--+--++-+", "measure": "dactylic.tri.plus"}}, "stanza.2": {"line.1": {"text": "An dem heitersten Morgen entst\u00fcrzte die fr\u00f6hlichste Schwalbe", "tokens": ["An", "dem", "hei\u00b7ters\u00b7ten", "Mor\u00b7gen", "ent\u00b7st\u00fcrz\u00b7te", "die", "fr\u00f6h\u00b7lichs\u00b7te", "Schwal\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Pl\u00f6tzlich dem Himmel und sank todt zu den F\u00fc\u00dfen mir hin.", "tokens": ["Pl\u00f6tz\u00b7lich", "dem", "Him\u00b7mel", "und", "sank", "todt", "zu", "den", "F\u00fc\u00b7\u00dfen", "mir", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "w