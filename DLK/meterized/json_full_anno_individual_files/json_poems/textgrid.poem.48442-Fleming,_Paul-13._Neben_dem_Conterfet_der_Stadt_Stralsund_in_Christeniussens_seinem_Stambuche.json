{"textgrid.poem.48442": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "13. Neben dem Conterfet der Stadt Stralsund in Christeniussens seinem Stambuche", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und dennoch stehst du noch, ob Mars, der W\u00fcterich,", "tokens": ["Und", "den\u00b7noch", "stehst", "du", "noch", ",", "ob", "Mars", ",", "der", "W\u00fc\u00b7te\u00b7rich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "NN", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "noch zweimal mehr so arg gesetzet h\u00e4tt' an dich,", "tokens": ["noch", "zwei\u00b7mal", "mehr", "so", "arg", "ge\u00b7set\u00b7zet", "h\u00e4tt'", "an", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADV", "ADJD", "VVPP", "VAFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "du unbezwungner ", "tokens": ["du", "un\u00b7be\u00b7zwung\u00b7ner"], "token_info": ["word", "word"], "pos": ["PPER", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "wenn du in Ketten erst am Himmel w\u00e4rst gehangen?", "tokens": ["wenn", "du", "in", "Ket\u00b7ten", "erst", "am", "Him\u00b7mel", "w\u00e4rst", "ge\u00b7han\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "ADV", "APPRART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Di\u00df sch\u00e4ndet ihn vielmehr, da\u00df deine Niedrigkeit", "tokens": [