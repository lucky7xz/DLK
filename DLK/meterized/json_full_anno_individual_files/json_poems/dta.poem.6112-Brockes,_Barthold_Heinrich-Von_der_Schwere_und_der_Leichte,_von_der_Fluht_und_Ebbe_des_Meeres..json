{"dta.poem.6112": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Schwere und der Leichte,  \n von der Fluht und Ebbe  \n des Meeres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Erden Wirbel hegt, in seinem \u00e4ussern Creise,", "tokens": ["Der", "Er\u00b7den", "Wir\u00b7bel", "hegt", ",", "in", "sei\u00b7nem", "\u00e4us\u00b7sern", "Crei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von d\u00fcnner Himmels-Lufft die zarten K\u00fcgelein:", "tokens": ["Von", "d\u00fcn\u00b7ner", "Him\u00b7mels\u00b7Lufft", "die", "zar\u00b7ten", "K\u00fc\u00b7ge\u00b7lein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die, da sie einfach, rund, und nicht verwickelt seyn;", "tokens": ["Die", ",", "da", "sie", "ein\u00b7fach", ",", "rund", ",", "und", "nicht", "ver\u00b7wi\u00b7ckelt", "seyn", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "ADV", "$,", "ADJD", "$,", "KON", "PTKNEG", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sich \u00f6ffnen und sich fliehn, auf eine leichte Weise.", "tokens": ["Sich", "\u00f6ff\u00b7nen", "und", "sich", "fliehn", ",", "auf", "ei\u00b7ne", "leich\u00b7te", "Wei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "KON", "PRF", "VVINF", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie scheinen recht dazu formirt,", "tokens": ["Sie", "schei\u00b7nen", "recht", "da\u00b7zu", "for\u00b7mirt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df sich ein jedes schnell beweget, regt und r\u00fchrt.", "tokens": ["Da\u00df", "sich", "ein", "je\u00b7des", "schnell", "be\u00b7we\u00b7get", ",", "regt", "und", "r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "PIAT", "ADJD", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Lufft-Creis ist ein anders fl\u00fc\u00dfigs Wesen,", "tokens": ["Der", "Lufft\u00b7Creis", "ist", "ein", "an\u00b7ders", "fl\u00fc\u00b7\u00dfigs", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Das sich um uns in blauen Circkeln lenckt.", "tokens": ["Das", "sich", "um", "uns", "in", "blau\u00b7en", "Cir\u00b7ckeln", "lenckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPR", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Das feuchte Element hat sich herab gesenckt,", "tokens": ["Das", "feuch\u00b7te", "E\u00b7le\u00b7ment", "hat", "sich", "her\u00b7ab", "ge\u00b7senckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Durch welches sich das Feld in seinem Durste tr\u00e4nckt,", "tokens": ["Durch", "wel\u00b7ches", "sich", "das", "Feld", "in", "sei\u00b7nem", "Durs\u00b7te", "tr\u00e4nckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die Erde ist das unterste von allen.", "tokens": ["Die", "Er\u00b7de", "ist", "das", "un\u00b7ters\u00b7te", "von", "al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "APPR", "PIAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Ein aufgeworffner Stein wird bald herunter fallen.", "tokens": ["Ein", "auf\u00b7ge\u00b7worff\u00b7ner", "Stein", "wird", "bald", "her\u00b7un\u00b7ter", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und werden wir nach unterschiednen H\u00f6hen,", "tokens": ["Und", "wer\u00b7den", "wir", "nach", "un\u00b7ter\u00b7schied\u00b7nen", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Die C\u00f6rper, die von ihrem Ort", "tokens": ["Die", "C\u00f6r\u00b7per", ",", "die", "von", "ih\u00b7rem", "Ort"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Entfernet sind, so fort", "tokens": ["Ent\u00b7fer\u00b7net", "sind", ",", "so", "fort"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVPP", "VAFIN", "$,", "ADV", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "In ihrer Ordnung liegen sehen.", "tokens": ["In", "ih\u00b7rer", "Ord\u00b7nung", "lie\u00b7gen", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}