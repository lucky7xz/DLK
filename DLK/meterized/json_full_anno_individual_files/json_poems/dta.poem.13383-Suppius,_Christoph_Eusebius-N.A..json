{"dta.poem.13383": {"metadata": {"author": {"name": "Suppius, Christoph Eusebius", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1749", "urn": "urn:nbn:de:kobv:b4-20594-7", "language": ["de:0.99"], "booktitle": "Suppius, Christoph Eusebius: Oden und Lieder. Gotha, 1749."}, "poem": {"stanza.1": {"line.1": {"text": "Ich seh", "tokens": ["Ich", "seh"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "von meiner Blumen reichen H\u00f6h,", "tokens": ["von", "mei\u00b7ner", "Blu\u00b7men", "rei\u00b7chen", "H\u00f6h", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "das mu\u00df auch mich in Freude setzen,", "tokens": ["das", "mu\u00df", "auch", "mich", "in", "Freu\u00b7de", "set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es reizt", "tokens": ["Es", "reizt"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Der theuren Landes-Mutter Nahmen,", "tokens": ["Der", "theu\u00b7ren", "Lan\u00b7des\u00b7Mut\u00b7ter", "Nah\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "und", "tokens": ["und"], "token_info": ["word"], "pos": ["KON"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Sie lebe! gut! hier ist mein Amen,", "tokens": ["Sie", "le\u00b7be", "!", "gut", "!", "hier", "ist", "mein", "A\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ADJD", "$.", "ADV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "ich bin", "tokens": ["ich", "bin"], "token_info": ["word", "word"], "pos": ["PPER", "VAFIN"], "meter": "-+", "measure": "iambic.single"}}}}}