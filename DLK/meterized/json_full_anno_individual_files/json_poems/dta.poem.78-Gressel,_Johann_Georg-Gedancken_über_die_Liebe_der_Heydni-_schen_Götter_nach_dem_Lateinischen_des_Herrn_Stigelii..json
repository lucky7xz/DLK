{"dta.poem.78": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Gedancken \u00fcber die Liebe der Heydni-  \n schen G\u00f6tter/ nach dem Lateinischen des  \n Herrn  Stigelii.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn das ein Fehler ist/ da\u00df ich verliebet bin/", "tokens": ["Wenn", "das", "ein", "Feh\u00b7ler", "ist", "/", "da\u00df", "ich", "ver\u00b7lie\u00b7bet", "bin", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ART", "NN", "VAFIN", "$(", "KOUS", "PPER", "VVFIN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So m\u00fcst ihr G\u00f6tter doch hier durch die Finger sehen/", "tokens": ["So", "m\u00fcst", "ihr", "G\u00f6t\u00b7ter", "doch", "hier", "durch", "die", "Fin\u00b7ger", "se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "ADV", "ADV", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil ", "tokens": ["Weil"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Und seinen scharffen Pfeil in euch hat lassen gehen.", "tokens": ["Und", "sei\u00b7nen", "scharf\u00b7fen", "Pfeil", "in", "euch", "hat", "las\u00b7sen", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "APPR", "PPER", "VAFIN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es muste ", "tokens": ["Es", "mus\u00b7te"], "token_info": ["word", "word"], "pos": ["PPER", "VMFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Umsonst nennt man ihn nicht Stier/ Widder/ Schwaan", "tokens": ["Um\u00b7sonst", "nennt", "man", "ihn", "nicht", "Stier", "/", "Wid\u00b7der", "/", "Schwaan"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "PTKNEG", "NN", "$(", "NN", "$(", "NE"], "meter": "+-+-+-++-+", "measure": "unknown.measure.hexa"}, "line.7": {"text": "und Regen/", "tokens": ["und", "Re\u00b7gen", "/"], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Er kleidete darum sich in solch Wesen ein/", "tokens": ["Er", "klei\u00b7de\u00b7te", "da\u00b7rum", "sich", "in", "solch", "We\u00b7sen", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "PRF", "APPR", "PIAT", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Damit nach Hertzens-Wunsch der Lieb\u2019 er k\u00f6nte pflegen.", "tokens": ["Da\u00b7mit", "nach", "Hert\u00b7zens\u00b7Wunsch", "der", "Lieb'", "er", "k\u00f6n\u00b7te", "pfle\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "NN", "ART", "NN", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Neptun hat in der Fluht offt lichter loh gebrennt/", "tokens": ["Nep\u00b7tun", "hat", "in", "der", "Fluht", "offt", "lich\u00b7ter", "loh", "ge\u00b7brennt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "ART", "NN", "ADV", "ADJD", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sein Hertze ist entz\u00fcndt durch heisse Gluht gewesen.", "tokens": ["Sein", "Hert\u00b7ze", "ist", "ent\u00b7z\u00fcndt", "durch", "heis\u00b7se", "Gluht", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "VAFIN", "ADJD", "APPR", "ADJA", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Auf da\u00df ein sch\u00f6nes Kind von ihme w\u00fcrd\u2019 erkennt/", "tokens": ["Auf", "da\u00df", "ein", "sch\u00f6\u00b7nes", "Kind", "von", "ih\u00b7me", "w\u00fcrd'", "er\u00b7kennt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "ADJA", "NN", "APPR", "PPER", "VAFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So lie\u00df zum Sch\u00e4ffer sich der ", "tokens": ["So", "lie\u00df", "zum", "Sch\u00e4f\u00b7fer", "sich", "der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "PRF", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Die G\u00f6ttin/ so der Welt die s\u00fcssen Schmertzen macht/", "tokens": ["Die", "G\u00f6t\u00b7tin", "/", "so", "der", "Welt", "die", "s\u00fcs\u00b7sen", "Schmert\u00b7zen", "macht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Hat offt ihr eigen Schwerdt auf ihre Brust gewetzet.", "tokens": ["Hat", "offt", "ihr", "ei\u00b7gen", "Schwerdt", "auf", "ih\u00b7re", "Brust", "ge\u00b7wet\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Jhr lahmer Ehe-Mann ist offt von ihr verlacht/", "tokens": ["Ihr", "lah\u00b7mer", "E\u00b7he\u00b7Mann", "ist", "offt", "von", "ihr", "ver\u00b7lacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADV", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wenn mit dem tapffern ", "tokens": ["Wenn", "mit", "dem", "tapf\u00b7fern"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.18": {"text": "Indem sie einst mit ihm die s\u00fcsse Wollust treibt/", "tokens": ["In\u00b7dem", "sie", "einst", "mit", "ihm", "die", "s\u00fcs\u00b7se", "Wol\u00b7lust", "treibt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPER", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So wird sie vom ", "tokens": ["So", "wird", "sie", "vom"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPRART"], "meter": "-+-+", "measure": "iambic.di"}, "line.20": {"text": "Man redete sonst nichts/ als da\u00df er schlimm beweibt/", "tokens": ["Man", "re\u00b7de\u00b7te", "sonst", "nichts", "/", "als", "da\u00df", "er", "schlimm", "be\u00b7weibt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "PIS", "$(", "KOKOM", "KOUS", "PPER", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Und seiner ward gedacht/ wo nur zwo G\u00f6tter stunden.", "tokens": ["Und", "sei\u00b7ner", "ward", "ge\u00b7dacht", "/", "wo", "nur", "zwo", "G\u00f6t\u00b7ter", "stun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "VAFIN", "VVPP", "$(", "PWAV", "ADV", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Selbst ", "tokens": ["Selbst"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.23": {"text": "Hat mehr denn einmahl sich im Eh-Bruch lassen k\u00fcssen!", "tokens": ["Hat", "mehr", "denn", "ein\u00b7mahl", "sich", "im", "Eh\u00b7Bruch", "las\u00b7sen", "k\u00fcs\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADV", "PRF", "APPRART", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "So reist mich ", "tokens": ["So", "reist", "mich"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.25": {"text": "Und der/ der alles zwingt/ hat mich auch zwingen m\u00fcssen.", "tokens": ["Und", "der", "/", "der", "al\u00b7les", "zwingt", "/", "hat", "mich", "auch", "zwin\u00b7gen", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$(", "ART", "PIS", "VVFIN", "$(", "VAFIN", "PPER", "ADV", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}