{"textgrid.poem.49493": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "Auf den Burgunderwein", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der war gewi\u00df ein frommer Mann", "tokens": ["Der", "war", "ge\u00b7wi\u00df", "ein", "from\u00b7mer", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Jupiter so liebgewann,", "tokens": ["Den", "Ju\u00b7pi\u00b7ter", "so", "lieb\u00b7ge\u00b7wann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df er ihm diesen Weinstock schenkte,", "tokens": ["Da\u00df", "er", "ihm", "die\u00b7sen", "Wein\u00b7stock", "schenk\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihn selbst in seinen Garten senkte,", "tokens": ["Ihn", "selbst", "in", "sei\u00b7nen", "Gar\u00b7ten", "senk\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und voll so sch\u00f6ner Trauben henkte.", "tokens": ["Und", "voll", "so", "sch\u00f6\u00b7ner", "Trau\u00b7ben", "henk\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Luna Horn mu\u00df ihn bethaut,", "tokens": ["Der", "Lu\u00b7na", "Horn", "mu\u00df", "ihn", "be\u00b7thaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Apollo huldreich angeschaut,", "tokens": ["A\u00b7pol\u00b7lo", "huld\u00b7reich", "an\u00b7ge\u00b7schaut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vertumnus Spate selbst umgraben,", "tokens": ["Ver\u00b7tum\u00b7nus", "Spa\u00b7te", "selbst", "um\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und f\u00fcr den Staaren und den Raben", "tokens": ["Und", "f\u00fcr", "den", "Staa\u00b7ren", "und", "den", "Ra\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Speer Priaps besch\u00fctzet haben.", "tokens": ["Der", "Speer", "Pri\u00b7aps", "be\u00b7sch\u00fct\u00b7zet", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Das war gewi\u00df Dianens Hand,", "tokens": ["Das", "war", "ge\u00b7wi\u00df", "Di\u00b7a\u00b7nens", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die mit dem Ulmbaum' ihn verband,", "tokens": ["Die", "mit", "dem", "Ulm\u00b7baum'", "ihn", "ver\u00b7band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ihren Seegen auf ihn legte,", "tokens": ["Und", "ih\u00b7ren", "See\u00b7gen", "auf", "ihn", "leg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weil er sie zu verbergen pflegte,", "tokens": ["Weil", "er", "sie", "zu", "ver\u00b7ber\u00b7gen", "pfleg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-----+-+-", "measure": "unknown.measure.di"}, "line.5": {"text": "Wenn sie den sch\u00f6nen J\u00fcngling hegte.", "tokens": ["Wenn", "sie", "den", "sch\u00f6\u00b7nen", "J\u00fcng\u00b7ling", "heg\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Eh Peleus in der ersten Nacht", "tokens": ["Eh", "Pe\u00b7leus", "in", "der", "ers\u00b7ten", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "APPR", "ART", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Der Braut den G\u00fcrtel losgemacht,", "tokens": ["Der", "Braut", "den", "G\u00fcr\u00b7tel", "los\u00b7ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So fehlte bey dem hohen Feste,", "tokens": ["So", "fehl\u00b7te", "bey", "dem", "ho\u00b7hen", "Fes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu der Bewirthung seiner G\u00e4ste,", "tokens": ["Zu", "der", "Be\u00b7wirt\u00b7hung", "sei\u00b7ner", "G\u00e4s\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der s\u00fcse Nectartrank, das Beste.", "tokens": ["Der", "s\u00fc\u00b7se", "Nec\u00b7tar\u00b7trank", ",", "das", "Bes\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Da sagte Zevs zur G\u00f6tterschaar:", "tokens": ["Da", "sag\u00b7te", "Zevs", "zur", "G\u00f6t\u00b7ter\u00b7schaar", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir trinken Nectar, Jahr f\u00fcr Jahr,", "tokens": ["Wir", "trin\u00b7ken", "Nec\u00b7tar", ",", "Jahr", "f\u00fcr", "Jahr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Seitdem wir in den Wolken leben:", "tokens": ["Seit\u00b7dem", "wir", "in", "den", "Wol\u00b7ken", "le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch heute sollen irdsche Reben", "tokens": ["Doch", "heu\u00b7te", "sol\u00b7len", "ird\u00b7sche", "Re\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Unsterblichen ein Labsal geben.", "tokens": ["U\u00b7nsterb\u00b7li\u00b7chen", "ein", "Lab\u00b7sal", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.6": {"line.1": {"text": "Er sch\u00fcttelt sein allm\u00e4chtig Haupt.", "tokens": ["Er", "sch\u00fct\u00b7telt", "sein", "all\u00b7m\u00e4ch\u00b7tig", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gleich steigt der edle Stock, belaubt,", "tokens": ["Gleich", "steigt", "der", "ed\u00b7le", "Stock", ",", "be\u00b7laubt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit schlanken \u00c4rmen in die L\u00fcfte,", "tokens": ["Mit", "schlan\u00b7ken", "\u00c4r\u00b7men", "in", "die", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verbreitet holde Fr\u00fccht' und D\u00fcfte,", "tokens": ["Ver\u00b7brei\u00b7tet", "hol\u00b7de", "Fr\u00fccht'", "und", "D\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df er den Ruhm des Meisters stifte.", "tokens": ["Da\u00df", "er", "den", "Ruhm", "des", "Meis\u00b7ters", "stif\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Gehabt euch wohl, schrie Cypria,", "tokens": ["Ge\u00b7habt", "euch", "wohl", ",", "schrie", "Cyp\u00b7ria", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "VVFIN", "NE", "$,"], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.2": {"text": "Du Nectar, du Ambrosia;", "tokens": ["Du", "Nec\u00b7tar", ",", "du", "A\u00b7mbro\u00b7sia", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PPER", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Euch so vermi\u00dfen, ist gewonnen.", "tokens": ["Euch", "so", "ver\u00b7mi\u00b7\u00dfen", ",", "ist", "ge\u00b7won\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "$,", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es lebe Zevs, der nach der Sonnen,", "tokens": ["Es", "le\u00b7be", "Zevs", ",", "der", "nach", "der", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "$,", "PRELS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Kein wundersch\u00f6ner Werk begonnen.", "tokens": ["Kein", "wun\u00b7der\u00b7sch\u00f6\u00b7ner", "Werk", "be\u00b7gon\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Sie streckt die Finger l\u00fcstern hin", "tokens": ["Sie", "streckt", "die", "Fin\u00b7ger", "l\u00fcs\u00b7tern", "hin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Rebenkind zu sich zu ziehn,", "toke