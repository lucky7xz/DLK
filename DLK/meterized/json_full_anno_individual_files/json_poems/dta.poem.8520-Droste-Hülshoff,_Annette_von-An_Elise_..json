{"dta.poem.8520": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "An Elise .", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das war gewi\u00df ein andrer M&#228;rz,", "tokens": ["Das", "war", "ge\u00b7wi\u00df", "ein", "an\u00b7drer", "M", "&#228;", "rz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ART", "ADJA", "NE", "$(", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Mond, den Bl\u00fcthenkr\u00e4nz\u2019 umhegten,", "tokens": ["Ein", "Mond", ",", "den", "Bl\u00fcthen\u00b7kr\u00e4nz'", "um\u00b7heg\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Als Engel dich, geliebtes Herz,", "tokens": ["Als", "En\u00b7gel", "dich", ",", "ge\u00b7lieb\u00b7tes", "Herz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In deine erste Wiege legten;", "tokens": ["In", "dei\u00b7ne", "ers\u00b7te", "Wie\u00b7ge", "leg\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Das war gewi\u00df ein Tag so frei,", "tokens": ["Das", "war", "ge\u00b7wi\u00df", "ein", "Tag", "so", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So frisch vom Sonnenstrahl umglommen!", "tokens": ["So", "frisch", "vom", "Son\u00b7nen\u00b7strahl", "um\u00b7glom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Doch auch im Wintermantel sei", "tokens": ["Doch", "auch", "im", "Win\u00b7ter\u00b7man\u00b7tel", "sei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Er, wie der sch\u00f6nste, mir willkommen.", "tokens": ["Er", ",", "wie", "der", "sch\u00f6ns\u00b7te", ",", "mir", "will\u00b7kom\u00b7men", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PWAV", "ART", "ADJA", "$,", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mir ward ein schlimmrer Mond zu Theil,", "tokens": ["Mir", "ward", "ein", "schlimm\u00b7rer", "Mond", "zu", "Theil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Um den kein Vogel je gesungen,", "tokens": ["Um", "den", "kein", "Vo\u00b7gel", "je", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "PIAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nur Eiseszapfen blank und steil", "tokens": ["Nur", "Ei\u00b7ses\u00b7zap\u00b7fen", "blank", "und", "steil"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "PTKVZ", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das kalte Diadem geschlungen;", "tokens": ["Das", "kal\u00b7te", "Di\u00b7a\u00b7dem", "ge\u00b7schlun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ach anders wirken Schnee und Eis!", "tokens": ["Ach", "an\u00b7ders", "wir\u00b7ken", "Schnee", "und", "Eis", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVINF", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und anders wohl der Sonnen G\u00fcte!", "tokens": ["Und", "an\u00b7ders", "wohl", "der", "Son\u00b7nen", "G\u00fc\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ich steh\u2019, ein d\u00fcstres Tannenreis,", "tokens": ["Ich", "steh'", ",", "ein", "d\u00fcst\u00b7res", "Tan\u00b7nen\u00b7reis", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Du eine zarte Veilchenbl\u00fcte.", "tokens": ["Du", "ei\u00b7ne", "zar\u00b7te", "Veil\u00b7chen\u00b7bl\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch fest zusammen, fest im Raum", "tokens": ["Doch", "fest", "zu\u00b7sam\u00b7men", ",", "fest", "im", "Raum"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "PTKVZ", "$,", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gehalten in des Winters St\u00fcrmen,", "tokens": ["Ge\u00b7hal\u00b7ten", "in", "des", "Win\u00b7ters", "St\u00fcr\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du schm\u00fccke mich zum Weihnachtsbaum", "tokens": ["Du", "schm\u00fc\u00b7cke", "mich", "zum", "Weih\u00b7nachts\u00b7baum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ich will deine Bl\u00fcte schirmen;", "tokens": ["Und", "ich", "will", "dei\u00b7ne", "Bl\u00fc\u00b7te", "schir\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dann mu\u00df uns willig oder nicht", "tokens": ["Dann", "mu\u00df", "uns", "wil\u00b7lig", "o\u00b7der", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "KON", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Leben reiche Gaben z\u00e4hlen,", "tokens": ["Das", "Le\u00b7ben", "rei\u00b7che", "Ga\u00b7ben", "z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und niemals wird das Himmelslicht,", "tokens": ["Und", "nie\u00b7mals", "wird", "das", "Him\u00b7mels\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Poesie Beleuchtung fehlen.", "tokens": ["Der", "Poe\u00b7sie", "Be\u00b7leuch\u00b7tung", "feh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}