{"dta.poem.18057": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "XiV.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198462", "language": ["de:0.71", "nl:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zween Spie\u00dfgesellen waren gut", "tokens": ["Zween", "Spie\u00df\u00b7ge\u00b7sel\u00b7len", "wa\u00b7ren", "gut"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "VAFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Worten, aber nicht von Muth.", "tokens": ["Von", "Wor\u00b7ten", ",", "a\u00b7ber", "nicht", "von", "Muth", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADV", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie giengen izt durch einen Wald,", "tokens": ["Sie", "gien\u00b7gen", "izt", "durch", "ei\u00b7nen", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gefahr war nah und ", "tokens": ["Ge\u00b7fahr", "war", "nah", "und"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ADJD", "KON"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Da sagten beyde auf den Eid,", "tokens": ["Da", "sag\u00b7ten", "bey\u00b7de", "auf", "den", "Eid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie wollten mit Aufrichtigkeit", "tokens": ["Sie", "woll\u00b7ten", "mit", "Auf\u00b7rich\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zusammenhalten, bis zum Tod.", "tokens": ["Zu\u00b7sam\u00b7men\u00b7hal\u00b7ten", ",", "bis", "zum", "Tod", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der eine Freund war braun, der andre roth.", "tokens": ["Der", "ei\u00b7ne", "Freund", "war", "braun", ",", "der", "and\u00b7re", "roth", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VAFIN", "ADJD", "$,", "PRELS", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Weil sie in dieser Rede sind,", "tokens": ["Weil", "sie", "in", "die\u00b7ser", "Re\u00b7de", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "K\u00f6mmt tr\u00e4ges Schritts ein B\u00e4r gegangen.", "tokens": ["K\u00f6mmt", "tr\u00e4\u00b7ges", "Schritts", "ein", "B\u00e4r", "ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Der Rothe wartet nicht, ihn zu empfangen,", "tokens": ["Der", "Ro\u00b7the", "war\u00b7tet", "nicht", ",", "ihn", "zu", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Er klimmt auf einen Baum geschwind.", "tokens": ["Er", "klimmt", "auf", "ei\u00b7nen", "Baum", "ge\u00b7schwind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Der Braune kam in grosse Noth;", "tokens": ["Der", "Brau\u00b7ne", "kam", "in", "gros\u00b7se", "Noth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Er stellt sich an, als w\u00e4r er todt.", "tokens": ["Er", "stellt", "sich", "an", ",", "als", "w\u00e4r", "er", "todt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKVZ", "$,", "KOKOM", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Er r\u00fchrt sich weder hin noch her.", "tokens": ["Er", "r\u00fchrt", "sich", "we\u00b7der", "hin", "noch", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "KON", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Bedachtsam kam zu ihm der B\u00e4r,", "tokens": ["Be\u00b7dacht\u00b7sam", "kam", "zu", "ihm", "der", "B\u00e4r", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Wo er am Boden lag, der L\u00e4nge nach gestreckt,", "tokens": ["Wo", "er", "am", "Bo\u00b7den", "lag", ",", "der", "L\u00e4n\u00b7ge", "nach", "ge\u00b7streckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "VVFIN", "$,", "ART", "NN", "APPR", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Er h\u00e4lt ihn f\u00fcr ein Aas, das l\u00e4ngst verreckt.", "tokens": ["Er", "h\u00e4lt", "ihn", "f\u00fcr", "ein", "Aas", ",", "das", "l\u00e4ngst", "ver\u00b7reckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "PRELS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Er wirfft ihn um, und riecht ihn an.", "tokens": ["Er", "wirfft", "ihn", "um", ",", "und", "riecht", "ihn", "an", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Zulezt gieng er davon, und lie\u00df den todten Mann.", "tokens": ["Zu\u00b7lezt", "gieng", "er", "da\u00b7von", ",", "und", "lie\u00df", "den", "tod\u00b7ten", "Mann", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PAV", "$,", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die\u00df alles sah der rothe Spie\u00dfgeselle;", "tokens": ["Die\u00df", "al\u00b7les", "sah", "der", "ro\u00b7the", "Spie\u00df\u00b7ge\u00b7sel\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Er gieng vom Baum herab zur Stelle,", "tokens": ["Er", "gieng", "vom", "Baum", "her\u00b7ab", "zur", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Und sprach zu seinem Freund: Ey mein!", "tokens": ["Und", "sprach", "zu", "sei\u00b7nem", "Freund", ":", "Ey", "mein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$.", "NN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Was mogte das Geraune seyn? ", "tokens": ["Was", "mog\u00b7te", "das", "Ge\u00b7rau\u00b7ne", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Was raunte dir der kluge B\u00e4r?", "tokens": ["Was", "raun\u00b7te", "dir", "der", "klu\u00b7ge", "B\u00e4r", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Ich sah wohl auf dem Baum da\u00df er", "tokens": ["Ich", "sah", "wohl", "auf", "dem", "Baum", "da\u00df", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "KOUS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Zu deinen Ohren hielt den Mund;", "tokens": ["Zu", "dei\u00b7nen", "Oh\u00b7ren", "hielt", "den", "Mund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Hat er dich was gelehrt, so thu mirs kund.", "tokens": ["Hat", "er", "dich", "was", "ge\u00b7lehrt", ",", "so", "thu", "mirs", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "PIS", "VVPP", "$,", "ADV", "VVFIN", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Der Braune sprach; Ja freylich raunt er mir,", "tokens": ["Der", "Brau\u00b7ne", "sprach", ";", "Ja", "frey\u00b7lich", "raunt", "er", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PTKANT", "ADV", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Und was er mir geraunt, das raun ich dir,", "tokens": ["Und", "was", "er", "mir", "ge\u00b7raunt", ",", "das", "raun", "ich", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PPER", "VVPP", "$,", "PDS", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Er sagte: Meide den, (das ist mein Rath,)", "tokens": ["Er", "sag\u00b7te", ":", "Mei\u00b7de", "den", ",", "(", "das", "ist", "mein", "Rath", ",", ")"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$.", "PIS", "ART", "$,", "$(", "PDS", "VAFIN", "PPOSAT", "NN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Der auf den Baum gefl\u00fcchtet hat:", "tokens": ["Der", "auf", "den", "Baum", "ge\u00b7fl\u00fcch\u00b7tet", "hat", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Denn geht es ernstlich an die Noth,", "tokens": ["Denn", "geht", "es", "ernst\u00b7lich", "an", "die", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "So l\u00e4\u00dft er dich, denn er ist roth.", "tokens": ["So", "l\u00e4\u00dft", "er", "dich", ",", "denn", "er", "ist", "roth", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "KON", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}