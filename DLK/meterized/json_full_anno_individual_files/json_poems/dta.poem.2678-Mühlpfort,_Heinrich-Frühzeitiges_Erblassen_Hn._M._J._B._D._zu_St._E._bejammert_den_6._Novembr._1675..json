, "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.76": {"text": "O seelig wer nicht lang als Pilger hier darff wallen", "tokens": ["O", "see\u00b7lig", "wer", "nicht", "lang", "als", "Pil\u00b7ger", "hier", "darff", "wal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "PWS", "PTKNEG", "ADJD", "KOKOM", "NN", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.77": {"text": "Und ruht in GOttes Hand/ voll Freud und Sicherheit!", "tokens": ["Und", "ruht", "in", "Got\u00b7tes", "Hand", "/", "voll", "Freud", "und", "Si\u00b7cher\u00b7heit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "NN", "$(", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.78": {"text": "Ich wei\u00df/ ", "tokens": ["Ich", "wei\u00df", "/"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$("], "meter": "-+", "measure": "iambic.single"}, "line.79": {"text": "Wie scharff es auch zu erst hat Fleisch und Blut gedr\u00fcckt/", "tokens": ["Wie", "scharff", "es", "auch", "zu", "erst", "hat", "Fleisch", "und", "Blut", "ge\u00b7dr\u00fcckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "APPR", "ADV", "VAFIN", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.80": {"text": "Und weil er Traurende mit viel bewehrten Proben/", "tokens": ["Und", "weil", "er", "Trau\u00b7ren\u00b7de", "mit", "viel", "be\u00b7wehr\u00b7ten", "Pro\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "APPR", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+