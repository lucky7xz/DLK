{"dta.poem.2678": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchzeitiges Erblassen  \n  Hn.   M.   J. B.   D.   zu St. E. bejammert den  \n 6. Novembr. 1675.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer kan den Wunder-Rath des H\u00f6chsten doch ergr\u00fcnd&#275;?", "tokens": ["Wer", "kan", "den", "Wun\u00b7der\u00b7Rath", "des", "H\u00f6chs\u00b7ten", "doch", "er\u00b7gr\u00fcnd", "&#275;", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "XML_entity", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "ART", "NN", "ADV", "ADJD", "$(", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sein unerforschlich Schlu\u00df bleibt heilig und ge-", "tokens": ["Sein", "un\u00b7er\u00b7for\u00b7schlich", "Schlu\u00df", "bleibt", "hei\u00b7lig", "und", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "NN", "VVFIN", "ADJD", "KON", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "(binden/", "tokens": ["(", "bin\u00b7den", "/"], "token_info": ["punct", "word", "punct"], "pos": ["$(", "VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Sein\u2019 Allmacht l\u00e4st sich nicht der Menschen Satzung", "tokens": ["Sein'", "All\u00b7macht", "l\u00e4st", "sich", "nicht", "der", "Men\u00b7schen", "Sat\u00b7zung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "PTKNEG", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Es wird hier Fleisch und Blut sammt der Vernunfft geschw\u00e4cht/", "tokens": ["Es", "wird", "hier", "Fleisch", "und", "Blut", "sammt", "der", "Ver\u00b7nunfft", "ge\u00b7schw\u00e4cht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "KON", "NN", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es schenckt\u2019/ ", "tokens": ["Es", "schen\u00b7ckt'", "/"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Die milde Vater-Hand ihm einen jungen Sohn/", "tokens": ["Die", "mil\u00b7de", "Va\u00b7ter\u00b7Hand", "ihm", "ei\u00b7nen", "jun\u00b7gen", "Sohn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Jetzt sieht er seinen Trost und Hoffnung unterbrochen/", "tokens": ["Jetzt", "sieht", "er", "sei\u00b7nen", "Trost", "und", "Hoff\u00b7nung", "un\u00b7ter\u00b7bro\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nun mit dem \u00e4ltesten zieht alle Lust davon.", "tokens": ["Nun", "mit", "dem", "\u00e4l\u00b7tes\u00b7ten", "zieht", "al\u00b7le", "Lust", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.10": {"text": "So wechselt Freud und Leid/ so mischen sich zusammen", "tokens": ["So", "wech\u00b7selt", "Freud", "und", "Leid", "/", "so", "mi\u00b7schen", "sich", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN", "$(", "ADV", "VVFIN", "PRF", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ein Quell von kurtzer Lust/ und Thr\u00e4nen-volles Meer;", "tokens": ["Ein", "Quell", "von", "kurt\u00b7zer", "Lust", "/", "und", "Thr\u00e4\u00b7nen\u00b7vol\u00b7les", "Meer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$(", "KON", "NE", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So folgt die schwartze Nacht den hellen Tages Flammen/", "tokens": ["So", "folgt", "die", "schwart\u00b7ze", "Nacht", "den", "hel\u00b7len", "Ta\u00b7ges", "Flam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und Freuden sind gantz leicht/ hergegen Schmertzen schwer.", "tokens": ["Und", "Freu\u00b7den", "sind", "gantz", "leicht", "/", "her\u00b7ge\u00b7gen", "Schmert\u00b7zen", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ADV", "ADJD", "$(", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wohin sein Auge blickt/ ist Zunder zu dem Trauren/", "tokens": ["Wo\u00b7hin", "sein", "Au\u00b7ge", "blickt", "/", "ist", "Zun\u00b7der", "zu", "dem", "Trau\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$(", "VAFIN", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Er siht nur \u00fcberall ein Bild der Sterbligkeit/", "tokens": ["Er", "siht", "nur", "\u00fc\u00b7be\u00b7rall", "ein", "Bild", "der", "Ster\u00b7blig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Denn Sarg und Wiege stehn bey ihm in gleichen Mauren:", "tokens": ["Denn", "Sarg", "und", "Wie\u00b7ge", "stehn", "bey", "ihm", "in", "glei\u00b7chen", "Mau\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "APPR", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Dort liegt der todte Sohn/ hier dieser weint und schreyt.", "tokens": ["Dort", "liegt", "der", "tod\u00b7te", "Sohn", "/", "hier", "die\u00b7ser", "weint", "und", "schreyt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$(", "ADV", "PDS", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Den einen hat er schon geliefert GOttes H\u00e4nden/", "tokens": ["Den", "ei\u00b7nen", "hat", "er", "schon", "ge\u00b7lie\u00b7fert", "Got\u00b7tes", "H\u00e4n\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "PPER", "ADV", "VVPP", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Der neugebohrne ist ein eingesetztes Gut;", "tokens": ["Der", "neu\u00b7ge\u00b7bohr\u00b7ne", "ist", "ein", "ein\u00b7ge\u00b7setz\u00b7tes", "Gut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Der \u00e4ltste kan nunmehr in Port des Friedens lenden/", "tokens": ["Der", "\u00e4lts\u00b7te", "kan", "nun\u00b7mehr", "in", "Port", "des", "Frie\u00b7dens", "len\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VMFIN", "ADV", "APPR", "NE", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Da dessen Lebens-Schiff noch schwimmt auff w\u00fcster Fluth.", "tokens": ["Da", "des\u00b7sen", "Le\u00b7bens\u00b7Schiff", "noch", "schwimmt", "auff", "w\u00fcs\u00b7ter", "Fluth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRELAT", "NN", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Es mu\u00df/ ", "tokens": ["Es", "mu\u00df", "/"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VMFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.23": {"text": "Und di\u00df Betr\u00fcbn\u00fc\u00df ihm bi\u00df an die Seele gehn.", "tokens": ["Und", "di\u00df", "Be\u00b7tr\u00fcb\u00b7n\u00fc\u00df", "ihm", "bi\u00df", "an", "die", "See\u00b7le", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "NN", "PPER", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Vor schlechte Wunden sind Heilpflaster leicht zufinden/", "tokens": ["Vor", "schlech\u00b7te", "Wun\u00b7den", "sind", "Heil\u00b7pflas\u00b7ter", "leicht", "zu\u00b7fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Nicht wenn sie sich so sperr\u2019n und immer offen stehn.", "tokens": ["Nicht", "wenn", "sie", "sich", "so", "sperr'n", "und", "im\u00b7mer", "of\u00b7fen", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOUS", "PPER", "PRF", "ADV", "VVINF", "KON", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Wie wenn der letzte Herbst die G\u00e4rten gantz entkleidet", "tokens": ["Wie", "wenn", "der", "letz\u00b7te", "Herbst", "die", "G\u00e4r\u00b7ten", "gantz", "ent\u00b7klei\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOKOM", "KOUS", "ART", "ADJA", "NN", "ART", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Den Blumen ihren Rock und Purpur-Mantel nimmt;", "tokens": ["Den", "Blu\u00b7men", "ih\u00b7ren", "Rock", "und", "Pur\u00b7pur\u00b7Man\u00b7tel", "nimmt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Die Floraferner nicht der Menschen Auge weidet/", "tokens": ["Die", "Flo\u00b7ra\u00b7fer\u00b7ner", "nicht", "der", "Men\u00b7schen", "Au\u00b7ge", "wei\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Das Lust-St\u00fcck ohne Lust in Reiff\u2019 und Regen schwimmt:", "tokens": ["Das", "Lust\u00b7St\u00fcck", "oh\u00b7ne", "Lust", "in", "Reiff'", "und", "Re\u00b7gen", "schwimmt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "APPR", "NE", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Der G\u00e4rtner sich betr\u00fcbt/ da\u00df seine Blume welcken/", "tokens": ["Der", "G\u00e4rt\u00b7ner", "sich", "be\u00b7tr\u00fcbt", "/", "da\u00df", "sei\u00b7ne", "Blu\u00b7me", "wel\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVPP", "$(", "KOUS", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Die Lilgen untergehn/ die Rosen sind verblast;", "tokens": ["Die", "Lil\u00b7gen", "un\u00b7ter\u00b7gehn", "/", "die", "Ro\u00b7sen", "sind", "ver\u00b7blast", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$(", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Es lacht kein Hyacinth/ es riechen keine Nelcken/", "tokens": ["Es", "lacht", "kein", "Hya\u00b7cin\u00b7th", "/", "es", "rie\u00b7chen", "kei\u00b7ne", "Nel\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$(", "PPER", "VVFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Weil alle Liebligkeit des Winters Sichel fast:", "tokens": ["Weil", "al\u00b7le", "Lieb\u00b7lig\u00b7keit", "des", "Win\u00b7ters", "Si\u00b7chel", "fast", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ART", "NN", "NE", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "So glaub\u2019 ich da\u00df auch jetzt der beyden Eltern Seele", "tokens": ["So", "glaub'", "ich", "da\u00df", "auch", "jetzt", "der", "bey\u00b7den", "El\u00b7tern", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KOUS", "ADV", "ADV", "ART", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Den schmertzlichen Verlust mit heissen Seufftzen klagt;", "tokens": ["Den", "schmertz\u00b7li\u00b7chen", "Ver\u00b7lust", "mit", "heis\u00b7sen", "Seufft\u00b7zen", "klagt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Der hochgeliebte Sohn mu\u00df in de\u00df Grabes H\u00f6le/", "tokens": ["Der", "hoch\u00b7ge\u00b7lieb\u00b7te", "Sohn", "mu\u00df", "in", "de\u00df", "Gra\u00b7bes", "H\u00f6\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Die sch\u00f6ne Blume hat des Todes Wurm zernagt.", "tokens": ["Die", "sch\u00f6\u00b7ne", "Blu\u00b7me", "hat", "des", "To\u00b7des", "Wurm", "zer\u00b7nagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Er wuchs von guter Art/ gleich B\u00e4umen an den B\u00e4chen/", "tokens": ["Er", "wuchs", "von", "gu\u00b7ter", "Art", "/", "gleich", "B\u00e4u\u00b7men", "an", "den", "B\u00e4\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$(", "ADV", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Der Jahre Morgenr\u00f6th erhellte sich in GOtt.", "tokens": ["Der", "Jah\u00b7re", "Mor\u00b7gen\u00b7r\u00f6th", "er\u00b7hell\u00b7te", "sich", "in", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "So bald sein zarter Mund nur deutlich konte sprechen/", "tokens": ["So", "bald", "sein", "zar\u00b7ter", "Mund", "nur", "deut\u00b7lich", "kon\u00b7te", "spre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "ADJA", "NN", "ADV", "ADJD", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Lernt er des HErren Weg und heilige Gebot;", "tokens": ["Lernt", "er", "des", "Her\u00b7ren", "Weg", "und", "hei\u00b7li\u00b7ge", "Ge\u00b7bot", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Lie\u00df zu der Eltern Lust und tr\u00f6stlichem Behagen/", "tokens": ["Lie\u00df", "zu", "der", "El\u00b7tern", "Lust", "und", "tr\u00f6st\u00b7li\u00b7chem", "Be\u00b7ha\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Bald seinen Schulen-Flei\u00df in vollem Eyfer schaun.", "tokens": ["Bald", "sei\u00b7nen", "Schu\u00b7len\u00b7Flei\u00df", "in", "vol\u00b7lem", "Ey\u00b7fer", "schaun", "