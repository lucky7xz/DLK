{"dta.poem.11613": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Charwoche.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Du stimmst so ernst zu dieser Fr\u00fchlingswonne,", "tokens": ["Du", "stimmst", "so", "ernst", "zu", "die\u00b7ser", "Fr\u00fch\u00b7lings\u00b7won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du breitest im verj\u00fcngten Strahl der Sonne", "tokens": ["Du", "brei\u00b7test", "im", "ver\u00b7j\u00fcng\u00b7ten", "Strahl", "der", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des Kreuzes Schatten auf die lichte Erde,", "tokens": ["Des", "Kreu\u00b7zes", "Schat\u00b7ten", "auf", "die", "lich\u00b7te", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und senkest schweigend deine Fl\u00f6re nieder:", "tokens": ["Und", "sen\u00b7kest", "schwei\u00b7gend", "dei\u00b7ne", "Fl\u00f6\u00b7re", "nie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Fr\u00fchling darf indessen immer keimen,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "darf", "in\u00b7des\u00b7sen", "im\u00b7mer", "kei\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Veilchen duftet unter Bl\u00fcthenb\u00e4umen", "tokens": ["Das", "Veil\u00b7chen", "duf\u00b7tet", "un\u00b7ter", "Bl\u00fc\u00b7then\u00b7b\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und alle V\u00f6glein singen Jubellieder.", "tokens": ["Und", "al\u00b7le", "V\u00f6\u00b7glein", "sin\u00b7gen", "Ju\u00b7bel\u00b7lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O schweigt, ihr V\u00f6glein auf den gr\u00fcnen Auen!", "tokens": ["O", "schweigt", ",", "ihr", "V\u00f6\u00b7glein", "auf", "den", "gr\u00fc\u00b7nen", "Au\u00b7en", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es schallen rings die dumpfen Glockenkl\u00e4nge,", "tokens": ["Es", "schal\u00b7len", "rings", "die", "dum\u00b7pfen", "Glo\u00b7cken\u00b7kl\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Engel singen leise Grabges\u00e4nge:", "tokens": ["Die", "En\u00b7gel", "sin\u00b7gen", "lei\u00b7se", "Grab\u00b7ge\u00b7s\u00e4n\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "O still, ihr V\u00f6glein hoch im Himmelblauen!", "tokens": ["O", "still", ",", "ihr", "V\u00f6\u00b7glein", "hoch", "im", "Him\u00b7mel\u00b7blau\u00b7en", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PPOSAT", "NN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ihr Veilchen, kr\u00e4nzt heut keine Lockenhaare!", "tokens": ["Ihr", "Veil\u00b7chen", ",", "kr\u00e4nzt", "heut", "kei\u00b7ne", "Lo\u00b7cken\u00b7haa\u00b7re", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Euch pfl\u00fcckt mein frommes Kind zum dunkeln Strau\u00dfe,", "tokens": ["Euch", "pfl\u00fcckt", "mein", "from\u00b7mes", "Kind", "zum", "dun\u00b7keln", "Strau\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr wandert mit zum Muttergotteshause,", "tokens": ["Ihr", "wan\u00b7dert", "mit", "zum", "Mut\u00b7ter\u00b7got\u00b7tes\u00b7hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da sollt ihr welken auf des Herrn Altare.", "tokens": ["Da", "sollt", "ihr", "wel\u00b7ken", "auf", "des", "Herrn", "Al\u00b7ta\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ach dort, von Trauermelodieen trunken", "tokens": ["Ach", "dort", ",", "von", "Trau\u00b7er\u00b7me\u00b7lo\u00b7di\u00b7e\u00b7en", "trun\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "ADV", "$,", "APPR", "NN", "VVINF"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und s\u00fc\u00df bet\u00e4ubt von schweren Weihrauchd\u00fcften,", "tokens": ["Und", "s\u00fc\u00df", "be\u00b7t\u00e4ubt", "von", "schwe\u00b7ren", "Weih\u00b7rauch\u00b7d\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sucht sie den Br\u00e4utigam in Todesgr\u00fcften:", "tokens": ["Sucht", "sie", "den", "Br\u00e4u\u00b7ti\u00b7gam", "in", "To\u00b7des\u00b7gr\u00fcf\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NE", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Lieb' und Fr\u00fchling, Alles ist versunken!", "tokens": ["Und", "Lieb'", "und", "Fr\u00fch\u00b7ling", ",", "Al\u00b7les", "ist", "ver\u00b7sun\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$,", "PIS", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}