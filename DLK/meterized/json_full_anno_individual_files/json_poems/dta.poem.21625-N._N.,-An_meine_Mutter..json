{"dta.poem.21625": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "An meine Mutter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Der reinste Demant dieser Erde,", "tokens": ["Der", "reins\u00b7te", "De\u00b7mant", "die\u00b7ser", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das k\u00f6stlichste, das reichste Erz,", "tokens": ["Das", "k\u00f6st\u00b7lichs\u00b7te", ",", "das", "reichs\u00b7te", "Erz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die sch\u00f6nste Sonne aller Sonnen,", "tokens": ["Die", "sch\u00f6ns\u00b7te", "Son\u00b7ne", "al\u00b7ler", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es ist das treue Mutterherz!", "tokens": ["Es", "ist", "das", "treu\u00b7e", "Mut\u00b7ter\u00b7herz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O Herz so tief, so unergr\u00fcndlich,", "tokens": ["O", "Herz", "so", "tief", ",", "so", "un\u00b7er\u00b7gr\u00fcnd\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O Herz so wahr, so gut, so rein \u2014", "tokens": ["O", "Herz", "so", "wahr", ",", "so", "gut", ",", "so", "rein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O ewig wie der Weltenlenker", "tokens": ["O", "e\u00b7wig", "wie", "der", "Wel\u00b7ten\u00b7len\u00b7ker"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kann nur die Mutterliebe sein!", "tokens": ["Kann", "nur", "die", "Mut\u00b7ter\u00b7lie\u00b7be", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Selbsts\u00fcchtig ist sonst jede Liebe,", "tokens": ["Selbst\u00b7s\u00fcch\u00b7tig", "ist", "sonst", "je\u00b7de", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In ihrer Qual, in ihrem Gl\u00fcck,", "tokens": ["In", "ih\u00b7rer", "Qual", ",", "in", "ih\u00b7rem", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie giebt ihr Herz dir hin, doch fordert", "tokens": ["Sie", "giebt", "ihr", "Herz", "dir", "hin", ",", "doch", "for\u00b7dert"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PPER", "PTKVZ", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie auch dein Herz daf\u00fcr zur\u00fcck;", "tokens": ["Sie", "auch", "dein", "Herz", "da\u00b7f\u00fcr", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PPOSAT", "NN", "PAV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Nur einer Mutter gro\u00dfes Lieben", "tokens": ["Nur", "ei\u00b7ner", "Mut\u00b7ter", "gro\u00b7\u00dfes", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Giebt sich dem Kinde ganz dahin", "tokens": ["Giebt", "sich", "dem", "Kin\u00b7de", "ganz", "da\u00b7hin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "NN", "ADV", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und fordert nicht, o, schon das Geben", "tokens": ["Und", "for\u00b7dert", "nicht", ",", "o", ",", "schon", "das", "Ge\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "FM", "$,", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist \u00fcberreichlich ihr Gewinn.", "tokens": ["Ist", "\u00fc\u00b7berr\u00b7eich\u00b7lich", "ihr", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "O Mutterherz, o Mutterliebe,", "tokens": ["O", "Mut\u00b7ter\u00b7herz", ",", "o", "Mut\u00b7ter\u00b7lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wer kann dich hier ermessen doch,", "tokens": ["Wer", "kann", "dich", "hier", "er\u00b7mes\u00b7sen", "doch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du Herz, ob auch vom Kind gebrochen,", "tokens": ["Du", "Herz", ",", "ob", "auch", "vom", "Kind", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "KOUS", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Sterben segnest du es noch!", "tokens": ["Im", "Ster\u00b7ben", "seg\u00b7nest", "du", "es", "noch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}