{"dta.poem.19127": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Cv.  Auf seiner Tochter Nahmens-Tag.   Am 6. Jun.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Meine liebe Tochter!", "tokens": ["Mei\u00b7ne", "lie\u00b7be", "Toch\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Unser angejochter", "tokens": ["Un\u00b7ser", "an\u00b7ge\u00b7joch\u00b7ter"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und gebundner Hals", "tokens": ["Und", "ge\u00b7bund\u00b7ner", "Hals"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Tr\u00e4gt des Lammes Ketten,", "tokens": ["Tr\u00e4gt", "des", "Lam\u00b7mes", "Ket\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Um sich zu erretten", "tokens": ["Um", "sich", "zu", "er\u00b7ret\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "PRF", "PTKZU", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Von der Last des Falls.", "tokens": ["Von", "der", "Last", "des", "Falls", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Weil uns Licht", "tokens": ["Weil", "uns", "Licht"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Und Krafft gebricht:", "tokens": ["Und", "Krafft", "ge\u00b7bricht", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "So ersetzt des Feindes Lencken,", "tokens": ["So", "er\u00b7setzt", "des", "Fein\u00b7des", "Len\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Was wir nicht bedencken.", "tokens": ["Was", "wir", "nicht", "be\u00b7den\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Darum sind wir S\u00fcnder,", "tokens": ["Da\u00b7rum", "sind", "wir", "S\u00fcn\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Darum sind wir blinder", "tokens": ["Da\u00b7rum", "sind", "wir", "blin\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Als ein Maulwurff ist;", "tokens": ["Als", "ein", "Maul\u00b7wurff", "ist", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Wenn wir uns selbst f\u00fchren,", "tokens": ["Wenn", "wir", "uns", "selbst", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Und nicht gantz verlieren,", "tokens": ["Und", "nicht", "gantz", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Wenn nicht JEsus Christ,", "tokens": ["Wenn", "nicht", "Je\u00b7sus", "Christ", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "NE", "NE", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Und wen er", "tokens": ["Und", "wen", "er"], "token_info": ["word", "word", "word"], "pos": ["KON", "PWS", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Uns zum Leiter zugef\u00fchret,", "tokens": ["Uns", "zum", "Lei\u00b7ter", "zu\u00b7ge\u00b7f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Unsern Gang regieret.", "tokens": ["Un\u00b7sern", "Gang", "re\u00b7gie\u00b7ret", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Bleib dem HErrn gefangen,", "tokens": ["Bleib", "dem", "Herrn", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "St\u00fcrme mit Verlangen", "tokens": ["St\u00fcr\u00b7me", "mit", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "In die Gnade ein:", "tokens": ["In", "die", "Gna\u00b7de", "ein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "La\u00df nicht ab zu beten,", "tokens": ["La\u00df", "nicht", "ab", "zu", "be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Und zum HErrn zu treten,", "tokens": ["Und", "zum", "Herrn", "zu", "tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Lerne stille seyn.", "tokens": ["Ler\u00b7ne", "stil\u00b7le", "seyn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "VAINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Aber sey", "tokens": ["A\u00b7ber", "sey"], "token_info": ["word", "word"], "pos": ["KON", "VAFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Auch froh und frey,", "tokens": ["Auch", "froh", "und", "frey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Und bewahre dich vor Dingen,", "tokens": ["Und", "be\u00b7wah\u00b7re", "dich", "vor", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Die in Schwermuth bringen.", "tokens": ["Die", "in", "Schwer\u00b7muth", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Mir und meiner Frauen", "tokens": ["Mir", "und", "mei\u00b7ner", "Frau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "KON", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Gib mit Lust zu schauen", "tokens": ["Gib", "mit", "Lust", "zu", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "APPR", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Was der HErr gethan,", "tokens": ["Was", "der", "Herr", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Der dich von dem Grabe", "tokens": ["Der", "dich", "von", "dem", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "F\u00fcnff mal wieder gabe,", "tokens": ["F\u00fcnff", "mal", "wie\u00b7der", "ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Dem geh\u00f6rst du an:", "tokens": ["Dem", "ge\u00b7h\u00f6rst", "du", "an", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Lebe Jhm", "tokens": ["Le\u00b7be", "Jhm"], "token_info": ["word", "word"], "pos": ["VVIMP", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Und sing und r\u00fchm,", "tokens": ["Und", "sing", "und", "r\u00fchm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Da\u00df du eine von den Esthern,", "tokens": ["Da\u00df", "du", "ei\u00b7ne", "von", "den", "Est\u00b7hern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wie die M\u00e4hrschen Schwestern.", "tokens": ["Wie", "die", "M\u00e4hr\u00b7schen", "Schwes\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}