{"dta.poem.9518": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Klage.  \n C. H. v. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df nicht was ich thu/ ich wei\u00df nicht was ich bin/", "tokens": ["Ich", "wei\u00df", "nicht", "was", "ich", "thu", "/", "ich", "wei\u00df", "nicht", "was", "ich", "bin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PWS", "PPER", "VVFIN", "$(", "PPER", "VVFIN", "PTKNEG", "PWS", "PPER", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich wei\u00df nicht ob ich lieb/ ich wei\u00df nicht ob ich haffe/", "tokens": ["Ich", "wei\u00df", "nicht", "ob", "ich", "lieb", "/", "ich", "wei\u00df", "nicht", "ob", "ich", "haf\u00b7fe", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "KOUS", "PPER", "ADJD", "$(", "PPER", "VVFIN", "PTKNEG", "KOUS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es will mir/ Florida/ nicht leichtlich in den sinn/", "tokens": ["Es", "will", "mir", "/", "Flo\u00b7ri\u00b7da", "/", "nicht", "leicht\u00b7lich", "in", "den", "sinn", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "$(", "NE", "$(", "PTKNEG", "ADJD", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df ich dich lassen soll/ indem ich dich umfasse.", "tokens": ["Da\u00df", "ich", "dich", "las\u00b7sen", "soll", "/", "in\u00b7dem", "ich", "dich", "um\u00b7fas\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "VMFIN", "$(", "KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch lieb ich dich zu sehr/ so liebt dich diese nicht/", "tokens": ["Doch", "lieb", "ich", "dich", "zu", "sehr", "/", "so", "liebt", "dich", "die\u00b7se", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "PRF", "APPR", "ADV", "$(", "ADV", "VVFIN", "PPER", "PDS", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die unsern vorsatz kan erhalten und zerbrechen/", "tokens": ["Die", "un\u00b7sern", "vor\u00b7satz", "kan", "er\u00b7hal\u00b7ten", "und", "zer\u00b7bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VMFIN", "VVINF", "KON", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und hassen kan ich nicht mein auge und mein licht/", "tokens": ["Und", "has\u00b7sen", "kan", "ich", "nicht", "mein", "au\u00b7ge", "und", "mein", "licht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Viel eher wolt ich mir das hertze selbst durchstechen.", "tokens": ["Viel", "e\u00b7her", "wolt", "ich", "mir", "das", "hert\u00b7ze", "selbst", "durch\u00b7ste\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "PPER", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Zwey wege sind allhier/ und keiner ist nicht gut/", "tokens": ["Zwey", "we\u00b7ge", "sind", "all\u00b7hier", "/", "und", "kei\u00b7ner", "ist", "nicht", "gut", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "ADV", "$(", "KON", "PIS", "VAFIN", "PTKNEG", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Denn beyde sind geschickt mein hoffen zu zerst\u00f6ren;", "tokens": ["Denn", "bey\u00b7de", "sind", "ge\u00b7schickt", "mein", "hof\u00b7fen", "zu", "zer\u00b7st\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "VVPP", "PPOSAT", "VVINF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Es streiten ietzt in mir so wol vernunfft als blut/", "tokens": ["Es", "strei\u00b7ten", "ietzt", "in", "mir", "so", "wol", "ver\u00b7nunfft", "als", "blut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "ADV", "ADV", "VVFIN", "KOUS", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Doch wei\u00df ich nicht/ was ich zu erste solte h\u00f6ren.", "tokens": ["Doch", "wei\u00df", "ich", "nicht", "/", "was", "ich", "zu", "ers\u00b7te", "sol\u00b7te", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$(", "PWS", "PPER", "APPR", "ADJA", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich bin ein einsam schiff/ das wind und wellen treibt/", "tokens": ["Ich", "bin", "ein", "ein\u00b7sam", "schiff", "/", "das", "wind", "und", "wel\u00b7len", "treibt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "VVFIN", "$(", "PDS", "VAFIN", "KON", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So bey dem ruder auch den ancker hat verlohren/", "tokens": ["So", "bey", "dem", "ru\u00b7der", "auch", "den", "an\u00b7cker", "hat", "ver\u00b7loh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ich finde keinen port/ da mein gel\u00fccke bleibt/", "tokens": ["Ich", "fin\u00b7de", "kei\u00b7nen", "port", "/", "da", "mein", "ge\u00b7l\u00fc\u00b7cke", "bleibt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NE", "$(", "KOUS", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und schl\u00fcsse/ da\u00df man mich zum leiden hat gebohren.", "tokens": ["Und", "schl\u00fcs\u00b7se", "/", "da\u00df", "man", "mich", "zum", "lei\u00b7den", "hat", "ge\u00b7boh\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KOUS", "PIS", "PRF", "APPRART", "VVINF", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Di\u00df was mir endlich sch\u00e4rfft die \u00fcberh\u00e4uffte noth", "tokens": ["Di\u00df", "was", "mir", "end\u00b7lich", "sch\u00e4rfft", "die", "\u00fc\u00b7berh\u00b7\u00e4uff\u00b7te", "noth"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PRELS", "PPER", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ist/ da\u00df ich nechst bey dir/ dich werde meiden m\u00fcssen/", "tokens": ["Ist", "/", "da\u00df", "ich", "nechst", "bey", "dir", "/", "dich", "wer\u00b7de", "mei\u00b7den", "m\u00fcs\u00b7sen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$(", "KOUS", "PPER", "ADV", "APPR", "PPER", "$(", "PPER", "VAFIN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Da\u00df mich der hunger plagt/ immittelst korn und brod", "tokens": ["Da\u00df", "mich", "der", "hun\u00b7ger", "plagt", "/", "im\u00b7mit\u00b7telst", "korn", "und", "brod"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "VVFIN", "$(", "ADV", "VVINF", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und d\u00fcrrer durst verzehrt bey brunnen und bey fl\u00fcssen.", "tokens": ["Und", "d\u00fcr\u00b7rer", "durst", "ver\u00b7zehrt", "bey", "brun\u00b7nen", "und", "bey", "fl\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "VVPP", "APPR", "NN", "KON", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}