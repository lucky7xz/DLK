{"dta.poem.10427": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "L.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn zwei von einander scheiden,", "tokens": ["Wenn", "zwei", "von", "ein\u00b7an\u00b7der", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "CARD", "APPR", "PRF", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "So geben sie sich die H\u00e4nd',", "tokens": ["So", "ge\u00b7ben", "sie", "sich", "die", "H\u00e4nd'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und fangen an zu weinen,", "tokens": ["Und", "fan\u00b7gen", "an", "zu", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und seufzen ohne End'.", "tokens": ["Und", "seuf\u00b7zen", "oh\u00b7ne", "End'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wir haben nicht geweinet,", "tokens": ["Wir", "ha\u00b7ben", "nicht", "ge\u00b7wei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wir seufzten nicht Weh und Ach!", "tokens": ["Wir", "seufz\u00b7ten", "nicht", "Weh", "und", "Ach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "NN", "KON", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Thr\u00e4nen und die Seufzer,", "tokens": ["Die", "Thr\u00e4\u00b7nen", "und", "die", "Seuf\u00b7zer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die kamen hintennach.", "tokens": ["Die", "ka\u00b7men", "hin\u00b7ten\u00b7nach", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}