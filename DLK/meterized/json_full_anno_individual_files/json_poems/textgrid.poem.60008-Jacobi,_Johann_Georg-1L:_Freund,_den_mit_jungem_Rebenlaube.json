{"textgrid.poem.60008": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Freund, den mit jungem Rebenlaube", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freund, den mit jungem Rebenlaube", "tokens": ["Freund", ",", "den", "mit", "jun\u00b7gem", "Re\u00b7ben\u00b7lau\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Die sch\u00f6nste M\u00e4nas einst geschm\u00fcckt,", "tokens": ["Die", "sch\u00f6ns\u00b7te", "M\u00e4\u00b7nas", "einst", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem noch ein Amor selbst die Traube", "tokens": ["Dem", "noch", "ein", "A\u00b7mor", "selbst", "die", "Trau\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "ART", "NE", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In den bekr\u00e4nzten Becher dr\u00fcckt;", "tokens": ["In", "den", "be\u00b7kr\u00e4nz\u00b7ten", "Be\u00b7cher", "dr\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den Lust und Lenz in Haine rief,", "tokens": ["Den", "Lust", "und", "Lenz", "in", "Hai\u00b7ne", "rief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dem \u00fcberall Dryaden lachten,", "tokens": ["Dem", "\u00fc\u00b7be\u00b7rall", "Dry\u00b7a\u00b7den", "lach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dem sie ein Blumenlager machten", "tokens": ["Dem", "sie", "ein", "Blu\u00b7men\u00b7la\u00b7ger", "mach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "An Quellen, wo er sorglos schlief;", "tokens": ["An", "Quel\u00b7len", ",", "wo", "er", "sorg\u00b7los", "schlief", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Du willst, entfernt von unsern Ch\u00f6ren,", "tokens": ["Du", "willst", ",", "ent\u00b7fernt", "von", "un\u00b7sern", "Ch\u00f6\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Kein spr\u00f6des M\u00e4dchen mehr bekehren", "tokens": ["Kein", "spr\u00f6\u00b7des", "M\u00e4d\u00b7chen", "mehr", "be\u00b7keh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und deine Lieder singen h\u00f6ren?", "tokens": ["Und", "dei\u00b7ne", "Lie\u00b7der", "sin\u00b7gen", "h\u00f6\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Getilgt ist jeder Freude Spur;", "tokens": ["Ge\u00b7tilgt", "ist", "je\u00b7der", "Freu\u00b7de", "Spur", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Die Aue dorrt, es stirbt die Flur,", "tokens": ["Die", "Au\u00b7e", "dorrt", ",", "es", "stirbt", "die", "Flur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Wo Bosheit gift'gen Samen streut?", "tokens": ["Wo", "Bos\u00b7heit", "gift'\u00b7gen", "Sa\u00b7men", "streut", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Mit dem Verrathe geht der Neid;", "tokens": ["Mit", "dem", "Ver\u00b7ra\u00b7the", "geht", "der", "Neid", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Ihm schweigt der West an stummen B\u00e4chen;", "tokens": ["Ihm", "schweigt", "der", "West", "an", "stum\u00b7men", "B\u00e4\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Das Blumenbeet zertritt sein Fu\u00df,", "tokens": ["Das", "Blu\u00b7men\u00b7beet", "zer\u00b7tritt", "sein", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Und Saitenklang, und reinen Ku\u00df", "tokens": ["Und", "Sai\u00b7ten\u00b7klang", ",", "und", "rei\u00b7nen", "Ku\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "$,", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Der Freundschaft macht er zum Verbrechen?", "tokens": ["Der", "Freund\u00b7schaft", "macht", "er", "zum", "Ver\u00b7bre\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Mehr, als die W\u00fcste, schauerlich", "tokens": ["Mehr", ",", "als", "die", "W\u00fcs\u00b7te", ",", "schau\u00b7er\u00b7lich"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PIS", "$,", "KOUS", "ART", "NN", "$,", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Sind dir, o Gleim! die Lustgefilde", "tokens": ["Sind", "dir", ",", "o", "Gleim", "!", "die", "Lust\u00b7ge\u00b7fil\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "$,", "FM", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Der Jugend? Fern in seine wilde", "tokens": ["Der", "Ju\u00b7gend", "?", "Fern", "in", "sei\u00b7ne", "wil\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "NN", "APPR", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Verborgne H\u00f6hle ruft zu sich", "tokens": ["Ver\u00b7borg\u00b7ne", "H\u00f6h\u00b7le", "ruft", "zu", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Der finstre Menschenhasser dich? \u2013", "tokens": ["Der", "finst\u00b7re", "Men\u00b7schen\u00b7has\u00b7ser", "dich", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "O folge nie! denn Tugend wohnt,", "tokens": ["O", "fol\u00b7ge", "nie", "!", "denn", "Tu\u00b7gend", "wohnt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$.", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Gesehen noch und ungesehen,", "tokens": ["Ge\u00b7se\u00b7hen", "noch", "und", "un\u00b7ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "In Th\u00e4lern und auf Alpenh\u00f6hen;", "tokens": ["In", "Th\u00e4\u00b7lern", "und", "auf", "Al\u00b7pen\u00b7h\u00f6\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Oft hat das fr\u00f6mmste Werk den Mond", "tokens": ["Oft", "hat", "das", "fr\u00f6mms\u00b7te", "Werk", "den", "Mond"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Zum Zeugen nur, und Liebe lohnt", "tokens": ["Zum", "Zeu\u00b7gen", "nur", ",", "und", "Lie\u00b7be", "lohnt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "$,", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Der Treue noch mit Seligkeiten", "tokens": ["Der", "Treu\u00b7e", "noch", "mit", "Se\u00b7lig\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Aus l\u00e4ngst verschwundnen goldnen Zeiten.", "tokens": ["Aus", "l\u00e4ngst", "ver\u00b7schwund\u00b7nen", "gold\u00b7nen", "Zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Wen lockte sonst der Wiese Gr\u00fcn?", "tokens": ["Wen", "lock\u00b7te", "sonst", "der", "Wie\u00b7se", "Gr\u00fcn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Wem sollten jene Veilchen bl\u00fchn?", "tokens": ["Wem", "soll\u00b7ten", "je\u00b7ne", "Veil\u00b7chen", "bl\u00fchn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Dem Frevel nur? Ihm t\u00f6nten wieder", "tokens": ["Dem", "Fre\u00b7vel", "nur", "?", "Ihm", "t\u00f6n\u00b7ten", "wie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$.", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Aus blauer Luft die Lerchenlieder;", "tokens": ["Aus", "blau\u00b7er", "Luft", "die", "Ler\u00b7chen\u00b7lie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Und jenes Nachtigallenchor", "tokens": ["Und", "je\u00b7nes", "Nach\u00b7ti\u00b7gal\u00b7len\u00b7chor"], "token_info": ["word", "word", "word"], "pos": ["KON", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Erf\u00fcllte des Verbrechers Ohr?", "tokens": ["Er\u00b7f\u00fcll\u00b7te", "des", "Ver\u00b7bre\u00b7chers", "Ohr", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "O nein! geflochten von dem Lenze", "tokens": ["O", "nein", "!", "ge\u00b7floch\u00b7ten", "von", "dem", "Len\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PTKANT", "$.", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "Sind diese tausendfachen Kr\u00e4nze", "tokens": ["Sind", "die\u00b7se", "tau\u00b7send\u00b7fa\u00b7chen", "Kr\u00e4n\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "F\u00fcr schwarze H\u00f6llenthaten nicht.", "tokens": ["F\u00fcr", "schwar\u00b7ze", "H\u00f6l\u00b7len\u00b7tha\u00b7ten", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Da, wo die Weisheit Rosen flicht,", "tokens": ["Da", ",", "wo", "die", "Weis\u00b7heit", "Ro\u00b7sen", "flicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Will Zephyr gern das Thal erfrischen,", "tokens": ["Will", "Ze\u00b7phyr", "gern", "das", "Thal", "er\u00b7fri\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Da bildet, in vertrauten B\u00fcschen,", "tokens": ["Da", "bil\u00b7det", ",", "in", "ver\u00b7trau\u00b7ten", "B\u00fc\u00b7schen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "Die stille Grotte sich f\u00fcr sie;", "tokens": ["Die", "stil\u00b7le", "Grot\u00b7te", "sich", "f\u00fcr", "sie", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Da lehrt der V\u00f6gel Harmonie", "tokens": ["Da", "lehrt", "der", "V\u00f6\u00b7gel", "Har\u00b7mo\u00b7nie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Den frommen Dichter, sich erfreuen,", "tokens": ["Den", "from\u00b7men", "Dich\u00b7ter", ",", "sich", "er\u00b7freu\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.47": {"text": "Der Bosheit lachen, und verzeihen.", "tokens": ["Der", "Bos\u00b7heit", "la\u00b7chen", ",", "und", "ver\u00b7zei\u00b7hen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.48": {"text": "Hier, o mein Bester! wo, bedeckt", "tokens": ["Hier", ",", "o", "mein", "Bes\u00b7ter", "!", "wo", ",", "be\u00b7deckt"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "$,", "FM", "PPOSAT", "NN", "$.", "PWAV", "$,", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Mit Moos, die H\u00fctte sich versteckt,", "tokens": ["Mit", "Moos", ",", "die", "H\u00fct\u00b7te", "sich", "ver\u00b7steckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Hier tanzet, bey des Landmanns Festen,", "tokens": ["Hier", "tan\u00b7zet", ",", "bey", "des", "Land\u00b7manns", "Fes\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.51": {"text": "Mit Daphnis und mit Galathee", "tokens": ["Mit", "Daph\u00b7nis", "und", "mit", "Ga\u00b7la\u00b7thee"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.52": {"text": "Die Redlichkeit auf jungem Klee;", "tokens": ["Die", "Red\u00b7lich\u00b7keit", "auf", "jun\u00b7gem", "Klee", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.53": {"text": "Und dort, in schimmernden Pal\u00e4sten,", "tokens": ["Und", "dort", ",", "in", "schim\u00b7mern\u00b7den", "Pa\u00b7l\u00e4s\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.54": {"text": "Baut oft die Gro\u00dfmuth ihren Sitz;", "tokens": ["Baut", "oft", "die", "Gro\u00df\u00b7muth", "ih\u00b7ren", "Sitz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.55": {"text": "Herab vom Throne f\u00e4hrt der Blitz", "tokens": ["Her\u00b7ab", "vom", "Thro\u00b7ne", "f\u00e4hrt", "der", "Blitz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.56": {"text": "Auf das emp\u00f6rt