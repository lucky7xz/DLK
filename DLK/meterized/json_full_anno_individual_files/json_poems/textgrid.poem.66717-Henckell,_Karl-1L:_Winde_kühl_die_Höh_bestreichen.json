{"textgrid.poem.66717": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Winde k\u00fchl die H\u00f6h bestreichen", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Winde k\u00fchl die H\u00f6h bestreichen", "tokens": ["Win\u00b7de", "k\u00fchl", "die", "H\u00f6h", "be\u00b7strei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob des jungen Rheines Land,", "tokens": ["Ob", "des", "jun\u00b7gen", "Rhei\u00b7nes", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NE", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wei\u00dfe Wolkenfl\u00f6ckchen schleichen", "tokens": ["Wei\u00b7\u00dfe", "Wol\u00b7ken\u00b7fl\u00f6ck\u00b7chen", "schlei\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00fcd an grauer Felsenwand.", "tokens": ["M\u00fcd", "an", "grau\u00b7er", "Fel\u00b7sen\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Tal hat \u00fcberw\u00f6lkt sich leise,", "tokens": ["Tal", "hat", "\u00fc\u00b7berw\u00b7\u00f6lkt", "sich", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "VVFIN", "PRF", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "H\u00f6chste Kuppe sacht verh\u00fcllt,", "tokens": ["H\u00f6chs\u00b7te", "Kup\u00b7pe", "sacht", "ver\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Senkrecht \u00fcberm Strom die Kreise", "tokens": ["Sen\u00b7krecht", "\u00fc\u00b7berm", "Strom", "die", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Zieht ein Weih. \u2013 Sag, was erf\u00fcllt", "tokens": ["Zieht", "ein", "Weih", ".", "\u2013", "Sag", ",", "was", "er\u00b7f\u00fcllt"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$.", "$(", "NN", "$,", "PWS", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Dich mit abendwolkenweichen", "tokens": ["Dich", "mit", "ab\u00b7end\u00b7wol\u00b7ken\u00b7wei\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["PPER", "APPR", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Stimmungen, die von den H\u00f6hn", "tokens": ["Stim\u00b7mun\u00b7gen", ",", "die", "von", "den", "H\u00f6hn"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}, "line.11": {"text": "Zu den flachen, windm\u00fchlreichen", "tokens": ["Zu", "den", "fla\u00b7chen", ",", "wind\u00b7m\u00fchl\u00b7rei\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Ebenen herniedergehn?", "tokens": ["E\u00b7be\u00b7nen", "her\u00b7nie\u00b7der\u00b7gehn", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.13": {"text": "Bist auf wunderlicher Reise,", "tokens": ["Bist", "auf", "wun\u00b7der\u00b7li\u00b7cher", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Und du selbst begreifst sie kaum,", "tokens": ["Und", "du", "selbst", "be\u00b7greifst", "sie", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Grubst dein Bett auf eigne Weise,", "tokens": ["Grubst", "dein", "Bett", "auf", "eig\u00b7ne", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Suchst in Windungen dir Raum.", "tokens": ["Suchst", "in", "Win\u00b7dun\u00b7gen", "dir", "Raum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PPER", "NN", "$."], "meter": "+-++--+", "measure": "iambic.tetra.chol"}, "line.17": {"text": "Mu\u00df dich \u00f6der Strecken Lauf bedr\u00fccken?", "tokens": ["Mu\u00df", "dich", "\u00f6\u00b7der", "Stre\u00b7cken", "Lauf", "be\u00b7dr\u00fc\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJA", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "Bangt vor j\u00e4her Biegung dir das Herz?", "tokens": ["Bangt", "vor", "j\u00e4\u00b7her", "Bie\u00b7gung", "dir", "das", "Herz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.19": {"text": "Traubengold wird deine Ufer schm\u00fccken,", "tokens": ["Trau\u00b7ben\u00b7gold", "wird", "dei\u00b7ne", "U\u00b7fer", "schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "Festlich heitre Schiffe tr\u00e4gt dein R\u00fccken", "tokens": ["Fest\u00b7lich", "heit\u00b7re", "Schif\u00b7fe", "tr\u00e4gt", "dein", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.21": {"text": "Durch begabte Gaue nordmeerw\u00e4rts.", "tokens": ["Durch", "be\u00b7gab\u00b7te", "Gau\u00b7e", "nord\u00b7meer\u00b7w\u00e4rts", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.22": {"text": "Nebenwasser, die dein Wachstum sind,", "tokens": ["Ne\u00b7ben\u00b7was\u00b7ser", ",", "die", "dein", "Wachs\u00b7tum", "sind", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.23": {"text": "Haben Berge rechts und links zerrissen \u2013", "tokens": ["Ha\u00b7ben", "Ber\u00b7ge", "rechts", "und", "links", "zer\u00b7ris\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "KON", "ADV", "VVPP", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.24": {"text": "Die sich sch\u00e4umend durchs Gestein gebissen,", "tokens": ["Die", "sich", "sch\u00e4u\u00b7mend", "durchs", "Ge\u00b7stein", "ge\u00b7bis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.25": {"text": "Die Tamina braust aus Finsternissen", "tokens": ["Die", "Ta\u00b7mi\u00b7na", "braust", "aus", "Fins\u00b7ter\u00b7nis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "APPR", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.26": {"text": "Keck dir zu, ein ungest\u00fcmes Kind.", "tokens": ["Keck", "dir", "zu", ",", "ein", "un\u00b7ge\u00b7st\u00fc\u00b7mes", "Kind", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PTKVZ", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.27": {"text": "Sahst du sie die schroffen Kl\u00fcfte sprengen,", "tokens": ["Sahst", "du", "sie", "die", "schrof\u00b7fen", "Kl\u00fcf\u00b7te", "spren\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.28": {"text": "Wo aus Schatten Drachenleiber drohn?", "tokens": ["Wo", "aus", "Schat\u00b7ten", "Dra\u00b7chen\u00b7lei\u00b7ber", "drohn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "PAV", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.29": {"text": "Gr\u00fcne Buchenwimpel turmhoch h\u00e4ngen", "tokens": ["Gr\u00fc\u00b7ne", "Bu\u00b7chen\u00b7wim\u00b7pel", "turm\u00b7hoch", "h\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJD", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.30": {"text": "Ob der Schlucht, hellschimmernd wie ein Ton", "tokens": ["Ob", "der", "Schlucht", ",", "hell\u00b7schim\u00b7mernd", "wie", "ein", "Ton"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "$,", "ADJD", "KOKOM", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.31": {"text": "Aus des s\u00fc\u00dfen Lebens Lichtges\u00e4ngen ...?", "tokens": ["Aus", "des", "s\u00fc\u00b7\u00dfen", "Le\u00b7bens", "Licht\u00b7ge\u00b7s\u00e4n\u00b7gen", "...", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$(", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.32": {"text": "Ach, so flie\u00dfe nur hernieder", "tokens": ["Ach", ",", "so", "flie\u00b7\u00dfe", "nur", "her\u00b7nie\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "ADV", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Und erweitre deine Bahn,", "tokens": ["Und", "er\u00b7weit\u00b7re", "dei\u00b7ne", "Bahn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.34": {"text": "Machtest du doch Felsenglieder", "tokens": ["Mach\u00b7test", "du", "doch", "Fel\u00b7sen\u00b7glie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Deinem Willen untertan.", "tokens": ["Dei\u00b7nem", "Wil\u00b7len", "un\u00b7ter\u00b7tan", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.36": {"text": "Gibst dem breiten Marktverkehre", "tokens": ["Gibst", "dem", "brei\u00b7ten", "Markt\u00b7ver\u00b7keh\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Mit dem weiten Scho\u00df dich hin,", "tokens": ["Mit", "dem", "wei\u00b7ten", "Scho\u00df", "dich", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.38": {"text": "Doch im Tiefland bis zum Meere", "tokens": ["Doch", "im", "Tief\u00b7land", "bis", "zum", "Mee\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "APPR", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Wahrst du deiner Quellen Sinn.", "tokens": ["Wahrst", "du", "dei\u00b7ner", "Quel\u00b7len", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.40": {"text": "Sprudeln wie am tollsten Tage,", "tokens": ["Spru\u00b7deln", "wie", "am", "tolls\u00b7ten", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Sch\u00e4umen m\u00f6g' es da und dort \u2013", "tokens": ["Sch\u00e4u\u00b7men", "m\u00f6g'", "es", "da", "und", "dort", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "KON", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.42": {"text": "Trage, Strom der Seele, trage", "tokens": ["Tra\u00b7ge", ",", "Strom", "der", "See\u00b7le", ",", "tra\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "ART", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "Deine Jugend mit dir fort!", "tokens": ["Dei\u00b7ne", "Ju\u00b7gend", "mit", "dir", "fort", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Winde k\u00fchl die H\u00f6h bestreichen", "tokens": ["Win\u00b7de", "k\u00fchl", "die", "H\u00