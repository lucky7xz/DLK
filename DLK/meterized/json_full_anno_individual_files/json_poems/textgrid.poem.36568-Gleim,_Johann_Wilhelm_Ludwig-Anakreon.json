{"textgrid.poem.36568": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Anakreon", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer war Anakreon?", "tokens": ["Wer", "war", "A\u00b7nak\u00b7re\u00b7on", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Fragt' einstens Doris mich.", "tokens": ["Fragt'", "eins\u00b7tens", "Do\u00b7ris", "mich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er war, antwortet ich,", "tokens": ["Er", "war", ",", "ant\u00b7wor\u00b7tet", "ich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Er war ein Mann wie ich!", "tokens": ["Er", "war", "ein", "Mann", "wie", "ich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KOKOM", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er sang am Helikon,", "tokens": ["Er", "sang", "am", "He\u00b7li\u00b7kon", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Den Musen Liederchen,", "tokens": ["Den", "Mu\u00b7sen", "Lie\u00b7der\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.7": {"text": "Und k\u00fc\u00dfte Grazien,", "tokens": ["Und", "k\u00fc\u00df\u00b7te", "Gra\u00b7zi\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und war mit ihnen froh,", "tokens": ["Und", "war", "mit", "ih\u00b7nen", "froh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Und lebt' er noch, o so", "tokens": ["Und", "lebt'", "er", "noch", ",", "o", "so"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "FM", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Verlie\u00df er sie f\u00fcr dich.", "tokens": ["Ver\u00b7lie\u00df", "er", "sie", "f\u00fcr", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Z\u00f6g' aber Doris mich", "tokens": ["Z\u00f6g'", "a\u00b7ber", "Do\u00b7ris", "mich"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "KON", "NE", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Ihm vor, o Himmel! so", "tokens": ["Ihm", "vor", ",", "o", "Him\u00b7mel", "!", "so"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "PTKVZ", "$,", "FM", "NN", "$.", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "W\u00e4r' in Arkadien", "tokens": ["W\u00e4r'", "in", "Ar\u00b7ka\u00b7di\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "APPR", "NE"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.14": {"text": "Kein Sch\u00e4fer wohl so froh,", "tokens": ["Kein", "Sch\u00e4\u00b7fer", "wohl", "so", "froh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line