{"dta.poem.9740": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Eines Findlings.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich ward ein kind/ und doch ward ich nie kind genennet/", "tokens": ["Ich", "ward", "ein", "kind", "/", "und", "doch", "ward", "ich", "nie", "kind", "ge\u00b7nen\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$(", "KON", "ADV", "VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Weil ich die eltern nie und sie mich nicht erkennet.", "tokens": ["Weil", "ich", "die", "el\u00b7tern", "nie", "und", "sie", "mich", "nicht", "er\u00b7ken\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "KON", "PPER", "PRF", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Letzt nahm die erde mich/ als aller mutter/ auff/", "tokens": ["Letzt", "nahm", "die", "er\u00b7de", "mich", "/", "als", "al\u00b7ler", "mut\u00b7ter", "/", "auff", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "$(", "KOKOM", "PIAT", "NN", "$(", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich fand im sterben di\u00df/ was nicht im lebenslauff.", "tokens": ["Ich", "fand", "im", "ster\u00b7ben", "di\u00df", "/", "was", "nicht", "im", "le\u00b7bens\u00b7lauff", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word",