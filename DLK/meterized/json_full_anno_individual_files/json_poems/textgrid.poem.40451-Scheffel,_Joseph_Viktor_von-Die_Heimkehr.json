{"textgrid.poem.40451": {"metadata": {"author": {"name": "Scheffel, Joseph Viktor von", "birth": "N.A.", "death": "N.A."}, "title": "Die Heimkehr", "genre": "verse", "period": "N.A.", "pub_year": 1856, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Pfarrer von A\u00dfmannshausen sprach:", "tokens": ["Der", "Pfar\u00b7rer", "von", "A\u00df\u00b7manns\u00b7hau\u00b7sen", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u00bbdie Welt steckt tief in S\u00fcnden,", "tokens": ["\u00bb", "die", "Welt", "steckt", "tief", "in", "S\u00fcn\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch wo der Meister Josephus steckt,", "tokens": ["Doch", "wo", "der", "Meis\u00b7ter", "Jo\u00b7se\u00b7phus", "steckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Wei\u00df keiner mir zu k\u00fcnden.\u00ab", "tokens": ["Wei\u00df", "kei\u00b7ner", "mir", "zu", "k\u00fcn\u00b7den", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PIS", "PPER", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und als man r\u00fcstet auf Weihnachtzeit,", "tokens": ["Und", "als", "man", "r\u00fcs\u00b7tet", "auf", "Weih\u00b7nacht\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da war der Rhein gefroren,", "tokens": ["Da", "war", "der", "Rhein", "ge\u00b7fro\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NE", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da stund ein Mann in Pilgramskleid", "tokens": ["Da", "stund", "ein", "Mann", "in", "Pil\u00b7grams\u00b7kleid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wohl vor des Pfarrhofs Toren:", "tokens": ["Wohl", "vor", "des", "Pfarr\u00b7hofs", "To\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u00bbherr Pfarr', Ihr sollt mir Indulgenz", "tokens": ["\u00bb", "herr", "Pfarr'", ",", "Ihr", "sollt", "mir", "In\u00b7dul\u00b7genz"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "NN", "NE", "$,", "PPER", "VMFIN", "PPER", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sollt mir Abla\u00df spenden,", "tokens": ["Und", "sollt", "mir", "Ab\u00b7la\u00df", "spen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df sich mein arm tr\u00fcbtraurig Herz", "tokens": ["Da\u00df", "sich", "mein", "arm", "tr\u00fcb\u00b7trau\u00b7rig", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "PPOSAT", "ADJD", "ADJD", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Zu neuer Freud' mag wenden.", "tokens": ["Zu", "neu\u00b7er", "Freud'", "mag", "wen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Herr Pfarr', es war nicht wohlgetan,", "tokens": ["Herr", "Pfarr'", ",", "es", "war", "nicht", "wohl\u00b7ge\u00b7tan", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PPER", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vom rheinischen Land zu scheiden,", "tokens": ["Vom", "rhei\u00b7ni\u00b7schen", "Land", "zu", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Man trifft halt doch kein zweites an,", "tokens": ["Man", "trifft", "halt", "doch", "kein", "zwei\u00b7tes", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "VVFIN", "ADV", "PIAT", "ADJA", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So weit man auch mag reiten.", "tokens": ["So", "weit", "man", "auch", "mag", "rei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PIS", "ADV", "VMFIN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Bis hundert Stunden hinter Lyon", "tokens": ["Bis", "hun\u00b7dert", "Stun\u00b7den", "hin\u00b7ter", "Ly\u00b7on"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "APPR", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bin ich ins Frankreich kommen,", "tokens": ["Bin", "ich", "ins", "Fran\u00b7kreich", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NE", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Manch gutes Fr\u00fchst\u00fcck von Austern und Sekt", "tokens": ["Manch", "gu\u00b7tes", "Fr\u00fch\u00b7st\u00fcck", "von", "Aus\u00b7tern", "und", "Sekt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hab' ich zu mir genommen.", "tokens": ["Hab'", "ich", "zu", "mir", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ich hab' zu Marseille im Caf\u00e9 T\u00fcrk", "tokens": ["Ich", "hab'", "zu", "Mar\u00b7seil\u00b7le", "im", "Ca\u00b7f\u00e9", "T\u00fcrk"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NE", "APPRART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Unter Heiden und Mohren gesessen,", "tokens": ["Un\u00b7ter", "Hei\u00b7den", "und", "Moh\u00b7ren", "ge\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Ich hab' am Pyren\u00e4engebirg'", "tokens": ["Ich", "hab'", "am", "Py\u00b7re\u00b7n\u00e4\u00b7en\u00b7ge\u00b7bir\u00b7g'"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Lauch und Garbanzos gegessen.", "tokens": ["Lauch", "und", "Gar\u00b7ban\u00b7zos", "ge\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVPP", "$."], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Noch saust der Kopf mir, wenn ich gedenk'", "tokens": ["Noch", "saust", "der", "Kopf", "mir", ",", "wenn", "ich", "ge\u00b7denk'"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "$,", "KOUS", "PPER", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Seealpenmaid Filumene:", "tokens": ["Der", "See\u00b7al\u00b7pen\u00b7maid", "Fi\u00b7lu\u00b7me\u00b7ne", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zigeunerbraun Antlitz, kohlschwarzkraus Haar,", "tokens": ["Zi\u00b7geu\u00b7ner\u00b7braun", "Ant\u00b7litz", ",", "kohl\u00b7schwarz\u00b7kraus", "Haar", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Wie Elfenbein gl\u00e4nzend die Z\u00e4hne.", "tokens": ["Wie", "El\u00b7fen\u00b7bein", "gl\u00e4n\u00b7zend", "die", "Z\u00e4h\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.8": {"line.1": {"text": "Doch verpecht und verschwefelt ist alles Land", "tokens": ["Doch", "ver\u00b7pecht", "und", "ver\u00b7schwe\u00b7felt", "ist", "al\u00b7les", "Land"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVPP", "VAFIN", "PIAT", "NN"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Ohne Freunde und Lieder und Liebe;", "tokens": ["Oh\u00b7ne", "Freun\u00b7de", "und", "Lie\u00b7der", "und", "Lie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Vom Fieber gesch\u00fcttelt und abgebrannt", "tokens": ["Vom", "Fie\u00b7ber", "ge\u00b7sch\u00fct\u00b7telt", "und", "ab\u00b7ge\u00b7brannt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVPP", "KON", "VVPP"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Kehr' ich heim aus dem fremden Getriebe.\u00ab", "tokens": ["Kehr'", "ich", "heim", "aus", "dem", "frem\u00b7den", "Ge\u00b7trie\u00b7be", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Der Pfarr' von A\u00dfmannshausen sprach:", "tokens": ["Der", "Pfarr'", "von", "A\u00df\u00b7manns\u00b7hau\u00b7sen", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbwohlauf, bu\u00dffertige Seele,", "tokens": ["\u00bb", "wohl\u00b7auf", ",", "bu\u00df\u00b7fer\u00b7ti\u00b7ge", "See\u00b7le", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mit unserm altheiligen Purpurwein", "tokens": ["Mit", "un\u00b7serm", "al\u00b7thei\u00b7li\u00b7gen", "Pur\u00b7pur\u00b7wein"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Salbe dir Lippen und Kehle.", "tokens": ["Sal\u00b7be", "dir", "Lip\u00b7pen", "und", "Keh\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.10": {"line.1": {"text": "Zu demselbigen Wein drei Tag, drei Nacht", "tokens": ["Zu", "dem\u00b7sel\u00b7bi\u00b7gen", "Wein", "drei", "Tag", ",", "drei", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PDAT", "NN", "CARD", "NN", "$,", "CARD", "NN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "In dunkelen Keller dich schlie\u00dfe", "tokens": ["In", "dun\u00b7ke\u00b7len", "Kel\u00b7ler", "dich", "schlie\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und halt' bei den F\u00e4ssern trinkend Wacht,", "tokens": ["Und", "halt'", "bei", "den", "F\u00e4s\u00b7sern", "trin\u00b7kend", "Wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "VVPP", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da\u00df Gnade sich \u00fcber dich gie\u00dfe.", "tokens": ["Da\u00df", "Gna\u00b7de", "sich", "\u00fc\u00b7ber", "dich", "gie\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.11": {"line.1": {"text": "In Krone und Anker ergib dich sodann", "tokens": ["In", "Kro\u00b7ne", "und", "An\u00b7ker", "er\u00b7gib", "dich", "so\u00b7dann"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVIMP", "PPER", "ADV"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den geistlichen \u00dcbungen flei\u00dfig,", "tokens": ["Den", "geist\u00b7li\u00b7chen", "\u00dc\u00b7bun\u00b7gen", "flei\u00b7\u00dfig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und erst bei des n\u00e4chtlichen W\u00e4chters Nahn", "tokens": ["Und", "erst", "bei", "des", "n\u00e4cht\u00b7li\u00b7chen", "W\u00e4ch\u00b7ters", "Nahn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Dem Chorgesange entrei\u00df' dich.", "tokens": ["Dem", "Chor\u00b7ge\u00b7san\u00b7ge", "ent\u00b7rei\u00df'", "dich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "Dann wird der Himmel ein Zeichen tun,", "tokens": ["Dann", "wird", "der", "Him\u00b7mel", "ein", "Zei\u00b7chen", "tun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er l\u00e4\u00dft keinen B\u00fc\u00dfer verderben:", "tokens": ["Er", "l\u00e4\u00dft", "kei\u00b7nen", "B\u00fc\u00b7\u00dfer", "ver\u00b7der\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ein lichtes Weingr\u00fcn, ein dunkles Rot", "tokens": ["Ein", "lich\u00b7tes", "Wein\u00b7gr\u00fcn", ",", "ein", "dunk\u00b7les", "Rot"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Wird Nase und Stirn dir f\u00e4rben.", "tokens": ["Wird", "Na\u00b7se", "und", "Stirn", "dir", "f\u00e4r\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "PPER", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.13": {"line.1": {"text": "Und prangt dein Gesicht in solchem Ton,", "tokens": ["Und", "prangt", "dein", "Ge\u00b7sicht", "in", "sol\u00b7chem", "Ton", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dann wird dein Tr\u00fcbsinn sich hellen,", "tokens": ["Dann", "wird", "dein", "Tr\u00fcb\u00b7sinn", "sich", "hel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dann magst du, o lang' verlorener Sohn,", "tokens": ["Dann", "magst", "du", ",", "o", "lang'", "ver\u00b7lo\u00b7re\u00b7ner", "Sohn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "FM", "ADV", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Den alten Freunden dich stellen.", "tokens": ["Den", "al\u00b7ten", "Freun\u00b7den", "dich", "stel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.14": {"line.1": {"text": "Wir sind die Alten; noch klingen beim Wein", "tokens": ["Wir", "sind", "die", "Al\u00b7ten", ";", "noch", "klin\u00b7gen", "beim", "Wein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.re