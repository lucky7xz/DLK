{"textgrid.poem.45751": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[f\u00fcr Ida Lieben]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In R\u00f6merbad \u2013 denkst du noch dran? \u2013", "tokens": ["In", "R\u00f6\u00b7mer\u00b7bad", "\u2013", "denkst", "du", "noch", "dran", "?", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NE", "$(", "VVFIN", "PPER", "ADV", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Warst du ein Kind, und ich ein Mann.", "tokens": ["Warst", "du", "ein", "Kind", ",", "und", "ich", "ein", "Mann", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "KON", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun bist du M\u00e4dchen, Fr\u00e4ulein gar,", "tokens": ["Nun", "bist", "du", "M\u00e4d\u00b7chen", ",", "Fr\u00e4u\u00b7lein", "gar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ich ein Greis mit grauem Haar;", "tokens": ["Und", "ich", "ein", "Greis", "mit", "grau\u00b7em", "Haar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch schwebt dein Bild mir vor noch heut", "tokens": ["Doch", "schwebt", "dein", "Bild", "mir", "vor", "noch", "heut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "