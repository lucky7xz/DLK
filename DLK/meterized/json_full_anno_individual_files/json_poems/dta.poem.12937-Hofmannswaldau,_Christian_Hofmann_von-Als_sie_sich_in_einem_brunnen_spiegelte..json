{"dta.poem.12937": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als sie sich in einem brunnen  \n spiegelte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Climene wirfft den blick nur einem brunnen zu,", "tokens": ["Cli\u00b7me\u00b7ne", "wirfft", "den", "blick", "nur", "ei\u00b7nem", "brun\u00b7nen", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Und l\u00e4sset mich in guter ruh.", "tokens": ["Und", "l\u00e4s\u00b7set", "mich", "in", "gu\u00b7ter", "ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach aber, werthes Licht!", "tokens": ["Ach", "a\u00b7ber", ",", "wert\u00b7hes", "Licht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Denckst du denn an Narcissen nicht?", "tokens": ["Denckst", "du", "denn", "an", "Nar\u00b7cis\u00b7sen", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "La\u00df seinen fall dir doch die grufft verriegeln,", "tokens": ["La\u00df", "sei\u00b7nen", "fall", "dir", "doch", "die", "grufft", "ver\u00b7rie\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und schau nicht mehr hinein:", "tokens": ["Und", "schau", "nicht", "mehr", "hin\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Willst du dich aber sicher spiegeln;", "tokens": ["Willst", "du", "dich", "a\u00b7ber", "si\u00b7cher", "spie\u00b7geln", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So la\u00df mein nasses aug\u2019 an statt des brunnen seyn!", "tokens": ["So", "la\u00df", "mein", "nas\u00b7ses", "aug'", "an", "statt", "des", "brun\u00b7nen", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPOSAT", "ADJA", "NN", "APPR", "APPR", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}