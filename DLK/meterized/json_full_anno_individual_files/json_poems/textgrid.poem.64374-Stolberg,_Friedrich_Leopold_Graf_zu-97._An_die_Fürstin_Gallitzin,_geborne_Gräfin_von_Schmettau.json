{"textgrid.poem.64374": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "97. An die F\u00fcrstin Gallitzin, geborne Gr\u00e4fin von Schmettau", "genre": "verse", "period": "N.A.", "pub_year": 1794, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schwebe, mein Geist,", "tokens": ["Schwe\u00b7be", ",", "mein", "Geist", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "In der R\u00f6te dieses Morgens,", "tokens": ["In", "der", "R\u00f6\u00b7te", "die\u00b7ses", "Mor\u00b7gens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PDAT", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In der Feier dieses Tages;", "tokens": ["In", "der", "Fei\u00b7er", "die\u00b7ses", "Ta\u00b7ges", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Segen des Himmels umwallet ihn in gl\u00e4nzendem Strom!", "tokens": ["Se\u00b7gen", "des", "Him\u00b7mels", "um\u00b7wal\u00b7let", "ihn", "in", "gl\u00e4n\u00b7zen\u00b7dem", "Strom", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Sch\u00f6ner war nicht", "tokens": ["Sch\u00f6\u00b7ner", "war", "nicht"], "token_info": ["word", "word", "word"], "pos": ["NN", "VAFIN", "PTKNEG"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Sein \u00e4lterer Bruder,", "tokens": ["Sein", "\u00e4l\u00b7te\u00b7rer", "Bru\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Der die Neugeborne", "tokens": ["Der", "die", "Neu\u00b7ge\u00b7bor\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["ART", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Freundlich und froh", "tokens": ["Freund\u00b7lich", "und", "froh"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KON", "ADJD"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Nahm aus den Armen der gesegneten Nacht.", "tokens": ["Nahm", "aus", "den", "Ar\u00b7men", "der", "ge\u00b7se\u00b7gne\u00b7ten", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Er hielt sie empor,", "tokens": ["Er", "hielt", "sie", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Gen Himmel empor, in strahlendem Arm,", "tokens": ["Gen", "Him\u00b7mel", "em\u00b7por", ",", "in", "strah\u00b7len\u00b7dem", "Arm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PTKVZ", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und der Ewigkeit Segen", "tokens": ["Und", "der", "E\u00b7wig\u00b7keit", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Ums\u00e4uselten sanft das mitkundige Kind.", "tokens": ["Um\u00b7s\u00e4u\u00b7sel\u00b7ten", "sanft", "das", "mit\u00b7kun\u00b7di\u00b7ge", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Willkommen! Willkommen!", "tokens": ["Will\u00b7kom\u00b7men", "!", "Will\u00b7kom\u00b7men", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Erw\u00fcnschter Tag!", "tokens": ["Er\u00b7w\u00fcnschter", "Tag", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Dein wehender Flug", "tokens": ["Dein", "we\u00b7hen\u00b7der", "Flug"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Ist erfreuender, labender mir,", "tokens": ["Ist", "er\u00b7freu\u00b7en\u00b7der", ",", "la\u00b7ben\u00b7der", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "PRELS", "PPER", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Als selbst in Hesperiens G\u00e4rten mir war", "tokens": ["Als", "selbst", "in", "Hes\u00b7pe\u00b7ri\u00b7ens", "G\u00e4r\u00b7ten", "mir", "war"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "NE", "NN", "PPER", "VAFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Des Fr\u00fchlings Ku\u00df;", "tokens": ["Des", "Fr\u00fch\u00b7lings", "Ku\u00df", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Als in \u00c4tnas Thalen mir war", "tokens": ["Als", "in", "\u00c4t\u00b7nas", "Tha\u00b7len", "mir", "war"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NE", "NN", "PPER", "VAFIN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Duftende Wallung der k\u00fchlenden Luft.", "tokens": ["Duf\u00b7ten\u00b7de", "Wal\u00b7lung", "der", "k\u00fch\u00b7len\u00b7den", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.5": {"line.1": {"text": "Leben des Menschen,", "tokens": ["Le\u00b7ben", "des", "Men\u00b7schen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ein Wunder dem Forscher!", "tokens": ["Ein", "Wun\u00b7der", "dem", "For\u00b7scher", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Sich selber ein R\u00e4tsel,", "tokens": ["Sich", "sel\u00b7ber", "ein", "R\u00e4t\u00b7sel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "W\u00fcnschet, in Erde geh\u00fcllet, der Geist.", "tokens": ["W\u00fcn\u00b7schet", ",", "in", "Er\u00b7de", "ge\u00b7h\u00fcl\u00b7let", ",", "der", "Geist", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "NN", "VVFIN", "$,", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Sein Wunsch ist Befehl!", "tokens": ["Sein", "Wunsch", "ist", "Be\u00b7fehl", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Die umh\u00fcllende Erde gehorcht!", "tokens": ["Die", "um\u00b7h\u00fcl\u00b7len\u00b7de", "Er\u00b7de", "ge\u00b7horcht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.7": {"text": "Umg\u00fcrtet mit Kraft,", "tokens": ["Um\u00b7g\u00fcr\u00b7tet", "mit", "Kraft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Erhebt er den Arm;", "tokens": ["Er\u00b7hebt", "er", "den", "Arm", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Dienstbar wallet und steht", "tokens": ["Dienst\u00b7bar", "wal\u00b7let", "und", "steht"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "KON", "VVFIN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "Auf seine Befehle der Fu\u00df.", "tokens": ["Auf", "sei\u00b7ne", "Be\u00b7feh\u00b7le", "der", "Fu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Auf Wogen erschallender Luft", "tokens": ["Auf", "Wo\u00b7gen", "er\u00b7schal\u00b7len\u00b7der", "Luft"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "T\u00f6nt der Gedanke!", "tokens": ["T\u00f6nt", "der", "Ge\u00b7dan\u00b7ke", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Es schwebet auf Seufzern,", "tokens": ["Es", "schwe\u00b7bet", "auf", "Seuf\u00b7zern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Schwimmt in der Thr\u00e4ne,", "tokens": ["Schwimmt", "in", "der", "Thr\u00e4\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Strahlet in freudigem Blick", "tokens": ["Strah\u00b7let", "in", "freu\u00b7di\u00b7gem", "Blick"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.7": {"line.1": {"text": "Die Empfindung, und fleugt", "tokens": ["Die", "Emp\u00b7fin\u00b7dung", ",", "und", "fleugt"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "VVFIN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Wie Bienen von Blume zu Blume;", "tokens": ["Wie", "Bie\u00b7nen", "von", "Blu\u00b7me", "zu", "Blu\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Steigt wie der Adler", "tokens": ["Steigt", "wie", "der", "Ad\u00b7ler"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Gen Himmel empor!", "tokens": ["Gen", "Him\u00b7mel", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.8": {"line.1": {"text": "Sohn und Krone der Natur,", "tokens": ["Sohn", "und", "Kro\u00b7ne", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Steht in seiner Kraft", "tokens": ["Steht", "in", "sei\u00b7ner", "Kraft"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Der stolze Mensch,", "tokens": ["Der", "stol\u00b7ze", "Mensch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Bis ein Hauch der Luft,", "tokens": ["Bis", "ein", "Hauch", "der", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Der des Wurmes schonte,", "tokens": ["Der", "des", "Wur\u00b7mes", "schon\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Den Sohn und die Krone der Natur", "tokens": ["Den", "Sohn", "und", "die", "Kro\u00b7ne", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "St\u00fcrzet in Staub.", "tokens": ["St\u00fcr\u00b7zet", "in", "Staub", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.9": {"line.1": {"text": "Leben der Natur,", "tokens": ["Le\u00b7ben", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Bist nicht wahres Leben!", "tokens": ["Bist", "nicht", "wah\u00b7res", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Fr\u00f6nest dem Tode,", "tokens": ["Fr\u00f6\u00b7nest", "dem", "To\u00b7de", ","], "token_in