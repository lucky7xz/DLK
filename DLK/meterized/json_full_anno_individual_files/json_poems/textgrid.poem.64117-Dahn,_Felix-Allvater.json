{"textgrid.poem.64117": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Allvater", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es seufzt meine Seele in uns\u00e4glichem Jammer", "tokens": ["Es", "seufzt", "mei\u00b7ne", "See\u00b7le", "in", "un\u00b7s\u00e4g\u00b7li\u00b7chem", "Jam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Um des Schmerzengeschlechts, um der Menschen Geschick.", "tokens": ["Um", "des", "Schmer\u00b7zen\u00b7ge\u00b7schlechts", ",", "um", "der", "Men\u00b7schen", "Ge\u00b7schick", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$,", "KOUI", "ART", "NN", "NN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Denn was in der Welt von wechselndem Wehe", "tokens": ["Denn", "was", "in", "der", "Welt", "von", "wech\u00b7seln\u00b7dem", "We\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Brandend sich bricht in jeglicher Brust: \u2013", "tokens": ["Bran\u00b7dend", "sich", "bricht", "in", "jeg\u00b7li\u00b7cher", "Brust", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVPP", "PRF", "VVFIN", "APPR", "PIAT", "NN", "$.", "$("], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Mitempfinden, mitdurchk\u00e4mpfen,", "tokens": ["Mi\u00b7temp\u00b7fin\u00b7den", ",", "mit\u00b7durch\u00b7k\u00e4mp\u00b7fen", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mitdurchklagen mu\u00df ich es alles \u2013", "tokens": ["Mit\u00b7durch\u00b7kla\u00b7gen", "mu\u00df", "ich", "es", "al\u00b7les", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPER", "PIS", "$("], "meter": "+-+-++-+-", "measure": "unknown.measure.penta"}, "line.7": {"text": "Alles, alles: \u2013 denn gehei\u00dfen", "tokens": ["Al\u00b7les", ",", "al\u00b7les", ":", "\u2013", "denn", "ge\u00b7hei\u00b7\u00dfen"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word"], "pos": ["PIS", "$,", "PIS", "$.", "$(", "KON", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bin ich Allvater:", "tokens": ["Bin", "ich", "All\u00b7va\u00b7ter", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Bald des besiegten bessern Mannes,", "tokens": ["Bald", "des", "be\u00b7sieg\u00b7ten", "bes\u00b7sern", "Man\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Den ein B\u00f6ser bezwungen,", "tokens": ["Den", "ein", "B\u00f6\u00b7ser", "be\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.11": {"text": "Bitter bei\u00dfenden Seelenbrand,", "tokens": ["Bit\u00b7ter", "bei\u00b7\u00dfen\u00b7den", "See\u00b7len\u00b7brand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.12": {"text": "Wie er grollend in Todesgram", "tokens": ["Wie", "er", "grol\u00b7lend", "in", "To\u00b7des\u00b7gram"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADJD", "APPR", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.13": {"text": "Flucht dem grausamen Schicksal: \u2013", "tokens": ["Flucht", "dem", "grau\u00b7sa\u00b7men", "Schick\u00b7sal", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+----", "measure": "unknown.measure.di"}, "line.14": {"text": "Bald des Liebenden t\u00f6dlich Leid,", "tokens": ["Bald", "des", "Lie\u00b7ben\u00b7den", "t\u00f6d\u00b7lich", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.15": {"text": "Der in leere Luft mit den Armen langt,", "tokens": ["Der", "in", "lee\u00b7re", "Luft", "mit", "den", "Ar\u00b7men", "langt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Dem langsam das Leben verlodert", "tokens": ["Dem", "lang\u00b7sam", "das", "Le\u00b7ben", "ver\u00b7lo\u00b7dert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ART", "NN", "VVPP"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.17": {"text": "An nie verl\u00f6schender Sehnsucht Licht: \u2013", "tokens": ["An", "nie", "ver\u00b7l\u00f6\u00b7schen\u00b7der", "Sehn\u00b7sucht", "Licht", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "NN", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Und der Witwe Wehklage,", "tokens": ["Und", "der", "Wit\u00b7we", "Weh\u00b7kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$,"], "meter": "--+-+--", "measure": "anapaest.init"}, "line.19": {"text": "Der Waisen Weinen", "tokens": ["Der", "Wai\u00b7sen", "Wei\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.20": {"text": "Und der versinkenden Seele", "tokens": ["Und", "der", "ver\u00b7sin\u00b7ken\u00b7den", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.21": {"text": "Letzten schrillen Verzweiflungsschrei: \u2013", "tokens": ["Letz\u00b7ten", "schril\u00b7len", "Ver\u00b7zwei\u00b7flungs\u00b7schrei", ":", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADJA", "ADJA", "NN", "$.", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.22": {"text": "All' dies Elend, \u00f6d' und endlos,", "tokens": ["All'", "dies", "E\u00b7lend", ",", "\u00f6d'", "und", "end\u00b7los", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "PDS", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Es empfindet's mit Allvater.", "tokens": ["Es", "emp\u00b7fin\u00b7det's", "mit", "All\u00b7va\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Und wie wenig wollen dawider", "tokens": ["Und", "wie", "we\u00b7nig", "wol\u00b7len", "da\u00b7wi\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PIS", "VMFIN", "PAV"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.25": {"text": "Ach die winzigen", "tokens": ["Ach", "die", "win\u00b7zi\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ITJ", "ART", "ADJA"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.26": {"text": "Wonnen wiegen,", "tokens": ["Won\u00b7nen", "wie\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.27": {"text": "Die wie verwehte Rosenbl\u00e4tter", "tokens": ["Die", "wie", "ver\u00b7weh\u00b7te", "Ro\u00b7sen\u00b7bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Wogen auf weiten, weiten Wellen,", "tokens": ["Wo\u00b7gen", "auf", "wei\u00b7ten", ",", "wei\u00b7ten", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.29": {"text": "Auf des Weh's unendlichem Ozean. \u2013", "tokens": ["Auf", "des", "Weh's", "un\u00b7end\u00b7li\u00b7chem", "O\u00b7ze\u00b7an", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$.", "$("], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.30": {"text": "Traun, ", "tokens": ["Traun", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.31": {"text": "Ein Ziel ist gezeichnet den zahllosen Z\u00e4hren,", "tokens": ["Ein", "Ziel", "ist", "ge\u00b7zeich\u00b7net", "den", "zahl\u00b7lo\u00b7sen", "Z\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+++-+-", "measure": "iambic.hexa.relaxed"}, "line.32": {"text": "Eine Endezeit.", "tokens": ["Ei\u00b7ne", "En\u00b7de\u00b7zeit", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.33": {"text": "Ich segne den Tag, da der sengende Surtur", "tokens": ["Ich", "seg\u00b7ne", "den", "Tag", ",", "da", "der", "sen\u00b7gen\u00b7de", "Sur\u00b7tur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "Erbarmend der letzten Menschen Gebilde", "tokens": ["Er\u00b7bar\u00b7mend", "der", "letz\u00b7ten", "Men\u00b7schen", "Ge\u00b7bil\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "ART", "ADJA", "NN", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.35": {"text": "Zugleich mit der m\u00fcden Erde zermalmt,", "tokens": ["Zu\u00b7gleich", "mit", "der", "m\u00fc\u00b7den", "Er\u00b7de", "zer\u00b7malmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.36": {"text": "Da endlich der Quell unersch\u00f6pflicher Qualen", "tokens": ["Da", "end\u00b7lich", "der", "Quell", "un\u00b7er\u00b7sch\u00f6pf\u00b7li\u00b7cher", "Qua\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.37": {"text": "Versiegt: das letzte menschliche Herz.", "tokens": ["Ver\u00b7siegt", ":", "das", "letz\u00b7te", "menschli\u00b7che", "Herz", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$.", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Willkommen dem Tag! \u2013 Und w\u00e4ren sie weise,", "tokens": ["Will\u00b7kom\u00b7men", "dem", "Tag", "!", "\u2013", "Und", "w\u00e4\u00b7ren", "sie", "wei\u00b7se", ","], "token_info": ["word", "word", "word", "punct",