{"textgrid.poem.66097": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Das Birkenb\u00e4umchen", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df den Tag, es war wie heute,", "tokens": ["Ich", "wei\u00df", "den", "Tag", ",", "es", "war", "wie", "heu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VAFIN", "KOKOM", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein erster Maitag, weich und mild,", "tokens": ["Ein", "ers\u00b7ter", "Mai\u00b7tag", ",", "weich", "und", "mild", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und die erwachten Augen freute", "tokens": ["Und", "die", "er\u00b7wach\u00b7ten", "Au\u00b7gen", "freu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das \u00fcbersonnte Morgenbild.", "tokens": ["Das", "\u00fc\u00b7ber\u00b7sonn\u00b7te", "Mor\u00b7gen\u00b7bild", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der frohe Blick lief hin und wieder,", "tokens": ["Der", "fro\u00b7he", "Blick", "lief", "hin", "und", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "KON", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie sammelt er die Sch\u00e4tze blo\u00