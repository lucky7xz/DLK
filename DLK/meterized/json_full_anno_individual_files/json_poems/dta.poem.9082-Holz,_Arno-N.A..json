{"dta.poem.9082": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1898", "urn": "urn:nbn:de:kobv:b4-200905192685", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin der reichste Mann der Welt!", "tokens": ["Ich", "bin", "der", "reichs\u00b7te", "Mann", "der", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Meine silbernen Yachten", "tokens": ["Mei\u00b7ne", "sil\u00b7ber\u00b7nen", "Y\u00b7ach\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "schwimmen auf allen Meeren.", "tokens": ["schwim\u00b7men", "auf", "al\u00b7len", "Mee\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Goldne Villen glitzern durch meine W\u00e4lder in Japan,", "tokens": ["Gold\u00b7ne", "Vil\u00b7len", "glit\u00b7zern", "durch", "mei\u00b7ne", "W\u00e4l\u00b7der", "in", "Ja\u00b7pan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "APPR", "NE", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "in himmelhohen Alpenseeen spiegeln sich meine Schl\u00f6sser,", "tokens": ["in", "him\u00b7mel\u00b7ho\u00b7hen", "Al\u00b7pen\u00b7se\u00b7e\u00b7en", "spie\u00b7geln", "sich", "mei\u00b7ne", "Schl\u00f6s\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PRF", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+-+--+-+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "auf tausend Inseln h\u00e4ngen meine purpurnen G\u00e4rten.", "tokens": ["auf", "tau\u00b7send", "In\u00b7seln", "h\u00e4n\u00b7gen", "mei\u00b7ne", "pur\u00b7pur\u00b7nen", "G\u00e4r\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Ich beachte sie kaum.", "tokens": ["Ich", "be\u00b7ach\u00b7te", "sie", "kaum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "An ihren aus Bronze gewundenen Schlangengittern", "tokens": ["An", "ih\u00b7ren", "aus", "Bron\u00b7ze", "ge\u00b7wun\u00b7de\u00b7nen", "Schlan\u00b7gen\u00b7git\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "APPR", "NN", "ADJA", "NN"], "meter": "-+--+--+--+-+-", "measure": "amphibrach.tetra.plus"}, "line.2": {"text": "geh ich vorbei,", "tokens": ["geh", "ich", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "\u00fcber meine Diamantgruben", "tokens": ["\u00fc\u00b7ber", "mei\u00b7ne", "Di\u00b7a\u00b7mant\u00b7gru\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "lass ich die L\u00e4mmer grasen.", "tokens": ["lass", "ich", "die", "L\u00e4m\u00b7mer", "gra\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die Sonne scheint,", "tokens": ["Die", "Son\u00b7ne", "scheint", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "ein Vogel singt,", "tokens": ["ein", "Vo\u00b7gel", "singt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "ich b\u00fccke mich", "tokens": ["ich", "b\u00fc\u00b7cke", "mich"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "und pfl\u00fccke eine kleine Wiesenblume.", "tokens": ["und", "pfl\u00fc\u00b7cke", "ei\u00b7ne", "klei\u00b7ne", "Wie\u00b7sen\u00b7blu\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Und pl\u00f6tzlich weiss ich: ich bin der \u00e4rmste Bettler!", "tokens": ["Und", "pl\u00f6tz\u00b7lich", "weiss", "ich", ":", "ich", "bin", "der", "\u00e4rms\u00b7te", "Bett\u00b7ler", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "$.", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "Ein Nichts ist meine ganze Herrlichkeit", "tokens": ["Ein", "Nichts", "ist", "mei\u00b7ne", "gan\u00b7ze", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "vor diesem Thautropfen,", "tokens": ["vor", "die\u00b7sem", "Thau\u00b7trop\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.3": {"text": "der in der Sonne funkelt.", "tokens": ["der", "in", "der", "Son\u00b7ne", "fun\u00b7kelt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}