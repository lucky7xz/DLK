{"dta.poem.3860": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen Musicum.  \n  J. A. Steinmetz.  1744.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Das Sterben kennt nicht Tact und Noten,", "tokens": ["Das", "Ster\u00b7ben", "kennt", "nicht", "Tact", "und", "No\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich war ein guter Musicus,", "tokens": ["Ich", "war", "ein", "gu\u00b7ter", "Mu\u00b7si\u00b7cus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und doch bin ich im Reich der Todten,", "tokens": ["Und", "doch", "bin", "ich", "im", "Reich", "der", "Tod\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Leben gehet zum Beschlu\u00df.", "tokens": ["Mein", "Le\u00b7ben", "ge\u00b7het", "zum", "Be\u00b7schlu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Jhr Eltern, euer lieber Sohn", "tokens": ["Ihr", "El\u00b7tern", ",", "eu\u00b7er", "lie\u00b7ber", "Sohn"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "H\u00f6rt itzo einen h\u00f6hern Thon.", "tokens": ["H\u00f6rt", "it\u00b7zo", "ei\u00b7nen", "h\u00f6\u00b7hern", "Thon", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Drum wischt die Jammer-Z\u00e4hren ab,", "tokens": ["Drum", "wischt", "die", "Jam\u00b7mer\u00b7Z\u00e4h\u00b7ren", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und g\u00f6nnet ihm das sanffte Grab.", "tokens": ["Und", "g\u00f6n\u00b7net", "ihm", "das", "sanff\u00b7te", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}