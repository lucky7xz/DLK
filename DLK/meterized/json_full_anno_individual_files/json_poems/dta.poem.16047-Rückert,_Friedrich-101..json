{"dta.poem.16047": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "101.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Des armen Menschen Gl\u00fcck ist meistens ein Vermeiden", "tokens": ["Des", "ar\u00b7men", "Men\u00b7schen", "Gl\u00fcck", "ist", "meis\u00b7tens", "ein", "Ver\u00b7mei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Ungl\u00fccks, seine Lust Abwesenheit der Leiden.", "tokens": ["Des", "Un\u00b7gl\u00fccks", ",", "sei\u00b7ne", "Lust", "Ab\u00b7we\u00b7sen\u00b7heit", "der", "Lei\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Verderben droht, und weicht, frei hebt er seine Brust,", "tokens": ["Ver\u00b7der\u00b7ben", "droht", ",", "und", "weicht", ",", "frei", "hebt", "er", "sei\u00b7ne", "Brust", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "KON", "VVFIN", "$,", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das nennt er dann sein Gl\u00fcck, das nennt er seine Lust.", "tokens": ["Das", "nennt", "er", "dann", "sein", "Gl\u00fcck", ",", "das", "nennt", "er", "sei\u00b7ne", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PPO