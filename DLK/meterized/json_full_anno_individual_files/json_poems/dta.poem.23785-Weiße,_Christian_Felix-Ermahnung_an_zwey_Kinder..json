{"dta.poem.23785": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "Ermahnung an zwey  \n Kinder.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20643-5", "language": ["de:0.99"], "booktitle": "Wei\u00dfe, Christian Felix: Lieder f\u00fcr Kinder. Leipzig, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "S\u00fc\u00dfes M\u00e4dchen, holder Knabe!", "tokens": ["S\u00fc\u00b7\u00dfes", "M\u00e4d\u00b7chen", ",", "hol\u00b7der", "Kna\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spielt nur, spielt in meinem Schoo\u00df!", "tokens": ["Spielt", "nur", ",", "spielt", "in", "mei\u00b7nem", "Schoo\u00df", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn ich Euch in Armen habe,", "tokens": ["Wenn", "ich", "Euch", "in", "Ar\u00b7men", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bin ich wie ein K\u00f6nig gro\u00df.", "tokens": ["Bin", "ich", "wie", "ein", "K\u00f6\u00b7nig", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "KOKOM", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Euer Stammeln, Euer Lallen,", "tokens": ["Eu\u00b7er", "Stam\u00b7meln", ",", "Eu\u00b7er", "Lal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist f\u00fcr mich Beredsamkeit:", "tokens": ["Ist", "f\u00fcr", "mich", "Be\u00b7red\u00b7sam\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Euer Wunsch, mir zu gefallen,", "tokens": ["Eu\u00b7er", "Wunsch", ",", "mir", "zu", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wollust und Zufriedenhelt.", "tokens": ["Wol\u00b7lust", "und", "Zu\u00b7frie\u00b7den\u00b7helt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn mich Eure H\u00e4ndchen streicheln,", "tokens": ["Wenn", "mich", "Eu\u00b7re", "H\u00e4nd\u00b7chen", "strei\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sanft mir Euer Auge lacht:", "tokens": ["Sanft", "mir", "Eu\u00b7er", "Au\u00b7ge", "lacht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "O so hab ich auf das Schmeicheln", "tokens": ["O", "so", "hab", "ich", "auf", "das", "Schmei\u00b7cheln"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einer ganzen Welt nicht Acht.", "tokens": ["Ei\u00b7ner", "gan\u00b7zen", "Welt", "nicht", "Acht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "CARD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gern misch ich in Eure Spiele", "tokens": ["Gern", "misch", "ich", "in", "Eu\u00b7re", "Spie\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mich mit froher Nachsicht ein.", "tokens": ["Mich", "mit", "fro\u00b7her", "Nach\u00b7sicht", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "O des Gl\u00fccks! da\u00df ich dann f\u00fchle", "tokens": ["O", "des", "Gl\u00fccks", "!", "da\u00df", "ich", "dann", "f\u00fch\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "$.", "KOUS", "PPER", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wieder einmal Kind zu seyn.", "tokens": ["Wie\u00b7der", "ein\u00b7mal", "Kind", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ja, geliebte, zarte Beyde,", "tokens": ["Ja", ",", "ge\u00b7lieb\u00b7te", ",", "zar\u00b7te", "Bey\u00b7de", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tausendmal umarm ich Euch!", "tokens": ["Tau\u00b7send\u00b7mal", "um\u00b7arm", "ich", "Euch", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jmmerdar sey Eure Freude", "tokens": ["Jm\u00b7mer\u00b7dar", "sey", "Eu\u00b7re", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eurer jetzgen Freude, gleich.", "tokens": ["Eu\u00b7rer", "jetz\u00b7gen", "Freu\u00b7de", ",", "gleich", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Unschuld wohn in Euern Herzen,", "tokens": ["Un\u00b7schuld", "wohn", "in", "Eu\u00b7ern", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Keine Bosheit komm in sie!", "tokens": ["Kei\u00b7ne", "Bos\u00b7heit", "komm", "in", "sie", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Jhr k\u00f6nnt singen, tanzen, scherzen,", "tokens": ["Ihr", "k\u00f6nnt", "sin\u00b7gen", ",", "tan\u00b7zen", ",", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nur verscherzt die Tugend nie!", "tokens": ["Nur", "ver\u00b7scherzt", "die", "Tu\u00b7gend", "nie", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}