{"textgrid.poem.44188": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "[davids Harfe, gro\u00dfer Mann, die dein Finger k\u00fcnstlich spielet]", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Davids Harfe, gro\u00dfer Mann, die dein Finger k\u00fcnstlich spielet,", "tokens": ["Da\u00b7vids", "Har\u00b7fe", ",", "gro\u00b7\u00dfer", "Mann", ",", "die", "dein", "Fin\u00b7ger", "k\u00fcnst\u00b7lich", "spie\u00b7let", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wenn die Andacht deiner Brust einen Trieb von oben f\u00fchlet,", "tokens": ["Wenn", "die", "An\u00b7dacht", "dei\u00b7ner", "Brust", "ei\u00b7nen", "Trieb", "von", "o\u00b7ben", "f\u00fch\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN", "ART", "NN", "APPR", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Warnet jezo meine Leyer, da mein Phoebus als ein Kind", "tokens": ["War\u00b7net", "je\u00b7zo", "mei\u00b7ne", "Le\u00b7yer", ",", "da", "mein", "Phoe\u00b7bus", "als", "ein", "Kind"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "$,", "KOUS", "PPOSAT", "NN", "KOKOM", "ART", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Deiner mir erwiesnen G\u00fcte auf ein reines Dancklied sinnt.", "tokens": ["Dei\u00b7ner", "mir", "er\u00b7wies\u00b7nen", "G\u00fc\u00b7te", "auf", "ein", "rei\u00b7nes", "Danck\u00b7lied", "sinnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "PPER", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Ich durchgeh den Helicon, einen Baum vor dich zu finden,", "tokens": ["Ich", "durch\u00b7geh", "den", "He\u00b7li\u00b7con", ",", "ei\u00b7nen", "Baum", "vor", "dich", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Ich verwunde manchen Ast, ich zerrize manche Rinden,", "tokens": ["Ich", "ver\u00b7wun\u00b7de", "man\u00b7chen", "Ast", ",", "ich", "zer\u00b7ri\u00b7ze", "man\u00b7che", "Rin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Das Ged\u00e4chtn\u00fc\u00df deines Nahmens der Verwesung zu entziehn;", "tokens": ["Das", "Ge\u00b7d\u00e4cht\u00b7n\u00fc\u00df", "dei\u00b7nes", "Nah\u00b7mens", "der", "Ver\u00b7we\u00b7sung", "zu", "ent\u00b7ziehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Aber ach, der stumpfe Grifel schilt mein th\u00f6richtes Bem\u00fchn.", "tokens": ["A\u00b7ber", "ach", ",", "der", "stump\u00b7fe", "Gri\u00b7fel", "schilt", "mein", "th\u00f6\u00b7rich\u00b7tes", "Be\u00b7m\u00fchn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "ART", "ADJA", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+---+", "measure": "unknown.measure.septa"}, "line.9": {"text": "Mir ger\u00e4th kein guter Strich, sondern lauter falsche Z\u00fcge,", "tokens": ["Mir", "ge\u00b7r\u00e4\u00b7th", "kein", "gu\u00b7ter", "Strich", ",", "son\u00b7dern", "lau\u00b7ter", "fal\u00b7sche", "Z\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN", "$,", "KON", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+--+-++-+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.10": {"text": "Da\u00df ich selbst aus Ungedult wieder mich zu Felde liege;", "tokens": ["Da\u00df", "ich", "selbst", "aus", "Un\u00b7ge\u00b7dult", "wie\u00b7der", "mich", "zu", "Fel\u00b7de", "lie\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "ADV", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+--+----+-+-+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Und das Wort Ber\u00fchmter S(chmolcke) kommt der Feder sauer an,", "tokens": ["Und", "das", "Wort", "Be\u00b7r\u00fchm\u00b7ter", "S", "(", "chmol\u00b7cke", ")", "kommt", "der", "Fe\u00b7der", "sau\u00b7er", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NE", "NE", "$(", "NE", "$(", "VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "--+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.12": {"text": "Weil es selbst die schwache Zunge langsam buchstabiren kan.", "tokens": ["Weil", "es", "selbst", "die", "schwa\u00b7che", "Zun\u00b7ge", "lang\u00b7sam", "buch\u00b7sta\u00b7bi\u00b7ren", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "ADJD", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.13": {"text": "Wundre dich nun also nicht, da\u00df ich deinen Ruhm verschweige,", "tokens": ["Wund\u00b7re", "dich", "nun", "al\u00b7so", "nicht", ",", "da\u00df", "ich", "dei\u00b7nen", "Ruhm", "ver\u00b7schwei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PTKNEG", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.14": {"text": "Denn mein Fu\u00df ist nicht geschickt, da\u00df er diesen Berg ersteige,", "tokens": ["Denn", "mein", "Fu\u00df", "ist", "nicht", "ge\u00b7schickt", ",", "da\u00df", "er", "die\u00b7sen", "Berg", "er\u00b7stei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PTKNEG", "VVPP", "$,", "KOUS", "PPER", "PDAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.15": {"text": "Wo das Chor der deutschen Dichter deiner Muse Plaz gemacht,", "tokens": ["Wo", "das", "Chor", "der", "deut\u00b7schen", "Dich\u00b7ter", "dei\u00b7ner", "Mu\u00b7se", "Plaz", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "ADJA", "NN", "PPOSAT", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.16": {"text": "Welche dir schon in der Wiege ihren Lorbeer zugedacht.", "tokens": ["Wel\u00b7che", "dir", "schon", "in", "der", "Wie\u00b7ge", "ih\u00b7ren", "Lor\u00b7beer", "zu\u00b7ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "APPR", "ART", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.17": {"text": "Jezt befiehlt der kurze Rest von dem hinterlegten Jahre", "tokens": ["Jezt", "be\u00b7fiehlt", "der", "kur\u00b7ze", "Rest", "von", "dem", "hin\u00b7ter\u00b7leg\u00b7ten", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.18": {"text": "Meiner alten Schuldigkeit, da\u00df sie keinen Weihrauch spare;", "tokens": ["Mei\u00b7ner", "al\u00b7ten", "Schul\u00b7dig\u00b7keit", ",", "da\u00df", "sie", "kei\u00b7nen", "Weih\u00b7rauch", "spa\u00b7re", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "KOUS", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.19": {"text": "Nimm den Mundvoll reiner W\u00fcntsche, gro\u00dfer S(chmolcke), von der Pflicht", "tokens": ["Nimm", "den", "Mund\u00b7voll", "rei\u00b7ner", "W\u00fcnt\u00b7sche", ",", "gro\u00b7\u00dfer", "S", "(", "chmol\u00b7cke", ")", ",", "von", "der", "Pflicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "punct", "word", "word", "word"], "pos": ["NE", "ART", "NN", "ADJA", "NN", "$,", "ADJA", "NE", "$(", "NE", "$(", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-+--+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.20": {"text": "Eines dir verbundnen Knechtes, dem die Noth den Willen bricht.", "tokens": ["Ei\u00b7nes", "dir", "ver\u00b7bund\u00b7nen", "Knech\u00b7tes", ",", "dem", "die", "Noth", "den", "Wil\u00b7len", "bricht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PPER", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.21": {"text": "Deines Amtes schwere Last als die Mutter grauer Haare", "tokens": ["Dei\u00b7nes", "Am\u00b7tes", "schwe\u00b7re", "Last", "als", "die", "Mut\u00b7ter", "grau\u00b7er", "Haa\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "KOKOM", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.22": {"text": "Mindre sich je mehr und mehr bey dem Wachsthum deiner Jahre,", "tokens": ["Mind\u00b7re", "sich", "je", "mehr", "und", "mehr", "bey", "dem", "Wach\u00b7sthum", "dei\u00b7ner", "Jah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ADV", "KON", "ADV", "APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-+-+-+-", "measure": "iambic.septa.invert"}, "line.23": {"text": "Bis der wiederholte Wechsel vieler Zeiten deinen Geist", "tokens": ["Bis", "der", "wie\u00b7der\u00b7hol\u00b7te", "Wech\u00b7sel", "vie\u00b7ler", "Zei\u00b7ten", "dei\u00b7nen", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "PIAT", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.24": {"text": "Hier aus einem Paradiese in das andre langsam weist.", "tokens": ["Hier", "aus", "ei\u00b7nem", "Pa\u00b7ra\u00b7die\u00b7se", "in", "das", "and\u00b7re", "lang\u00b7sam", "weist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APPR", "ART", "ADJA", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+