{"textgrid.poem.54312": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was kommt doch vor ein Ruff von deinen Leben aus!", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was kommt doch vor ein Ruff von deinen Leben aus!", "tokens": ["Was", "kommt", "doch", "vor", "ein", "Ruff", "von", "dei\u00b7nen", "Le\u00b7ben", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Man sagt, der Mensche kommt fast keine Nacht ins Hau\u00df,", "tokens": ["Man", "sagt", ",", "der", "Men\u00b7sche", "kommt", "fast", "kei\u00b7ne", "Nacht", "ins", "Hau\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "ART", "NN", "VVFIN", "ADV", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bald s\u00e4ufft er da, bald dort, man h\u00f6rt ihn n\u00e4chtlich schw\u00e4rmen", "tokens": ["Bald", "s\u00e4ufft", "er", "da", ",", "bald", "dort", ",", "man", "h\u00f6rt", "ihn", "n\u00e4cht\u00b7lich", "schw\u00e4r\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "ADV", "ADV", "$,", "PIS", "VVFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durch alle Gassen durch, mit ungeheuren L\u00e4rmen.", "tokens": ["Durch", "al\u00b7le", "Gas\u00b7sen", "durch", ",", "mit", "un\u00b7ge\u00b7heu\u00b7ren", "L\u00e4r\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKVZ", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der grimmge Degen haut auf alle Steine lo\u00df,", "tokens": ["Der", "grimm\u00b7ge", "De\u00b7gen", "haut", "auf", "al\u00b7le", "Stei\u00b7ne", "lo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und dadurch giebst du dich, elende Seele, blo\u00df.", "tokens": ["Und", "da\u00b7durch", "giebst", "du", "dich", ",", "e\u00b7len\u00b7de", "See\u00b7le", ",", "blo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "PPER", "PRF", "$,", "ADJA", "NN", "$,", "ADV", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Du suchst ein Schlupff-Loch aus, die Geilheit auszudr\u00fccken.", "tokens": ["Du", "suchst", "ein", "Schlupf\u00b7fLoch", "aus", ",", "die", "Geil\u00b7heit", "aus\u00b7zu\u00b7dr\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wo sich in Schleyer nur l\u00e4st eine Ziege blicken,", "tokens": ["Wo", "sich", "in", "Schle\u00b7yer", "nur", "l\u00e4st", "ei\u00b7ne", "Zie\u00b7ge", "bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "APPR", "NE", "ADV", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Da trifft man dich gewi\u00df, verliebter Ritter, an,", "tokens": ["Da", "trifft", "man", "dich", "ge\u00b7wi\u00df", ",", "ver\u00b7lieb\u00b7ter", "Rit\u00b7ter", ",", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PRF", "ADV", "$,", "ADJA", "NN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wovon dein siecher Leib am besten zeugen kan.", "tokens": ["Wo\u00b7von", "dein", "sie\u00b7cher", "Leib", "am", "bes\u00b7ten", "zeu\u00b7gen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "PTKA", "ADJD", "VVFIN", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Nun must du, Livio, davor das Bette h\u00fcten;", "tokens": ["Nun", "must", "du", ",", "Li\u00b7vio", ",", "da\u00b7vor", "das", "Bet\u00b7te", "h\u00fc\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "NE", "$,", "PAV", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Hier hilfft kein kl\u00e4glich thun, kein Br\u00fcllen, Fluchen, W\u00fcten,", "tokens": ["Hier", "hilfft", "kein", "kl\u00e4g\u00b7lich", "thun", ",", "kein", "Br\u00fcl\u00b7len", ",", "Flu\u00b7chen", ",", "W\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "ADJD", "VVINF", "$,", "PIAT", "NN", "$,", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Mich d\u00fcnckt der Vogel singt aus einen andern Thon,", "tokens": ["Mich", "d\u00fcnckt", "der", "Vo\u00b7gel", "singt", "aus", "ei\u00b7nen", "an\u00b7dern", "Thon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NE", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Du sp\u00fchrst, ach allzusp\u00e4th, der geilen B\u00f6cke Lohn.", "tokens": ["Du", "sp\u00fchrst", ",", "ach", "all\u00b7zu\u00b7sp\u00e4th", ",", "der", "gei\u00b7len", "B\u00f6\u00b7cke", "Lohn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "XY", "XY", "$,", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Erwege die verderbt und mehr als viehsche Sitten,", "tokens": ["Er\u00b7we\u00b7ge", "die", "ver\u00b7derbt", "und", "mehr", "als", "vieh\u00b7sche", "Sit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJD", "KON", "PIAT", "KOKOM", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Kanst du, verliebter Geck, nun um Genesen bitten?", "tokens": ["Kanst", "du", ",", "ver\u00b7lieb\u00b7ter", "Geck", ",", "nun", "um", "Ge\u00b7ne\u00b7sen", "bit\u00b7ten", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "ADJA", "NN", "$,", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.17": {"text": "Betrachte deinen Stand und deines Hauses Schein,", "tokens": ["Be\u00b7trach\u00b7te", "dei\u00b7nen", "Stand", "und", "dei\u00b7nes", "Hau\u00b7ses", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Den du so sch\u00e4ndlich suchst mit Flecken zu bestreun.", "tokens": ["Den", "du", "so", "sch\u00e4nd\u00b7lich", "suchst", "mit", "Fle\u00b7cken", "zu", "be\u00b7streun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVFIN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "La\u00df Tugend und Vernunfft dein Leit-Gestirne heissen,", "tokens": ["La\u00df", "Tu\u00b7gend", "und", "Ver\u00b7nunfft", "dein", "Leit\u00b7Ge\u00b7stir\u00b7ne", "heis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "KON", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die werden dir gewi\u00df gantz andre Wege weissen.", "tokens": ["Die", "wer\u00b7den", "dir", "ge\u00b7wi\u00df", "gantz", "and\u00b7re", "We\u00b7ge", "weis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Man schliest dich nicht darum in Zell und Kloster ein,", "tokens": ["Man", "schliest", "dich", "nicht", "da\u00b7rum", "in", "Zell", "und", "Klos\u00b7ter", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PTKNEG", "PAV", "APPR", "NE", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Du kanst in Umgang doch mit sch\u00f6nen Kindern seyn,", "tokens": ["Du", "kanst", "in", "Um\u00b7gang", "doch", "mit", "sch\u00f6\u00b7nen", "Kin\u00b7dern", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "ADV", "APPR", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Denn diese werden dich in keine S\u00fcnde f\u00fchren,", "tokens": ["Denn", "die\u00b7se", "wer\u00b7den", "dich", "in", "kei\u00b7ne", "S\u00fcn\u00b7de", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "PPER", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Du wirst bey selbigen dein Ansehn nicht verliehren.", "tokens": ["Du", "wirst", "bey", "sel\u00b7bi\u00b7gen", "dein", "An\u00b7sehn", "nicht", "ver\u00b7lieh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.25": {"text": "Dein abgezehrter Mund und fahles Angesicht", "tokens": ["Dein", "ab\u00b7ge\u00b7zehr\u00b7ter", "Mund", "und", "fah\u00b7les", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Weist nunmehr aller Welt, da\u00df dir gar recht geschicht.", "tokens": ["Weist", "nun\u00b7mehr", "al\u00b7ler", "Welt", ",", "da\u00df", "dir", "gar", "recht", "ge\u00b7schicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "$,", "KOUS", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Man mu\u00df nicht jeden Busch, den Amor zeigt, entdecken,", "tokens": ["Man", "mu\u00df", "nicht", "je\u00b7den", "Busch", ",", "den", "A\u00b7mor", "zeigt", ",", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "VMFIN", "PTKNEG", "PIAT", "NN", "$,", "ART", "NE", "VVFIN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Sonst kan so Ro\u00df als Mann gar leichte sich beflecken.", "tokens": ["Sonst", "kan", "so", "Ro\u00df", "als", "Mann", "gar", "leich\u00b7te", "sich", "be\u00b7fle\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "NN", "KOUS", "NN", "ADV", "VVFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was kommt doch vor ein Ruff von deinen Leben aus!", "tokens": ["Was", "kommt", "doch", "vor", "ein", "Ruff", "von", "dei\u00b7nen", "Le\u00b7ben", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Man sagt, der Mensche kommt fast keine Nacht ins Hau\u00df,", "tokens": ["Man", "sagt", ",", "der", "Men\u00b7sche", "kommt", "fast", "kei\u00b7ne", "Nacht", "ins", "Hau\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "ART", "NN", "VVFIN", "ADV", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bald s\u00e4ufft er da, bald dort, man h\u00f6rt ihn n\u00e4chtlich schw\u00e4rmen", "tokens": ["Bald", "s\u00e4ufft", 