{"textgrid.poem.37240": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[ich meine doch, so sprach er mal]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich meine doch, so sprach er mal,", "tokens": ["Ich", "mei\u00b7ne", "doch", ",", "so", "sprach", "er", "mal", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Welt ist recht pl\u00e4sierlich.", "tokens": ["Die", "Welt", "ist", "recht", "pl\u00e4\u00b7sier\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das dumme Geschw\u00e4tz von Schmerz und Qual", "tokens": ["Das", "dum\u00b7me", "Ge\u00b7schw\u00e4tz", "von", "Schmerz", "und", "Qual"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Erscheint mir ganz ungeb\u00fchrlich.", "tokens": ["Er\u00b7scheint", "mir", "ganz", "un\u00b7ge\u00b7b\u00fchr\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Mit reinem kindlichen Gem\u00fct", "tokens": ["Mit", "rei\u00b7nem", "kind\u00b7li\u00b7chen", "Ge\u00b7m\u00fct"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Genie\u00df ich, was mir beschieden,", "tokens": ["Ge\u00b7nie\u00df", "ich"