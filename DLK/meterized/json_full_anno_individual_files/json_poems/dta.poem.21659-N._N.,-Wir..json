{"dta.poem.21659": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Wir.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Schranken brechen nieder,", "tokens": ["Schran\u00b7ken", "bre\u00b7chen", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "L\u00e4ngst zermorscht und grau,", "tokens": ["L\u00e4ngst", "zer\u00b7morscht", "und", "grau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "KON", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wir sind ", "tokens": ["Wir", "sind"], "token_info": ["word", "word"], "pos": ["PPER", "VAFIN"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Und ein einz\u2019ger Bau.", "tokens": ["Und", "ein", "einz'\u00b7ger", "Bau", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "--+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Die sich selbst nur lebten,", "tokens": ["Die", "sich", "selbst", "nur", "leb\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Werden opfernd gut,", "tokens": ["Wer\u00b7den", "op\u00b7fernd", "gut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Die da feindlich strebten,", "tokens": ["Die", "da", "feind\u00b7lich", "streb\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Kn\u00fcpft der Liebe Gluth.", "tokens": ["Kn\u00fcpft", "der", "Lie\u00b7be", "Gluth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Alle engverbunden", "tokens": ["Al\u00b7le", "eng\u00b7ver\u00b7bun\u00b7den"], "token_info": ["word", "word"], "pos": ["PIAT", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ringen k\u00fchn empor,", "tokens": ["Rin\u00b7gen", "k\u00fchn", "em\u00b7por", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Bis der Kranz gefunden,", "tokens": ["Bis", "der", "Kranz", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Den der Mensch erkor:", "tokens": ["Den", "der", "Mensch", "er\u00b7kor", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Auf des Lebens Flusse", "tokens": ["Auf", "des", "Le\u00b7bens", "Flus\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Hinzuzieh\u2019n begl\u00fcckt,", "tokens": ["Hin\u00b7zu\u00b7zieh'n", "be\u00b7gl\u00fcckt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Selig im Genusse,", "tokens": ["Se\u00b7lig", "im", "Ge\u00b7nus\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Der den Geist entz\u00fcckt.", "tokens": ["Der", "den", "Geist", "ent\u00b7z\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}