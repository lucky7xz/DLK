{"dta.poem.390": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Sie schleu\u00dft die Th\u00fcr zu vor ihn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Ogrosses Ungel\u00fcck! o Noht! o herbe Seelen-Pein!", "tokens": ["O\u00b7gros\u00b7ses", "Un\u00b7ge\u00b7l\u00fcck", "!", "o", "Noht", "!", "o", "her\u00b7be", "See\u00b7len\u00b7Pein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "FM", "NN", "$.", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Die schleu\u00dft den ", "tokens": ["Die", "schleu\u00dft", "den"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VVFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Ach la\u00df dein Paradies doch deinem Diener offen", "tokens": ["Ach", "la\u00df", "dein", "Pa\u00b7ra\u00b7dies", "doch", "dei\u00b7nem", "Die\u00b7ner", "of\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VVIMP", "PPOSAT", "NN", "ADV", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So darf ich auf den Stand der s\u00fcssen Unschuld hoffen.", "tokens": ["So", "darf", "ich", "auf", "den", "Stand", "der", "s\u00fcs\u00b7sen", "Un\u00b7schuld", "hof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}