{"textgrid.poem.40107": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Verf\u00fchrung", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df es wol, wie's keimt von Trieben", "tokens": ["Ich", "wei\u00df", "es", "wol", ",", "wie's", "keimt", "von", "Trie\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PWAV", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "und quillt in dir und aufw\u00e4rts bricht;", "tokens": ["und", "quillt", "in", "dir", "und", "auf\u00b7w\u00e4rts", "bricht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "KON", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "doch Mich, mich liebst du dennoch nicht,", "tokens": ["doch", "Mich", ",", "mich", "liebst", "du", "den\u00b7noch", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "sonst \u2013 m\u00fc\u00dfte ich dich wiederlieben.", "tokens": ["sonst", "\u2013", "m\u00fc\u00df\u00b7te", "ich", "dich", "wie\u00b7der\u00b7lie\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sonst k\u00e4mst du jauchzend hergeflogen", "tokens": ["Sonst", "k\u00e4mst", "du", "jauch\u00b7zend", "her\u00b7ge\u00b7flo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "und fragtest nicht nach Ruf und Pflicht", "tokens": ["und", "frag\u00b7test", "nicht", "nach", "Ruf", "und", "Pflicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und tauchtest schauernd mein Gesicht", "tokens": ["und", "tauch\u00b7test", "schau\u00b7ernd", "mein", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "in deines Busens hei\u00dfe Wogen.", "tokens": ["in", "dei\u00b7nes", "Bu\u00b7sens", "hei\u00b7\u00dfe", "Wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sonst w\u00fcrdest du dich ", "tokens": ["Sonst", "w\u00fcr\u00b7dest", "du", "dich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PRF"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "so wie die Winde z\u00e4rtlich dicht", "tokens": ["so", "wie", "die", "Win\u00b7de", "z\u00e4rt\u00b7lich", "dicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "ADJD", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "im Busch den Bl\u00fctenzweig umflicht,", "tokens": ["im", "Busch", "den", "Bl\u00fc\u00b7ten\u00b7zweig", "um\u00b7flicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "in heimlich bangen s\u00fc\u00dfen N\u00e4chten.", "tokens": ["in"