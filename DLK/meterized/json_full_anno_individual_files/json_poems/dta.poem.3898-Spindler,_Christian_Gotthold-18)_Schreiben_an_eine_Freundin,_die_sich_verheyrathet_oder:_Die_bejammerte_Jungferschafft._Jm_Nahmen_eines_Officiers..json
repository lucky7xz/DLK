{"dta.poem.3898": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "18) Schreiben an eine Freundin,  \n die sich verheyrathet/ oder:  \n Die bejammerte Jungferschafft.  \n Jm Nahmen eines  Officiers.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Viel-Ehr- und Tugendsames Kind!", "tokens": ["Viel\u00b7Ehr", "und", "Tu\u00b7gend\u00b7sa\u00b7mes", "Kind", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["TRUNC", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So hast du denn aus Liebe blind", "tokens": ["So", "hast", "du", "denn", "aus", "Lie\u00b7be", "blind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die edle Jungfrauschafft verlohren?", "tokens": ["Die", "ed\u00b7le", "Jung\u00b7frausc\u00b7hafft", "ver\u00b7loh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der du doch treu und \u00e4cht geschworen;", "tokens": ["Der", "du", "doch", "treu", "und", "\u00e4cht", "ge\u00b7schwo\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ist das der Keuschheit sch\u00f6ner Sieg?", "tokens": ["Ist", "das", "der", "Keuschheit", "sch\u00f6\u00b7ner", "Sieg", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Ist das der wohlgef\u00fchrte Krieg?", "tokens": ["Ist", "das", "der", "wohl\u00b7ge\u00b7f\u00fchr\u00b7te", "Krieg", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der dir die Jungferschafft entrissen,", "tokens": ["Der", "dir", "die", "Jung\u00b7fer\u00b7schafft", "ent\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nunmehro kan ich deutlich schliessen,", "tokens": ["Nun\u00b7meh\u00b7ro", "kan", "ich", "deut\u00b7lich", "schlies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Da\u00df stille Wasser gerne tief,", "tokens": ["Da\u00df", "stil\u00b7le", "Was\u00b7ser", "ger\u00b7ne", "tief", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Da dein Versprechen scheitern lief;", "tokens": ["Da", "dein", "Ver\u00b7spre\u00b7chen", "schei\u00b7tern", "lief", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Der Geier mu\u00df dich w\u00fcrcklich plagen,", "tokens": ["Der", "Gei\u00b7er", "mu\u00df", "dich", "w\u00fcrck\u00b7lich", "pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "So bald, so jung ", "tokens": ["So", "bald", ",", "so", "jung"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "$,", "ADV", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Das thut kein redlicher Soldat,", "tokens": ["Das", "thut", "kein", "red\u00b7li\u00b7cher", "Sol\u00b7dat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Der Hertz in seinem Leibe hat.", "tokens": ["Der", "Hertz", "in", "sei\u00b7nem", "Lei\u00b7be", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Da liegt der Muth, er ist gebunden,", "tokens": ["Da", "liegt", "der", "Muth", ",", "er", "ist", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Da liegt er g\u00e4ntzlich \u00fcberwunden,", "tokens": ["Da", "liegt", "er", "g\u00e4ntz\u00b7lich", "\u00fc\u00b7berw\u00b7un\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Mich schmertzt mit Rechte und Verdru\u00df", "tokens": ["Mich", "schmertzt", "mit", "Rech\u00b7te", "und", "Ver\u00b7dru\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Der \u00fcbermannten Keuschheit Mu\u00df.", "tokens": ["Der", "\u00fc\u00b7berm\u00b7ann\u00b7ten", "Keuschheit", "Mu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.19": {"text": "Die Unschuld wird noch dein Versprechen", "tokens": ["Die", "Un\u00b7schuld", "wird", "noch", "dein", "Ver\u00b7spre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Mit \u00fcberh\u00e4ufften Kindern r\u00e4chen.", "tokens": ["Mit", "\u00fc\u00b7berh\u00b7\u00e4uff\u00b7ten", "Kin\u00b7dern", "r\u00e4\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Der Unschuld hast du weh gethan,", "tokens": ["Der", "Un\u00b7schuld", "hast", "du", "weh", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Drum h\u00e4nge bald die Trommel an.", "tokens": ["Drum", "h\u00e4n\u00b7ge", "bald", "die", "Trom\u00b7mel", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Dein Ehe-Schatz mu\u00df hertzlich lachen,", "tokens": ["Dein", "E\u00b7he\u00b7Schatz", "mu\u00df", "hertz\u00b7lich", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Dein Stand mu\u00df dich zum Weibgen machen,", "tokens": ["Dein", "Stand", "mu\u00df", "dich", "zum", "Weib\u00b7gen", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Bedencke nur das Wort: ein Weib.", "tokens": ["Be\u00b7den\u00b7cke", "nur", "das", "Wort", ":", "ein", "Weib", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$.", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Ich g\u00f6nne dir den Zeitvertreib.", "tokens": ["Ich", "g\u00f6n\u00b7ne", "dir", "den", "Zeit\u00b7ver\u00b7treib", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Alleine la\u00df mich zum Erg\u00f6tzen", "tokens": ["Al\u00b7lei\u00b7ne", "la\u00df", "mich", "zum", "Er\u00b7g\u00f6t\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Der Jungferschafft ihr Grabmahl setzen:", "tokens": ["Der", "Jung\u00b7fer\u00b7schafft", "ihr", "Grab\u00b7mahl", "set\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Hier mu\u00df das arme Thier in seiner Unschuld", "tokens": ["Hier", "mu\u00df", "das", "ar\u00b7me", "Thier", "in", "sei\u00b7ner", "Un\u00b7schuld"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "sterben,", "tokens": ["ster\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.31": {"text": "Denn meine Schw\u00e4gerin bringt solches ins Ver-", "tokens": ["Denn", "mei\u00b7ne", "Schw\u00e4\u00b7ge\u00b7rin", "bringt", "sol\u00b7ches", "ins", "Ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PIS", "APPRART", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "derben,", "tokens": ["der\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["PDS", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.33": {"text": "Die sonsten gar von nichts als reiner Keusch-", "tokens": ["Die", "sons\u00b7ten", "gar", "von", "nichts", "als", "rei\u00b7ner", "Keusch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADV", "APPR", "PIS", "KOKOM", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "heit sprach,", "tokens": ["heit", "sprach", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.35": {"text": "Sie rufft den Jungfern zu: Folgt alle balde", "tokens": ["Sie", "rufft", "den", "Jung\u00b7fern", "zu", ":", "Folgt", "al\u00b7le", "bal\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$.", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+++-+-", "measure": "unknown.measure.hexa"}, "line.36": {"text": "nach.", "tokens": ["nach", "."], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$."], "meter": "+", "measure": "single.up"}}}}}