{"dta.poem.4013": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Des Morgens .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Vom Thaue gl\u00e4nzt der Rasen, beweglicher", "tokens": ["Vom", "Thau\u00b7e", "gl\u00e4nzt", "der", "Ra\u00b7sen", ",", "be\u00b7weg\u00b7li\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,", "ADJA"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Eilt schon die wache Quelle; die Birke neigt", "tokens": ["Eilt", "schon", "die", "wa\u00b7che", "Quel\u00b7le", ";", "die", "Bir\u00b7ke", "neigt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ihr schwankes Haupt und im Gebl\u00e4tter", "tokens": ["Ihr", "schwan\u00b7kes", "Haupt", "und", "im", "Ge\u00b7bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Rauscht es und schimmert; und um die grauen", "tokens": ["Rauscht", "es", "und", "schim\u00b7mert", ";", "und", "um", "die", "grau\u00b7en"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "$.", "KON", "APPR", "ART", "ADJA"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Gew\u00f6lke streifen r\u00f6thliche Flammen dort,", "tokens": ["Ge\u00b7w\u00f6l\u00b7ke", "strei\u00b7fen", "r\u00f6th\u00b7li\u00b7che", "Flam\u00b7men", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Verk\u00fcndende, sie wallen ger\u00e4uschlos auf;", "tokens": ["Ver\u00b7k\u00fcn\u00b7den\u00b7de", ",", "sie", "wal\u00b7len", "ge\u00b7r\u00e4uschlos", "auf", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVINF", "ADJD", "PTKVZ", "$."], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Wie Fluten am Gestade, wogen", "tokens": ["Wie", "Flu\u00b7ten", "am", "Ge\u00b7sta\u00b7de", ",", "wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "NN", "APPRART", "NN", "$,", "PWAV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00f6her und h\u00f6her die wandelbaren.", "tokens": ["H\u00f6\u00b7her", "und", "h\u00f6\u00b7her", "die", "wan\u00b7del\u00b7ba\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Komm nun, o komm, und eile mir nicht zu schnell,", "tokens": ["Komm", "nun", ",", "o", "komm", ",", "und", "ei\u00b7le", "mir", "nicht", "zu", "schnell", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "FM", "VVFIN", "$,", "KON", "VVFIN", "PPER", "PTKNEG", "PTKA", "ADJD", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Du goldner Tag, zum Gipfel des Himmels fort!", "tokens": ["Du", "gold\u00b7ner", "Tag", ",", "zum", "Gip\u00b7fel", "des", "Him\u00b7mels", "fort", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "APPRART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Denn offner fliegt, vertrauter Dir mein", "tokens": ["Denn", "off\u00b7ner", "fliegt", ",", "ver\u00b7trau\u00b7ter", "Dir", "mein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "$,", "ADJD", "PPER", "PPOSAT"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Auge, Du Freudiger! zu, so lang Du", "tokens": ["Au\u00b7ge", ",", "Du", "Freu\u00b7di\u00b7ger", "!", "zu", ",", "so", "lang", "Du"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "NN", "$.", "PTKVZ", "$,", "ADV", "ADJD", "PPER"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "In Deiner Sch\u00f6ne jugendlich blickst und noch", "tokens": ["In", "Dei\u00b7ner", "Sch\u00f6\u00b7ne", "ju\u00b7gend\u00b7lich", "blickst", "und", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVFIN", "KON", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zu herrlich nicht, zu stolz mir geworden bist;", "tokens": ["Zu", "herr\u00b7lich", "nicht", ",", "zu", "stolz", "mir", "ge\u00b7wor\u00b7den", "bist", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "PTKNEG", "$,", "PTKA", "ADJD", "PPER", "VAPP", "VAFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Du m\u00f6chtest immer eilen, k\u00f6nnt' ich,", "tokens": ["Du", "m\u00f6ch\u00b7test", "im\u00b7mer", "ei\u00b7len", ",", "k\u00f6nnt'", "ich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVFIN", "$,", "VMFIN", "PPER", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "G\u00f6ttlicher Wanderer, mit Dir! &#8212; doch l\u00e4chelst", "tokens": ["G\u00f6tt\u00b7li\u00b7cher", "Wan\u00b7de\u00b7rer", ",", "mit", "Dir", "!", "&#8212;", "doch", "l\u00e4\u00b7chelst"], "token_info": ["word", "word", "punct", "word", "word", "punct", "XML_entity", "word", "word"], "pos": ["ADJA", "NN", "$,", "APPR", "PPER", "$.", "$(", "ADV", "VVFIN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.5": {"line.1": {"text": "Des frohen Ueberm\u00fcthigen Du, da\u00df er", "tokens": ["Des", "fro\u00b7hen", "Ue\u00b7ber\u00b7m\u00fct\u00b7hi\u00b7gen", "Du", ",", "da\u00df", "er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPER", "$,", "KOUS", "PPER"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dir gleichen m\u00f6chte; segne mir lieber dann", "tokens": ["Dir", "glei\u00b7chen", "m\u00f6ch\u00b7te", ";", "seg\u00b7ne", "mir", "lie\u00b7ber", "dann"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "VMFIN", "$.", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mein sterblich Thun und heitre wieder,", "tokens": ["Mein", "sterb\u00b7lich", "Thun", "und", "heit\u00b7re", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "KON", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "G\u00fctiger! heute den stillen Pfad mir!", "tokens": ["G\u00fc\u00b7ti\u00b7ger", "!", "heu\u00b7te", "den", "stil\u00b7len", "Pfad", "mir", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "ART", "ADJA", "NN", "PPER", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}