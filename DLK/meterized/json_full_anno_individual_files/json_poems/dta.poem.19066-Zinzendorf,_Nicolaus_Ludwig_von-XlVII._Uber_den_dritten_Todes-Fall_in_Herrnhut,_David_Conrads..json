{"dta.poem.19066": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "XlVII.   Uber den dritten Todes-Fall in  \n Herrnhut, David Conrads.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin noch nicht zur\u00fcck aus Schlesien und M\u00e4hren;", "tokens": ["Ich", "bin", "noch", "nicht", "zu\u00b7r\u00fcck", "aus", "Schle\u00b7si\u00b7en", "und", "M\u00e4h\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "PTKVZ", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So schreibt mir meine Frau, die eine J\u00fcngerin:", "tokens": ["So", "schreibt", "mir", "mei\u00b7ne", "Frau", ",", "die", "ei\u00b7ne", "J\u00fcn\u00b7ge\u00b7rin", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "PRELS", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wir k\u00f6nnen dir die Zahl der Br\u00fcder nicht gew\u00e4hren;", "tokens": ["Wir", "k\u00f6n\u00b7nen", "dir", "die", "Zahl", "der", "Br\u00fc\u00b7der", "nicht", "ge\u00b7w\u00e4h\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ART", "NN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Denn diesen Augenblick zieht unser Conrad hin,", "tokens": ["Denn", "die\u00b7sen", "Au\u00b7gen\u00b7blick", "zieht", "un\u00b7ser", "Con\u00b7rad", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bey einem Liebes-Dienst, mit unge\u00fcbten H\u00e4nden,", "tokens": ["Bey", "ei\u00b7nem", "Lie\u00b7bes\u00b7Dienst", ",", "mit", "un\u00b7ge\u00b7\u00fcb\u00b7ten", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mu\u00df er durch einen Schlag sein muntres Leben enden.", "tokens": ["Mu\u00df", "er", "durch", "ei\u00b7nen", "Schlag", "sein", "mun\u00b7tres", "Le\u00b7ben", "en\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mein K\u00f6nig! (f\u00e4llt mir ein) denn, wenn ich dencken soll,", "tokens": ["Mein", "K\u00f6\u00b7nig", "!", "(", "f\u00e4llt", "mir", "ein", ")", "denn", ",", "wenn", "ich", "den\u00b7cken", "soll", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "$(", "VVFIN", "PPER", "ART", "$(", "KON", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So pflegt mir niemand eh als dieser einzukommen,", "tokens": ["So", "pflegt", "mir", "nie\u00b7mand", "eh", "als", "die\u00b7ser", "ein\u00b7zu\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "KOUS", "KOKOM", "PDS", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein K\u00f6nig! dieses ist der dritte Menschen- ", "tokens": ["Mein", "K\u00f6\u00b7nig", "!", "die\u00b7ses", "ist", "der", "drit\u00b7te", "Men\u00b7schen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "PDS", "VAFIN", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Seit dem, da\u00df du die Hut im W\u00e4ldgen \u00fcbernommen.", "tokens": ["Seit", "dem", ",", "da\u00df", "du", "die", "Hut", "im", "W\u00e4ld\u00b7gen", "\u00fc\u00b7ber\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "KOUS", "PPER", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ists Schutz-HErr oder Feind, der unsre Ruhe st\u00f6hrt?", "tokens": ["Ists", "Schutz\u00b7HErr", "o\u00b7der", "Feind", ",", "der", "uns\u00b7re", "Ru\u00b7he", "st\u00f6hrt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "KON", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wer wird dirs k\u00fcnftig hin, du treue Liebe! glauben,", "tokens": ["Wer", "wird", "dirs", "k\u00fcnf\u00b7tig", "hin", ",", "du", "treu\u00b7e", "Lie\u00b7be", "!", "glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VAFIN", "PIS", "ADJD", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$.", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df dein geringes Volck in sein es HErren Hut", "tokens": ["Da\u00df", "dein", "ge\u00b7rin\u00b7ges", "Volck", "in", "sein", "es", "Her\u00b7ren", "Hut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vollkommen unbesorgt vor Schaden und Berauben,", "tokens": ["Voll\u00b7kom\u00b7men", "un\u00b7be\u00b7sorgt", "vor", "Scha\u00b7den", "und", "Be\u00b7rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So einsam, wie es ist, als unter Schl\u00f6ssern ruht;", "tokens": ["So", "ein\u00b7sam", ",", "wie", "es", "ist", ",", "als", "un\u00b7ter", "Schl\u00f6s\u00b7sern", "ruht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAV", "PPER", "VAFIN", "$,", "KOUS", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wenn, was am sichersten, sonst zu geschehen pflegt,", "tokens": ["Wenn", ",", "was", "am", "si\u00b7chers\u00b7ten", ",", "sonst", "zu", "ge\u00b7sche\u00b7hen", "pflegt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PRELS", "APPRART", "ADJA", "$,", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Die Br\u00fcder dieses Orts, so bald zu Boden legt?", "tokens": ["Die", "Br\u00fc\u00b7der", "die\u00b7ses", "Orts", ",", "so", "bald", "zu", "Bo\u00b7den", "legt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "$,", "ADV", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wie ist denn aber dir? red ich mich selber an,", "tokens": ["Wie", "ist", "denn", "a\u00b7ber", "dir", "?", "red", "ich", "mich", "sel\u00b7ber", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "ADV", "PPER", "$.", "VVFIN", "PPER", "PRF", "ADV", "PTKVZ", "$,"], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Weil ich in meinem Sinn, auch wieder meinen Willen", "tokens": ["Weil", "ich", "in", "mei\u00b7nem", "Sinn", ",", "auch", "wie\u00b7der", "mei\u00b7nen", "Wil\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "$,", "ADV", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nicht das geringste Leid noch Kummer mercken kan,", "tokens": ["Nicht", "das", "ge\u00b7rings\u00b7te", "Leid", "noch", "Kum\u00b7mer", "mer\u00b7cken", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "ADV", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was kan dein zartes Hertz hierum so hurtig stillen?", "tokens": ["Was", "kan", "dein", "zar\u00b7tes", "Hertz", "hie\u00b7rum", "so", "hur\u00b7tig", "stil\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPOSAT", "ADJA", "NN", "PAV", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da eines Bruders Fall so tiefe Wunden rei\u00dft,", "tokens": ["Da", "ei\u00b7nes", "Bru\u00b7ders", "Fall", "so", "tie\u00b7fe", "Wun\u00b7den", "rei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und selbst die Schuldigkeit dich Thr\u00e4nen zollen hei\u00dft?", "tokens": ["Und", "selbst", "die", "Schul\u00b7dig\u00b7keit", "dich", "Thr\u00e4\u00b7nen", "zol\u00b7len", "hei\u00dft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PPER", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ich glaube darum ist, weils unser K\u00f6nig thut,", "tokens": ["Ich", "glau\u00b7be", "da\u00b7rum", "ist", ",", "weils", "un\u00b7ser", "K\u00f6\u00b7nig", "thut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "VAFIN", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er ist mein Held, mein Freund, mein Br\u00e4utgam, mein Re-", "tokens": ["Er", "ist", "mein", "Held", ",", "mein", "Freund", ",", "mein", "Br\u00e4ut\u00b7gam", ",", "mein", "Re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NE", "$,", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Sein Thun ist immer recht, und wohl gemeynt und gut,", "tokens": ["Sein", "Thun", "ist", "im\u00b7mer", "recht", ",", "und", "wohl", "ge\u00b7meynt", "und", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$,", "KON", "ADV", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und gro\u00df und \u00fcberlegt, da\u00df ich nicht sagen k\u00f6nte,", "tokens": ["Und", "gro\u00df", "und", "\u00fc\u00b7ber\u00b7legt", ",", "da\u00df", "ich", "nicht", "sa\u00b7gen", "k\u00f6n\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "VVPP", "$,", "KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wiewol ichs sagen darf, wiewol ichs hindern kan,", "tokens": ["Wie\u00b7wol", "ichs", "sa\u00b7gen", "darf", ",", "wie\u00b7wol", "ichs", "hin\u00b7dern", "kan", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVINF", "VMFIN", "$,", "KOUS", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}