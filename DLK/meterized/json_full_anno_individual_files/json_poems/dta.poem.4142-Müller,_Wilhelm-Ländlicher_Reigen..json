{"dta.poem.4142": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "L\u00e4ndlicher Reigen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wohl in dem gr\u00fcnen Mai,", "tokens": ["Wohl", "in", "dem", "gr\u00fc\u00b7nen", "Mai", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und Keine will mir sagen,", "tokens": ["Und", "Kei\u00b7ne", "will", "mir", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo's nun geblieben sei.", "tokens": ["Wo's", "nun", "ge\u00b7blie\u00b7ben", "sei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ihr schmucken Dirnen alle,", "tokens": ["Ihr", "schmu\u00b7cken", "Dir\u00b7nen", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nun Eine hat es doch,", "tokens": ["Nun", "Ei\u00b7ne", "hat", "es", "doch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und habt ihr's nicht gefunden,", "tokens": ["Und", "habt", "ih\u00b7r's", "nicht", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "So liegt's im Grase noch.", "tokens": ["So", "liegt's", "im", "Gra\u00b7se", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und wenn es liegt im Grase,", "tokens": ["Und", "wenn", "es", "liegt", "im", "Gra\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So liegt's auf k\u00fchler Streu,", "tokens": ["So", "liegt's", "auf", "k\u00fch\u00b7ler", "Streu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wann ihr m\u00e4ht die Wiesen,", "tokens": ["Und", "wann", "ihr", "m\u00e4ht", "die", "Wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So schneidet's nicht entzwei.", "tokens": ["So", "schnei\u00b7det's", "nicht", "ent\u00b7zwei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}