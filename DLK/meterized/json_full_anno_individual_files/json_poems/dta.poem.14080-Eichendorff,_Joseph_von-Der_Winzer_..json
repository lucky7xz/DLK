{"dta.poem.14080": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Winzer .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Es hat die Nacht geregnet,", "tokens": ["Es", "hat", "die", "Nacht", "ge\u00b7reg\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es zog noch grau in's Thal,", "tokens": ["Es", "zog", "noch", "grau", "in's", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ruhten stillgesegnet", "tokens": ["Und", "ruh\u00b7ten", "still\u00b7ge\u00b7seg\u00b7net"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Felder \u00fcberall;", "tokens": ["Die", "Fel\u00b7der", "\u00fc\u00b7be\u00b7rall", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Von L\u00fcften kaum gef\u00e4chelt,", "tokens": ["Von", "L\u00fcf\u00b7ten", "kaum", "ge\u00b7f\u00e4\u00b7chelt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Durch's ungewisse Blau", "tokens": ["Durch's", "un\u00b7ge\u00b7wis\u00b7se", "Blau"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Sonne verschlafen l\u00e4chelt'", "tokens": ["Die", "Son\u00b7ne", "ver\u00b7schla\u00b7fen", "l\u00e4\u00b7chelt'"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Wie eine wundersch\u00f6ne Frau.", "tokens": ["Wie", "ei\u00b7ne", "wun\u00b7der\u00b7sch\u00f6\u00b7ne", "Frau", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}