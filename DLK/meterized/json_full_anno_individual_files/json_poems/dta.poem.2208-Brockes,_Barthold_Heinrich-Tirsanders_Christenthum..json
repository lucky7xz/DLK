{"dta.poem.2208": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Tirsanders Christenthum.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wir werden uns umsonst bem\u00fchn,", "tokens": ["Wir", "wer\u00b7den", "uns", "um\u00b7sonst", "be\u00b7m\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das menschliche Geschlecht zu bessern, und es", "tokens": ["Das", "menschli\u00b7che", "Ge\u00b7schlecht", "zu", "bes\u00b7sern", ",", "und", "es"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,", "KON", "PPER"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "der Thorheit zu entziehn,", "tokens": ["der", "Thor\u00b7heit", "zu", "ent\u00b7ziehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Augen ihnen zu er\u00f6ffnen, zu zeigen, wie sie wan-", "tokens": ["Die", "Au\u00b7gen", "ih\u00b7nen", "zu", "er\u00b7\u00f6ff\u00b7nen", ",", "zu", "zei\u00b7gen", ",", "wie", "sie", "wan"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$,", "PWAV", "PPER", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.5": {"text": "deln sollen;", "tokens": ["deln", "sol\u00b7len", ";"], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Da sie die Augen immer schlie\u00dfen, und ihren Fehl nicht", "tokens": ["Da", "sie", "die", "Au\u00b7gen", "im\u00b7mer", "schlie\u00b7\u00dfen", ",", "und", "ih\u00b7ren", "Fehl", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVINF", "$,", "KON", "PPOSAT", "NN", "PTKNEG"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "fehen wollen.", "tokens": ["fe\u00b7hen", "wol\u00b7len", "."], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Da jedermann sein liebes Ich, als unverbesserlich besieht,", "tokens": ["Da", "je\u00b7der\u00b7mann", "sein", "lie\u00b7bes", "Ich", ",", "als", "un\u00b7ver\u00b7bes\u00b7ser\u00b7lich", "be\u00b7sieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPOSAT", "ADJA", "PPER", "$,", "KOUS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.9": {"text": "Was Wunder, da\u00df er Rath verachtet, Erinnrung has-", "tokens": ["Was", "Wun\u00b7der", ",", "da\u00df", "er", "Rath", "ver\u00b7ach\u00b7tet", ",", "E\u00b7rinn\u00b7rung", "has"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "NN", "$,", "KOUS", "PPER", "NN", "VVFIN", "$,", "NN", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "set, Lehren flieht!", "tokens": ["set", ",", "Leh\u00b7ren", "flieht", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}