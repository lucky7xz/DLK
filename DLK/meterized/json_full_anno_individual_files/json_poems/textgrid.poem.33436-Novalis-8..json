{"textgrid.poem.33436": {"metadata": {"author": {"name": "Novalis", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weinen mu\u00df ich, immer weinen:", "tokens": ["Wei\u00b7nen", "mu\u00df", "ich", ",", "im\u00b7mer", "wei\u00b7nen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "$,", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00f6cht' er einmal nur erscheinen,", "tokens": ["M\u00f6cht'", "er", "ein\u00b7mal", "nur", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Einmal nur von Ferne mir.", "tokens": ["Ein\u00b7mal", "nur", "von", "Fer\u00b7ne", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Heilge Wehmuth! ewig w\u00e4hren", "tokens": ["Heil\u00b7ge", "Weh\u00b7muth", "!", "e\u00b7wig", "w\u00e4h\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$.", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Meine Schmerzen, meine Z\u00e4hren;", "tokens": ["Mei\u00b7ne", "Schmer\u00b7zen", ",", "mei\u00b7ne", "Z\u00e4h\u00b7ren", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gleich erstarren m\u00f6cht' ich hier.", "tokens": ["Gleich", "er\u00b7star\u00b7ren", "m\u00f6cht'", "ich", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ewig seh ich ihn nur leiden,", "tokens": ["E\u00b7wig", "seh", "ich", "ihn", "nur", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ewig bittend ihn verscheiden.", "tokens": ["E\u00b7wig", "bit\u00b7tend", "ihn", "ver\u00b7schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "O! da\u00df dieses Herz nicht bricht,", "tokens": ["O", "!", "da\u00df", "die\u00b7ses", "Herz", "nicht", "bricht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "KOUS", "PDAT", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine Augen sich nicht schlie\u00dfen.", "tokens": ["Mei\u00b7ne", "Au\u00b7gen", "sich", "nicht", "schlie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ganz in Thr\u00e4nen zu zerflie\u00dfen,", "tokens": ["Ganz", "in", "Thr\u00e4\u00b7nen", "zu", "zer\u00b7flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dieses Gl\u00fcck verdient' ich nicht.", "tokens": ["Die\u00b7ses", "Gl\u00fcck", "ver\u00b7dient'", "ich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Weint denn keiner nicht von allen?", "tokens": ["Weint", "denn", "kei\u00b7ner", "nicht", "von", "al\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIS", "PTKNEG", "APPR", "PIAT", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Soll sein Name so verhallen?", "tokens": ["Soll", "sein", "Na\u00b7me", "so", "ver\u00b7hal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist die Welt auf einmal todt?", "tokens": ["Ist", "die", "Welt", "auf", "ein\u00b7mal", "todt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Werd' ich nie aus seinen Augen", "tokens": ["Werd'", "ich", "nie", "aus", "sei\u00b7nen", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wieder Lieb' und Leben saugen?", "tokens": ["Wie\u00b7der", "Lieb'", "und", "Le\u00b7ben", "sau\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist er nun auf ewig todt?", "tokens": ["Ist", "er", "nun", "auf", "e\u00b7wig", "todt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Todt, \u2013 was kann, was soll das hei\u00dfen?", "tokens": ["Todt", ",", "\u2013", "was", "kann", ",", "was", "soll", "das", "hei\u00b7\u00dfen", "?"], "token_info": ["word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "$(", "PWS", "VMFIN", "$,", "PWS", "VMFIN", "PDS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "O! so sagt mir doch ihr Weisen,", "tokens": ["O", "!", "so", "sagt", "mir", "doch", "ihr", "Wei\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sagt mir diese Deutung an.", "tokens": ["Sagt", "mir", "die\u00b7se", "Deu\u00b7tung", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Er ist stumm, und alle schweigen,", "tokens": ["Er", "ist", "stumm", ",", "und", "al\u00b7le", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "KON", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Keiner kann auf Erden zeigen,", "tokens": ["Kei\u00b7ner", "kann", "auf", "Er\u00b7den", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo mein Herz ihn finden kann.", "tokens": ["Wo", "mein", "Herz", "ihn", "fin\u00b7den", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Nirgend kann ich hier auf Erden", "tokens": ["Nir\u00b7gend", "kann", "ich", "hier", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jemals wieder gl\u00fccklich werden,", "tokens": ["Je\u00b7mals", "wie\u00b7der", "gl\u00fcck\u00b7lich", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles ist ein d\u00fcstrer Traum.", "tokens": ["Al\u00b7les", "ist", "ein", "d\u00fcst\u00b7rer", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich bin auch mit ihm verschieden,", "tokens": ["Ich", "bin", "auch", "mit", "ihm", "ver\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "L\u00e4g' ich doch mit ihm in Frieden", "tokens": ["L\u00e4g'", "ich", "doch", "mit", "ihm", "in", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "PPER", "APPR", "NN"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.6": {"text": "Schon im unterirdschen Raum.", "tokens": ["Schon", "im", "un\u00b7ter\u00b7ird\u00b7schen", "Raum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Du, sein Vater und der meine,", "tokens": ["Du", ",", "sein", "Va\u00b7ter", "und", "der", "mei\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "KON", "ART", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sammle du doch mein Gebeine", "tokens": ["Samm\u00b7le", "du", "doch", "mein", "Ge\u00b7bei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zu dem seinigen nur bald.", "tokens": ["Zu", "dem", "sei\u00b7ni\u00b7gen", "nur", "bald", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSS", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gr\u00fcn wird bald sein H\u00fcgel stehen", "tokens": ["Gr\u00fcn", "wird", "bald", "sein", "H\u00fc\u00b7gel", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ADV", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und der Wind dar\u00fcber wehen,", "tokens": ["Und", "der", "Wind", "da\u00b7r\u00fc\u00b7ber", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PAV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und verwesen die Gestalt.", "tokens": ["Und", "ver\u00b7we\u00b7sen", "die", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Wenn sie seine Liebe w\u00fcsten,", "tokens": ["Wenn", "sie", "sei\u00b7ne", "Lie\u00b7be", "w\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alle Menschen w\u00fcrden Christen,", "tokens": ["Al\u00b7le", "Men\u00b7schen", "w\u00fcr\u00b7den", "Chris\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lie\u00dfen alles andre stehn;", "tokens": ["Lie\u00b7\u00dfen", "al\u00b7les", "and\u00b7re", "stehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PIS", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Liebten alle nur den Einen,", "tokens": ["Lieb\u00b7ten", "al\u00b7le", "nur", "den", "Ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "W\u00fcrden alle mit mir weinen", "tokens": ["W\u00fcr\u00b7den", "al\u00b7le", "mit", "mir", "wei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIS", "APPR", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und in bitterm Weh vergehn.", "tokens": ["Und", "in", "bit\u00b7term", "Weh", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Weinen mu\u00df ich, immer weinen:", "tokens": ["Wei\u00b7nen", "mu\u00df", "ich", ",", "im\u00b7mer", "wei\u00b7nen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "$,", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00f6cht' er einmal nur erscheinen,", "tokens": ["M\u00f6cht'", "er", "ein\u00b7mal", "nur", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.t