, "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Er ist stumm, und alle schweigen,", "tokens": ["Er", "ist", "stumm", ",", "und", "al\u00b7le", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "KON", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Keiner kann auf Erden zeigen,", "tokens": ["Kei\u00b7ner", "kann", "auf", "Er\u00b7den", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo mein Herz ihn finden kann.", "tokens": ["Wo", "mein", "Herz", "ihn", "fin\u00b7den", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Nirgend kann ich hier auf Erden", "tokens": ["Nir\u00b7gend", "kann", "ich", "hier", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jemals wieder gl\u00fccklich werden,", "tokens": ["Je\u00b7mals", "wie\u00b7der", "gl\u00fcck\u00b7lich", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles ist ein d\u00fcstrer Traum.", "tokens": ["Al\u00b7les", "ist", "ein", "d\u00fcst\u00b7rer", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich bin auch mit ihm verschieden,", "tokens": ["Ich", "bin", 