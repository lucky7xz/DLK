{"textgrid.poem.49725": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "\u00bbein Gentleman hetzt nicht\u00ab", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hast d' as g'h\u00f6rt, Kop'ratawaschel,", "tokens": ["Hast", "d'", "as", "g'\u00b7h\u00f6rt", ",", "Kop'\u00b7ra\u00b7ta\u00b7wa\u00b7schel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "NE", "NE", "VVFIN", "$,", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df a Dschendleman net hetzt?", "tokens": ["Da\u00df", "a", "Dschend\u00b7le\u00b7man", "net", "hetzt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NE", "NE", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Drunt in W\u00fcrzburg hamm s' d\u00f6s g'funden.", "tokens": ["Drunt", "in", "W\u00fcrz\u00b7burg", "hamm", "s'", "d\u00f6s", "g'\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ja, was sagst denn jetzt?", "tokens": ["Ja", ",", "was", "sagst", "denn", "jetzt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PWS", "VVFIN", "ADV", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Hat da Mensch vielleicht sei Votzen,", "tokens": ["Hat", "da", "Mensch", "viel\u00b7leicht", "sei", "Vot\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "ADV", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "