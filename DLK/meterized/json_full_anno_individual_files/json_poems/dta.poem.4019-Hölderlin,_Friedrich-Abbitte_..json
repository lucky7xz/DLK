{"dta.poem.4019": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Abbitte .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Heilig Wesen! gest\u00f6rt hab' ich die goldene", "tokens": ["Hei\u00b7lig", "We\u00b7sen", "!", "ge\u00b7st\u00f6rt", "hab'", "ich", "die", "gol\u00b7de\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "VVPP", "VAFIN", "PPER", "ART", "ADJA"], "meter": "+-+--+-+-+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "G\u00f6tterruhe Dir oft, und der geheimeren,", "tokens": ["G\u00f6t\u00b7ter\u00b7ru\u00b7he", "Dir", "oft", ",", "und", "der", "ge\u00b7hei\u00b7me\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "$,", "KON", "ART", "ADJA", "$,"], "meter": "+-+--+-+-+--", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Tiefern Schmerzen des Lebens", "tokens": ["Tie\u00b7fern", "Schmer\u00b7zen", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Hast Du manche, getrennt von mir.", "tokens": ["Hast", "Du", "man\u00b7che", ",", "ge\u00b7trennt", "von", "mir", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIS", "$,", "VVPP", "APPR", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "O vergi\u00df es, vergieb! gleich dem Gew\u00f6lke dort", "tokens": ["O", "ver\u00b7gi\u00df", "es", ",", "ver\u00b7gieb", "!", "gleich", "dem", "Ge\u00b7w\u00f6l\u00b7ke", "dort"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$,", "VVIMP", "$.", "ADV", "ART", "NN", "ADV"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Vor dem friedlichen Mond, geh' ich dahin und Du", "tokens": ["Vor", "dem", "fried\u00b7li\u00b7chen", "Mond", ",", "geh'", "ich", "da\u00b7hin", "und", "Du"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "VVFIN", "PPER", "PAV", "KON", "PPER"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Ruhst und gl\u00e4nzest in deiner", "tokens": ["Ruhst", "und", "gl\u00e4n\u00b7zest", "in", "dei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "APPR", "PPOSAT"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Sch\u00f6ne wieder, du s\u00fc\u00dfes Licht!", "tokens": ["Sch\u00f6\u00b7ne", "wie\u00b7der", ",", "du", "s\u00fc\u00b7\u00dfes", "Licht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}