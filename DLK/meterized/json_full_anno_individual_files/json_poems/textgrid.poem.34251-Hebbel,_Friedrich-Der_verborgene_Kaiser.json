{"textgrid.poem.34251": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der verborgene Kaiser", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihre K\u00f6nige kennen die V\u00f6lker der Erde: sie rollen", "tokens": ["Ih\u00b7re", "K\u00f6\u00b7ni\u00b7ge", "ken\u00b7nen", "die", "V\u00f6l\u00b7ker", "der", "Er\u00b7de", ":", "sie", "rol\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$.", "PPER", "VVINF"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Stolz in Carossen daher, Trommeln und Fahnen voran;", "tokens": ["Stolz", "in", "Ca\u00b7ros\u00b7sen", "da\u00b7her", ",", "Trom\u00b7meln", "und", "Fah\u00b7nen", "vo\u00b7ran", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "PAV", "$,", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Aber sie haben zugleich auch einen verborgenen Kaiser,", "tokens": ["A\u00b7ber", "sie", "ha\u00b7ben", "zu\u00b7gleich", "auch", "ei\u00b7nen", "ver\u00b7bor\u00b7ge\u00b7nen", "Kai\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Welcher am Brunnen vielleicht selber das Wasser sich sch\u00f6pft,", "tokens": ["Wel\u00b7cher", "am", "Brun\u00b7nen", "viel\u00b7leicht", "sel\u00b7ber", "das", "Was\u00b7ser", "sich", "sch\u00f6pft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "APPRART", "NN", "ADV", "ADV", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Und, sei dieser ein K\u00fcnstler, ein Denker oder ein Weiser,", "tokens": ["Und", ",", "sei", "die\u00b7ser", "ein", "K\u00fcnst\u00b7ler", ",", "ein", "Den\u00b7ker", "o\u00b7der", "ein", "Wei\u00b7ser", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VAFIN", "PDAT", "ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Eh' das Jahrhundert vergeht, tr\u00e4gt er die Krone allein.", "tokens": ["Eh'", "das", "Jahr\u00b7hun\u00b7dert", "ver\u00b7geht", ",", "tr\u00e4gt", "er", "die", "Kro\u00b7ne", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "ART", "NN", "ADV", "$."], "meter": "+-+---++--+--+", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Ihre K\u00f6nige kennen die V\u00f6lker der Erde: sie rollen", "tokens": ["Ih\u00b7re", "K\u00f6\u00b7ni\u00b7ge", "ken\u00b7nen", "die", "V\u00f6l\u00b7ker", "der", "Er\u00b7de", ":", "sie", "rol\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$.", "PPER", "VVINF"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Stolz in Carossen daher, Trommeln und Fahnen voran;", "tokens": ["Stolz", "in", "Ca\u00b7ros\u00b7sen", "da\u00b7her", ",", "Trom\u00b7meln", "und", "Fah\u00b7nen", "vo\u00b7ran", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "PAV", "$,", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+--+-+-+--+--+", "measure": "iamb