{"textgrid.poem.56287": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Der Sohn der Witwe", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Her zogen die Schw\u00e4ne mit Kriegsgesang:", "tokens": ["Her", "zo\u00b7gen", "die", "Schw\u00e4\u00b7ne", "mit", "Kriegs\u00b7ge\u00b7sang", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Zu Ro\u00df, zu Ro\u00df! es dr\u00f6hnend erklang.", "tokens": ["Zu", "Ro\u00df", ",", "zu", "Ro\u00df", "!", "es", "dr\u00f6h\u00b7nend", "er\u00b7klang", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$.", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Es reiten aus allen H\u00f6fen umher", "tokens": ["Es", "rei\u00b7ten", "aus", "al\u00b7len", "H\u00f6\u00b7fen", "um\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "PTKVZ"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die j\u00fcngern S\u00f6hne zum Kriegesheer.", "tokens": ["Die", "j\u00fcn\u00b7gern", "S\u00f6h\u00b7ne", "zum", "Krie\u00b7ges\u00b7heer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Es ist mit uns gar schlimm bestellt,", "tokens": ["Es", "ist", "mit", "uns", "gar", "schlimm", "be\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und keiner bleibt, wenn einer sich stellt.", "tokens": ["Und", "kei\u00b7ner", "bleibt", ",", "wenn", "ei\u00b7ner", "sich", "stellt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$,", "KOUS", "PIS", "PRF", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Du ziehst, mein Br\u00e4ut'gam, mein Bruder, mein Sohn,", "tokens": ["Du", "ziehst", ",", "mein", "Br\u00e4ut'\u00b7gam", ",", "mein", "Bru\u00b7der", ",", "mein", "Sohn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPOSAT", "NE", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Du ziehst in den Krieg, das wissen wir schon.", "tokens": ["Du", "ziehst", "in", "den", "Krieg", ",", "das", "wis\u00b7sen", "wir", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "PDS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Wir Frauen bedienen den Kriegesknecht,", "tokens": ["Wir", "Frau\u00b7en", "be\u00b7die\u00b7nen", "den", "Krie\u00b7ges\u00b7knecht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Den Helmbusch steckt die Braut dir zurecht,", "tokens": ["Den", "Helm\u00b7busch", "steckt", "die", "Braut", "dir", "zu\u00b7recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.6": {"line.1": {"text": "Den Rappen f\u00fchrt die Schwester dir vor,", "tokens": ["Den", "Rap\u00b7pen", "f\u00fchrt", "die", "Schwes\u00b7ter", "dir", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Dir \u00f6ffnet die Mutter des Hofes Tor.", "tokens": ["Dir", "\u00f6ff\u00b7net", "die", "Mut\u00b7ter", "des", "Ho\u00b7fes", "Tor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "NE", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.7": {"line.1": {"text": "Wann kehrst du, mein Br\u00e4ut'gam, mein Bruder, mein Kind,", "tokens": ["Wann", "kehrst", "du", ",", "mein", "Br\u00e4ut'\u00b7gam", ",", "mein", "Bru\u00b7der", ",", "mein", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "PPOSAT", "NE", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Wann kehrst du zur\u00fcck? das sag uns geschwind. \u2013", "tokens": ["Wann", "kehrst", "du", "zu\u00b7r\u00fcck", "?", "das", "sag", "uns", "ge\u00b7schwind", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PTKVZ", "$.", "PDS", "VVFIN", "PPER", "ADJD", "$.", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Sind Luft und Wasser und Land erst frei,", "tokens": ["Sind", "Luft", "und", "Was\u00b7ser", "und", "Land", "erst", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "KON", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dann s\u00e4um ich nicht l\u00e4nger, dann eil ich herbei. \u2013", "tokens": ["Dann", "s\u00e4um", "ich", "nicht", "l\u00e4n\u00b7ger", ",", "dann", "eil", "ich", "her\u00b7bei", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PTKNEG", "ADJD", "$,", "ADV", "KOUS", "PPER", "PTKVZ", "$.", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.9": {"line.1": {"text": "Und Luft und Wasser und Land sind frei,", "tokens": ["Und", "Luft", "und", "Was\u00b7ser", "und", "Land", "sind", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "KON", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Was s\u00e4umt er noch l\u00e4nger, und eilt nicht herbei?", "tokens": ["Was", "s\u00e4umt", "er", "noch", "l\u00e4n\u00b7ger", ",", "und", "eilt", "nicht", "her\u00b7bei", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADJD", "$,", "KON", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "-+--+-+++-+", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "Wir Frauen, wir wollen entgegen ihm gehn,", "tokens": ["Wir", "Frau\u00b7en", ",", "wir", "wol\u00b7len", "ent\u00b7ge\u00b7gen", "ihm", "gehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PPER", "VMFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wir wollen vom H\u00fcgel entgegen ihm sehn.", "tokens": ["Wir", "wol\u00b7len", "vom", "H\u00fc\u00b7gel", "ent\u00b7ge\u00b7gen", "ihm", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.11": {"line.1": {"text": "Dort harren die Frauen und lauschen zu Tal", "tokens": ["Dort", "har\u00b7ren", "die", "Frau\u00b7en", "und", "lau\u00b7schen", "zu", "Tal"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "VVFIN", "APPR", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die Stra\u00dfe entlang im Sonnenstrahl.", "tokens": ["Die", "Stra\u00b7\u00dfe", "ent\u00b7lang", "im", "Son\u00b7nen\u00b7strahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPO", "APPRART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.12": {"line.1": {"text": "Und auf und nieder die Sonne steigt,", "tokens": ["Und", "auf", "und", "nie\u00b7der", "die", "Son\u00b7ne", "steigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "KON", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Kein Reitersmann dem Blicke sich zeigt.", "tokens": ["Kein", "Rei\u00b7ters\u00b7mann", "dem", "Bli\u00b7cke", "sich", "zeigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.13": {"line.1": {"text": "Jetzt hebt sich Staub, jetzt kommt im Lauf", "tokens": ["Jetzt", "hebt", "sich", "Staub", ",", "jetzt", "kommt", "im", "Lauf"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "NN", "$,", "ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Rappe daher \u2013 kein Reiter sitzt drauf.", "tokens": ["Ein", "Rap\u00b7pe", "da\u00b7her", "\u2013", "kein", "Rei\u00b7ter", "sitzt", "drauf", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "$(", "PIAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.14": {"line.1": {"text": "Sie fangen ihn ein, sie fragen ihn aus:", "tokens": ["Sie", "fan\u00b7gen", "ihn", "ein", ",", "sie", "fra\u00b7gen", "ihn", "aus", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie kommst du, mein Rappe, doch ledig nach Haus?", "tokens": ["Wie", "kommst", "du", ",", "mein", "Rap\u00b7pe", ",", "doch", "le\u00b7dig", "nach", "Haus", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "ADV", "ADJD", "APPR", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.15": {"line.1": {"text": "Bist, schlechter Gaul, dem Herrn du entflohn?", "tokens": ["Bist", ",", "schlech\u00b7ter", "Gaul", ",", "dem", "Herrn", "du", "ent\u00b7flohn", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "ADJA", "NN", "$,", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Wo blieb mein Br\u00e4ut'gam, mein Bruder, mein Sohn?", "tokens": ["Wo", "blieb", "mein", "Br\u00e4ut'\u00b7gam", ",", "mein", "Bru\u00b7der", ",", "mein", "Sohn", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NE", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.16": {"line.1": {"text": "Sie haben erschossen ihn in der Schlacht,", "tokens": ["Sie", "ha\u00b7ben", "er\u00b7schos\u00b7sen", "ihn", "in", "der", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Auf gr\u00fcner Heide sein Bett ihm gemacht.", "tokens": ["Auf", "gr\u00fc\u00b7ner", "Hei\u00b7de", "sein", "Bett", "ihm", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.17": {"line.1": {"text": "Mich lie\u00dfen sie laufen in alle Welt,", "tokens": ["Mich", "lie\u00b7\u00dfen", "sie", "lau\u00b7fen", "in", "al\u00b7le", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ich habe die Botschaft trauernd bestellt.", "tokens": ["Ich", "ha\u00b7be", "die", "Bot\u00b7schaft", "trau\u00b7ernd", "be\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.18": {"line.1": {"text": "Es zogen drei Schw\u00e4ne mit Klaggesang,", "tokens": ["Es", "zo\u00b7gen", "drei", "Schw\u00e4\u00b7ne", "mit", "Klag\u00b7ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ein Grab zu suchen, die Heide entlang.", "tokens": ["Ein", "Grab", "zu", "su\u00b7chen", ",", "die", "Hei\u00b7de", "ent\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.19": {"line.1": {"text": "Sie lie\u00dfen sich nieder, wie sie es ersahn,", "tokens": ["Sie", "lie\u00b7\u00dfen", "sich", "nie\u00b7der", ",", "wie", "sie", "es", "er\u00b7sahn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKVZ", "$,", "PWAV", "PPER", "PPER", "VVPP", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zu F\u00fc\u00dfen, zu Haupte, zur Seite ein Schwan.", "tokens": ["Zu", "F\u00fc\u00b7\u00dfen", ",", "zu", "Haup\u00b7te", ",", "zur", "Sei\u00b7te", "ein", "Schwan", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$,", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.20": {"line.1": {"text": "Zu Haupte die Schwester, zu F\u00fc\u00dfen die Braut,", "tokens": ["Zu", "Haup\u00b7te", "die", "Schwes\u00b7ter", ",", "zu", "F\u00fc\u00b7\u00dfen", "die", "Braut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Zur Seite die Mutter, hoch ergraut:", "tokens": ["Zur", "Sei\u00b7te", "die", "Mut\u00b7ter", ",", "hoch", "er\u00b7graut", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "ADJD", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.21": {"line.1": {"text": "O wehe, weh, Verwaisten uns drei'n!", "tokens": ["O", "we\u00b7he", ",", "weh", ",", "Ver\u00b7wais\u00b7ten", "uns", "drei'n", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "PTKVZ", "$,", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Wer stimmt in unsre Klage mit ein?", "tokens": ["Wer", "stimmt", "in", "uns\u00b7re", "Kla\u00b7ge", "mit", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "PPOSAT", "NN", "APPR", "ART", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.22": {"line.1": {"text": "Darauf die Sonne, sich neigend, begann:", "tokens": ["Da\u00b7rauf", "die", "Son\u00b7ne", ",", "sich", "nei\u00b7gend", ",", "be\u00b7gann", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "ART", "NN", "$,", "PRF", "ADJD", "$,", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich stimme mit ein, so gut ich kann.", "tokens": ["Ich", "stim\u00b7me", "mit", "ein", ",", "so", "gut", "ich", "kann", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "$,", "ADV", "ADJD", "PPER", "VMFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.23": {"line.1": {"text": "Neun Tage traur ich in Nebelflor,", "tokens": ["Neun", "Ta\u00b7ge", "traur", "ich", "in", "Ne\u00b7bel\u00b7flor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "APPR", "