{"textgrid.poem.43993": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: M\u00fcdes Herz,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "M\u00fcdes Herz,", "tokens": ["M\u00fc\u00b7des", "Herz", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Las den Schmerz", "tokens": ["Las", "den", "Schmerz"], "token_info": ["word", "word", "word"], "pos": ["NE", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Mit dem Athem fahren!", "tokens": ["Mit", "dem", "A\u00b7them", "fah\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Lebstu doch", "tokens": ["Lebs\u00b7tu", "doch"], "token_info": ["word", "word"], "pos": ["NE", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Jezo noch", "tokens": ["Je\u00b7zo", "noch"], "token_info": ["word", "word"], "pos": ["ADV", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "In den besten Jahren.", "tokens": ["In", "den", "bes\u00b7ten", "Jah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Thoren dencken vor der Zeit", "tokens": ["Tho\u00b7ren", "den\u00b7cken", "vor", "der", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "An die Nacht der Eitelkeit;", "tokens": ["An", "die", "Nacht", "der", "Ei\u00b7tel\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Gnug, wenn uns das Alter zwingt", "tokens": ["Gnug", ",", "wenn", "uns", "das", "Al\u00b7ter", "zwingt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Und den Kummer mit sich bringt.", "tokens": ["Und", "den", "Kum\u00b7mer", "mit", "sich", "bringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Alle Noth,", "tokens": ["Al\u00b7le", "Noth", ","], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Die uns droht,", "tokens": ["Die", "uns", "droht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Kommt von eignem Wahne;", "tokens": ["Kommt", "von", "eig\u00b7nem", "Wah\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Da\u00df das Weh", "tokens": ["Da\u00df", "das", "Weh"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Bald vergeh,", "tokens": ["Bald", "ver\u00b7geh", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Bohrt man nicht im Zahne.", "tokens": ["Bohrt", "man", "nicht", "im", "Zah\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PTKNEG", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Unser m\u00fcrrischer Verdru\u00df", "tokens": ["Un\u00b7ser", "m\u00fcr\u00b7ri\u00b7scher", "Ver\u00b7dru\u00df"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ist wie ein gesalzner Flu\u00df,", "tokens": ["Ist", "wie", "ein", "ge\u00b7salz\u00b7ner", "Flu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Der, je mehr man Thr\u00e4nen reizt,", "tokens": ["Der", ",", "je", "mehr", "man", "Thr\u00e4\u00b7nen", "reizt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "ADV", "PIS", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Wang und Auge sch\u00e4rfer beizt.", "tokens": ["Wang", "und", "Au\u00b7ge", "sch\u00e4r\u00b7fer", "beizt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Br\u00fcder, wir", "tokens": ["Br\u00fc\u00b7der", ",", "wir"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Sind jezt hier,", "tokens": ["Sind", "jezt", "hier", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Und wer weis wie lange?", "tokens": ["Und", "wer", "weis", "wie", "lan\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PTKVZ", "KOKOM", "ADV", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Jeder Schritt", "tokens": ["Je\u00b7der", "Schritt"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Ist ein Tritt", "tokens": ["Ist", "ein", "Tritt"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Zu dem lezten Gange.", "tokens": ["Zu", "dem", "lez\u00b7ten", "Gan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Nehmt die Wollust zum Voraus", "tokens": ["Nehmt", "die", "Wol\u00b7lust", "zum", "Vo\u00b7raus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Und besucht das Freudenhaus,", "tokens": ["Und", "be\u00b7sucht", "das", "Freu\u00b7den\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Eh ein ungewi\u00dfer Tag", "tokens": ["Eh", "ein", "un\u00b7ge\u00b7wi\u00b7\u00dfer", "Tag"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Uns der Baare liefern mag.", "tokens": ["Uns", "der", "Baa\u00b7re", "lie\u00b7fern", "mag."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["PPER", "ART", "NN", "VVFIN", "NE"], "meter": "--+-+-", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Glaubt doch nur,", "tokens": ["Glaubt", "doch", "nur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Epicur", "tokens": ["E\u00b7pi\u00b7cur"], "token_info": ["word"], "pos": ["NN"], "meter": "---", "measure": "unknown.measure.zero"}, "line.3": {"text": "Macht die kl\u00fcgsten Weisen;", "tokens": ["Macht", "die", "kl\u00fcgs\u00b7ten", "Wei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Die Vernunft", "tokens": ["Die", "Ver\u00b7nunft"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Seiner Zunft", "tokens": ["Sei\u00b7ner", "Zunft"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Sprengt die Foltereisen,", "tokens": ["Sprengt", "die", "Fol\u00b7ter\u00b7ei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Die der Aberglaube st\u00e4hlt,", "tokens": ["Die", "der", "A\u00b7berg\u00b7lau\u00b7be", "st\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wenn er schlechte Seelen qu\u00e4lt", "tokens": ["Wenn", "er", "schlech\u00b7te", "See\u00b7len", "qu\u00e4lt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Und des P\u00f6bels bl\u00f6den Geist", "tokens": ["Und", "des", "P\u00f6\u00b7bels", "bl\u00f6\u00b7den", "Geist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "In die Nacht des Irrthums rei\u00dft.", "tokens": ["In", "die", "Nacht", "des", "Irr\u00b7thums", "rei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Diese Nacht", "tokens": ["Die\u00b7se", "Nacht"], "token_info": ["word", "word"], "pos": ["PDAT", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Giebt uns Macht,", "tokens": ["Giebt", "uns", "Macht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Franck und frey zu leben.", "tokens": ["Franck", "und", "frey", "zu", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Jeder Stern", "tokens": ["Je\u00b7der", "Stern"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Sieht es gern,", "tokens": ["Sieht", "es", "gern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Da\u00df wir Feuer geben;", "tokens": ["Da\u00df", "wir", "Feu\u00b7er", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Unsre B\u00fcchsen sind zwar Thon,", "tokens": ["Uns\u00b7re", "B\u00fcch\u00b7sen", "sind", "zwar", "Thon", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Aber sie verjagen schon", "tokens": ["A\u00b7ber", "sie", "ver\u00b7ja\u00b7gen", "schon"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Aller Grillen starckes Heer,", "tokens": ["Al\u00b7ler", "Gril\u00b7len", "star\u00b7ckes", "Heer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Wenn es noch so heftig w\u00e4r.", "tokens": ["Wenn", "es", "noch", "so", "hef\u00b7tig", "w\u00e4r", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Nehmt doch wahr,", "tokens": ["Nehmt", "doch", "wahr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Wie sogar", "tokens": ["Wie", "so\u00b7gar"], "token_info": ["word", "word"], "pos": ["PWAV", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Todte Kr\u00e4uter lehren!", "tokens": ["Tod\u00b7te", "Kr\u00e4u\u00b7ter", "leh\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Last uns noch,", "tokens": ["Last", "uns", "noch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Last uns doch", "tokens": ["Last", "uns", "doch"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Ihre Warnung h\u00f6ren!", "tokens": ["Ih\u00b7re", "War\u00b7nung", "h\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "So verfliegt der sachte Rauch,", "tokens": ["So", "ver\u00b7fliegt", "der", "sach\u00b7te", "Rauch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.8": {"text": "So verfliegt das Leben auch,", "tokens": ["So", "ver\u00b7fliegt", "das", "Le\u00b7ben", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Und die Asche mahlet hier", "tokens": ["Und", "die", "A\u00b7sche", "mah\u00b7let", "hier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Unsers Leichnahms Bildn\u00fc\u00df f\u00fcr.", "tokens": ["Un\u00b7sers", "Leich\u00b7nahms", "Bild\u00b7n\u00fc\u00df", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "APPR", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Nun wohlan,", "tokens": ["Nun", "wo\u00b7hlan", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADV", "$,"], "meter": "+--", "measure": "dactylic.init"}, "line.2": {"text": "Nehmt doch an!", "tokens": ["Nehmt", "doch", "an", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Hier ist Engell\u00e4nder,", "tokens": ["Hier", "ist", "En\u00b7gel\u00b7l\u00e4n\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "De\u00dfen Dampf", "tokens": ["De\u00b7\u00dfen", "Dampf"], "token_info": ["word", "word"], "pos": ["ADV", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Trozt den Kampf", "tokens": ["Trozt", "den", "Kampf"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Aller Tobackssch\u00e4nder.", "tokens": ["Al\u00b7ler", "To\u00b7backs\u00b7sch\u00e4n\u00b7der", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Kostet auch den Wurzner Saft!", "tokens": ["Kos\u00b7tet", "auch", "den", "Wurz\u00b7ner", "Saft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Gerstenblut macht Br\u00fcderschaft;", "tokens": ["Gers\u00b7ten\u00b7blut", "macht", "Br\u00fc\u00b7der\u00b7schaft", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Treu und ofenherzig seyn", "tokens": ["Treu", "und", "o\u00b7fen\u00b7her\u00b7zig", "seyn"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "VAINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Fl\u00f6st mit diesen Str\u00f6men ein!", "tokens": ["Fl\u00f6st", "mit", "die\u00b7sen", "Str\u00f6\u00b7men", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Dieser Schlung,", "tokens": ["Die\u00b7ser", "Schlung", ","], "token_info": ["word", "word", "punct"], "pos": ["PDAT", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Dieser Trunck", "tokens": ["Die\u00b7ser", "Trunck"], "token_info": ["word", "word"], "pos": ["PDAT", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Geht auf das Vergn\u00fcgen", "tokens": ["Geht", "auf", "das", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Derer, die