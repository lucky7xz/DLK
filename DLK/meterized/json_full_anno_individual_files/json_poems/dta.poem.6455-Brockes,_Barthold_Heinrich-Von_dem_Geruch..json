{"dta.poem.6455": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Geruch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In einem C\u00f6rper wird erzeuget und vermehret,", "tokens": ["In", "ei\u00b7nem", "C\u00f6r\u00b7per", "wird", "er\u00b7zeu\u00b7get", "und", "ver\u00b7meh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn er beweget wird, des Niechens Krafft.", "tokens": ["Wenn", "er", "be\u00b7we\u00b7get", "wird", ",", "des", "Nie\u00b7chens", "Krafft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "VAFIN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Ambra hat die Eigenschafft,", "tokens": ["Der", "A\u00b7mbra", "hat", "die", "Ei\u00b7gen\u00b7schafft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er riecht, wenn man ihn reibt. Wenn Feuer Wachs verzehret,", "tokens": ["Er", "riecht", ",", "wenn", "man", "ihn", "reibt", ".", "Wenn", "Feu\u00b7er", "Wachs", "ver\u00b7zeh\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PIS", "PPER", "VVFIN", "$.", "KOUS", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Vergn\u00fcgt es den Geruch. Nicht minder, wenn im Lenzen", "tokens": ["Ver\u00b7gn\u00fcgt", "es", "den", "Ge\u00b7ruch", ".", "Nicht", "min\u00b7der", ",", "wenn", "im", "Len\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$.", "PTKNEG", "ADV", "$,", "KOUS", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Morgenr\u00f6the Flei\u00df auf die bebl\u00fchmte Au\u2019", "tokens": ["Der", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", "Flei\u00df", "auf", "die", "be\u00b7bl\u00fchm\u00b7te", "Au'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die s\u00fcssen Thr\u00e4nen giesst, den Seegen-reichen Thau,", "tokens": ["Die", "s\u00fcs\u00b7sen", "Thr\u00e4\u00b7nen", "giesst", ",", "den", "See\u00b7gen\u00b7rei\u00b7chen", "Thau", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und da\u00df der Sonnen Gluht, wodurch die L\u00fcffte gl\u00e4ntzen,", "tokens": ["Und", "da\u00df", "der", "Son\u00b7nen", "Gluht", ",", "wo\u00b7durch", "die", "L\u00fcff\u00b7te", "gl\u00e4nt\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "NN", "$,", "PWAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Den Schmeltz der Wiesen d\u00f6rrt, der Farben Schmuck beseelet,", "tokens": ["Den", "Sch\u00b7meltz", "der", "Wie\u00b7sen", "d\u00f6rrt", ",", "der", "Far\u00b7ben", "Schmuck", "be\u00b7see\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Der junge Zephir dann zu der Vollkommenheit", "tokens": ["Der", "jun\u00b7ge", "Ze\u00b7phir", "dann", "zu", "der", "Voll\u00b7kom\u00b7men\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Derjenigen, die er zu seiner Lieb\u2019 erwehlet,", "tokens": ["Der\u00b7je\u00b7ni\u00b7gen", ",", "die", "er", "zu", "sei\u00b7ner", "Lieb'", "er\u00b7weh\u00b7let", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der Seufzer Lauigkeit,", "tokens": ["Der", "Seuf\u00b7zer", "Lau\u00b7ig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Die n\u00fctz-und fruchtbar, treibt; er Florens s\u00fcssen Geist", "tokens": ["Die", "n\u00fctz\u00b7\u00b7und", "frucht\u00b7bar", ",", "treibt", ";", "er", "Flo\u00b7rens", "s\u00fcs\u00b7sen", "Geist"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "$,", "VVFIN", "$.", "PPER", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "An allen Orten f\u00fchrt. Da von den Blumen sich", "tokens": ["An", "al\u00b7len", "Or\u00b7ten", "f\u00fchrt", ".", "Da", "von", "den", "Blu\u00b7men", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$.", "ADV", "APPR", "ART", "NN", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ein Geist voll Balsam reisst.", "tokens": ["Ein", "Geist", "voll", "Bal\u00b7sam", "reisst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}