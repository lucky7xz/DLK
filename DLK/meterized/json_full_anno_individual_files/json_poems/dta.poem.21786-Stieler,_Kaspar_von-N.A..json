{"dta.poem.21786": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Qwell aller Liebligkeit/ dehm Orfeus sei-", "tokens": ["Qwell", "al\u00b7ler", "Lieb\u00b7lig\u00b7keit", "/", "dehm", "Or\u00b7feus", "sei"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PIAT", "NN", "$(", "ART", "NE", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "ne Weisen", "tokens": ["ne", "Wei\u00b7sen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "mit Zukker fl\u00f6sset ein/ dehn das Sarmater-", "tokens": ["mit", "Zuk\u00b7ker", "fl\u00f6s\u00b7set", "ein", "/", "dehn", "das", "Sar\u00b7ma\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ART", "$(", "VVIMP", "ART", "TRUNC"], "meter": "-+-+--+-+--", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Land", "tokens": ["Land"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "bi\u00df an den Adriac und seine Macht be-", "tokens": ["bi\u00df", "an", "den", "Ad\u00b7ri\u00b7ac", "und", "sei\u00b7ne", "Macht", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "KON", "PPOSAT", "NN", "TRUNC"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.6": {"text": "kant/", "tokens": ["kant", "/"], "token_info": ["word", "punct"], "pos": ["ADJD", "$("], "meter": "+", "measure": "single.up"}, "line.7": {"text": "den Tamesis/ der Sund und jenes Reich das", "tokens": ["den", "Ta\u00b7me\u00b7sis", "/", "der", "Sund", "und", "je\u00b7nes", "Reich", "das"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "$(", "ART", "NN", "KON", "PDAT", "NN", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Eisen", "tokens": ["Ei\u00b7sen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "zu Gold und Silber macht/ vor F\u00f6bus S\u00e4n-", "tokens": ["zu", "Gold", "und", "Sil\u00b7ber", "macht", "/", "vor", "F\u00f6\u00b7bus", "S\u00e4n"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$(", "APPR", "NE", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "ger preisen.", "tokens": ["ger", "prei\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Und du gebrudert-Paar/ das mir im Un-", "tokens": ["Und", "du", "ge\u00b7bru\u00b7der\u00b7tPaar", "/", "das", "mir", "im", "Un"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "NN", "$(", "PRELS", "PPER", "APPRART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "gl\u00fckks-stand'", "tokens": ["gl\u00fc\u00b7kks\u00b7stand'"], "token_info": ["word"], "pos": ["NE"], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "in dehm die Freund schafft siegt/ boht Guht", "tokens": ["in", "dehm", "die", "Freund", "schafft", "siegt", "/", "boht", "Guht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PRELS", "ART", "NN", "VVFIN", "VVFIN", "$(", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "und treue Hand/", "tokens": ["und", "treu\u00b7e", "Hand", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "als mich ein Nord verfolgt\u2019 in meinen Nor-", "tokens": ["als", "mich", "ein", "Nord", "ver\u00b7folgt'", "in", "mei\u00b7nen", "Nor"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "den-reisen.", "tokens": ["den\u00b7rei\u00b7sen", "."], "token_info": ["word", "punct"], "pos": ["PDS", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.17": {"text": "Gegr\u00fc\u00dft/ gek\u00fc\u00dft/ geehrt seyd ihr au\u00df dieser", "tokens": ["Ge\u00b7gr\u00fc\u00dft", "/", "ge\u00b7k\u00fc\u00dft", "/", "ge\u00b7ehrt", "seyd", "ihr", "au\u00df", "die\u00b7ser"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$(", "VVPP", "$(", "VVPP", "VAFIN", "PPER", "APPR", "PDAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Stat/", "tokens": ["Stat", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.19": {"text": "die mit der Balter-See Geschwister-b\u00fcnd-", "tokens": ["die", "mit", "der", "Bal\u00b7ter\u00b7See", "Ge\u00b7schwis\u00b7ter\u00b7b\u00fcn\u00b7d"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "n\u00fc\u00df hat.", "tokens": ["n\u00fc\u00df", "hat", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.21": {"text": "Ich stunde bey mir an/ wehm ich das dritte", "tokens": ["Ich", "stun\u00b7de", "bey", "mir", "an", "/", "wehm", "ich", "das", "drit\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PTKVZ", "$(", "PWS", "PPER", "ART", "ADJA"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Zehen", "tokens": ["Ze\u00b7hen"], "token_info": ["word"], "pos": ["CARD"], "meter": "+-", "measure": "trochaic.single"}, "line.23": {"text": "ergeben solt\u2019. Indehm hat Amor l\u00e4ch-", "tokens": ["er\u00b7ge\u00b7ben", "solt'", ".", "In\u00b7dehm", "hat", "A\u00b7mor", "l\u00e4ch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVINF", "VMFIN", "$.", "PPER", "VAFIN", "NE", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "lend mich", "tokens": ["lend", "mich"], "token_info": ["word", "word"], "pos": ["ADJD", "PPER"], "meter": "-+", "measure": "iambic.single"}, "line.25": {"text": "und Venus/ wei\u00df entr\u00fcst im Harnisch ange-", "tokens": ["und", "Ve\u00b7nus", "/", "wei\u00df", "ent\u00b7r\u00fcst", "im", "Har\u00b7nisch", "an\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "$(", "VVFIN", "ADV", "APPRART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "sehen.", "tokens": ["se\u00b7hen", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.27": {"text": "Undankbahr Filidor (sprach die) besinnstu", "tokens": ["Un\u00b7dank\u00b7bahr", "Fi\u00b7li\u00b7dor", "(", "sprach", "die", ")", "be\u00b7sinns\u00b7tu"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NE", "NE", "$(", "VVFIN", "ART", "$(", "XY"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.28": {"text": "dich?", "tokens": ["dich", "?"], "token_info": ["word", "punct"], "pos": ["PPER", "$."], "meter": "-", "measure": "single.down"}, "line.29": {"text": "Eumelus hat gemacht/ da\u00df ich besungen", "tokens": ["Eu\u00b7me\u00b7lus", "hat", "ge\u00b7macht", "/", "da\u00df", "ich", "be\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VAFIN", "VVPP", "$(", "KOUS", "PPER", "VVINF"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.30": {"text": "bin", "tokens": ["bin"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "+", "measure": "single.up"}, "line.31": {"text": "von dir: die Tingrien sind sie dir au\u00df dem", "tokens": ["von", "dir", ":", "die", "Tin\u00b7gri\u00b7en", "sind", "sie", "dir", "au\u00df", "dem"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "$.", "ART", "NN", "VAFIN", "PPER", "PPER", "APPR", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Sinn:", "tokens": ["Sinn", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}}}}}