{"textgrid.poem.66133": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Zwischen Tag und Abend", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "D\u00e4mmerung bringt mit weichen H\u00e4nden", "tokens": ["D\u00e4m\u00b7me\u00b7rung", "bringt", "mit", "wei\u00b7chen", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Friedevolle Ruh,", "tokens": ["Frie\u00b7de\u00b7vol\u00b7le", "Ruh", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Tr\u00e4umen ohne Ende,", "tokens": ["Tr\u00e4u\u00b7men", "oh\u00b7ne", "En\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wandeln ohne Wende,", "tokens": ["Wan\u00b7deln", "oh\u00b7ne", "Wen\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Sch\u00f6nern Sternen zu.", "tokens": ["Sch\u00f6\u00b7nern", "Ster\u00b7nen", "zu", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Und ich f\u00fchl mich hingetragen,", "tokens": ["Und", "ich", "f\u00fchl", "mich", "hin\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo die reinen Flammen wehn,", "tokens": ["Wo", "die", "rei\u00b7nen", "Flam\u00b7men", "wehn", "