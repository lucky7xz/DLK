{"dta.poem.9605": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ein kupferh\u00e4ndler an seine liebste.  \n C. S. L.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du tadelst/ eckler schatz/ nur immer meinen spiegel/", "tokens": ["Du", "ta\u00b7delst", "/", "eck\u00b7ler", "schatz", "/", "nur", "im\u00b7mer", "mei\u00b7nen", "spie\u00b7gel", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "ADJA", "NN", "$(", "ADV", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der/ wie du sp\u00f6ttisch sagst/ dir gar zu feurig scheint:", "tokens": ["Der", "/", "wie", "du", "sp\u00f6t\u00b7tisch", "sagst", "/", "dir", "gar", "zu", "feu\u00b7rig", "scheint", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "PWAV", "PPER", "ADJD", "VVFIN", "$(", "PPER", "ADV", "PTKA", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In deinen augen gleich ich einem kupfer-tiegel/", "tokens": ["In", "dei\u00b7nen", "au\u00b7gen", "gleich", "ich", "ei\u00b7nem", "kup\u00b7fer\u00b7tie\u00b7gel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mir selber bistu gut/ nur meiner r\u00f6the feind.", "tokens": ["Mir", "sel\u00b7ber", "bis\u00b7tu", "gut", "/", "nur", "mei\u00b7ner", "r\u00f6\u00b7the", "feind", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADJD", "$(", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es h\u00e4ufft ja deine brust auf schnee carfunckel-spitzen/", "tokens": ["Es", "h\u00e4ufft", "ja", "dei\u00b7ne", "brust", "auf", "schnee", "ca\u00b7rfun\u00b7ckel\u00b7spit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es schliessen deinen hal\u00df corallen-b\u00e4nder ein;", "tokens": ["Es", "schlies\u00b7sen", "dei\u00b7nen", "hal\u00df", "coral\u00b7len\u00b7b\u00e4n\u00b7der", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NE", "PTKVZ", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Und doch soll kein rubin auf meinen wangen blitzen/", "tokens": ["Und", "doch", "soll", "kein", "ru\u00b7bin", "auf", "mei\u00b7nen", "wan\u00b7gen", "blit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und ich mu\u00df dir zum spott ein jubilirer seyn.", "tokens": ["Und", "ich", "mu\u00df", "dir", "zum", "spott", "ein", "ju\u00b7bi\u00b7li\u00b7rer", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "APPRART", "NN", "ART", "ADJA", "VAINF", "$."], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.9": {"text": "Wie \u00e4rgert dich so sehr mein z\u00fcchtiges gesichte/", "tokens": ["Wie", "\u00e4r\u00b7gert", "dich", "so", "sehr", "mein", "z\u00fcch\u00b7ti\u00b7ges", "ge\u00b7sich\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Das angebohrne scham mit purpur \u00fcberf\u00e4rbt!", "tokens": ["Das", "an\u00b7ge\u00b7bohr\u00b7ne", "scham", "mit", "pur\u00b7pur", "\u00fc\u00b7berf\u00b7\u00e4rbt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Liebstu die erbarkeit/ so hasse nicht die fr\u00fcchte/", "tokens": ["Liebs\u00b7tu", "die", "er\u00b7bar\u00b7keit", "/", "so", "has\u00b7se", "nicht", "die", "fr\u00fcch\u00b7te", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$(", "ADV", "VVFIN", "PTKNEG", "ART", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Denn beydes hab\u2019 ich gleich vom vater angeerbt.", "tokens": ["Denn", "bey\u00b7des", "hab'", "ich", "gleich", "vom", "va\u00b7ter", "an\u00b7ge\u00b7erbt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "PPER", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wie kan mein scharlach doch so \u00fcbel dir gefallen?", "tokens": ["Wie", "kan", "mein", "schar\u00b7lach", "doch", "so", "\u00fc\u00b7bel", "dir", "ge\u00b7fal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPOSAT", "VVFIN", "ADV", "ADV", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Es sticht zinober ja die andern farben weg;", "tokens": ["Es", "sticht", "zi\u00b7no\u00b7ber", "ja", "die", "an\u00b7dern", "far\u00b7ben", "weg", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Dem purpur bleibet doch der vorzug unter allen;", "tokens": ["Dem", "pur\u00b7pur", "blei\u00b7bet", "doch", "der", "vor\u00b7zug", "un\u00b7ter", "al\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "ADV", "ART", "NN", "APPR", "PIAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wer in das rothe trift/ der fehlet nicht den zweck.", "tokens": ["Wer", "in", "das", "ro\u00b7the", "trift", "/", "der", "feh\u00b7let", "nicht", "den", "zweck", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "ADJA", "VVFIN", "$(", "ART", "VVFIN", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Aurora pfleget sich mit purpur auszukleiden/", "tokens": ["Au\u00b7ro\u00b7ra", "pfle\u00b7get", "sich", "mit", "pur\u00b7pur", "aus\u00b7zu\u00b7klei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "APPR", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wenn ihr beflammter glantz die finstre nacht verzehrt.", "tokens": ["Wenn", "ihr", "be\u00b7flamm\u00b7ter", "glantz", "die", "finst\u00b7re", "nacht", "ver\u00b7zehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Der regenbogen kan das licht der augen weiden/", "tokens": ["Der", "re\u00b7gen\u00b7bo\u00b7gen", "kan", "das", "licht", "der", "au\u00b7gen", "wei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VMFIN", "ART", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wenn dessen bunter crey\u00df den tr\u00fcben himmel kl\u00e4rt.", "tokens": ["Wenn", "des\u00b7sen", "bun\u00b7ter", "crey\u00df", "den", "tr\u00fc\u00b7ben", "him\u00b7mel", "kl\u00e4rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ADJD", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wer wolte den Vulcan de\u00dfwegen he\u00dflich nennen/", "tokens": ["Wer", "wol\u00b7te", "den", "Vul\u00b7can", "de\u00df\u00b7we\u00b7gen", "he\u00df\u00b7lich", "nen\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NE", "PAV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Dieweil er feuer-roth in seiner schmide schwitzt?", "tokens": ["Die\u00b7weil", "er", "feu\u00b7er\u00b7roth", "in", "sei\u00b7ner", "schmi\u00b7de", "schwitzt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "PPOSAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wer wolte nicht den Mars vor einen gott erkennen/", "tokens": ["Wer", "wol\u00b7te", "nicht", "den", "Mars", "vor", "ei\u00b7nen", "gott", "er\u00b7ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PTKNEG", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Wenn sein bef\u00e4rbtes kleid der feinde blut bespr\u00fctzt?", "tokens": ["Wenn", "sein", "be\u00b7f\u00e4rb\u00b7tes", "kleid", "der", "fein\u00b7de", "blut", "be\u00b7spr\u00fctzt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Was war wol lieblicher und sch\u00f6ner anzusehen/", "tokens": ["Was", "war", "wol", "lieb\u00b7li\u00b7cher", "und", "sch\u00f6\u00b7ner", "an\u00b7zu\u00b7se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "VVIZU", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Als da der rosen-dorn der Venus fu\u00df verletzt:", "tokens": ["Als", "da", "der", "ro\u00b7sen\u00b7dorn", "der", "Ve\u00b7nus", "fu\u00df", "ver\u00b7letzt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ART", "NN", "PTKVZ", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Als auf der marmol-haut ein sanfter ri\u00df geschehen/", "tokens": ["Als", "auf", "der", "mar\u00b7mol\u00b7haut", "ein", "sanf\u00b7ter", "ri\u00df", "ge\u00b7sche\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "ADJD", "VVFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Indem ihr schneller fu\u00df dem buhlen nachgesetzt?", "tokens": ["In\u00b7dem", "ihr", "schnel\u00b7ler", "fu\u00df", "dem", "buh\u00b7len", "nach\u00b7ge\u00b7setzt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PTKVZ", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Es zeigt gesundes blut der geister kraft und wesen/", "tokens": ["Es", "zeigt", "ge\u00b7sun\u00b7des", "blut", "der", "geis\u00b7ter", "kraft", "und", "we\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "ART", "ADJA", "NN", "KON", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und das gesunde blut ist anders nicht als roth.", "tokens": ["Und", "das", "ge\u00b7sun\u00b7de", "blut", "ist", "an\u00b7ders", "nicht", "als", "roth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "ADV", "PTKNEG", "KOUS", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wird solches angeschw\u00e4rtzt/ so sucht man zu genesen.", "tokens": ["Wird", "sol\u00b7ches", "an\u00b7ge\u00b7schw\u00e4rtzt", "/", "so", "sucht", "man", "zu", "ge\u00b7ne\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "VVPP", "$(", "ADV", "VVFIN", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.32": {"text": "Und ist es gar erstarrt/ so deutet es den tod.", "tokens": ["Und", "ist", "es", "gar", "er\u00b7starrt", "/", "so", "deu\u00b7tet", "es", "den", "tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "VVPP", "$(", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Es mag die gantze welt die schwartzen augen preisen/", "tokens": ["Es", "mag", "die", "gant\u00b7ze", "welt", "die", "schwart\u00b7zen", "au\u00b7gen", "prei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Es hebe wer da will derselben ruhm empor!", "tokens": ["Es", "he\u00b7be", "wer", "da", "will", "der\u00b7sel\u00b7ben", "ruhm", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PWS", "ADV", "VMFIN", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Weit sch\u00f6ner k\u00f6nnen sich die rothen lippen weisen/", "tokens": ["Weit", "sch\u00f6\u00b7ner", "k\u00f6n\u00b7nen", "sich", "die", "ro\u00b7then", "lip\u00b7pen", "wei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "VMFIN", "PRF", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Denn diese gehen auch den weissen br\u00fcsten vor.", "tokens": ["Denn", "die\u00b7se", "ge\u00b7hen", "auch", "den", "weis\u00b7sen", "br\u00fcs\u00b7ten", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ADV", "ART", "ADJA", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Wer fr\u00fchlings-rosen bricht/ der wil die r\u00f6thsten haben;", "tokens": ["Wer", "fr\u00fch\u00b7lings\u00b7ro\u00b7sen", "bricht", "/", "der", "wil", "die", "r\u00f6ths\u00b7ten", "ha\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "$(", "ART", "VMFIN", "ART", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Des sommers lesen wir die rothen beeren ab;", "tokens": ["Des", "som\u00b7mers", "le\u00b7sen", "wir", "die", "ro\u00b7then", "bee\u00b7ren", "ab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "PPER", "ART", "ADJA", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Die rothe \u00e4pfel sind des reifen herbstes gaben;", "tokens": ["Die", "ro\u00b7the", "\u00e4p\u00b7fel", "sind", "des", "rei\u00b7fen", "herbs\u00b7tes", "ga\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "ADJA", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Der todte winter ist ein allgemeines grab.", "tokens": ["Der", "tod\u00b7te", "win\u00b7ter", "ist", "ein", "all\u00b7ge\u00b7mei\u00b7nes", "grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "So mag den vorzug denn die rothe farbe nehmen/", "tokens": ["So", "mag", "den", "vor\u00b7zug", "denn", "die", "ro\u00b7the", "far\u00b7be", "neh\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "KON", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Weil himmel und natur nebst menschen sie verspricht:", "tokens": ["Weil", "him\u00b7mel", "und", "na\u00b7tur", "nebst", "men\u00b7schen", "sie", "ver\u00b7spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "APPR", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Und ich will nimmermehr mich meiner r\u00f6the sch\u00e4men;", "tokens": ["Und", "ich", "will", "nim\u00b7mer\u00b7mehr", "mich", "mei\u00b7ner", "r\u00f6\u00b7the", "sch\u00e4\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Auch acht\u2019 ich/ eckler schatz/ mehr deines spottens nicht.", "tokens": ["Auch", "acht'", "ich", "/", "eck\u00b7ler", "schatz", "/", "mehr", "dei\u00b7nes", "spot\u00b7tens", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "ADJA", "NN", "$(", "ADV", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Mein purpur mu\u00df bey dir ver\u00e4chtlich kupfer hei\u00dfen;", "tokens": ["Mein", "pur\u00b7pur", "mu\u00df", "bey", "dir", "ver\u00b7\u00e4cht\u00b7lich", "kup\u00b7fer", "hei\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "VMFIN", "APPR", "PPER", "ADJD", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Ob aber wol mein ertzt kein g\u00f6ldner f\u00fcrnis deckt/", "tokens": ["Ob", "a\u00b7ber", "wol", "mein", "ertzt", "kein", "g\u00f6ld\u00b7ner", "f\u00fcr\u00b7nis", "deckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "PPOSAT", "VVFIN", "PIAT", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "So pr\u00fcfe meine treu/ ich werde zwar nicht gleissen/", "tokens": ["So", "pr\u00fc\u00b7fe", "mei\u00b7ne", "treu", "/", "ich", "wer\u00b7de", "zwar", "nicht", "gleis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJD", "$(", "PPER", "VAFIN", "ADV", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Doch zeigen/ da\u00df auch gold in meinem kupfer steckt.", "tokens": ["Doch", "zei\u00b7gen", "/", "da\u00df", "auch", "gold", "in", "mei\u00b7nem", "kup\u00b7fer", "steckt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$(", "KOUS", "ADV", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Der gold- und silber-schein kan leicht den k\u00e4ufer tr\u00fcgen/", "tokens": ["Der", "gold", "und", "sil\u00b7ber\u00b7schein", "kan", "leicht", "den", "k\u00e4u\u00b7fer", "tr\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "NN", "VMFIN", "ADJD", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Wer tauscht vor kupfer gold/ das silber-ertzt vor bley?", "tokens": ["Wer", "tauscht", "vor", "kup\u00b7fer", "gold", "/", "das", "sil\u00b7ber\u00b7ertzt", "vor", "bley", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "ADJA", "NN", "$(", "PDS", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Ich geh\u2019 gerade zu/ ich will dir gar nicht l\u00fcgen/", "tokens": ["Ich", "geh'", "ge\u00b7ra\u00b7de", "zu", "/", "ich", "will", "dir", "gar", "nicht", "l\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKZU", "$(", "PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Besihe was mein schatz und was mein reichthum sey.", "tokens": ["Be\u00b7si\u00b7he", "was", "mein", "schatz", "und", "was", "mein", "reicht\u00b7hum", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPOSAT", "NN", "KON", "PWS", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Gesetzt/ ich habe nichts denn kupfer aufzusetzen/", "tokens": ["Ge\u00b7setzt", "/", "ich", "ha\u00b7be", "nichts", "denn", "kup\u00b7fer", "auf\u00b7zu\u00b7set\u00b7zen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$(", "PPER", "VAFIN", "PIS", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Genug/ da\u00df kupfer auch kan gelt und gangbar seyn;", "tokens": ["Ge\u00b7nug", "/", "da\u00df", "kup\u00b7fer", "auch", "kan", "gelt", "und", "gang\u00b7bar", "seyn", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "KOUS", "NN", "ADV", "VMFIN", "ADJD", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.55": {"text": "Die kupfer-groschen sind die schlimsten nicht zu sch\u00e4tzen/", "tokens": ["Die", "kup\u00b7fer\u00b7gro\u00b7schen", "sind", "die", "schlims\u00b7ten", "nicht", "zu", "sch\u00e4t\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ART", "ADJA", "PTKNEG", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Um solche wechselt man auch oft ducaten ein.", "tokens": ["Um", "sol\u00b7che", "wech\u00b7selt", "man", "auch", "oft", "du\u00b7ca\u00b7ten", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PIAT", "VVFIN", "PIS", "ADV", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Verachtest du bey mir die funckelnden rubinen/", "tokens": ["Ver\u00b7ach\u00b7test", "du", "bey", "mir", "die", "fun\u00b7ckeln\u00b7den", "ru\u00b7bi\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.58": {"text": "Und sprichst: ach w\u00e4r das geld an crystallin gewandt!", "tokens": ["Und", "sprichst", ":", "ach", "w\u00e4r", "das", "geld", "an", "crys\u00b7tal\u00b7lin", "ge\u00b7wandt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "XY", "VAFIN", "ART", "NN", "APPR", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "So lerne was dir dient/ mein schatz/ und la\u00df dir dienen/", "tokens": ["So", "ler\u00b7ne", "was", "dir", "dient", "/", "mein", "schatz", "/", "und", "la\u00df", "dir", "die\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PWS", "PPER", "VVFIN", "$(", "PPOSAT", "NN", "$(", "KON", "VVIMP", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Der k\u00f6stliche rubin folgt nechst dem diamant.", "tokens": ["Der", "k\u00f6st\u00b7li\u00b7che", "ru\u00b7bin", "folgt", "nechst", "dem", "di\u00b7a\u00b7mant", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "Seh ich dir feurig aus/ so k\u00fchle meine flammen/", "tokens": ["Seh", "ich", "dir", "feu\u00b7rig", "aus", "/", "so", "k\u00fch\u00b7le", "mei\u00b7ne", "flam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADJD", "PTKVZ", "$(", "ADV", "VVFIN", "PPOSAT", "VVINF", "$("], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.62": {"text": "Und \u00f6fne deinen bach/ daraus mir labsal rinnt;", "tokens": ["Und", "\u00f6f\u00b7ne", "dei\u00b7nen", "bach", "/", "da\u00b7raus", "mir", "lab\u00b7sal", "rinnt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$(", "PAV", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Sonst wird uns beyderseits der tadel-spruch verdammen/", "tokens": ["Sonst", "wird", "uns", "bey\u00b7der\u00b7seits", "der", "ta\u00b7del\u00b7spruch", "ver\u00b7dam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "Da\u00df ich zu hitzig bin/ und du zu kalt gesinnt.", "tokens": ["Da\u00df", "ich", "zu", "hit\u00b7zig", "bin", "/", "und", "du", "zu", "kalt", "ge\u00b7sinnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKA", "ADJD", "VAFIN", "$(", "KON", "PPER", "PTKA", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.65": {"text": "Ja du hast selbst in mir den Aetna angeleget/", "tokens": ["Ja", "du", "hast", "selbst", "in", "mir", "den", "A\u00b7et\u00b7na", "an\u00b7ge\u00b7le\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VAFIN", "ADV", "APPR", "PPER", "ART", "NE", "VVPP", "$("], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.66": {"text": "Der flammen \u00fcber sich durch nas\u2019 und augen streut:", "tokens": ["Der", "flam\u00b7men", "\u00fc\u00b7ber", "sich", "durch", "nas'", "und", "au\u00b7gen", "streut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PRF", "APPR", "NE", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.67": {"text": "Du bist Enceladus/ der meine brust beweget/", "tokens": ["Du", "bist", "En\u00b7ce\u00b7la\u00b7dus", "/", "der", "mei\u00b7ne", "brust", "be\u00b7we\u00b7get", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "$(", "ART", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.68": {"text": "Und die verborgne glut in mein gesichte speyt.", "tokens": ["Und", "die", "ver\u00b7borg\u00b7ne", "glut", "in", "mein", "ge\u00b7sich\u00b7te", "speyt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.69": {"text": "La\u00df deiner gegengunst ein zeichen mich erkennen/", "tokens": ["La\u00df", "dei\u00b7ner", "ge\u00b7gen\u00b7gunst", "ein", "zei\u00b7chen", "mich", "er\u00b7ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "ART", "NN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.70": {"text": "Und springe meiner noth mit k\u00fchlem labsal bey;", "tokens": ["Und", "sprin\u00b7ge", "mei\u00b7ner", "noth", "mit", "k\u00fch\u00b7lem", "lab\u00b7sal", "bey", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.71": {"text": "Wo nicht so la\u00df mich nur mit dir zugleich verbrennen/", "tokens": ["Wo", "nicht", "so", "la\u00df", "mich", "nur", "mit", "dir", "zu\u00b7gleich", "ver\u00b7bren\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "ADV", "VVIMP", "PPER", "ADV", "APPR", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.72": {"text": "So da\u00df dein schwanen-leib mein scheiter-hausen sey.", "tokens": ["So", "da\u00df", "dein", "schwa\u00b7nen\u00b7leib", "mein", "schei\u00b7ter\u00b7hau\u00b7sen", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPOSAT", "NN", "PPOSAT", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.73": {"text": "Du beist mich zwar gar oft ein kleiner gla\u00df erwehlen/", "tokens": ["Du", "beist", "mich", "zwar", "gar", "oft", "ein", "klei\u00b7ner", "gla\u00df", "er\u00b7weh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "ADV", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.74": {"text": "Und wilst/ ich soll nicht mehr so starck als vormals ziehn;", "tokens": ["Und", "wilst", "/", "ich", "soll", "nicht", "mehr", "so", "starck", "als", "vor\u00b7mals", "ziehn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "$(", "PPER", "VMFIN", "PTKNEG", "ADV", "ADV", "ADJD", "KOKOM", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.75": {"text": "Ja wol! allein du must der leber auch befehlen/", "tokens": ["Ja", "wol", "!", "al\u00b7lein", "du", "must", "der", "le\u00b7ber", "auch", "be\u00b7feh\u00b7len", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "$.", "ADV", "PPER", "VMFIN", "ART", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.76": {"text": "Da\u00df sie nicht weiter kocht/ und ich so durstig bin.", "tokens": ["Da\u00df", "sie", "nicht", "wei\u00b7ter", "kocht", "/", "und", "ich", "so", "durs\u00b7tig", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "VVFIN", "$(", "KON", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.77": {"text": "Es gl\u00fchet dieser stein und dampft von rauch und hitze/", "tokens": ["Es", "gl\u00fc\u00b7het", "die\u00b7ser", "stein", "und", "dampft", "von", "rauch", "und", "hit\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "KON", "VVFIN", "APPR", "ADJD", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.78": {"text": "Ich finde dieser qual kein ende/ keine ruh;", "tokens": ["Ich", "fin\u00b7de", "die\u00b7ser", "qual", "kein", "en\u00b7de", "/", "kei\u00b7ne", "ruh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "PIAT", "NN", "$(", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.79": {"text": "Ich d\u00fcrste tag und nacht/ ich \u00e4chze/ lechze/ schwitze/", "tokens": ["Ich", "d\u00fcrs\u00b7te", "tag", "und", "nacht", "/", "ich", "\u00e4ch\u00b7ze", "/", "lech\u00b7ze", "/", "schwit\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "KON", "NN", "$(", "PPER", "VVFIN", "$(", "VVFIN", "$(", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.80": {"text": "Ich gie\u00dfe stets/ und doch schier nie genung/ hinzu:", "tokens": ["Ich", "gie\u00b7\u00dfe", "stets", "/", "und", "doch", "schier", "nie", "ge\u00b7nung", "/", "hin\u00b7zu", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$(", "KON", "ADV", "ADJD", "ADV", "ADV", "$(", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.81": {"text": "So schilt nun wie du wilt/ ich will nicht wieder fluchen/", "tokens": ["So", "schilt", "nun", "wie", "du", "wilt", "/", "ich", "will", "nicht", "wie\u00b7der", "flu\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "KOKOM", "PPER", "VMFIN", "$(", "PPER", "VMFIN", "PTKNEG", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.82": {"text": "Ichtrage mit gedult/ was ich nicht \u00e4ndern kan.", "tokens": ["Ich\u00b7tra\u00b7ge", "mit", "ge\u00b7dult", "/", "was", "ich", "nicht", "\u00e4n\u00b7dern", "kan", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$(", "PWS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.83": {"text": "Doch eines f\u00e4llt mir bey/ es stehet zu versuchen/", "tokens": ["Doch", "ei\u00b7nes", "f\u00e4llt", "mir", "bey", "/", "es", "ste\u00b7het", "zu", "ver\u00b7su\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "APPR", "$(", "PPER", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.84": {"text": "Vielleichte geht zuletzt noch dieser anschlag an:", "tokens": ["Viel\u00b7leich\u00b7te", "geht", "zu\u00b7letzt", "noch", "die\u00b7ser", "an\u00b7schlag", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ADV", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.85": {"text": "Schleu\u00df deinen busen auf/ nicht \u00fcbel wird sichs schicken/", "tokens": ["Schleu\u00df", "dei\u00b7nen", "bu\u00b7sen", "auf", "/", "nicht", "\u00fc\u00b7bel", "wird", "sichs", "schi\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "$(", "PTKNEG", "ADJD", "VAFIN", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.86": {"text": "Wenn mein rubinen schmuck in deinen schnee versteckt:", "tokens": ["Wenn", "mein", "ru\u00b7bi\u00b7nen", "schmuck", "in", "dei\u00b7nen", "schnee", "ver\u00b7steckt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.87": {"text": "Ich will mein angesicht auf deine br\u00fcste dr\u00fccken/", "tokens": ["Ich", "will", "mein", "an\u00b7ge\u00b7sicht", "auf", "dei\u00b7ne", "br\u00fcs\u00b7te", "dr\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.88": {"text": "Zu sehn/ ob deine milch mein purpur an sich leckt.", "tokens": ["Zu", "sehn", "/", "ob", "dei\u00b7ne", "milch", "mein", "pur\u00b7pur", "an", "sich", "leckt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$(", "KOUS", "PPOSAT", "NN", "PPOSAT", "ADJD", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}