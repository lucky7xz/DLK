{"dta.poem.9830": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Grabschrifft einer h\u00fcndin Fidele genant/  \n als sie in den wochen starb.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die treue/ die ich sonst in meinem nahmen trug/", "tokens": ["Die", "treu\u00b7e", "/", "die", "ich", "sonst", "in", "mei\u00b7nem", "nah\u00b7men", "trug", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$(", "PRELS", "PPER", "ADV", "APPR", "PPOSAT", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "War zwar im leben gro\u00df/ doch ach! nicht starck genug;", "tokens": ["War", "zwar", "im", "le\u00b7ben", "gro\u00df", "/", "doch", "ach", "!", "nicht", "starck", "ge\u00b7nug", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPRART", "VVFIN", "ADJD", "$(", "ADV", "ADV", "$.", "PTKNEG", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn da der tod itzt kam die jungen mir zu w\u00fcrgen/", "tokens": ["Denn", "da", "der", "tod", "itzt", "kam", "die", "jun\u00b7gen", "mir", "zu", "w\u00fcr\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ADV", "VVFIN", "ART", "ADJA", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Trat ich den kampff mit ihm auff leib und leben an.", "tokens": ["Trat", "ich", "den", "kampff", "mit", "ihm", "auff", "leib", "und", "le\u00b7ben", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "PPER", "APPR", "NN", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich wagte mich f\u00fcr sie: wiewohl der todt gewann/", "tokens": ["Ich", "wag\u00b7te", "mich", "f\u00fcr", "sie", ":", "wie\u00b7wohl", "der", "todt", "ge\u00b7wann", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPER", "$.", "KOUS", "ART", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und fra\u00df die schuldner auff zusamt dem treuen b\u00fcrgen.", "tokens": ["Und", "fra\u00df", "die", "schuld\u00b7ner", "auff", "zu\u00b7samt", "dem", "treu\u00b7en", "b\u00fcr\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "APPR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Jtzt deckt den kleinen leib ein kleiner leichenstein.", "tokens": ["Jtzt", "deckt", "den", "klei\u00b7nen", "leib", "ein", "klei\u00b7ner", "lei\u00b7chen\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mein leser/ nim zuletzt noch hier die grabschrifft ein:", "tokens": ["Mein", "le\u00b7ser", "/", "nim", "zu\u00b7letzt", "noch", "hier", "die", "grab\u00b7schrifft", "ein", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "VVIMP", "ADV", "ADV", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Aus liebe lie\u00df ich mich in diese grufft versperren/", "tokens": ["Aus", "lie\u00b7be", "lie\u00df", "ich", "mich", "in", "die\u00b7se", "grufft", "ver\u00b7sper\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVFIN", "PPER", "PRF", "APPR", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Denn treue schlug allhier selbst ihren eignen herren.", "tokens": ["Denn", "treu\u00b7e", "schlug", "all\u00b7hier", "selbst", "ih\u00b7ren", "eig\u00b7nen", "her\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "VVFIN", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}