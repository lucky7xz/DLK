{"textgrid.poem.67907": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "2. Klage \u00fcber die Tyrannen der Leibeignen", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tochter, ich flieh nicht die Arbeit,", "tokens": ["Toch\u00b7ter", ",", "ich", "flieh", "nicht", "die", "Ar\u00b7beit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+++-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Fliehe nicht die Beerenstr\u00e4ucher,", "tokens": ["Flie\u00b7he", "nicht", "die", "Bee\u00b7ren\u00b7str\u00e4u\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Fliehe nicht von Jaans", "tokens": ["Flie\u00b7he", "nicht", "von", "Jaans"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "APPR", "NE"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Vor dem b\u00f6sen Deutschen flieh ich,", "tokens": ["Vor", "dem", "b\u00f6\u00b7sen", "Deut\u00b7schen", "flieh", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Vor dem schrecklich b\u00f6sen Herren.", "tokens": ["Vor", "dem", "schreck\u00b7lich", "b\u00f6\u00b7sen", "Her\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Arme Bauren an dem Pfosten", "tokens": ["Ar\u00b7me", "Bau\u00b7ren", "an", "dem", "Pfos\u0