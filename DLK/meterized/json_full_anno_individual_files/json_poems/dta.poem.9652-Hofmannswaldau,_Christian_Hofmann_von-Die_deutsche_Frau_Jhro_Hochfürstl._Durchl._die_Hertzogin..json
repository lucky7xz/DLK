{"dta.poem.9652": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die deutsche Frau/ Jhro Hochf\u00fcrstl.  \n Durchl. die Hertzogin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die deutsche frau voll ernst und g\u00fcte", "tokens": ["Die", "deut\u00b7sche", "frau", "voll", "ernst", "und", "g\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "ADJD", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zeigt uns der Deutschen majest\u00e4t;", "tokens": ["Zeigt", "uns", "der", "Deut\u00b7schen", "ma\u00b7jes\u00b7t\u00e4t", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch ist ihr k\u00f6niglich gebl\u00fcte", "tokens": ["Doch", "ist", "ihr", "k\u00f6\u00b7nig\u00b7lich", "ge\u00b7bl\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das minste st\u00fcck das sie erh\u00f6ht.", "tokens": ["Das", "mins\u00b7te", "st\u00fcck", "das", "sie", "er\u00b7h\u00f6ht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die sprache vieler nationen", "tokens": ["Die", "spra\u00b7che", "vie\u00b7ler", "na\u00b7ti\u00b7o\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hat sich in ihren mund gesetzt;", "tokens": ["Hat", "sich", "in", "ih\u00b7ren", "mund", "ge\u00b7setzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und tr\u00e4gt sie gleich nicht so viel kronen/", "tokens": ["Und", "tr\u00e4gt", "sie", "gleich", "nicht", "so", "viel", "kro\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "PIAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wird sie doch derer werth gesch\u00e4tzt.", "tokens": ["Wird", "sie", "doch", "de\u00b7rer", "werth", "ge\u00b7sch\u00e4tzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PDS", "ADJD", "VVPP", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}