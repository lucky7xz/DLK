{"textgrid.poem.41604": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zur Zeit, als die Natur, von wilder Kraft durchdrungen,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zur Zeit, als die Natur, von wilder Kraft durchdrungen,", "tokens": ["Zur", "Zeit", ",", "als", "die", "Na\u00b7tur", ",", "von", "wil\u00b7der", "Kraft", "durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "ART", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gewaltge Kinder trug, h\u00e4tt ich nach meinem Sinn", "tokens": ["Ge\u00b7walt\u00b7ge", "Kin\u00b7der", "trug", ",", "h\u00e4tt", "ich", "nach", "mei\u00b7nem", "Sinn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "$,", "VAFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bei einer Riesin gern gelebt, bei einer jungen,", "tokens": ["Bei", "ei\u00b7ner", "Rie\u00b7sin", "gern", "ge\u00b7lebt", ",", "bei", "ei\u00b7ner", "jun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVPP", "$,", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie eine Katze streicht um eine K\u00f6nigin.", "tokens": ["Wie", "ei\u00b7ne", "Kat\u00b7ze", "streicht", "um", "ei\u00b7ne", "K\u00f6\u00b7ni\u00b7gin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie Leib und Seele ihr bei grimmem Spiel erbl\u00fchten", "tokens": ["Wie", "Leib", "und", "See\u00b7le", "ihr", "bei", "grim\u00b7mem", "Spiel", "er\u00b7bl\u00fch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN", "PPOSAT", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wuchsen, h\u00e4tt ich gern erschaut von Anbeginn,", "tokens": ["Und", "wuch\u00b7sen", ",", "h\u00e4tt", "ich", "gern", "er\u00b7schaut", "von", "An\u00b7be\u00b7ginn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VAFIN", "PPER", "ADV", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ersp\u00e4ht, wie in der Brust ihr finstre Flammen gl\u00fchten", "tokens": ["Er\u00b7sp\u00e4ht", ",", "wie", "in", "der", "Brust", "ihr", "finst\u00b7re", "Flam\u00b7men", "gl\u00fch\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "PWAV", "APPR", "ART", "NN", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und Nebel traumhaft zog durch ihre Augen hin.", "tokens": ["Und", "Ne\u00b7bel", "traum\u00b7haft", "zog", "durch", "ih\u00b7re", "Au\u00b7gen", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Mit Mu\u00dfe h\u00e4tte ich erforscht die pr\u00e4chtgen Glieder,", "tokens": ["Mit", "Mu\u00b7\u00dfe", "h\u00e4t\u00b7te", "ich", 