{"textgrid.poem.36676": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Sp\u00e4te Einsicht", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Lieb' ist wie die Sonne,", "tokens": ["Die", "Lieb'", "ist", "wie", "die", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verwegner Uebermuth,", "tokens": ["Ver\u00b7weg\u00b7ner", "Ue\u00b7ber\u00b7muth", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der schaudernd in der Wonne", "tokens": ["Der", "schau\u00b7dernd", "in", "der", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der hei\u00dfen Lebensgluth,", "tokens": ["Der", "hei\u00b7\u00dfen", "Le\u00b7bens\u00b7gluth", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Den Lichtquell zu ergr\u00fcnden,", "tokens": ["Den", "Licht\u00b7quell", "zu", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In seine Tiefe blickt,", "tokens": ["In", "sei\u00b7ne", "Tie\u00b7fe", "blickt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mu\u00df da zuletzt erblinden", "tokens": ["Mu\u00df", "da", "zu\u00b7letzt", "er\u00b7blin\u00b7d