{"dta.poem.8038": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Ritter Paris .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Paris ist der sch\u00f6nste Ritter,", "tokens": ["Pa\u00b7ris", "ist", "der", "sch\u00f6ns\u00b7te", "Rit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Alle Herzen nimmt er hin.", "tokens": ["Al\u00b7le", "Her\u00b7zen", "nimmt", "er", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jede Dame kann\u2019s beschw\u00f6ren", "tokens": ["Je\u00b7de", "Da\u00b7me", "kann's", "be\u00b7schw\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An dem Hof der K\u00f6nigin.", "tokens": ["An", "dem", "Hof", "der", "K\u00f6\u00b7ni\u00b7gin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Was der sch\u00f6nen Siegeszeichen", "tokens": ["Was", "der", "sch\u00f6\u00b7nen", "Sie\u00b7ges\u00b7zei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Warf das Gl\u00fcck in seinen Schoo\u00df!", "tokens": ["Warf", "das", "Gl\u00fcck", "in", "sei\u00b7nen", "Schoo\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Briefe, die von K\u00fcssen rauschen,", "tokens": ["Brie\u00b7fe", ",", "die", "von", "K\u00fcs\u00b7sen", "rau\u00b7schen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Locken, Ringe, zahlenlos.", "tokens": ["Lo\u00b7cken", ",", "Rin\u00b7ge", ",", "zah\u00b7len\u00b7los", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Allzu leichter Siege Zeichen!", "tokens": ["All\u00b7zu", "leich\u00b7ter", "Sie\u00b7ge", "Zei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Ungebetnes Minnegl\u00fcck!", "tokens": ["Un\u00b7ge\u00b7bet\u00b7nes", "Min\u00b7ne\u00b7gl\u00fcck", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Bann und Fessel nennt euch Paris,", "tokens": ["Bann", "und", "Fes\u00b7sel", "nennt", "euch", "Pa\u00b7ris", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VVFIN", "PPER", "NE", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.12": {"text": "St\u00f6\u00dft sein s\u00fc\u00dfes Loos zur\u00fcck.", "tokens": ["St\u00f6\u00dft", "sein", "s\u00fc\u00b7\u00dfes", "Loos", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Schwingt zu Ro\u00df sich, schwerger\u00fcstet,", "tokens": ["Schwingt", "zu", "Ro\u00df", "sich", ",", "schwer\u00b7ge\u00b7r\u00fcs\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PRF", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Gl\u00fcht von edler Heldenlust,", "tokens": ["Gl\u00fcht", "von", "ed\u00b7ler", "Hel\u00b7den\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Beut den Frauen all den R\u00fccken,", "tokens": ["Beut", "den", "Frau\u00b7en", "all", "den", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PIAT", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Beut den M\u00e4nnern keck die Brust.", "tokens": ["Beut", "den", "M\u00e4n\u00b7nern", "keck", "die", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Doch es will kein Feind sich zeigen,", "tokens": ["Doch", "es", "will", "kein", "Feind", "sich", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PIAT", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Fr\u00fchling waltet im Gefild,", "tokens": ["Fr\u00fch\u00b7ling", "wal\u00b7tet", "im", "Ge\u00b7fild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Mit dem Helmbusch spielen L\u00fcftchen,", "tokens": ["Mit", "dem", "Helm\u00b7busch", "spie\u00b7len", "L\u00fcft\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Sonne spiegelt sich im Schild.", "tokens": ["Son\u00b7ne", "spie\u00b7gelt", "sich", "im", "Schild", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Weit schon ist er so geritten,", "tokens": ["Weit", "schon", "ist", "er", "so", "ge\u00b7rit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Siehe! da an Waldes Thor", "tokens": ["Sie\u00b7he", "!", "da", "an", "Wal\u00b7des", "Thor"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "$.", "ADV", "APPR", "NN", "NN"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.23": {"text": "H\u00e4lt ein Ritter, hoch zu Rosse,", "tokens": ["H\u00e4lt", "ein", "Rit\u00b7ter", ",", "hoch", "zu", "Ros\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Strecket ihm die Lanze vor.", "tokens": ["Stre\u00b7cket", "ihm", "die", "Lan\u00b7ze", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Ritter Paris fliegt zum Kampfe,", "tokens": ["Rit\u00b7ter", "Pa\u00b7ris", "fliegt", "zum", "Kamp\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Eilte nie zum Reihn so sehr;", "tokens": ["Eil\u00b7te", "nie", "zum", "Reihn", "so", "sehr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Wirft den Gegner stracks zur Erde,", "tokens": ["Wirft", "den", "Geg\u00b7ner", "stracks", "zur", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Blickt als Sieger stolz umher;", "tokens": ["Blickt", "als", "Sie\u00b7ger", "stolz", "um\u00b7her", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOUS", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Naht sich h\u00fclfreich dem Geworfnen,", "tokens": ["Naht", "sich", "h\u00fclf\u00b7reich", "dem", "Ge\u00b7worf\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Nimmt ihm ab des Helms Gewicht:", "tokens": ["Nimmt", "ihm", "ab", "des", "Helms", "Ge\u00b7wicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Sieh! da wallen reiche Locken", "tokens": ["Sieh", "!", "da", "wal\u00b7len", "rei\u00b7che", "Lo\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "ADV", "VVINF", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Um ein zartes Angesicht.", "tokens": ["Um", "ein", "zar\u00b7tes", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Wie er Schien\u2019 und Panzer l\u00f6set,", "tokens": ["Wie", "er", "Schien'", "und", "Pan\u00b7zer", "l\u00f6\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Welch ein Busen! welch ein Leib!", "tokens": ["Welch", "ein", "Bu\u00b7sen", "!", "welch", "ein", "Leib", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "$.", "PWAT", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "Hingegossen ohne Leben,", "tokens": ["Hin\u00b7ge\u00b7gos\u00b7sen", "oh\u00b7ne", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Liegt vor ihm das sch\u00f6nste Weib.", "tokens": ["Liegt", "vor", "ihm", "das", "sch\u00f6ns\u00b7te", "Weib", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.37": {"text": "W\u00fcrden erst die bleichen Wangen", "tokens": ["W\u00fcr\u00b7den", "erst", "die", "blei\u00b7chen", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "R\u00f6then sich von neuer Glut,", "tokens": ["R\u00f6\u00b7then", "sich", "von", "neu\u00b7er", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.39": {"text": "H\u00fcben erst sich diese Wimpern:", "tokens": ["H\u00fc\u00b7ben", "erst", "sich", "die\u00b7se", "Wim\u00b7pern", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PRF", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Wie dann, Paris, junges Blut?", "tokens": ["Wie", "dann", ",", "Pa\u00b7ris", ",", "jun\u00b7ges", "Blut", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "NE", "$,", "ADJA", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.41": {"text": "Ja! schon holt sie tiefen Athem,", "tokens": ["Ja", "!", "schon", "holt", "sie", "tie\u00b7fen", "A\u00b7them", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ADV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Schl\u00e4gt die Augen z\u00e4rtlich auf;", "tokens": ["Schl\u00e4gt", "die", "Au\u00b7gen", "z\u00e4rt\u00b7lich", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.43": {"text": "Die als wilder Feind gestorben,", "tokens": ["Die", "als", "wil\u00b7der", "Feind", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOUS", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Lebt als milde Freundin auf.", "tokens": ["Lebt", "als", "mil\u00b7de", "Freun\u00b7din", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.45": {"text": "Dort, in St\u00fccken, liegt die H\u00fclle,", "tokens": ["Dort", ",", "in", "St\u00fc\u00b7cken", ",", "liegt", "die", "H\u00fcl\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Die ein starrer Ritter war,", "tokens": ["Die", "ein", "star\u00b7rer", "Rit\u00b7ter", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.47": {"text": "Hier, in Paris Arm, die F\u00fclle,", "tokens": ["Hier", ",", "in", "Pa\u00b7ris", "Arm", ",", "die", "F\u00fcl\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "NE", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "S\u00fc\u00dfer Kern, der Schaale baar.", "tokens": ["S\u00fc\u00b7\u00dfer", "Kern", ",", "der", "Schaa\u00b7le", "baar", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.49": {"text": "Paris spricht, der sch\u00f6ne Ritter:", "tokens": ["Pa\u00b7ris", "spricht", ",", "der", "sch\u00f6\u00b7ne", "Rit\u00b7ter", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "\u201ewelcher Sieg nun? welcher Ruhm?", "tokens": ["\u201e", "wel\u00b7cher", "Sieg", "nun", "?", "wel\u00b7cher", "Ruhm", "?"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PWAT", "NN", "ADV", "$.", "PWAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.51": {"text": "Soll mir nie ein Strau\u00df gelingen", "tokens": ["Soll", "mir", "nie", "ein", "Strau\u00df", "ge\u00b7lin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NE", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.52": {"text": "In dem ernsten Ritterthum?", "tokens": ["In", "dem", "erns\u00b7ten", "Rit\u00b7ter\u00b7thum", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.53": {"text": "Wandelt stets, was ich ber\u00fchre,", "tokens": ["Wan\u00b7delt", "stets", ",", "was", "ich", "be\u00b7r\u00fch\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.54": {"text": "Sich in Scherz und Liebe mir?", "tokens": ["Sich", "in", "Scherz", "und", "Lie\u00b7be", "mir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "KON", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.55": {"text": "Minnegl\u00fcck, das mich verfolget,", "tokens": ["Min\u00b7ne\u00b7gl\u00fcck", ",", "das", "mich", "ver\u00b7fol\u00b7get", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.56": {"text": "Z\u00fcrn\u2019 ich oder dank\u2019 ich dir?\u201c", "tokens": ["Z\u00fcrn'", "ich", "o\u00b7der", "dank'", "ich", "dir", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "PPER", "KON", "VVFIN", "PPER", "PPER", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}