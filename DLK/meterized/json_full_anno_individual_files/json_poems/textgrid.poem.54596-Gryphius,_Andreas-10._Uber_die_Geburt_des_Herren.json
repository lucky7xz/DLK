{"textgrid.poem.54596": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "10. Uber die Geburt des Herren", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist mit Juda doch gantz auff die Neige kommen!", "tokens": ["Es", "ist", "mit", "Ju\u00b7da", "doch", "gantz", "auff", "die", "Nei\u00b7ge", "kom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NE", "ADV", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Regiment/ die Cron/ die Freyheit ist genommen.", "tokens": ["Das", "Re\u00b7gi\u00b7ment", "/", "die", "Cron", "/", "die", "Frey\u00b7heit", "ist", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "$(", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und zweifelt iemand das Messias Zeit nun dar!", "tokens": ["Und", "zwei\u00b7felt", "ie\u00b7mand", "das", "Mes\u00b7si\u00b7as", "Zeit", "nun", "dar", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ART", "NN", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Der seh Judeen an! Sie wird sein nicht gewahr/", "tokens": ["Der", "seh", "Ju\u00b7deen", "an", "!", "Sie", "wird", "sein", "nicht", "ge\u00b7wahr", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "PTKVZ", "$.", "PPER", "VAFIN", "PPOSAT", "PTKNEG", "ADJD", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "So hat Verstand und Sinn und Wei\u00dfheit abgenommen/", "tokens": ["So", "hat", "Ver\u00b7stand", "und", "Sinn", "und", "Wei\u00df\u00b7heit", "ab\u00b7ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "KON", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df Sion nichts versteht/ ob gleich Messias kommen.", "tokens": ["Da\u00df", "Si\u00b7on", "nichts", "ver\u00b7steht", "/", "ob", "gleich", "Mes\u00b7si\u00b7as", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PIS", "VVFIN", "$(", "KOUS", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Es ist mit Juda doch gantz auff die Neige kommen!", "tokens": ["Es", "ist", "mit", "Ju\u00b7da", "doch", "gantz", "auff", "die", "Nei\u00b7ge", "kom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NE", "ADV", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Regiment/ die Cron/ die Freyheit ist genommen.", "tokens": ["Das", "Re\u00b7gi\u00b7ment", "/", "die", "Cron", "/", "die", "Frey\u00b7heit", "ist", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "$(", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und zweifelt iemand das Messias Zeit nun dar!", "tokens": ["Und", "zwei\u00b7felt", "ie\u00b7mand", "das", "Mes\u00b7si\u00b7as", "Zeit", "nun