{"textgrid.poem.46278": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Alters eigenschaften", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer, wan er zweinzig jahr nun alt,", "tokens": ["Wer", ",", "wan", "er", "zwein\u00b7zig", "jahr", "nun", "alt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "PWAV", "PPER", "CARD", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "hat noch nicht sch\u00f6ne leibsgestalt,", "tokens": ["hat", "noch", "nicht", "sch\u00f6\u00b7ne", "leibs\u00b7ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und keine st\u00e4rke, wan er drei\u00dfig,", "tokens": ["und", "kei\u00b7ne", "st\u00e4r\u00b7ke", ",", "wan", "er", "drei\u00b7\u00dfig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "VVFIN", "$,", "PWAV", "PPER", "CARD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "und vierzig kein hirn und verstand,", "tokens": ["und", "vier\u00b7zig", "kein", "hirn", "und", "ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "PIAT", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "und f\u00fcnfzigj\u00e4hrig ist nicht flei\u00dfig", "tokens": ["und", "f\u00fcnf\u00b7zig\u00b7j\u00e4h\u00b7rig", "ist", "nicht", "flei\u00b7\u00dfig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "PTKNEG", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "