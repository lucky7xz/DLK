{"dta.poem.21306": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput III.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zu Aachen, auf dem Posthausschild,", "tokens": ["Zu", "Aa\u00b7chen", ",", "auf", "dem", "Post\u00b7haus\u00b7schild", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sah ich den Vogel wieder,", "tokens": ["Sah", "ich", "den", "Vo\u00b7gel", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NE", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der mir so tief verha\u00dft! Voll Gift", "tokens": ["Der", "mir", "so", "tief", "ver\u00b7ha\u00dft", "!", "Voll", "Gift"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVPP", "$.", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schaute er auf mich nieder.", "tokens": ["Schau\u00b7te", "er", "auf", "mich", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Du h\u00e4\u00dflicher Vogel, wirst du einst", "tokens": ["Du", "h\u00e4\u00df\u00b7li\u00b7cher", "Vo\u00b7gel", ",", "wirst", "du", "einst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "VAFIN", "PPER", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mir in die H\u00e4nde fallen,", "tokens": ["Mir", "in", "die", "H\u00e4n\u00b7de", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So rupfe ich dir die Federn aus", "tokens": ["So", "rup\u00b7fe", "ich", "dir", "die", "Fe\u00b7dern", "aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "APPR"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und hacke dir ab die Krallen.", "tokens": ["Und", "ha\u00b7cke", "dir", "ab", "die", "Kral\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Du sollst mir dann, in luft\u2019ger H\u00f6h,", "tokens": ["Du", "sollst", "mir", "dann", ",", "in", "luft'\u00b7ger", "H\u00f6h", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf einer Stange sitzen,", "tokens": ["Auf", "ei\u00b7ner", "Stan\u00b7ge", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und ich rufe zum lustigen Schie\u00dfen herbei", "tokens": ["Und", "ich", "ru\u00b7fe", "zum", "lus\u00b7ti\u00b7gen", "Schie\u00b7\u00dfen", "her\u00b7bei"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "PTKVZ"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Die Rheinischen Vogelsch\u00fctzen.", "tokens": ["Die", "Rhei\u00b7ni\u00b7schen", "Vo\u00b7gel\u00b7sch\u00fct\u00b7zen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Wer mir den Vogel herunterschie\u00dft,", "tokens": ["Wer", "mir", "den", "Vo\u00b7gel", "her\u00b7un\u00b7ter\u00b7schie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit Zepter und Krone belehn\u2019 ich", "tokens": ["Mit", "Zep\u00b7ter", "und", "Kro\u00b7ne", "be\u00b7lehn'", "ich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PPER"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Den wackern Mann! Wir blasen Tusch", "tokens": ["Den", "wa\u00b7ckern", "Mann", "!", "Wir", "bla\u00b7sen", "Tusch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und rufen: es lebe der K\u00f6nig!", "tokens": ["Und", "ru\u00b7fen", ":", "es", "le\u00b7be", "der", "K\u00f6\u00b7nig", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}