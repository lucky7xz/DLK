{"dta.poem.7945": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "An Sie .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Deine Augen sind nicht himmelblau,", "tokens": ["Dei\u00b7ne", "Au\u00b7gen", "sind", "nicht", "him\u00b7mel\u00b7blau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Dein Mund, er ist kein Rosenmund,", "tokens": ["Dein", "Mund", ",", "er", "ist", "kein", "Ro\u00b7sen\u00b7mund", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht Brust und Arme Lilien.", "tokens": ["Nicht", "Brust", "und", "Ar\u00b7me", "Li\u00b7li\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "KON", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ach! welch ein Fr\u00fchling w\u00e4re das,", "tokens": ["Ach", "!", "welch", "ein", "Fr\u00fch\u00b7ling", "w\u00e4\u00b7re", "das", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAT", "ART", "NN", "VAFIN", "PDS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wo solche Lilien, solche Rosen", "tokens": ["Wo", "sol\u00b7che", "Li\u00b7li\u00b7en", ",", "sol\u00b7che", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "$,", "PIAT", "NN"], "meter": "-+-+----+-", "measure": "unknown.measure.tri"}, "line.6": {"text": "Im Thal und auf den H\u00f6hen bl\u00fchten,", "tokens": ["Im", "Thal", "und", "auf", "den", "H\u00f6\u00b7hen", "bl\u00fch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und alles das ein klarer Himmel", "tokens": ["Und", "al\u00b7les", "das", "ein", "kla\u00b7rer", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "ART", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Umfienge, wie dein blaues Aug\u2019!", "tokens": ["Um\u00b7fi\u00b7en\u00b7ge", ",", "wie", "dein", "blau\u00b7es", "Aug'", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPOSAT", "ADJA", "NN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}}}}}