{"textgrid.poem.33875": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Korn auf dem Dache", "genre": "verse", "period": "N.A.", "pub_year": 1853, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Fr\u00fchling ist gekommen,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "ist", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Doch war der Winter scharf", "tokens": ["Doch", "war", "der", "Win\u00b7ter", "scharf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und hat mit weggenommen", "tokens": ["Und", "hat", "mit", "weg\u00b7ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den n\u00f6thigsten Bedarf;", "tokens": ["Den", "n\u00f6\u00b7thigs\u00b7ten", "Be\u00b7darf", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Die Pfl\u00fcge bleiben stehen,", "tokens": ["Die", "Pfl\u00fc\u00b7ge", "blei\u00b7ben", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Es fehlt ja an der Saat,", "tokens": ["Es", "fehlt", "ja", "an", "der", "Saat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und mu\u00df auch was geschehen,", "tokens": ["Und", "mu\u00df", "auch", "was", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "PIS", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So wei\u00df doch Keiner Rath.", "tokens": ["So", "wei\u00df", "doch", "Kei\u00b7ner", "Rath", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PIS", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da hinkt ein alter Jude", "tokens": ["Da", "hinkt", "ein", "al\u00b7ter", "Ju\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In wei\u00dfem Bart durch's Dorf,", "tokens": ["In", "wei\u00b7\u00dfem", "Bart", "durch's", "Dorf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der kroch aus seiner Bude", "tokens": ["Der", "kroch", "aus", "sei\u00b7ner", "Bu\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Um etwas Sprock und Torf.", "tokens": ["Um", "et\u00b7was", "Sprock", "und", "Torf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PIAT", "NN", "KON", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er weilt bei jedem Schober", "tokens": ["Er", "weilt", "bei", "je\u00b7dem", "Scho\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und sp\u00e4ht und b\u00fcckt sich oft,", "tokens": ["Und", "sp\u00e4ht", "und", "b\u00fcckt", "sich", "oft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PRF", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und voll ist ihm der Kober,", "tokens": ["Und", "voll", "ist", "ihm", "der", "Ko\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Bevor er's noch gehofft.", "tokens": ["Be\u00b7vor", "er's", "noch", "ge\u00b7hofft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Arbeit ward ihm sauer,", "tokens": ["Die", "Ar\u00b7beit", "ward", "ihm", "sau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nun will er denn nach Haus,", "tokens": ["Nun", "will", "er", "denn", "nach", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da tritt ein m\u00fc\u00df'ger Bauer", "tokens": ["Da", "tritt", "ein", "m\u00fc\u00df'\u00b7ger", "Bau\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aus seiner Th\u00fcr heraus.", "tokens": ["Aus", "sei\u00b7ner", "Th\u00fcr", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der ruft: Du hast dir Feu'rung", "tokens": ["Der", "ruft", ":", "Du", "hast", "dir", "Feu'\u00b7rung"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$.", "PPER", "VAFIN", "PPER", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gesammelt aus dem Mist,", "tokens": ["Ge\u00b7sam\u00b7melt", "aus", "dem", "Mist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "So sag' auch, ob der Theurung", "tokens": ["So", "sag'", "auch", ",", "ob", "der", "Theu\u00b7rung"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nicht noch zu wehren ist.", "tokens": ["Nicht", "noch", "zu", "weh\u00b7ren", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Alte hebt die Blicke,", "tokens": ["Der", "Al\u00b7te", "hebt", "die", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Doch bis zum Himmel nicht,", "tokens": ["Doch", "bis", "zum", "Him\u00b7mel", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann tickt er mit der Kr\u00fccke", "tokens": ["Dann", "tickt", "er", "mit", "der", "Kr\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf's H\u00fcttendach, und spricht:", "tokens": ["Auf's", "H\u00fct\u00b7ten\u00b7dach", ",", "und", "spricht", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbwar das nicht eine Aehre,", "tokens": ["\u00bb", "war", "das", "nicht", "ei\u00b7ne", "A\u00b7eh\u00b7re", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PDS", "PTKNEG", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Was ich im Stroh dort sah?", "tokens": ["Was", "ich", "im", "Stroh", "dort", "sah", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wenn's nicht die einz'ge w\u00e4re,", "tokens": ["Wenn's", "nicht", "die", "einz'\u00b7ge", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "ADJA", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So ist die H\u00fclfe nah'!\u00ab", "tokens": ["So", "ist", "die", "H\u00fcl\u00b7fe", "nah'", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der Bauer geht zur Leiter", "tokens": ["Der", "Bau\u00b7er", "geht", "zur", "Lei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und deckt die H\u00fctte ab,", "tokens": ["Und", "deckt", "die", "H\u00fct\u00b7te", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er drischt sein Stroh noch weiter,", "tokens": ["Er", "drischt", "sein", "Stroh", "noch", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im lust'gen Klipp und Klapp,", "tokens": ["Im", "lust'\u00b7gen", "Klipp", "und", "Klapp", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und als die K\u00f6rner springen,", "tokens": ["Und", "als", "die", "K\u00f6r\u00b7ner", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da folgt ihm Mann f\u00fcr Mann,", "tokens": ["Da", "folgt", "ihm", "Mann", "f\u00fcr", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und das wird so viel bringen,", "tokens": ["Und", "das", "wird", "so", "viel", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Da\u00df Jeder s\u00e4en kann.", "tokens": ["Da\u00df", "Je\u00b7der", "s\u00e4\u00b7en", "kann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der Fr\u00fchling ist gekommen,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "ist", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"},