, "measure": "iambic.penta"}, "line.8": {"text": "Mir flimmert's mystisch her vom Sternenschilde.", "tokens": ["Mir", "flim\u00b7mert's", "mys\u00b7tisch", "her", "vom", "Ster\u00b7nen\u00b7schil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Denn manches Lasters k\u00f6nnt' ich mich versehen,", "tokens": ["Denn", "man\u00b7ches", "Las\u00b7ters", "k\u00f6nnt'", "ich", "mich", "ver\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Fehler z\u00e4hl' ich wie der Sand am Meer,", "tokens": ["Und", "Feh\u00b7ler", "z\u00e4hl'", "ich", "wie", "der", "Sand", "am", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "KOKOM", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch bei der Sonne Licht darf ich gestehen:", "tokens": ["Doch", "bei", "der", "Son\u00b7ne", "Licht", "darf", "ich", "ge\u00b7ste\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VMFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nie bohrte mir durchs Herz des Neides Speer.", "tokens": ["Nie", "bohr\u00b7te", "mir", "durchs", "Herz", "des", "Nei\u00b7des", "Speer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich m\u00fc\u00