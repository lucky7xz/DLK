{"textgrid.poem.66688": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "29.", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Denn manches Lasters k\u00f6nnt' ich mich versehen,", "tokens": ["Denn", "man\u00b7ches", "Las\u00b7ters", "k\u00f6nnt'", "ich", "mich", "ver\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Fehler z\u00e4hl' ich wie der Sand am Meer,", "tokens": ["Und", "Feh\u00b7ler", "z\u00e4hl'", "ich", "wie", "der", "Sand", "am", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "KOKOM", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch bei der Sonne Licht darf ich gestehen:", "tokens": ["Doch", "bei", "der", "Son\u00b7ne", "Licht", "darf", "ich", "ge\u00b7ste\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VMFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nie bohrte mir durchs Herz des Neides Speer.", "tokens": ["Nie", "bohr\u00b7te", "mir", "durchs", "Herz", "des", "Nei\u00b7des", "Speer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich m\u00fc\u00dfte v\u00f6llig aus der Haut mir gehen,", "tokens": ["Ich", "m\u00fc\u00df\u00b7te", "v\u00f6l\u00b7lig", "aus", "der", "Haut", "mir", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "APPR", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wollt' ich vergr\u00f6\u00dfern noch der Neider Heer:", "tokens": ["Wollt'", "ich", "ver\u00b7gr\u00f6\u00b7\u00dfern", "noch", "der", "Nei\u00b7der", "Heer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dem W\u00fcrdigsten m\u00f6cht' ich zu beiden Seiten", "tokens": ["Dem", "W\u00fcr\u00b7digs\u00b7ten", "m\u00f6cht'", "ich", "zu", "bei\u00b7den", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "APPR", "PIAT", "NN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Den Hermelin selbst um die Schulter breiten.", "tokens": ["Den", "Her\u00b7me\u00b7lin", "selbst", "um", "die", "Schul\u00b7ter", "brei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Zwar l\u00e4chelnd m\u00fc\u00dfte, der mir ganz gefiele,", "tokens": ["Zwar", "l\u00e4\u00b7chelnd", "m\u00fc\u00df\u00b7te", ",", "der", "mir", "ganz", "ge\u00b7fie\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VMFIN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mundwinkelzuckend steigen auf den Thron,", "tokens": ["Mund\u00b7win\u00b7kel\u00b7zu\u00b7ckend", "stei\u00b7gen", "auf", "den", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das goldne Zepter nehmen wie zum Spiele", "tokens": ["Das", "gold\u00b7ne", "Zep\u00b7ter", "neh\u00b7men", "wie", "zum", "Spie\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF", "KOKOM", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und sich geb\u00e4rden als Olympiersohn,", "tokens": ["Und", "sich", "ge\u00b7b\u00e4r\u00b7den", "als", "O\u00b7lym\u00b7pier\u00b7sohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "VVPP", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der flott agiert im Byzantiner Stile", "tokens": ["Der", "flott", "a\u00b7giert", "im", "By\u00b7zan\u00b7ti\u00b7ner", "Sti\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und selber komisch sich als Kaiser schon,", "tokens": ["Und", "sel\u00b7ber", "ko\u00b7misch", "sich", "als", "Kai\u00b7ser", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "PRF", "KOUS", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der bald nach abgestreiftem Hermeline", "tokens": ["Der", "bald", "nach", "ab\u00b7ge\u00b7streif\u00b7tem", "Her\u00b7me\u00b7li\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Als schlichter Meister seines Volks erschiene.", "tokens": ["Als", "schlich\u00b7ter", "Meis\u00b7ter", "sei\u00b7nes", "Volks", "er\u00b7schie\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich lache schmerzlich. Fern mir, da\u00df ich ha\u00dfte!", "tokens": ["Ich", "la\u00b7che", "schmerz\u00b7lich", ".", "Fern", "mir", ",", "da\u00df", "ich", "ha\u00df\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$.", "NN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich m\u00f6chte lieben, ging' es irgend an.", "tokens": ["Ich", "m\u00f6ch\u00b7te", "lie\u00b7ben", ",", "ging'", "es", "ir\u00b7gend", "an", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mir weht die Flagge silberwei\u00df vom Maste,", "tokens": ["Mir", "weht", "die", "Flag\u00b7ge", "sil\u00b7ber\u00b7wei\u00df", "vom", "Mas\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich traure nur, wo ich nicht lieben kann.", "tokens": ["Ich", "trau\u00b7re", "nur", ",", "wo", "ich", "nicht", "lie\u00b7ben", "kann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWAV", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Sinn der allerh\u00f6chsten Kriegerkaste", "tokens": ["Der", "Sinn", "der", "al\u00b7ler\u00b7h\u00f6chs\u00b7ten", "Krie\u00b7ger\u00b7kas\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ist Wahnsinn f\u00fcr den echten Edelmann.", "tokens": ["Ist", "Wahn\u00b7sinn", "f\u00fcr", "den", "ech\u00b7ten", "E\u00b7del\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Das fordert Blut und ruft dem Geist der Rache", "tokens": ["Das", "for\u00b7dert", "Blut", "und", "ruft", "dem", "Geist", "der", "Ra\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "NN", "KON", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "In Jesu Namen! \u2013 Schmerz, sei stark und lache!", "tokens": ["In", "Je\u00b7su", "Na\u00b7men", "!", "\u2013", "Schmerz", ",", "sei", "stark", "und", "la\u00b7che", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$.", "$(", "NN", "$,", "VAFIN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Mein Herz ist leidenschaftlich, voller Schw\u00e4chen,", "tokens": ["Mein", "Herz", "ist", "lei\u00b7den\u00b7schaft\u00b7lich", ",", "vol\u00b7ler", "Schw\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schon oft hat hei\u00dfer Zorn mich \u00fcbermannt,", "tokens": ["Schon", "oft", "hat", "hei\u00b7\u00dfer", "Zorn", "mich", "\u00fc\u00b7berm\u00b7annt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df mir Besinnung schwand in Wirbelb\u00e4chen", "tokens": ["Da\u00df", "mir", "Be\u00b7sin\u00b7nung", "schwand", "in", "Wir\u00b7bel\u00b7b\u00e4\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und mich das liebste Wesen kaum erkannt.", "tokens": ["Und", "mich", "das", "liebs\u00b7te", "We\u00b7sen", "kaum", "er\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nur eines wu\u00dft' ich nie: Was ist: sich r\u00e4chen?", "tokens": ["Nur", "ei\u00b7nes", "wu\u00dft'", "ich", "nie", ":", "Was", "ist", ":", "sich", "r\u00e4\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PPER", "ADV", "$.", "PWS", "VAFIN", "$.", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wer mich beleidigt ha\u00df- und wutentbrannt,", "tokens": ["Wer", "mich", "be\u00b7lei\u00b7digt", "ha\u00df", "und", "wu\u00b7tent\u00b7brannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "TRUNC", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Soll ich dem armen Schlucker zu Gefallen", "tokens": ["Soll", "ich", "dem", "ar\u00b7men", "Schlu\u00b7cker", "zu", "Ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Gleich zeigen: \u00bb", "tokens": ["Gleich", "zei\u00b7gen", ":", "\u00bb"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ADV", "VVINF", "$.", "$("], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.5": {"line.1": {"text": "\u00bbich bin wie du. Brauchst dich nicht zu genieren.", "tokens": ["\u00bb", "ich", "bin", "wie", "du", ".", "Brauchst", "dich", "nicht", "zu", "ge\u00b7nie\u00b7ren", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "KOKOM", "PPER", "$.", "VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Man stillt sich zur Revanche seinen Durst.", "tokens": ["Man", "stillt", "sich", "zur", "Re\u00b7van\u00b7che", "sei\u00b7nen", "Durst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "S\u00fc\u00df ist die Rache. La\u00df doch mal probieren!", "tokens": ["S\u00fc\u00df", "ist", "die", "Ra\u00b7che", ".", "La\u00df", "doch", "mal", "pro\u00b7bie\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$.", "VVIMP", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Famoses W\u00f6rtchen das: Wurst wider Wurst!", "tokens": ["Fa\u00b7mo\u00b7ses", "W\u00f6rt\u00b7chen", "das", ":", "Wurst", "wi\u00b7der", "Wurst", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PDS", "$.", "VAFIN", "APPR", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Wie feige, Kraftinstinkte zu kastrieren!", "tokens": ["Wie", "fei\u00b7ge", ",", "Kraf\u00b7tins\u00b7tink\u00b7te", "zu", "kast\u00b7rie\u00b7ren", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Fest um sich haun! \u2013 Arme Vernunft, du murrst?", "tokens": ["Fest", "um", "sich", "haun", "!", "\u2013", "Ar\u00b7me", "Ver\u00b7nunft", ",", "du", "murrst", "?"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "VVINF", "$.", "$(", "NN", "NN", "$,", "PPER", "VVFIN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Soll ich des Bruders Lehrer sein im Lieben?", "tokens": ["Soll", "ich", "des", "Bru\u00b7ders", "Leh\u00b7rer", "sein", "im", "Lie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "NN", "PPOSAT", "APPRART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Moral hei\u00dft: \u00bbZahn um Zahn und Heil den Hieben!\u00ab", "tokens": ["Mo\u00b7ral", "hei\u00dft", ":", "\u00bb", "Zahn", "um", "Zahn", "und", "Heil", "den", "Hie\u00b7ben", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "$.", "$(", "NN", "APPR", "NN", "KON", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Nun gut! ", "tokens": ["Nun", "gut", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$."], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Mit einer Geste, die mir ganz entspricht.", "tokens": ["Mit", "ei\u00b7ner", "Ges\u00b7te", ",", "die", "mir", "ganz", "ent\u00b7spricht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich will mich selbst nicht einen Narren schelten,", "tokens": ["Ich", "will", "mich", "selbst", "nicht", "ei\u00b7nen", "Nar\u00b7ren", "schel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der S\u00fchne nimmt, wenn er gestochen sticht.", "tokens": ["Der", "S\u00fch\u00b7ne", "nimmt", ",", "wenn", "er", "ge\u00b7sto\u00b7chen", "sticht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ob noch so sehr die Zornesadern schwellten,", "tokens": ["Ob", "noch", "so", "sehr", "die", "Zor\u00b7ne\u00b7sa\u00b7dern", "schwell\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein h\u00f6herer Trieb sucht h\u00f6heres Gericht,", "tokens": ["Ein", "h\u00f6\u00b7he\u00b7rer", "Trieb", "sucht", "h\u00f6\u00b7he\u00b7res", "Ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Und Kr\u00e4nkungen mit Gro\u00dfmut zu ertragen", "tokens": ["Und", "Kr\u00e4n\u00b7kun\u00b7gen", "mit", "Gro\u00df\u00b7mut", "zu", "er\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "