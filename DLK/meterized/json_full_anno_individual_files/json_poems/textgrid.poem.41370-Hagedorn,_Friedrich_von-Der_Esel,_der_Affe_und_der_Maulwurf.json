{"textgrid.poem.41370": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der Esel, der Affe und der Maulwurf", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein betr\u00fcbter Esel heulte,", "tokens": ["Ein", "be\u00b7tr\u00fcb\u00b7ter", "E\u00b7sel", "heul\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil des Schicksals karge Hand", "tokens": ["Weil", "des", "Schick\u00b7sals", "kar\u00b7ge", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihm nicht H\u00f6rner zugewandt,", "tokens": ["Ihm", "nicht", "H\u00f6r\u00b7ner", "zu\u00b7ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die sie doch dem Stier ertheilte;", "tokens": ["Die", "sie", "doch", "dem", "Stier", "er\u00b7theil\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und der Affe fiel ihm bei,", "tokens": ["Und", "der", "Af\u00b7fe", "fiel", "ihm", "bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Da\u00df der Himmel grausam sei,", "tokens": ["Da\u00df", "der", "Him\u00b7mel", "grau\u00b7sam", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Weil er ihm den Schwanz versagte.", "tokens": ["Weil", "er", "ihm", "den", "Schwanz", "ver\u00b7sag\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Als nun jeder m\u00fcrrisch klagte,", "tokens": ["Als", "nun", "je\u00b7der", "m\u00fcr\u00b7risch", "klag\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIS", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Sprach der Maulwurf: Ich bin blind;", "tokens": ["Sprach", "der", "Maul\u00b7wurf", ":", "Ich", "bin", "blind", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "PPER", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Da\u00df man sich mit mir vergleiche,", "tokens": ["Da\u00df", "man", "sich", "mit", "mir", "ver\u00b7glei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "APPR", "PPER", "VVFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.11": {"text": "Wenn des Schicksals Zorn und Streiche", "tokens": ["Wenn", "des", "Schick\u00b7sals", "Zorn", "und", "Strei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Andern unertr\u00e4glich sind!", "tokens": ["An\u00b7dern", "un\u00b7er\u00b7tr\u00e4g\u00b7lich", "sind", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ei