{"dta.poem.10699": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Eines J\u00e4gers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "In dieser holen Klufft gesuchet hat sein l\u00e4ger/", "tokens": ["In", "die\u00b7ser", "ho\u00b7len", "Klufft", "ge\u00b7su\u00b7chet", "hat", "sein", "l\u00e4\u00b7ger", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "VVPP", "VAFIN", "PPOSAT", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein grausamer Tyrann vnd Feind der Wilden thier/", "tokens": ["Ein", "grau\u00b7sa\u00b7mer", "Ty\u00b7rann", "vnd", "Feind", "der", "Wil\u00b7den", "thier", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jetzt hat er wiederumb auch seinen Lohn darf\u00fcr/", "tokens": ["Jetzt", "hat", "er", "wie\u00b7de\u00b7rumb", "auch", "sei\u00b7nen", "Lohn", "dar\u00b7f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "PPOSAT", "NN", "PAV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Todt der war sein Hundt/ die Kranckheit war sein J\u00e4ger/", "tokens": ["Der", "Todt", "der", "war", "sein", "Hundt", "/", "die", "Kran\u00b7ck\u00b7heit", "war", "sein", "J\u00e4\u00b7ger", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VAFIN", "PPOSAT", "NN", "$(", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "So ist der J\u00e4ger nun/ wie k\u00fchn er sey vnd starck/", "tokens": ["So", "ist", "der", "J\u00e4\u00b7ger", "nun", "/", "wie", "k\u00fchn", "er", "sey", "vnd", "starck", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "$(", "PWAV", "ADJD", "PPER", "VAFIN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Gejaget durch den Todt hier vnder disen Sarck.", "tokens": ["Ge\u00b7ja\u00b7get", "durch", "den", "Todt", "hier", "vn\u00b7der", "di\u00b7sen", "Sarck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}