{"textgrid.poem.53331": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Trost-Liedchen", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am allerbesten ist es zwar", "tokens": ["Am", "al\u00b7ler\u00b7bes\u00b7ten", "ist", "es", "zwar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Herren seyn verschieden,", "tokens": ["Im", "Her\u00b7ren", "seyn", "ver\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vnd leben bey der Frommen Schar", "tokens": ["Vnd", "le\u00b7ben", "bey", "der", "From\u00b7men", "Schar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vergn\u00fcget vnd in Frieden,", "tokens": ["Ver\u00b7gn\u00fc\u00b7get", "vnd", "in", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Seyn ewig ausser Trug vnd List", "tokens": ["Seyn", "e\u00b7wig", "aus\u00b7ser", "Trug", "vnd", "List"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mang Abrahams Geschlechten,", "tokens": ["Mang", "Ab\u00b7ra\u00b7hams", "Ge\u00b7schlech\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NE", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da Frewd' vnd lieblichs Wesen ist,", "tokens": ["Da", "Fre\u00b7wd'", "vnd", "lieb\u00b7lichs", "We\u00b7sen", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "O Gott, zu deiner Rechten.", "tokens": ["O", "Gott", ",", "zu", "dei\u00b7ner", "Rech\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Vnd hett' ich aller Lust Genie\u00df", "tokens": ["Vnd", "hett'", "ich", "al\u00b7ler", "Lust", "Ge\u00b7nie\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PIAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So hier erdacht mag werden,", "tokens": ["So", "hier", "er\u00b7dacht", "mag", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ja s\u00e4h' ein rechtes Paradie\u00df", "tokens": ["Ja", "s\u00e4h'", "ein", "rech\u00b7tes", "Pa\u00b7ra\u00b7die\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKANT", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr mich gebawt auff Erden,", "tokens": ["F\u00fcr", "mich", "ge\u00b7bawt", "auff", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Herscht' herrlich \u00fcber Leut und Landt,", "tokens": ["Herscht'", "herr\u00b7lich", "\u00fc\u00b7ber", "Leut", "und", "Landt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gro\u00df, m\u00e4chtig und erhaben,", "tokens": ["Gro\u00df", ",", "m\u00e4ch\u00b7tig", "und", "er\u00b7ha\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Vnd w\u00e4r' in aller Welt bekandt", "tokens": ["Vnd", "w\u00e4r'", "in", "al\u00b7ler", "Welt", "be\u00b7kandt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "PIAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Durch Kunst Verdienst und Gaben:", "tokens": ["Durch", "Kunst", "Ver\u00b7dienst", "und", "Ga\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Was w\u00e4r' es denn nun endlich mehr?", "tokens": ["Was", "w\u00e4r'", "es", "denn", "nun", "end\u00b7lich", "mehr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Zeit-Flucht heisst mich altten,", "tokens": ["Die", "Zeit\u00b7Flucht", "heisst", "mich", "alt\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verg\u00e4nglich ist Welt, Lust und Ehr',", "tokens": ["Ver\u00b7g\u00e4ng\u00b7lich", "ist", "Welt", ",", "Lust", "und", "Ehr'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd dan mus ich erkaltten,", "tokens": ["Vnd", "dan", "mus", "ich", "er\u00b7kalt\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Bin aus, verrotte ja sogleich", "tokens": ["Bin", "aus", ",", "ver\u00b7rot\u00b7te", "ja", "sog\u00b7leich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PTKVZ", "$,", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als h\u00e4tt' ich unter dessen", "tokens": ["Als", "h\u00e4tt'", "ich", "un\u00b7ter", "des\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PPER", "APPR", "PRELAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Nichts, oder aller Erden Reich", "tokens": ["Nichts", ",", "o\u00b7der", "al\u00b7ler", "Er\u00b7den", "Reich"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "$,", "KON", "PIAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In dieser Welt besessen.", "tokens": ["In", "die\u00b7ser", "Welt", "be\u00b7ses\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wer aber lebt so wol alhie", "tokens": ["Wer", "a\u00b7ber", "lebt", "so", "wol", "al\u00b7hie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "VVFIN", "ADV", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vnd nur in gutten Tagen?", "tokens": ["Vnd", "nur", "in", "gut\u00b7ten", "Ta\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein ander wei\u00df von seiner M\u00fch", "tokens": ["Ein", "an\u00b7der", "wei\u00df", "von", "sei\u00b7ner", "M\u00fch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von meiner ich zu sagen,", "tokens": ["Von", "mei\u00b7ner", "ich", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Viel ist der Stern' am Himmels-Sal", "tokens": ["Viel", "ist", "der", "Stern'", "am", "Him\u00b7mels\u00b7Sal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vnd viel der Meeres-Wellen,", "tokens": ["Vnd", "viel", "der", "Mee\u00b7res\u00b7Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mehr aber ist der Menschen Qual", "tokens": ["Mehr", "a\u00b7ber", "ist", "der", "Men\u00b7schen", "Qual"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In mehr als tausent F\u00e4llen.", "tokens": ["In", "mehr", "als", "tau\u00b7sent", "F\u00e4l\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "KOKOM", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nein, unser bestes bleibet wol", "tokens": ["Nein", ",", "un\u00b7ser", "bes\u00b7tes", "blei\u00b7bet", "wol"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPOSAT", "ADJA", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von hinnen selig scheiden,", "tokens": ["Von", "hin\u00b7nen", "se\u00b7lig", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vnd aller Rhue und Anmuth voll", "tokens": ["Vnd", "al\u00b7ler", "Rhue", "und", "An\u00b7muth", "voll"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "KON", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bey Christo seyn in Frewden,", "tokens": ["Bey", "Chris\u00b7to", "seyn", "in", "Frew\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PPOSAT", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Vnd jung zwar, denn aus diesem Licht", "tokens": ["Vnd", "jung", "zwar", ",", "denn", "aus", "die\u00b7sem", "Licht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "$,", "KON", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kaum alt erst wollen scheiden,", "tokens": ["Kaum", "alt", "erst", "wol\u00b7len", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ist Lust sich gern, ohn Thorheit nicht,", "tokens": ["Ist", "Lust", "sich", "gern", ",", "ohn", "Thor\u00b7heit", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PRF", "ADV", "$,", "KOUI", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Im Tode zu verweilen.", "tokens": ["Im", "To\u00b7de", "zu", "ver\u00b7wei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Nur da\u00df, die hinterblieben seyn", "tokens": ["Nur", "da\u00df", ",", "die", "hin\u00b7ter\u00b7blie\u00b7ben", "seyn"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "KOUS", "$,", "PRELS", "VVPP", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich gar zu hefftig kr\u00e4ncken,", "tokens": ["Sich", "gar", "zu", "heff\u00b7tig", "kr\u00e4n\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "PTKA", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vnd kaum einmal f\u00fcr grosser Pein", "tokens": ["Vnd", "kaum", "ein\u00b7mal", "f\u00fcr", "gros\u00b7ser", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An jhren Gott gedencken,", "tokens": ["An", "jhren", "Gott", "ge\u00b7den\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Der uns doch allen setzt ein Ziel,", "tokens": ["Der", "uns", "doch", "al\u00b7len", "setzt", "ein", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PIS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das heut k\u00f6mpt oder morgen,", "tokens": ["Das", "heut", "k\u00f6mpt", "o\u00b7der", "mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJD", "KON", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ob wir gleich wenig oder viel", "tokens": ["Ob", "wir", "gleich", "we\u00b7nig", "o\u00b7der", "viel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PIS", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Desselben uns besorgen.", "tokens": ["Des\u00b7sel\u00b7ben", "uns", "be\u00b7sor\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "La\u00df, Herr, des Glaubens Liecht allzeit", "tokens": ["La\u00df", ",", "Herr", ",", "des", "Glau\u00b7bens", "Liecht", "all\u00b7zeit"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "NN", "$,", "ART", "NN", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In unsern Hertzen brennen,", "tokens": ["In", "un\u00b7sern", "Hert\u00b7zen", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df wir die selig' Ewigheit", "tokens": ["Da\u00df", "wir", "die", "se\u00b7lig'", "E\u00b7wig\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Ja m\u00f6gen recht erkennen,", "tokens": ["Ja", "m\u00f6\u00b7gen", "recht", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Vnd klagen dann der Vnsern Todt", "tokens": ["Vnd", "kla\u00b7gen", "dann", "der", "Vn\u00b7sern", "Todt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Trost-gem\u00e4\u00dften Thr\u00e4nen,", "tokens": ["Mit", "Trost\u00b7ge\u00b7m\u00e4\u00df\u00b7ten", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Vns aber stets aus dieser Noth", "tokens": ["Vns", "a\u00b7ber", "stets", "aus", "die\u00b7ser", "Noth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In deinen Himmel sehnen.", "tokens": ["In", "dei\u00b7nen", "Him\u00b7mel", "seh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Am allerbesten ist es zwar", "tokens": ["Am", "al\u00b7ler\u00b7bes\u00b7ten", "ist", "es", "zwar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Herren seyn verschieden,", "tokens": ["Im", "Her\u00b7ren", "seyn", "ver\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vnd leben bey der Frommen Schar", "tokens": ["Vnd", "le\u00b7ben", "bey", "der", "From\u00b7men", "Schar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vergn\u00fcget vnd in Frieden,", "tokens": ["Ver\u00b7gn\u00fc\u00b7get", "vnd", "in", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN"