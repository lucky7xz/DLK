{"textgrid.poem.55202": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "K\u00fcnstlers Morgenlied", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Tempel ist euch aufgebaut,", "tokens": ["Der", "Tem\u00b7pel", "ist", "euch", "auf\u00b7ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr hohen Musen all,", "tokens": ["Ihr", "ho\u00b7hen", "Mu\u00b7sen", "all", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PIAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und hier in meinem Herzen ist", "tokens": ["Und", "hier", "in", "mei\u00b7nem", "Her\u00b7zen", "ist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Allerheiligste.", "tokens": ["Das", "Al\u00b7ler\u00b7hei\u00b7ligs\u00b7te", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "ADJA", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}}, "stanza.2": {"line.1": {"text": "Wenn morgens mich die Sonne weckt,", "tokens": ["Wenn", "mor\u00b7gens", "mich", "die", "Son\u00b7ne", "weckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Warm, froh ich schau umher,", "tokens": ["Warm", ",", "froh", "ich", "schau", "um\u00b7her", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Steht rings ihr Ewiglebenden", "tokens": ["Steht", "rings", "ihr", "E\u00b7wig\u00b7le\u00b7ben\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Im heil'gen Morgenglanz.", "tokens": ["Im", "heil'\u00b7gen", "Mor\u00b7gen\u00b7glanz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich bet hinan, und Lobgesang", "tokens": ["Ich", "bet", "hi\u00b7nan", ",", "und", "Lob\u00b7ge\u00b7sang"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist lauter mein Gebet,", "tokens": ["Ist", "lau\u00b7ter", "mein", "Ge\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und freudeklingend Saitenspiel", "tokens": ["Und", "freu\u00b7de\u00b7klin\u00b7gend", "Sai\u00b7ten\u00b7spiel"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Begleitet mein Gebet.", "tokens": ["Be\u00b7glei\u00b7tet", "mein", "Ge\u00b7bet", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}}, "stanza.4": {"line.1": {"text": "Ich trete vor den Altar hin", "tokens": ["Ich", "tre\u00b7te", "vor", "den", "Al\u00b7tar", "hin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und lese, wie sich's ziemt,", "tokens": ["Und", "le\u00b7se", ",", "wie", "sich's", "ziemt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Andacht liturg'scher Lektion", "tokens": ["An\u00b7dacht", "li\u00b7tur\u00b7g'\u00b7scher", "Lek\u00b7ti\u00b7on"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Im heiligen Homer.", "tokens": ["Im", "hei\u00b7li\u00b7gen", "Ho\u00b7mer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NE", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Und wenn er ins Get\u00fcmmel mich", "tokens": ["Und", "wenn", "er", "ins", "Ge\u00b7t\u00fcm\u00b7mel", "mich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPRART", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von L\u00f6wenkriegern rei\u00dft", "tokens": ["Von", "L\u00f6\u00b7wen\u00b7krie\u00b7gern", "rei\u00dft"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und G\u00f6tters\u00f6hn auf Wagen hoch", "tokens": ["Und", "G\u00f6t\u00b7ter\u00b7s\u00f6hn", "auf", "Wa\u00b7gen", "hoch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Rachgl\u00fchend st\u00fcrmen an", "tokens": ["Rach\u00b7gl\u00fc\u00b7hend", "st\u00fcr\u00b7men", "an"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und Ro\u00df dann vor dem Wagen st\u00fcrzt", "tokens": ["Und", "Ro\u00df", "dann", "vor", "dem", "Wa\u00b7gen", "st\u00fcrzt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "ADV", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und drunter und dr\u00fcber sich", "tokens": ["Und", "drun\u00b7ter", "und", "dr\u00fc\u00b7ber", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "KON", "PAV", "PRF"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Freund', Feinde w\u00e4lzen in Todesblut \u2013", "tokens": ["Freund'", ",", "Fein\u00b7de", "w\u00e4l\u00b7zen", "in", "To\u00b7des\u00b7blut", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Er sengte sie dahin", "tokens": ["Er", "seng\u00b7te", "sie", "da\u00b7hin"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PAV"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Mit Flammenschwert, der Heldensohn,", "tokens": ["Mit", "Flam\u00b7men\u00b7schwert", ",", "der", "Hel\u00b7den\u00b7sohn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zehntausend auf einmal,", "tokens": ["Zehn\u00b7tau\u00b7send", "auf", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "APPR", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bis dann auch er, geb\u00e4ndiget", "tokens": ["Bis", "dann", "auch", "er", ",", "ge\u00b7b\u00e4n\u00b7di\u00b7get"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ADV", "ADV", "PPER", "$,", "VVFIN"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.4": {"text": "Von einer G\u00f6tterhand,", "tokens": ["Von", "ei\u00b7ner", "G\u00f6t\u00b7ter\u00b7hand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ab auf den Rogus niederst\u00fcrzt,", "tokens": ["Ab", "auf", "den", "Ro\u00b7gus", "nie\u00b7ders\u00b7t\u00fcrzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NE", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den er sich selbst geh\u00e4uft,", "tokens": ["Den", "er", "sich", "selbst", "ge\u00b7h\u00e4uft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Feinde nun den sch\u00f6nen Leib", "tokens": ["Und", "Fein\u00b7de", "nun", "den", "sch\u00f6\u00b7nen", "Leib"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Versch\u00e4ndend tasten an:", "tokens": ["Ver\u00b7sch\u00e4n\u00b7dend", "tas\u00b7ten", "an", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Da greif ich mutig auf, es wird", "tokens": ["Da", "greif", "ich", "mu\u00b7tig", "auf", ",", "es", "wird"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,", "PPER", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Kohle zum Gewehr,", "tokens": ["Die", "Koh\u00b7le", "zum", "Ge\u00b7wehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und jene meine hohe Wand", "tokens": ["Und", "je\u00b7ne", "mei\u00b7ne", "ho\u00b7he", "Wand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Schlachtfeldwogen braust.", "tokens": ["In", "Schlacht\u00b7feld\u00b7wo\u00b7gen", "braust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Hinan! Hinan! Es heulet laut", "tokens": ["Hi\u00b7nan", "!", "Hi\u00b7nan", "!", "Es", "heu\u00b7let", "laut"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$.", "ADV", "$.", "PPER", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gebr\u00fcll der Feindeswut,", "tokens": ["Ge\u00b7br\u00fcll", "der", "Fein\u00b7des\u00b7wut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Schild an Schild, und Schwert auf Helm,", "tokens": ["Und", "Schild", "an", "Schild", ",", "und", "Schwert", "auf", "Helm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "$,", "KON", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und um den Toten Tod.", "tokens": ["Und", "um", "den", "To\u00b7ten", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Ich dr\u00e4nge mich hinan, hinan,", "tokens": ["Ich", "dr\u00e4n\u00b7ge", "mich", "hi\u00b7nan", ",", "hi\u00b7nan", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "$,"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Da k\u00e4mpfen sie um ihn,", "tokens": ["Da", "k\u00e4mp\u00b7fen", "sie", "um", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die tapfern Freunde, tapferer", "tokens": ["Die", "tap\u00b7fern", "Freun\u00b7de", ",", "tap\u00b7fe\u00b7rer"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ADJA"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "In ihrer Tr\u00e4nenwut.", "tokens": ["In", "ih\u00b7rer", "Tr\u00e4\u00b7nen\u00b7wut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Ach, rettet! K\u00e4mpfet! Rettet ihn!", "tokens": ["Ach", ",", "ret\u00b7tet", "!", "K\u00e4mp\u00b7fet", "!", "Ret\u00b7tet", "ihn", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "$.", "NE", "$.", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ins Lager tragt ihn fort,", "tokens": ["Ins", "La\u00b7ger", "tragt", "ihn", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Balsam gie\u00dft dem Toten auf", "tokens": ["Und", "Bal\u00b7sam", "gie\u00dft", "dem", "To\u00b7ten", "auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "VVFIN", "ART", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Tr\u00e4nen Totenehr!", "tokens": ["Und", "Tr\u00e4\u00b7nen", "To\u00b7te\u00b7nehr", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Und find ich mich zur\u00fcck hierher,", "tokens": ["Und", "find", "ich", "mich", "zu\u00b7r\u00fcck", "hier\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "ADV", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Empf\u00e4ngst du, Liebe, mich,", "tokens": ["Emp\u00b7f\u00e4ngst", "du", ",", "Lie\u00b7be", ",", "mich", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein M\u00e4dchen, ach, im Bilde nur", "tokens": ["Mein", "M\u00e4d\u00b7chen", ",", "ach", ",", "im", "Bil\u00b7de", "nur"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ITJ", "$,", "APPRART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und so im Bilde warm!", "tokens": ["Und", "so", "im", "Bil\u00b7de", "warm", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Ach, wie du ruhtest neben mir", "tokens": ["Ach", ",", "wie", "du", "ruh\u00b7test", "ne\u00b7ben", "mir"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "PPER", "VVFIN", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und schmachtetest mich an,", "tokens": ["Und", "schmach\u00b7te\u00b7test", "mich", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und mir's vom Aug durchs Herz hindurch", "tokens": ["Und", "mir's", "vom", "Aug", "durchs", "Herz", "hin\u00b7durch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "APPRART", "NN", "APPRART", "NN", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Griffel schmachtete!", "tokens": ["Zum", "Grif\u00b7fel", "schmach\u00b7te\u00b7te", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Wie ich an Aug und Wange mich", "tokens": ["Wie", "ich", "an", "Aug", "und", "Wan\u00b7ge", "mich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "NN", "KON", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Mund mich weidete,", "tokens": ["Und", "Mund", "mich", "wei\u00b7de\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und mir's im Busen jung und frisch,", "tokens": ["Und", "mir's", "im", "Bu\u00b7sen", "jung", "und", "frisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "APPRART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie einer Gottheit, war !", "tokens": ["Wie", "ei\u00b7ner", "Got\u00b7theit", ",", "war", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "O kehre doch und bleibe dann", "tokens": ["O", "keh\u00b7re", "doch", "und", "blei\u00b7be", "dann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "KON", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In meinen Armen fest,", "tokens": ["In", "mei\u00b7nen", "Ar\u00b7men", "fest", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und keine, keine Schlachten mehr,", "tokens": ["Und", "kei\u00b7ne", ",", "kei\u00b7ne", "Schlach\u00b7ten", "mehr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "$,", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur dich in meinem Arm!", "tokens": ["Nur", "dich", "in", "mei\u00b7nem", "Arm", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Und sollst mir, meine Liebe, sein", "tokens": ["Und", "sollst", "mir", ",", "mei\u00b7ne", "Lie\u00b7be", ",", "sein"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "VMFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Alldeutend Ideal,", "tokens": ["A\u00b7lldeu\u00b7tend", "I\u00b7deal", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Madonna sein, ein Erstlingskind,", "tokens": ["Ma\u00b7don\u00b7na", "sein", ",", "ein", "Erst\u00b7lings\u00b7kind", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VAINF", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein heiligs, an der Brust;", "tokens": ["Ein", "hei\u00b7ligs", ",", "an", "der", "Brust", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "APPR", "ART", "NN", "$."], "