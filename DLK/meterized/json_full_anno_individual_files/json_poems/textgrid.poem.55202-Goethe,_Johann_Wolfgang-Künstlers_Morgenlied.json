ct"], "pos": ["PPOSAT", "ADJA", "NN", "PIAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und hier in meinem Herzen ist", "tokens": ["Und", "hier", "in", "mei\u00b7nem", "Her\u00b7zen", "ist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Allerheiligste.", "tokens": ["Das", "Al\u00b7ler\u00b7hei\u00b7ligs\u00b7te", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "ADJA", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}}, "stanza.22": {"line.1": {"text": "Wenn morgens mich die Sonne weckt,", "tokens": ["Wenn", "mor\u00b7gens", "mich", "die", "Son\u00b7ne", "weckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Warm, froh ich schau umher,", "tokens": ["Warm", ",", "froh", "ich", "schau", "um\u00b7her", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Steht rings ihr Ewiglebenden", "tokens": ["Steht", "rings", "ihr", "E\u00b7wig\u00b7le\u00b7ben\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Im heil'gen Morgenglanz.", "tokens": ["Im", "heil'\u00b7gen", "Mor\u00b7gen\u00b7glanz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.23": {"line.1": {"text": "Ich bet hinan, und Lobgesang", "tokens": ["Ich", "bet", "hi\u00b7nan", ",", "und", "Lob\u00b7ge\u00b7sang"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER"