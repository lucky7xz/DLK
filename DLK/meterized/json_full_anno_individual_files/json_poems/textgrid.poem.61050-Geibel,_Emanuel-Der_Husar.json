{"textgrid.poem.61050": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Der Husar", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Schlacht ist aus, zersprengt des Feindes Scharen,", "tokens": ["Die", "Schlacht", "ist", "aus", ",", "zer\u00b7sprengt", "des", "Fein\u00b7des", "Scha\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKVZ", "$,", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein schwarzes Bahrtuch sinkt die Nacht hernieder,", "tokens": ["Ein", "schwar\u00b7zes", "Bahr\u00b7tuch", "sinkt", "die", "Nacht", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da lagern rings ums Feuer die Husaren", "tokens": ["Da", "la\u00b7gern", "rings", "ums", "Feu\u00b7er", "die", "Hu\u00b7sa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und w\u00e4rmen ihre kampfesm\u00fcden Glieder.", "tokens": ["Und", "w\u00e4r\u00b7men", "ih\u00b7re", "kamp\u00b7fes\u00b7m\u00fc\u00b7den", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein b\u00e4rt'ger Reiter sieht nach seiner Wunde,", "tokens": ["Ein", "b\u00e4rt'\u00b7ger", "Rei\u00b7ter", "sieht", "nach", "sei\u00b7ner", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein andrer ladet emsig die Pistolen,", "tokens": ["Ein", "an\u00b7drer", "la\u00b7det", "em\u00b7sig", "die", "Pis\u00b7to\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die volle Flasche geht von Mund zu Munde;", "tokens": ["Die", "vol\u00b7le", "Fla\u00b7sche", "geht", "von", "Mund", "zu", "Mun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kein Wort erschallt, nur tiefes Atemholen.", "tokens": ["Kein", "Wort", "er\u00b7schallt", ",", "nur", "tie\u00b7fes", "A\u00b7tem\u00b7ho\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$,", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und still ist's ringsum. Nur die Fr\u00fchlingswinde,", "tokens": ["Und", "still", "ist's", "ring\u00b7sum", ".", "Nur", "die", "Fr\u00fch\u00b7lings\u00b7win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ADV", "$.", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gewohnt mit holden Blumen sonst zu kosen,", "tokens": ["Ge\u00b7wohnt", "mit", "hol\u00b7den", "Blu\u00b7men", "sonst", "zu", "ko\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie spielen durchs Gefild' und f\u00e4cheln linde", "tokens": ["Sie", "spie\u00b7len", "durchs", "Ge\u00b7fild'", "und", "f\u00e4\u00b7cheln", "lin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "KON", "VVFIN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Todeswunden dunkle Purpurrosen.", "tokens": ["Der", "To\u00b7des\u00b7wun\u00b7den", "dunk\u00b7le", "Pur\u00b7pur\u00b7ro\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch sieh! Dort unterm Lindendach am Turme", "tokens": ["Doch", "sieh", "!", "Dort", "un\u00b7term", "Lin\u00b7den\u00b7dach", "am", "Tur\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "$.", "ADV", "APPRART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist sanft ein junger Reiter eingeschlafen,", "tokens": ["Ist", "sanft", "ein", "jun\u00b7ger", "Rei\u00b7ter", "ein\u00b7ge\u00b7schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es rettet' aus des Krieges w\u00fcstem Sturme", "tokens": ["Es", "ret\u00b7tet'", "aus", "des", "Krie\u00b7ges", "w\u00fcs\u00b7tem", "Stur\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sein Geist sich in der Tr\u00e4ume Friedenshafen.", "tokens": ["Sein", "Geist", "sich", "in", "der", "Tr\u00e4u\u00b7me", "Frie\u00b7dens\u00b7ha\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Er schlummert s\u00fc\u00df. Es hat um seine Wangen", "tokens": ["Er", "schlum\u00b7mert", "s\u00fc\u00df", ".", "Es", "hat", "um", "sei\u00b7ne", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$.", "PPER", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein ros'ger Freudenschimmer sich ergossen,", "tokens": ["Ein", "ros'\u00b7ger", "Freu\u00b7den\u00b7schim\u00b7mer", "sich", "er\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein mildes L\u00e4cheln h\u00e4lt den Mund umfangen,", "tokens": ["Ein", "mil\u00b7des", "L\u00e4\u00b7cheln", "h\u00e4lt", "den", "Mund", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um den die ersten blonden Flaumen sprossen.", "tokens": ["Um", "den", "die", "ers\u00b7ten", "blon\u00b7den", "Flau\u00b7men", "spros\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ART", "ADJA", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Er tr\u00e4umt sich heim vielleicht ins enge Zimmer,", "tokens": ["Er", "tr\u00e4umt", "sich", "heim", "viel\u00b7leicht", "ins", "en\u00b7ge", "Zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKVZ", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In seines Jugendspiels geliebte R\u00e4ume -", "tokens": ["In", "sei\u00b7nes", "Ju\u00b7gend\u00b7spiels", "ge\u00b7lieb\u00b7te", "R\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durchs offne Fenster f\u00e4llt der Sonnenschimmer,", "tokens": ["Durchs", "off\u00b7ne", "Fens\u00b7ter", "f\u00e4llt", "der", "Son\u00b7nen\u00b7schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und drau\u00dfen duften Wein und Bl\u00fctenb\u00e4ume.", "tokens": ["Und", "drau\u00b7\u00dfen", "duf\u00b7ten", "Wein", "und", "Bl\u00fc\u00b7ten\u00b7b\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Und vor ihm steht ein M\u00e4dchen hold ergl\u00fchend,", "tokens": ["Und", "vor", "ihm", "steht", "ein", "M\u00e4d\u00b7chen", "hold", "er\u00b7gl\u00fc\u00b7hend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Morgenstrahl vergoldet ihre Wangen,", "tokens": ["Der", "Mor\u00b7gen\u00b7strahl", "ver\u00b7gol\u00b7det", "ih\u00b7re", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df sch\u00f6ner noch der Mund, in Purpur bl\u00fchend,", "tokens": ["Da\u00df", "sch\u00f6\u00b7ner", "noch", "der", "Mund", ",", "in", "Pur\u00b7pur", "bl\u00fc\u00b7hend", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "ADV", "ART", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df gl\u00e4nzender die braunen Locken prangen.", "tokens": ["Da\u00df", "gl\u00e4n\u00b7zen\u00b7der", "die", "brau\u00b7nen", "Lo\u00b7cken", "pran\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Sie reicht im Glas ihm feurigen Tokaier,", "tokens": ["Sie", "reicht", "im", "Glas", "ihm", "feu\u00b7ri\u00b7gen", "To\u00b7kai\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Nachdem sie nicht verschm\u00e4ht, zum Gru\u00df zu nippen;", "tokens": ["Nach\u00b7dem", "sie", "nicht", "ver\u00b7schm\u00e4ht", ",", "zum", "Gru\u00df", "zu", "nip\u00b7pen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVPP", "$,", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er aber k\u00fc\u00dft, ein ungest\u00fcmer Freier,", "tokens": ["Er", "a\u00b7ber", "k\u00fc\u00dft", ",", "ein", "un\u00b7ge\u00b7st\u00fc\u00b7mer", "Frei\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Anstatt des s\u00fc\u00dfen Weins die s\u00fc\u00dfern Lippen.", "tokens": ["An\u00b7statt", "des", "s\u00fc\u00b7\u00dfen", "Weins", "die", "s\u00fc\u00b7\u00dfern", "Lip\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Umschlungen stehn sie, ganz in sich versunken,", "tokens": ["Um\u00b7schlun\u00b7gen", "stehn", "sie", ",", "ganz", "in", "sich", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "ADV", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und schaun sich selig l\u00e4chelnd an und schweigen,", "tokens": ["Und", "schaun", "sich", "se\u00b7lig", "l\u00e4\u00b7chelnd", "an", "und", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "ADJD", "PTKVZ", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und nur die Nachtigallen schmettern, trunken", "tokens": ["Und", "nur", "die", "Nach\u00b7ti\u00b7gal\u00b7len", "schmet\u00b7tern", ",", "trun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADV", "ART", "NN", "VVINF", "$,", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von Rosenduft, ein Brautlied in den Zweigen.", "tokens": ["Von", "Ro\u00b7sen\u00b7duft", ",", "ein", "Braut\u00b7lied", "in", "den", "Zwei\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "So tr\u00e4umt der J\u00fcngling - aber pl\u00f6tzlich t\u00f6nen", "tokens": ["So", "tr\u00e4umt", "der", "J\u00fcng\u00b7ling", "a\u00b7ber", "pl\u00f6tz\u00b7lich", "t\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$(", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Trompeten fern in lustigen Fanfaren,", "tokens": ["Trom\u00b7pe\u00b7ten", "fern", "in", "lus\u00b7ti\u00b7gen", "Fan\u00b7fa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es fallen Sch\u00fcsse, dumpfe Trommeln dr\u00f6hnen,", "tokens": ["Es", "fal\u00b7len", "Sch\u00fcs\u00b7se", ",", "dump\u00b7fe", "Trom\u00b7meln", "dr\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und auf vom Boden springen die Husaren.", "tokens": ["Und", "auf", "vom", "Bo\u00b7den", "sprin\u00b7gen", "die", "Hu\u00b7sa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Der Tr\u00e4umer auch erwacht. Er f\u00e4hrt zusammen,", "tokens": ["Der", "Tr\u00e4u\u00b7mer", "auch", "er\u00b7wacht", ".", "Er", "f\u00e4hrt", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$.", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dann sitzt er eilig auf mit den Genossen;", "tokens": ["Dann", "sitzt", "er", "ei\u00b7lig", "auf", "mit", "den", "Ge\u00b7nos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie jagen fort; zu Asche gl\u00fchn die Flammen,", "tokens": ["Sie", "ja\u00b7gen", "fort", ";", "zu", "A\u00b7sche", "gl\u00fchn", "die", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und fern verhallt der Hufschlag von den Rossen.", "tokens": ["Und", "fern", "ver\u00b7hallt", "der", "Huf\u00b7schlag", "von", "den", "Ros\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Die Schlacht ist aus, zersprengt des Feindes Scharen,", "tokens": ["Die", "Schlacht", "ist", "aus", ",", "zer\u00b7sprengt", "des", "Fein\u00b7des", "Scha\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKVZ", "$,", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein schwarzes Bahrtuch sinkt die Nacht hernieder,", "tokens": ["Ein", "schwar\u00b7zes", "Bahr\u00b7tuch", "sinkt", "die", "Nacht", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measur