{"dta.poem.419": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Auf ihre Augen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Brunettens Augen-Strahl besch\u00e4mt den Glantz der Sternen/", "tokens": ["Bru\u00b7net\u00b7tens", "Au\u00b7gen\u00b7Strahl", "be\u00b7sch\u00e4mt", "den", "Glantz", "der", "Ster\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Und der ", "tokens": ["Und", "der"], "token_info": ["word", "word"], "pos": ["KON", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Die Sonne selbst wird blind durch jenen Schmuck gemacht/", "tokens": ["Die", "Son\u00b7ne", "selbst", "wird", "blind", "durch", "je\u00b7nen", "Schmuck", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ADJD", "APPR", "PDAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der aus den schwartzen Pech der sch\u00f6nen Augen lacht.", "tokens": ["Der", "aus", "den", "schwart\u00b7zen", "Pech", "der", "sch\u00f6\u00b7nen", "Au\u00b7gen", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Des Himmels Feuer mus vor ihren Blicken weichen/", "tokens": ["Des", "Him\u00b7mels", "Feu\u00b7er", "mus", "vor", "ih\u00b7ren", "Bli\u00b7cken", "wei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Kein Demant kan den Glantz/ den sch\u00f6nen Glantz erreichen.", "tokens": ["Kein", "De\u00b7mant", "kan", "den", "Glantz", "/", "den", "sch\u00f6\u00b7nen", "Glantz", "er\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "ART", "NN", "$(", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}