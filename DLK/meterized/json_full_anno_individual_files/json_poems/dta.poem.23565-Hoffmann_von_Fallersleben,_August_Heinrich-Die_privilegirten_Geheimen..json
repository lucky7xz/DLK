{"dta.poem.23565": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die privilegirten Geheimen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sie mauern und sie bauen", "tokens": ["Sie", "mau\u00b7ern", "und", "sie", "bau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Am Tempel alles Lichts,", "tokens": ["Am", "Tem\u00b7pel", "al\u00b7les", "Lichts", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch ist noch nichts zu schauen \u2014", "tokens": ["Doch", "ist", "noch", "nichts", "zu", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PIS", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sagt an, woran gebricht's?", "tokens": ["Sagt", "an", ",", "wo\u00b7ran", "ge\u00b7bricht's", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PWAV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wird's klar an jenem Tage,", "tokens": ["Wird's", "klar", "an", "je\u00b7nem", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Am Tage des Gerichts,", "tokens": ["Am", "Ta\u00b7ge", "des", "Ge\u00b7richts", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wann w\u00e4gen wird die Wage", "tokens": ["Wann", "w\u00e4\u00b7gen", "wird", "die", "Wa\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVINF", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Etwas und das Nichts?", "tokens": ["Das", "Et\u00b7was", "und", "das", "Nichts", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "KON", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ein K\u00f6nig wollt' erfreuen", "tokens": ["Ein", "K\u00f6\u00b7nig", "wollt'", "er\u00b7freu\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sich einst auch dieses Lichts,", "tokens": ["Sich", "einst", "auch", "die\u00b7ses", "Lichts", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADV", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er sprach als th\u00e4t's ihn reuen:", "tokens": ["Er", "sprach", "als", "th\u00e4t's", "ihn", "reu\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es ist ein gro\u00dfes Nichts!", "tokens": ["Es", "ist", "ein", "gro\u00b7\u00dfes", "Nichts", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Drum ist es auch erkl\u00e4rlich:", "tokens": ["Drum", "ist", "es", "auch", "er\u00b7kl\u00e4r\u00b7lich", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "W\u00e4r's etwas mehr als nichts,", "tokens": ["W\u00e4r's", "et\u00b7was", "mehr", "als", "nichts", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "KOKOM", "PIS", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Erlaubte man wohl schwerlich", "tokens": ["Er\u00b7laub\u00b7te", "man", "wohl", "schwer\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bei uns dies gro\u00dfe Nichts.", "tokens": ["Bei", "uns", "dies", "gro\u00b7\u00dfe", "Nichts", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PDS", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}