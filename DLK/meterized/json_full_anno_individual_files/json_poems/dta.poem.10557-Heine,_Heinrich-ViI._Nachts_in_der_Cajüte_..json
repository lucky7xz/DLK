{"dta.poem.10557": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "ViI.  \n  Nachts in der Caj\u00fcte .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Aus den Himmelsaugen droben", "tokens": ["Aus", "den", "Him\u00b7mel\u00b7sau\u00b7gen", "dro\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fallen zitternd lichte Funken", "tokens": ["Fal\u00b7len", "zit\u00b7ternd", "lich\u00b7te", "Fun\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch die Nacht, und meine Seele", "tokens": ["Durch", "die", "Nacht", ",", "und", "mei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dehnt sich liebeweit und weiter.", "tokens": ["Dehnt", "sich", "lie\u00b7be\u00b7weit", "und", "wei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "KON", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "O, Ihr Himmelsaugen droben!", "tokens": ["O", ",", "Ihr", "Him\u00b7mel\u00b7sau\u00b7gen", "dro\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weint Euch aus in meine Seele,", "tokens": ["Weint", "Euch", "aus", "in", "mei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df von lieben Sternenthr\u00e4nen", "tokens": ["Da\u00df", "von", "lie\u00b7ben", "Ster\u00b7nen\u00b7thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ueberflie\u00dfet meine Seele.", "tokens": ["Ue\u00b7ber\u00b7flie\u00b7\u00dfet", "mei\u00b7ne", "See\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}