{"dta.poem.3359": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Wie man\u2019s macht !", "genre": "Lyrik", "period": "N.A.", "pub_year": "1846", "urn": "urn:nbn:de:kobv:b4-200905191464", "language": ["de:0.99"], "booktitle": "Freiligrath, Ferdinand: \u00c7a ira! Herisau, 1846."}, "poem": {"stanza.1": {"line.1": {"text": "So wird es kommen, eh\u2019 ihr denkt: \u2014 Das Volk", "tokens": ["So", "wird", "es", "kom\u00b7men", ",", "eh'", "ihr", "denkt", ":", "Das", "Volk"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVINF", "$,", "KOUS", "PPER", "VVFIN", "$.", "$(", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "hat Nichts zu bei\u00dfen mehr!", "tokens": ["hat", "Nichts", "zu", "bei\u00b7\u00dfen", "mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PTKZU", "VVINF", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durch seine Lumpen pfeift der Wind! Wo nimmt", "tokens": ["Durch", "sei\u00b7ne", "Lum\u00b7pen", "pfeift", "der", "Wind", "!", "Wo", "nimmt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$.", "PWAV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "es Brot und Kleider her? \u2014", "tokens": ["es", "Brot", "und", "Klei\u00b7der", "her", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "NN", "KON", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da tritt ein kecker Bursche vor; der spricht: \u201eDie", "tokens": ["Da", "tritt", "ein", "ke\u00b7cker", "Bur\u00b7sche", "vor", ";", "der", "spricht", ":", "\u201e", "Die"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$.", "ART", "VVFIN", "$.", "$(", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Kleider w\u00fc\u00dft\u2019 ich schon!", "tokens": ["Klei\u00b7der", "w\u00fc\u00dft'", "ich", "schon", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Mir nach, wer Rock und Hosen will! Zeug f\u00fcr", "tokens": ["Mir", "nach", ",", "wer", "Rock", "und", "Ho\u00b7sen", "will", "!", "Zeug", "f\u00fcr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "PTKVZ", "$,", "PWS", "NN", "KON", "NN", "VMFIN", "$.", "NN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "ein ganzes Bataillon!\u201c", "tokens": ["ein", "gan\u00b7zes", "Ba\u00b7tail\u00b7lon", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Und wie man eine Hand umdreht, stellt er in Rot-", "tokens": ["Und", "wie", "man", "ei\u00b7ne", "Hand", "um\u00b7dreht", ",", "stellt", "er", "in", "Rot"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PIS", "ART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ten sie und Reih\u2019n,", "tokens": ["ten", "sie", "und", "Reih'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Schreit: \u201eLinksum kehrt!\u201c und: \u201eVorw\u00e4rts Marsch!\u201e", "tokens": ["Schreit", ":", "\u201e", "Link\u00b7sum", "kehrt", "!", "\u201c", "und", ":", "\u201e", "Vor\u00b7w\u00e4rts", "Marsch", "!", "\u201e"], "token_info": ["word", "punct", "punct", "word", "word", "punct", "punct", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "$.", "$(", "NE", "VVFIN", "$.", "$(", "KON", "$.", "$(", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und f\u00fchrt zur Kreisstadt sie hinein.", "tokens": ["und", "f\u00fchrt", "zur", "Kreis\u00b7stadt", "sie", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vor einem steinernen Geb\u00e4u Halt machen l\u00e4\u00dft er", "tokens": ["Vor", "ei\u00b7nem", "stei\u00b7ner\u00b7nen", "Ge\u00b7b\u00e4u", "Halt", "ma\u00b7chen", "l\u00e4\u00dft", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "VVINF", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "trutziglich:", "tokens": ["trut\u00b7zig\u00b7lich", ":"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "\u201eseht da, mein Kleidermagazin \u2014 das Landwehr-", "tokens": ["\u201e", "seht", "da", ",", "mein", "Klei\u00b7der\u00b7ma\u00b7ga\u00b7zin", "das", "Land\u00b7wehr"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "VVFIN", "ADV", "$,", "PPOSAT", "NN", "$(", "ART", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "zeughaus nennt es sich!", "tokens": ["zeug\u00b7haus", "nennt", "es", "sich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "\u201edarinnen liegt, was ihr bed\u00fcrft: Leinwand zu", "tokens": ["\u201e", "da\u00b7rin\u00b7nen", "liegt", ",", "was", "ihr", "be\u00b7d\u00fcrft", ":", "Lein\u00b7wand", "zu"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "ADV", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$.", "NN", "PTKZU"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Hemden, derb und schwer!", "tokens": ["Hem\u00b7den", ",", "derb", "und", "schwer", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wattirte Jacken, frisch gen\u00e4ht \u2014 dazu von zweier-", "tokens": ["Wat\u00b7tir\u00b7te", "Ja\u00b7cken", ",", "frisch", "ge\u00b7n\u00e4ht", "da\u00b7zu", "von", "zwei\u00b7er"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJD", "VVPP", "$(", "PAV", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "lei Couleur!", "tokens": ["lei", "Cou\u00b7leur", "!"], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Tuchm\u00e4ntel f\u00fcr die Regennacht! Feldm\u00fctzen auch", "tokens": ["Tuch\u00b7m\u00e4n\u00b7tel", "f\u00fcr", "die", "Re\u00b7gen\u00b7nacht", "!", "Feld\u00b7m\u00fct\u00b7zen", "auch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "$.", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "und Handschuh\u2019 viel,", "tokens": ["und", "Hand\u00b7schuh'", "viel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und Alles, was sich sonst geh\u00f6rt zu Heerschau und", "tokens": ["Und", "Al\u00b7les", ",", "was", "sich", "sonst", "ge\u00b7h\u00f6rt", "zu", "Heer\u00b7schau", "und"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "$,", "PRELS", "PRF", "ADV", "VVFIN", "APPR", "NN", "KON"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.8": {"text": "Paradespiel!", "tokens": ["Pa\u00b7ra\u00b7de\u00b7spiel", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "\u201eihr kennt den ganzen Rummel ja! Ob auch mit", "tokens": ["\u201e", "ihr", "kennt", "den", "gan\u00b7zen", "Rum\u00b7mel", "ja", "!", "Ob", "auch", "mit"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "ART", "ADJA", "NN", "ADV", "$.", "KOUS", "ADV", "APPR"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Hadern jetzt bedeckt,", "tokens": ["Ha\u00b7dern", "jetzt", "be\u00b7deckt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Haben die Meisten doch von euch in der Monti-", "tokens": ["Ha\u00b7ben", "die", "Meis\u00b7ten", "doch", "von", "euch", "in", "der", "Mon\u00b7ti"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ADV", "APPR", "PPER", "APPR", "ART", "TRUNC"], "meter": "+--+---+--+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "rung schon gesteckt!", "tokens": ["rung", "schon", "ge\u00b7steckt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Wehrm\u00e4nner seid ihr allzumal! So lange Jeder", "tokens": ["Wehr\u00b7m\u00e4n\u00b7ner", "seid", "ihr", "all\u00b7zu\u00b7mal", "!", "So", "lan\u00b7ge", "Je\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "ADV", "$.", "ADV", "ADV", "PIS"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "denn vom Pflock", "tokens": ["denn", "vom", "Pflock"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPRART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Sich seinen eignen Hosensack und seinen eignen", "tokens": ["Sich", "sei\u00b7nen", "eig\u00b7nen", "Ho\u00b7sen\u00b7sack", "und", "sei\u00b7nen", "eig\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PRF", "PPOSAT", "ADJA", "NN", "KON", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "blauen Rock!", "tokens": ["blau\u00b7en", "Rock", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "\u201eja, ", "tokens": ["\u201e", "ja", ","], "token_info": ["punct", "word", "punct"], "pos": ["$(", "PTKANT", "$,"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "K\u00f6nigs? \u2014 Liebe Zeit!", "tokens": ["K\u00f6\u00b7nigs", "?", "Lie\u00b7be", "Zeit", "!"], "token_info": ["word", "punct", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Gabt ", "tokens": ["Gabt"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "die ihr seid!", "tokens": ["die", "ihr", "seid", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VAFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Du da \u2014 ist nicht die Leinwand hier der Flachs,", "tokens": ["Du", "da", "ist", "nicht", "die", "Lein\u00b7wand", "hier", "der", "Flachs", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$(", "VAFIN", "PTKNEG", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "den deine Mutter spann,", "tokens": ["den", "dei\u00b7ne", "Mut\u00b7ter", "spann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Inde\u00df vom kummervollen Aug\u2019 die Thr\u00e4n\u2019 ihr auf", "tokens": ["In\u00b7de\u00df", "vom", "kum\u00b7mer\u00b7vol\u00b7len", "Aug'", "die", "Thr\u00e4n'", "ihr", "auf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "ADJA", "NN", "ART", "NN", "PPOSAT", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "den Faden rann?", "tokens": ["den", "Fa\u00b7den", "rann", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "\u201enehmt denn! So recht! Da prunkt ihr ja, als", "tokens": ["\u201e", "nehmt", "denn", "!", "So", "recht", "!", "Da", "prunkt", "ihr", "ja", ",", "als"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["$(", "VVFIN", "ADV", "$.", "ADV", "ADJD", "$.", "ADV", "VVFIN", "PPER", "ADV", "$,", "KOUS"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "ging\u2019s zu Felde morgen fr\u00fch,", "tokens": ["ging's", "zu", "Fel\u00b7de", "mor\u00b7gen", "fr\u00fch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Oder doch allerwenigstens nach Grimlinghausen zur", "tokens": ["O\u00b7der", "doch", "al\u00b7ler\u00b7we\u00b7nigs\u00b7tens", "nach", "Grim\u00b7ling\u00b7hau\u00b7sen", "zur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "APPR", "NE", "APPRART"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Revue!", "tokens": ["Re\u00b7vue", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Nur die Muskete fehlt euch noch! Doch sieh\u2019, da", "tokens": ["Nur", "die", "Mus\u00b7ke\u00b7te", "fehlt", "euch", "noch", "!", "Doch", "sieh'", ",", "da"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "ADV", "$.", "KON", "VVFIN", "$,", "KOUS"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "steht von ungef\u00e4hr", "tokens": ["steht", "von", "un\u00b7ge\u00b7f\u00e4hr"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Der ganze Saal voll! Zum Versuch: \u2014 Gewehr", "tokens": ["Der", "gan\u00b7ze", "Saal", "voll", "!", "Zum", "Ver\u00b7such", ":", "Ge\u00b7wehr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "$.", "APPRART", "NN", "$.", "$(", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "in Arm! Schultert\u2019s Gewehr!", "tokens": ["in", "Arm", "!", "Schul\u00b7tert's", "Ge\u00b7wehr", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "NE", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "\u201eganz, wie sich\u2019s h\u00f6rt! Das nenn\u2019 ich Schick! Am", "tokens": ["\u201e", "ganz", ",", "wie", "sich's", "h\u00f6rt", "!", "Das", "nenn'", "ich", "Schick", "!", "Am"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["$(", "ADV", "$,", "PWAV", "PIS", "VVFIN", "$.", "PDS", "VVFIN", "PPER", "NN", "$.", "APPRART"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ende \u2026 Jungens, wi\u00dft ihr was?", "tokens": ["En\u00b7de", "\u2026", "Jun\u00b7gens", ",", "wi\u00dft", "ihr", "was", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$,", "VVFIN", "PPER", "PIS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auch die Gewehre wandern mit! \u2014 Gewehr bei", "tokens": ["Auch", "die", "Ge\u00b7weh\u00b7re", "wan\u00b7dern", "mit", "!", "Ge\u00b7wehr", "bei"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "PTKVZ", "$.", "$(", "NN", "APPR"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Fu\u00df! \u2014 Das wird ein Spa\u00df!", "tokens": ["Fu\u00df", "!", "Das", "wird", "ein", "Spa\u00df", "!"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "PDS", "VAFIN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Und w\u00fcrd\u2019 es Ernst \u2026 Nun, m\u00f6glich ist\u2019s! Sie ma-", "tokens": ["Und", "w\u00fcrd'", "es", "Ernst", "\u2026", "Nun", ",", "m\u00f6g\u00b7lich", "ist's", "!", "Sie", "ma"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "NE", "$(", "ADV", "$,", "ADJD", "VAFIN", "$.", "PPER", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "chen immer gro\u00df Geschrei,", "tokens": ["chen", 