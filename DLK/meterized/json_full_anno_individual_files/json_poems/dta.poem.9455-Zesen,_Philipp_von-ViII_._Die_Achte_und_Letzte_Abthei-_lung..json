{"dta.poem.9455": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "ViII .  \n Die Achte und Letzte Abthei-  \n lung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was bringen wir unserer Schwester vor sachen?", "tokens": ["Was", "brin\u00b7gen", "wir", "un\u00b7se\u00b7rer", "Schwes\u00b7ter", "vor", "sa\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Denn unsere Schwester ist z\u00e4hrtlich und klein/", "tokens": ["Denn", "un\u00b7se\u00b7re", "Schwes\u00b7ter", "ist", "z\u00e4hrt\u00b7lich", "und", "klein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Die Br\u00fcste nach keine beliebung uns machen/", "tokens": ["Die", "Br\u00fcs\u00b7te", "nach", "kei\u00b7ne", "be\u00b7lie\u00b7bung", "uns", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "PPER", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Sie geben nicht Wein/", "tokens": ["Sie", "ge\u00b7ben", "nicht", "Wein", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Was wollen wir machen und reden mit jhr?", "tokens": ["Was", "wol\u00b7len", "wir", "ma\u00b7chen", "und", "re\u00b7den", "mit", "jhr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VVINF", "KON", "VVFIN", "APPR", "PPER", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Wann Sie sich ins k\u00fcnfftige giebet herf\u00fcr?", "tokens": ["Wann", "Sie", "sich", "ins", "k\u00fcnff\u00b7ti\u00b7ge", "gie\u00b7bet", "her\u00b7f\u00fcr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "APPRART", "ADJA", "VVFIN", "PTKVZ", "$."], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}}}}}