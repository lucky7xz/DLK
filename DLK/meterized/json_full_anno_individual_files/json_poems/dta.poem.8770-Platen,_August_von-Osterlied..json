{"dta.poem.8770": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Osterlied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Doch Er ist auferstanden!", "tokens": ["Doch", "Er", "ist", "auf\u00b7er\u00b7stan\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O tr\u00fcg' ich meinen Pilgerstab", "tokens": ["O", "tr\u00fcg'", "ich", "mei\u00b7nen", "Pil\u00b7ger\u00b7stab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nach jenen Morgenlanden,", "tokens": ["Nach", "je\u00b7nen", "Mor\u00b7gen\u00b7lan\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zur Felsenkluft", "tokens": ["Zur", "Fel\u00b7sen\u00b7kluft"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Mit hohler Gruft,", "tokens": ["Mit", "hoh\u00b7ler", "Gruft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Denn Er ist auferstanden!", "tokens": ["Denn", "Er", "ist", "auf\u00b7er\u00b7stan\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wer nur sein eigner G\u00f6tze war,", "tokens": ["Wer", "nur", "sein", "eig\u00b7ner", "G\u00f6t\u00b7ze", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPOSAT", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geht unter in dem Staube,", "tokens": ["Geht", "un\u00b7ter", "in", "dem", "Stau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit jener lichten Engelschaar", "tokens": ["Mit", "je\u00b7ner", "lich\u00b7ten", "En\u00b7gel\u00b7schaar"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verschwistert nur der Glaube:", "tokens": ["Ver\u00b7schwis\u00b7tert", "nur", "der", "Glau\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wer liebend strebt", "tokens": ["Wer", "lie\u00b7bend", "strebt"], "token_info": ["word", "word", "word"], "pos": ["PWS", "ADJD", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "So lang' er lebt,", "tokens": ["So", "lang'", "er", "lebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Der hebt sich aus dem Staube!", "tokens": ["Der", "hebt", "sich", "aus", "dem", "Stau\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "So la\u00df uns, wie du selbst, o Sohn,", "tokens": ["So", "la\u00df", "uns", ",", "wie", "du", "selbst", ",", "o", "Sohn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "$,", "PWAV", "PPER", "ADV", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "R\u00fcckkehren aus der H\u00f6lle!", "tokens": ["R\u00fcck\u00b7keh\u00b7ren", "aus", "der", "H\u00f6l\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O da\u00df schon jezt Posaunenton", "tokens": ["O", "da\u00df", "schon", "jezt", "Po\u00b7sau\u00b7nen\u00b7ton"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "ADV", "ADV", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Pol zu Pol ersch\u00f6lle!", "tokens": ["Von", "Pol", "zu", "Pol", "er\u00b7sch\u00f6l\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dein Stachel sticht,", "tokens": ["Dein", "Sta\u00b7chel", "sticht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "O Tod, uns nicht,", "tokens": ["O", "Tod", ",", "uns", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "PTKNEG", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Du siegest nicht, o H\u00f6lle!", "tokens": ["Du", "sie\u00b7gest", "nicht", ",", "o", "H\u00f6l\u00b7le", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "FM", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}