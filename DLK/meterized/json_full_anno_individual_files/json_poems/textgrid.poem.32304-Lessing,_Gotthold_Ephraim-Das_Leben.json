{"textgrid.poem.32304": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "Das Leben", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sechs Tage kannt' ich sie,", "tokens": ["Sechs", "Ta\u00b7ge", "kannt'", "ich", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VMFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und liebte sie sechs Tage.", "tokens": ["Und", "lieb\u00b7te", "sie", "sechs", "Ta\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Am siebenten erbla\u00dfte sie,", "tokens": ["Am", "sie\u00b7ben\u00b7ten", "er\u00b7bla\u00df\u00b7te", "sie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVFIN", "PPER", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Dem ersten meiner ew'gen Klage.", "tokens": ["Dem", "ers\u00b7ten", "mei\u00b7ner", "ew'\u00b7gen", "Kla\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Noch leb' ich, zauderndes Geschick!", "tokens": ["Noch", "leb'", "ich", ",", "zau\u00b7dern\u00b7des", "Ge\u00b7schick", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein pflanzengleiches Leben.", "tokens": ["Ein", "pflan\u00b7zen\u00b7glei\u00b7ches", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "O Himmel, ist f\u00fcr den kein Gl\u00fcck,", "tokens": ["O", "Him\u00b7mel", ",", "ist", "f\u00fcr", "den", "kein", "Gl\u00fcck", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VAFIN", "APPR", "ART", "PIAT", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.8": {"text": "Dem du Gef\u00fchl und Herz gegeben!", "tokens": ["Dem", "du", "Ge\u00b7f\u00fchl", "und", "Herz", "ge\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "O! nimm dem K\u00f6rper W\u00e4rm' und Blut,", "tokens": ["O", "!", "nimm", "dem", "K\u00f6r\u00b7per", "W\u00e4rm'", "und", "Blut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVIMP", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Dem du die Seele schon genommen!", "tokens": ["Dem", "du", "die", "See\u00b7le", "schon", "ge\u00b7nom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Hier, wo ich wein', und wo sie ruht,", "tokens": ["Hier", ",", "wo", "ich", "wein'", ",", "und", "wo", "sie", "ruht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "PTKVZ", "$,", "KON", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Hier la\u00df den Tod auf mich herab gebeten kommen!", "tokens": ["Hier", "la\u00df", "den", "Tod", "auf", "mich", "her\u00b7ab", "ge\u00b7be\u00b7ten", "