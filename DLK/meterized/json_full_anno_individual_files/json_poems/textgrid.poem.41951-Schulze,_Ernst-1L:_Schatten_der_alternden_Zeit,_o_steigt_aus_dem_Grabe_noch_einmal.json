{"textgrid.poem.41951": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schatten der alternden Zeit, o steigt aus dem Grabe noch einmal", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schatten der alternden Zeit, o steigt aus dem Grabe noch einmal", "tokens": ["Schat\u00b7ten", "der", "al\u00b7tern\u00b7den", "Zeit", ",", "o", "steigt", "aus", "dem", "Gra\u00b7be", "noch", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "FM", "VVFIN", "APPR", "ART", "NN", "ADV", "ADV"], "meter": "+--+--+-+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Freundlich empor, o naht, d\u00e4mmernde Bilder der Lust,", "tokens": ["Freund\u00b7lich", "em\u00b7por", ",", "o", "naht", ",", "d\u00e4m\u00b7mern\u00b7de", "Bil\u00b7der", "der", "Lust", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "FM", "VVFIN", "$,", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.3": {"text": "Rosig und leicht, wie ihr einst mich gek\u00fc\u00dft, da der fl\u00fcchtigen Thorheit", "tokens": ["Ro\u00b7sig", "und", "leicht", ",", "wie", "ihr", "einst", "mich", "ge\u00b7k\u00fc\u00dft", ",", "da", "der", "fl\u00fcch\u00b7ti\u00b7gen", "Thor\u00b7heit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "$,", "PWAV", "PPER", "ADV", "PPER", "VVPP", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Rasches Gespann durch der Welt fr\u00f6hlichen Taumel mich trug.", "tokens": ["Ra\u00b7sches", "Ge\u00b7spann", "durch", "der", "Welt", "fr\u00f6h\u00b7li\u00b7chen", "Tau\u00b7mel", "mich", "trug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Damals lenktet mit irrender Hand ihr den luftigen Wagen,", "tokens": ["Da\u00b7mals", "lenk\u00b7tet", "mit", "ir\u00b7ren\u00b7der", "Hand", "ihr", "den", "luf\u00b7ti\u00b7gen", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.6": {"text": "Ueber der Laune Gebot herrschte kein andres Gesetz;", "tokens": ["Ue\u00b7ber", "der", "Lau\u00b7ne", "Ge\u00b7bot", "herrschte", "kein", "and\u00b7res", "Ge\u00b7setz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.7": {"text": "Stolz auf den Z\u00f6gernden lacht' ich hinab, der bed\u00e4chtig am Kreuzweg", "tokens": ["Stolz", "auf", "den", "Z\u00f6\u00b7gern\u00b7den", "lacht'", "ich", "hin\u00b7ab", ",", "der", "be\u00b7d\u00e4ch\u00b7tig", "am", "Kreuz\u00b7weg"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "PRELS", "ADJD", "APPRART", "NN"], "meter": "+--+--+--+--+--++", "measure": "dactylic.tetra.plus"}, "line.8": {"text": "Stand und durch pr\u00fcfenden Rath k\u00fcrzer sich schuf den Genu\u00df.", "tokens": ["Stand", "und", "durch", "pr\u00fc\u00b7fen\u00b7den", "Rath", "k\u00fcr\u00b7zer", "sich", "schuf", "den", "Ge\u00b7nu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "APPR", "ADJA", "NN", "ADJD", "PRF", "VVFIN", "ART", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.9": {"text": "Herzlos nannte wohl Mancher mich dann und z\u00fcrnte des Leichtsinns", "tokens": ["Herz\u00b7los", "nann\u00b7te", "wohl", "Man\u00b7cher", "mich", "dann", "und", "z\u00fcrn\u00b7te", "des", "Leicht\u00b7sinns"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "NN", "PPER", "ADV", "KON", "VVFIN", "ART", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Ueppigem Hauch, der empor \u00fcber die Wolken mich hob:", "tokens": ["Uep\u00b7pi\u00b7gem", "Hauch", ",", "der", "em\u00b7por", "\u00fc\u00b7ber", "die", "Wol\u00b7ken", "mich", "hob", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "PTKVZ", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.11": {"text": "Doch nie holte den leichten Entschlu\u00df der gewichtige Rath ein;", "tokens": ["Doch", "nie", "hol\u00b7te", "den", "leich\u00b7ten", "Ent\u00b7schlu\u00df", "der", "ge\u00b7wich\u00b7ti\u00b7ge", "Rath", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-++--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Flammt doch, eh des Gew\u00f6lks Donner euch warnte, der Blitz.", "tokens": ["Flammt", "doch", ",", "eh", "des", "Ge\u00b7w\u00f6lks", "Don\u00b7ner", "euch", "warn\u00b7te", ",", "der", "Blitz", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "KOUS", "ART", "NN", "NN", "PPER", "VVFIN", "$,", "ART", "NN", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "Gl\u00e4nzend umgab mich die Welt, und es wechselte bunt die Erscheinung,", "tokens": ["Gl\u00e4n\u00b7zend", "um\u00b7gab", "mich", "die", "Welt", ",", "und", "es", "wech\u00b7sel\u00b7te", "bunt", "die", "Er\u00b7schei\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "$,", "KON", "PPER", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.14": {"text": "Und ein heitres Geschick reihete Fest mir an Fest;", "tokens": ["Und", "ein", "heit\u00b7res", "Ge\u00b7schick", "rei\u00b7he\u00b7te", "Fest", "mir", "an", "Fest", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "NN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.15": {"text": "Willig empfing den best\u00e4ndigen Gast die fantastische Freude,", "tokens": ["Wil\u00b7lig", "emp\u00b7fing", "den", "be\u00b7st\u00e4n\u00b7di\u00b7gen", "Gast", "die", "fan\u00b7tas\u00b7ti\u00b7sche", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.16": {"text": "Weich im Schoo\u00dfe des Gl\u00fccks lag der verzogene Sohn.", "tokens": ["Weich", "im", "Schoo\u00b7\u00dfe", "des", "Gl\u00fccks", "lag", "der", "ver\u00b7zo\u00b7ge\u00b7ne", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.17": {"text": "Wahrlich, ich habe gelebt! Nicht reut mich die fr\u00f6hliche Wildheit.", "tokens": ["Wahr\u00b7lich", ",", "ich", "ha\u00b7be", "ge\u00b7lebt", "!", "Nicht", "reut", "mich", "die", "fr\u00f6h\u00b7li\u00b7che", "Wild\u00b7heit", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VAFIN", "VVPP", "$.", "PTKNEG", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.18": {"text": "Fest an die feurige Brust dr\u00fcckt' ich das bl\u00fchende Seyn,", "tokens": ["Fest", "an", "die", "feu\u00b7ri\u00b7ge", "Brust", "dr\u00fcckt'", "ich", "das", "bl\u00fc\u00b7hen\u00b7de", "Seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.19": {"text": "K\u00fc\u00dfte die scheidende Lust, und der nahenden lacht' ich entgegen,", "tokens": ["K\u00fc\u00df\u00b7te", "die", "schei\u00b7den\u00b7de", "Lust", ",", "und", "der", "na\u00b7hen\u00b7den", "lacht'", "ich", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "KON", "ART", "ADJA", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.20": {"text": "Und zur geliebtesten Braut ward die Minute mir stets.", "tokens": ["Und", "zur", "ge\u00b7lieb\u00b7tes\u00b7ten", "Braut", "ward", "die", "Mi\u00b7nu\u00b7te", "mir", "stets", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "VAFIN", "ART", "NN", "PPER", "ADV", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "Was ich am Morgen geliebt, das verlie\u00df ich am d\u00e4mmernden Abend,", "tokens": ["Was", "ich", "am", "Mor\u00b7gen", "ge\u00b7liebt", ",", "das", "ver\u00b7lie\u00df", "ich", "am", "d\u00e4m\u00b7mern\u00b7den", "A\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "VVPP", "$,", "PDS", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Treulos wurde, was mich liebte, schon fr\u00fcher mir oft.", "tokens": ["Treu\u00b7los", "wur\u00b7de", ",", "was", "mich", "lieb\u00b7te", ",", "schon", "fr\u00fc\u00b7her", "mir", "oft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,", "PWS", "PPER", "VVFIN", "$,", "ADV", "ADJD", "PPER", "ADV", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.23": {"text": "Aber begegneten einst sich auf wechselndem Pfad die Getrennten,", "tokens": ["A\u00b7ber", "be\u00b7ge\u00b7gne\u00b7ten", "einst", "sich", "auf", "wech\u00b7seln\u00b7dem", "Pfad", "die", "Ge\u00b7trenn\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PRF", "APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+---+-+-+---+--+-", "measure": "trochaic.hexa.relaxed"}, "line.24": {"text": "Schmerzlos reichten sie dann freundlich einander die Hand.", "tokens": ["Schmerz\u00b7los", "reich\u00b7ten", "sie", "dann", "freund\u00b7lich", "ein\u00b7an\u00b7der", "die", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "ADJD", "PRF", "ART", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.25": {"text": "Jeglicher w\u00e4hnte, so lange der Traum ihn umgaukelte, treu sich,", "tokens": ["Jeg\u00b7li\u00b7cher", "w\u00e4hn\u00b7te", ",", "so", "lan\u00b7ge", "der", "Traum", "ihn", "um\u00b7gau\u00b7kel\u00b7te", ",", "treu", "sich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "ADV", "ADV", "ART", "NN", "PPER", "VVFIN", "$,", "ADJD", "PRF", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.26": {"text": "Jeglicher fragte: was frommt Treue, wenn Liebe verschwand?", "tokens": ["Jeg\u00b7li\u00b7cher", "frag\u00b7te", ":", "was", "frommt", "Treu\u00b7e", ",", "wenn", "Lie\u00b7be", "ver\u00b7schwand", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$.", "PWS", "VVFIN", "NN", "$,", "KOUS", "NN", "VVFIN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.27": {"text": "Oft auch wandelte schnell sich der Sinn; aus des Gl\u00fcckes Erinnrung,", "tokens": ["Oft", "auch", "wan\u00b7del\u00b7te", "schnell", "sich", "der", "Sinn", ";", "aus", "des", "Gl\u00fc\u00b7ckes", "E\u00b7rinn\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ADJD", "PRF", "ART", "NN", "$.", "APPR", "ART", "NN", "NN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.28": {"text": "S\u00fc\u00df oft t\u00e4uscht sich das Herz, bl\u00fchte von neuem das Gl\u00fcck.", "tokens": ["S\u00fc\u00df", "oft", "t\u00e4uscht", "sich", "das", "Herz", ",", "bl\u00fch\u00b7te", "von", "neu\u00b7em", "das", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PRF", "ART", "NN", "$,", "VVFIN", "APPR", "ADJA", "ART", "NN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.29": {"text": "Doch l\u00e4ngst schwand sie, die fr\u00f6hliche Zeit, und der flatternde Geist strebt", "tokens": ["Doch", "l\u00e4ngst", "schwand", "sie", ",", "die", "fr\u00f6h\u00b7li\u00b7che", "Zeit", ",", "und", "der", "flat\u00b7tern\u00b7de", "Geist", "strebt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "$,", "KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.30": {"text": "Fruchtlos unter der Pflicht eisernen Banden empor.", "tokens": ["Frucht\u00b7los", "un\u00b7ter", "der", "Pflicht", "ei\u00b7ser\u00b7nen", "Ban\u00b7den", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.31": {"text": "Was mir mit Blumen die Fesseln umwand, hin welket es h\u00fclflos;", "tokens": ["Was", "mir", "mit", "Blu\u00b7men", "die", "Fes\u00b7seln", "um\u00b7wand", ",", "hin", "wel\u00b7ket", "es", "h\u00fcl\u00b7flos", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+--+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.32": {"text": "Ach, um die Sch\u00f6nheit h\u00fcllt finstre Gew\u00f6lke der Tod!", "tokens": ["Ach", ",", "um", "die", "Sch\u00f6n\u00b7heit", "h\u00fcllt", "finst\u00b7re", "Ge\u00b7w\u00f6l\u00b7ke", "der", "Tod", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUI", "ART", "NN", "VVFIN", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.33": {"text": "Liebe rief mich herab von dem luftigen Pfad und Verzweiflung", "tokens": ["Lie\u00b7be", "rief", "mich", "her\u00b7ab", "von", "dem", "luf\u00b7ti\u00b7gen", "Pfad", "und", "Ver\u00b7zwei\u00b7flung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "KON", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.34": {"text": "Winkt mich zur\u00fcck: doch dem Geist wurden die Schwingen gek\u00fcrzt.", "tokens": ["Winkt", "mich", "zu\u00b7r\u00fcck", ":", "doch", "dem", "Geist", "wur\u00b7den", "die", "Schwin\u00b7gen", "ge\u00b7k\u00fcrzt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$.", "ADV", "ART", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.35": {"text": "Dankbar weiht' ich sie einst den errettenden G\u00f6ttern und w\u00e4hnte", "tokens": ["Dank\u00b7bar", "weiht'", "ich", "sie", "einst", "den", "er\u00b7ret\u00b7ten\u00b7den", "G\u00f6t\u00b7tern", "und", "w\u00e4hn\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "ADV", "ART", "ADJA", "NN", "KON", "VVFIN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.36": {"text": "Nicht, da\u00df vom friedlichen Heerd wieder mich risse der Sturm.", "tokens": ["Nicht", ",", "da\u00df", "vom", "fried\u00b7li\u00b7chen", "Heerd", "wie\u00b7der", "mich", "ris\u00b7se", "der", "Sturm", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "KOUS", "APPRART", "ADJA", "NN", "ADV", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.37": {"text": "Doch oft greift mit der eisernen Hand in die Harfe des Lebens,", "tokens": ["Doch", "oft", "greift", "mit", "der", "ei\u00b7ser\u00b7nen", "Hand", "in", "die", "Har\u00b7fe", "des", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-++--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.38": {"text": "Wenn sie am zartesten klang, gellend das d\u00fcstre Geschick.", "tokens": ["Wenn", "sie", "am", "zar\u00b7tes\u00b7ten", "klang", ",", "gel\u00b7lend", "das", "d\u00fcst\u00b7re", "Ge\u00b7schick", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "VVFIN", "$,", "VVPP", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.39": {"text": "Ruhend schauen die G\u00f6tter hinab, und der Kampf mit dem Schicksal", "tokens": ["Ru\u00b7hend", "schau\u00b7en", "die", "G\u00f6t\u00b7ter", "hin\u00b7ab", ",", "und", "der", "Kampf", "mit", "dem", "Schick\u00b7sal"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PTKVZ", "$,", "KON", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+--+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.40": {"text": "Reizt, wie ein tragische Spiel wechselnd, die m\u00fcssige Schaar.", "tokens": ["Reizt", ",", "wie", "ein", "tra\u00b7gi\u00b7sche", "Spiel", "wech\u00b7selnd", ",", "die", "m\u00fcs\u00b7si\u00b7ge", "Schaar", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], 