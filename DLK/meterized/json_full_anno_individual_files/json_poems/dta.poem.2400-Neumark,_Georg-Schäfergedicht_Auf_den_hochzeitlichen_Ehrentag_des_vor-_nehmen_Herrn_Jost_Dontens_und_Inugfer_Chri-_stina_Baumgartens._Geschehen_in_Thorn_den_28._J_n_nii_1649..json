{"dta.poem.2400": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Sch\u00e4fergedicht  \n Auf den hochzeitlichen Ehrentag des vor-  \n  nehmen Herrn Jost Dontens/ und Inugfer Chri-  \n stina Baumgartens.  Geschehen in Thorn  \n den 28. J n nii/ 1649.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Ich h\u00e4tte nimmermehr bey mir gedenkken k\u00f6nnen/", "tokens": ["Ich", "h\u00e4t\u00b7te", "nim\u00b7mer\u00b7mehr", "bey", "mir", "ge\u00b7denk\u00b7ken", "k\u00f6n\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PPER", "VVINF", "VMINF", "$("], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Was die getreue Lieb\u2019 in manches Menschen Sinnen", "tokens": ["Was", "die", "ge\u00b7treu\u00b7e", "Lieb'", "in", "man\u00b7ches", "Men\u00b7schen", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "ADJA", "NN", "APPR", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vor Schmertzen bringt zuweg\u2019 und was sie stifften kan", "tokens": ["Vor", "Schmert\u00b7zen", "bringt", "zu\u00b7weg'", "und", "was", "sie", "stiff\u00b7ten", "kan"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PTKVZ", "KON", "PWS", "PPER", "VVFIN", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Jm fall ichs nicht gesehn am Sch\u00e4fer Kloriman.", "tokens": ["Jm", "fall", "ichs", "nicht", "ge\u00b7sehn", "am", "Sch\u00e4\u00b7fer", "Klo\u00b7ri\u00b7man", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIS", "PTKNEG", "VVPP", "APPRART", "NE", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es jammert seiner mich. Ich wils mit wenig W\u00f6rtern", "tokens": ["Es", "jam\u00b7mert", "sei\u00b7ner", "mich", ".", "Ich", "wils", "mit", "we\u00b7nig", "W\u00f6r\u00b7tern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "PPER", "$.", "PPER", "VMFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Auffs k\u00fcrtzste wie ich kan/ dir teutsches Blut er\u00f6rtern/", "tokens": ["Auffs", "k\u00fcrtz\u00b7ste", "wie", "ich", "kan", "/", "dir", "teut\u00b7sches", "Blut", "er\u00b7\u00f6r\u00b7tern", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "KOKOM", "PPER", "VMFIN", "$(", "PPER", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Nur thu mir diesen Dienst/ du redlichs Hertze du/", "tokens": ["Nur", "thu", "mir", "die\u00b7sen", "Dienst", "/", "du", "red\u00b7lichs", "Hert\u00b7ze", "du", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDAT", "NN", "$(", "PPER", "VVFIN", "VVFIN", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Verzeih der schlechten Red\u2019 und h\u00f6re g\u00fcnstig zu:", "tokens": ["Ver\u00b7zeih", "der", "schlech\u00b7ten", "Red'", "und", "h\u00f6\u00b7re", "g\u00fcns\u00b7tig", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "KON", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Umb eben jene Zeit/ wenn F\u00f6bus mit den Pferden", "tokens": ["Umb", "e\u00b7ben", "je\u00b7ne", "Zeit", "/", "wenn", "F\u00f6\u00b7bus", "mit", "den", "Pfer\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ADV", "PDAT", "NN", "$(", "KOUS", "NE", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Den Morgen f\u00fchret ein der Mexikanschen Erden", "tokens": ["Den", "Mor\u00b7gen", "f\u00fch\u00b7ret", "ein", "der", "Me\u00b7xi\u00b7kan\u00b7schen", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und bey uns Abend macht/ gieng ich am Weisselflu\u00df", "tokens": ["Und", "bey", "uns", "A\u00b7bend", "macht", "/", "gieng", "ich", "am", "Weis\u00b7sel\u00b7flu\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "NN", "VVFIN", "$(", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Umb was zu linderen den schwehren Uberdru\u00df/", "tokens": ["Umb", "was", "zu", "lin\u00b7de\u00b7ren", "den", "schweh\u00b7ren", "U\u00b7berd\u00b7ru\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "APPR", "ADJA", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Spatziren auf und ab; in dem ich nun besinne", "tokens": ["Spat\u00b7zi\u00b7ren", "auf", "und", "ab", ";", "in", "dem", "ich", "nun", "be\u00b7sin\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "KON", "PTKVZ", "$.", "APPR", "PRELS", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Bey mir bald die\u00df bald das/ werd\u2019 ich von weitem inne/", "tokens": ["Bey", "mir", "bald", "die\u00df", "bald", "das", "/", "werd'", "ich", "von", "wei\u00b7tem", "in\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "PDS", "ADV", "ART", "$(", "VAFIN", "PPER", "APPR", "PIS", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Des Sch\u00e4fer Klorimans/ der sonst mit freyem Muht", "tokens": ["Des", "Sch\u00e4\u00b7fer", "Klo\u00b7ri\u00b7mans", "/", "der", "sonst", "mit", "frey\u00b7em", "Muht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "NE", "$(", "ART", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und edler Tapfferkeit verlachte diese Gluht/", "tokens": ["Und", "ed\u00b7ler", "Tapf\u00b7fer\u00b7keit", "ver\u00b7lach\u00b7te", "die\u00b7se", "Gluht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die Amor fechelt auf; der/ sag ich/ gieng und nagte/", "tokens": ["Die", "A\u00b7mor", "fe\u00b7chelt", "auf", ";", "der", "/", "sag", "ich", "/", "gieng", "und", "nag\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "PTKVZ", "$.", "ART", "$(", "VVIMP", "PPER", "$(", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Am Daumen wie verwirrt/ als einer welchen plagte", "tokens": ["Am", "Dau\u00b7men", "wie", "ver\u00b7wirrt", "/", "als", "ei\u00b7ner", "wel\u00b7chen", "plag\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KOKOM", "ADJD", "$(", "KOUS", "ART", "PWAT", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So manche Hertzensangst; Er war schon wie halb tod;", "tokens": ["So", "man\u00b7che", "Hert\u00b7zen\u00b7sangst", ";", "Er", "war", "schon", "wie", "halb", "tod", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$.", "PPER", "VAFIN", "ADV", "KOKOM", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Er irrte hin und her; Es trieb\u2019 ihm seine Noht", "tokens": ["Er", "irr\u00b7te", "hin", "und", "her", ";", "Es", "trieb'", "ihm", "sei\u00b7ne", "Noht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$.", "PPER", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Viel tausend Seufftzer au\u00df; Ein rohtvermischt Erblassen", "tokens": ["Viel", "tau\u00b7send", "Seufft\u00b7zer", "au\u00df", ";", "Ein", "roht\u00b7ver\u00b7mischt", "Er\u00b7blas\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "CARD", "NN", "PTKVZ", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Scho\u00df ihm au\u00df dem Gesicht\u2019; Er wuste nicht zu fassen/", "tokens": ["Scho\u00df", "ihm", "au\u00df", "dem", "Ge\u00b7sicht'", ";", "Er", "wus\u00b7te", "nicht", "zu", "fas\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$.", "PPER", "VVFIN", "PTKNEG", "PTKZU", "VVINF", "$("], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.23": {"text": "Was einen tr\u00f6sten mag; Sein bittres Hertzeleid", "tokens": ["Was", "ei\u00b7nen", "tr\u00f6s\u00b7ten", "mag", ";", "Sein", "bitt\u00b7res", "Hert\u00b7ze\u00b7leid"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "PIS", "VVINF", "VMFIN", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "War Ursach da\u00df sein Vieh lieff hie und da zerstreut.", "tokens": ["War", "Ur\u00b7sach", "da\u00df", "sein", "Vieh", "lieff", "hie", "und", "da", "zer\u00b7streut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KOUS", "PPOSAT", "NN", "VVFIN", "ADV", "KON", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ich sah\u2019 ihm welter zu/ es that mir weh im Hertzen/", "tokens": ["Ich", "sah'", "ihm", "wel\u00b7ter", "zu", "/", "es", "that", "mir", "weh", "im", "Hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKZU", "$(", "PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Da\u00df dieser arme H\u00fcrt\u2019 erlitte solche Schmertzen.", "tokens": ["Da\u00df", "die\u00b7ser", "ar\u00b7me", "H\u00fcrt'", "er\u00b7lit\u00b7te", "sol\u00b7che", "Schmert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ADJA", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Der m\u00fcste ja ein Klotz und nicht ein Mensche seyn", "tokens": ["Der", "m\u00fcs\u00b7te", "ja", "ein", "Klotz", "und", "nicht", "ein", "Men\u00b7sche", "seyn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "ADV", "ART", "NN", "KON", "PTKNEG", "ART", "NN", "VAINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Der kein Mikleiden tr\u00fcg\u2019 ob eines andern Pein.", "tokens": ["Der", "kein", "Mik\u00b7lei\u00b7den", "tr\u00fcg'", "ob", "ei\u00b7nes", "an\u00b7dern", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Drauf gieng er nach dem Pusch\u2019 und lie\u00df die matte\u0303 Glieder", "tokens": ["Drauf", "gieng", "er", "nach", "dem", "Pusch'", "und", "lie\u00df", "die", "mat\u00b7t\u1ebd", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "ART", "NN", "KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Da unter einem Baum\u2019 in dikkem Grase nieder;", "tokens": ["Da", "un\u00b7ter", "ei\u00b7nem", "Baum'", "in", "dik\u00b7kem", "Gra\u00b7se", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "St\u00fctzt auf das Knie die Hand/ legt in die Hand den", "tokens": ["St\u00fctzt", "auf", "das", "Knie", "die", "Hand", "/", "legt", "in", "die", "Hand", "den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$(", "VVFIN", "APPR", "ART", "NN", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Kopff/", "tokens": ["Kopff", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.33": {"text": "Und griltte mancherley der ungl\u00fckselge Tropff.", "tokens": ["Und", "grilt\u00b7te", "man\u00b7cher\u00b7ley", "der", "un\u00b7gl\u00fck\u00b7sel\u00b7ge", "Tropff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Er nam sein Seitenspiel so er stets bey sich f\u00fchrte/", "tokens": ["Er", "nam", "sein", "Sei\u00b7ten\u00b7spiel", "so", "er", "stets", "bey", "sich", "f\u00fchr\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "PPER", "ADV", "APPR", "PRF", "VVFIN", "$("], "meter": "-+-+-+--++-+-", "measure": "iambic.hexa.relaxed"}, "line.35": {"text": "Fieng es zu stimmen an und solches sanffte r\u00fchrte/", "tokens": ["Fi\u00b7eng", "es", "zu", "stim\u00b7men", "an", "und", "sol\u00b7ches", "sanff\u00b7te", "r\u00fchr\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKZU", "VVFIN", "PTKVZ", "KON", "PIAT", "ADJA", "VVFIN", "$("], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.36": {"text": "Vermischte seine Stimm\u2019 in seiner Seiten Klang", "tokens": ["Ver\u00b7mischte", "sei\u00b7ne", "Stimm'", "in", "sei\u00b7ner", "Sei\u00b7ten", "Klang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.37": {"text": "Mit wiederhohlten Ach/ und dieses Liedchen sang.", "tokens": ["Mit", "wie\u00b7der\u00b7hohl\u00b7ten", "Ach", "/", "und", "die\u00b7ses", "Lied\u00b7chen", "sang", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$(", "KON", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Die\u00df waren Wort und Weis\u2019: ", "tokens": ["Die\u00df", "wa\u00b7ren", "Wort", "und", "Weis'", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}