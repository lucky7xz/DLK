{"dta.poem.9095": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1898", "urn": "urn:nbn:de:kobv:b4-200905192685", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich liege zwischen dunklen Spiegelw\u00e4nden.", "tokens": ["Ich", "lie\u00b7ge", "zwi\u00b7schen", "dunk\u00b7len", "Spie\u00b7gel\u00b7w\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Gr\u00fcne, glimmende Seesterne,", "tokens": ["Gr\u00fc\u00b7ne", ",", "glim\u00b7men\u00b7de", "Sees\u00b7ter\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Augen, die glotzen,", "tokens": ["Au\u00b7gen", ",", "die", "glot\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "ein riesiger Rochen reisst sein Maul auf.", "tokens": ["ein", "rie\u00b7si\u00b7ger", "Ro\u00b7chen", "reisst", "sein", "Maul", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Ein Druck, und sie leuchten!", "tokens": ["Ein", "Druck", ",", "und", "sie", "leuch\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "PPER", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Durch einen roten Korallenwald segelt ein silberner Mondfisch!", "tokens": ["Durch", "ei\u00b7nen", "ro\u00b7ten", "Ko\u00b7ral\u00b7len\u00b7wald", "se\u00b7gelt", "ein", "sil\u00b7ber\u00b7ner", "Mond\u00b7fisch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-+--+-", "measure": "iambic.septa.relaxed"}}, "stanza.5": {"line.1": {"text": "Ich liege und rauche aus meiner Wasserpfeife.", "tokens": ["Ich", "lie\u00b7ge", "und", "rau\u00b7che", "aus", "mei\u00b7ner", "Was\u00b7ser\u00b7pfei\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}}}}