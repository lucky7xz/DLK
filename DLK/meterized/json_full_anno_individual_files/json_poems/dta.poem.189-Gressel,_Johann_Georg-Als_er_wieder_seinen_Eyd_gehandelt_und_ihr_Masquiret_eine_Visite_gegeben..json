{"dta.poem.189": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als er wieder seinen Eyd gehandelt und ihr  \n  Masquiret  eine  Visite  gegeben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Licht! ach d\u00fcrffte ich sie/ Sch\u00f6nste/ also nennen/", "tokens": ["Mein", "Licht", "!", "ach", "d\u00fcrff\u00b7te", "ich", "sie", "/", "Sch\u00f6ns\u00b7te", "/", "al\u00b7so", "nen\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "XY", "VMFIN", "PPER", "PPER", "$(", "NN", "$(", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und h\u00f6rte sie von mir den holden Nahmen an/", "tokens": ["Und", "h\u00f6r\u00b7te", "sie", "von", "mir", "den", "hol\u00b7den", "Nah\u00b7men", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So h\u00e4tte meine Pflicht nichts Widriges gethan/", "tokens": ["So", "h\u00e4t\u00b7te", "mei\u00b7ne", "Pflicht", "nichts", "Wid\u00b7ri\u00b7ges", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und ihr entbraunter Zorn k\u00f6nnt nicht so hefftig brennen/", "tokens": ["Und", "ihr", "ent\u00b7braun\u00b7ter", "Zorn", "k\u00f6nnt", "nicht", "so", "heff\u00b7tig", "bren\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "PTKNEG", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ja m\u00f6chte sie sich selbst und ihre Anmuth kennen/", "tokens": ["Ja", "m\u00f6ch\u00b7te", "sie", "sich", "selbst", "und", "ih\u00b7re", "An\u00b7muth", "ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VMFIN", "PPER", "PRF", "ADV", "KON", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und was ihr Reitz-", "tokens": ["Und", "was", "ihr", "Reitz"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWS", "PPER", "TRUNC"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "So schw\u00fcnde gleich ihr Ha\u00df/ und der gefa\u00dfte Wahn", "tokens": ["So", "schw\u00fcn\u00b7de", "gleich", "ihr", "Ha\u00df", "/", "und", "der", "ge\u00b7fa\u00df\u00b7te", "Wahn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$(", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Als wolte ich das Band des festen Eydes trennen.", "tokens": ["Als", "wol\u00b7te", "ich", "das", "Band", "des", "fes\u00b7ten", "Ey\u00b7des", "tren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Doch was besch\u00f6n ich mich! der Bruch der ist begangen/", "tokens": ["Doch", "was", "be\u00b7sch\u00f6n", "ich", "mich", "!", "der", "Bruch", "der", "ist", "be\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADJD", "PPER", "PRF", "$.", "ART", "NN", "ART", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mein Fehler/ der verdammt/ und richtet mich zugleich/", "tokens": ["Mein", "Feh\u00b7ler", "/", "der", "ver\u00b7dammt", "/", "und", "rich\u00b7tet", "mich", "zu\u00b7gleich", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "ART", "VVPP", "$(", "KON", "VVFIN", "PPER", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ich mu\u00df des Eydbruchs-straf den Rechten nach empfangen/", "tokens": ["Ich", "mu\u00df", "des", "Eyd\u00b7bruchs\u00b7straf", "den", "Rech\u00b7ten", "nach", "emp\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ART", "NN", "APPR", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Gerechte Richters macht kein Flehn bey Unrecht weich.", "tokens": ["Ge\u00b7rech\u00b7te", "Rich\u00b7ters", "macht", "kein", "Flehn", "bey", "Un\u00b7recht", "weich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PIAT", "NN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Allein! dis bitt ich aus/ soll ohne Gnad ich sterben/", "tokens": ["Al\u00b7lein", "!", "dis", "bitt", "ich", "aus", "/", "soll", "oh\u00b7ne", "Gnad", "ich", "ster\u00b7ben", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PDS", "VVFIN", "PPER", "PTKVZ", "$(", "VMFIN", "APPR", "NN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da\u00df ich in ihrer Schoo\u00df/ mein Engel/ mag verderben.", "tokens": ["Da\u00df", "ich", "in", "ih\u00b7rer", "Schoo\u00df", "/", "mein", "En\u00b7gel", "/", "mag", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "$(", "VMFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}