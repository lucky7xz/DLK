, "pos": ["VVFIN", "KON", "VVFIN", "KON", "VVFIN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Als das junge Jahr erlau'te", "tokens": ["Als", "das", "jun\u00b7ge", "Jahr", "er\u00b7lau'\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "O des Jammers! sank der Traute", "tokens": ["O", "des", "Jam\u00b7mers", "!", "sank", "der", "Trau\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ART", "NN", "$.", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Von der Mutter warmen Busen", "tokens": ["Von", "der", "Mut\u00b7ter", "war\u00b7men", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In des Grabes schaudernd K\u00fchl.", "tokens": ["In", "des", "Gra\u00b7bes", "schau\u00b7dernd", "K\u00fchl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Tochter, w\u00e4hne nicht, auf immer", "tokens": ["Toch\u00b7ter", ",", "w\u00e4h\u00b7ne", "nicht", ",", "auf", "im\u00b7mer"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PTKNEG", "$,", "APPR", "ADV"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Werde dich der Arm beschirmen,", "tokens": ["Wer\u00b7de", "dich", "der", "Arm", "be\u00b7schir\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Welcher in des Lebens St\u00fcrmen", "tokens": ["Wel\u00b7cher", "in", "des", "Le\u00b7bens", "St\u00fcr\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAT", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Itzt noch deine Schw\u00e4che st\u00fctzt.", "tokens": ["Itzt", "noch", "dei\u00b7ne", "Schw\u00e4\u00b7che", "st\u00fctzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Einsam durch die Wildniss wanken,", "tokens": ["Ein\u00b7sam", "durch", "die", "Wild\u00b7niss", "wan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Stablos wirst du niederschwanken,", "tokens": ["Stab\u00b7los", "wirst", "du", "nie\u00b7der\u00b7schwan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn dich nicht der Trost der Unschuld,", "tokens": ["Wenn", "dich", "nicht", "der", "Trost", "der", "Un\u00b7schuld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und der Unschuld Retter st\u00fctzt.", "tokens": ["Und", "der", "Un\u00b7schuld", "Ret\u00b7ter", "st\u00fctzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Drum beschw\u00f6r' ich bey dem Frieden", "tokens": ["Drum", "be\u00b7schw\u00f6r'", "ich", "bey", "dem", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deiner Zukunft, bey dem Herzen,", "tokens": ["Dei\u00b7ner", "Zu\u00b7kunft", ",", "bey", "dem", "Her\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Draus du sprosstest, bey den Schmerzen", "tokens": ["Draus", "du", "spross\u00b7test", ",", "bey", "den", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "PPER", "VVFIN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jener welche dich gebar;", "tokens": ["Je\u00b7ner", "wel\u00b7che", "dich", "ge\u00b7bar", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "PRELS", "PRF", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich beschw\u00f6re dich und bitte:", "tokens": ["Ich", "be\u00b7schw\u00f6\u00b7re", "dich", "und", "bit\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "PTKANT", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bleib getreu der sch\u00f6nern Sitte!", "tokens": ["Bleib", "ge\u00b7treu", "der", "sch\u00f6\u00b7nern", "Sit\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "O mein Erstling, o mein Liebling,", "tokens": ["O", "mein", "Erst\u00b7ling", ",", "o", "mein", "Lieb\u00b7ling", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,", "FM", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bleibe schuldlos, gut und wahr!", "tokens": ["Blei\u00b7be", "schuld\u00b7los", ",", "gut", "und", "wahr", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}