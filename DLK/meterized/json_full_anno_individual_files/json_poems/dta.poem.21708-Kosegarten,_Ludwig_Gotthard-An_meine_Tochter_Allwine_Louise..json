{"dta.poem.21708": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "An meine Tochter  \n Allwine Louise.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Erstlingstochter heil'ger Liebe,", "tokens": ["Erst\u00b7lings\u00b7toch\u00b7ter", "heil'\u00b7ger", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine Hoffnung, meine Freude,", "tokens": ["Mei\u00b7ne", "Hoff\u00b7nung", ",", "mei\u00b7ne", "Freu\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Meiner Augen liebste Weide,", "tokens": ["Mei\u00b7ner", "Au\u00b7gen", "liebs\u00b7te", "Wei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mein Juwel, mein k\u00f6stlichst Gut;", "tokens": ["Mein", "Ju\u00b7wel", ",", "mein", "k\u00f6st\u00b7lichst", "Gut", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "VVFIN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Dich beschw\u00f6r' ich bey dem Herzen,", "tokens": ["Dich", "be\u00b7schw\u00f6r'", "ich", "bey", "dem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Draus du sprosstest, bey den Schmerzen", "tokens": ["Draus", "du", "spross\u00b7test", ",", "bey", "den", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "PPER", "VVFIN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Jener, die dich trug und tr\u00e4nkte,", "tokens": ["Je\u00b7ner", ",", "die", "dich", "trug", "und", "tr\u00e4nk\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "$,", "PRELS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bleibe schuldlos, bleibe gut!", "tokens": ["Blei\u00b7be", "schuld\u00b7los", ",", "blei\u00b7be", "gut", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Holde Tochter, noch besch\u00e4met", "tokens": ["Hol\u00b7de", "Toch\u00b7ter", ",", "noch", "be\u00b7sch\u00e4\u00b7met"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deines Auges Glanz und Helle", "tokens": ["Dei\u00b7nes", "Au\u00b7ges", "Glanz", "und", "Hel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Den Krystall der Gletscherquelle,", "tokens": ["Den", "Krys\u00b7tall", "der", "Glet\u00b7scher\u00b7quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Noch Golkondens sch\u00f6nsten Stein.", "tokens": ["Noch", "Gol\u00b7kon\u00b7dens", "sch\u00f6ns\u00b7ten", "Stein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "M\u00f6gst du nie im Hauch der S\u00fcnden,", "tokens": ["M\u00f6gst", "du", "nie", "im", "Hauch", "der", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Funkelnder Brilliant, erblinden!", "tokens": ["Fun\u00b7keln\u00b7der", "Bril\u00b7li\u00b7ant", ",", "er\u00b7blin\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "M\u00f6gst du ewig lautrer Spiegel", "tokens": ["M\u00f6gst", "du", "e\u00b7wig", "laut\u00b7rer", "Spie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Einer lautern Seele seyn!", "tokens": ["Ei\u00b7ner", "lau\u00b7tern", "See\u00b7le", "seyn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie um Blumen Bienen gaukeln,", "tokens": ["Wie", "um", "Blu\u00b7men", "Bie\u00b7nen", "gau\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie durch Bl\u00fcthen Weste streifen;", "tokens": ["Wie", "durch", "Bl\u00fc\u00b7then", "Wes\u00b7te", "strei\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Also schweif mit leichtem Schweifen", "tokens": ["Al\u00b7so", "schweif", "mit", "leich\u00b7tem", "Schwei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch das Leben froh dein Fuss.", "tokens": ["Durch", "das", "Le\u00b7ben", "froh", "dein", "Fuss", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nie befl\u00fcgle dieser Tritte", "tokens": ["Nie", "be\u00b7fl\u00fcg\u00b7le", "die\u00b7ser", "Trit\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Reinen Rythmus freche Sitte.", "tokens": ["Rei\u00b7nen", "Ry\u00b7th\u00b7mus", "fre\u00b7che", "Sit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ADJA", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Nimmer l\u00e4hm' ihn tr\u00e4ge Sorge,", "tokens": ["Nim\u00b7mer", "l\u00e4hm'", "ihn", "tr\u00e4\u00b7ge", "Sor\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nie der bleyerne Verdruss!", "tokens": ["Nie", "der", "bley\u00b7er\u00b7ne", "Ver\u00b7druss", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Welches Gl\u00f6ckleins hellem Klingeln,", "tokens": ["Wel\u00b7ches", "Gl\u00f6c\u00b7kleins", "hel\u00b7lem", "Klin\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Welcher Fl\u00f6te, welcher Laute", "tokens": ["Wel\u00b7cher", "Fl\u00f6\u00b7te", ",", "wel\u00b7cher", "Lau\u00b7te"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PWAT", "NN", "$,", "PWAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Klarem Klang' vergleich ich, Traute,", "tokens": ["Kla\u00b7rem", "Klang'", "ver\u00b7gleich", "ich", ",", "Trau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "PPER", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deiner Stimme Silberschall.", "tokens": ["Dei\u00b7ner", "Stim\u00b7me", "Sil\u00b7ber\u00b7schall", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nie verf\u00e4lsche dumpfes Grollen,", "tokens": ["Nie", "ver\u00b7f\u00e4l\u00b7sche", "dum\u00b7pfes", "Grol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Finstres Z\u00fcrnen, d\u00fcstres Schmollen,", "tokens": ["Finst\u00b7res", "Z\u00fcr\u00b7nen", ",", "d\u00fcst\u00b7res", "Schmol\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Feiges Wimmern dieses Gl\u00f6ckleins", "tokens": ["Fei\u00b7ges", "Wim\u00b7mern", "die\u00b7ses", "Gl\u00f6c\u00b7kleins"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Silberhaltiges Metall.", "tokens": ["Sil\u00b7ber\u00b7hal\u00b7ti\u00b7ges", "Me\u00b7tall", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Holde Tochter, spross und schosse,", "tokens": ["Hol\u00b7de", "Toch\u00b7ter", ",", "spross", "und", "schos\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fr\u00f6hlich, wie die Bins' am Teiche,", "tokens": ["Fr\u00f6h\u00b7lich", ",", "wie", "die", "Bins'", "am", "Tei\u00b7che", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie die Feldros' im Gestr\u00e4uche,", "tokens": ["Wie", "die", "Feldros'", "im", "Ge\u00b7str\u00e4u\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Wie der Waizenhalm im May.", "tokens": ["Wie", "der", "Wai\u00b7zen\u00b7halm", "im", "May", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber rastlos sey dein Sorgen", "tokens": ["A\u00b7ber", "rast\u00b7los", "sey", "dein", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sp\u00e4t am Abend, fr\u00fch am Morgen,", "tokens": ["Sp\u00e4t", "am", "A\u00b7bend", ",", "fr\u00fch", "am", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Dass der Leib nur sch\u00f6ne Fassung", "tokens": ["Dass", "der", "Leib", "nur", "sch\u00f6\u00b7ne", "Fas\u00b7sung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Einer sch\u00f6nern Seele sey.", "tokens": ["Ei\u00b7ner", "sch\u00f6\u00b7nern", "See\u00b7le", "sey", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Nie von hohlem Schein geblendet,", "tokens": ["Nie", "von", "hoh\u00b7lem", "Schein", "ge\u00b7blen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch vom Netz des Trug's umwoben,", "tokens": ["Noch", "vom", "Netz", "des", "Trug's", "um\u00b7wo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Noch von falschem Wahn verschroben,", "tokens": ["Noch", "von", "fal\u00b7schem", "Wahn", "ver\u00b7schro\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bleibe frommer Einfalt treu.", "tokens": ["Blei\u00b7be", "from\u00b7mer", "Ein\u00b7falt", "treu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Feindinn jedes Rollenspieles,", "tokens": ["Fein\u00b7dinn", "je\u00b7des", "Rol\u00b7len\u00b7spie\u00b7les", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Jedes l\u00fcgenden Gef\u00fchles,", "tokens": ["Je\u00b7des", "l\u00fc\u00b7gen\u00b7den", "Ge\u00b7f\u00fch\u00b7les", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie der \u00c4ther klar und offen,", "tokens": ["Wie", "der", "\u00c4\u00b7ther", "klar", "und", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie der Lichtstrahl frank und frey.", "tokens": ["Wie", "der", "Licht\u00b7strahl", "frank", "und", "frey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "KON", "ADJD", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.7": {"line.1": {"text": "H\u00f6re, Tochter, was ich bitte:", "tokens": ["H\u00f6\u00b7re", ",", "Toch\u00b7ter", ",", "was", "ich", "bit\u00b7te", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "PWS", "PPER", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wahr' in kindlichem Gem\u00fcthe", "tokens": ["Wahr'", "in", "kind\u00b7li\u00b7chem", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lebensl\u00e4nglich deine G\u00fcte,", "tokens": ["Le\u00b7bens\u00b7l\u00e4ng\u00b7lich", "dei\u00b7ne", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine Wahrheit, Zucht und Huld,", "tokens": ["Dei\u00b7ne", "Wahr\u00b7heit", ",", "Zucht", "und", "Huld", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Deine Ehrfurcht f\u00fcr das Sollen,", "tokens": ["Dei\u00b7ne", "Ehr\u00b7furcht", "f\u00fcr", "das", "Sol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Deine Gn\u00fcgsamkeit im Wollen,", "tokens": ["Dei\u00b7ne", "Gn\u00fcg\u00b7sam\u00b7keit", "im", "Wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Deine Innigkeit im Lieben,", "tokens": ["Dei\u00b7ne", "In\u00b7nig\u00b7keit", "im", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Deine schweigende Geduld.", "tokens": ["Dei\u00b7ne", "schwei\u00b7gen\u00b7de", "Ge\u00b7duld", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Um den Taumel lauter Freuden,", "tokens": ["Um", "den", "Tau\u00b7mel", "lau\u00b7ter", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die bet\u00e4uben und erm\u00fcden,", "tokens": ["Die", "be\u00b7t\u00e4u\u00b7ben", "und", "er\u00b7m\u00fc\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Tausche nie den t