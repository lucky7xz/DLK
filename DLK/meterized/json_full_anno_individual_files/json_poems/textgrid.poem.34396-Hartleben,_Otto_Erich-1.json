{"textgrid.poem.34396": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht sank in Schwachheit unserer Sprache Kunst,", "tokens": ["Nicht", "sank", "in", "Schwach\u00b7heit", "un\u00b7se\u00b7rer", "Spra\u00b7che", "Kunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "APPR", "NN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "seitdem verhallt ist fr\u00fcher Heroen Schritt \u2013", "tokens": ["seit\u00b7dem", "ver\u00b7hallt", "ist", "fr\u00fc\u00b7her", "He\u00b7roen", "Schritt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVPP", "VAFIN", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "wir wandeln weiter ihre Bahnen", "tokens": ["wir", "wan\u00b7deln", "wei\u00b7ter", "ih\u00b7re", "Bah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "t\u00f6nenden Fusses \u2013 und schauen lichtw\u00e4rts.", "tokens": ["t\u00f6\u00b7nen\u00b7den", "Fus\u00b7ses", "\u2013", "und", "schau\u00b7en", "licht\u00b7w\u00e4rts", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "KON", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Wir meistern, stolz nicht minder wie jene, noch", "tokens": ["Wir", "meis\u00b7tern", ",", "stolz", "nicht", "min\u00b7der", "wie", "je\u00b7ne", ",", "noch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "pu