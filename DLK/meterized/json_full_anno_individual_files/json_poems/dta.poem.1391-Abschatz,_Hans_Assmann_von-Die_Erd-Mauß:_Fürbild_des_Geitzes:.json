{"dta.poem.1391": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die Erd-Mau\u00df: F\u00fcrbild des Geitzes:", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Ich vergrabe/ was ich habe.", "tokens": ["Ich", "ver\u00b7gra\u00b7be", "/", "was", "ich", "ha\u00b7be", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PWS", "PPER", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Palleam dum polleam.", "tokens": ["Pal\u00b7leam", "dum", "pol\u00b7le\u00b7am", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Es gilt mir gleich/", "tokens": ["Es", "gilt", "mir", "gleich", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Bin ich nur reich/", "tokens": ["Bin", "ich", "nur", "reich", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Ob roth ob bleich.", "tokens": ["Ob", "roth", "ob", "bleich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "KOUS", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}