{"dta.poem.21155": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "An Selinen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was ist der Muse Pflicht an diesem festlichen Tage,", "tokens": ["Was", "ist", "der", "Mu\u00b7se", "Pflicht", "an", "die\u00b7sem", "fest\u00b7li\u00b7chen", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "NN", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der deinen holden Namen f\u00fchrt;", "tokens": ["Der", "dei\u00b7nen", "hol\u00b7den", "Na\u00b7men", "f\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als da\u00df sie ihn f\u00fcr sich in stiller Einsamkeit feyret,", "tokens": ["Als", "da\u00df", "sie", "ihn", "f\u00fcr", "sich", "in", "stil\u00b7ler", "Ein\u00b7sam\u00b7keit", "fey\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "PPER", "APPR", "PRF", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und ihm die Winterblumen weiht.", "tokens": ["Und", "ihm", "die", "Win\u00b7ter\u00b7blu\u00b7men", "weiht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du, Knabe, nimm zur Hand die lockenschaffenden Ei-", "tokens": ["Du", ",", "Kna\u00b7be", ",", "nimm", "zur", "Hand", "die", "lo\u00b7cken\u00b7schaf\u00b7fen\u00b7den", "Ei"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "NN", "$,", "VVIMP", "APPRART", "NN", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Und kr\u00e4usle mir mein braunes Haar!", "tokens": ["Und", "kr\u00e4us\u00b7le", "mir", "mein", "brau\u00b7nes", "Haar", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verschwende deine Kunst in sanfterduftenden Locken", "tokens": ["Ver\u00b7schwen\u00b7de", "dei\u00b7ne", "Kunst", "in", "sanf\u00b7ter\u00b7duf\u00b7ten\u00b7den", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Von Puder und von Rosen\u00f6l!", "tokens": ["Von", "Pu\u00b7der", "und", "von", "Ro\u00b7sen\u00b7\u00f6l", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich will geputzter seyn, als ein besiegender J\u00fcngling,", "tokens": ["Ich", "will", "ge\u00b7putz\u00b7ter", "seyn", ",", "als", "ein", "be\u00b7sie\u00b7gen\u00b7der", "J\u00fcng\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "VAINF", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Auf den sein wei\u00dfes M\u00e4dchen hoft;", "tokens": ["Auf", "den", "sein", "wei\u00b7\u00dfes", "M\u00e4d\u00b7chen", "hoft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Pracht und Jugend schm\u00fcckt, und dem Verlangen", "tokens": ["Den", "Pracht", "und", "Ju\u00b7gend", "schm\u00fcckt", ",", "und", "dem", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die aufgebl\u00fchten Wangen f\u00e4rbt.", "tokens": ["Die", "auf\u00b7ge\u00b7bl\u00fch\u00b7ten", "Wan\u00b7gen", "f\u00e4rbt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der sch\u00f6nste Weihranch soll mein heitres Zimmer durch-", "tokens": ["Der", "sch\u00f6ns\u00b7te", "Wei\u00b7hranch", "soll", "mein", "heit\u00b7res", "Zim\u00b7mer", "durch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPOSAT", "ADJA", "NN", "TRUNC"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da\u00df Gram und schwere D\u00fcnste fliehn.", "tokens": ["Da\u00df", "Gram", "und", "schwe\u00b7re", "D\u00fcns\u00b7te", "fliehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und der geschm\u00fcckte Tisch, mit indischem Thone bede-", "tokens": ["Und", "der", "ge\u00b7schm\u00fcck\u00b7te", "Tisch", ",", "mit", "in\u00b7di\u00b7schem", "Tho\u00b7ne", "be\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Soll unter meinem Spiegel stehn.", "tokens": ["Soll", "un\u00b7ter", "mei\u00b7nem", "Spie\u00b7gel", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Auf dem will ich dies Lied zu einem Opfer dir bringen,", "tokens": ["Auf", "dem", "will", "ich", "dies", "Lied", "zu", "ei\u00b7nem", "Op\u00b7fer", "dir", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VMFIN", "PPER", "PDS", "NN", "APPR", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Nebst einem bunten Blumenstrau\u00df;", "tokens": ["Nebst", "ei\u00b7nem", "bun\u00b7ten", "Blu\u00b7men\u00b7strau\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und f\u00fcr ein besseres Gl\u00fcck schick ich die treuesten W\u00fcn-", "tokens": ["Und", "f\u00fcr", "ein", "bes\u00b7se\u00b7res", "Gl\u00fcck", "schick", "ich", "die", "treu\u00b7es\u00b7ten", "W\u00fcn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "ADJA", "TRUNC"], "meter": "---+--++--+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Zu dem vers\u00f6hneten Olymp.", "tokens": ["Zu", "dem", "ver\u00b7s\u00f6h\u00b7ne\u00b7ten", "O\u00b7lymp", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Auch soll mein Saitenspiel in seinen sanftesten T\u00f6nen", "tokens": ["Auch", "soll", "mein", "Sai\u00b7ten\u00b7spiel", "in", "sei\u00b7nen", "sanf\u00b7tes\u00b7ten", "T\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Zum allzuharten Schicksal flehn.", "tokens": ["Zum", "all\u00b7zu\u00b7har\u00b7ten", "Schick\u00b7sal", "flehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sang eine Leyer doch ein M\u00e4dchen aus dem Gebiete", "tokens": ["Sang", "ei\u00b7ne", "Le\u00b7yer", "doch", "ein", "M\u00e4d\u00b7chen", "aus", "dem", "Ge\u00b7bie\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADV", "ART", "NN", "APPR", "ART", "NN"], "meter": "++-++--+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Des fabelhaften H\u00f6llengotts.", "tokens": ["Des", "fa\u00b7bel\u00b7haf\u00b7ten", "H\u00f6l\u00b7len\u00b7gotts", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Erh\u00f6re meinen Wunsch, o unerbittliches Schicksal,", "tokens": ["Er\u00b7h\u00f6\u00b7re", "mei\u00b7nen", "Wunsch", ",", "o", "un\u00b7er\u00b7bitt\u00b7li\u00b7ches", "Schick\u00b7sal", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "FM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Da dieser Wunsch nicht eitel ist!", "tokens": ["Da", "die\u00b7ser", "Wunsch", "nicht", "ei\u00b7tel", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "PTKNEG", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df mich Selinens Haar mit Wintergr\u00fcne bekr\u00e4nzen,", "tokens": ["La\u00df", "mich", "Se\u00b7li\u00b7nens", "Haar", "mit", "Win\u00b7ter\u00b7gr\u00fc\u00b7ne", "be\u00b7kr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Wenn dieser Tag mir wieder lacht.", "tokens": ["Wenn", "die\u00b7ser", "Tag", "mir", "wie\u00b7der", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}