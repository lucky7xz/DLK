{"textgrid.poem.57322": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Freude! da steht's, ein Geniuswerk; und mir ist doch", "genre": "verse", "period": "N.A.", "pub_year": 1781, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freude! da steht's, ein Geniuswerk; und mir ist doch", "tokens": ["Freu\u00b7de", "!", "da", "steht's", ",", "ein", "Ge\u00b7nius\u00b7werk", ";", "und", "mir", "ist", "doch"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "ADV", "VVFIN", "$,", "ART", "NN", "$.", "KON", "PPER", "VAFIN", "ADV"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Etwas nicht da, ich entbehre! Der Entz\u00fcckung", "tokens": ["Et\u00b7was", "nicht", "da", ",", "ich", "ent\u00b7beh\u00b7re", "!", "Der", "Ent\u00b7z\u00fc\u00b7ckung"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PTKNEG", "ADV", "$,", "PPER", "VVFIN", "$.", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Strahlen, die es auf mich herstr\u00f6met,", "tokens": ["Strah\u00b7len", ",", "die", "es", "auf", "mich", "her\u00b7str\u00f6\u00b7met", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Treffen, wie ist das? nicht ganz;", "tokens": ["Tref\u00b7fen", ",", "wie", "ist", "das", "?", "nicht", "ganz", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VAFIN", "PDS", "$.", "PTKNEG", "ADV", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "H\u00fcllen sich dort, und h\u00fcllen sich da, wie in D\u00e4mrung,", "tokens": ["H\u00fcl\u00b7len", "sich", "dort", ",", "und", "h\u00fcl\u00b7len", "sich", "da", ",", "wie", "in", "D\u00e4m\u00b7rung", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ADV", "$,", "KON", "VVFIN", "PRF", "ADV", "$,", "PWAV", "APPR", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Strahlen nicht ganz in das Herz hin; denn ich w\u00fcnsche!", "tokens": ["Strah\u00b7len", "nicht", "ganz", "in", "das", "Herz", "hin", ";", "denn", "ich", "w\u00fcn\u00b7sche", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "APPR", "ART", "NN", "PTKVZ", "$.", "KON", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Und doch lockt ihm das Haar die Sch\u00f6nheit,", "tokens": ["Und", "doch", "lockt", "ihm", "das", "Haar", "die", "Sch\u00f6n\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Hellt ihm mit L\u00e4cheln den Blick;", "tokens": ["Hellt", "ihm", "mit", "L\u00e4\u00b7cheln", "den", "Blick", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "F\u00fcllt ihm die Stirn die Hoheit mit Ernst, mit dem heitren", "tokens": ["F\u00fcllt", "ihm", "die", "Stirn", "die", "Ho\u00b7heit", "mit", "Ernst", ",", "mit", "dem", "hei\u00b7tren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "APPR", "NE", "$,", "APPR", "ART", "ADJA"], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "In dem Gesicht des Entschlossnen, wenn er That thut,", "tokens": ["In", "dem", "Ge\u00b7sicht", "des", "Ent\u00b7schloss\u00b7nen", ",", "wenn", "er", "That", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Oder thun will. O du der Irre", "tokens": ["O\u00b7der", "thun", "will", ".", "O", "du", "der", "Ir\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "VMFIN", "$.", "NE", "NE", "ART", "NN"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Faden, wo liegst du? Was fehlt?", "tokens": ["Fa\u00b7den", ",", "wo", "liegst", "du", "?", "Was", "fehlt", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VVFIN", "PPER", "$.", "PWS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Stimmet vielleicht der Theile Verein nicht harmonisch?", "tokens": ["Stim\u00b7met", "viel\u00b7leicht", "der", "Thei\u00b7le", "Ver\u00b7ein", "nicht", "har\u00b7mo\u00b7nisch", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN", "PTKNEG", "ADJD", "$."], "meter": "+-+--+--+----", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Dich, Harmonie, der gehorchend, sich zu Mauren", "tokens": ["Dich", ",", "Har\u00b7mo\u00b7nie", ",", "der", "ge\u00b7hor\u00b7chend", ",", "sich", "zu", "Mau\u00b7ren"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "NN", "$,", "PRELS", "VVPP", "$,", "PRF", "APPR", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Felsen w\u00e4lzen! der Baum, zu schatten,", "tokens": ["Fel\u00b7sen", "w\u00e4l\u00b7zen", "!", "der", "Baum", ",", "zu", "schat\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$.", "ART", "NN", "$,", "PTKZU", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Wandelt ins Sonnengefild!", "tokens": ["Wan\u00b7delt", "ins", "Son\u00b7nen\u00b7ge\u00b7fild", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "Zaubert so gar der Meister nicht stets. Hat das Urtheil", "tokens": ["Zau\u00b7bert", "so", "gar", "der", "Meis\u00b7ter", "nicht", "stets", ".", "Hat", "das", "Ur\u00b7theil"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "ART", "NN", "PTKNEG", "ADV", "$.", "VAFIN", "ART", "NN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Etwa den Theil, und das Theilchen nicht mit scharfem", "tokens": ["Et\u00b7wa", "den", "Theil", ",", "und", "das", "Theil\u00b7chen", "nicht", "mit", "schar\u00b7fem"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "KON", "ART", "NN", "PTKNEG", "APPR", "ADJA"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Blick gemessen? bemerkt' es Ausart", "tokens": ["Blick", "ge\u00b7mes\u00b7sen", "?", "be\u00b7merkt'", "es", "Au\u00b7sart"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVPP", "$.", "VVFIN", "PPER", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "In das zu Gross, und zu Klein,", "tokens": ["In", "das", "zu", "Gross", ",", "und", "zu", "Klein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPR", "NN", "$,", "KON", "APPR", "NE", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.6": {"line.1": {"text": "Die nicht? Genau das Mass nicht gedacht; und der Umriss", "tokens": ["Die", "nicht", "?", "Ge\u00b7nau", "das", "Mass", "nicht", "ge\u00b7dacht", ";", "und", "der", "Um\u00b7riss"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PTKNEG", "$.", "NN", "ART", "NN", "PTKNEG", "VVPP", "$.", "KON", "ART", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "R\u00fcndet sich nicht mit der Biegung, der es gl\u00fccket.", "tokens": ["R\u00fcn\u00b7det", "sich", "nicht", "mit", "der", "Bie\u00b7gung", ",", "der", "es", "gl\u00fc\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PTKNEG", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Ohne Messung gelang selbst Venus", "tokens": ["Oh\u00b7ne", "Mes\u00b7sung", "ge\u00b7lang", "selbst", "Ve\u00b7nus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ADV", "NN"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "G\u00fcrtel den Grazien nicht.", "tokens": ["G\u00fcr\u00b7tel", "den", "Gra\u00b7zi\u00b7en", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKNEG", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.7": {"line.1": {"text": "Faden, o da, da windest du dich, von Athene's", "tokens": ["Fa\u00b7den", ",", "o", "da", ",", "da", "win\u00b7dest", "du", "dich", ",", "von", "A\u00b7the\u00b7ne's"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "FM", "ADV", "$,", "ADV", "VVFIN", "PPER", "PRF", "$,", "APPR", "NE"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Finger gedreht zu der Leitung aus der Irre.", "tokens": ["Fin\u00b7ger", "ge\u00b7dreht", "zu", "der", "Lei\u00b7tung", "aus", "der", "Ir\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Massbestimmug! auch du lehrst Felsen", "tokens": ["Mass\u00b7be\u00b7stim\u00b7mug", "!", "auch", "du", "lehrst", "Fel\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "ADV", "PPER", "VVFIN", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Wallen, und Haine, den Strom", "tokens": ["Wal\u00b7len", ",", "und", "Hai\u00b7ne", ",", "den", "Strom"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "KON", "NE", "$,", "ART", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.8": {"line.1": {"text": "S\u00e4umen! Vermiss' im Lied' ich dich oft; so entschl\u00fcpf' ich,", "tokens": ["S\u00e4u\u00b7men", "!", "Ver\u00b7miss'", "im", "Lied'", "ich", "dich", "oft", ";", "so", "ent\u00b7schl\u00fcpf'", "ich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "APPRART", "NN", "PPER", "PRF", "ADV", "$.", "ADV", "VVFIN", "PPER", "$,"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Frey nun, dem Kreis, den sein Zauber um mich herzog:", "tokens": ["Frey", "nun", ",", "dem", "Kreis", ",", "den", "sein", "Zau\u00b7ber", "um", "mich", "her\u00b7zog", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Und der winkt mir vielleicht vergebens", "tokens": ["Und", "der", "winkt", "mir", "viel\u00b7leicht", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "VVFIN", "PPER", "ADV", "ADV"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dann mit dem m\u00e4chtigen Stab.", "tokens": ["Dann", "mit", "dem", "m\u00e4ch\u00b7ti\u00b7gen", "Stab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.9": {"line.1": {"text": "Freude! da steht's, ein Geniuswerk; und mir ist doch", "tokens": ["Freu\u00b7de", "!", "da", "steht's", ",", "ein", "Ge\u00b7nius\u00b7werk", ";", "und", "mir", "ist", "doch"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "ADV", "VVFIN", "$,", "ART", "NN", "$.", "KON", "PPER", "VAFIN", "ADV"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Etwas nicht da, ich entbehre! Der Entz\u00fcckung", "tokens": ["Et\u00b7was", "nicht", "da", ",", "ich", "ent\u00b7beh\u00b7re", "!", "Der", "Ent\u00b7z\u00fc\u00b7ckung"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PTKNEG", "ADV", "$,", "PPER", "VVFIN", "$.", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Strahlen, die es auf mich herstr\u00f6met,", "tokens": ["Strah\u00b7len", ",", "die", "es", "auf", "mich", "her\u00b