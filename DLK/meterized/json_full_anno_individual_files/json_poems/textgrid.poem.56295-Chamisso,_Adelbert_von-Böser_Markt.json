{"textgrid.poem.56295": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "B\u00f6ser Markt", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einer kam vom K\u00f6nigsmahle", "tokens": ["Ei\u00b7ner", "kam", "vom", "K\u00f6\u00b7nigs\u00b7mah\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In den Park sich zu bewegen,", "tokens": ["In", "den", "Park", "sich", "zu", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus dem Busch mit einem Male", "tokens": ["Aus", "dem", "Busch", "mit", "ei\u00b7nem", "Ma\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Trat ein andrer ihm entgegen;", "tokens": ["Trat", "ein", "an\u00b7drer", "ihm", "ent\u00b7ge\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "PPER", "PTKVZ", "$."], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Zwischen Rock und Kamisole", "tokens": ["Zwi\u00b7schen", "Rock", "und", "Ka\u00b7mi\u00b7so\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Griff der schnell, und die Pistole", "tokens": ["Griff", "der", "schnell", ",", "und", "die", "Pis\u00b7to\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "ADJD", "$,", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Setzt er jenem auf die Brust.", "tokens": ["Setzt", "er", "je\u00b7nem", "auf", "die", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbleise, leise! mu\u00df ich bitten;", "tokens": ["\u00bb", "lei\u00b7se", ",", "lei\u00b7se", "!", "mu\u00df", "ich", "bit\u00b7ten", ";"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "ADJD", "$.", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was wir hier f\u00fcr Handel treiben,", "tokens": ["Was", "wir", "hier", "f\u00fcr", "Han\u00b7del", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mag vom unberufnen Dritten", "tokens": ["Mag", "vom", "un\u00b7be\u00b7ruf\u00b7nen", "Drit\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcglich unbelauschet bleiben.", "tokens": ["F\u00fcg\u00b7lich", "un\u00b7be\u00b7lau\u00b7schet", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wollt Ihr Uhren nebst Gehenken", "tokens": ["Wollt", "Ihr", "Uh\u00b7ren", "nebst", "Ge\u00b7hen\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wohl verkaufen? nicht verschenken;", "tokens": ["Wohl", "ver\u00b7kau\u00b7fen", "?", "nicht", "ver\u00b7schen\u00b7ken", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$.", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nehmt drei Batzen Ihr daf\u00fcr?\u00ab \u2013", "tokens": ["Nehmt", "drei", "Bat\u00b7zen", "Ihr", "da\u00b7f\u00fcr", "?", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "CARD", "NN", "PPER", "PAV", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Mit Vergn\u00fcgen!\u00ab \u2013 \u00bbNimmer richtig", "tokens": ["Mit", "Ver\u00b7gn\u00fc\u00b7gen", "!", "\u00ab", "\u2013", "\u00bb", "Nim\u00b7mer", "rich\u00b7tig"], "token_info": ["word", "word", "punct", "punct", "punct", "punct", "word", "word"], "pos": ["APPR", "NN", "$.", "$(", "$(", "$(", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist die Dorfuhr noch gegangen;", "tokens": ["Ist", "die", "Dor\u00b7fuhr", "noch", "ge\u00b7gan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Tut der K\u00fcster auch so wichtig,", "tokens": ["Tut", "der", "K\u00fcs\u00b7ter", "auch", "so", "wich\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wei\u00df er's doch nicht anzufangen;", "tokens": ["Wei\u00df", "er's", "doch", "nicht", "an\u00b7zu\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "PTKNEG", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Jeder wei\u00df in unsern Tagen,", "tokens": ["Je\u00b7der", "wei\u00df", "in", "un\u00b7sern", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Was die Glocke hat geschlagen;", "tokens": ["Was", "die", "Glo\u00b7cke", "hat", "ge\u00b7schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Gottlob! nun erfahr ich's auch.", "tokens": ["Gott\u00b7lob", "!", "nun", "er\u00b7fahr", "ich's", "auch", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VVFIN", "PIS", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sagt mir ferner: k\u00f6nnt Ihr missen,", "tokens": ["Sagt", "mir", "fer\u00b7ner", ":", "k\u00f6nnt", "Ihr", "mis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$.", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was da blinkt an Euren Fingern?", "tokens": ["Was", "da", "blinkt", "an", "Eu\u00b7ren", "Fin\u00b7gern", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine Hausfrau, sollt Ihr wissen,", "tokens": ["Mei\u00b7ne", "Haus\u00b7frau", ",", "sollt", "Ihr", "wis\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist gar arg nach solchen Dingern;", "tokens": ["Ist", "gar", "arg", "nach", "sol\u00b7chen", "Din\u00b7gern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Solche Ringe, solche Sterne,", "tokens": ["Sol\u00b7che", "Rin\u00b7ge", ",", "sol\u00b7che", "Ster\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie Ihr da habt, kauf ich gerne;", "tokens": ["Wie", "Ihr", "da", "habt", ",", "kauf", "ich", "ger\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VAFIN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Nehmt drei Batzen Ihr daf\u00fcr?\u00ab \u2013", "tokens": ["Nehmt", "drei", "Bat\u00b7zen", "Ihr", "da\u00b7f\u00fcr", "?", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "CARD", "NN", "PPER", "PAV", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbmit Vergn\u00fcgen!\u00ab \u2013 \u00bbHabt Ihr k\u00fcnftig", "tokens": ["\u00bb", "mit", "Ver\u00b7gn\u00fc\u00b7gen", "!", "\u00ab", "\u2013", "\u00bb", "Habt", "Ihr", "k\u00fcnf\u00b7tig"], "token_info": ["punct", "word", "word", "punct", "punct", "punct", "punct", "word", "word", "word"], "pos": ["$(", "APPR", "NN", "$.", "$(", "$(", "$(", "VAFIN", "PPER", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mehr zu handeln, la\u00dft mich holen;", "tokens": ["Mehr", "zu", "han\u00b7deln", ",", "la\u00dft", "mich", "ho\u00b7len", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "PTKZU", "VVINF", "$,", "VVIMP", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Edel seid Ihr und vern\u00fcnftig,", "tokens": ["E\u00b7del", "seid", "Ihr", "und", "ver\u00b7n\u00fcnf\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ich lob Euch unverhohlen.", "tokens": ["Und", "ich", "lob", "Euch", "un\u00b7ver\u00b7hoh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Gleich mich dankbar Euch zu zeigen,", "tokens": ["Gleich", "mich", "dank\u00b7bar", "Euch", "zu", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADJD", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "La\u00df ich jede R\u00fccksicht schweigen,", "tokens": ["La\u00df", "ich", "je\u00b7de", "R\u00fcck\u00b7sicht", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und verkauf Euch, was Ihr wollt.", "tokens": ["Und", "ver\u00b7kauf", "Euch", ",", "was", "Ihr", "wollt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PWS", "PPER", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Seht den Ring da, den ich habe;", "tokens": ["Seht", "den", "Ring", "da", ",", "den", "ich", "ha\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$,", "PRELS", "PPER", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur von Messing, schlecht, unscheinsam,", "tokens": ["Nur", "von", "Mes\u00b7sing", ",", "schlecht", ",", "un\u00b7schein\u00b7sam", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "APPR", "NE", "$,", "VVFIN", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber, meiner Liebsten Gabe;", "tokens": ["A\u00b7ber", ",", "mei\u00b7ner", "Liebs\u00b7ten", "Ga\u00b7be", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach sie starb, und lie\u00df mich einsam!", "tokens": ["Ach", "sie", "starb", ",", "und", "lie\u00df", "mich", "ein\u00b7sam", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "VVFIN", "$,", "KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nicht um einen Goldesheufen...!", "tokens": ["Nicht", "um", "ei\u00b7nen", "Gol\u00b7des\u00b7heu\u00b7fen", "...", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "$(", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aber Ihr, wollt Ihr ihn kaufen,", "tokens": ["A\u00b7ber", "Ihr", ",", "wollt", "Ihr", "ihn", "kau\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "Gebt mir zehn Dukaten nur.\u00ab \u2013", "tokens": ["Gebt", "mir", "zehn", "Du\u00b7ka\u00b7ten", "nur", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVIMP", "PPER", "CARD", "NN", "ADV", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "\u00bbmit Vergn\u00fcgen!\u00ab \u2013 \u00bbEi! was seh ich?!", "tokens": ["\u00bb", "mit", "Ver\u00b7gn\u00fc\u00b7gen", "!", "\u00ab", "\u2013", "\u00bb", "Ei", "!", "was", "seh", "ich", "?!"], "token_info": ["punct", "word", "word", "punct", "punct", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "$.", "$(", "$(", "$(", "NN", "$.", "PWS", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6ner Beutel goldgeschwollen,", "tokens": ["Sch\u00f6\u00b7ner", "Beu\u00b7tel", "gold\u00b7ge\u00b7schwol\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Du gef\u00e4llst mir, das gesteh ich;", "tokens": ["Du", "ge\u00b7f\u00e4llst", "mir", ",", "das", "ge\u00b7steh", "ich", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PDS", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die Pistole f\u00fcr den vollen!", "tokens": ["Die", "Pis\u00b7to\u00b7le", "f\u00fcr", "den", "vol\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sie ist von dem besten Meister,", "tokens": ["Sie", "ist", "von", "dem", "bes\u00b7ten", "Meis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Kuchenreuter, glaub ich, hei\u00dft er,", "tokens": ["Ku\u00b7chen\u00b7reu\u00b7ter", ",", "glaub", "ich", ",", "hei\u00dft", "er", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nehmt sie f\u00fcr den Beutel hin!\u00ab \u2013", "tokens": ["Nehmt", "sie", "f\u00fcr", "den", "Beu\u00b7tel", "hin", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "\u00bbmit Vergn\u00fcgen! Nun Geselle,", "tokens": ["\u00bb", "mit", "Ver\u00b7gn\u00fc\u00b7gen", "!", "Nun", "Ge\u00b7sel\u00b7le", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "$.", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist die Reih an mich gekommen!", "tokens": ["Ist", "die", "Reih", "an", "mich", "ge\u00b7kom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Her den Beutel auf der Stelle!", "tokens": ["Her", "den", "Beu\u00b7tel", "auf", "der", "Stel\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Her, was du mir abgenommen!", "tokens": ["Her", ",", "was", "du", "mir", "ab\u00b7ge\u00b7nom\u00b7men", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "PPER", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Gib mir das Geraubte wieder,", "tokens": ["Gib", "mir", "das", "Ge\u00b7raub\u00b7te", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gleich! ich schie\u00dfe sonst dich nieder,", "tokens": ["Gleich", "!", "ich", "schie\u00b7\u00dfe", "sonst", "dich", "nie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VVFIN", "ADV", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie man einen Hund erschie\u00dft!\u00ab", "tokens": ["Wie", "man", "ei\u00b7nen", "Hund", "er\u00b7schie\u00dft", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "\u00bbschie\u00dft nur, schie\u00dft nur! wahrlich, Schaden", "tokens": ["\u00bb", "schie\u00dft", "nur", ",", "schie\u00dft", "nur", "!", "wahr\u00b7lich", ",", "Scha\u00b7den"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["$(", "VVFIN", "ADV", "$,", "VVFIN", "ADV", "$.", "ADV", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00e4rt Ihr f\u00e4hig anzurichten,", "tokens": ["W\u00e4rt", "Ihr", "f\u00e4\u00b7hig", "an\u00b7zu\u00b7rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00e4re nur das Ding geladen.", "tokens": ["W\u00e4\u00b7re", "nur", "das", "Ding", "ge\u00b7la\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihr gefallt mir so mit nichten.", "tokens": ["Ihr", "ge\u00b7fallt", "mir", "so", "mit", "nich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "PIS", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Unfein d\u00fcrft ich wohl Euch schelten;", "tokens": ["Un\u00b7fein", "d\u00fcrft", "ich", "wohl", "Euch", "schel\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Abgeschlo\u00dfne H\u00e4ndel gelten,", "tokens": ["Ab\u00b7ge\u00b7schlo\u00df\u00b7ne", "H\u00e4n\u00b7del", "gel\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Merkt es Euch und, gute Nacht!\u00ab", "tokens": ["Merkt", "es", "Euch", "und", ",", "gu\u00b7te", "Nacht", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "PPER", "KON", "$,", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Ihn verlachend unumwunden,", "tokens": ["Ihn", "ver\u00b7la\u00b7chend", "un\u00b7um\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Langgebeint, mit leichten S\u00e4tzen,", "tokens": ["Lang\u00b7ge\u00b7beint", ",", "mit", "leich\u00b7ten", "S\u00e4t\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "War er in dem Busch verschwunden", "tokens": ["War", "er", "in", "dem", "Busch", "ver\u00b7schwun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit den eingetauschten Sch\u00e4tzen.", "tokens": ["Mit", "den", "ein\u00b7ge\u00b7tauschten", "Sch\u00e4t\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Jener mit dem Kuchenreuter", "tokens": ["Je\u00b7ner", "mit", "dem", "Ku\u00b7chen\u00b7reu\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In der Hand, sah nicht gescheuter", "tokens": ["In", "der", "Hand", ",", "sah", "nicht", "ge\u00b7scheu\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "VVFIN", "PTKNEG", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Aus, als augenblicks zuvor.", "tokens": ["Aus", ",", "als", "au\u00b7gen\u00b7blicks", "zu\u00b7vor", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "KOUS", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Einer kam vom K\u00f6nigsmahle", "tokens": ["Ei\u00b7ner", "kam", "vom", "K\u00f6\u00b7nigs\u00b7mah\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In den Park sich zu bewegen,", "tokens": ["In", "den", "Park", "sich", "zu", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus dem Busch mit einem Male", "tokens": ["Aus", "dem", "Busch", "mit", "ei\u00b7nem", "Ma\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Trat ein andrer ihm entgegen;", "tokens": ["Trat", "ein", "an\u00b7drer", "ihm", "ent\u00b7ge\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "PPER", "PTKVZ", "$."], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Zwischen Rock und Kamisole", "tokens": ["Zwi\u00b7schen", "Rock", "und", "Ka\u00b7mi\u00b7so\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Griff der schnell, und die Pistole", "tokens": ["Griff", "der", "schnell", ",", "und", "die", "Pis\u00b7to\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "ADJD", "$,", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Setzt er jenem auf die Brust.", "tokens": ["Setzt", "er", "je\u00b7nem", "auf", "die", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "\u00bbleise, leise! mu\u00df ich bitten;", "tokens": ["\u00bb", "lei\u00b7se", ",", "lei\u00b7se", "!", "mu\u00df", "ich", "bit\u00b7ten", ";"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "ADJD", "$.", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was wir hier f\u00fcr Handel treiben,", "tokens": ["Was", "wir", "hier", "f\u00fcr", "Han\u00b7del", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mag vom unberufnen Dritten", "tokens": ["Mag", "vom", "un\u00b7be\u00b7ruf\u00b7nen", "Drit\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcglich unbelauschet bleiben.", "tokens": ["F\u00fcg\u00b7lich", "un\u00b7be\u00b7lau\u00b7schet", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wollt Ihr Uhren nebst Gehenken", "tokens": ["Wollt", "Ihr", "Uh\u00b7ren", "nebst", "Ge\u00b7hen\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wohl verkaufen? nicht verschenken;", "tokens": ["Wohl", "ver\u00b7kau\u00b7fen", "?", "nicht", "ver\u00b7schen\u00b7ken", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$.", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nehmt drei Batzen Ihr daf\u00fcr?\u00ab \u2013", "tokens": ["Nehmt", "drei", "Bat\u00b7zen", "Ihr", "da\u00b7f\u00fcr", "?", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "CARD", "NN", "PPER", "PAV", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Mit Vergn\u00fcgen!\u00ab \u2013 \u00bbNimmer richtig", "tokens": ["Mit", "Ver\u00b7gn\u00fc\u00b7gen", "!", "\u00ab", "\u2013", "\u00bb", "Nim\u00b7mer", "rich\u00b7tig"], "token_info": ["word", "word", "punct", "punct", "punct", "punct", "word", "word"], "pos": ["APPR", "NN", "$.", "$(", "$(", "$(", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist die Dorfuhr noch gegangen;", "tokens": ["Ist", "die", "Dor\u00b7fuhr", "noch", "ge\u00b7gan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Tut der K\u00fcster auch so wichtig,", "tokens": ["Tut", "der", "K\u00fcs\u00b7ter", "auch", "so", "wich\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wei\u00df er's doch nicht anzufangen;", "tokens": ["Wei\u00df", "er's", "doch", "nicht", "an\u00b7zu\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "PTKNEG", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Jeder wei\u00df in unsern Tagen,", "tokens": ["Je\u00b7der", "wei\u00df", "in", "un\u00b7sern", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Was die Glocke hat geschlagen;", "tokens": ["Was", "die", "Glo\u00b7cke", "hat", "ge\u00b7schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Gottlob! nun erfahr ich's auch.", "tokens": ["Gott\u00b7lob", "!", "nun", "er\u00b7fahr", "ich's", "auch", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VVFIN", "PIS", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Sagt mir ferner: k\u00f6nnt Ihr missen,", "tokens": ["Sagt", "mir", "fer\u00b7ner", ":", "k\u00f6nnt", "Ihr", "mis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$.", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was da blinkt an Euren Fingern?", "tokens": ["Was", "da", "blinkt", "an", "Eu\u00b7ren", "Fin\u00b7gern", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine Hausfrau, sollt Ihr wissen,", "tokens": ["Mei\u00b7ne", "Haus\u00b7frau", ",", "sollt", "Ihr", "wis\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist gar arg nach solchen Dingern;", "tokens": ["Ist", "gar", "arg", "nach", "sol\u00b7chen", "Din\u00b7gern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Solche Ringe, solche Sterne,", "tokens": ["Sol\u00b7che", "Rin\u00b7ge", ",", "sol\u00b7che", "Ster\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie Ihr da habt, kauf ich gerne;", "tokens": ["Wie", "Ihr", "da", "habt", ",", "kauf", "ich", "ger\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VAFIN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Nehmt drei Batzen Ihr daf\u00fcr?\u00ab \u2013", "tokens": ["Nehmt", "drei", "Bat\u00b7zen", "Ihr", "da\u00b7f\u00fcr", "?", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "CARD", "NN", "PPER", "PAV", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "\u00bbmit Vergn\u00fcgen!\u00ab \u2013 \u00bbHabt Ihr k\u00fcnftig", "tokens": ["\u00bb", "mit", "Ver\u00b7gn\u00fc\u00b7gen", "!", "\u00ab", "\u2013", "\u00bb", "Habt", "Ihr", "k\u00fcnf\u00b7tig"], "token_info": ["punct", "word", "word", "punct", "punct", "punct", "punct", "word", "word", "word"], "pos": ["$(", "APPR", "NN", "$.", "$(", "$(", "$(", "VAFIN", "PPER", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mehr zu handeln, la\u00dft mich holen;", "tokens": ["Mehr", "zu", "han\u00b7deln", ",", "la\u00dft", "mich", "ho\u00b7len", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "PTKZU", "VVINF", "$,", "VVIMP", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Edel seid Ihr und vern\u00fcnftig,", "tokens": ["E\u00b7del", "seid", "Ihr", "und", "ver\u00b7n\u00fcnf\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ich lob Euch unverhohlen.", "tokens": ["Und", "ich", "lob", "Euch", "un\u00b7ver\u00b7hoh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Gleich mich dankbar Euch zu zeigen,", "tokens": ["Gleich", "mich", "dank\u00b7bar", "Euch", "zu", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADJD", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "La\u00df ich jede R\u00fccksicht schweigen,", "tokens": ["La\u00df", "ich", "je\u00b7de", "R\u00fcck\u00b7sicht", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und verkauf Euch, was Ihr wollt.", "tokens": ["Und", "ver\u00b7kauf", "Euch", ",", "was", "Ihr", "wollt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PWS", "PPER", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Seht den Ring da, den ich habe;", "tokens": ["Seht", "den", "Ring", "da", ",", "den", "ich", "ha\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$,", "PRELS", "PPER", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur von Messing, schlecht, unscheinsam,", "tokens": ["Nur", "von", "Mes\u00b7sing", ",", "schlecht", ",", "un\u00b7schein\u00b7sam", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "APPR", "NE", "$,", "VVFIN", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber, meiner Liebsten Gabe;", "tokens": ["A\u00b7ber", ",", "mei\u00b7ner", "Liebs\u00b7ten", "Ga\u00b7be", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach sie starb, und lie\u00df mich einsam!", "tokens": ["Ach", "sie", "starb", ",", "und", "lie\u00df", "mich", "ein\u00b7sam", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "VVFIN", "$,", "KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nicht um einen Goldesheufen...!", "tokens": ["Nicht", "um", "ei\u00b7nen", "Gol\u00b7des\u00b7heu\u00b7fen", "...", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "$(", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aber Ihr, wollt Ihr ihn kaufen,", "tokens": ["A\u00b7ber", "Ihr", ",", "wollt", "Ihr", "ihn", "kau\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "Gebt mir zehn Dukaten nur.\u00ab \u2013", "tokens": ["Gebt", "mir", "zehn", "Du\u00b7ka\u00b7ten", "nur", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVIMP", "PPER", "CARD", "NN", "ADV", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "\u00bbmit Vergn\u00fcgen!\u00ab \u2013 \u00bbEi! was seh ich?!", "tokens": ["\u00bb", "mit", "Ver\u00b7gn\u00fc\u00b7gen", "!", "\u00ab", "\u2013", "\u00bb", "Ei", "!", "was", "seh", "ich", "?!"], "token_info": ["punct", "word", "word", "punct", "punct", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "$.", "$(", "$(", "$(", "NN", "$.", "PWS", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6ner Beutel goldgeschwollen,", "tokens": ["Sch\u00f6\u00b7ner", "Beu\u00b7tel", "gold\u00b7ge\u00b7schwol\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Du gef\u00e4llst mir, das gesteh ich;", "tokens": ["Du", "ge\u00b7f\u00e4llst", "mir", ",", "das", "ge\u00b7steh", "ich", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PDS", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die Pistole f\u00fcr den vollen!", "tokens": ["Die", "Pis\u00b7to\u00b7le", "f\u00fcr", "den", "vol\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sie ist von dem besten Meister,", "tokens": ["Sie", "ist", "von", "dem", "bes\u00b7ten", "Meis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Kuchenreuter, glaub ich, hei\u00dft er,", "tokens": ["Ku\u00b7chen\u00b7reu\u00b7ter", ",", "glaub", "ich", ",", "hei\u00dft", "er", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nehmt sie f\u00fcr den Beutel hin!\u00ab \u2013", "tokens": ["Nehmt", "sie", "f\u00fcr", "den", "Beu\u00b7tel", "hin", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "\u00bbmit Vergn\u00fcgen! Nun Geselle,", "tokens": ["\u00bb", "mit", "Ver\u00b7gn\u00fc\u00b7gen", "!", "Nun", "Ge\u00b7sel\u00b7le", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "$.", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist die Reih an mich gekommen!", "tokens": ["Ist", "die", "Reih", "an", "mich", "ge\u00b7kom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Her den Beutel auf der Stelle!", "tokens": ["Her", "den", "Beu\u00b7tel", "auf", "der", "Stel\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Her, was du mir abgenommen!", "tokens": ["Her", ",", "was", "du", "mir", "ab\u00b7ge\u00b7nom\u00b7men", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "PPER", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Gib mir das Geraubte wieder,", "tokens": ["Gib", "mir", "das", "Ge\u00b7raub\u00b7te", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gleich! ich schie\u00dfe sonst dich nieder,", "tokens": ["Gleich", "!", "ich", "schie\u00b7\u00dfe", "sonst", "dich", "nie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VVFIN", "ADV", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie man einen Hund erschie\u00dft!\u00ab", "tokens": ["Wie", "man", "ei\u00b7nen", "Hund", "er\u00b7schie\u00dft", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.19": {"line.1": {"text": "\u00bbschie\u00dft nur, schie\u00dft nur! wahrlich, Schaden", "tokens": ["\u00bb", "schie\u00dft", "nur", ",", "schie\u00dft", "nur", "!", "wahr\u00b7lich", ",", "Scha\u00b7den"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["$(", "VVFIN", "ADV", "$,", "VVFIN", "ADV", "$.", "ADV", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00e4rt Ihr f\u00e4hig anzurichten,", "tokens": ["W\u00e4rt", "Ihr", "f\u00e4\u00b7hig", "an\u00b7zu\u00b7rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00e4re nur das Ding geladen.", "tokens": ["W\u00e4\u00b7re", "nur", "das", "Ding", "ge\u00b7la\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihr gefallt mir so mit nichten.", "tokens": ["Ihr", "ge\u00b7fallt", "mir", "so", "mit", "nich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "PIS", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Unfein d\u00fcrft ich wohl Euch schelten;", "tokens": ["Un\u00b7fein", "d\u00fcrft", "ich", "wohl", "Euch", "schel\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Abgeschlo\u00dfne H\u00e4ndel gelten,", "tokens": ["Ab\u00b7ge\u00b7schlo\u00df\u00b7ne", "H\u00e4n\u00b7del", "gel\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Merkt es Euch und, gute Nacht!\u00ab", "tokens": ["Merkt", "es", "Euch", "und", ",", "gu\u00b7te", "Nacht", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "PPER", "KON", "$,", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.20": {"line.1": {"text": "Ihn verlachend unumwunden,", "tokens": ["Ihn", "ver\u00b7la\u00b7chend", "un\u00b7um\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Langgebeint, mit leichten S\u00e4tzen,", "tokens": ["Lang\u00b7ge\u00b7beint", ",", "mit", "leich\u00b7ten", "S\u00e4t\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "War er in dem Busch verschwunden", "tokens": ["War", "er", "in", "dem", "Busch", "ver\u00b7schwun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit den eingetauschten Sch\u00e4tzen.", "tokens": ["Mit", "den", "ein\u00b7ge\u00b7tauschten", "Sch\u00e4t\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Jener mit dem Kuchenreuter", "tokens": ["Je\u00b7ner", "mit", "dem", "Ku\u00b7chen\u00b7reu\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In der Hand, sah nicht gescheuter", "tokens": ["In", "der", "Hand", ",", "sah", "nicht", "ge\u00b7scheu\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "VVFIN", "PTKNEG", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Aus, als augenblicks zuvor.", "tokens": ["Aus", ",", "als", "au\u00b7gen\u00b7blicks", "zu\u00b7vor", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "KOUS", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}