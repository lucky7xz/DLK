{"textgrid.poem.33992": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Wein", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du blinkst so hell und gl\u00e4nzend aus dem Becher,", "tokens": ["Du", "blinkst", "so", "hell", "und", "gl\u00e4n\u00b7zend", "aus", "dem", "Be\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als w\u00e4re jeder Stral in dir zerronnen,", "tokens": ["Als", "w\u00e4\u00b7re", "je\u00b7der", "Stral", "in", "dir", "zer\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PIAT", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Woraus du einst die Feuerkraft gewonnen,", "tokens": ["Wo\u00b7raus", "du", "einst", "die", "Feu\u00b7er\u00b7kraft", "ge\u00b7won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die gl\u00fchend jetzt entgegen sch\u00e4umt dem Zecher.", "tokens": ["Die", "gl\u00fc\u00b7hend", "jetzt", "ent\u00b7ge\u00b7gen", "sch\u00e4umt", "dem", "Ze\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "PTKVZ", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich aber s\u00e4ume, reizender Versprecher", "tokens": ["Ich", "a\u00b7ber", "s\u00e4u\u00b7me", ",", "rei\u00b7zen\u00b7der", "Ver\u00b7spr