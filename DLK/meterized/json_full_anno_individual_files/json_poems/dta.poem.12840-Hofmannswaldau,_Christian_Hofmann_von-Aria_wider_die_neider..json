{"dta.poem.12840": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Aria wider die neider.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich mu\u00df mich endlich noch verschliessen,", "tokens": ["Ich", "mu\u00df", "mich", "end\u00b7lich", "noch", "ver\u00b7schlies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Jhr neider! kommt und sperrt mich ein;", "tokens": ["Ihr", "nei\u00b7der", "!", "kommt", "und", "sperrt", "mich", "ein", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So darff es euch nicht mehr verdrie\u00dfen,", "tokens": ["So", "darff", "es", "euch", "nicht", "mehr", "ver\u00b7drie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df mir die leute g\u00fcnstig seyn:", "tokens": ["Da\u00df", "mir", "die", "leu\u00b7te", "g\u00fcns\u00b7tig", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So sitz ich dann in guter ruh,", "tokens": ["So", "sitz", "ich", "dann", "in", "gu\u00b7ter", "ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und h\u00f6re euren possen zu.", "tokens": ["Und", "h\u00f6\u00b7re", "eu\u00b7ren", "pos\u00b7sen", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Jhr wisset alle meine thaten:", "tokens": ["Ihr", "wis\u00b7set", "al\u00b7le", "mei\u00b7ne", "tha\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich darff nicht auf die gasse gehn,", "tokens": ["Ich", "darff", "nicht", "auf", "die", "gas\u00b7se", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So bin ich schon bey euch verrathen,", "tokens": ["So", "bin", "ich", "schon", "bey", "euch", "ver\u00b7ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und mu\u00df im zeddul oben stehn.", "tokens": ["Und", "mu\u00df", "im", "zed\u00b7dul", "o\u00b7ben", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPRART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das macht, der w\u00e4chter ist bestellt,", "tokens": ["Das", "macht", ",", "der", "w\u00e4ch\u00b7ter", "ist", "be\u00b7stellt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ART", "ADJA", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der mich in steter hut beh\u00e4lt.", "tokens": ["Der", "mich", "in", "ste\u00b7ter", "hut", "be\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Allein ihr mercket nicht die t\u00fccke;", "tokens": ["Al\u00b7lein", "ihr", "mer\u00b7cket", "nicht", "die", "t\u00fc\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die w\u00e4chter sind euch nicht getreu:", "tokens": ["Die", "w\u00e4ch\u00b7ter", "sind", "euch", "nicht", "ge\u00b7treu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie bringen euch manch falsches st\u00fccke,", "tokens": ["Sie", "brin\u00b7gen", "euch", "manch", "fal\u00b7sches", "st\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und manche derbe l\u00fcgen bey.", "tokens": ["Und", "man\u00b7che", "der\u00b7be", "l\u00fc\u00b7gen", "bey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PDS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Denn was mein tage nicht geschehn,", "tokens": ["Denn", "was", "mein", "ta\u00b7ge", "nicht", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPOSAT", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das haben sie von mir gesehn.", "tokens": ["Das", "ha\u00b7ben", "sie", "von", "mir", "ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Bald treffen sie bey finstern stunden,", "tokens": ["Bald", "tref\u00b7fen", "sie", "bey", "fins\u00b7tern", "stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mich mitten auf den gassen an:", "tokens": ["Mich", "mit\u00b7ten", "auf", "den", "gas\u00b7sen", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bald werd ich hier, bald da gefunden,", "tokens": ["Bald", "werd", "ich", "hier", ",", "bald", "da", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und habe nirgends recht gethan.", "tokens": ["Und", "ha\u00b7be", "nir\u00b7gends", "recht", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Allein sie haben bey der nacht", "tokens": ["Al\u00b7lein", "sie", "ha\u00b7ben", "bey", "der", "nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die brillen nicht recht klar gemacht.", "tokens": ["Die", "bril\u00b7len", "nicht", "recht", "klar", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKNEG", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich setze mir offt vor, zu schweigen,", "tokens": ["Ich", "set\u00b7ze", "mir", "offt", "vor", ",", "zu", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So wird die einfalt angeklagt:", "tokens": ["So", "wird", "die", "ein\u00b7falt", "an\u00b7ge\u00b7klagt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und will ich mich was frey bezeigen,", "tokens": ["Und", "will", "ich", "mich", "was", "frey", "be\u00b7zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PRF", "PRELS", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So hab ich gleich zu viel gesagt.", "tokens": ["So", "hab", "ich", "gleich", "zu", "viel", "ge\u00b7sagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PTKA", "PIS", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wo treff\u2019 ich diesen meister an,", "tokens": ["Wo", "treff'", "ich", "die\u00b7sen", "meis\u00b7ter", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PDAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der mich die k\u00fcnste lehren kan.", "tokens": ["Der", "mich", "die", "k\u00fcns\u00b7te", "leh\u00b7ren", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wiewol ich werde mich nicht kr\u00e4ncken,", "tokens": ["Wie\u00b7wol", "ich", "wer\u00b7de", "mich", "nicht", "kr\u00e4n\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein hertz ist mir noch viel zu gut.", "tokens": ["Mein", "hertz", "ist", "mir", "noch", "viel", "zu", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADV", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jhr neider d\u00fcrffet nicht gedencken,", "tokens": ["Ihr", "nei\u00b7der", "d\u00fcrf\u00b7fet", "nicht", "ge\u00b7den\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df mir das leiden wehe thut.", "tokens": ["Da\u00df", "mir", "das", "lei\u00b7den", "we\u00b7he", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "VVFIN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die unschuld kan in dieser pein", "tokens": ["Die", "un\u00b7schuld", "kan", "in", "die\u00b7ser", "pein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mein allerbestes labsal seyn.", "tokens": ["Mein", "al\u00b7ler\u00b7bes\u00b7tes", "lab\u00b7sal", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Gewi\u00df, ich mu\u00df nur eurer lachen,", "tokens": ["Ge\u00b7wi\u00df", ",", "ich", "mu\u00df", "nur", "eu\u00b7rer", "la\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VMFIN", "ADV", "PPOSAT", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ob ich euch noch so alber bin.", "tokens": ["Ob", "ich", "euch", "noch", "so", "al\u00b7ber", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADV", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jhr m\u00f6gt es k\u00fcnfftig \u00e4rger machen,", "tokens": ["Ihr", "m\u00f6gt", "es", "k\u00fcnff\u00b7tig", "\u00e4r\u00b7ger", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Trett selber auf die wachen hin,", "tokens": ["Trett", "sel\u00b7ber", "auf", "die", "wa\u00b7chen", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "ADJA", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und nehmet dann bey tag und nacht", "tokens": ["Und", "neh\u00b7met", "dann", "bey", "tag", "und", "nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Mein thun und lassen wohl in acht.", "tokens": ["Mein", "thun", "und", "las\u00b7sen", "wohl", "in", "acht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVINF", "KON", "VVFIN", "ADV", "APPR", "CARD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Indessen dient es mir zum besten:", "tokens": ["In\u00b7des\u00b7sen", "dient", "es", "mir", "zum", "bes\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPRART", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich sage nichts, doch merck ich wohl,", "tokens": ["Ich", "sa\u00b7ge", "nichts", ",", "doch", "merck", "ich", "wohl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$,", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie man bey solchen schlimmen g\u00e4sten", "tokens": ["Wie", "man", "bey", "sol\u00b7chen", "schlim\u00b7men", "g\u00e4s\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sich k\u00fcnfftig recht verhalten soll.", "tokens": ["Sich", "k\u00fcnff\u00b7tig", "recht", "ver\u00b7hal\u00b7ten", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich wei\u00df schon, was im hertzen steckt,", "tokens": ["Ich", "wei\u00df", "schon", ",", "was", "im", "hert\u00b7zen", "steckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ob gleich der ku\u00df nach zucker schmeck", "tokens": ["Ob", "gleich", "der", "ku\u00df", "nach", "zu\u00b7cker", "schmeck"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Jhr m\u00f6gt die unschuld immer hassen,", "tokens": ["Ihr", "m\u00f6gt", "die", "un\u00b7schuld", "im\u00b7mer", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Genung, da\u00df sie der himmel liebt.", "tokens": ["Ge\u00b7nung", ",", "da\u00df", "sie", "der", "him\u00b7mel", "liebt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jhr m\u00fcsset mir doch endlich lassen,", "tokens": ["Ihr", "m\u00fcs\u00b7set", "mir", "doch", "end\u00b7lich", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was dessen gunst und gnade giebt.", "tokens": ["Was", "des\u00b7sen", "gunst", "und", "gna\u00b7de", "giebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDS", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Will der mir wohl, so hab\u2019 ich\u2019s gut,", "tokens": ["Will", "der", "mir", "wohl", ",", "so", "hab'", "ich's", "gut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "PPER", "ADV", "$,", "ADV", "VAFIN", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und achte nicht, was mi\u00dfgunst thut.", "tokens": ["Und", "ach\u00b7te", "nicht", ",", "was", "mi\u00df\u00b7gunst", "thut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "PTKNEG", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}