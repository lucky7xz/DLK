": "Weil um der Magdalenen Gunst", "tokens": ["Weil", "um", "der", "Mag\u00b7da\u00b7le\u00b7nen", "Gunst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die hagern Dichter flehen?", "tokens": ["Die", "ha\u00b7gern", "Dich\u00b7ter", "fle\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Such nicht, such nicht nach Liebesdank", "tokens": ["Such", "nicht", ",", "such", "nicht", "nach", "Lie\u00b7bes\u00b7dank"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PTKNEG", "$,", "ADV", "PTKNEG", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bei der, die man verrieth,", "tokens": ["Bei", "der", ",", "die", "man", "ver\u00b7rieth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Denn ist des Volkes Seele krank,", "tokens": ["Denn", "ist", "des", "Vol\u00b7kes", "See\u00b7le", "krank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Krankt auch des Volkes Lied.", "tokens": ["Krankt", "auch", "des", "Vol\u00b7kes", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}