{"dta.poem.16606": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "267.  Ein ding behagt nicht im\u0304er.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Freund/ solln wir allesambt/ nur jmmer Eines schreyn/", "tokens": ["Freund", "/", "solln", "wir", "al\u00b7le\u00b7sambt", "/", "nur", "jm\u00b7mer", "Ei\u00b7nes", "schreyn", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "VMFIN", "PPER", "VVFIN", "$(", "ADV", "ADV", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was wird di\u00df f\u00fcr ein Lied/ und f\u00fcr Gesinge seyn?", "tokens": ["Was", "wird", "di\u00df", "f\u00fcr", "ein", "Lied", "/", "und", "f\u00fcr", "Ge\u00b7sin\u00b7ge", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PDS", "APPR", "ART", "NN", "$(", "KON", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}