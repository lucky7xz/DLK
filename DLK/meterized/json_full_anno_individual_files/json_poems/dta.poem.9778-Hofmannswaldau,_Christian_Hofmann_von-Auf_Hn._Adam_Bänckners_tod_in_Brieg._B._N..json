{"dta.poem.9778": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf Hn. Adam B\u00e4nckners tod in Brieg.  \n B. N.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich schreibe/ seligster/ von lieb und treu getrieben/", "tokens": ["Ich", "schrei\u00b7be", "/", "se\u00b7ligs\u00b7ter", "/", "von", "lieb", "und", "treu", "ge\u00b7trie\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "ADJD", "$(", "APPR", "ADJD", "KON", "ADJD", "VVPP", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wo anders thr\u00e4nen-saltz kan schwartze dinte seyn?", "tokens": ["Wo", "an\u00b7ders", "thr\u00e4\u00b7nen\u00b7saltz", "kan", "schwart\u00b7ze", "din\u00b7te", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "VMFIN", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn di\u00df ist nur allein von allem \u00fcbrig blieben/", "tokens": ["Denn", "di\u00df", "ist", "nur", "al\u00b7lein", "von", "al\u00b7lem", "\u00fcb\u00b7rig", "blie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADV", "ADV", "APPR", "PIS", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was dir mein hertze kan auf deine leiche streun.", "tokens": ["Was", "dir", "mein", "hert\u00b7ze", "kan", "auf", "dei\u00b7ne", "lei\u00b7che", "streun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPOSAT", "NN", "VMFIN", "APPR", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ach freund! was aber freund? ach kern gerechter seelen!", "tokens": ["Ach", "freund", "!", "was", "a\u00b7ber", "freund", "?", "ach", "kern", "ge\u00b7rech\u00b7ter", "see\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "$.", "PWS", "ADV", "ADJD", "$.", "ADV", "VVFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wird so die redligkeit auf erden unterdr\u00fcckt?", "tokens": ["Wird", "so", "die", "red\u00b7lig\u00b7keit", "auf", "er\u00b7den", "un\u00b7ter\u00b7dr\u00fcckt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Was wunder/ da\u00df mein trost in deines grabes h\u00f6len/", "tokens": ["Was", "wun\u00b7der", "/", "da\u00df", "mein", "trost", "in", "dei\u00b7nes", "gra\u00b7bes", "h\u00f6\u00b7len", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$(", "KOUS", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So wie der sonnen glantz bey tr\u00fcber lufft erstickt?", "tokens": ["So", "wie", "der", "son\u00b7nen", "glantz", "bey", "tr\u00fc\u00b7ber", "lufft", "er\u00b7stickt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du bist von jugend auf der ulmen-baum gewesen/", "tokens": ["Du", "bist", "von", "ju\u00b7gend", "auf", "der", "ul\u00b7men\u00b7baum", "ge\u00b7we\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "APPR", "ART", "NN", "VAPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "An dem mein auge so wie gr\u00fcne reben hieng/", "tokens": ["An", "dem", "mein", "au\u00b7ge", "so", "wie", "gr\u00fc\u00b7ne", "re\u00b7ben", "hieng", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "NN", "ADV", "KOKOM", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der spiegel/ der mich oft von weitem lassen lesen/", "tokens": ["Der", "spie\u00b7gel", "/", "der", "mich", "oft", "von", "wei\u00b7tem", "las\u00b7sen", "le\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "ADV", "APPR", "PIS", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wie die verlarvte welt mit flecken schwanger gieng.", "tokens": ["Wie", "die", "ver\u00b7larv\u00b7te", "welt", "mit", "fle\u00b7cken", "schwan\u00b7ger", "gieng", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPR", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Denn dieses ist und bleibt der freunde bestes zeichen/", "tokens": ["Denn", "die\u00b7ses", "ist", "und", "bleibt", "der", "freun\u00b7de", "bes\u00b7tes", "zei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "KON", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Beysammen voller frucht wie dattel-b\u00e4ume stehn/", "tokens": ["Bey\u00b7sam\u00b7men", "vol\u00b7ler", "frucht", "wie", "dat\u00b7tel\u00b7b\u00e4u\u00b7me", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "KOKOM", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Am wesen aber sich den reinen sternen gleichen/", "tokens": ["Am", "we\u00b7sen", "a\u00b7ber", "sich", "den", "rei\u00b7nen", "ster\u00b7nen", "glei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "PRF", "ART", "ADJA", "ADJA", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und ohne schminck und fleck stets von einander gehn.", "tokens": ["Und", "oh\u00b7ne", "schminck", "und", "fleck", "stets", "von", "ein\u00b7an\u00b7der", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "ADV", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die meisten haben zwar mit hermelinen fellen", "tokens": ["Die", "meis\u00b7ten", "ha\u00b7ben", "zwar", "mit", "her\u00b7me\u00b7li\u00b7nen", "fel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VAFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Jhr hertze/ wie der schnee mit schimmer/ sich bedeckt;", "tokens": ["Ihr", "hert\u00b7ze", "/", "wie", "der", "schnee", "mit", "schim\u00b7mer", "/", "sich", "be\u00b7deckt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "KOKOM", "ART", "NN", "APPR", "ADJA", "$(", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wenn aber sonn und zeit sie auf die probe stellen/", "tokens": ["Wenn", "a\u00b7ber", "sonn", "und", "zeit", "sie", "auf", "die", "pro\u00b7be", "stel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "KON", "NN", "PPER", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "So sieht man/ da\u00df auch nur in beyden k\u00e4lte steckt.", "tokens": ["So", "sieht", "man", "/", "da\u00df", "auch", "nur", "in", "bey\u00b7den", "k\u00e4l\u00b7te", "steckt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$(", "KOUS", "ADV", "ADV", "APPR", "PIAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Du hast/ gleich wie ein blat von pommerantzen-zweigen/", "tokens": ["Du", "hast", "/", "gleich", "wie", "ein", "blat", "von", "pom\u00b7merant\u00b7zen\u00b7zwei\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "ADV", "KOKOM", "ART", "NN", "APPR", "NE", "$("], "meter": "-++--+-++-+-", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Die zung und auch das hertz beysammen stets gef\u00fchrt;", "tokens": ["Die", "zung", "und", "auch", "das", "hertz", "bey\u00b7sam\u00b7men", "stets", "ge\u00b7f\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADV", "ART", "NN", "VVFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ich aber offt mehr lust aus deinem treu-bezeigen/", "tokens": ["Ich", "a\u00b7ber", "offt", "mehr", "lust", "aus", "dei\u00b7nem", "treu\u00b7be\u00b7zei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADV", "VVFIN", "APPR", "PPOSAT", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Als ein granaten-baum bey myrthen krafft/ gesp\u00fchrt.", "tokens": ["Als", "ein", "gra\u00b7na\u00b7ten\u00b7baum", "bey", "myr\u00b7then", "krafft", "/", "ge\u00b7sp\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "ADJA", "NN", "$(", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Jedoch was nenn ich mich? Brieg hat dich auch verlohren.", "tokens": ["Je\u00b7doch", "was", "nenn", "ich", "mich", "?", "Brieg", "hat", "dich", "auch", "ver\u00b7loh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VVFIN", "PPER", "PRF", "$.", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Denn thau und sonne sind vor einen garten nicht/", "tokens": ["Denn", "thau", "und", "son\u00b7ne", "sind", "vor", "ei\u00b7nen", "gar\u00b7ten", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "KON", "NN", "VAFIN", "APPR", "ART", "VVFIN", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Die besten mandeln nicht vor einen mund gebohren/", "tokens": ["Die", "bes\u00b7ten", "man\u00b7deln", "nicht", "vor", "ei\u00b7nen", "mund", "ge\u00b7boh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und kluge k\u00f6pffe nur vor alle zugericht.", "tokens": ["Und", "klu\u00b7ge", "k\u00f6pf\u00b7fe", "nur", "vor", "al\u00b7le", "zu\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "VVFIN", "ADV", "APPR", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Drum war dein meistes thun/ dem lande recht zu dienen/", "tokens": ["Drum", "war", "dein", "meis\u00b7tes", "thun", "/", "dem", "lan\u00b7de", "recht", "zu", "die\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPOSAT", "NN", "VVINF", "$(", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Dem oberherren treu/ den gleichen hold zu seyn/", "tokens": ["Dem", "o\u00b7ber\u00b7her\u00b7ren", "treu", "/", "den", "glei\u00b7chen", "hold", "zu", "seyn", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJD", "$(", "ART", "ADJA", "ADJD", "PTKZU", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Und unter schwei\u00df und m\u00fch/ wie arbeits-volle bienen/", "tokens": ["Und", "un\u00b7ter", "schwei\u00df", "und", "m\u00fch", "/", "wie", "ar\u00b7beits\u00b7vol\u00b7le", "bie\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJD", "KON", "ADJD", "$(", "KOKOM", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Die undanck-volle welt mit honig zu erfreun.", "tokens": ["Die", "un\u00b7dan\u00b7ck\u00b7vol\u00b7le", "welt", "mit", "ho\u00b7nig", "zu", "er\u00b7freun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.33": {"text": "Und warlich nicht umsonst. Denn rath und b\u00fcrger wissen/", "tokens": ["Und", "war\u00b7lich", "nicht", "um\u00b7sonst", ".", "Denn", "rath", "und", "b\u00fcr\u00b7ger", "wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "ADV", "$.", "KON", "NN", "KON", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Was deine redligkeit vor proben abgelegt/", "tokens": ["Was", "dei\u00b7ne", "red\u00b7lig\u00b7keit", "vor", "pro\u00b7ben", "ab\u00b7ge\u00b7legt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Was du vor knoten offt durch deinen witz zerrissen/", "tokens": ["Was", "du", "vor", "kno\u00b7ten", "offt", "durch", "dei\u00b7nen", "witz", "zer\u00b7ris\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und vor bescheidenheit den untern eingepr\u00e4gt.", "tokens": ["Und", "vor", "be\u00b7schei\u00b7den\u00b7heit", "den", "un\u00b7tern", "ein\u00b7ge\u00b7pr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "An palmen-b\u00e4umen ist sonst alles zu gebrauchen;", "tokens": ["An", "pal\u00b7men\u00b7b\u00e4u\u00b7men", "ist", "sonst", "al\u00b7les", "zu", "ge\u00b7brau\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ADV", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Dein gantzer lebens-baum war voller nutzbarkeit/", "tokens": ["Dein", "gant\u00b7zer", "le\u00b7bens\u00b7baum", "war", "vol\u00b7ler", "nutz\u00b7bar\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Und hat/ wenn andere wie Sodoms frucht verrauchen/", "tokens": ["Und", "hat", "/", "wenn", "an\u00b7de\u00b7re", "wie", "So\u00b7doms", "frucht", "ver\u00b7rau\u00b7chen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$(", "KOUS", "PIS", "KOKOM", "NE", "ADJD", "VVINF", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.40": {"text": "Die meisten \u00e4pffel offt beym kummer ausgestreut.", "tokens": ["Die", "meis\u00b7ten", "\u00e4pf\u00b7fel", "offt", "beym", "kum\u00b7mer", "aus\u00b7ge\u00b7streut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Was prei\u00df ich aber noch die kinder deiner sorgen?", "tokens": ["Was", "prei\u00df", "ich", "a\u00b7ber", "noch", "die", "kin\u00b7der", "dei\u00b7ner", "sor\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"