{"textgrid.poem.50727": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "3", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Lenz ist da, die Lauine f\u00e4llt,", "tokens": ["Der", "Lenz", "ist", "da", ",", "die", "Lau\u00b7i\u00b7ne", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Sie f\u00e4llt mit Tosen und Brausen ins Tal!", "tokens": ["Sie", "f\u00e4llt", "mit", "To\u00b7sen", "und", "Brau\u00b7sen", "ins", "Tal", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich habe mein H\u00fcttlein daneben gestellt", "tokens": ["Ich", "ha\u00b7be", "mein", "H\u00fctt\u00b7lein", "da\u00b7ne\u00b7ben", "ge\u00b7stellt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "PAV", "VVPP"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Auf gr\u00fcnende Matte am sonnigen Strahl!", "tokens": ["Auf", "gr\u00fc\u00b7nen\u00b7de", "Mat\u00b7te", "am", "son\u00b7ni\u00b7gen", "Strahl", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Und wenn die Lauine mein H\u00fcttlein trifft", "tokens": ["Und", "wenn", "die", "Lau\u00b7i\u00b7ne", "mein", "H\u00fctt\u00b7lein", "trifft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.2": {"text": "Und niederrei\u00dfet mit donnerndem Lauf,", "tokens": ["Und", "nie\u00b7der\u00b7rei\u00b7\u00dfet", "mit", "don\u00b7nern\u00b7dem", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sobald wieder trocken die Alpentrift,", "tokens": ["So\u00b7bald", "wie\u00b7der", "tro\u00b7cken", "die", "Al\u00b7pen\u00b7trift", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Bau ich ein neues mir singend auf!", "tokens": ["Bau", "ich", "ein", "neu\u00b7es", "mir", "sin\u00b7gend", "auf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Doch wenn einst in meines Landes Bann", "tokens": ["Doch", "wenn", "einst", "in", "mei\u00b7nes", "Lan\u00b7des", "Bann"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Erstarrend die Laue der Knechtschaft f\u00e4llt,", "tokens": ["Er\u00b7star\u00b7rend", "die", "Lau\u00b7e", "der", "Knecht\u00b7schaft", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "So z\u00fcnd ich die h\u00f6lzerne H\u00fctte an", "tokens": ["So", "z\u00fcnd", "ich", "die", "h\u00f6l\u00b7zer\u00b7ne", "H\u00fct\u00b7te", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "APPR"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und ziehe hinaus in die weite Welt!", "tokens": ["Und", "zie\u00b7he", "hin\u00b7aus", "in", "die", "wei\u00b7te", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-----+-+", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Denn lieber gepeitscht in Sibirien sein", "tokens": ["Denn", "lie\u00b7ber", "ge\u00b7peitscht", "in", "Si\u00b7bi\u00b7ri\u00b7en", "sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVPP", "APPR", "NE", "PPOSAT"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Als Herrendiener im Vaterland!", "tokens": ["Als", "Her\u00b7ren\u00b7die\u00b7ner", "im", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und lieber mich fremden Tyrannen weihn", "tokens": ["Und", "lie\u00b7ber", "mich", "frem\u00b7den", "Ty\u00b7ran\u00b7nen", "weihn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PPER", "ADJA", "NN", "VVINF"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Als meiner eigenen Heimat Schand!", "tokens": ["Als", "mei\u00b7ner", "ei\u00b7ge\u00b7nen", "Hei\u00b7mat", "Schand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Der Lenz ist da, die Lauine f\u00e4llt,", "tokens": ["Der", "Lenz", "ist", "da", ",", "die"