{"dta.poem.3772": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1892", "urn": "urn:nbn:de:kobv:b4-200905191524", "language": ["de:0.99"], "booktitle": "George, Stefan: Algabal. Paris u. a., 1892."}, "poem": {"stanza.1": {"line.1": {"text": "Sie herscht auf flacher kuppel unter sternen", "tokens": ["Sie", "herscht", "auf", "fla\u00b7cher", "kup\u00b7pel", "un\u00b7ter", "ster\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nach ohmen schnellen aus dem feuerbronne", "tokens": ["Nach", "oh\u00b7men", "schnel\u00b7len", "aus", "dem", "feu\u00b7er\u00b7bron\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Topase untermengt mit bernstein-kernen", "tokens": ["To\u00b7pa\u00b7se", "un\u00b7ter\u00b7mengt", "mit", "bern\u00b7stein\u00b7ker\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ADJA"], "meter": "+--+-+--+--", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "An allen seiten aufgereiht zu spiegeln", "tokens": ["An", "al\u00b7len", "sei\u00b7ten", "auf\u00b7ge\u00b7reiht", "zu", "spie\u00b7geln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVPP", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u2014 Gesamter st\u00e4dte ganzer Staaten beute \u2014", "tokens": ["Ge\u00b7sam\u00b7ter", "st\u00e4d\u00b7te", "gan\u00b7zer", "Staa\u00b7ten", "beu\u00b7te"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "VVFIN", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die ungeschm\u00fcckten platten goldnen ziegeln", "tokens": ["Die", "un\u00b7ge\u00b7schm\u00fcck\u00b7ten", "plat\u00b7ten", "gold\u00b7nen", "zie\u00b7geln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und an der erde breiten l\u00f6wenh\u00e4ute", "tokens": ["Und", "an", "der", "er\u00b7de", "brei\u00b7ten", "l\u00f6\u00b7wen\u00b7h\u00e4u\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nur nicht des Einen scharfen blick zu blenden", "tokens": ["Nur", "nicht", "des", "Ei\u00b7nen", "schar\u00b7fen", "blick", "zu", "blen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKNEG", "ART", "ART", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vermag die stechend grelle weltenkrone", "tokens": ["Ver\u00b7mag", "die", "ste\u00b7chend", "grel\u00b7le", "wel\u00b7ten\u00b7kro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und dreimal tausend schwere urnen spenden", "tokens": ["Und", "drei\u00b7mal", "tau\u00b7send", "schwe\u00b7re", "ur\u00b7nen", "spen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "CARD", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den geist von amber Weihrauch und zitrone", "tokens": ["Den", "geist", "von", "am\u00b7ber", "Weih\u00b7rauch", "und", "zit\u00b7ro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "KON", "VVFIN"], "meter": "-+-+-+--++-", "measure": "iambic.penta.relaxed"}}}}}