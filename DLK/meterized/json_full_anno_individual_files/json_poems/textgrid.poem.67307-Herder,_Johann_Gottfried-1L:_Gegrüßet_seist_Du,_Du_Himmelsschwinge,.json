{"textgrid.poem.67307": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gegr\u00fc\u00dfet seist Du, Du Himmelsschwinge,", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gegr\u00fc\u00dfet seist Du, Du Himmelsschwinge,", "tokens": ["Ge\u00b7gr\u00fc\u00b7\u00dfet", "seist", "Du", ",", "Du", "Him\u00b7mels\u00b7schwin\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "PPER", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Des Fr\u00fchlings Bote, Du Liederfreundin!", "tokens": ["Des", "Fr\u00fch\u00b7lings", "Bo\u00b7te", ",", "Du", "Lie\u00b7der\u00b7freun\u00b7din", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PPER", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sei mir gegr\u00fc\u00dfet, geliebte Lerche,", "tokens": ["Sei", "mir", "ge\u00b7gr\u00fc\u00b7\u00dfet", ",", "ge\u00b7lieb\u00b7te", "Ler\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Die Beides lehret, Gesang und Leben.", "tokens": ["Die", "Bei\u00b7des", "leh\u00b7ret", ",", "Ge\u00b7sang", "und", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Morgenr\u00f6the, des Flei\u00dfes Freundin,", "tokens": ["Der", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", ",", "des", "Flei\u00b7\u00dfes", "Freun\u00b7din", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Erweckst Du Felder, belebst Du Hirten;", "tokens": ["Er\u00b7weckst", "Du", "Fel\u00b7der", ",", "be\u00b7lebst", "Du", "Hir\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sie treiben munter den Schlaf vom Auge,", "tokens": ["Sie", "trei\u00b7ben", "mun\u00b7ter", "den", "Schlaf", "vom", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Denn ihnen singet die fr\u00fche Lerche.", "tokens": ["Denn", "ih\u00b7nen", "sin\u00b7get", "die", "fr\u00fc\u00b7he", "Ler\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Du st\u00e4rkst dem Landmann die Hand am Pfluge", "tokens": ["Du", "st\u00e4rkst", "dem", "Land\u00b7mann", "die", "Hand", "am", "Pflu\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und giebst den Ton ihm zum Morgenliede.", "tokens": ["Und", "giebst", "den", "Ton", "ihm", "zum", "Mor\u00b7gen\u00b7lie\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "\u00bbwach auf und singe, mein Herz, voll Freude!", "tokens": ["\u00bb", "wach", "auf", "und", "sin\u00b7ge", ",", "mein", "Herz", ",", "voll", "Freu\u00b7de", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADJD", "PTKVZ", "KON", "VVFIN", "$,", "PPOSAT", "NN", "$,", "ADJD", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wach auf und singe, mein Herz, voll Dankes!\u00ab", "tokens": ["Wach", "auf", "und", "sin\u00b7ge", ",", "mein", "Herz", ",", "voll", "Dan\u00b7kes", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PTKVZ", "KON", "VVFIN", "$,", "PPOSAT", "NN", "$,", "ADJD", "NN", "$.", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Und alle Sch\u00f6pfung, die Braut der Sonne,", "tokens": ["Und", "al\u00b7le", "Sch\u00f6p\u00b7fung", ",", "die", "Braut", "der", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Erwacht, verj\u00fcnget vom langen Schlafe.", "tokens": ["Er\u00b7wacht", ",", "ver\u00b7j\u00fcn\u00b7get", "vom", "lan\u00b7gen", "Schla\u00b7fe", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die starren B\u00e4ume, sie h\u00f6ren wundernd", "tokens": ["Die", "star\u00b7ren", "B\u00e4u\u00b7me", ",", "sie", "h\u00f6\u00b7ren", "wun\u00b7dernd"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "ADJD"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Gesang von oben und gr\u00fcnen wieder.", "tokens": ["Ge\u00b7sang", "von", "o\u00b7ben", "und", "gr\u00fc\u00b7nen", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADV", "KON", "VVFIN", "ADV", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Die Zweige sprie\u00dfen, die Bl\u00e4tter keimen,", "tokens": ["Die", "Zwei\u00b7ge", "sprie\u00b7\u00dfen", ",", "die", "Bl\u00e4t\u00b7ter", "kei\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das Laub entschl\u00fcpfet und horcht dem Liede.", "tokens": ["Das", "Laub", "ent\u00b7schl\u00fcp\u00b7fet", "und", "horcht", "dem", "Lie\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die V\u00f6gel girren im jungen Neste,", "tokens": ["Die", "V\u00f6\u00b7gel", "gir\u00b7ren", "im", "jun\u00b7gen", "Nes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie \u00fcben zweifelnd die alten Stimmen.", "tokens": ["Sie", "\u00fc\u00b7ben", "zwei\u00b7felnd", "die", "al\u00b7ten", "Stim\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Denn Du ermunterst sie, k\u00fchne Lerche,", "tokens": ["Denn", "Du", "er\u00b7mun\u00b7terst", "sie", ",", "k\u00fch\u00b7ne", "Ler\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Beim ersten Blicke des jungen Fr\u00fchlings,", "tokens": ["Beim", "ers\u00b7ten", "Bli\u00b7cke", "des", "jun\u00b7gen", "Fr\u00fch\u00b7lings", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Hoch \u00fcber Beifall und Neid erhoben,", "tokens": ["Hoch", "\u00fc\u00b7ber", "Bei\u00b7fall", "und", "Neid", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dem Aug' entflogen, doch stets im Ohre.", "tokens": ["Dem", "Aug'", "ent\u00b7flo\u00b7gen", ",", "doch", "stets", "im", "Oh\u00b7re", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Inbr\u00fcnstig schwingst Du Dich auf zum Himmel", "tokens": ["In\u00b7br\u00fcns\u00b7tig", "schwingst", "Du", "Dich", "auf", "zum", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "APPR", "APPRART", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und schl\u00fcpfst bescheiden zur Erde nieder;", "tokens": ["Und", "schl\u00fcpfst", "be\u00b7schei\u00b7den", "zur", "Er\u00b7de", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dem\u00fcthig nistest Du tief am Boden", "tokens": ["De\u00b7m\u00fct\u00b7hig", "nis\u00b7test", "Du", "tief", "am", "Bo\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ADJD", "APPRART", "NN"], "meter": "-+----+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Und steigst frohlockend zum Himmel wieder.", "tokens": ["Und", "steigst", "froh\u00b7lo\u00b7ckend", "zum", "Him\u00b7mel", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPRART", "NN", "ADV", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Drum gab, o fromme, bescheidne Lerche,", "tokens": ["Drum", "gab", ",", "o", "from\u00b7me", ",", "be\u00b7scheid\u00b7ne", "Ler\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "FM", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Du \u00fcber Beifall und Stolz erhobne,", "tokens": ["Du", "\u00fc\u00b7ber", "Bei\u00b7fall", "und", "Stolz", "er\u00b7hob\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Du muntre Freundin des fr\u00fchen Flei\u00dfes,", "tokens": ["Du", "mun\u00b7tre", "Freun\u00b7din", "des", "fr\u00fc\u00b7hen", "Flei\u00b7\u00dfes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Drum gab der Himmel Dir auch zum Lohne", "tokens": ["Drum", "gab", "der", "Him\u00b7mel", "Dir", "auch", "zum", "Loh\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "NN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Die unerm\u00fcdlich beherzte Stimme,", "tokens": ["Die", "un\u00b7er\u00b7m\u00fcd\u00b7lich", "be\u00b7herz\u00b7te", "Stim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den Ton der Freude, den langen Fr\u00fchling.", "tokens": ["Den", "Ton", "der", "Freu\u00b7de", ",", "den", "lan\u00b7gen", "Fr\u00fch\u00b7ling", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Selbst Philomele, die Liederg\u00f6ttin,", "tokens": ["Selbst", "Phi\u00b7lo\u00b7me\u00b7le", ",", "die", "Lie\u00b7der\u00b7g\u00f6t\u00b7tin", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NE", "$,", "ART", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Mu\u00df Deinem langen Gesange weichen.", "tokens": ["Mu\u00df", "Dei\u00b7nem", "lan\u00b7gen", "Ge\u00b7san\u00b7ge", "wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "Denn, ach! der Liebe, der Sehnsucht Klagen", "tokens": ["Denn", ",", "ach", "!", "der", "Lie\u00b7be", ",", "der", "Sehn\u00b7sucht", "Kla\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word