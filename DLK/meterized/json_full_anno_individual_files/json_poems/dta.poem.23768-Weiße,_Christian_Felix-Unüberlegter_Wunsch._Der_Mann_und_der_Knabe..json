{"dta.poem.23768": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "Un\u00fcberlegter Wunsch.  \n Der Mann und der Knabe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20643-5", "language": ["de:0.99"], "booktitle": "Wei\u00dfe, Christian Felix: Lieder f\u00fcr Kinder. Leipzig, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Die\u00df braune Pferd &#8212; welch sch\u00f6nes", "tokens": ["Die\u00df", "brau\u00b7ne", "Pferd", "&#8212;", "welch", "sch\u00f6\u00b7nes"], "token_info": ["word", "word", "word", "XML_entity", "word", "word"], "pos": ["PDS", "VVFIN", "NN", "$(", "PWAT", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Thier!", "tokens": ["Thier", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.3": {"text": "O lieber Mann, erlaubet mir", "tokens": ["O", "lie\u00b7ber", "Mann", ",", "er\u00b7lau\u00b7bet", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "ADV", "NN", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein wenig drauf herum zu traben,", "tokens": ["Ein", "we\u00b7nig", "drauf", "he\u00b7rum", "zu", "tra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "PAV", "APZR", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Was wollt ich nicht f\u00fcr Freude haben!", "tokens": ["Was", "wollt", "ich", "nicht", "f\u00fcr", "Freu\u00b7de", "ha\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PTKNEG", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Pr\u00fcf deine Kr\u00e4fte dich zuvor,", "tokens": ["Pr\u00fcf", "dei\u00b7ne", "Kr\u00e4f\u00b7te", "dich", "zu\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Eh du was w\u00fcnschest, kleiner Thor!", "tokens": ["Eh", "du", "was", "w\u00fcn\u00b7schest", ",", "klei\u00b7ner", "Thor", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Weist du ein Pferd auch zu regieren,", "tokens": ["Weist", "du", "ein", "Pferd", "auch", "zu", "re\u00b7gie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Um nicht dein Leben zu verlieren?", "tokens": ["Um", "nicht", "dein", "Le\u00b7ben", "zu", "ver\u00b7lie\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PTKNEG", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}