{"dta.poem.21879": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "XxXV,", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Hier ist dein \u00f6des Bette nicht/", "tokens": ["Hier", "ist", "dein", "\u00f6\u00b7des", "Bet\u00b7te", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "PTKNEG", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "hier darffstu/ Dulus/ mir nicht dienen.", "tokens": ["hier", "darffs\u00b7tu", "/", "Du\u00b7lus", "/", "mir", "nicht", "die\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "$(", "NE", "$(", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Seht wie uns kan der Schlaaff erk\u00fch-", "tokens": ["Seht", "wie", "uns", "kan", "der", "Schlaaff", "er\u00b7k\u00fch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "PPER", "VMFIN", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es greifft mir Dulus ins Gesicht\u2019/", "tokens": ["Es", "greifft", "mir", "Du\u00b7lus", "ins", "Ge\u00b7sicht'", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NE", "APPRART", "NN", "$("], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "ich schweige still/ und bin zu frieden", "tokens": ["ich", "schwei\u00b7ge", "still", "/", "und", "bin", "zu", "frie\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$(", "KON", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "da\u00df ihn der Traum und Nacht verblendt", "tokens": ["da\u00df", "ihn", "der", "Traum", "und", "Nacht", "ver\u00b7blendt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "KON", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "ach! w\u00fcrde doch so einem Jeden", "tokens": ["ach", "!", "w\u00fcr\u00b7de", "doch", "so", "ei\u00b7nem", "Je\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "VAFIN", "ADV", "ADV", "ART", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "sein sch\u00e4ler Eyfer abgewendt/", "tokens": ["sein", "sch\u00e4\u00b7ler", "Ey\u00b7fer", "ab\u00b7ge\u00b7wendt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}