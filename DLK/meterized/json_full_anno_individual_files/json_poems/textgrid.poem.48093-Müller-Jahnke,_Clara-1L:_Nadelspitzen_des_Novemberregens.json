{"textgrid.poem.48093": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nadelspitzen des Novemberregens", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nadelspitzen des Novemberregens", "tokens": ["Na\u00b7del\u00b7spit\u00b7zen", "des", "No\u00b7vem\u00b7ber\u00b7re\u00b7gens"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "peitscht der Nordwind prasselnd an die Scheiben,", "tokens": ["peitscht", "der", "Nord\u00b7wind", "pras\u00b7selnd", "an", "die", "Schei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "aber warm und mollig ist's hier drinnen.", "tokens": ["a\u00b7ber", "warm", "und", "mol\u00b7lig", "ist's", "hier", "drin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VAFIN", "ADV", "ADV", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Zigarettenduft durchwellt das Zimmer,", "tokens": ["Zi\u00b7ga\u00b7ret\u00b7ten\u00b7duft", "durch\u00b7wellt", "das", "Zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "in den Gl\u00e4sern flammt der Sorrentiner.", "tokens": ["in", "den", "Gl\u00e4\u00b7sern", "flammt", "der", "Sor\u00b7ren\u00b7ti\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und du hebst das Glas, und l\u00e4chelnd trinken,", "tokens": ["Und", "du", "hebst", "das", "Glas", ",", "und", "l\u00e4\u00b7chelnd", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,", "KON", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "langsam schl\u00fcrfend, tropfenweise trinken", "tokens": ["lang\u00b7sam", "schl\u00fcr\u00b7fend", ",", "trop\u00b7fen\u00b7wei\u00b7se", "trin\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJD", "ADJD", "$,", "ADJD", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "wir das Herzblut einer hei\u00dfern Sonne.", "tokens": ["wir", "das", "Herz\u00b7blut", "ei\u00b7ner", "hei\u00b7\u00dfern", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Setz dich zu mir, komm!", "tokens": ["Setz", "dich", "zu", "mir", ",", "komm", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "$,", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.10": {"text": "Auf Deine Schulter", "tokens": ["Auf", "Dei\u00b7ne", "Schul\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "la\u00df die sehnsuchtfeuchte Stirn mich st\u00fctzen,", "tokens": ["la\u00df", "die", "sehn\u00b7sucht\u00b7feuch\u00b7te", "Stirn", "mich", "st\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "lauschen la\u00df mich deinen wachen Tr\u00e4umen,", "tokens": ["lau\u00b7schen", "la\u00df", "mich", "dei\u00b7nen", "wa\u00b7chen", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVINF", "VVIMP", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "deinen M\u00e4rchen aus dem Reich der Sonne,", "tokens": ["dei\u00b7nen", "M\u00e4r\u00b7chen", "aus", "dem", "Reich", "der", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "deinen Liedern von der goldnen Katie . . .", "tokens": ["dei\u00b7nen", "Lie\u00b7dern", "von", "der", "gold\u00b7nen", "Ka\u00b7tie", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$.", "$.", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.15": {"text": "Lies mir, Liebster, von der goldnen Katie!", "tokens": ["Lies", "mir", ",", "Liebs\u00b7ter", ",", "von", "der", "gold\u00b7nen", "Ka\u00b7tie", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.2": {"line.1": {"text": "Linde heilend will ich mit den Fingern", "tokens": ["Lin\u00b7de", "hei\u00b7lend", "will", "ich", "mit", "den", "Fin\u00b7gern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "VMFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "deiner Stirne blutige Male r\u00fchren,", "tokens": ["dei\u00b7ner", "Stir\u00b7ne", "blu\u00b7ti\u00b7ge", "Ma\u00b7le", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "d\u00fcrstend k\u00fc\u00df ich alle deine Wunden,", "tokens": ["d\u00fcrs\u00b7tend", "k\u00fc\u00df", "ich", "al\u00b7le", "dei\u00b7ne", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PIAT", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "k\u00fc\u00df sie zu mit meinen weichen Lippen.", "tokens": ["k\u00fc\u00df", "sie", "zu", "mit", "mei\u00b7nen", "wei\u00b7chen", "Lip\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Lies mir, Liebster, von der goldnen Katie.", "tokens": ["Lies", "mir", ",", "Liebs\u00b7ter", ",", "von", "der", "gold\u00b7nen", "Ka\u00b7tie", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.3": {"line.1": {"text": "Lies mir deine allerblausten Tr\u00e4ume", "tokens": ["Lies", "mir", "dei\u00b7ne", "al\u00b7ler\u00b7blaus\u00b7ten", "Tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "aus den Zaubergrotten von Caprera,", "tokens": ["aus", "den", "Zau\u00b7ber\u00b7grot\u00b7ten", "von", "Cap\u00b7re\u00b7ra", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NE", "$,"], "meter": "--+-+--+-+", "measure": "iambic.te