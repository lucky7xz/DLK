{"dta.poem.7319": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Der Traum.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "J\u00fcngst hab' ich dich gesehn im Traum,", "tokens": ["J\u00fcngst", "hab'", "ich", "dich", "ge\u00b7sehn", "im", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So lieblich sa\u00dfest du beh\u00fctet,", "tokens": ["So", "lieb\u00b7lich", "sa\u00b7\u00dfest", "du", "be\u00b7h\u00fc\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In einer Laube gr\u00fcnem Raum,", "tokens": ["In", "ei\u00b7ner", "Lau\u00b7be", "gr\u00fc\u00b7nem", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von duftendem Jasmin umbl\u00fcthet,", "tokens": ["Von", "duf\u00b7ten\u00b7dem", "Jas\u00b7min", "um\u00b7bl\u00fc\u00b7thet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Durch Zweige fiel das goldne Licht,", "tokens": ["Durch", "Zwei\u00b7ge", "fiel", "das", "gold\u00b7ne", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Aus Vogelkehlen ward gesungen,", "tokens": ["Aus", "Vo\u00b7gel\u00b7keh\u00b7len", "ward", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Du sa\u00dfest da, wie ein Gedicht", "tokens": ["Du", "sa\u00b7\u00dfest", "da", ",", "wie", "ein", "Ge\u00b7dicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Von einem Blumenkranz umschlungen.", "tokens": ["Von", "ei\u00b7nem", "Blu\u00b7men\u00b7kranz", "um\u00b7schlun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und deine liebe Rechte trug", "tokens": ["Und", "dei\u00b7ne", "lie\u00b7be", "Rech\u00b7te", "trug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Antlitz mit so edlen Sitten,", "tokens": ["Das", "Ant\u00b7litz", "mit", "so", "ed\u00b7len", "Sit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Im Sand das aufgeschlagne Buch", "tokens": ["Im", "Sand", "das", "auf\u00b7ge\u00b7schlag\u00b7ne", "Buch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schien von dem Schoo\u00dfe dir geglitten;", "tokens": ["Schien", "von", "dem", "Schoo\u00b7\u00dfe", "dir", "ge\u00b7glit\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PPER", "VVPP", "$."], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dich lehnend an den frischen Haag", "tokens": ["Dich", "leh\u00b7nend", "an", "den", "fri\u00b7schen", "Haag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hauchtest du fl\u00fcsternd leise K\u00fcsse,", "tokens": ["Hauch\u00b7test", "du", "fl\u00fcs\u00b7ternd", "lei\u00b7se", "K\u00fcs\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Im Auge eine Thr\u00e4ne lag", "tokens": ["Im", "Au\u00b7ge", "ei\u00b7ne", "Thr\u00e4\u00b7ne", "lag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wie Thau im Kelche der Narzisse.", "tokens": ["Wie", "Thau", "im", "Kel\u00b7che", "der", "Nar\u00b7zis\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dich anzuschaun war meine Lust,", "tokens": ["Dich", "an\u00b7zu\u00b7schaun", "war", "mei\u00b7ne", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVIZU", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu lauschen deiner Z\u00fcge Regen,", "tokens": ["Zu", "lau\u00b7schen", "dei\u00b7ner", "Z\u00fc\u00b7ge", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und dennoch h\u00e4tt' ich gern gewu\u00dft,", "tokens": ["Und", "den\u00b7noch", "h\u00e4tt'", "ich", "gern", "ge\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was dich so innig mocht' bewegen?", "tokens": ["Was", "dich", "so", "in\u00b7nig", "mocht'", "be\u00b7we\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da bogst du sacht hinab den Zweig,", "tokens": ["Da", "bogst", "du", "sacht", "hin\u00b7ab", "den", "Zweig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Strichst l\u00e4chelnd an der Spitzenhaube,", "tokens": ["Strichst", "l\u00e4\u00b7chelnd", "an", "der", "Spit\u00b7zen\u00b7hau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "An deine Schulter huscht' ich gleich,", "tokens": ["An", "dei\u00b7ne", "Schul\u00b7ter", "huscht'", "ich", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sah einen Baum in schlichtem Laube:", "tokens": ["Sah", "ei\u00b7nen", "Baum", "in", "schlich\u00b7tem", "Lau\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und auf dem Baume sa\u00df ein Fink,", "tokens": ["Und", "auf", "dem", "Bau\u00b7me", "sa\u00df", "ein", "Fink", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der schleppte d\u00fcrres Moos und Reisig,", "tokens": ["Der", "schlepp\u00b7te", "d\u00fcr\u00b7res", "Moos", "und", "Rei\u00b7sig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "KON", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "\u201eschau her, schau wieder!\u201c zirpt' er flink", "tokens": ["\u201e", "schau", "her", ",", "schau", "wie\u00b7der", "!", "\u201c", "zirpt'", "er", "flink"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["$(", "ADJD", "PTKVZ", "$,", "ADJD", "ADV", "$.", "$(", "VVFIN", "PPER", "VVFIN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Und f\u00f6rderte am Nestchen flei\u00dfig;", "tokens": ["Und", "f\u00f6r\u00b7der\u00b7te", "am", "Nest\u00b7chen", "flei\u00b7\u00dfig", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Er sah so keck und fr\u00f6hlich aus,", "tokens": ["Er", "sah", "so", "keck", "und", "fr\u00f6h\u00b7lich", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als tr\u00fcg er des Flamingo Kleider,", "tokens": ["Als", "tr\u00fcg", "er", "des", "Fla\u00b7min\u00b7go", "Klei\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So sorglich h\u00fcpft er um sein Haus,", "tokens": ["So", "sorg\u00b7lich", "h\u00fcpft", "er", "um", "sein", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Als f\u00fcrcht' er b\u00f6sen Blick und Neider.", "tokens": ["Als", "f\u00fcrcht'", "er", "b\u00f6\u00b7sen", "Blick", "und", "Nei\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und wenn ein Reischen er gelegt,", "tokens": ["Und", "wenn", "ein", "Rei\u00b7schen", "er", "ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dann rief er alle Welt zu Zeugen,", "tokens": ["Dann", "rief", "er", "al\u00b7le", "Welt", "zu", "Zeu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Als m\u00fcsse was der Garten hegt,", "tokens": ["Als", "m\u00fcs\u00b7se", "was", "der", "Gar\u00b7ten", "hegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PWS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Blum' und Gestr\u00e4uch sich vor ihm neigen;", "tokens": ["Blum'", "und", "Ge\u00b7str\u00e4uch", "sich", "vor", "ihm", "nei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Um deine Lippe flog ein Zug,", "tokens": ["Um", "dei\u00b7ne", "Lip\u00b7pe", "flog", "ein", "Zug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie ich ihn oft an ihr gesehen,", "tokens": ["Wie", "ich", "ihn", "oft", "an", "ihr", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und meinen Namen lie\u00df im Flug", "tokens": ["Und", "mei\u00b7nen", "Na\u00b7men", "lie\u00df", "im", "Flug"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sie \u00fcber ihre Spalte gehen.", "tokens": ["Sie", "\u00fc\u00b7ber", "ih\u00b7re", "Spal\u00b7te", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Schon hob ich meine Hand hinauf", "tokens": ["Schon", "hob", "ich", "mei\u00b7ne", "Hand", "hin\u00b7auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit leisem Schlage dich zu strafen,", "tokens": ["Mit", "lei\u00b7sem", "Schla\u00b7ge", "dich", "zu", "stra\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Allein da wacht ich pl\u00f6tzlich auf", "tokens": ["Al\u00b7lein", "da", "wacht", "ich", "pl\u00f6tz\u00b7lich", "auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADJD", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und bin nicht wieder eingeschlafen;", "tokens": ["Und", "bin", "nicht", "wie\u00b7der", "ein\u00b7ge\u00b7schla\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nur deiner hab' ich fortgedacht,", "tokens": ["Nur", "dei\u00b7ner", "hab'", "ich", "fort\u00b7ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "S\u00e4h dich so gern am gr\u00fcnen Haage,", "tokens": ["S\u00e4h", "dich", "so", "gern", "am", "gr\u00fc\u00b7nen", "Haa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Mich d\u00fcnkt, so lieb wie in der Nacht", "tokens": ["Mich", "d\u00fcnkt", ",", "so", "lieb", "wie", "in", "der", "Nacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ADV", "ADJD", "KOKOM", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sah ich dich noch an keinem Tage.", "tokens": ["Sah", "ich", "dich", "noch", "an", "kei\u00b7nem", "Ta\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Im Eise schlummern Blum' und Zweig,", "tokens": ["Im", "Ei\u00b7se", "schlum\u00b7mern", "Blum'", "und", "Zweig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dezemberwinde schneidend wehen,", "tokens": ["De\u00b7zem\u00b7ber\u00b7win\u00b7de", "schnei\u00b7dend", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Garten steht im Wolkenreich,", "tokens": ["Der", "Gar\u00b7ten", "steht", "im", "Wol\u00b7ken\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo tausend sch\u00f6nre G\u00e4rten stehen;", "tokens": ["Wo", "tau\u00b7send", "sch\u00f6n\u00b7re", "G\u00e4r\u00b7ten", "ste\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "CARD", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So golden ist kein Sonnenschein,", "tokens": ["So", "gol\u00b7den", "ist", "kein", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df er wie der ertr\u00e4umte blinke;", "tokens": ["Da\u00df", "er", "wie", "der", "er\u00b7tr\u00e4um\u00b7te", "blin\u00b7ke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOKOM", "ART", "ADJA", "VVFIN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Doch du, bist du nicht wirklich mein?", "tokens": ["Doch", "du", ",", "bist", "du", "nicht", "wirk\u00b7lich", "mein", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "VAFIN", "PPER", "PTKNEG", "ADJD", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und bin ich nicht dein dummer Finke?", "tokens": ["Und", "bin", "ich", "nicht", "dein", "dum\u00b7mer", "Fin\u00b7ke", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}