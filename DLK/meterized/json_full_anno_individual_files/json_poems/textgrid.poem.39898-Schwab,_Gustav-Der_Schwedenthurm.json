{"textgrid.poem.39898": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Der Schwedenthurm", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu W\u00fcrzburg steht ein grauer Thurm", "tokens": ["Zu", "W\u00fcrz\u00b7burg", "steht", "ein", "grau\u00b7er", "Thurm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weitab vom lust'gen Maine,", "tokens": ["Weit\u00b7ab", "vom", "lust'\u00b7gen", "Mai\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In seinen Balken pickt der Wurm,", "tokens": ["In", "sei\u00b7nen", "Bal\u00b7ken", "pickt", "der", "Wurm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es nagt das Moos am Steine.", "tokens": ["Es", "nagt", "das", "Moos", "am", "Stei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die hohle Brust durchr\u00f6chelt schwach", "tokens": ["Die", "hoh\u00b7le", "Brust", "durch\u00b7r\u00f6\u00b7chelt", "schwach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein rostig Uhrwerk st\u00f6hnend,", "tokens": ["Ein", "ros\u00b7tig", "Uhr\u00b7werk", "st\u00f6h\u00b7nend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sein Stundenschlag ist auch noch wach,", "tokens": ["Sein", "Stun\u00b7den\u00b7schlag", "ist", "auch", "noch", "wach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch nur die Zeit verh\u00f6hnend.", "tokens": ["Doch", "nur", "die", "Zeit", "ver\u00b7h\u00f6h\u00b7nend", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Denn wenn die Glocken alle ruhn", "tokens": ["Denn", "wenn", "die", "Glo\u00b7cken", "al\u00b7le", "ruhn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "PIS", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Viertel vor der Stunde,", "tokens": ["Ein", "Vier\u00b7tel", "vor", "der", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Beginnt er ein verkehrtes Thun", "tokens": ["Be\u00b7ginnt", "er", "ein", "ver\u00b7kehr\u00b7tes", "Thun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit eh'rnem L\u00fcgenmunde.", "tokens": ["Mit", "eh'r\u00b7nem", "L\u00fc\u00b7gen\u00b7mun\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ob seinem fr\u00fchen Schlage qu\u00e4lt", "tokens": ["Ob", "sei\u00b7nem", "fr\u00fc\u00b7hen", "Schla\u00b7ge", "qu\u00e4lt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich, was auf M\u00e4rkten handelt,", "tokens": ["Sich", ",", "was", "auf", "M\u00e4rk\u00b7ten", "han\u00b7delt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Kranke, der die Stunden z\u00e4hlt,", "tokens": ["Der", "Kran\u00b7ke", ",", "der", "die", "Stun\u00b7den", "z\u00e4hlt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Reisende, der wandelt.", "tokens": ["Der", "Rei\u00b7sen\u00b7de", ",", "der", "wan\u00b7delt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wie dulden es die St\u00e4dter nur,", "tokens": ["Wie", "dul\u00b7den", "es", "die", "St\u00e4d\u00b7ter", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Tr\u00fcger stets zu h\u00f6ren?", "tokens": ["Den", "Tr\u00fc\u00b7ger", "stets", "zu", "h\u00f6\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So wi\u00dft: sie m\u00f6gen seiner Uhr", "tokens": ["So", "wi\u00dft", ":", "sie", "m\u00f6\u00b7gen", "sei\u00b7ner", "Uhr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$.", "PPER", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den alten Fluch nicht st\u00f6ren.", "tokens": ["Den", "al\u00b7ten", "Fluch", "nicht", "st\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Denn in dem drei\u00dfigj\u00e4hr'gen Sturm,", "tokens": ["Denn", "in", "dem", "drei\u00b7\u00dfig\u00b7j\u00e4hr'\u00b7gen", "Sturm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im langen Jammerkriege,", "tokens": ["Im", "lan\u00b7gen", "Jam\u00b7mer\u00b7krie\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da war der falsche Schwedenthurm", "tokens": ["Da", "war", "der", "fal\u00b7sche", "Schwe\u00b7den\u00b7thurm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Einst eines Greuels Wiege.", "tokens": ["Einst", "ei\u00b7nes", "Greu\u00b7els", "Wie\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Verschw\u00f6rer sa\u00dfen dort versteckt", "tokens": ["Ver\u00b7schw\u00f6\u00b7rer", "sa\u00b7\u00dfen", "dort", "ver\u00b7steckt"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In seiner Glockenstube;", "tokens": ["In", "sei\u00b7ner", "Glo\u00b7cken\u00b7stu\u00b7be", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein dumpfer Streich ward ausgeheckt", "tokens": ["Ein", "dum\u00b7pfer", "Streich", "ward", "aus\u00b7ge\u00b7heckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In luft'ger M\u00f6rdergrube.", "tokens": ["In", "luft'\u00b7ger", "M\u00f6r\u00b7der\u00b7gru\u00b7be", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Als drauf die Stadt voll Frieden schlief,", "tokens": ["Als", "drauf", "die", "Stadt", "voll", "Frie\u00b7den", "schlief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PAV", "ART", "NN", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die unbewehrte Rechte", "tokens": ["Die", "un\u00b7be\u00b7wehr\u00b7te", "Rech\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In sichrem Schlummer senkten tief", "tokens": ["In", "sich\u00b7rem", "Schlum\u00b7mer", "senk\u00b7ten", "tief"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Reiches treue Knechte,", "tokens": ["Des", "Rei\u00b7ches", "treu\u00b7e", "Knech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ein Viertel hub vor Mitternacht", "tokens": ["Ein", "Vier\u00b7tel", "hub", "vor", "Mit\u00b7ter\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Thurm an irr zu reden:", "tokens": ["Der", "Thurm", "an", "irr", "zu", "re\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zw\u00f6lf Schl\u00e4ge dr\u00f6hnten da mit Macht,", "tokens": ["Zw\u00f6lf", "Schl\u00e4\u00b7ge", "dr\u00f6hn\u00b7ten", "da", "mit", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Laut riefen sie dem Schweden.", "tokens": ["Laut", "rie\u00b7fen", "sie", "dem", "Schwe\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "ART", "NE", "$."], "meter": "++-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Und der verstand das Zeichen wohl,", "tokens": ["Und", "der", "ver\u00b7stand", "das", "Zei\u00b7chen", "wohl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Pf\u00f6rtlein fand er offen,", "tokens": ["Ein", "Pf\u00f6rt\u00b7lein", "fand", "er", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Blut in allen Kammern quoll,", "tokens": ["Das", "Blut", "in", "al\u00b7len", "Kam\u00b7mern", "quoll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Schlummerkissen troffen.", "tokens": ["Die", "Schlum\u00b7mer\u00b7kis\u00b7sen", "trof\u00b7fen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Der Strom empfing, als tiefes Grab,", "tokens": ["Der", "Strom", "emp\u00b7fing", ",", "als", "tie\u00b7fes", "Grab", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Leichen schwer Ger\u00f6lle;", "tokens": ["Der", "Lei\u00b7chen", "schwer", "Ge\u00b7r\u00f6l\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch Jubel soll vom Thurm herab,", "tokens": ["Doch", "Ju\u00b7bel", "soll", "vom", "Thurm", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VMFIN", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hoch oben jauchzt die H\u00f6lle.", "tokens": ["Hoch", "o\u00b7ben", "jauchzt", "die", "H\u00f6l\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Ihr Sieg war kurz, ihr Stachel ward", "tokens": ["Ihr", "Sieg", "war", "kurz", ",", "ihr", "Sta\u00b7chel", "ward"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geknickt durch schnelle Rache;", "tokens": ["Ge\u00b7knickt", "durch", "schnel\u00b7le", "Ra\u00b7che", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dem Thurm verr\u00e4terischer Art", "tokens": ["Dem", "Thurm", "ver\u00b7r\u00e4\u00b7te\u00b7ri\u00b7scher", "Art"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Lie\u00df man des Truges Sprache.", "tokens": ["Lie\u00df", "man", "des", "Tru\u00b7ges", "Spra\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.13": {"line.1": {"text": "Im R\u00e4derwerk der Wahnsinn knarrt;", "tokens": ["Im", "R\u00e4\u00b7der\u00b7werk", "der", "Wahn\u00b7sinn", "knarrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So steht er grau, zerfallen;", "tokens": ["So", "steht", "er", "grau", ",", "zer\u00b7fal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mu\u00df, bis man ihn als Schutt verscharrt,", "tokens": ["Mu\u00df", ",", "bis", "man", "ihn", "als", "Schutt", "ver\u00b7scharrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "KOUS", "PIS", "PPER", "KOUS", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von seiner S\u00fcnde lallen. \u2013", "tokens": ["Von", "sei\u00b7ner", "S\u00fcn\u00b7de", "lal\u00b7len", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "