{"dta.poem.19051": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "XxXII.  Auf seinen erstgebohrnen Sohn,  \n Christian Ernsten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Njmms wieder hin, du hattest es gegeben,", "tokens": ["Njmms", "wie\u00b7der", "hin", ",", "du", "hat\u00b7test", "es", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PTKVZ", "$,", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nimm, ", "tokens": ["Nimm", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Du wilst uns gern der M\u00fche \u00fcberheben,", "tokens": ["Du", "wilst", "uns", "gern", "der", "M\u00fc\u00b7he", "\u00fc\u00b7ber\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der schweren Pflicht, der Sorge, die uns band.", "tokens": ["Der", "schwe\u00b7ren", "Pflicht", ",", "der", "Sor\u00b7ge", ",", "die", "uns", "band", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die zarten Lippen regten sich noch schwach,", "tokens": ["Die", "zar\u00b7ten", "Lip\u00b7pen", "reg\u00b7ten", "sich", "noch", "schwach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Das andre Thun bestand in Kleinigkeit,", "tokens": ["Das", "and\u00b7re", "Thun", "be\u00b7stand", "in", "Klei\u00b7nig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Es machte sich mit seiner Kunst nicht breit;", "tokens": ["Es", "mach\u00b7te", "sich", "mit", "sei\u00b7ner", "Kunst", "nicht", "breit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wenn die Vernunft was drein zu reden taugte,", "tokens": ["Wenn", "die", "Ver\u00b7nunft", "was", "drein", "zu", "re\u00b7den", "taug\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PWS", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So spr\u00e4che sie: Warum denn nun so bald?", "tokens": ["So", "spr\u00e4\u00b7che", "sie", ":", "Wa\u00b7rum", "denn", "nun", "so", "bald", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "PWAV", "ADV", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denn wenn der Mensch nicht Zeit zur Arbeit brauchte;", "tokens": ["Denn", "wenn", "der", "Mensch", "nicht", "Zeit", "zur", "Ar\u00b7beit", "brauch\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PTKNEG", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So w\u00fcrde ja viel lieber niemand alt.", "tokens": ["So", "w\u00fcr\u00b7de", "ja", "viel", "lie\u00b7ber", "nie\u00b7mand", "alt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "ADV", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch da die Eltern vor dir freudig seyn,", "tokens": ["Doch", "da", "die", "El\u00b7tern", "vor", "dir", "freu\u00b7dig", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "PPER", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df sie ihr Kind dir lediglich geweyht;", "tokens": ["Da\u00df", "sie", "ihr", "Kind", "dir", "le\u00b7di\u00b7glich", "ge\u00b7weyht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So ist ihr Ja! zu jeden Winck bereit,", "tokens": ["So", "ist", "ihr", "Ja", "!", "zu", "je\u00b7den", "Win\u00b7ck", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$.", "APPR", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.3": {"line.1": {"text": "Dir kan der Tod des S\u00fcnders nicht belieben,", "tokens": ["Dir", "kan", "der", "Tod", "des", "S\u00fcn\u00b7ders", "nicht", "be\u00b7lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das glaubet die Vernunft; doch glaubet sie,", "tokens": ["Das", "glau\u00b7bet", "die", "Ver\u00b7nunft", ";", "doch", "glau\u00b7bet", "sie", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$.", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du habest itzt ein ", "tokens": ["Du", "ha\u00b7best", "itzt", "ein"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ART"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Ein Rei\u00df, gepflantzt durch deiner H\u00e4nde M\u00fch.", "tokens": ["Ein", "Rei\u00df", ",", "ge\u00b7pflantzt", "durch", "dei\u00b7ner", "H\u00e4n\u00b7de", "M\u00fch", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "APPR", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wenn die Vernunft nicht eine Th\u00f6rin w\u00e4r;", "tokens": ["Wenn", "die", "Ver\u00b7nunft", "nicht", "ei\u00b7ne", "Th\u00f6\u00b7rin", "w\u00e4r", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Sie glaubte so was ungereimtes nicht:", "tokens": ["Sie", "glaub\u00b7te", "so", "was", "un\u00b7ge\u00b7reim\u00b7tes", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PWS", "ADJA", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da\u00df der, dems Hertz von Feindes Liebe bricht,", "tokens": ["Da\u00df", "der", ",", "dems", "Hertz", "von", "Fein\u00b7des", "Lie\u00b7be", "bricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "$,", "ART", "NN", "APPR", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wir haben uns einander nicht gewehlet,", "tokens": ["Wir", "ha\u00b7ben", "uns", "ein\u00b7an\u00b7der", "nicht", "ge\u00b7weh\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PRF", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du hast es selbst gantz offenbar gef\u00fcgt;", "tokens": ["Du", "hast", "es", "selbst", "gantz", "of\u00b7fen\u00b7bar", "ge\u00b7f\u00fcgt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So waren wir, nachdem du uns verm\u00e4hlet,", "tokens": ["So", "wa\u00b7ren", "wir", ",", "nach\u00b7dem", "du", "uns", "ver\u00b7m\u00e4h\u00b7let", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In deinem Schoo\u00df auch ohne Kind vergn\u00fcgt.", "tokens": ["In", "dei\u00b7nem", "Schoo\u00df", "auch", "oh\u00b7ne", "Kind", "ver\u00b7gn\u00fcgt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Du gabest es, und hast es oft bewahrt:", "tokens": ["Du", "ga\u00b7best", "es", ",", "und", "hast", "es", "oft", "be\u00b7wahrt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KON", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Es langte hier, als wie ein Wunder an,", "tokens": ["Es", "lang\u00b7te", "hier", ",", "als", "wie", "ein", "Wun\u00b7der", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "KOKOM", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Das zeugen die, so seine Ankunft sahn,", "tokens": ["Das", "zeu\u00b7gen", "die", ",", "so", "sei\u00b7ne", "An\u00b7kunft", "sahn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "$,", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wir merckens wol, du unergr\u00fcndte Liebe!", "tokens": ["Wir", "mer\u00b7ckens", "wol", ",", "du", "un\u00b7er\u00b7gr\u00fcnd\u00b7te", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir halten nichts aufs Trauren dieser Welt.", "tokens": ["Wir", "hal\u00b7ten", "nichts", "aufs", "Trau\u00b7ren", "die\u00b7ser", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPRART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gefallen dir des Kindes zarte Triebe,", "tokens": ["Ge\u00b7fal\u00b7len", "dir", "des", "Kin\u00b7des", "zar\u00b7te", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dir, deme man in Christo leicht gef\u00e4llt;", "tokens": ["Dir", ",", "de\u00b7me", "man", "in", "Chris\u00b7to", "leicht", "ge\u00b7f\u00e4llt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PIS", "APPR", "NE", "ADJD", "VVPP", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Ist unsrer Erst-Geburt dein Hertz verg\u00f6nnt;", "tokens": ["Ist", "uns\u00b7rer", "Er\u00b7stGe\u00b7burt", "dein", "Hertz", "ver\u00b7g\u00f6nnt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wir d\u00fcrffen dir das Kind nicht erstlich loben,", "tokens": ["Wir", "d\u00fcrf\u00b7fen", "dir", "das", "Kind", "nicht", "erst\u00b7lich", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ART", "NN", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was dir gefiel, das stammete von oben;", "tokens": ["Was", "dir", "ge\u00b7fiel", ",", "das", "stam\u00b7me\u00b7te", "von", "o\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PDS", "VVFIN", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mi\u00dffiel dir was, das war sein Wille nicht.", "tokens": ["Mi\u00df\u00b7fiel", "dir", "was", ",", "das", "war", "sein", "Wil\u00b7le", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PIS", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nimms immer hin, du unsrer Seelen Mann,", "tokens": ["Nimms", "im\u00b7mer", "hin", ",", "du", "uns\u00b7rer", "See\u00b7len", "Mann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PTKVZ", "$,", "PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Auf kurtze Zeit zu treuer Hand vertraut:", "tokens": ["Auf", "kurt\u00b7ze", "Zeit", "zu", "treu\u00b7er", "Hand", "ver\u00b7traut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Man sp\u00fcrte ja an ihm kein Widerstreben,", "tokens": ["Man", "sp\u00fcr\u00b7te", "ja", "an", "ihm", "kein", "Wi\u00b7der\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "APPR", "PPER", "PIAT", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Als man es dir ins Sterben \u00fcbergab:", "tokens": ["Als", "man", "es", "dir", "ins", "Ster\u00b7ben", "\u00fc\u00b7berg\u00b7ab", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es deuchte ihm, itzt w\u00fcrd es erstlich leben,", "tokens": ["Es", "deuch\u00b7te", "ihm", ",", "itzt", "w\u00fcrd", "es", "erst\u00b7lich", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADV", "VAFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es wuste nichts von F\u00e4ulni\u00df oder Grab!", "tokens": ["Es", "wus\u00b7te", "nichts", "von", "F\u00e4ul\u00b7ni\u00df", "o\u00b7der", "Grab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Was unsre Unvernunft uns glauben macht,", "tokens": ["Was", "uns\u00b7re", "Un\u00b7ver\u00b7nunft", "uns", "glau\u00b7ben", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "PPER", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Dasselbe hat sein Kinder-Sinn verlacht:", "tokens": ["Das\u00b7sel\u00b7be", "hat", "sein", "Kin\u00b7der\u00b7Sinn", "ver\u00b7lacht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Die Prediger der eiteln Wissenschaften,", "tokens": ["Die", "Pre\u00b7di\u00b7ger", "der", "ei\u00b7teln", "Wis\u00b7sen\u00b7schaf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Meister von der falsch ber\u00fchmten Kunst,", "tokens": ["Die", "Meis\u00b7ter", "von", "der", "falsch", "be\u00b7r\u00fchm\u00b7ten", "Kunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bereiten hier doch nichts als blauen Dunst.", "tokens": ["Be\u00b7rei\u00b7ten", "hier", "doch", "nichts", "als", "blau\u00b7en", "Dunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "PIS", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Man komme nur erst an des Todes Thor,", "toke