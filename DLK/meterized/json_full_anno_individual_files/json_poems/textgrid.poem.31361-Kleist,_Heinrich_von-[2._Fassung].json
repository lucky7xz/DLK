{"textgrid.poem.31361": {"metadata": {"author": {"name": "Kleist, Heinrich von", "birth": "N.A.", "death": "N.A."}, "title": "[2. Fassung]", "genre": "verse", "period": "N.A.", "pub_year": 1794, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du, die das Ungl\u00fcck mit der Grazie Schritten,", "tokens": ["Du", ",", "die", "das", "Un\u00b7gl\u00fcck", "mit", "der", "Gra\u00b7zie", "Schrit\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf jungen Schultern, herrlich j\u00fcngsthin trug:", "tokens": ["Auf", "jun\u00b7gen", "Schul\u00b7tern", ",", "herr\u00b7lich", "j\u00fcng\u00b7sthin", "trug", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie wunderbar ist meine Brust verwirrt,", "tokens": ["Wie", "wun\u00b7der\u00b7bar", "ist", "mei\u00b7ne", "Brust", "ver\u00b7wirrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In diesem Augenblick, da ich auf Knieen,", "tokens": ["In", "die\u00b7sem", "Au\u00b7gen\u00b7blick", ",", "da", "ich", "auf", "Kni\u00b7e\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "KOUS", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Um dich zu segnen, vor dir niedersinke.", "tokens": ["Um", "dich", "zu", "seg\u00b7nen", ",", "vor", "dir", "nie\u00b7der\u00b7sin\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "PTKZU", "VVINF", "$,", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ich soll dir ungetr\u00fcbte Tag' erflehn:", "tokens": ["Ich", "soll", "dir", "un\u00b7ge\u00b7tr\u00fcb\u00b7te", "Tag'", "er\u00b7flehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dir, die der hohen Himmelssonne gleich,", "tokens": ["Dir", ",", "die", "der", "ho\u00b7hen", "Him\u00b7mels\u00b7son\u00b7ne", "gleich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "In voller Pracht nur strahlt und Herrlichkeit,", "tokens": ["In", "vol\u00b7ler", "Pracht", "nur", "strahlt", "und", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVFIN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wenn sie durch finstre Wetterwolken bricht.", "tokens": ["Wenn", "sie", "durch", "finst\u00b7re", "Wet\u00b7ter\u00b7wol\u00b7ken", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "O du, die aus dem Kampf emp\u00f6rter Zeit,", "tokens": ["O", "du", ",", "die", "aus", "dem", "Kampf", "em\u00b7p\u00f6r\u00b7ter", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PRELS", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.12": {"text": "Was f\u00fcr ein Wort, dein w\u00fcrdig, sag ich dir?", "tokens": ["Was", "f\u00fcr", "ein", "Wort", ",", "dein", "w\u00fcr\u00b7dig", ",", "sag", "ich", "dir", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "$,", "PPOSAT", "ADJD", "$,", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "So zieht ein Cherub, mit gespreizten Fl\u00fcgeln,", "tokens": ["So", "zieht", "ein", "Che\u00b7rub", ",", "mit", "ge\u00b7spreiz\u00b7ten", "Fl\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NE", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Zur Nachtzeit durch die Luft, und, auf den R\u00fccken", "tokens": ["Zur", "Nacht\u00b7zeit", "durch", "die", "Luft", ",", "und", ",", "auf", "den", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$,", "KON", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Geworfen, staunen ihn, von Glanz geblendet,", "tokens": ["Ge\u00b7wor\u00b7fen", ",", "stau\u00b7nen", "ihn", ",", "von", "Glanz", "ge\u00b7blen\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Der Welt betroffene Geschlechter an.", "tokens": ["Der", "Welt", "be\u00b7trof\u00b7fe\u00b7ne", "Ge\u00b7schlech\u00b7ter", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.17": {"text": "Wir alle m\u00f6gen, Hoh' und Niedere,", "tokens": ["Wir", "al\u00b7le", "m\u00f6\u00b7gen", ",", "Hoh'", "und", "Nie\u00b7de\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VMFIN", "$,", "NE", "KON", "PIS", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Von den Ruinen unsers Gl\u00fccks umgeben,", "tokens": ["Von", "den", "Ru\u00b7i\u00b7nen", "un\u00b7sers", "Gl\u00fccks", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Gebeugt von Schmerz, die Himmlischen verklagen,", "tokens": ["Ge\u00b7beugt", "von", "Schmerz", ",", "die", "Himm\u00b7li\u00b7schen", "ver\u00b7kla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Doch du Erhabene, du darfst es nicht!", "tokens": ["Doch", "du", "Er\u00b7ha\u00b7be\u00b7ne", ",", "du", "darfst", "es", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Denn eine Glorie, in jenen N\u00e4chten,", "tokens": ["Denn", "ei\u00b7ne", "Glo\u00b7rie", ",", "in", "je\u00b7nen", "N\u00e4ch\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "Umgl\u00e4nzte deine Stirn, von der die Welt", "tokens": ["Um\u00b7gl\u00e4nz\u00b7te", "dei\u00b7ne", "Stirn", ",", "von", "der", "die", "Welt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "APPR", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Am lichten Tag der Freude nichts geahnt:", "tokens": ["Am", "lich\u00b7ten", "Tag", "der", "Freu\u00b7de", "nichts", "ge\u00b7ahnt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Wir sahn dich Anmut endlos niederregnen,", "tokens": ["Wir", "sahn", "dich", "An\u00b7mut", "end\u00b7los", "nie\u00b7der\u00b7reg\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Da\u00df du so gro\u00df als sch\u00f6n warst, war uns fremd!", "tokens": ["Da\u00df", "du", "so", "gro\u00df", "als", "sch\u00f6n", "warst", ",", "war", "uns", "fremd", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "KOKOM", "ADJD", "VAFIN", "$,", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Viel Blumen bl\u00fchen in dem Scho\u00df der Deinen", "tokens": ["Viel", "Blu\u00b7men", "bl\u00fc\u00b7hen", "in", "dem", "Scho\u00df", "der", "Dei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Noch deinem Gurt zum Strau\u00df, und du bists wert,", "tokens": ["Noch", "dei\u00b7nem", "Gurt", "zum", "Strau\u00df", ",", "und", "du", "bists", "wert", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "APPRART", "NE", "$,", "KON", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Doch eine sch\u00f6nre Palm erringst du nicht!", "tokens": ["Doch", "ei\u00b7ne", "sch\u00f6n\u00b7re", "Palm", "er\u00b7ringst", "du", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Und w\u00fcrde dir, durch einen Schlu\u00df der Zeiten,", "tokens": ["Und", "w\u00fcr\u00b7de", "dir", ",", "durch", "ei\u00b7nen", "Schlu\u00df", "der", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "$,", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Die Krone auch der Welt: die goldenste,", "tokens": ["Die", "Kro\u00b7ne", "auch", "der", "Welt", ":", "die", "gol\u00b7dens\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "$.", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Die dich zur K\u00f6nigin der Erde macht,", "tokens": ["Die", "dich", "zur", "K\u00f6\u00b7ni\u00b7gin", "der", "Er\u00b7de", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Hat still die Tugend schon dir aufgedr\u00fcckt.", "tokens": ["Hat", "still", "die", "Tu\u00b7gend", "schon", "dir", "auf\u00b7ge\u00b7dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ART", "NN", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Sei Teure, lange noch des Landes Stolz,", "tokens": ["Sei", "Teu\u00b7re", ",", "lan\u00b7ge", "noch", "des", "Lan\u00b7des", "Stolz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "ADV", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Durch frohe Jahre, wie, durch frohe Jahre,", "tokens": ["Durch", "fro\u00b7he", "Jah\u00b7re", ",", "wie", ",", "durch", "fro\u00b7he", "Jah\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PWAV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Du seine Lust und sein Entz\u00fccken warst!", "tokens": ["Du", "sei\u00b7ne", "Lust", "und", "sein", "Ent\u00b7z\u00fc\u00b7cken", "warst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du, die das Ungl\u00fcck mit der Grazie Schritten,", "tokens": ["Du", ",", "die", "das", "Un\u00b7gl\u00fcck", "mit", "der", "Gra\u00b7zie", "Schrit\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf jungen Schultern, herrlich j\u00fcngsthin trug:", "tokens": ["A