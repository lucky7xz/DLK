{"textgrid.poem.40421": {"metadata": {"author": {"name": "Scheffel, Joseph Viktor von", "birth": "N.A.", "death": "N.A."}, "title": "Die Maulbronner Fuge", "genre": "verse", "period": "N.A.", "pub_year": 1856, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Winterrefektorium", "tokens": ["Im", "Win\u00b7ter\u00b7re\u00b7fek\u00b7to\u00b7ri\u00b7um"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu Maulbronn in dem Kloster", "tokens": ["Zu", "Maul\u00b7bronn", "in", "dem", "Klos\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da geht was um den Tisch herum,", "tokens": ["Da", "geht", "was", "um", "den", "Tisch", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Klingt nicht wie Paternoster:", "tokens": ["Klingt", "nicht", "wie", "Pa\u00b7ter\u00b7nos\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "KOKOM", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Martinsgans hat wohlgetan,", "tokens": ["Die", "Mar\u00b7tins\u00b7gans", "hat", "wohl\u00b7ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Eilfinger blinkt im Kruge,", "tokens": ["Eil\u00b7fin\u00b7ger", "blinkt", "im", "Kru\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Nun hebt die nasse Andacht an", "tokens": ["Nun", "hebt", "die", "nas\u00b7se", "An\u00b7dacht", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und alles singt die Fuge:", "tokens": ["Und", "al\u00b7les", "singt", "die", "Fu\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Abt Johannes Entenfu\u00df", "tokens": ["Der", "Abt", "Jo\u00b7han\u00b7nes", "En\u00b7ten\u00b7fu\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kam unwirsch hergewatschelt:", "tokens": ["Kam", "un\u00b7wirsch", "her\u00b7ge\u00b7wat\u00b7schelt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbwas wird so sp\u00e4t als Festtagschlu\u00df", "tokens": ["\u00bb", "was", "wird", "so", "sp\u00e4t", "als", "Fest\u00b7tag\u00b7schlu\u00df"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PWS", "VAFIN", "ADV", "ADJD", "KOKOM", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Beim Geigenschall gefratschelt?", "tokens": ["Beim", "Gei\u00b7gen\u00b7schall", "ge\u00b7frat\u00b7schelt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "La\u00dft ab, Ihr st\u00f6rt den Doktor Faust", "tokens": ["La\u00dft", "ab", ",", "Ihr", "st\u00f6rt", "den", "Dok\u00b7tor", "Faust"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PTKVZ", "$,", "PPER", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Im Gartenturm dahinten:", "tokens": ["Im", "Gar\u00b7ten\u00b7turm", "da\u00b7hin\u00b7ten", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wenn solch ein Singsang zu ihm braust,", "tokens": ["Wenn", "solch", "ein", "Sings\u00b7ang", "zu", "ihm", "braust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Kann er kein Gold nicht finden:", "tokens": ["Kann", "er", "kein", "Gold", "nicht", "fin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Derweilen bracht der Zellerar,", "tokens": ["Der\u00b7wei\u00b7len", "bracht", "der", "Zel\u00b7le\u00b7rar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Herr Godefrit von Niefern,", "tokens": ["Herr", "Go\u00b7de\u00b7frit", "von", "Nie\u00b7fern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den Sankt Martinuszuspitz dar", "tokens": ["Den", "Sankt", "Mar\u00b7ti\u00b7nus\u00b7zu\u00b7spitz", "dar"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vom Keller mit den K\u00fcfern.", "tokens": ["Vom", "Kel\u00b7ler", "mit", "den", "K\u00fc\u00b7fern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der rief: \u00bbHerr Abbas, was Ihr sagt,", "tokens": ["Der", "rief", ":", "\u00bb", "Herr", "Ab\u00b7bas", ",", "was", "Ihr", "sagt", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$.", "$(", "NN", "NE", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Soll man in Z\u00fcchten ehren,", "tokens": ["Soll", "man", "in", "Z\u00fcch\u00b7ten", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "NN", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Doch wenn kein andrer Schmerz Euch plagt,", "tokens": ["Doch", "wenn", "kein", "an\u00b7drer", "Schmerz", "Euch", "plagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIAT", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So m\u00f6gt Ihr uns nicht wehren:", "tokens": ["So", "m\u00f6gt", "Ihr", "uns", "nicht", "weh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Der Faust sitzt selbst schon da!\u00ab", "tokens": ["Der", "Faust", "sitzt", "selbst", "schon", "da", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Faust sa\u00df r\u00fcckw\u00e4rts an der Wand", "tokens": ["Der", "Faust", "sa\u00df", "r\u00fcck\u00b7w\u00e4rts", "an", "der", "Wand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und trank vergn\u00fcgt im Dunkeln,", "tokens": ["Und", "trank", "ver\u00b7gn\u00fcgt", "im", "Dun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nun lie\u00df der blasse Nekromant", "tokens": ["Nun", "lie\u00df", "der", "blas\u00b7se", "Ne\u00b7kro\u00b7mant"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Glas am Licht karfunkeln", "tokens": ["Sein", "Glas", "am", "Licht", "kar\u00b7fun\u00b7keln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und sprach: \u00bbIch br\u00fct' schon Tag und Jahr", "tokens": ["Und", "sprach", ":", "\u00bb", "Ich", "br\u00fct'", "schon", "Tag", "und", "Jahr"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "$(", "PPER", "VVFIN", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Am schwarzen Zauberbuche", "tokens": ["Am", "schwar\u00b7zen", "Zau\u00b7ber\u00b7bu\u00b7che"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und merk' erst heut, ich bin ein Narr,", "tokens": ["Und", "merk'", "erst", "heut", ",", "ich", "bin", "ein", "Narr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df ich das Gold dort suche:", "tokens": ["Da\u00df", "ich", "das", "Gold", "dort", "su\u00b7che", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Das echte Gold ist da!\u00ab", "tokens": ["Das", "ech\u00b7te", "Gold", "ist", "da", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u00bbmit Hermes Trismegistos List", "tokens": ["\u00bb", "mit", "Her\u00b7mes", "Tris\u00b7me\u00b7gis\u00b7tos", "List"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "APPR", "NE", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird keins erlaborieret,", "tokens": ["Wird", "keins", "er\u00b7la\u00b7bo\u00b7rie\u00b7ret", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Sonne ist der Alchimist,", "tokens": ["Die", "Son\u00b7ne", "ist", "der", "Al\u00b7chi\u00b7mist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der's fl\u00fcssig destillieret:", "tokens": ["Der's", "fl\u00fcs\u00b7sig", "des\u00b7til\u00b7lie\u00b7ret", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wenn's durch die Adern gl\u00fcht und rollt", "tokens": ["Wenn's", "durch", "die", "A\u00b7dern", "gl\u00fcht", "und", "rollt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit des Eilfingers Wonnen,", "tokens": ["Mit", "des", "Eil\u00b7fin\u00b7gers", "Won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Dann habt Ihr Gold, habt echtes Gold,", "tokens": ["Dann", "habt", "Ihr", "Gold", ",", "habt", "ech\u00b7tes", "Gold", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$,", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und ehrlich selbst gewonnen.", "tokens": ["Und", "ehr\u00b7lich", "selbst", "ge\u00b7won\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Da lacht der Abt:\u00bbMit solcher Lehr'", "tokens": ["Da", "lacht", "der", "Abt", ":", "\u00bb", "Mit", "sol\u00b7cher", "Lehr'"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "$(", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zwingt Ihr auch mich zum Kruge,", "tokens": ["Zwingt", "Ihr", "auch", "mich", "zum", "Kru\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Denn ", "tokens": ["Denn"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Ist eine feuchte Fuge.", "tokens": ["Ist", "ei\u00b7ne", "feuch\u00b7te", "Fu\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Als Fausti Goldspruch lass' ich sie", "tokens": ["Als", "Faus\u00b7ti", "Gold\u00b7spruch", "lass'", "ich", "sie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "NN", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Jetzt in den Kreuzgang malen,", "tokens": ["Jetzt", "in", "den", "Kreuz\u00b7gang", "ma\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Man kennt die ganze Melodie", "tokens": ["Man", "kennt", "die", "gan\u00b7ze", "Me\u00b7lo\u00b7die"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Schon an den Initialen:", "tokens": ["Schon", "an", "den", "I\u00b7ni\u00b7ti\u00b7a\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Im Winterrefektorium", "tokens": ["Im", "Win\u00b7ter\u00b7re\u00b7fek\u00b7to\u00b7ri\u00b7um"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu Maulbronn in dem Kloster", "tokens": ["Zu", "Maul\u00b7bronn", "in", "dem", "Klos\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da geht was um den Tisch herum,", "tokens": ["Da", "geht", "was", "um", "den", "Tisch", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "m