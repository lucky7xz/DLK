{"dta.poem.8629": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Auf geheimem Waldespfade", "tokens": ["Auf", "ge\u00b7hei\u00b7mem", "Wal\u00b7des\u00b7pfa\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schleich' ich gern im Abendschein", "tokens": ["Schleich'", "ich", "gern", "im", "A\u00b7bend\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "An das \u00f6de Schilfgestade,", "tokens": ["An", "das", "\u00f6\u00b7de", "Schilf\u00b7ge\u00b7sta\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00e4dchen, und gedenke dein!", "tokens": ["M\u00e4d\u00b7chen", ",", "und", "ge\u00b7den\u00b7ke", "dein", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "VVFIN", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn sich dann der Busch verd\u00fcstert,", "tokens": ["Wenn", "sich", "dann", "der", "Busch", "ver\u00b7d\u00fcs\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rauscht das Rohr geheimni\u00dfvoll,", "tokens": ["Rauscht", "das", "Rohr", "ge\u00b7heim\u00b7ni\u00df\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es klaget und es fl\u00fcstert,", "tokens": ["Und", "es", "kla\u00b7get", "und", "es", "fl\u00fcs\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ich weinen, weinen soll.", "tokens": ["Da\u00df", "ich", "wei\u00b7nen", ",", "wei\u00b7nen", "soll", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "$,", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und ich mein', ich h\u00f6re wehen", "tokens": ["Und", "ich", "mein'", ",", "ich", "h\u00f6\u00b7re", "we\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Leise deiner Stimme Klang,", "tokens": ["Lei\u00b7se", "dei\u00b7ner", "Stim\u00b7me", "Klang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und im Weiher untergehen", "tokens": ["Und", "im", "Wei\u00b7her", "un\u00b7ter\u00b7ge\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deinen lieblichen Gesang.", "tokens": ["Dei\u00b7nen", "lieb\u00b7li\u00b7chen", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}