{"dta.poem.23648": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "In der Heimath.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gelichtet ist der Wald und kahl das Feld,", "tokens": ["Ge\u00b7lich\u00b7tet", "ist", "der", "Wald", "und", "kahl", "das", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "KON", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie alt geworden ist die junge Welt!", "tokens": ["Wie", "alt", "ge\u00b7wor\u00b7den", "ist", "die", "jun\u00b7ge", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAPP", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Geebnet sind der Gr\u00e4ber lange Reihn,", "tokens": ["Geeb\u00b7net", "sind", "der", "Gr\u00e4\u00b7ber", "lan\u00b7ge", "Reihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "ADV", "NE", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Neu sind die H\u00e4user, neu von Holz und Stein,", "tokens": ["Neu", "sind", "die", "H\u00e4u\u00b7ser", ",", "neu", "von", "Holz", "und", "Stein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Sogar der Bach verlie\u00df den alten Zug \u2014", "tokens": ["So\u00b7gar", "der", "Bach", "ver\u00b7lie\u00df", "den", "al\u00b7ten", "Zug"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die Glocke nur, sie schl\u00e4gt noch wie sie schlug.", "tokens": ["Die", "Glo\u00b7cke", "nur", ",", "sie", "schl\u00e4gt", "noch", "wie", "sie", "schlug", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PPER", "VVFIN", "ADV", "KOKOM", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Von allem was du hattest \u2014 keine Spur,", "tokens": ["Von", "al\u00b7lem", "was", "du", "hat\u00b7test", "kei\u00b7ne", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIS", "PWS", "PPER", "VAFIN", "$(", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du findest es im Menschenherzen nur,", "tokens": ["Du", "fin\u00b7dest", "es", "im", "Men\u00b7schen\u00b7her\u00b7zen", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und jedes hegt f\u00fcr dich Erinnerung,", "tokens": ["Und", "je\u00b7des", "hegt", "f\u00fcr", "dich", "E\u00b7rin\u00b7ne\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "VVFIN", "APPR", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und jedes macht dich wieder froh und jung;", "tokens": ["Und", "je\u00b7des", "macht", "dich", "wie\u00b7der", "froh", "und", "jung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "VVFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das Herz bleibt ohne Wandel, ohne Trug,", "tokens": ["Das", "Herz", "bleibt", "oh\u00b7ne", "Wan\u00b7del", ",", "oh\u00b7ne", "Trug", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,", "KOUI", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Es schl\u00e4gt noch immer wie es weiland schlug.", "tokens": ["Es", "schl\u00e4gt", "noch", "im\u00b7mer", "wie", "es", "wei\u00b7land", "schlug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "KOKOM", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}