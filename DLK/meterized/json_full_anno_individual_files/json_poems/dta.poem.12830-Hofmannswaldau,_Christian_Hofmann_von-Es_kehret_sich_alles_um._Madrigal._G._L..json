{"dta.poem.12830": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Es kehret sich alles um.  \n Madrigal.  \n G. L.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die welt ist um und um gest\u00fcrtzt:", "tokens": ["Die", "welt", "ist", "um", "und", "um", "ge\u00b7st\u00fcrtzt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "KON", "APPR", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der alte deutsche glaube", "tokens": ["Der", "al\u00b7te", "deut\u00b7sche", "glau\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Schmeckt kaum so gut, als eine faule traube.", "tokens": ["Schmeckt", "kaum", "so", "gut", ",", "als", "ei\u00b7ne", "fau\u00b7le", "trau\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADJD", "$,", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die wahrheit ist mit schimpff verjaget,", "tokens": ["Die", "wahr\u00b7heit", "ist", "mit", "schimpff", "ver\u00b7ja\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und wegen mord und ehebruch verklaget.", "tokens": ["Und", "we\u00b7gen", "mord", "und", "e\u00b7heb\u00b7ruch", "ver\u00b7kla\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "KON", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die adler werden so gemein,", "tokens": ["Die", "ad\u00b7ler", "wer\u00b7den", "so", "ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df ihrer mehr als kleine m\u00fccken seyn.", "tokens": ["Da\u00df", "ih\u00b7rer", "mehr", "als", "klei\u00b7ne", "m\u00fc\u00b7cken", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "PIAT", "KOKOM", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}