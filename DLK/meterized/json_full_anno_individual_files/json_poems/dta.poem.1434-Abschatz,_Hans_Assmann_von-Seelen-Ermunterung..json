{"dta.poem.1434": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Seelen-Ermunterung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Freue dich/ bekr\u00e4nckte Seele/", "tokens": ["Freu\u00b7e", "dich", "/", "be\u00b7kr\u00e4nck\u00b7te", "See\u00b7le", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die gew\u00fcnschte Zeit ist hier/", "tokens": ["Die", "ge\u00b7w\u00fcnschte", "Zeit", "ist", "hier", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da du aus der finstern H\u00f6le", "tokens": ["Da", "du", "aus", "der", "fins\u00b7tern", "H\u00f6\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deines Leibes brichst herf\u00fcr/", "tokens": ["Dei\u00b7nes", "Lei\u00b7bes", "brichst", "her\u00b7f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da du aller Sorge lo\u00df/", "tokens": ["Da", "du", "al\u00b7ler", "Sor\u00b7ge", "lo\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Die dich in der Welt umschlo\u00df/", "tokens": ["Die", "dich", "in", "der", "Welt", "um\u00b7schlo\