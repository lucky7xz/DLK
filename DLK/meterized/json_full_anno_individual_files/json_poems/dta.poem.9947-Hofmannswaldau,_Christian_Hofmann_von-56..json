{"dta.poem.9947": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "56.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "J\u00e4hrlich werd ich einmahl jung/ werd auch alt im selben jahre/", "tokens": ["J\u00e4hr\u00b7lich", "werd", "ich", "ein\u00b7mahl", "jung", "/", "werd", "auch", "alt", "im", "sel\u00b7ben", "jah\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "ADJD", "$(", "VAFIN", "ADV", "ADJD", "APPRART", "ADJA", "NN", "$("], "meter": "+------+-+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Sterb ohn da\u00df von meinen kindern ich was sehe noch erfahre/", "tokens": ["Sterb", "ohn", "da\u00df", "von", "mei\u00b7nen", "kin\u00b7dern", "ich", "was", "se\u00b7he", "noch", "er\u00b7fah\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "KOUS", "APPR", "PPOSAT", "NN", "PPER", "PIS", "VVFIN", "ADV", "VVFIN", "$("], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Ich selbst spinne/ was die Parce sonst der welt zu spin\u0303en pflegt/", "tokens": ["Ich", "selbst", "spin\u00b7ne", "/", "was", "die", "Par\u00b7ce", "sonst", "der", "welt", "zu", "spi\u00f1en", "pflegt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$(", "PWS", "ART", "NN", "ADV", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "Werde pr\u00e4chtig wie monarchen in ein reiches grab gelegt.", "tokens": ["Wer\u00b7de", "pr\u00e4ch\u00b7tig", "wie", "mon\u00b7ar\u00b7chen", "in", "ein", "rei\u00b7ches", "grab", "ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KOKOM", "NN", "APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}