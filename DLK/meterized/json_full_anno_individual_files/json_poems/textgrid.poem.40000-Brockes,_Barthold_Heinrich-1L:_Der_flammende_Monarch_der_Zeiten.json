{"textgrid.poem.40000": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der flammende Monarch der Zeiten", "genre": "verse", "period": "N.A.", "pub_year": 1713, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der flammende Monarch der Zeiten", "tokens": ["Der", "flam\u00b7men\u00b7de", "Mon\u00b7arch", "der", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Bestrahlte, sonder Duft und Wolcken, Luft und Land;", "tokens": ["Be\u00b7strahl\u00b7te", ",", "son\u00b7der", "Duft", "und", "Wol\u00b7cken", ",", "Luft", "und", "Land", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sein Lebens-reicher Wunder-Brand", "tokens": ["Sein", "Le\u00b7bens\u00b7rei\u00b7cher", "Wun\u00b7der\u00b7Brand"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ergo\u00df sich, wie ein Meer von Glantz und Heiterkeiten;", "tokens": ["Er\u00b7go\u00df", "sich", ",", "wie", "ein", "Meer", "von", "Glantz", "und", "Hei\u00b7ter\u00b7kei\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "PWAV", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und kurtz, es war ein angenehmer Tag,", "tokens": ["Und", "kurtz", ",", "es", "war", "ein", "an\u00b7ge\u00b7neh\u00b7mer", "Tag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Als Thyrsis, wie er \u00f6fters pflag,", "tokens": ["Als", "Thyr\u00b7sis", ",", "wie", "er", "\u00f6f\u00b7ters", "pflag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Im k\u00fchlen Schatten einer Linden,", "tokens": ["Im", "k\u00fch\u00b7len", "Schat\u00b7ten", "ei\u00b7ner", "Lin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Auf weichem Gras' halb sa\u00df, halb lag,", "tokens": ["Auf", "wei\u00b7chem", "Gras'", "halb", "sa\u00df", ",", "halb", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVFIN", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und bald den Himmel, bald die Welt,", "tokens": ["Und", "bald", "den", "Him\u00b7mel", ",", "bald", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Bald der Natur vollkomm'ne Pracht,", "tokens": ["Bald", "der", "Na\u00b7tur", "voll\u00b7kom\u00b7m'\u00b7ne", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.11": {"text": "Zum Vorwurf seiner Lieder macht'.", "tokens": ["Zum", "Vor\u00b7wurf", "sei\u00b7ner", "Lie\u00b7der", "macht'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Indem ward er ein reifes Feld,", "tokens": ["In\u00b7dem", "ward", "er", "ein", "rei\u00b7fes", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.13": {"text": "Worauf der Bauern muntre Schaar", "tokens": ["Wo\u00b7rauf", "der", "Bau\u00b7ern", "mun\u00b7tre", "Schaar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Mit M\u00e4hen theils, und theils mit Binden", "tokens": ["Mit", "M\u00e4\u00b7hen", "theils", ",", "und", "theils", "mit", "Bin\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "$,", "KON", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Besch\u00e4fftigt war, gewahr.", "tokens": ["Be\u00b7sch\u00e4ff\u00b7tigt", "war", ",", "ge\u00b7wahr", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Schnitter erndteten, die Scheunen anzuf\u00fcllen,", "tokens": ["Die", "Schnit\u00b7ter", "ernd\u00b7te\u00b7ten", ",", "die", "Scheu\u00b7nen", "an\u00b7zu\u00b7f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Aecker gelben Schmuck, mit sauren Freuden, ein;", "tokens": ["Der", "A\u00b7e\u00b7cker", "gel\u00b7ben", "Schmuck", ",", "mit", "sau\u00b7ren", "Freu\u00b7den", ",", "ein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,", "PTKVZ", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Man sah so manchen Bach, von lauem Schweisse, quillen;", "tokens": ["Man", "sah", "so", "man\u00b7chen", "Bach", ",", "von", "lau\u00b7em", "Schweis\u00b7se", ",", "quil\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "PIAT", "NN", "$,", "APPR", "ADJA", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Sensen blitzeten, es rauschte jeder Hieb,", "tokens": ["Die", "Sen\u00b7sen", "blit\u00b7ze\u00b7ten", ",", "es", "rauschte", "je\u00b7der", "Hieb", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+---+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Wenn die beschwitzte Faust ihn durch die Halmen trieb.", "tokens": ["Wenn", "die", "be\u00b7schwitz\u00b7te", "Faust", "ihn", "durch", "die", "Hal\u00b7men", "trieb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Hier band, dort lud man auf; kaum konnten grosse Wagen", "tokens": ["Hier", "band", ",", "dort", "lud", "man", "auf", ";", "kaum", "konn\u00b7ten", "gros\u00b7se", "Wa\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "ADV", "VVFIN", "PIS", "PTKVZ", "$.", "ADV", "VMFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Das raschelnde Gewicht gebund'ner Garben tragen.", "tokens": ["Das", "ra\u00b7scheln\u00b7de", "Ge\u00b7wicht", "ge\u00b7bun\u00b7d'\u00b7ner", "Gar\u00b7ben", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "--+--+-+--+-+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Die Achse seuftzt und knarrt; der Fuhr-Mann klatscht, und schwingt", "tokens": ["Die", "A\u00b7chse", "seuftzt", "und", "knarrt", ";", "der", "Fuhr\u00b7Mann", "klatscht", ",", "und", "schwingt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$.", "ART", "NN", "VVFIN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die schlancke Geissel um; indem er fr\u00f6hlich singt,", "tokens": ["Die", "schlan\u00b7cke", "Geis\u00b7sel", "um", ";", "in\u00b7dem", "er", "fr\u00f6h\u00b7lich", "singt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$.", "KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Verdoppelt er die Kraft der wei\u00df-besch\u00e4umten Pferde.", "tokens": ["Ver\u00b7dop\u00b7pelt", "er", "die", "Kraft", "der", "wei\u00df\u00b7be\u00b7sch\u00e4um\u00b7ten", "Pfer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sie ziehn; der Boden beb't: es zittert selbst die Erde,", "tokens": ["Sie", "ziehn", ";", "der", "Bo\u00b7den", "be\u00b7b't", ":", "es", "zit\u00b7tert", "selbst", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$.", "ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Gedr\u00fcckt von eigner Frucht. Ein \u00e4msiges Gew\u00fchl,", "tokens": ["Ge\u00b7dr\u00fcckt", "von", "eig\u00b7ner", "Frucht", ".", "Ein", "\u00e4m\u00b7si\u00b7ges", "Ge\u00b7w\u00fchl", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Das denen selbst, die es mit M\u00fch' erreg't, gefiel,", "tokens": ["Das", "de\u00b7nen", "selbst", ",", "die", "es", "mit", "M\u00fch'", "er\u00b7reg't", ",", "ge\u00b7fiel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "PRELS", "ADV", "$,", "PRELS", "PPER", "APPR", "NN", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.14": {"text": "Gefiel auch ihm; er f\u00fchlt', in der gereitzten Brust,", "tokens": ["Ge\u00b7fiel", "auch", "ihm", ";", "er", "f\u00fchlt'", ",", "in", "der", "ge\u00b7reitz\u00b7ten", "Brust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "$.", "PPER", "VVFIN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ein' Andachts-Gluht, ein Freuden-Feuer, glimmen,", "tokens": ["Ein'", "An\u00b7dachts\u00b7Gluht", ",", "ein", "Freu\u00b7den\u00b7Feu\u00b7er", ",", "glim\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Und fieng gleich an, vor ", "tokens": ["Und", "fi\u00b7eng", "gleich", "an", ",", "vor"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "ADV", "PTKVZ", "$,", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Die\u00df Sommer-Lied dem Sch\u00f6pfer anzustimmen:", "tokens": ["Die\u00df", "Som\u00b7mer\u00b7Lied", "dem", "Sch\u00f6p\u00b7fer", "an\u00b7zu\u00b7stim\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der flammende Monarch der Zeiten", "tokens": ["Der", "flam\u00b7men\u00b7de", "Mon\u00b7arch", "der", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Bestrahlte, sonder Duft und Wolcken, Luft und Land;", "tokens": ["Be\u00b7strahl\u00b7te", ",", "son\u00b7der", "Duft", "und", "Wol\u00b7cken", ",", "Luft", "und", "Land", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sein Lebens-reicher Wunder-Brand", "tokens": ["Sein", "Le\u00b7bens\u00b7rei\u00b7cher", "Wun\u00b7der\u00b7Brand"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ergo\u00df sich, wie ein Meer von Glantz und Heiterkeiten;", "tokens": ["Er\u00b7go\u00df", "sich", ",", "wie", "ein", "Meer", "von", "Glantz", "und", "Hei\u00b