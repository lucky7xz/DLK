{"dta.poem.21461": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Meiner Seele Seele.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Welch\u2019 Drang, welch\u2019 Beben", "tokens": ["Welch'", "Drang", ",", "welch'", "Be\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "PWAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Durchgraut, durchzittert", "tokens": ["Durch\u00b7graut", ",", "durch\u00b7zit\u00b7tert"], "token_info": ["word", "punct", "word"], "pos": ["ADJD", "$,", "VVFIN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wie wonniger Gl\u00fccksahnung", "tokens": ["Wie", "won\u00b7ni\u00b7ger", "Gl\u00fcck\u00b7sah\u00b7nung"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "J\u00e4he Gewi\u00dfheit", "tokens": ["J\u00e4\u00b7he", "Ge\u00b7wi\u00df\u00b7heit"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Die qualm\u00fcden Sinne!", "tokens": ["Die", "qual\u00b7m\u00fc\u00b7den", "Sin\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Wie schrecklich-sch\u00f6n", "tokens": ["Wie", "schreck\u00b7lich\u00b7sch\u00f6n"], "token_info": ["word", "word"], "pos": ["PWAV", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Bl\u00fcht entgegen mir", "tokens": ["Bl\u00fcht", "ent\u00b7ge\u00b7gen", "mir"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "In flammenden Reizen", "tokens": ["In", "flam\u00b7men\u00b7den", "Rei\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Deine s\u00fc\u00dfe Gestalt!", "tokens": ["Dei\u00b7ne", "s\u00fc\u00b7\u00dfe", "Ge\u00b7stalt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "Wie durchschau\u2019rt mein Herz,", "tokens": ["Wie", "durch\u00b7schau'rt", "mein", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Das liebeschmachtende,", "tokens": ["Das", "lie\u00b7be\u00b7schmach\u00b7ten\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "ADJA", "$,"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.12": {"text": "Deines schimmernden Gluthauges", "tokens": ["Dei\u00b7nes", "schim\u00b7mern\u00b7den", "Glut\u00b7hau\u00b7ges"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+--++-", "measure": "trochaic.tetra.relaxed"}, "line.13": {"text": "Seligjubelnde M\u00e4rchensprache!", "tokens": ["Se\u00b7lig\u00b7ju\u00b7beln\u00b7de", "M\u00e4r\u00b7chen\u00b7spra\u00b7che", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.14": {"text": "Ach, du bist meiner Sehnsucht Ziel \u2026", "tokens": ["Ach", ",", "du", "bist", "mei\u00b7ner", "Sehn\u00b7sucht", "Ziel", "\u2026"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Jauchzend gr\u00fc\u00df\u2019 ich dich:", "tokens": ["Jauch\u00b7zend", "gr\u00fc\u00df'", "ich", "dich", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.16": {"text": "Meiner Seele: Seele!", "tokens": ["Mei\u00b7ner", "See\u00b7le", ":", "See\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.17": {"text": "Du bist\u2019s, die ich suchte", "tokens": ["Du", "bist's", ",", "die", "ich", "such\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "NE", "$,", "PRELS", "PPER", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.18": {"text": "In dunklen Mittern\u00e4chten,", "tokens": ["In", "dunk\u00b7len", "Mit\u00b7ter\u00b7n\u00e4ch\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Da ich rang und rang", "tokens": ["Da", "ich", "rang", "und", "rang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "KON", "VVFIN"], "meter": "--+-+", "measure": "anapaest.init"}, "line.20": {"text": "In stummer Verzweiflung", "tokens": ["In", "stum\u00b7mer", "Ver\u00b7zwei\u00b7flung"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.21": {"text": "Und kein Stern mir winkte", "tokens": ["Und", "kein", "Stern", "mir", "wink\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "PPER", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.22": {"text": "Vom grausamen Himmel \u2026", "tokens": ["Vom", "grau\u00b7sa\u00b7men", "Him\u00b7mel", "\u2026"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.23": {"text": "Du bist\u2019s, die ich suchte,", "tokens": ["Du", "bist's", ",", "die", "ich", "such\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.24": {"text": "Wenn ich einsam trank", "tokens": ["Wenn", "ich", "ein\u00b7sam", "trank"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.25": {"text": "Des Fr\u00fchlings frische", "tokens": ["Des", "Fr\u00fch\u00b7lings", "fri\u00b7sche"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.26": {"text": "Quellende Gluth", "tokens": ["Quel\u00b7len\u00b7de", "Gluth"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.27": {"text": "Die in alle Poren mir drang,", "tokens": ["Die", "in", "al\u00b7le", "Po\u00b7ren", "mir", "drang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIAT", "NN", "PPER", "VVFIN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.28": {"text": "Den s\u00fc\u00dfen Duft", "tokens": ["Den", "s\u00fc\u00b7\u00dfen", "Duft"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.29": {"text": "Der \u00fcber den Auen schwamm", "tokens": ["Der", "\u00fc\u00b7ber", "den", "Au\u00b7en", "schwamm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.30": {"text": "Und den Busen mir schwellte", "tokens": ["Und", "den", "Bu\u00b7sen", "mir", "schwell\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PPER", "VVFIN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.31": {"text": "In heiliger Sehnsucht \u2026", "tokens": ["In", "hei\u00b7li\u00b7ger", "Sehn\u00b7sucht", "\u2026"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+-++-", "measure": "unknown.measure.tri"}, "line.32": {"text": "Du bist\u2019s, in dir wurzelt", "tokens": ["Du", "bist's", ",", "in", "dir", "wur\u00b7zelt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "NE", "$,", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.33": {"text": "Mit allen Fasern", "tokens": ["Mit", "al\u00b7len", "Fa\u00b7sern"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.34": {"text": "Mein ganzes Sein,", "tokens": ["Mein", "gan\u00b7zes", "Sein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.35": {"text": "Jede Knospe der Hoffnung,", "tokens": ["Je\u00b7de", "Knos\u00b7pe", "der", "Hoff\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.36": {"text": "All\u2019 Frieden und Gl\u00fcck.", "tokens": ["All'", "Frie\u00b7den", "und", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.37": {"text": "Gebannt in deiner Sch\u00f6nheit", "tokens": ["Ge\u00b7bannt", "in", "dei\u00b7ner", "Sch\u00f6n\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.38": {"text": "Magischen Lichtkreis", "tokens": ["Ma\u00b7gi\u00b7schen", "Licht\u00b7kreis"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.39": {"text": "Lass\u2019 mich tr\u00e4umend vergessen,", "tokens": ["Lass'", "mich", "tr\u00e4u\u00b7mend", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.40": {"text": "Lass\u2019 Leben mich saugen", "tokens": ["Lass'", "Le\u00b7ben", "mich", "sau\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NN", "PPER", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.41": {"text": "Von deinen Lippen,", "tokens": ["Von", "dei\u00b7nen", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.42": {"text": "Im Schoo\u00dfe dir rasten:", "tokens": ["Im", "Schoo\u00b7\u00dfe", "dir", "ras\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.43": {"text": "In holder Umarmung", "tokens": ["In", "hol\u00b7der", "Um\u00b7ar\u00b7mung"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.44": {"text": "Sch\u00f6pf\u2019 ich Erquickung", "tokens": ["Sch\u00f6pf'", "ich", "Er\u00b7quic\u00b7kung"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "PPER", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.45": {"text": "Wie der Baum aus der Erde \u2026", "tokens": ["Wie", "der", "Baum", "aus", "der", "Er\u00b7de", "\u2026"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "--+--+-", "measure": "anapaest.di.plus"}}}}}