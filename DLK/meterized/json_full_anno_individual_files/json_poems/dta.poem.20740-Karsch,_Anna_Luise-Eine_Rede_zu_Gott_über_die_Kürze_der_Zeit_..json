"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Bey allen Handlungen des kurzen Lebens blicken:", "tokens": ["Bey", "al\u00b7len", "Hand\u00b7lun\u00b7gen", "des", "kur\u00b7zen", "Le\u00b7bens", "bli\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "O dann verschwend ich meine Stunden nicht,", "tokens": ["O", "dann", "ver\u00b7schwend", "ich", "mei\u00b7ne", "Stun\u00b7den", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVPP", "PPER", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Mich holt der Tod zu dir, und tausend Jahre werden,", "tokens": ["Mich", "holt", "der", "Tod", "zu", "dir", ",", "und", "tau\u00b7send", "Jah\u00b7re", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPER", "$,", "KON", "CARD", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Mir k\u00fcrzer seyn vor deinem Angesicht,", "tokens": ["Mir", "k\u00fcr\u00b7zer", "seyn", "vor", "dei\u00b7nem", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VAINF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Als jezt ein Tag auf Erden.", "tokens": ["Als", "jezt", "ein", "Tag", "auf", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}