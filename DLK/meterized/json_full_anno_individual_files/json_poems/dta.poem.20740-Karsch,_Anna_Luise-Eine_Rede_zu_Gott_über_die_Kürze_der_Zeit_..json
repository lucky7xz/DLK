{"dta.poem.20740": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Eine Rede zu Gott  \n  \u00fcber die K\u00fcrze der Zeit .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Herr, der du \u00fcber uns des Tages Wagen lenken                 ", "tokens": ["Herr", ",", "der", "du", "\u00fc\u00b7ber", "uns", "des", "Ta\u00b7ges", "Wa\u00b7gen", "len\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "PPER", "APPR", "PPER", "ART", "NN", "NN", "VVINF"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Mit deinem Winke kannst, Herr, lehre mich bedenken,", "tokens": ["Mit", "dei\u00b7nem", "Win\u00b7ke", "kannst", ",", "Herr", ",", "leh\u00b7re", "mich", "be\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "$,", "NN", "$,", "VVFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df ich davon mu\u00df aus der Zeit.", "tokens": ["Da\u00df", "ich", "da\u00b7von", "mu\u00df", "aus", "der", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "VMFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Leben flieht dahin, ist kurz, ist nur so breit,", "tokens": ["Mein", "Le\u00b7ben", "flieht", "da\u00b7hin", ",", "ist", "kurz", ",", "ist", "nur", "so", "breit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PAV", "$,", "VAFIN", "ADJD", "$,", "VAFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Als diese Hand, mit der ich schreibe;", "tokens": ["Als", "die\u00b7se", "Hand", ",", "mit", "der", "ich", "schrei\u00b7be", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "O warum denk\u2019 ich mir hier eine Ewigkeit", "tokens": ["O", "wa\u00b7rum", "denk'", "ich", "mir", "hier", "ei\u00b7ne", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PWAV", "VVFIN", "PPER", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "In einer Welt, in der ich vierzig, funfzig Jahr,", "tokens": ["In", "ei\u00b7ner", "Welt", ",", "in", "der", "ich", "vier\u00b7zig", ",", "funf\u00b7zig", "Jahr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "CARD", "$,", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wenns hoch k\u00f6mmt, zweymahl vierzig Ernten bleibe!", "tokens": ["Wenns", "hoch", "k\u00f6mmt", ",", "zwey\u00b7mahl", "vier\u00b7zig", "Ern\u00b7ten", "blei\u00b7be", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VVFIN", "$,", "ADV", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Herr! wenn du fragen wirst, wie lang ich Wandrer war", "tokens": ["Herr", "!", "wenn", "du", "fra\u00b7gen", "wirst", ",", "wie", "lang", "ich", "Wand\u00b7rer", "war"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "KOUS", "PPER", "VVINF", "VAFIN", "$,", "PWAV", "ADJD", "PPER", "NN", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Hier unter dir auf deiner Erde?", "tokens": ["Hier", "un\u00b7ter", "dir", "auf", "dei\u00b7ner", "Er\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Wie ich die Stunden angewandt?", "tokens": ["Wie", "ich", "die", "Stun\u00b7den", "an\u00b7ge\u00b7wandt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "O Gott! bey dem Gedank erzittert mein Verstand,", "tokens": ["O", "Gott", "!", "bey", "dem", "Ge\u00b7dank", "er\u00b7zit\u00b7tert", "mein", "Ver\u00b7stand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.13": {"text": "Was ich dir Antwort geben werde.", "tokens": ["Was", "ich", "dir", "Ant\u00b7wort", "ge\u00b7ben", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "NN", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Fr\u00fch oder sp\u00e4t schl\u00e4gt meine Stunde mir,", "tokens": ["Fr\u00fch", "o\u00b7der", "sp\u00e4t", "schl\u00e4gt", "mei\u00b7ne", "Stun\u00b7de", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VVFIN", "PPOSAT", "NN", "PPER", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.15": {"text": "Bestimmt zur letzten meiner Stunden,", "tokens": ["Be\u00b7stimmt", "zur", "letz\u00b7ten", "mei\u00b7ner", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Eh ich gebildet ward von dir,", "tokens": ["Eh", "ich", "ge\u00b7bil\u00b7det", "ward", "von", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Zehntausend werden wie erw\u00fcrgt befunden,", "tokens": ["Zehn\u00b7tau\u00b7send", "wer\u00b7den", "wie", "er\u00b7w\u00fcrgt", "be\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAINF", "KOKOM", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Zehntausend bracht ich f\u00fchllo\u00df zu,", "tokens": ["Zehn\u00b7tau\u00b7send", "bracht", "ich", "f\u00fchl\u00b7lo\u00df", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "VVFIN", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Ohn da\u00df ich Guts ohn da\u00df ich B\u00f6ses wollte,", "tokens": ["Ohn", "da\u00df", "ich", "Guts", "ohn", "da\u00df", "ich", "B\u00f6\u00b7ses", "woll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "NN", "APPR", "KOUS", "PPER", "NE", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Und viele Tausende verschlief ich in der Ruh,", "tokens": ["Und", "vie\u00b7le", "Tau\u00b7sen\u00b7de", "ver\u00b7schlief", "ich", "in", "der", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die mich zum Guten st\u00e4rken sollte.", "tokens": ["Die", "mich", "zum", "Gu\u00b7ten", "st\u00e4r\u00b7ken", "soll\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Wie wenig Stunden braucht ich wohl", "tokens": ["Wie", "we\u00b7nig", "Stun\u00b7den", "braucht", "ich", "wohl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Mich zu dem Schritt vorzubereiten,", "tokens": ["Mich", "zu", "dem", "Schritt", "vor\u00b7zu\u00b7be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Den ich her\u00fcber schreiten soll", "tokens": ["Den", "ich", "her\u00b7\u00fc\u00b7ber", "schrei\u00b7ten", "soll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "ADV", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Ins Thor der unumgr\u00e4nzt \u2014 endlosen Ewigkeiten!", "tokens": ["Ins", "Thor", "der", "un\u00b7um\u00b7gr\u00e4nzt", "end\u00b7lo\u00b7sen", "E\u00b7wig\u00b7kei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJD", "$(", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "Die Zeit flieht schneller als ein Pfeil,", "tokens": ["Die", "Zeit", "flieht", "schnel\u00b7ler", "als", "ein", "Pfeil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "O m\u00f6cht ich ihren kleinsten Theil", "tokens": ["O", "m\u00f6cht", "ich", "ih\u00b7ren", "kleins\u00b7ten", "Theil"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Noch haschen um ihn anzuwenden!", "tokens": ["Noch", "ha\u00b7schen", "um", "ihn", "an\u00b7zu\u00b7wen\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "M\u00f6cht\u2019 ich den Tag mit deinem Umgang enden,", "tokens": ["M\u00f6cht'", "ich", "den", "Tag", "mit", "dei\u00b7nem", "Um\u00b7gang", "en\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Und wenn die Sonne mich begr\u00fc\u00dft,", "tokens": ["Und", "wenn", "die", "Son\u00b7ne", "mich", "be\u00b7gr\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Erwachend danken, da\u00df sie meine Tage mi\u00dft;", "tokens": ["Er\u00b7wa\u00b7chend", "dan\u00b7ken", ",", "da\u00df", "sie", "mei\u00b7ne", "Ta\u00b7ge", "mi\u00dft", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und wenn der Mond erleuchten kommt die N\u00e4chte,", "tokens": ["Und", "wenn", "der", "Mond", "er\u00b7leuch\u00b7ten", "kommt", "die", "N\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVINF", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "La\u00df mich ihn sehn und denken, da\u00df", "tokens": ["La\u00df", "mich", "ihn", "sehn", "und", "den\u00b7ken", ",", "da\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVIMP", "PPER", "PPER", "VVINF", "KON", "VVINF", "$,", "KOUS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Er auch mir Zeit zur Vorbereitung messe!", "tokens": ["Er", "auch", "mir", "Zeit", "zur", "Vor\u00b7be\u00b7rei\u00b7tung", "mes\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PPER", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Erinnre du mich selbst, da\u00df ich es nicht vergesse,", "tokens": ["E\u00b7rinn\u00b7re", "du", "mich", "selbst", ",", "da\u00df", "ich", "es", "nicht", "ver\u00b7ges\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "$,", "KOUS", "PPER", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Mein K\u00f6rper welkt dahin wie Gra\u00df.", "tokens": ["Mein", "K\u00f6r\u00b7per", "welkt", "da\u00b7hin", "wie", "Gra\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PAV", "KOKOM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Seh ich die Gr\u00e4ber, la\u00df mich nicht erschrecken,", "tokens": ["Seh", "ich", "die", "Gr\u00e4\u00b7ber", ",", "la\u00df", "mich", "nicht", "er\u00b7schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "VVIMP", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.2": {"text": "Mich wird auch einst solch kleiner H\u00fcgel decken,", "tokens": ["Mich", "wird", "auch", "einst", "solch", "klei\u00b7ner", "H\u00fc\u00b7gel", "de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PIAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mich tr\u00e4gt man auch, die Hand voll Staub, zu Staub,", "tokens": ["Mich", "tr\u00e4gt", "man", "auch", ",", "die", "Hand", "voll", "Staub", ",", "zu", "Staub", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADV", "$,", "ART", "NN", "ADJD", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und ich beschuldige mir selber diesen Raub", "tokens": ["Und", "ich", "be\u00b7schul\u00b7di\u00b7ge", "mir", "sel\u00b7ber", "die\u00b7sen", "Raub"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der gr\u00f6sten H\u00e4lfte meiner Tage;", "tokens": ["Der", "gr\u00f6s\u00b7ten", "H\u00e4lf\u00b7te", "mei\u00b7ner", "Ta\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nur da\u00df dein Ausruf mirs nicht vor den Engeln sage,", "tokens": ["Nur", "da\u00df", "dein", "Aus\u00b7ruf", "mirs", "nicht", "vor", "den", "En\u00b7geln", "sa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPOSAT", "NN", "NE", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Du Ursprung alles dessen, was da ist.", "tokens": ["Du", "Ur\u00b7sprung", "al\u00b7les", "des\u00b7sen", ",", "was", "da", "ist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PIS", "PDS", "$,", "PRELS", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Herr, der du aller Dinge Anfang bist,", "tokens": ["Herr", ",", "der", "du", "al\u00b7ler", "Din\u00b7ge", "An\u00b7fang", "bist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "PIAT", "NN", "NN", "VAFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.9": {"text": "La\u00df mich, so oft ein Jahr vor mir herum gelaufen,", "tokens": ["La\u00df", "mich", ",", "so", "oft", "ein", "Jahr", "vor", "mir", "he\u00b7rum", "ge\u00b7lau\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "ADV", "ADV", "ART", "NN", "APPR", "PPER", "APZR", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Nachrechnen meine durchgelebte Zeit,", "tokens": ["Nach\u00b7rech\u00b7nen", "mei\u00b7ne", "durch\u00b7ge\u00b7leb\u00b7te", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Mit Vorsatz besser sie zu kaufen,", "tokens": ["Mit", "Vor\u00b7satz", "bes\u00