{"dta.poem.6649": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von denen Sinnlichkeiten  \n insgemein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wird nicht in uns versp\u00fcrt", "tokens": ["Wird", "nicht", "in", "uns", "ver\u00b7sp\u00fcrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "APPR", "PPER", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df sich die Steine selbst, ja gar Metallen, regen?", "tokens": ["Da\u00df", "sich", "die", "Stei\u00b7ne", "selbst", ",", "ja", "gar", "Me\u00b7tal\u00b7len", ",", "re\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "ADV", "$,", "ADV", "ADV", "NN", "$,", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das Eisen, vom Magnet ber\u00fchrt,", "tokens": ["Das", "Ei\u00b7sen", ",", "vom", "Mag\u00b7net", "be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Wird Nordtwerts hingef\u00fchrt.", "tokens": ["Wird", "Nordt\u00b7werts", "hin\u00b7ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es scheinet das Metall bem\u00fcht den Stein zu finden,", "tokens": ["Es", "schei\u00b7net", "das", "Me\u00b7tall", "be\u00b7m\u00fcht", "den", "Stein", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Stein sucht ebenfalls mit ihm sich zu verbinden,", "tokens": ["Der", "Stein", "sucht", "e\u00b7ben\u00b7falls", "mit", "ihm", "sich", "zu", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "PPER", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und \u00f6ffters siehet man sie beyde sich bem\u00fchn,", "tokens": ["Und", "\u00f6ff\u00b7ters", "sie\u00b7het", "man", "sie", "bey\u00b7de", "sich", "be\u00b7m\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "PPER", "PIS", "PRF", "VVINF", "$,"], "meter": "-+-++--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Einander zu entfliehn,", "tokens": ["Ein\u00b7an\u00b7der", "zu", "ent\u00b7fliehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Als wenn sie sich einander scheuten,", "tokens": ["Als", "wenn", "sie", "sich", "ein\u00b7an\u00b7der", "scheu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Als wollten sie zusammen streiten.", "tokens": ["Als", "woll\u00b7ten", "sie", "zu\u00b7sam\u00b7men", "strei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Doch kan man ihnen ja wol keine Wahl", "tokens": ["Doch", "kan", "man", "ih\u00b7nen", "ja", "wol", "kei\u00b7ne", "Wahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PIS", "PPER", "ADV", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und auch kein F\u00fchlen zuerkennen.", "tokens": ["Und", "auch", "kein", "F\u00fch\u00b7len", "zu\u00b7er\u00b7ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Ein zarter Zeug, den keiner Augen Strahl", "tokens": ["Ein", "zar\u00b7ter", "Zeug", ",", "den", "kei\u00b7ner", "Au\u00b7gen", "Strahl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Gewahr wird und vermag zu sehen,", "tokens": ["Ge\u00b7wahr", "wird", "und", "ver\u00b7mag", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "KON", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Wird sonder Zweifel durch sie gehen,", "tokens": ["Wird", "son\u00b7der", "Zwei\u00b7fel", "durch", "sie", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Stoss\u2019t und beweget sie und kan sie zwingen,", "tokens": ["Sto\u00b7ss't", "und", "be\u00b7we\u00b7get", "sie", "und", "kan", "sie", "zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPER", "KON", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.17": {"text": "Gesetze, die geheim sind, zu vollbringen.", "tokens": ["Ge\u00b7set\u00b7ze", ",", "die", "ge\u00b7heim", "sind", ",", "zu", "voll\u00b7brin\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADJD", "VAFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}