{"textgrid.poem.67213": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das Gl\u00e4nzen der Natur ist h\u00f6heres Erscheinen,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Gl\u00e4nzen der Natur ist h\u00f6heres Erscheinen,", "tokens": ["Das", "Gl\u00e4n\u00b7zen", "der", "Na\u00b7tur", "ist", "h\u00f6\u00b7he\u00b7res", "Er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo sich der Tag mit vielen Freuden endet,", "tokens": ["Wo", "sich", "der", "Tag", "mit", "vie\u00b7len", "Freu\u00b7den", "en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es ist das Jahr, das sich mit Pracht vollendet,", "tokens": ["Es", "ist", "das", "Jahr", ",", "das", "sich", "mit", "Pracht", "voll\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "PRF", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo Fr\u00fcchte sich mit frohem Glanz vereinen.", "tokens": ["Wo", "Fr\u00fcch\u00b7te", "sich", "mit", "fro\u00b7hem", "Glanz", "ver\u00b7ei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PRF", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das Erdenrund ist so g