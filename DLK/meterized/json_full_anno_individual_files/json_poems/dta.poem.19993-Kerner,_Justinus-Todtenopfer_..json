{"dta.poem.19993": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Todtenopfer .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Frisch aufgebl\u00fchet stand die Heimat wieder,", "tokens": ["Frisch", "auf\u00b7ge\u00b7bl\u00fc\u00b7het", "stand", "die", "Hei\u00b7mat", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vers\u00f6hnt dich lieben Fl\u00fcchtling zu empfangen,", "tokens": ["Ver\u00b7s\u00f6hnt", "dich", "lie\u00b7ben", "Fl\u00fccht\u00b7ling", "zu", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aus dunklem Gr\u00fcn mondhelle Bl\u00fcten drangen,", "tokens": ["Aus", "dunk\u00b7lem", "Gr\u00fcn", "mond\u00b7hel\u00b7le", "Bl\u00fc\u00b7ten", "dran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den V\u00f6geln wuchs ein farbig neu Gefieder.", "tokens": ["Den", "V\u00f6\u00b7geln", "wuchs", "ein", "far\u00b7big", "neu", "Ge\u00b7fie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJD", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Aus wolk'gen W\u00e4ldern t\u00f6nten ihre Lieder,", "tokens": ["Aus", "wol\u00b7k'\u00b7gen", "W\u00e4l\u00b7dern", "t\u00f6n\u00b7ten", "ih\u00b7re", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Im Thal, auf Bergen, Hirt und Hirtin sangen,", "tokens": ["Im", "Thal", ",", "auf", "Ber\u00b7gen", ",", "Hirt", "und", "Hir\u00b7tin", "san\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "NN", "$,", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es war als senkt' in aller Farben Prangen", "tokens": ["Es", "war", "als", "senkt'", "in", "al\u00b7ler", "Far\u00b7ben", "Pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "KOKOM", "VVFIN", "APPR", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der reiche Himmel sich zur Erde nieder.", "tokens": ["Der", "rei\u00b7che", "Him\u00b7mel", "sich", "zur", "Er\u00b7de", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und Arme waren ausgereckt in Freude,", "tokens": ["Und", "Ar\u00b7me", "wa\u00b7ren", "aus\u00b7ge\u00b7reckt", "in", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Herzen schlugen sehnend dir entgegen,", "tokens": ["Und", "Her\u00b7zen", "schlu\u00b7gen", "seh\u00b7nend", "dir", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADJD", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vom rauhen Norden solltest du erwarmen.", "tokens": ["Vom", "rau\u00b7hen", "Nor\u00b7den", "soll\u00b7test", "du", "er\u00b7war\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Da nahm dich uns der Tod mit blassem Neide.", "tokens": ["Da", "nahm", "dich", "uns", "der", "Tod", "mit", "blas\u00b7sem", "Nei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nun welke nur, du reicher Fr\u00fchlingssegen!", "tokens": ["Nun", "wel\u00b7ke", "nur", ",", "du", "rei\u00b7cher", "Fr\u00fch\u00b7lings\u00b7se\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "PPER", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nichts frommst du mehr mit deinem Schmuck uns Armen.", "tokens": ["Nichts", "frommst", "du", "mehr", "mit", "dei\u00b7nem", "Schmuck", "uns", "Ar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Du theurer Bruder! der durch's steilste Leben", "tokens": ["Du", "theu\u00b7rer", "Bru\u00b7der", "!", "der", "durch's", "steils\u00b7te", "Le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$.", "ART", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kraftvoll, ein Wandrer ohne Stab, gegangen!", "tokens": ["Kraft\u00b7voll", ",", "ein", "Wand\u00b7rer", "oh\u00b7ne", "Stab", ",", "ge\u00b7gan\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "ART", "NN", "APPR", "NN", "$,", "VVPP", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.3": {"text": "O k\u00f6nnt' auch ich die Herberg bald erlangen,", "tokens": ["O", "k\u00f6nnt'", "auch", "ich", "die", "Her\u00b7berg", "bald", "er\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ADV", "PPER", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die dir der Tod, der letzte Wirth, gegeben!", "tokens": ["Die", "dir", "der", "Tod", ",", "der", "letz\u00b7te", "Wirth", ",", "ge\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Nach hellem Trunk von heimatlichen Reben", "tokens": ["Nach", "hel\u00b7lem", "Trunk", "von", "hei\u00b7mat\u00b7li\u00b7chen", "Re\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Trugst du im fernen Norden hei\u00df Verlangen;", "tokens": ["Trugst", "du", "im", "fer\u00b7nen", "Nor\u00b7den", "hei\u00df", "Ver\u00b7lan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In dieser Herberg hast du ihn empfangen,", "tokens": ["In", "die\u00b7ser", "Her\u00b7berg", "hast", "du", "ihn", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Liebend der Heimat Geister Dich umschweben.", "tokens": ["Lie\u00b7bend", "der", "Hei\u00b7mat", "Geis\u00b7ter", "Dich", "um\u00b7schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "NN", "PPER", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.7": {"line.1": {"text": "Und nach dem Weg voll Unruh' und Beschwerde", "tokens": ["Und", "nach", "dem", "Weg", "voll", "Un\u00b7ruh'", "und", "Be\u00b7schwer\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie ruhen s\u00fc\u00df nun deine m\u00fcden Glieder!", "tokens": ["Wie", "ru\u00b7hen", "s\u00fc\u00df", "nun", "dei\u00b7ne", "m\u00fc\u00b7den", "Glie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADJD", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie ist dir's wohl im heimatlichen Bette!", "tokens": ["Wie", "ist", "dir's", "wohl", "im", "hei\u00b7mat\u00b7li\u00b7chen", "Bet\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PIS", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}}, "stanza.8": {"line.1": {"text": "Noch tobet w\u00fcster Streit hier auf der Erde,", "tokens": ["Noch", "to\u00b7bet", "w\u00fcs\u00b7ter", "Streit", "hier", "auf", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Still blickt der Mond auf deinen H\u00fcgel nieder,", "tokens": ["Still", "blickt", "der", "Mond", "auf", "dei\u00b7nen", "H\u00fc\u00b7gel", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Rosen sprossen friedsam an der St\u00e4tte.", "tokens": ["Und", "Ro\u00b7sen", "spros\u00b7sen", "fried\u00b7sam", "an", "der", "St\u00e4t\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Du strebtest oft, ein herzlich Kind, mit Thr\u00e4nen", "tokens": ["Du", "streb\u00b7test", "oft", ",", "ein", "herz\u00b7lich", "Kind", ",", "mit", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ART", "ADJD", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zur\u00fcck zur s\u00fc\u00dfen Heimat, zu den Lieben,", "tokens": ["Zu\u00b7r\u00fcck", "zur", "s\u00fc\u00b7\u00dfen", "Hei\u00b7mat", ",", "zu", "den", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die fern in Kampf und Sturm Dich mu\u00dften w\u00e4hnen,", "tokens": ["Die", "fern", "in", "Kampf", "und", "Sturm", "Dich", "mu\u00df\u00b7ten", "w\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "NN", "KON", "NN", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Indessen sie im sichern Port geblieben.", "tokens": ["In\u00b7des\u00b7sen", "sie", "im", "si\u00b7chern", "Port", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Du treues Herz! nun ist erf\u00fcllt dein Sehnen,", "tokens": ["Du", "treu\u00b7es", "Herz", "!", "nun", "ist", "er\u00b7f\u00fcllt", "dein", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "ADV", "VAFIN", "VVPP", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein Auge soll fortan sich nimmer tr\u00fcben;", "tokens": ["Mein", "Au\u00b7ge", "soll", "for\u00b7tan", "sich", "nim\u00b7mer", "tr\u00fc\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hast deine Heimat nun, bist nun bei jenen,", "tokens": ["Hast", "dei\u00b7ne", "Hei\u00b7mat", "nun", ",", "bist", "nun", "bei", "je\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "$,", "VAFIN", "ADV", "APPR", "PDAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "An die du weinend Gru\u00df und Ku\u00df geschrieben.", "tokens": ["An", "die", "du", "wei\u00b7nend", "Gru\u00df", "und", "Ku\u00df", "ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVPP", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Im Morgenroth seh' ich verkl\u00e4rt dich wallen,", "tokens": ["Im", "Mor\u00b7gen\u00b7roth", "seh'", "ich", "ver\u00b7kl\u00e4rt", "dich", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo Sterne durch den Dom des Himmels ziehen,", "tokens": ["Wo", "Ster\u00b7ne", "durch", "den", "Dom", "des", "Him\u00b7mels", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du gehst mit mir durch stille Au'n und Haine.", "tokens": ["Du", "gehst", "mit", "mir", "durch", "stil\u00b7le", "Au'n", "und", "Hai\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPR", "ADJA", "NN", "KON", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Oft h\u00f6r' ich deine liebe Stimme schallen,", "tokens": ["Oft", "h\u00f6r'", "ich", "dei\u00b7ne", "lie\u00b7be", "Stim\u00b7me", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fchl' deinen Ku\u00df auf meinen Lippen gl\u00fchen,", "tokens": ["F\u00fchl'", "dei\u00b7nen", "Ku\u00df", "auf", "mei\u00b7nen", "Lip\u00b7pen", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Seh' dich mitleidig l\u00e4cheln, wenn ich weine.", "tokens": ["Seh'", "dich", "mit\u00b7lei\u00b7dig", "l\u00e4\u00b7cheln", ",", "wenn", "ich", "wei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}