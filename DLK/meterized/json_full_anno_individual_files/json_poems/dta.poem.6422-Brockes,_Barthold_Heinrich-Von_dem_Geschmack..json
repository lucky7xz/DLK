{"dta.poem.6422": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Geschmack.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Daher kommt Eckel offt und Lust bey einer Speise,", "tokens": ["Da\u00b7her", "kommt", "E\u00b7ckel", "offt", "und", "Lust", "bey", "ei\u00b7ner", "Spei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "NN", "ADV", "KON", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die diesem widrig ist, und jenem herrlich schmeckt.", "tokens": ["Die", "die\u00b7sem", "wid\u00b7rig", "ist", ",", "und", "je\u00b7nem", "herr\u00b7lich", "schmeckt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "ADJD", "VAFIN", "$,", "KON", "PDAT", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Daher nun kan ein jeder wehlen,", "tokens": ["Da\u00b7her", "nun", "kan", "ein", "je\u00b7der", "weh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VMFIN", "ART", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nach eigner Lust, was ihm gef\u00e4llt,", "tokens": ["Nach", "eig\u00b7ner", "Lust", ",", "was", "ihm", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So, da\u00df man \u00f6ffters isst, mit Lust der Seelen,", "tokens": ["So", ",", "da\u00df", "man", "\u00f6ff\u00b7ters", "isst", ",", "mit", "Lust", "der", "See\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PIS", "ADV", "VVFIN", "$,", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Das, was ein andrer hier f\u00fcr bitter h\u00e4lt.", "tokens": ["Das", ",", "was", "ein", "an\u00b7drer", "hier", "f\u00fcr", "bit\u00b7ter", "h\u00e4lt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "ART", "ADJA", "ADV", "APPR", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Allein, wir sehen gar,", "tokens": ["Al\u00b7lein", ",", "wir", "se\u00b7hen", "gar", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Da\u00df unser eigener Geschmack sich \u00e4ndern kan.", "tokens": ["Da\u00df", "un\u00b7ser", "ei\u00b7ge\u00b7ner", "Ge\u00b7schmack", "sich", "\u00e4n\u00b7dern", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PRF", "VVINF", "VMFIN", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.9": {"text": "Denn man gew\u00f6hnet offt gewissen Schmack sich an,", "tokens": ["Denn", "man", "ge\u00b7w\u00f6h\u00b7net", "offt", "ge\u00b7wis\u00b7sen", "Schmack", "sich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADV", "ADJA", "NN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und \u00f6ffters wird uns s\u00fc\u00df, was erst verdr\u00fc\u00dflich war.", "tokens": ["Und", "\u00f6ff\u00b7ters", "wird", "uns", "s\u00fc\u00df", ",", "was", "erst", "ver\u00b7dr\u00fc\u00df\u00b7lich", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADJD", "$,", "PRELS", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}