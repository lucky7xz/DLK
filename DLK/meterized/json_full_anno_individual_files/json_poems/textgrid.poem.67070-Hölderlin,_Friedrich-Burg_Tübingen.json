{"textgrid.poem.67070": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Burg T\u00fcbingen", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Still und \u00f6de steht der V\u00e4ter Feste,", "tokens": ["Still", "und", "\u00f6\u00b7de", "steht", "der", "V\u00e4\u00b7ter", "Fes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schwarz und moosbewachsen Pfort und Turm,", "tokens": ["Schwarz", "und", "moos\u00b7be\u00b7wach\u00b7sen", "Pfort", "und", "Turm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Durch der Felsenw\u00e4nde tr\u00fcbe Reste", "tokens": ["Durch", "der", "Fel\u00b7sen\u00b7w\u00e4n\u00b7de", "tr\u00fc\u00b7be", "Res\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Saust um Mitternacht der Wintersturm,", "tokens": ["Saust", "um", "Mit\u00b7ter\u00b7nacht", "der", "Win\u00b7ter\u00b7sturm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Dieser schaurigen Gemache Tr\u00fcmmer", "tokens": ["Die\u00b7ser", "schau\u00b7ri\u00b7gen", "Ge\u00b7ma\u00b7che", "Tr\u00fcm\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "ADJA", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Heischen sich umsonst ein Siegesmal,", "tokens": ["Hei\u00b7schen", "sich", "um\u00b7sonst", "ein", "Sie\u00b7ges\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Und des Schlachtger\u00e4tes Heiligt\u00fcmer", "tokens": ["Und", "des", "Schlacht\u00b7ge\u00b7r\u00e4\u00b7tes", "Hei\u00b7lig\u00b7t\u00fc\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Schlummern Todesschlaf im Waffensaal.", "tokens": ["Schlum\u00b7mern", "To\u00b7des\u00b7schlaf", "im", "Waf\u00b7fen\u00b7saal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Hier ert\u00f6nen keine Festges\u00e4nge,", "tokens": ["Hier", "er\u00b7t\u00f6\u00b7nen", "kei\u00b7ne", "Fest\u00b7ge\u00b7s\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Lobzupreisen Manas Heldenland,", "tokens": ["Lo\u00b7bzu\u00b7prei\u00b7sen", "Ma\u00b7nas", "Hel\u00b7den\u00b7land", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NE", "NE", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Keine Fahne weht im Siegsgepr\u00e4nge", "tokens": ["Kei\u00b7ne", "Fah\u00b7ne", "weht", "im", "Siegs\u00b7ge\u00b7pr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Hochgehoben in des Kriegers Hand,", "tokens": ["Hoch\u00b7ge\u00b7ho\u00b7ben", "in", "des", "Krie\u00b7gers", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Keine Rosse wiehern in den Toren,", "tokens": ["Kei\u00b7ne", "Ros\u00b7se", "wie\u00b7hern", "in", "den", "To\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Bis die Edeln zum Turniere nahn,", "tokens": ["Bis", "die", "E\u00b7deln", "zum", "Tur\u00b7nie\u00b7re", "nahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "ADJA", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Keine Doggen, treu, und auserkoren,", "tokens": ["Kei\u00b7ne", "Dog\u00b7gen", ",", "treu", ",", "und", "au\u00b7ser\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "ADJD", "$,", "KON", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Schmiegen sich den blanken Panzern an.", "tokens": ["Schmie\u00b7gen", "sich", "den", "blan\u00b7ken", "Pan\u00b7zern", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Bei des Hiefhorns schallendem Get\u00f6ne", "tokens": ["Bei", "des", "Hief\u00b7horns", "schal\u00b7len\u00b7dem", "Ge\u00b7t\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Zieht kein Fr\u00e4ulein in der Hirsche Tal,", "tokens": ["Zieht", "kein", "Fr\u00e4u\u00b7lein", "in", "der", "Hir\u00b7sche", "Tal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Siegesd\u00fcrstend g\u00fcrten keine S\u00f6hne", "tokens": ["Sie\u00b7ges\u00b7d\u00fcrs\u00b7tend", "g\u00fcr\u00b7ten", "kei\u00b7ne", "S\u00f6h\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Um die Lenden ihrer V\u00e4ter Stahl,", "tokens": ["Um", "die", "Len\u00b7den", "ih\u00b7rer", "V\u00e4\u00b7ter", "Stahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Keine M\u00fctter jauchzen von der Zinne", "tokens": ["Kei\u00b7ne", "M\u00fct\u00b7ter", "jauch\u00b7zen", "von", "der", "Zin\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Ob der Knaben stolzer Wiederkehr,", "tokens": ["Ob", "der", "Kna\u00b7ben", "stol\u00b7zer", "Wie\u00b7der\u00b7kehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Und den ersten Ku\u00df versch\u00e4mter Minne", "tokens": ["Und", "den", "ers\u00b7ten", "Ku\u00df", "ver\u00b7sch\u00e4m\u00b7ter", "Min\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Weihn der Narbe keine Br\u00e4ute mehr.", "tokens": ["Weihn", "der", "Nar\u00b7be", "kei\u00b7ne", "Br\u00e4u\u00b7te", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PIAT", "NN", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Aber schaurige Begeisterungen", "tokens": ["A\u00b7ber", "schau\u00b7ri\u00b7ge", "Be\u00b7geis\u00b7te\u00b7run\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Weckt die Riesin in des Enkels Brust,", "tokens": ["Weckt", "die", "Rie\u00b7sin", "in", "des", "En\u00b7kels", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "S\u00e4nge, die der V\u00e4ter Mund gesungen,", "tokens": ["S\u00e4n\u00b7ge", ",", "die", "der", "V\u00e4\u00b7ter", "Mund", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Zeugt der Wehmut zauberische Lust,", "tokens": ["Zeugt", "der", "Weh\u00b7mut", "zau\u00b7be\u00b7ri\u00b7sche", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Ferne von dem t\u00f6rigen Gew\u00fchle,", "tokens": ["Fer\u00b7ne", "von", "dem", "t\u00f6\u00b7ri\u00b7gen", "Ge\u00b7w\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Von dem Stolze der Gefallenen,", "tokens": ["Von", "dem", "Stol\u00b7ze", "der", "Ge\u00b7fal\u00b7le\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.7": {"text": "D\u00e4mmern niegeahndete Gef\u00fchle", "tokens": ["D\u00e4m\u00b7mern", "nie\u00b7geahn\u00b7de\u00b7te", "Ge\u00b7f\u00fch\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "In der Seele des Begeisterten.", "tokens": ["In", "der", "See\u00b7le", "des", "Be\u00b7geis\u00b7ter\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}}, "stanza.5": {"line.1": {"text": "Hier im Schatten grauer Felsenw\u00e4nde,", "tokens": ["Hier", "im", "Schat\u00b7ten", "grau\u00b7er", "Fel\u00b7sen\u00b7w\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Von des St\u00e4dters Blicken unentweiht,", "tokens": ["Von", "des", "St\u00e4d\u00b7ters", "Bli\u00b7cken", "un\u00b7ent\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Kn\u00fcpfe Freundschaft deutsche Biederh\u00e4nde,", "tokens": ["Kn\u00fcp\u00b7fe", "Freund\u00b7schaft", "deut\u00b7sche", "Bie\u00b7der\u00b7h\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Schw\u00f6re Liebe f\u00fcr die Ewigkeit,", "tokens": ["Schw\u00f6\u00b7re", "Lie\u00b7be", "f\u00fcr", "die", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Hier, wo Heldenschatten niederrauschen,", "tokens": ["Hier", ",", "wo", "Hel\u00b7den\u00b7schat\u00b7ten", "nie\u00b7der\u00b7rau\u00b7schen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Traufe Vatersegen auf den Sohn,", "tokens": ["Trau\u00b7fe", "Va\u00b7ter\u00b7se\u00b7gen", "auf", "den", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Wo den Lieblingen die Geister lauschen,", "tokens": ["Wo", "den", "Lieb\u00b7lin\u00b7gen", "die", "Geis\u00b7ter", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-++--+-+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Spreche Freiheit den Tyrannen Hohn!", "tokens": ["Spre\u00b7che", "Frei\u00b7heit", "den", "Ty\u00b7ran\u00b7nen", "Hohn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Hier verweine die verschlo\u00dfne Z\u00e4hre,", "tokens": ["Hier", "ver\u00b7wei\u00b7ne", "die", "ver\u00b7schlo\u00df\u00b7ne", "Z\u00e4h\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wer umsonst nach Menschenfreude ringt,", "tokens": ["Wer", "um\u00b7sonst", "nach", "Men\u00b7schen\u00b7freu\u00b7de", "ringt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wen die Krone nicht der Bardenehre,", "tokens": ["Wen", "die", "Kro\u00b7ne", "nicht", "der", "Bar\u00b7de\u00b7neh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Nicht des Liebchens Schwanenarm umschlingt,", "tokens": ["Nicht", "des", "Lieb\u00b7chens", "Schwa\u00b7nen\u00b7arm", "um\u00b7schlingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Wer von Zweifeln ohne Rast gequ\u00e4let,", "tokens": ["Wer", "von", "Zwei\u00b7feln", "oh\u00b7ne", "Rast", "ge\u00b7qu\u00e4\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Von des Irrtums peinigendem Los,", "tokens": ["Von", "des", "Irr\u00b7tums", "pei\u00b7ni\u00b7gen\u00b7dem", "Los", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.7": {"text": "Schlummerlose Mittern\u00e4chte z\u00e4hlet,", "tokens": ["Schlum\u00b7mer\u00b7lo\u00b7se", "Mit\u00b7ter\u00b7n\u00e4ch\u00b7te", "z\u00e4h\u00b7let", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Komme zu genesen in der Ruhe Scho\u00df.", "tokens": ["Kom\u00b7me", "zu", "ge\u00b7ne\u00b7sen", "in", "der", "Ru\u00b7he", "Scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "APPR", "ART", "NN", "NN", "$."], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Aber wer des Bruders Fehle r\u00fcget", "tokens": ["A\u00b7ber", "wer", "des", "Bru\u00b7ders", "Feh\u00b7le", "r\u00fc\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ART", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mit der Schlangenzunge losem Spott,", "tokens": ["Mit", "der", "Schlan\u00b7gen\u00b7zun\u00b7ge", "lo\u00b7sem", "Spott", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wem f\u00fcr Adeltaten Gold gen\u00fcget,", "tokens": ["Wem", "f\u00fcr", "A\u00b7del\u00b7ta\u00b7ten", "Gold", "ge\u00b7n\u00fc\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sei er Sklave oder Erdengott,", "tokens": ["Sei", "er", "Skla\u00b7ve", "o\u00b7der", "Er\u00b7den\u00b7gott", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Er entweihe nicht die heilge Reste,", "tokens": ["Er", "ent\u00b7wei\u00b7he", "nicht", "die", "heil\u00b7ge", "Res\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Die der V\u00e4ter stolzer Fu\u00df betrat,", "tokens": ["Die", "der", "V\u00e4\u00b7ter", "stol\u00b7zer", "Fu\u00df", "be\u00b7trat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJA", "NN", "VVFIN", "$