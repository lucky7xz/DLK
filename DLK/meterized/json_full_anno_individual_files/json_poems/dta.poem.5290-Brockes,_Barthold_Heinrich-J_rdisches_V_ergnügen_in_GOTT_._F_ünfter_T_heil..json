{"dta.poem.5290": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In diesem wird man mit Verwunderung gewahr", "tokens": ["In", "die\u00b7sem", "wird", "man", "mit", "Ver\u00b7wun\u00b7de\u00b7rung", "ge\u00b7wahr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "VAFIN", "PIS", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Kn\u00f6spgen schon f\u00fcrs k\u00fcnft\u2019ge Jahr.", "tokens": ["Ein", "Kn\u00f6sp\u00b7gen", "schon", "f\u00fcrs", "k\u00fcnft'\u00b7ge", "Jahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hier kann man nun zugleich der Wunder Endzweck sehn,", "tokens": ["Hier", "kann", "man", "nun", "zu\u00b7gleich", "der", "Wun\u00b7der", "End\u00b7zweck", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADV", "ADV", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In dem wir \u00f6fters vier, oft f\u00fcnf, gerade Stangen,", "tokens": ["In", "dem", "wir", "\u00f6f\u00b7ters", "vier", ",", "oft", "f\u00fcnf", ",", "ge\u00b7ra\u00b7de", "Stan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "CARD", "$,", "ADV", "CARD", "$,", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die neue Wunder noch in sich befangen,", "tokens": ["Die", "neu\u00b7e", "Wun\u00b7der", "noch", "in", "sich", "be\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "APPR", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Vor Lust mit Recht erstaunet, sehn.", "tokens": ["Vor", "Lust", "mit", "Recht", "er\u00b7stau\u00b7net", ",", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Man sieht wie diese Stiel\u2019 all\u2019 an gewissen Stellen", "tokens": ["Man", "sieht", "wie", "die\u00b7se", "Stiel'", "all'", "an", "ge\u00b7wis\u00b7sen", "Stel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "KOKOM", "PDAT", "NN", "PIS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "(kein Mensch begreift wodurch) gem\u00e4chlich schwellen,", "tokens": ["(", "kein", "Mensch", "be\u00b7greift", "wo\u00b7durch", ")", "ge\u00b7m\u00e4ch\u00b7lich", "schwel\u00b7len", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PIAT", "NN", "VVFIN", "PWAV", "$(", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Sich dehnen, da wir denn in ihren hohlen Rinden", "tokens": ["Sich", "deh\u00b7nen", ",", "da", "wir", "denn", "in", "ih\u00b7ren", "hoh\u00b7len", "Rin\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PRF", "PDS", "$,", "KOUS", "PPER", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Den Sitz der jungen Kirsche finden.", "tokens": ["Den", "Sitz", "der", "jun\u00b7gen", "Kir\u00b7sche", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Die Rinden, die darauf sich h\u00f6her noch erstrecken,", "tokens": ["Die", "Rin\u00b7den", ",", "die", "da\u00b7rauf", "sich", "h\u00f6\u00b7her", "noch", "er\u00b7stre\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PAV", "PRF", "ADJD", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Formiren Knospen abermahl,", "tokens": ["For\u00b7mi\u00b7ren", "Knos\u00b7pen", "a\u00b7ber\u00b7mahl", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "ADV", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.13": {"text": "Worin der zarten Bl\u00fcth\u2019 schnee-weisse Bl\u00e4tter stecken,", "tokens": ["Wo\u00b7rin", "der", "zar\u00b7ten", "Bl\u00fcth'", "schnee\u00b7weis\u00b7se", "Bl\u00e4t\u00b7ter", "ste\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Jmgleichen weisse Z\u00e4serlein", "tokens": ["Jm\u00b7glei\u00b7chen", "weis\u00b7se", "Z\u00e4\u00b7ser\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Noch \u00fcber drey\u00dfig an der Zahl,", "tokens": ["Noch", "\u00fc\u00b7ber", "drey\u00b7\u00dfig", "an", "der", "Zahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "CARD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Die alle, echt als wie mit g\u00fcldnen Kn\u00f6pfgen, prangen,", "tokens": ["Die", "al\u00b7le", ",", "echt", "als", "wie", "mit", "g\u00fcld\u00b7nen", "Kn\u00f6pf\u00b7gen", ",", "pran\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PIS", "$,", "ADJD", "KOKOM", "KOKOM", "APPR", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und die die kleinen gr\u00fcnen Stangen,", "tokens": ["Und", "die", "die", "klei\u00b7nen", "gr\u00fc\u00b7nen", "Stan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Die aus der Frucht sich aufw\u00e4rts strecken,", "tokens": ["Die", "aus", "der", "Frucht", "sich", "auf\u00b7w\u00e4rts", "stre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Umgeben und bedecken.", "tokens": ["Um\u00b7ge\u00b7ben", "und", "be\u00b7de\u00b7cken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Wer nun noch erst von einer Bl\u00fcht\u2019", "tokens": ["Wer", "nun", "noch", "erst", "von", "ei\u00b7ner", "Bl\u00fcht'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Ein eintzigs Blatt", "tokens": ["Ein", "eint\u00b7zigs", "Blatt"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.22": {"text": "Betrachtet und besieht,", "tokens": ["Be\u00b7trach\u00b7tet", "und", "be\u00b7sieht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Wie viel es kleine Adern hat,", "tokens": ["Wie", "viel", "es", "klei\u00b7ne", "A\u00b7dern", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Und die\u00df zusammen nimmt, und mit Vernunft erweget,", "tokens": ["Und", "die\u00df", "zu\u00b7sam\u00b7men", "nimmt", ",", "und", "mit", "Ver\u00b7nunft", "er\u00b7we\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADV", "VVFIN", "$,", "KON", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Was eine Kirsche nur f\u00fcr Wunder in sich heget;", "tokens": ["Was", "ei\u00b7ne", "Kir\u00b7sche", "nur", "f\u00fcr", "Wun\u00b7der", "in", "sich", "he\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADV", "APPR", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ja, welcher erst bedenckt, wie eine solche Menge", "tokens": ["Ja", ",", "wel\u00b7cher", "erst", "be\u00b7denckt", ",", "wie", "ei\u00b7ne", "sol\u00b7che", "Men\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PRELS", "ADV", "VVFIN", "$,", "PWAV", "ART", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Von Blumen einen Zweig erf\u00fcllt,", "tokens": ["Von", "Blu\u00b7men", "ei\u00b7nen", "Zweig", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "So da\u00df sie ihn durchaus bedeckt und gantz verh\u00fcllt;", "tokens": ["So", "da\u00df", "sie", "ihn", "durc\u00b7haus", "be\u00b7deckt", "und", "gantz", "ver\u00b7h\u00fcllt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PPER", "ADV", "ADJD", "KON", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ja wie, im lieblichen Gepr\u00e4nge,", "tokens": ["Ja", "wie", ",", "im", "lieb\u00b7li\u00b7chen", "Ge\u00b7pr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "PWAV", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Die Blumen an viel tausend Zweigen", "tokens": ["Die", "Blu\u00b7men", "an", "viel", "tau\u00b7send", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PIAT", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Auf einem eintz\u2019gen Baum, sich zeigen,", "tokens": ["Auf", "ei\u00b7nem", "eint\u00b7z'\u00b7gen", "Baum", ",", "sich", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRF", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}}}}