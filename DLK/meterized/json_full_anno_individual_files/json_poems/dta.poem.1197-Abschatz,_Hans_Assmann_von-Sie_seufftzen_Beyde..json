{"dta.poem.1197": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Sie seufftzen Beyde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Du pflegest dich gantz laut/ ich heimlich zu beklagen/", "tokens": ["Du", "pfle\u00b7gest", "dich", "gantz", "laut", "/", "ich", "heim\u00b7lich", "zu", "be\u00b7kla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "$(", "PPER", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Seufftzer sind gemein bey dir und mir/ mein Kind:", "tokens": ["Die", "Seufft\u00b7zer", "sind", "ge\u00b7mein", "bey", "dir", "und", "mir", "/", "mein", "Kind", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPR", "PPER", "KON", "PPER", "$(", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich wei\u00df/ da\u00df meine nur auff dich gerichtet sind/", "tokens": ["Ich", "wei\u00df", "/", "da\u00df", "mei\u00b7ne", "nur", "auff", "dich", "ge\u00b7rich\u00b7tet", "sind", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPOSAT", "ADV", "APPR", "PPER", "VVPP", "VAFIN", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Von deinen wei\u00df ich nichts zu sagen.", "tokens": ["Von", "dei\u00b7nen", "wei\u00df", "ich", "nichts", "zu", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "VVFIN", "PPER", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Ander mag uns Neyd um unsre Seufftzer tragen:", "tokens": ["Ein", "An\u00b7der", "mag", "uns", "Neyd", "um", "uns\u00b7re", "Seufft\u00b7zer", "tra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich wei\u00df/ da\u00df meine nur auff dich gerichtet sind.", "tokens": ["Ich", "wei\u00df", "/", "da\u00df", "mei\u00b7ne", "nur", "auff", "dich", "ge\u00b7rich\u00b7tet", "sind", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPOSAT", "ADV", "APPR", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Wohin die deinen gehn/ mein allerliebstes Kind/", "tokens": ["Wo\u00b7hin", "die", "dei\u00b7nen", "gehn", "/", "mein", "al\u00b7ler\u00b7liebs\u00b7tes", "Kind", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "PPOSAT", "VVINF", "$(", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da wei\u00df ich nichts/ und will nichts sagen.", "tokens": ["Da", "wei\u00df", "ich", "nichts", "/", "und", "will", "nichts", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "$(", "KON", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}