{"dta.poem.14644": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der G\u00f6tter Irrfahrt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Denn sie seh'n zum Erstenmale", "tokens": ["Denn", "sie", "seh'n", "zum", "Ers\u00b7ten\u00b7ma\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nun die Sonne niedergeh'n", "tokens": ["Nun", "die", "Son\u00b7ne", "nie\u00b7der\u00b7geh'n"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und verwundert Berg' und Thale", "tokens": ["Und", "ver\u00b7wun\u00b7dert", "Ber\u00b7g'", "und", "Tha\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NN", "KON", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Tief im Abendrothe steh'n,", "tokens": ["Tief", "im", "A\u00b7ben\u00b7dro\u00b7the", "steh'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und der sch\u00f6nste Gott von allen", "tokens": ["Und", "der", "sch\u00f6ns\u00b7te", "Gott", "von", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sank erbleichend in den Duft,", "tokens": ["Sank", "er\u00b7blei\u00b7chend", "in", "den", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn dem Tode ist verfallen,", "tokens": ["Denn", "dem", "To\u00b7de", "ist", "ver\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wer geathmet ird'sche Luft.", "tokens": ["Wer", "ge\u00b7ath\u00b7met", "ird'\u00b7sche", "Luft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}