{"dta.poem.3408": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Elegie .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193292", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Meine Ida, wie anders seit gestern! Dein leuch-", "tokens": ["Mei\u00b7ne", "I\u00b7da", ",", "wie", "an\u00b7ders", "seit", "ge\u00b7stern", "!", "Dein", "leuch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NE", "$,", "PWAV", "ADV", "APPR", "ADV", "$.", "PPOSAT", "TRUNC"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "tendes Antlitz", "tokens": ["ten\u00b7des", "Ant\u00b7litz"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "--+-", "measure": "anapaest.init"}, "line.3": {"text": "Leuchtete gestern, es sah gestern dein Auge", "tokens": ["Leuch\u00b7te\u00b7te", "ge\u00b7stern", ",", "es", "sah", "ge\u00b7stern", "dein", "Au\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVINF", "$,", "PPER", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "so klar.", "tokens": ["so", "klar", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Welche Wolken schatten denn heute dein freundli-", "tokens": ["Wel\u00b7che", "Wol\u00b7ken", "schat\u00b7ten", "denn", "heu\u00b7te", "dein", "freund\u00b7li"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAT", "NN", "VVFIN", "ADV", "ADV", "PPOSAT", "TRUNC"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "ches Antlitz?", "tokens": ["ches", "Ant\u00b7litz", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Welche D\u00e4mmerung h\u00fcllt seinen erl\u00f6schen-", "tokens": ["Wel\u00b7che", "D\u00e4m\u00b7me\u00b7rung", "h\u00fcllt", "sei\u00b7nen", "er\u00b7l\u00f6\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAT", "NN", "VVFIN", "PPOSAT", "TRUNC"], "meter": "+-+---+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "den Glanz?", "tokens": ["den", "Glanz", "?"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Schwerarbeitend athmet dein Busen von lastenden", "tokens": ["Schwer\u00b7ar\u00b7bei\u00b7tend", "ath\u00b7met", "dein", "Bu\u00b7sen", "von", "las\u00b7ten\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "APPR", "VVINF"], "meter": "+-+-+--+--+--", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Seufzern;", "tokens": ["Seuf\u00b7zern", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Matter wanket dein Schritt; weinender", "tokens": ["Mat\u00b7ter", "wan\u00b7ket", "dein", "Schritt", ";", "wei\u00b7nen\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$.", "ADJA"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.12": {"text": "wimmert dein Laut.", "tokens": ["wim\u00b7mert", "dein", "Laut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.13": {"text": "Meine Ida, wie ist dir? Umnachten dich Schatten", "tokens": ["Mei\u00b7ne", "I\u00b7da", ",", "wie", "ist", "dir", "?", "Um\u00b7nach\u00b7ten", "dich", "Schat\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NE", "$,", "PWAV", "VAFIN", "PPER", "$.", "VVFIN", "PPER", "NN"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.14": {"text": "der Schwermuth?", "tokens": ["der", "Schwer\u00b7muth", "?"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "Sch\u00fcttert Fieberwuth deinen zu z\u00e4rtlichen", "tokens": ["Sch\u00fct\u00b7tert", "Fie\u00b7ber\u00b7wuth", "dei\u00b7nen", "zu", "z\u00e4rt\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "PPOSAT", "PTKZU", "VVINF"], "meter": "+-+--+--+--", "measure": "trochaic.tetra.relaxed"}, "line.16": {"text": "Bau?", "tokens": ["Bau", "?"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.17": {"text": "Schwermuth mag nicht haften an meiner unstr\u00e4f-", "tokens": ["Schwer\u00b7muth", "mag", "nicht", "haf\u00b7ten", "an", "mei\u00b7ner", "un\u00b7str\u00e4f"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PTKNEG", "VVFIN", "APPR", "PPOSAT", "TRUNC"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.18": {"text": "lichen Ida", "tokens": ["li\u00b7chen", "I\u00b7da"], "token_info": ["word", "word"], "pos": ["ADJA", "NE"], "meter": "--+-", "measure": "anapaest.init"}, "line.19": {"text": "Himmellauterem Sinn. Krankheit, dein feind-", "tokens": ["Him\u00b7mel\u00b7lau\u00b7te\u00b7rem", "Sinn", ".", "Krank\u00b7heit", ",", "dein", "fein\u00b7d"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$.", "NN", "$,", "PPOSAT", "TRUNC"], "meter": "+-+--++--+-", "measure": "trochaic.penta.relaxed"}, "line.20": {"text": "licher Arm", "tokens": ["li\u00b7cher", "Arm"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.21": {"text": "Sch\u00fcttert schonungslos die Feste des edelsten M\u00e4d-", "tokens": ["Sch\u00fct\u00b7tert", "scho\u00b7nungs\u00b7los", "die", "Fes\u00b7te", "des", "e\u00b7dels\u00b7ten", "M\u00e4d"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ART", "NN", "ART", "ADJA", "TRUNC"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.22": {"text": "chens \u2014", "tokens": ["chens"], "token_info": ["word", "punct"], "pos": ["NE", "$("], "meter": "-", "measure": "single.down"}, "line.23": {"text": "Meine Ida, du sinkst? Lehne dich, Theure,", "tokens": ["Mei\u00b7ne", "I\u00b7da", ",", "du", "sinkst", "?", "Leh\u00b7ne", "dich", ",", "Theu\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NE", "$,", "PPER", "VVFIN", "$.", "NN", "PPER", "$,", "ADJA", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.24": {"text": "an mich!", "tokens": ["an", "mich", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "PPER", "$."], "meter": "-+", "measure": "iambic.single"}, "line.25": {"text": "Also lehnt an den Stab die wettergegeisselte", "tokens": ["Al\u00b7so", "lehnt", "an", "den", "Stab", "die", "wet\u00b7ter\u00b7ge\u00b7geis\u00b7sel\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "--+--+-+--+--", "measure": "anapaest.di.plus"}, "line.26": {"text": "Nelke.", "tokens": ["Nel\u00b7ke", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.27": {"text": "Wilder w\u00fcthet der Sturm, wilder der Re-", "tokens": ["Wil\u00b7der", "w\u00fct\u00b7het", "der", "Sturm", ",", "wil\u00b7der", "der", "Re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "ADJA", "ART", "TRUNC"], "meter": "+-+--+---+", "measure": "trochaic.tetra.relaxed"}, "line.28": {"text": "gen. Der Stab", "tokens": ["gen", ".", "Der", "Stab"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVPP", "$.", "ART", "NN"], "meter": "--+", "measure": "anapaest.init"}, "line.29": {"text": "Sinkt, und mit ihm sinkt die sch\u00f6nste der Blumen.", "tokens": ["Sinkt", ",", "und", "mit", "ihm", "sinkt", "die", "sch\u00f6ns\u00b7te", "der", "Blu\u00b7men", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "APPR", "PPER", "VVFIN", "ART", "ADJA", "ART", "NN", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.30": {"text": "Gebrochen", "tokens": ["Ge\u00b7bro\u00b7chen"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.31": {"text": "Liegt sie am Boden. Der Wind streuet die", "tokens": ["Liegt", "sie", "am", "Bo\u00b7den", ".", "Der", "Wind", "streu\u00b7et", "die"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$.", "ART", "NN", "VVFIN", "ART"], "meter": "-+-+--++-+", "measure": "iambic.penta.relaxed"}, "line.32": {"text": "Bl\u00e4tter umher.", "tokens": ["Bl\u00e4t\u00b7ter", "um\u00b7her", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.33": {"text": "Also liegt und schmachtet mein herrliches M\u00e4dchen.", "tokens": ["Al\u00b7so", "liegt", "und", "schmach\u00b7tet", "mein", "herr\u00b7li\u00b7ches", "M\u00e4d\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.34": {"text": "Der Locken", "tokens": ["Der", "Lo\u00b7cken"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.35": {"text": "Ringelnde F\u00fcll' umstr\u00f6mt h\u00fcllend die stei-", "tokens": ["Rin\u00b7geln\u00b7de", "F\u00fcll'", "um\u00b7str\u00f6mt", "h\u00fcl\u00b7lend", "die", "stei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ADJD", "ART", "TRUNC"], "meter": "+--+-+---+", "measure": "iambic.tetra.invert"}, "line.36": {"text": "gende Brust.", "tokens": ["gen\u00b7de", "Brust", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "--+", "measure": "anapaest.init"}, "line.37": {"text": "Mit dem Steigen des Schwanenbusens steiget die", "tokens": ["Mit", "dem", "Stei\u00b7gen", "des", "Schwa\u00b7nen\u00b7bu\u00b7sens", "stei\u00b7get", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "ART"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.38": {"text": "Decke.", "tokens": ["De\u00b7cke", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.39": {"text": "Wie das T\u00e4ubchen im Hayn, girret ihr kuss-", "tokens": ["Wie", "das", "T\u00e4ub\u00b7chen", "im", "Hayn", ",", "gir\u00b7ret", "ihr", "kuss"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "$,", "VVFIN", "PPOSAT", "TRUNC"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.40": {"text": "licher Mund.", "tokens": ["li\u00b7cher", "Mund", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.41": {"text": "Meine Ida, du leidest, und ferne von deinem Ge-", "tokens": ["Mei\u00b7ne", "I\u00b7da", ",", "du", "lei\u00b7dest", ",", "und", "fer\u00b7ne", "von", "dei\u00b7nem", "Ge"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NE", "$,", "PPER", "VVFIN", "$,", "KON", "ADV", "APPR", "PPOSAT", "TRUNC"], "meter": "+-+--+--+--+--", "measure": "trochaic.penta.relaxed"}, "line.42": {"text": "treuen.", "tokens": ["treu\u00b7en", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.43": {"text": "Dennoch zucket dein Ach mir durch das", "tokens": ["Den\u00b7noch", "zu\u00b7cket", "dein", "Ach", "mir", "durch", "das"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PPER", "APPR", "ART"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.44": {"text": "innerste Mark;", "tokens": ["in\u00b7ners\u00b7te", "Mark", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.45": {"text": "Dennoch f\u00e4hrt mir dein ungesehener Schmerz durch", "tokens": ["Den\u00b7noch", "f\u00e4hrt", "mir", "dein", "un\u00b7ge\u00b7se\u00b7he\u00b7ner", "Schmerz", "durch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "APPR"], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.46": {"text": "das Leben.", "tokens": ["das", "Le\u00b7ben", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.47": {"text": "Mich durchschauern, wie dich, wechselnd", "tokens": ["Mich", "durch\u00b7schau\u00b7ern", ",", "wie", "dich", ",", "wech\u00b7selnd"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "$,", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "die Schw\u00fcl' und der Frost.", "tokens": ["die", "Schw\u00fcl'", "und", "der", "Frost", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.49": {"text": "Unst\u00e4t wandl' ich im Zimmer, und gebe nicht Ant-", "tokens": ["Un\u00b7st\u00e4t", "wandl'", "ich", "im", "Zim\u00b7mer", ",", "und", "ge\u00b7be", "nicht", "Ant"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN", "$,", "KON", "VVFIN", "PTKNEG", "TRUNC"], "meter": "---+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.50": {"text": "wort dem Frager,", "tokens": ["wort", "dem", "Fra\u00b7ger", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.51": {"text": "Fahr' aus Tr\u00e4umen empor, schweig' und", "tokens": ["Fahr'", "aus", "Tr\u00e4u\u00b7men", "em\u00b7por", ",", "schweig'", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "APPR", "NN", "PTKVZ", "$,", "VVFIN", "KON"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.52": {"text": "bedenke nur dich,", "tokens": ["be\u00b7den\u00b7ke", "nur", "dich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.53": {"text": "Durste nach deinem Anblick und zaubre mich dir", "tokens": ["Durs\u00b7te", "nach", "dei\u00b7nem", "An\u00b7blick", "und", "zaub\u00b7re", "mich", "dir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN", "KON", "VVFIN", "PPER", "PPER"], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}, "line.54": {"text": "in die Arme \u2014", "tokens": ["in", "die", "Ar\u00b7me"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.55": {"text": "Meine Ida, ach, wann wird mich dein", "tokens": ["Mei\u00b7ne", "I\u00b7da", ",", "ach", ",", "wann", "wird", "mich", "dein"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NE", "$,", "ITJ", "$,", "PWAV", "VAFIN", "PPER", "PPOSAT"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.56": {"text": "Anblick erfreun?", "tokens": ["An\u00b7blick", "er\u00b7freun", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "---+", "measure": "unknown.measure.single"}, "line.57": {"text": "Wann wird wieder mein Auge dem deinen begegnen?", "tokens": ["Wann", "wird", "wie\u00b7der", "mein", "Au\u00b7ge", "dem", "dei\u00b7nen", "be\u00b7geg\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "PPOSAT", "NN", "ART", "PPOSAT", "VVINF", "$."], "meter": "-+---+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.58": {"text": "Dein L\u00e4cheln", "tokens": ["Dein", "L\u00e4\u00b7cheln"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.59": {"text": "Mich umgl\u00e4nzen, und mich seliges Ahnen", "tokens": ["Mich", "um\u00b7gl\u00e4n\u00b7zen", ",", "und", "mich", "se\u00b7li\u00b7ges", "Ah\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "PPER", "ADJA", "NN"], "meter": "--+--++--+-", "measure": "anapaest.di.plus"}, "line.60": {"text": "umwehn? \u2014", "tokens": ["um\u00b7wehn", "?"], "token_info": ["word", "punct", "punct"], "pos": ["VVINF", "$.", "$("], "meter": "-+", "measure": "iambic.single"}, "line.61": {"text": "Ida, Ida, getrost! Mir lispelt die Liebe: Nicht", "tokens": ["I\u00b7da", ",", "I\u00b7da", ",", "ge\u00b7trost", "!", "Mir", "lis\u00b7pelt", "die", "Lie\u00b7be", ":", "Nicht"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["NE", "$,", "NE", "$,", "VVPP", "$."