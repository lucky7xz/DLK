{"dta.poem.13957": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Abl\u00f6sung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Im Fr\u00fchlingsglanz still auf und nieder", "tokens": ["Im", "Fr\u00fch\u00b7lings\u00b7glanz", "still", "auf", "und", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJD", "PTKVZ", "KON", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ergingen der Frauen sich viel,", "tokens": ["Er\u00b7gin\u00b7gen", "der", "Frau\u00b7en", "sich", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PRF", "ADV", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und liebliche Augen und Lieder", "tokens": ["Und", "lieb\u00b7li\u00b7che", "Au\u00b7gen", "und", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Sie hielten ein herzliches Spiel.", "tokens": ["Sie", "hiel\u00b7ten", "ein", "herz\u00b7li\u00b7ches", "Spiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}