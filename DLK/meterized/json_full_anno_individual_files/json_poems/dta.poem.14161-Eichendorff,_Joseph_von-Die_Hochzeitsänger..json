{"dta.poem.14161": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Hochzeits\u00e4nger.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Und am Hause seine Runde", "tokens": ["Und", "am", "Hau\u00b7se", "sei\u00b7ne", "Run\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tritt der W\u00e4chter g\u00e4hnend an,", "tokens": ["Tritt", "der", "W\u00e4ch\u00b7ter", "g\u00e4h\u00b7nend", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NE", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rufet aus die Schlafensstunde,", "tokens": ["Ru\u00b7fet", "aus", "die", "Schla\u00b7fens\u00b7stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sieht ganz erbost uns an.", "tokens": ["Und", "sieht", "ganz", "er\u00b7bost", "uns", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Doch nach ihrem Kabinette", "tokens": ["Doch", "nach", "ih\u00b7rem", "Ka\u00b7bi\u00b7net\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schwing' ich noch mein Tambourin,", "tokens": ["Schwing'", "ich", "noch", "mein", "Tam\u00b7bo\u00b7u\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Fahr' wohl in Dein Himmelbette,", "tokens": ["Fahr'", "wohl", "in", "Dein", "Him\u00b7mel\u00b7bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Weil wir m\u00fcssen weiter zieh'n!", "tokens": ["Weil", "wir", "m\u00fcs\u00b7sen", "wei\u00b7ter", "zieh'n", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}