word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ich will und mu\u00df bei meinen V\u00e4tern ruhn!", "tokens": ["Ich", "will", "und", "mu\u00df", "bei", "mei\u00b7nen", "V\u00e4\u00b7tern", "ruhn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KON", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Dich sollt ich meiden, trautes Netz der Wege,", "tokens": ["Dich", "sollt", "ich", "mei\u00b7den", ",", "trau\u00b7tes", "Netz", "der", "We\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$,", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Und dich, Gebirg, wo ich des Abgrunds Stege", "tokens": ["Und", "dich", ",", "Ge\u00b7birg", ",", "wo", "ich", "des", "Ab\u00b7grunds", "Ste\u00b7ge"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "NN", "$,", "PWAV", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auch mit verbundnem Aug beschreiten kann?", "tokens": ["Auch", "mit", "ver\u00b7bund\u00b7nem", "Aug", "be\u00b7schrei\u00b7ten", "kann", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wo ich der Quellen tiefen Ursprung kenne", "tokens": ["Wo", "ich", "der", "Quel\u00b7len", "tie\u00b7fen", "Ur\u00b7sprung", "ken\u