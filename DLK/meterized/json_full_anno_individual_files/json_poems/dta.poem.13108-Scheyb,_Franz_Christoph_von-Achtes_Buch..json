{"dta.poem.13108": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Achtes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eso bin ich unbesorgt und dieses Kummers frey:", "tokens": ["\u201e", "so", "bin", "ich", "un\u00b7be\u00b7sorgt", "und", "die\u00b7ses", "Kum\u00b7mers", "frey", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "ADJD", "KON", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201eob ich von ihm gelobt, von ihm verachtet sey.", "tokens": ["\u201e", "ob", "ich", "von", "ihm", "ge\u00b7lobt", ",", "von", "ihm", "ver\u00b7ach\u00b7tet", "sey", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "APPR", "PPER", "VVPP", "$,", "APPR", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201ees ist da nicht der Ort der Einfalt anzuzeigen,", "tokens": ["\u201e", "es", "ist", "da", "nicht", "der", "Ort", "der", "Ein\u00b7falt", "an\u00b7zu\u00b7zei\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "PTKNEG", "ART", "NN", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201ewas hier der Dichter-Kunst, was dort der Wahrheit eigen;", "tokens": ["\u201e", "was", "hier", "der", "Dich\u00b7ter\u00b7Kunst", ",", "was", "dort", "der", "Wahr\u00b7heit", "ei\u00b7gen", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "ADV", "ART", "NN", "$,", "PRELS", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201eviel weniger wie sie sich dem Gedicht vereint;", "tokens": ["\u201e", "viel", "we\u00b7ni\u00b7ger", "wie", "sie", "sich", "dem", "Ge\u00b7dicht", "ver\u00b7eint", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PIS", "KOKOM", "PPER", "PRF", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "430\u201dWarum die Wahrheit oft nur eine Blendung scheint;", "tokens": ["\"", "Wa\u00b7rum", "die", "Wahr\u00b7heit", "oft", "nur", "ei\u00b7ne", "Blen\u00b7dung", "scheint", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "ART", "NN", "ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201ewarum man ein Gedicht oft vor die Wahrheit halte.", "tokens": ["\u201e", "wa\u00b7rum", "man", "ein", "Ge\u00b7dicht", "oft", "vor", "die", "Wahr\u00b7heit", "hal\u00b7te", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PIS", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201ewie ihr Zusammenhang ein Ehren-Mahl gestallte.", "tokens": ["\u201e", "wie", "ihr", "Zu\u00b7sam\u00b7men\u00b7hang", "ein", "Eh\u00b7ren\u00b7Mahl", "ge\u00b7stall\u00b7te", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOKOM", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}