{"textgrid.poem.50827": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Trinklied", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die gr\u00fcnen R\u00f6mer blinken,", "tokens": ["Die", "gr\u00fc\u00b7nen", "R\u00f6\u00b7mer", "blin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wir trinken draus mit Lust:", "tokens": ["Wir", "trin\u00b7ken", "draus", "mit", "Lust", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das ist ein fr\u00f6hlich Leben,", "tokens": ["Das", "ist", "ein", "fr\u00f6h\u00b7lich", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das hebt die junge Brust!", "tokens": ["Das", "hebt", "die", "jun\u00b7ge", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Sonne spielet lieblich", "tokens": ["Die", "Son\u00b7ne", "spie\u00b7let", "lieb\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch all den klaren Wein,", "tokens": ["Durch", "all", "den", "kla\u00b7ren", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie spiegelt hin und wider", "tokens": ["Sie", "spie\u00b7gelt", "hin", "und", "wi\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In unsern \u00c4ugelein!", "tokens": ["In", "un\u00b7sern", "\u00c4u\u00b7ge\u00b7lein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Was liegt denn an der Schwelle", "tokens": ["Was", "liegt", "denn", "an", "der", "Schwel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dort f\u00fcr ein bleiches Weib?", "tokens": ["Dort", "f\u00fcr", "ein", "blei\u00b7ches", "Weib", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Geschlagen und gebunden", "tokens": ["Ge\u00b7schla\u00b7gen", "und", "ge\u00b7bun\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den wundersch\u00f6nen Leib?", "tokens": ["Den", "wun\u00b7der\u00b7sch\u00f6\u00b7nen", "Leib", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wie kommt so kranke Dirne", "tokens": ["Wie", "kommt", "so", "kran\u00b7ke", "Dir\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Denn unserm Jubel nah? \u2013", "tokens": ["Denn", "un\u00b7serm", "Ju\u00b7bel", "nah", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Oh, schleudert weg die Becher!", "tokens": ["Oh", ",", "schleu\u00b7dert", "weg", "die", "Be\u00b7cher", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das ist Germania!", "tokens": ["Das", "ist", "Ger\u00b7ma\u00b7nia", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Wir nehmen still die H\u00fcte", "tokens": ["Wir", "neh\u00b7men", "still", "die", "H\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und schleichen aus dem Schank,", "tokens": ["Und", "schlei\u00b7chen", "aus", "dem", "Schank", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie einer, der ein R\u00e4uschchen", "tokens": ["Wie", "ei\u00b7ner", ",", "der", "ein", "R\u00e4uschchen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PIS", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sich am Karfreitag trank.", "tokens": ["Sich", "am", "Kar\u00b7frei\u00b7tag", "trank", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die gr\u00fcnen R\u00f6mer blinken,", "tokens": ["Die", "gr\u00fc\u00b7nen", "R\u00f6\u00b7mer", "blin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART"