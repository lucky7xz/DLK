{"dta.poem.196": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Der Traum.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Was sehe ich? einpfindliche Climene!", "tokens": ["Was", "se\u00b7he", "ich", "?", "ein\u00b7pfind\u00b7li\u00b7che", "Cli\u00b7me\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist euer Hertz durch ", "tokens": ["Ist", "eu\u00b7er", "Hertz", "durch"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "APPR"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Ists m\u00fcglich/ da\u00df ihrs seyd/ gestrenge Sch\u00f6ne?", "tokens": ["Ists", "m\u00fcg\u00b7lich", "/", "da\u00df", "ihrs", "seyd", "/", "ge\u00b7stren\u00b7ge", "Sch\u00f6\u00b7ne", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJD", "$(", "KOUS", "PPER", "VAFIN", "$(", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ists nicht ein Traum der mich in Jrrthum f\u00fchrt?", "tokens": ["Ists", "nicht", "ein", "Traum", "der", "mich", "in", "Jrr\u00b7thum", "f\u00fchrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ART", "NN", "ART", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie! sehe ich allhier die stoltze Sch\u00f6ne?", "tokens": ["Wie", "!", "se\u00b7he", "ich", "all\u00b7hier", "die", "stolt\u00b7ze", "Sch\u00f6\u00b7ne", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die meinen Wunsch durch ihren Ha\u00df verletzt;", "tokens": ["Die", "mei\u00b7nen", "Wunsch", "durch", "ih\u00b7ren", "Ha\u00df", "ver\u00b7letzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die streng\u2019 und unbarmhertzige ", "tokens": ["Die", "streng'", "und", "un\u00b7barm\u00b7hert\u00b7zi\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "KON", "ADJA"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Jhr weint; macht da\u00df eur Seuffzen mich erg\u00f6tzt.", "tokens": ["Ihr", "weint", ";", "macht", "da\u00df", "eur", "Seuff\u00b7zen", "mich", "er\u00b7g\u00f6tzt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "VVFIN", "KOUS", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Ein stoltzer Schaam hat schon vor langen Zeiten/", "tokens": ["Ein", "stolt\u00b7zer", "Schaam", "hat", "schon", "vor", "lan\u00b7gen", "Zei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Den stillen Wunsch in eurer Brust bekriegt/", "tokens": ["Den", "stil\u00b7len", "Wunsch", "in", "eu\u00b7rer", "Brust", "be\u00b7kriegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Ach! da\u00df anjetzt das ", "tokens": ["Ach", "!", "da\u00df", "an\u00b7jetzt", "das"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "KOUS", "ADV", "ART"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.12": {"text": "Was nach Verdru\u00df! so Hertz als Geist vergn\u00fcgt.", "tokens": ["Was", "nach", "Ver\u00b7dru\u00df", "!", "so", "Hertz", "als", "Geist", "ver\u00b7gn\u00fcgt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "$.", "ADV", "NN", "KOUS", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Die Z\u00e4rtlichkeit/ die unser Hertzen brennet/", "tokens": ["Die", "Z\u00e4rt\u00b7lich\u00b7keit", "/", "die", "un\u00b7ser", "Hert\u00b7zen", "bren\u00b7net", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Berauschet uns ", "tokens": ["Be\u00b7rau\u00b7schet", "uns"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Mein Hertz ist schon - - - wer ist es/ der mich nennet?", "tokens": ["Mein", "Hertz", "ist", "schon", "wer", "ist", "es", "/", "der", "mich", "nen\u00b7net", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "$(", "$(", "$(", "PWS", "VAFIN", "PPER", "$(", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.16": {"text": "Grausahmes Hertz! warum erweckt ihr mich?", "tokens": ["Grau\u00b7sah\u00b7mes", "Hertz", "!", "wa\u00b7rum", "er\u00b7weckt", "ihr", "mich", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PWAV", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}