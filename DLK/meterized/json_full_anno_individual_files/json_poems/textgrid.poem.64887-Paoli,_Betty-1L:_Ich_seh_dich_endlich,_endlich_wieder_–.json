{"textgrid.poem.64887": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich seh dich endlich, endlich wieder \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich seh dich endlich, endlich wieder \u2013", "tokens": ["Ich", "seh", "dich", "end\u00b7lich", ",", "end\u00b7lich", "wie\u00b7der", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "ADV", "ADV", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dein tr\u00e4um'risch s\u00fc\u00dfes Angesicht!", "tokens": ["Dein", "tr\u00e4um'\u00b7risch", "s\u00fc\u00b7\u00dfes", "An\u00b7ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich h\u00f6re deiner Stimme Lieder \u2013", "tokens": ["Ich", "h\u00f6\u00b7re", "dei\u00b7ner", "Stim\u00b7me", "Lie\u00b7der", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "O das Entz\u00fccken t\u00f6dtet nicht!", "tokens": ["O", "das", "Ent\u00b7z\u00fc\u00b7cken", "t\u00f6d\u00b7tet", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVFIN", "PTKNEG", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Ich lieg' in deinen treuen Armen,", "tokens": ["Ich", "lieg'", "in", "dei\u00b7nen", "treu\u00b7en", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du pressest mich an deine Brust;", "tokens": ["Du", "pres\u00b7sest", "mich", "an", "dei\u00b7ne", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In sel'ger Freude Gluterwarmen", "tokens": ["In", "sel'\u00b7ger", "Freu\u00b7de", "Glu\u00b7ter\u00b7war\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sag' ich dir, was dir l\u00e4ngst bewu\u00dft!", "tokens": ["Sag'", "ich", "dir", ",", "was", "dir", "l\u00e4ngst", "be\u00b7wu\u00dft", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "$,", "PWS", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und goldne Tage seh ich schweben", "tokens": ["Und", "gold\u00b7ne", "Ta\u00b7ge", "seh", "ich", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aus ferner Zukunft dunklem Meer,", "tokens": ["Aus", "fer\u00b7ner", "Zu\u00b7kunft", "dunk\u00b7lem", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vereinigt unser innerst Leben \u2013", "tokens": ["Ver\u00b7ei\u00b7nigt", "un\u00b7ser", "in\u00b7nerst", "Le\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJD", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "O da\u00df es also, also w\u00e4r'!", "tokens": ["O", "da\u00df", "es", "al\u00b7so", ",", "al\u00b7so", "w\u00e4r'", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "ADV", "$,", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie viel und hei\u00df ich auch gelitten", "tokens": ["Wie", "viel", "und", "hei\u00df", "ich", "auch", "ge\u00b7lit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "KON", "ADJD", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch eine lange, lange Zeit,", "tokens": ["Durch", "ei\u00b7ne", "lan\u00b7ge", ",", "lan\u00b7ge", "Zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Schmerz hat Herrliches erstritten:", "tokens": ["Mein", "Schmerz", "hat", "Herr\u00b7li\u00b7ches", "er\u00b7strit\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Wiederfindens Seligkeit.", "tokens": ["Des", "Wie\u00b7der\u00b7fin\u00b7dens", "Se\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich seh dich endlich, endlich wieder \u2013", "tokens": ["Ich", "seh", "dich", "end\u00b7lich", ",", "end\u00b7lich", "wie\u00b7der", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "ADV", "ADV", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dein tr\u00e4um'risch s\u00fc\u00dfes Angesicht!", "tokens": ["Dein", "tr\u00e4um'\u00b7risch", "s\u00