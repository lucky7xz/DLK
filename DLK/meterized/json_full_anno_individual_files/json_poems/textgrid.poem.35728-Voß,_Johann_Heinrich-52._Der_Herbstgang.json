{"textgrid.poem.35728": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "52. Der Herbstgang", "genre": "verse", "period": "N.A.", "pub_year": 1794, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die B\u00e4ume stehn der Frucht entladen,", "tokens": ["Die", "B\u00e4u\u00b7me", "stehn", "der", "Frucht", "ent\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und gelbes Laub verweht ins Thal;", "tokens": ["Und", "gel\u00b7bes", "Laub", "ver\u00b7weht", "ins", "Thal", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Stoppelfeld in Schimmerfaden", "tokens": ["Das", "Stop\u00b7pel\u00b7feld", "in", "Schim\u00b7mer\u00b7fa\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ergl\u00e4nzt am niedern Mittagsstrahl.", "tokens": ["Er\u00b7gl\u00e4nzt", "am", "nie\u00b7dern", "Mit\u00b7tags\u00b7strahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es kreist der V\u00f6gel Schwarm und ziehet;", "tokens": ["Es", "kreist", "der", "V\u00f6\u00b7gel", "Schwarm", "und", "zie\u00b7het", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das Vieh verlangt zum Stall und fliehet", "tokens": ["Das", "Vieh", "ver\u00b7langt", "zum", "Stall", "und", "flie\u00b7het"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die magern Au'n, vom Reife fahl.", "tokens": ["Die", "ma\u00b7gern", "Au'n", ",", "vom", "Rei\u00b7fe", "fahl", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O, geh' am sanften Scheidetage", "tokens": ["O", ",", "geh'", "am", "sanf\u00b7ten", "Schei\u00b7de\u00b7ta\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Jahrs zu guter Letzt hinaus;", "tokens": ["Des", "Jahrs", "zu", "gu\u00b7ter", "Letzt", "hin\u00b7aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und nenn' ihn Sommertag und trage", "tokens": ["Und", "nenn'", "ihn", "Som\u00b7mer\u00b7tag", "und", "tra\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den letzten, schwer gefundnen Strau\u00df.", "tokens": ["Den", "letz\u00b7ten", ",", "schwer", "ge\u00b7fund\u00b7nen", "Strau\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJD", "ADJA", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bald steigt Gew\u00f6lk, und schwarz dahinter", "tokens": ["Bald", "steigt", "Ge\u00b7w\u00f6lk", ",", "und", "schwarz", "da\u00b7hin\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "$,", "KON", "ADJD", "PAV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Sturm und sein Geno\u00df, der Winter,", "tokens": ["Der", "Sturm", "und", "sein", "Ge\u00b7no\u00df", ",", "der", "Win\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPOSAT", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und h\u00fcllt in Flocken Feld und Haus.", "tokens": ["Und", "h\u00fcllt", "in", "Flo\u00b7cken", "Feld", "und", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein weiser Mann, ihr Lieben, haschet", "tokens": ["Ein", "wei\u00b7ser", "Mann", ",", "ihr", "Lie\u00b7ben", ",", "ha\u00b7schet"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Freuden im Vor\u00fcberfliehn,", "tokens": ["Die", "Freu\u00b7den", "im", "Vor\u00b7\u00fc\u00b7berf\u00b7liehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Empf\u00e4ngt, was kommt, un\u00fcberraschet,", "tokens": ["Emp\u00b7f\u00e4ngt", ",", "was", "kommt", ",", "un\u00b7\u00fc\u00b7berr\u00b7a\u00b7schet", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "VVFIN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und pfl\u00fcckt die Blumen, weil sie bl\u00fchn.", "tokens": ["Und", "pfl\u00fcckt", "die", "Blu\u00b7men", ",", "weil", "sie", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und sind die Blumen auch verschwunden;", "tokens": ["Und", "sind", "die", "Blu\u00b7men", "auch", "ver\u00b7schwun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So steht am Winterherd' umwunden", "tokens": ["So", "steht", "am", "Win\u00b7ter\u00b7herd'", "um\u00b7wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sein Festpokal mit Immergr\u00fcn.", "tokens": ["Sein", "Fest\u00b7po\u00b7kal", "mit", "Im\u00b7mer\u00b7gr\u00fcn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Noch trocken f\u00fchrt durch Thal und H\u00fcgel", "tokens": ["Noch", "tro\u00b7cken", "f\u00fchrt", "durch", "Thal", "und", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der l\u00e4ngst vertraute Sommerpfad.", "tokens": ["Der", "l\u00e4ngst", "ver\u00b7trau\u00b7te", "Som\u00b7mer\u00b7pfad", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur r\u00f6tlich h\u00e4ngt am Wasserspiegel", "tokens": ["Nur", "r\u00f6t\u00b7lich", "h\u00e4ngt", "am", "Was\u00b7ser\u00b7spie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Baum, den gr\u00fcn ihr neulich saht.", "tokens": ["Der", "Baum", ",", "den", "gr\u00fcn", "ihr", "neu\u00b7lich", "saht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch gr\u00fcnt der Kamp von Winterkorne;", "tokens": ["Doch", "gr\u00fcnt", "der", "Kamp", "von", "Win\u00b7ter\u00b7kor\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Doch gr\u00fcnt, beim Rot der Hagedorne", "tokens": ["Doch", "gr\u00fcnt", ",", "beim", "Rot", "der", "Ha\u00b7ge\u00b7dor\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und Spillbeer'n, unsre Lagerstatt!", "tokens": ["Und", "Spill\u00b7beer'n", ",", "uns\u00b7re", "La\u00b7ger\u00b7statt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "So still an warmer Sonne liegend,", "tokens": ["So", "still", "an", "war\u00b7mer", "Son\u00b7ne", "lie\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sehn wir das bunte Feld hinan,", "tokens": ["Sehn", "wir", "das", "bun\u00b7te", "Feld", "hi\u00b7nan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und dort, auf schwarzer Brache pfl\u00fcgend,", "tokens": ["Und", "dort", ",", "auf", "schwar\u00b7zer", "Bra\u00b7che", "pfl\u00fc\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Lustgepfeif', den Ackermann;", "tokens": ["Mit", "Lust\u00b7ge\u00b7pfeif'", ",", "den", "A\u00b7cker\u00b7mann", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Kr\u00e4hn in frischer Furche schw\u00e4rmen", "tokens": ["Die", "Kr\u00e4hn", "in", "fri\u00b7scher", "Fur\u00b7che", "schw\u00e4r\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dem Pfluge nach und schrein und l\u00e4rmen,", "tokens": ["Dem", "Pflu\u00b7ge", "nach", "und", "schrein", "und", "l\u00e4r\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "KON", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und dampfend zieht das Gaulgespann.", "tokens": ["Und", "damp\u00b7fend", "zieht", "das", "Gaul\u00b7ge\u00b7spann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Natur, wie sch\u00f6n in jedem Kleide!", "tokens": ["Na\u00b7tur", ",", "wie", "sch\u00f6n", "in", "je\u00b7dem", "Klei\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ADJD", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auch noch im Sterbekleid wie sch\u00f6n!", "tokens": ["Auch", "noch", "im", "Ster\u00b7be\u00b7kleid", "wie", "sch\u00f6n", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "KOKOM", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie mischt in Wehmut sanfte Freude", "tokens": ["Sie", "mischt", "in", "Weh\u00b7mut", "sanf\u00b7te", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und l\u00e4chelt thr\u00e4nend noch im Gehn.", "tokens": ["Und", "l\u00e4\u00b7chelt", "thr\u00e4\u00b7nend", "noch", "im", "Gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du, welkes Laub, das niederschauert,", "tokens": ["Du", ",", "wel\u00b7kes", "Laub", ",", "das", "nie\u00b7der\u00b7schau\u00b7ert", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PWAT", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Du, Bl\u00fcmchen, lispelst: Nicht getrauert!", "tokens": ["Du", ",", "Bl\u00fcm\u00b7chen", ",", "lis\u00b7pelst", ":", "Nicht", "ge\u00b7trau\u00b7ert", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VVFIN", "$.", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wir werden sch\u00f6ner auferstehn!", "tokens": ["Wir", "wer\u00b7den", "sch\u00f6\u00b7ner", "auf\u00b7er\u00b7stehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Die B\u00e4ume stehn der Frucht entladen,", "tokens": ["Die", "B\u00e4u\u00b7me", "stehn", "der", "Frucht", "ent\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und gelbes Laub verweht ins Thal;", "tokens": ["Und", "gel\u00b7bes", "Laub", "ver\u00b7weht", "ins", "Thal", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Stoppelfeld in Schimmerfaden", "tokens": ["Das", "Stop\u00b7pel\u00b7feld", "in", "Schim\u00b7mer\u00b7fa\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ergl\u00e4nzt am niedern Mittagsstrahl.", "tokens": ["Er\u00b7gl\u00e4nzt", "am", "nie\u00b7dern", "Mit\u00b7tags\u00b7strahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es kreist der V\u00f6gel Schwarm und ziehet;", "tokens": ["Es", "kreist", "der", "V\u00f6\u00b7gel", "Schwarm", "und", "zie\u00b7het", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das Vieh verlangt zum Stall