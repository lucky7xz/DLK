{"textgrid.poem.66883": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: O tiefer Traum der reinsten Utopieen,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O tiefer Traum der reinsten Utopieen,", "tokens": ["O", "tie\u00b7fer", "Traum", "der", "reins\u00b7ten", "U\u00b7to\u00b7pi\u00b7e\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dein Gold im Alltagsgrau verblasse nie!", "tokens": ["Dein", "Gold", "im", "All\u00b7tags\u00b7grau", "ver\u00b7blas\u00b7se", "nie", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du bist das Bild, nach dem die Besten ziehen,", "tokens": ["Du", "bist", "das", "Bild", ",", "nach", "dem", "die", "Bes\u00b7ten", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "APPR", "ART", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Glanz bist du der Geisterenergie.", "tokens": ["Der", "Glanz", "bist", "du", "der", "Geis\u00b7te\u00b7re\u00b7ner\u00b7gie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du brauchst vor keiner Wirklichkeit zu fliehen,", "tokens": ["Du", "brauchst", "vor", "kei\u00b7ner", "Wirk\u00b7lich\u00b7keit", "zu", "flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn du vollendest und durchschimmerst sie,", "tokens": ["Denn", "du", "voll\u00b7en\u00b7dest", "und", "durch\u00b7schim\u00b7merst", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es k\u00e4mpft der Held f\u00fcr deine Harmonieen,", "tokens": ["Es", "k\u00e4mpft", "der", "Held", "f\u00fcr", "dei\u00b7ne", "Har\u00b7mo\u00b7ni\u00b7e\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+----", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Ob eine Flut von Mi\u00dfklang ihn umschrie.", "tokens": ["Ob", "ei\u00b7ne", "Flut", "von", "Mi\u00df\u00b7klang", "ihn", "um\u00b7schrie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wem nicht Utopia aus lichten Sph\u00e4ren", "tokens": ["Wem", "nicht", "U\u00b7to\u00b7pia", "aus", "lich\u00b7ten", "Sph\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PTKNEG", "NE", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Durch dieses Leben gl\u00e4nzt als seliger Gral,", "tokens": ["Durch", "die\u00b7ses", "Le\u00b7ben", "gl\u00e4nzt", "als", "se\u00b7li\u00b7ger", "Gral", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+",