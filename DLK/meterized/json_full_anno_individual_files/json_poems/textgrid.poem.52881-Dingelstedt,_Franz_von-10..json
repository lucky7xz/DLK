{"textgrid.poem.52881": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So oft ich dieses G\u00e4\u00dflein gehe,", "tokens": ["So", "oft", "ich", "die\u00b7ses", "G\u00e4\u00df\u00b7lein", "ge\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl sp\u00e4ter noch als Mitternacht,", "tokens": ["Wohl", "sp\u00e4\u00b7ter", "noch", "als", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "KOUS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "H\u00e4lt dort in respektabler H\u00f6he", "tokens": ["H\u00e4lt", "dort", "in", "res\u00b7pek\u00b7tab\u00b7ler", "H\u00f6\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ein eifers\u00fcchtig' L\u00e4mpchen Wacht.", "tokens": ["Ein", "ei\u00b7fer\u00b7s\u00fcch\u00b7tig'", "L\u00e4mp\u00b7chen", "Wacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da droben wohnt ein Versedrechsler,", "tokens": ["Da", "dro\u00b7ben", "wohnt", "ein", "Ver\u00b7sed\u00b7rechs\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ein Reimeschmied, ein B\u00fccherwurm,", "tokens": ["Ein", "Rei\u00b7me\u00b7schmied", ",", "ein", "B\u00fc\u00b7cher\u00b7wurm", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hoch sitzt er, der Gedankenwechsler,", "tokens": ["Hoch", "sitzt", "er", ",", "der", "Ge\u00b7dan\u00b7ken\u00b7wechs\u00b7ler", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Klas auf dem Kathrinen-Turm.", "tokens": ["Wie", "Klas", "auf", "dem", "Ka\u00b7thri\u00b7nen\u00b7Turm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und z\u00e4hlt die F\u00fc\u00dfe, feilscht um Silben,", "tokens": ["Und", "z\u00e4hlt", "die", "F\u00fc\u00b7\u00dfe", ",", "feilscht", "um", "Sil\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und putzt die alte Ware rein,", "tokens": ["Und", "putzt", "die", "al\u00b7te", "Wa\u00b7re", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und fri\u00dft wie z\u00e4he K\u00e4semilben", "tokens": ["Und", "fri\u00dft", "wie", "z\u00e4\u00b7he", "K\u00e4\u00b7se\u00b7mil\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sich in papiernen Quark hinein.", "tokens": ["Sich", "in", "pa\u00b7pier\u00b7nen", "Quark", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Verdammter Kerl! Wenn ich nur w\u00fc\u00dfte,", "tokens": ["Ver\u00b7damm\u00b7ter", "Kerl", "!", "Wenn", "ich", "nur", "w\u00fc\u00df\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wer ihn zur Wacht berufen hat,", "tokens": ["Wer", "ihn", "zur", "Wacht", "be\u00b7ru\u00b7fen", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ob er mit mir h\u00fcten m\u00fc\u00dfte,", "tokens": ["Und", "ob", "er", "mit", "mir", "h\u00fc\u00b7ten", "m\u00fc\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als angestellter Mann, die Stadt?", "tokens": ["Als", "an\u00b7ge\u00b7stell\u00b7ter", "Mann", ",", "die", "Stadt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Es tut's ihm niemand kommandieren,", "tokens": ["Es", "tut's", "ihm", "nie\u00b7mand", "kom\u00b7man\u00b7die\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er treibt's f\u00fcr seinen eignen Spa\u00df,", "tokens": ["Er", "treibt's", "f\u00fcr", "sei\u00b7nen", "eig\u00b7nen", "Spa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Predigen und Schrein und Schmieren,", "tokens": ["Das", "Pre\u00b7di\u00b7gen", "und", "Schrein", "und", "Schmie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und niemand wei\u00df so recht f\u00fcr was?", "tokens": ["Und", "nie\u00b7mand", "wei\u00df", "so", "recht", "f\u00fcr", "was", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADV", "ADJD", "APPR", "PWS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die drunten k\u00f6nnen ihn nicht riechen,", "tokens": ["Die", "drun\u00b7ten", "k\u00f6n\u00b7nen", "ihn", "nicht", "rie\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie fliehn ihn alle wie die Pest,", "tokens": ["Sie", "fliehn", "ihn", "al\u00b7le", "wie", "die", "Pest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Am Tag seh' ich umher ihn kriechen", "tokens": ["Am", "Tag", "seh'", "ich", "um\u00b7her", "ihn", "krie\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PTKVZ", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Scheu, wie ein Sp\u00e4tzchen, fern vom Nest.", "tokens": ["Scheu", ",", "wie", "ein", "Sp\u00e4tz\u00b7chen", ",", "fern", "vom", "Nest", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "$,", "ADJD", "APPRART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.7": {"line.1": {"text": "Sie schelten ihn Poet und Barde,", "tokens": ["Sie", "schel\u00b7ten", "ihn", "Po\u00b7et", "und", "Bar\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie sch\u00fctteln stark und zischeln sacht,", "tokens": ["Sie", "sch\u00fct\u00b7teln", "stark", "und", "zi\u00b7scheln", "sacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch er auf seiner Leibmansarde", "tokens": ["Doch", "er", "auf", "sei\u00b7ner", "Leib\u00b7man\u00b7sar\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hat, scheint es, dessen wenig acht.", "tokens": ["Hat", ",", "scheint", "es", ",", "des\u00b7sen", "we\u00b7nig", "acht", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "VVFIN", "PPER", "$,", "PDS", "ADV", "CARD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Mag wohl in seinem Oberst\u00fcbchen", "tokens": ["Mag", "wohl", "in", "sei\u00b7nem", "O\u00b7bers\u00b7t\u00fcb\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht allzurichtig mit ihm sein,", "tokens": ["Nicht", "all\u00b7zu\u00b7rich\u00b7tig", "mit", "ihm", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "APPR", "PPER", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie sperren mir das arme B\u00fcbchen", "tokens": ["Sie", "sper\u00b7ren", "mir", "das", "ar\u00b7me", "B\u00fcb\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Am End' noch ein auf Sonnenstein.", "tokens": ["Am", "End'", "noch", "ein", "auf", "Son\u00b7nen\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ART", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "W\u00e4r' schad' um seine Gab' zu wachen,", "tokens": ["W\u00e4r'", "schad'", "um", "sei\u00b7ne", "Gab'", "zu", "wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und kennt' ich ihn, den tollen Christ,", "tokens": ["Und", "kennt'", "ich", "ihn", ",", "den", "tol\u00b7len", "Christ", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wollt' ich ihn zum Nachtw\u00e4chter machen,", "tokens": ["Wollt'", "ich", "ihn", "zum", "Nacht\u00b7w\u00e4ch\u00b7ter", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter"