{"dta.poem.5352": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die sanfte Ruhe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn man in einem weichen Bette, gesund, mit m\u00fcden", "tokens": ["Wenn", "man", "in", "ei\u00b7nem", "wei\u00b7chen", "Bet\u00b7te", ",", "ge\u00b7sund", ",", "mit", "m\u00fc\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "ART", "ADJA", "NN", "$,", "ADJD", "$,", "APPR", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Gliedern, liegt,", "tokens": ["Glie\u00b7dern", ",", "liegt", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Und die Beqvemlichkeit und Anmuth in diesem Zustand", "tokens": ["Und", "die", "Be\u00b7qvem\u00b7lich\u00b7keit", "und", "An\u00b7muth", "in", "die\u00b7sem", "Zu\u00b7stand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "KON", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "\u00fcberleget:", "tokens": ["\u00fc\u00b7ber\u00b7le\u00b7get", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Empfindet nicht allein der C\u00f6rper ein Etwas, das ihn", "tokens": ["Emp\u00b7fin\u00b7det", "nicht", "al\u00b7lein", "der", "C\u00f6r\u00b7per", "ein", "Et\u00b7was", ",", "das", "ihn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ADV", "ART", "NN", "ART", "ADV", "$,", "PRELS", "PPER"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "sehr vergn\u00fcgt,", "tokens": ["sehr", "ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Wenn unsre Seele, nicht zerstreuet, das, was sie r\u00fchret,", "tokens": ["Wenn", "uns\u00b7re", "See\u00b7le", ",", "nicht", "zer\u00b7streu\u00b7et", ",", "das", ",", "was", "sie", "r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "PTKNEG", "VVFIN", "$,", "PDS", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "nur erweget.", "tokens": ["nur", "er\u00b7we\u00b7get", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Man findet nicht allein mit Lust, wenn sich die Geister-", "tokens": ["Man", "fin\u00b7det", "nicht", "al\u00b7lein", "mit", "Lust", ",", "wenn", "sich", "die", "Geis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PTKNEG", "ADV", "APPR", "NN", "$,", "KOUS", "PRF", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "reichen Sehnen", "tokens": ["rei\u00b7chen", "Seh\u00b7nen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Aus ihrer sonst gewohnten Lage gem\u00e4chlich aus einander", "tokens": ["Aus", "ih\u00b7rer", "sonst", "ge\u00b7wohn\u00b7ten", "La\u00b7ge", "ge\u00b7m\u00e4ch\u00b7lich", "aus", "ein\u00b7an\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADV", "ADJA", "NN", "ADJD", "APPR", "PRF"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.12": {"text": "dehnen,", "tokens": ["deh\u00b7nen", ","], "token_info": ["word", "punct"], "pos": ["PDS", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Ein s\u00fc\u00df und z\u00e4rtliches Gef\u00fchl\u2019; man wird, wenn man mit", "tokens": ["Ein", "s\u00fc\u00df", "und", "z\u00e4rt\u00b7li\u00b7ches", "Ge\u00b7f\u00fchl'", ";", "man", "wird", ",", "wenn", "man", "mit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJD", "KON", "ADJA", "NN", "$.", "PIS", "VAFIN", "$,", "KOUS", "PIS", "APPR"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Achtsamkeit", "tokens": ["Acht\u00b7sam\u00b7keit"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.15": {"text": "Jm Dunckeln, bey geschlossnen Augen, so dann sein We-", "tokens": ["Jm", "Dun\u00b7ckeln", ",", "bey", "ge\u00b7schloss\u00b7nen", "Au\u00b7gen", ",", "so", "dann", "sein", "We"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "APPR", "ADJA", "NN", "$,", "ADV", "ADV", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "sen \u00fcberdencket,", "tokens": ["sen", "\u00fc\u00b7ber\u00b7den\u00b7cket", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.17": {"text": "Kaum da\u00df man worauf liegt gewahr.", "tokens": ["Kaum", "da\u00df", "man", "wo\u00b7rauf", "liegt", "ge\u00b7wahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PIS", "PWAV", "VVFIN", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.18": {"text": "Es kommt uns vor, zu solcher Zeit,", "tokens": ["Es", "kommt", "uns", "vor", ",", "zu", "sol\u00b7cher", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Als ob wir unterwerts so wenig, als wie wir oberwerts,", "tokens": ["Als", "ob", "wir", "un\u00b7ter\u00b7werts", "so", "we\u00b7nig", ",", "als", "wie", "wir", "o\u00b7ber\u00b7werts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "ADV", "PIS", "$,", "KOUS", "KOKOM", "PPER", "ADV", "$,"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.20": {"text": "umschr\u00e4ncket,", "tokens": ["um\u00b7schr\u00e4n\u00b7cket", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.21": {"text": "Vielmehr von allen Seiten frey, und, einer Insel \u00e4hnlich", "tokens": ["Viel\u00b7mehr", "von", "al\u00b7len", "Sei\u00b7ten", "frey", ",", "und", ",", "ei\u00b7ner", "In\u00b7sel", "\u00e4hn\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "PIAT", "NN", "PTKVZ", "$,", "KON", "$,", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.22": {"text": "w\u00e4ren.", "tokens": ["w\u00e4\u00b7ren", "."], "token_info": ["word", "punct"], "pos": ["VAFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.23": {"text": "Es scheint man finde mehr, als sonst, so dann im Schosse", "tokens": ["Es", "scheint", "man", "fin\u00b7de", "mehr", ",", "als", "sonst", ",", "so", "dann", "im", "Schos\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "VVFIN", "ADV", "$,", "KOUS", "ADV", "$,", "ADV", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "der Natur", "tokens": ["der", "Na\u00b7tur"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "--+", "measure": "anapaest.init"}, "line.25": {"text": "Sich gantz besonders eingeschlossen, gantz einsam; es ist", "tokens": ["Sich", "gantz", "be\u00b7son\u00b7ders", "ein\u00b7ge\u00b7schlos\u00b7sen", ",", "gantz", "ein\u00b7sam", ";", "es", "ist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PRF", "ADV", "ADV", "VVPP", "$,", "ADV", "ADJD", "$.", "PPER", "VAFIN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.26": {"text": "keine Spur", "tokens": ["kei\u00b7ne", "Spur"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.27": {"text": "In dieser Dunckelheit zu finden, kein End\u2019 in diesem Raum", "tokens": ["In", "die\u00b7ser", "Dun\u00b7ckel\u00b7heit", "zu", "fin\u00b7den", ",", "kein", "End'", "in", "die\u00b7sem", "Raum"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "PTKZU", "VVINF", "$,", "PIAT", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.28": {"text": "zu schauen,", "tokens": ["zu", "schau\u00b7en", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.29": {"text": "Und keinem Geiste sind hier Gr\u00e4ntzen, noch Innhalt dieses", "tokens": ["Und", "kei\u00b7nem", "Geis\u00b7te", "sind", "hier", "Gr\u00e4nt\u00b7zen", ",", "noch", "Inn\u00b7halt", "die\u00b7ses"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ADV", "NN", "$,", "ADV", "NN", "PDAT"], "meter": "-+-+-+-+--+-++", "measure": "iambic.septa.relaxed"}, "line.30": {"text": "Raums bewust.", "tokens": ["Raums", "be\u00b7wust", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}