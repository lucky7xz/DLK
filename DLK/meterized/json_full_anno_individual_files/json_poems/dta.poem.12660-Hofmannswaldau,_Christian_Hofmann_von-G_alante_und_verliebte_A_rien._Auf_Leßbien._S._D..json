{"dta.poem.12660": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "G alante und verliebte  A rien.  \n Auf Le\u00dfbien.  \n S. D.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Le\u00dfbia, mein leben,", "tokens": ["Le\u00df\u00b7bia", ",", "mein", "le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Hat sich mir ergeben,", "tokens": ["Hat", "sich", "mir", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "PPER", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "In gew\u00fcnschter pflicht,", "tokens": ["In", "ge\u00b7w\u00fcnschter", "pflicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Ich will bey ihr stehen,", "tokens": ["Ich", "will", "bey", "ihr", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Bi\u00df ich werde gehen", "tokens": ["Bi\u00df", "ich", "wer\u00b7de", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VAFIN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Hie aus diesem licht,", "tokens": ["Hie", "aus", "die\u00b7sem", "licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Was vor leid", "tokens": ["Was", "vor", "leid"], "token_info": ["word", "word", "word"], "pos": ["PWS", "APPR", "ADJD"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Ich iederzeit", "tokens": ["Ich", "ie\u00b7der\u00b7zeit"], "token_info": ["word", "word"], "pos": ["PPER", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Um sie hab ertragen m\u00fcssen,", "tokens": ["Um", "sie", "hab", "er\u00b7tra\u00b7gen", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VAFIN", "VVINF", "VMINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Will ich itzt beschliessen.", "tokens": ["Will", "ich", "itzt", "be\u00b7schlies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Die gew\u00fcnschten freuden,", "tokens": ["Die", "ge\u00b7w\u00fcnschten", "freu\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "So sie vor mein leiben", "tokens": ["So", "sie", "vor", "mein", "lei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "APPR", "PPOSAT", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Mir ertheilen will,", "tokens": ["Mir", "er\u00b7thei\u00b7len", "will", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VMFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Soll kein leid beschweren,", "tokens": ["Soll", "kein", "leid", "be\u00b7schwe\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "ADJD", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Ja sie sollen wehren", "tokens": ["Ja", "sie", "sol\u00b7len", "weh\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKANT", "PPER", "VMFIN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Ohne maa\u00df und ziel:", "tokens": ["Oh\u00b7ne", "maa\u00df", "und", "ziel", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Jhre zier,", "tokens": ["Ih\u00b7re", "zier", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Will einig mir,", "tokens": ["Will", "ei\u00b7nig", "mir", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Sich in allen liebes-f\u00e4llen", "tokens": ["Sich", "in", "al\u00b7len", "lie\u00b7bes\u00b7f\u00e4l\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Zu gebothe stellen.", "tokens": ["Zu", "ge\u00b7bo\u00b7the", "stel\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Aller pracht und prangen", "tokens": ["Al\u00b7ler", "pracht", "und", "pran\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Jhrer s\u00fcssen wangen,", "tokens": ["Ih\u00b7rer", "s\u00fcs\u00b7sen", "wan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Jhr corallen-mund,", "tokens": ["Ihr", "coral\u00b7len\u00b7mund", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Jhre zarten h\u00e4nde,", "tokens": ["Ih\u00b7re", "zar\u00b7ten", "h\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Jhrer armen b\u00e4nde", "tokens": ["Ih\u00b7rer", "ar\u00b7men", "b\u00e4n\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Sind mir nun vergunt:", "tokens": ["Sind", "mir", "nun", "ver\u00b7gunt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Ehe mu\u00df", "tokens": ["E\u00b7he", "mu\u00df"], "token_info": ["word", "word"], "pos": ["NN", "VMFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Ein \u00fcberflu\u00df,", "tokens": ["Ein", "\u00fc\u00b7berf\u00b7lu\u00df", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Als ein mangel in den sachen", "tokens": ["Als", "ein", "man\u00b7gel", "in", "den", "sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Mich verdrossen machen.", "tokens": ["Mich", "ver\u00b7dros\u00b7sen", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Sind im obst viel kerne,", "tokens": ["Sind", "im", "obst", "viel", "ker\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wie am himmel sterne,", "tokens": ["Wie", "am", "him\u00b7mel", "ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wirfft der Nord viel schnee:", "tokens": ["Wirfft", "der", "Nord", "viel", "schnee", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PIAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Sind viel rauhe wellen,", "tokens": ["Sind", "viel", "rau\u00b7he", "wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJA", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wenn die winde bellen", "tokens": ["Wenn", "die", "win\u00b7de", "bel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Auf der w\u00fcsten see:", "tokens": ["Auf", "der", "w\u00fcs\u00b7ten", "see", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVFIN", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Mehr sind k\u00fc\u00df,", "tokens": ["Mehr", "sind", "k\u00fc\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Ich mei\u00df gewi\u00df,", "tokens": ["Ich", "mei\u00df", "ge\u00b7wi\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Die sie mir zum liebes-zeichen", "tokens": ["Die", "sie", "mir", "zum", "lie\u00b7bes\u00b7zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PPER", "APPRART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wird mit willen reichen.", "tokens": ["Wird", "mit", "wil\u00b7len", "rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Solt ich solcher massen", "tokens": ["Solt", "ich", "sol\u00b7cher", "mas\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PIAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Mich gereuen lassen", "tokens": ["Mich", "ge\u00b7reu\u00b7en", "las\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVINF", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Meiner sorg und pein!", "tokens": ["Mei\u00b7ner", "sorg", "und", "pein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Wer auf sein verdriessen", "tokens": ["Wer", "auf", "sein", "ver\u00b7dries\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "APPR", "PPOSAT", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Di\u00df hat zu geniessen,", "tokens": ["Di\u00df", "hat", "zu", "ge\u00b7nies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Kan nicht elend seyn:", "tokens": ["Kan", "nicht", "e\u00b7lend", "seyn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Elend kan", "tokens": ["E\u00b7lend", "kan"], "token_info": ["word", "word"], "pos": ["NN", "VMFIN"], "meter": "---", "measure": "unknown.measure.zero"}, "line.8": {"text": "Nicht seyn der mann,", "tokens": ["Nicht", "seyn", "der", "mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VAINF", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Dem sein lieb auf alles leiden", "tokens": ["Dem", "sein", "lieb", "auf", "al\u00b7les", "lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "ADJD", "APPR", "PIS", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Lohnt mit solchen freuden.", "tokens": ["Lohnt", "mit", "sol\u00b7chen", "freu\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}