{"dta.poem.9218": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Iv.  \n Als das M\u00e4dgen sich zu keinen Liebes H\u00e4n-  \n deln verstehen wolte.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Ljebstes kind/ ich bin nicht bl\u00f6de/", "tokens": ["Ljebs\u00b7tes", "kind", "/", "ich", "bin", "nicht", "bl\u00f6\u00b7de", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "PPER", "VAFIN", "PTKNEG", "ADJA", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stelle dich nur gegen mir", "tokens": ["Stel\u00b7le", "dich", "nur", "ge\u00b7gen", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jmmer noch einmahl so spr\u00f6de/", "tokens": ["Jm\u00b7mer", "noch", "ein\u00b7mahl", "so", "spr\u00f6\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADV", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich verbleibe doch bey dir/", "tokens": ["Ich", "ver\u00b7blei\u00b7be", "doch", "bey", "dir", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und empfinde meine lust", "tokens": ["Und", "emp\u00b7fin\u00b7de", "mei\u00b7ne", "lust"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Bey der unverliebten brust.", "tokens": ["Bey", "der", "un\u00b7ver\u00b7lieb\u00b7ten", "brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "2. Spotte meiner treuen liebe/", "tokens": ["Spot\u00b7te", "mei\u00b7ner", "treu\u00b7en", "lie\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lache meine reden aus/", "tokens": ["La\u00b7che", "mei\u00b7ne", "re\u00b7den", "aus", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "VVFIN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schertze wann ich mich betr\u00fcbe/", "tokens": ["Schert\u00b7ze", "wann", "ich", "mich", "be\u00b7tr\u00fc\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWAV", "PPER", "PRF", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und verschlie\u00df mir gar das haus/", "tokens": ["Und", "ver\u00b7schlie\u00df", "mir", "gar", "das", "haus", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich wil doch bey dir allein", "tokens": ["Ich", "wil", "doch", "bey", "dir", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "PPER", "ADV"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "In verliebten diensten seyn.", "tokens": ["In", "ver\u00b7lieb\u00b7ten", "diens\u00b7ten", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "3. Zeuch die h\u00e4nde stracks zur\u00fccke", "tokens": ["Zeuch", "die", "h\u00e4n\u00b7de", "stracks", "zu\u00b7r\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wann ich sie ergreiffen wil/", "tokens": ["Wann", "ich", "sie", "er\u00b7greif\u00b7fen", "wil", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Spare deine s\u00fcsse blicke/", "tokens": ["Spa\u00b7re", "dei\u00b7ne", "s\u00fcs\u00b7se", "bli\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und verderbe mir das spiel/", "tokens": ["Und", "ver\u00b7der\u00b7be", "mir", "das", "spiel", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PDS", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich bins sonsten wohl gewohnt/", "tokens": ["Ich", "bins", "sons\u00b7ten", "wohl", "ge\u00b7wohnt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVPP", "$("], "meter": "-++-+-+", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Da\u00df man mich so schlecht belohnt.", "tokens": ["Da\u00df", "man", "mich", "so", "schlecht", "be\u00b7lohnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "4. Darff ich keine r\u00f6sgen brechen", "tokens": ["Darff", "ich", "kei\u00b7ne", "r\u00f6s\u00b7gen", "bre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PIAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die auf deinen lippen stehn/", "tokens": ["Die", "auf", "dei\u00b7nen", "lip\u00b7pen", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Darff ich nicht mein liebgen sprechen/", "tokens": ["Darff", "ich", "nicht", "mein", "lieb\u00b7gen", "spre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "PPOSAT", "ADJA", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und mit dir spatzieren gehn;", "tokens": ["Und", "mit", "dir", "spat\u00b7zie\u00b7ren", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVINF", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "So verla\u00df ich doch das liecht", "tokens": ["So", "ver\u00b7la\u00df", "ich", "doch", "das", "liecht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Deiner stillen sch\u00f6nheit nicht.", "tokens": ["Dei\u00b7ner", "stil\u00b7len", "sch\u00f6n\u00b7heit", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "5. Ich erkenne dein gem\u00fcthe/", "tokens": ["Ich", "er\u00b7ken\u00b7ne", "dein", "ge\u00b7m\u00fc\u00b7the", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie es alle liebes-arth", "tokens": ["Wie", "es", "al\u00b7le", "lie\u00b7bes\u00b7arth"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die unbefleckte bl\u00fcte", "tokens": ["Und", "die", "un\u00b7be\u00b7fleck\u00b7te", "bl\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vor demselben liebsten sparth/", "tokens": ["Vor", "dem\u00b7sel\u00b7ben", "liebs\u00b7ten", "sparth", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Welcher k\u00fcnfftig mehr als wohl", "tokens": ["Wel\u00b7cher", "k\u00fcnff\u00b7tig", "mehr", "als", "wohl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAT", "ADJD", "PIAT", "KOKOM", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dich allein vergn\u00fcgen sol.", "tokens": ["Dich", "al\u00b7lein", "ver\u00b7gn\u00fc\u00b7gen", "sol", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "6. Nun ich mu\u00df dich selber loben/", "tokens": ["Nun", "ich", "mu\u00df", "dich", "sel\u00b7ber", "lo\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil die zarte sittsamkeit", "tokens": ["Weil", "die", "zar\u00b7te", "sitt\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch die allersch\u00f6nsten proben", "tokens": ["Durch", "die", "al\u00b7ler\u00b7sch\u00f6ns\u00b7ten", "pro\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Solche tugend von sich streut/", "tokens": ["Sol\u00b7che", "tu\u00b7gend", "von", "sich", "streut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PRF", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach w\u00e4r mir ein solches kind", "tokens": ["Ach", "w\u00e4r", "mir", "ein", "sol\u00b7ches", "kind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VAFIN", "PPER", "ART", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Von dem himmel auch verg\u00fcnt!", "tokens": ["Von", "dem", "him\u00b7mel", "auch", "ver\u00b7g\u00fcnt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "7. Unterdessen la\u00df mich lieben/", "tokens": ["Un\u00b7ter\u00b7des\u00b7sen", "la\u00df", "mich", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil ich dich nicht hassen kan:", "tokens": ["Weil", "ich", "dich", "nicht", "has\u00b7sen", "kan", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist mir sonst nichts \u00fcberblieben/", "tokens": ["Ist", "mir", "sonst", "nichts", "\u00fc\u00b7berb\u00b7lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PIS", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nun so bin ich wohl daran/", "tokens": ["Nun", "so", "bin", "ich", "wohl", "da\u00b7ran", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADV", "PAV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df wenn mich nach dir gel\u00fcst/", "tokens": ["Da\u00df", "wenn", "mich", "nach", "dir", "ge\u00b7l\u00fcst", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "APPR", "PPER", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mir dein ansehn offen ist.", "tokens": ["Mir", "dein", "an\u00b7sehn", "of\u00b7fen", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "VVINF", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}