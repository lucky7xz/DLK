{"dta.poem.15954": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Gl\u00fcck des Mannes kann nicht Etwas seyn, o Sohn,", "tokens": ["Das", "Gl\u00fcck", "des", "Man\u00b7nes", "kann", "nicht", "Et\u00b7was", "seyn", ",", "o", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VMFIN", "PTKNEG", "ADV", "VAINF", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo einer wenig hat und einer viel davon.", "tokens": ["Wo", "ei\u00b7ner", "we\u00b7nig", "hat", "und", "ei\u00b7ner", "viel", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "PIS", "VAFIN", "KON", "ART", "ADV", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das Gl\u00fcck mu\u00df etwas seyn wie Luft und Licht und Leben,", "tokens": ["Das", "Gl\u00fcck", "mu\u00df", "et\u00b7was", "seyn", "wie", "Luft", "und", "Licht", "und", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PIS", "VAINF", "KOKOM", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das allen allgemein, ist allen gleich gegeben.", "tokens": ["Das", "al\u00b7len", "all\u00b7ge\u00b7mein", ",", "ist", "al\u00b7len", "gleich", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJD", "$,", "VAFIN", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nicht Reichthum kann es seyn und Macht und solche Gaben,", "tokens": ["Nicht", "Reicht\u00b7hum", "kann", "es", "seyn", "und", "Macht", "und", "sol\u00b7che", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "VMFIN", "PPER", "VAINF", "KON", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wovon den einen fehlt soviel die andern haben.", "tokens": ["Wo\u00b7von", "den", "ei\u00b7nen", "fehlt", "so\u00b7viel", "die", "an\u00b7dern", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "PIS", "VVFIN", "PIS", "ART", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nicht Weisheit kann es seyn und Kunst, zu deren Stufen", "tokens": ["Nicht", "Weis\u00b7heit", "kann", "es", "seyn", "und", "Kunst", ",", "zu", "de\u00b7ren", "Stu\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "NN", "VMFIN", "PPER", "VAINF", "KON", "NN", "$,", "APPR", "PRELAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die wen'gen kommen, die besonders sind berufen.", "tokens": ["Die", "wen'\u00b7gen", "kom\u00b7men", ",", "die", "be\u00b7son\u00b7ders", "sind", "be\u00b7ru\u00b7fen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVINF", "$,", "PRELS", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Nur gut seyn ohne Groll ist h\u00f6chstes Gut des Manns,", "tokens": ["Nur", "gut", "seyn", "oh\u00b7ne", "Groll", "ist", "h\u00f6chs\u00b7tes", "Gut", "des", "Manns", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAINF", "APPR", "NN", "VAFIN", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil gut seyn jeder soll, und wer es will, der kanns.", "tokens": ["Weil", "gut", "seyn", "je\u00b7der", "soll", ",", "und", "wer", "es", "will", ",", "der", "kanns", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VAINF", "PIS", "VMFIN", "$,", "KON", "PWS", "PPER", "VMFIN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}