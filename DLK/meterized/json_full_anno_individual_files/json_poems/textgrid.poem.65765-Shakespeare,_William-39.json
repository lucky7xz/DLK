{"textgrid.poem.65765": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "39", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie mag ich sittsam denn von deinem Werte singen,", "tokens": ["Wie", "mag", "ich", "sitt\u00b7sam", "denn", "von", "dei\u00b7nem", "Wer\u00b7te", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADJD", "KON", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn du der bess're Teil nur bist von mir?", "tokens": ["Wenn", "du", "der", "bess'\u00b7re", "Teil", "nur", "bist", "von", "mir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ADV", "VAFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was kann mein Selbstlob ", "tokens": ["Was", "kann", "mein", "Selbst\u00b7lob"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Und ist's nicht Selbstlob, was ich lob' an dir?", "tokens": ["Und", "ist's", "nicht", "Selbst\u00b7lob", ",", "was", "ich", "lob'", "an", "dir", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "NN", "$,", "PWS", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "La\u00df eben darum uns gesondert leben,", "tokens": ["La\u00df", "e\u00b7ben", "da\u00b7rum", "uns", "ge\u00b7son\u00b7dert", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PAV", "PPER", "VVPP", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "La\u00df zwei f\u00fcr eine treue Liebe sein;", "tokens": ["La\u00df", "zwei", "f\u00fcr", "ei\u00b7ne", "treu\u00b7e", "Lie\u00b7be", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "CARD", "APPR", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So kann ich dir in dieser Trennung geben,", "tokens": ["So", "kann", "ich", "dir", "in", "die\u00b7ser", "Tren\u00b7nung", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Was dir dem einzigen geb\u00fchrt allein.", "tokens": ["Was", "dir", "dem", "ein\u00b7zi\u00b7gen", "ge\u00b7b\u00fchrt", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "ADJA", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Entfernung, qu\u00e4lende! wie w\u00e4r'st du tr\u00fcbe,", "tokens": ["Ent\u00b7fer\u00b7nung", ",", "qu\u00e4\u00b7len\u00b7de", "!", "wie", "w\u00e4r'st", "du", "tr\u00fc\u00b7be", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$.", "PWAV", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "W\u00e4r nicht der s\u00fc\u00dfe Trost in deiner sauern Frist:", "tokens": ["W\u00e4r", "nicht", "der", "s\u00fc\u00b7\u00dfe", "Trost", "in", "dei\u00b7ner", "sau\u00b7ern", "Frist", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "ADJ