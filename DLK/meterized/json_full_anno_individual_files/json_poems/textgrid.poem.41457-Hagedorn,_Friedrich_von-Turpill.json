{"textgrid.poem.41457": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Turpill", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Turpill, der reiche Filz, gab einmal, doch im Traum,", "tokens": ["Tur\u00b7pill", ",", "der", "rei\u00b7che", "Filz", ",", "gab", "ein\u00b7mal", ",", "doch", "im", "Traum", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "$,", "VVFIN", "ADV", "$,", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein k\u00f6nigliches Mahl, und hatte f\u00fcnfzig G\u00e4ste.", "tokens": ["Ein", "k\u00f6\u00b7nig\u00b7li\u00b7ches", "Mahl", ",", "und", "hat\u00b7te", "f\u00fcnf\u00b7zig", "G\u00e4s\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "VAFIN", "CARD", "NN", "$."], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.3": {"text": "Aus Cypern war der Wein bei diesem Freudenfeste,", "tokens": ["Aus", "Cy\u00b7pern", "war", "der", "Wein", "bei", "die\u00b7sem", "Freu\u00b7den\u00b7fes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "ART", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Sch\u00fcsseln Menge fand nicht auf der Tafel Raum.", "tokens": ["Der", "Sch\u00fcs\u00b7seln", "Men\u00b7ge", "fand", "nicht", "auf", "der", "Ta\u00b7fel", "Raum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Zugleich sieht er sich selbst im besten Stutzerkleide.", "tokens": ["Zu\u00b7gleich", "sieht", "er", "sich", "selbst", "im", "bes\u00b7ten", "Stut\u00b7zer\u00b7klei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Wie kr\u00fcmmt und qu\u00e4let sich der \u00e4chzende Turpill!", "tokens": ["Wie", "kr\u00fcmmt", "und", "qu\u00e4\u00b7let", "sich", "der", "\u00e4ch\u00b7zen\u00b7de", "Tur\u00b7pill", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "KON", "VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--++", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Ihn wecken Geiz und Angst. Gleich schw\u00f6rt er tausend Eide,", "tokens": ["Ihn", "we\u00b7cken", "Geiz", "und", "Angst", ".", "Gleich", "schw\u00f6rt", "er", "tau\u00b7send", "Ei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$.", "ADV", "VVFIN", "PPER", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df er, so lang er lebt, nicht wieder tr\u00e4umen will.", "tokens": ["Da\u00df", "er", ",", "so", "lang", "er", "lebt", ",", "nicht", "wie\u00b7der", "tr\u00e4u\u00b7men", "will", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADV", "ADJD", "PPER", "VVFIN", "$,", "PTKNEG", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Turpill, der reich