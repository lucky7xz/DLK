{"textgrid.poem.44283": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Elegie eines Schiffbr\u00fcchigen auf den Tod seines Hundes auf einer w\u00fcsten Insel", "genre": "verse", "period": "N.A.", "pub_year": 1804, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, so war noch dieser Schlag dem blutenden Herzen", "tokens": ["Ach", ",", "so", "war", "noch", "die\u00b7ser", "Schlag", "dem", "blu\u00b7ten\u00b7den", "Her\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "VAFIN", "ADV", "PDAT", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Von dem grausen Geschick zu meiner Folter bestimmt!", "tokens": ["Von", "dem", "grau\u00b7sen", "Ge\u00b7schick", "zu", "mei\u00b7ner", "Fol\u00b7ter", "be\u00b7stimmt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Mir, dem schon ein Heer von Schmerzen den Busen durchw\u00fchlet,", "tokens": ["Mir", ",", "dem", "schon", "ein", "Heer", "von", "Schmer\u00b7zen", "den", "Bu\u00b7sen", "durch\u00b7w\u00fch\u00b7let", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ADV", "ART", "NN", "APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Rei\u00dft seine mordende Hand auch noch den Treuen hinweg!", "tokens": ["Rei\u00dft", "sei\u00b7ne", "mor\u00b7den\u00b7de", "Hand", "auch", "noch", "den", "Treu\u00b7en", "hin\u00b7weg", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "ADV", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Ach, nun ist er dahin! \u2013 Mein Retter, mein Bruder, mein alles!", "tokens": ["Ach", ",", "nun", "ist", "er", "da\u00b7hin", "!", "\u2013", "Mein", "Ret\u00b7ter", ",", "mein", "Bru\u00b7der", ",", "mein", "al\u00b7les", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VAFIN", "PPER", "PAV", "$.", "$(", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "PIS", "$."], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Der mir durchs st\u00fcrmische Meer, der mir durch W\u00fcsten gefolgt,", "tokens": ["Der", "mir", "durchs", "st\u00fcr\u00b7mi\u00b7sche", "Meer", ",", "der", "mir", "durch", "W\u00fcs\u00b7ten", "ge\u00b7folgt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "ADJA", "NN", "$,", "PRELS", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Wo aus dem g\u00e4hnenden Schlund der Wogen der Tod uns entgegen-", "tokens": ["Wo", "aus", "dem", "g\u00e4h\u00b7nen\u00b7den", "Schlund", "der", "Wo\u00b7gen", "der", "Tod", "uns", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "ART", "NN", "ART", "NN", "PPER", "TRUNC"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Blickt, vom gefletschten Zahn schrecklicher Tiger uns droht. \u2013", "tokens": ["Blickt", ",", "vom", "ge\u00b7fletschten", "Zahn", "schreck\u00b7li\u00b7cher", "Ti\u00b7ger", "uns", "droht", ".", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "$,", "APPRART", "ADJA", "NN", "ADJA", "NN", "PPER", "VVFIN", "$.", "$("], "meter": "-+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Als der heulende Sturm das Schiff von Welle zu Welle", "tokens": ["Als", "der", "heu\u00b7len\u00b7de", "Sturm", "das", "Schiff", "von", "Wel\u00b7le", "zu", "Wel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "NN", "APPR", "NN", "APPR", "NN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Warf, und von Felsen zu Fels, donnernd die Woge sich brach,", "tokens": ["Warf", ",", "und", "von", "Fel\u00b7sen", "zu", "Fels", ",", "don\u00b7nernd", "die", "Wo\u00b7ge", "sich", "brach", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "APPR", "NN", "APPR", "NE", "$,", "ADJD", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.11": {"text": "Als das sonst mutige Volk, nun zagend, bebend, betend,", "tokens": ["Als", "das", "sonst", "mu\u00b7ti\u00b7ge", "Volk", ",", "nun", "za\u00b7gend", ",", "be\u00b7bend", ",", "be\u00b7tend", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "PDS", "ADV", "ADJA", "NN", "$,", "ADV", "ADJD", "$,", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Dem erhabnen Neptun heilige Opfer versprach,", "tokens": ["Dem", "er\u00b7hab\u00b7nen", "Nep\u00b7tun", "hei\u00b7li\u00b7ge", "Op\u00b7fer", "ver\u00b7sprach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "VVFIN", "