{"textgrid.poem.58649": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "81.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O du h\u00f6chst vergn\u00fcgend Gut, du Licht- und Liebeswesen,", "tokens": ["O", "du", "h\u00f6chst", "ver\u00b7gn\u00fc\u00b7gend", "Gut", ",", "du", "Licht", "und", "Lie\u00b7bes\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "ADJD", "ADJD", "$,", "PPER", "TRUNC", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Wer dich kennet, wie er soll, wer's nicht hat von H\u00f6r'n und Lesen,", "tokens": ["Wer", "dich", "ken\u00b7net", ",", "wie", "er", "soll", ",", "wer's", "nicht", "hat", "von", "H\u00f6r'n", "und", "Le\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VMFIN", "$,", "VAFIN", "PTKNEG", "VAFIN", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Wer zu dir sein ganzes Herz kehrt von aller Kreatur,", "tokens": ["Wer", "zu", "dir", "sein", "gan\u00b7zes", "Herz", "kehrt", "von", "al\u00b7ler", "Kre\u00b7a\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-++-+-+-+", "measure": "unknown.measure.octa.plus"}, "line.4": {"text": "Dem gibst du auch selbst ein Herz, da\u00df er dich erkennet nur.", "tokens": ["Dem", "gibst", "du", "auch", "selbst", "ein", "Herz", ",", "da\u00df", "er", "dich", "er\u00b7ken\u00