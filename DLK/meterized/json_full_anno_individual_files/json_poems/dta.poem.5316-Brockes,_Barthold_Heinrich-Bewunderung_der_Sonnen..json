{"dta.poem.5316": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Bewunderung der Sonnen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Solander, wie er einst zur Sommers-Zeit erblickte,", "tokens": ["So\u00b7lan\u00b7der", ",", "wie", "er", "einst", "zur", "Som\u00b7mer\u00b7sZeit", "er\u00b7blick\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit welchem Anmuths-Meer die Sonne diese Welt,", "tokens": ["Mit", "wel\u00b7chem", "An\u00b7muths\u00b7Meer", "die", "Son\u00b7ne", "die\u00b7se", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Luft, Wasser, Berg\u2019 und Thal, Land, Garten, Wald", "tokens": ["Luft", ",", "Was\u00b7ser", ",", "Ber\u00b7g'", "und", "Thal", ",", "Land", ",", "Gar\u00b7ten", ",", "Wald"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "und Feld", "tokens": ["und", "Feld"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Beflo\u00df, erleuchtete, belebt\u2019, erw\u00e4rmt\u2019 und schm\u00fcckte;", "tokens": ["Be\u00b7flo\u00df", ",", "er\u00b7leuch\u00b7te\u00b7te", ",", "be\u00b7lebt'", ",", "er\u00b7w\u00e4rmt'", "und", "schm\u00fcck\u00b7te", ";"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Fing, da ihn die\u00df f\u00fcr Anmuth fast entz\u00fcckte,", "tokens": ["Fing", ",", "da", "ihn", "die\u00df", "f\u00fcr", "An\u00b7muth", "fast", "ent\u00b7z\u00fcck\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "PDS", "APPR", "NN", "ADV", "VVFIN", "$,"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Derselben Sch\u00f6pfer an zu preisen.", "tokens": ["Der\u00b7sel\u00b7ben", "Sch\u00f6p\u00b7fer", "an", "zu", "prei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Allein, es fiel A - - - ihm ein:", "tokens": ["Al\u00b7lein", ",", "es", "fiel", "A", "ihm", "ein", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VVFIN", "NE", "$(", "$(", "$(", "PPER", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.9": {"text": "Mit Recht ergetzet dich der Sonne Wunderschein,", "tokens": ["Mit", "Recht", "er\u00b7get\u00b7zet", "dich", "der", "Son\u00b7ne", "Wun\u00b7der\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mit Recht verehrest du den Sch\u00f6pfer der Natur;", "tokens": ["Mit", "Recht", "ver\u00b7eh\u00b7rest", "du", "den", "Sch\u00f6p\u00b7fer", "der", "Na\u00b7tur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Doch die\u00df ist eine Sonne nur;", "tokens": ["Doch", "die\u00df", "ist", "ei\u00b7ne", "Son\u00b7ne", "nur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Ich will dir Millionen weisen:", "tokens": ["Ich", "will", "dir", "Mil\u00b7lion\u00b7en", "wei\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "Und darauf zeigt er ihm, in einer heitern Nacht,", "tokens": ["Und", "da\u00b7rauf", "zeigt", "er", "ihm", ",", "in", "ei\u00b7ner", "hei\u00b7tern", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "PPER", "PPER", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Des Firmaments gestirnte Pracht.", "tokens": ["Des", "Fir\u00b7ma\u00b7ments", "ge\u00b7stirn\u00b7te", "Pracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}