{"textgrid.poem.27852": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Frantzosen tragen Schuld, da\u00df so sie, wie sie schreiben,", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frantzosen tragen Schuld, da\u00df so sie, wie sie schreiben,", "tokens": ["Frant\u00b7zo\u00b7sen", "tra\u00b7gen", "Schuld", ",", "da\u00df", "so", "sie", ",", "wie", "sie", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$,", "KOUS", "ADV", "PPER", "$,", "PWAV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht reden auch; dazu so, wie sie Worte treiben,", "tokens": ["Nicht", "re\u00b7den", "auch", ";", "da\u00b7zu", "so", ",", "wie", "sie", "Wor\u00b7te", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ADV", "$.", "PAV", "ADV", "$,", "PWAV", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gar selten sind gesinnt. O, Deutschland kan es auch!", "tokens": ["Gar", "sel\u00b7ten", "sind", "ge\u00b7sinnt", ".", "O", ",", "Deutschland", "kan", "es", "auch", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "VVPP", "$.", "NE", "$,", "NE", "VMFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Sih an das W\u00f6rtlein Beut und seinen frischen Brauch.", "tokens": ["Sih", "an", "das", "W\u00f6rt\u00b7lein", "Beut", "und", "sei\u00b7nen", "fri\u00b7schen", "Brauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "NN", "KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was Feinden wird entwandt, da\u00df heisse, meinstu, Beute?", "tokens": ["Was", "Fein\u00b7den", "wird", "ent\u00b7wandt", ",", "da\u00df", "heis\u00b7se", ",", "meins\u00b7tu", ",", "Beu\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "VVPP", "$,", "KOUS", "ADJA", "$,", "VVFIN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nein, was der Bauer hat, und was die Edelleute,", "tokens": ["Nein", ",", "was", "der", "Bau\u00b7er", "hat", ",", "und", "was", "die", "E\u00b7del\u00b7leu\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PRELS", "ART", "NN", "VAFIN", "$,", "KON", "PWS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Auff Strassen was man stielt, au\u00df Kirchen was man raubt;", "tokens": ["Auff", "Stras\u00b7sen", "was", "man", "stielt", ",", "au\u00df", "Kir\u00b7chen", "was", "man", "raubt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PWS", "PIS", "VVFIN", "$,", "APPR", "NN", "PWS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Di\u00df hat das W\u00f6rtlein Beut an Freund und Feind erlaubt.", "tokens": ["Di\u00df", "hat", "das", "W\u00f6rt\u00b7lein", "Beut", "an", "Freund", "und", "Feind", "er\u00b7laubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN"