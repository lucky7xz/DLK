{"dta.poem.7677": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "49.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vielseitigkeit gef\u00e4llt an zierlichen Kristallen,", "tokens": ["Viel\u00b7sei\u00b7tig\u00b7keit", "ge\u00b7f\u00e4llt", "an", "zier\u00b7li\u00b7chen", "Kris\u00b7tal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Licht gebrochen spielt darin mit Wohlgefallen.", "tokens": ["Das", "Licht", "ge\u00b7bro\u00b7chen", "spielt", "da\u00b7rin", "mit", "Wohl\u00b7ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VVFIN", "PAV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch auch Einseitigkeit in rechter Art ist gut;", "tokens": ["Doch", "auch", "Ein\u00b7sei\u00b7tig\u00b7keit", "in", "rech\u00b7ter", "Art", "ist", "gut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "APPR", "ADJA", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Lust des Himmels ist des Se'es Spiegelflut.", "tokens": ["Die", "Lust", "des", "Him\u00b7mels", "ist", "des", "Se'\u00b7es", "Spie\u00b7gel\u00b7flut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}