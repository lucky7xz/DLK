{"dta.poem.19162": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sieh wie die Bl\u00e4ttchen sich um ihren Stengel stellen,", "tokens": ["Sieh", "wie", "die", "Bl\u00e4tt\u00b7chen", "sich", "um", "ih\u00b7ren", "Sten\u00b7gel", "stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ART", "NN", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die lebenslustigen vertr\u00e4glichen Gesellen!", "tokens": ["Die", "le\u00b7bens\u00b7lus\u00b7ti\u00b7gen", "ver\u00b7tr\u00e4g\u00b7li\u00b7chen", "Ge\u00b7sel\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Stets eines oberhalb des andern, aber so,", "tokens": ["Stets", "ei\u00b7nes", "o\u00b7ber\u00b7halb", "des", "an\u00b7dern", ",", "a\u00b7ber", "so", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "APPR", "ART", "ADJA", "$,", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df keines hinderlich ist keinem irgendwo;", "tokens": ["Da\u00df", "kei\u00b7nes", "hin\u00b7der\u00b7lich", "ist", "kei\u00b7nem", "ir\u00b7gend\u00b7wo", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VAFIN", "PIS", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So in gewundenen Abstufungen erhoben,", "tokens": ["So", "in", "ge\u00b7wun\u00b7de\u00b7nen", "Ab\u00b7stu\u00b7fun\u00b7gen", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da\u00df keines keins verdeckt, von unten frei und oben;", "tokens": ["Da\u00df", "kei\u00b7nes", "keins", "ver\u00b7deckt", ",", "von", "un\u00b7ten", "frei", "und", "o\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PIS", "VVPP", "$,", "APPR", "ADV", "ADJD", "KON", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Da\u00df jedes saugen kann von unten her den Segen", "tokens": ["Da\u00df", "je\u00b7des", "sau\u00b7gen", "kann", "von", "un\u00b7ten", "her", "den", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "VVINF", "VMFIN", "APPR", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Thaus der aufsteigt, und von oben her den Regen,", "tokens": ["Des", "Thaus", "der", "auf\u00b7steigt", ",", "und", "von", "o\u00b7ben", "her", "den", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VVPP", "$,", "KON", "APPR", "ADV", "APZR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Nach allen Seiten hin sich breitend in die Luft,", "tokens": ["Nach", "al\u00b7len", "Sei\u00b7ten", "hin", "sich", "brei\u00b7tend", "in", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "PRF", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schwelgend in Sonnenglanz und in der N\u00e4chte Duft.", "tokens": ["Schwel\u00b7gend", "in", "Son\u00b7nen\u00b7glanz", "und", "in", "der", "N\u00e4ch\u00b7te", "Duft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}}}}