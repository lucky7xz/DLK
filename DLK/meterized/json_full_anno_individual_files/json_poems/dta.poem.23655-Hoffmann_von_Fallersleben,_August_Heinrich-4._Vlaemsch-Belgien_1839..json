{"dta.poem.23655": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "4. Vlaemsch-Belgien 1839.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nein, du bist noch nicht verloren,", "tokens": ["Nein", ",", "du", "bist", "noch", "nicht", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6nes gottgesegnet Land!", "tokens": ["Sch\u00f6\u00b7nes", "gott\u00b7ge\u00b7seg\u00b7net", "Land", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ueber dir und deinen Thoren", "tokens": ["Ue\u00b7ber", "dir", "und", "dei\u00b7nen", "Tho\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ruht noch schirmend Gottes Hand.", "tokens": ["Ruht", "noch", "schir\u00b7mend", "Got\u00b7tes", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Deine Sprach' und Sitte lebt noch", "tokens": ["Dei\u00b7ne", "Sprach'", "und", "Sit\u00b7te", "lebt", "noch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueberall in Stadt und Land,", "tokens": ["Ue\u00b7be\u00b7rall", "in", "Stadt", "und", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Vorzeit Ruhm erhebt noch", "tokens": ["Und", "der", "Vor\u00b7zeit", "Ruhm", "er\u00b7hebt", "noch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jedes Herz und jede Hand.", "tokens": ["Je\u00b7des", "Herz", "und", "je\u00b7de", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Freiheit hat dir Gott gegeben:", "tokens": ["Frei\u00b7heit", "hat", "dir", "Gott", "ge\u00b7ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sei dann frei, du freies Land!", "tokens": ["Sei", "dann", "frei", ",", "du", "frei\u00b7es", "Land", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Frei zu edlem Thun und Streben!", "tokens": ["Frei", "zu", "ed\u00b7lem", "Thun", "und", "Stre\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Frei von w\u00e4lschem Lug und Tand!", "tokens": ["Frei", "von", "w\u00e4l\u00b7schem", "Lug", "und", "Tand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}