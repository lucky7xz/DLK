{"dta.poem.10881": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Jagd-Cantata.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf auf! es f\u00e4ngt schon an zu tagen.", "tokens": ["Auf", "auf", "!", "es", "f\u00e4ngt", "schon", "an", "zu", "ta\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PTKVZ", "$.", "PPER", "VVFIN", "ADV", "APPR", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Berge Gipfel gl\u00fchen schon;", "tokens": ["Der", "Ber\u00b7ge", "Gip\u00b7fel", "gl\u00fc\u00b7hen", "schon", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es schallt des Waldhorns muntrer Ton.", "tokens": ["Es", "schallt", "des", "Wald\u00b7horns", "mun\u00b7trer", "Ton", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf! auf! zur Lust! auf! auf! zum Jagen!", "tokens": ["Auf", "!", "auf", "!", "zur", "Lust", "!", "auf", "!", "auf", "!", "zum", "Ja\u00b7gen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "$.", "PTKVZ", "$.", "APPRART", "NN", "$.", "PTKVZ", "$.", "PTKVZ", "$.", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}