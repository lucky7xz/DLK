{"textgrid.poem.42092": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ganz fr\u00fch: Du tauchtest Dampf der morgendlichen G\u00e4rten", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ganz fr\u00fch: Du tauchtest Dampf der morgendlichen G\u00e4rten", "tokens": ["Ganz", "fr\u00fch", ":", "Du", "tauch\u00b7test", "Dampf", "der", "mor\u00b7gend\u00b7li\u00b7chen", "G\u00e4r\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$.", "PPER", "VVFIN", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In meine Seele. Der blauen Sichel des Mondes", "tokens": ["In", "mei\u00b7ne", "See\u00b7le", ".", "Der", "blau\u00b7en", "Si\u00b7chel", "des", "Mon\u00b7des"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$.", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Namen gabst du, verwandt. Es reihten die Tiere", "tokens": ["Na\u00b7men", "gabst", "du", ",", "ver\u00b7wandt", ".", "Es", "reih\u00b7ten", "die", "Tie\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "$,", "VVPP", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Dir sich gehorsam, Zauberer, der du die Blumen", "tokens": ["Dir", "sich", "ge\u00b7hor\u00b7sam", ",", "Zau\u00b7be\u00b7rer", ",", "der", "du", "die", "Blu\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "PRF", "ADJD", "$,", "NN", "$,", "PRELS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Fremd im Abend beschworst zwischen Farrnkraut und Steinen.", "tokens": ["Fremd", "im", "A\u00b7bend", "be\u00b7schworst", "zwi\u00b7schen", "Farrn\u00b7kraut", "und", "Stei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Mich auch einmal. Und gehst mir weiter die fremden", "tokens": ["Mich", "auch", "ein\u00b7mal", ".", "Und", "gehst", "mir", "wei\u00b7ter", "die", "frem\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADV", "$.", "KON", "VVFIN", "PPER", "ADV", "ART", "ADJA"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Wege wie damals voran. Wei\u00dft schon den wei\u00dfen", "tokens": ["We\u00b7ge", "wie", "da\u00b7mals", "vo\u00b7ran", ".", "Wei\u00dft", "schon", "den", "wei\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "ADV", "PTKVZ", "$.", "VVFIN", "ADV", "ART", "ADJA"], "meter": "+--+---+--+-", "measure": "dactylic.di.plus"}, "line.8": {"text": "Schimmer des Haars. So reichst du, das Endliche kennend", "tokens": ["Schim\u00b7mer", "des", "Haars", ".", "So", "reichst", "du", ",", "das", "End\u00b7li\u00b7che", "ken\u00b7nend"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$.", "ADV", "VVFIN", "PPER", "$,", "ART", "NN", "VVPP"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.9": {"text": "Immer zuerst, den Tod wie damals den Tau, wie die Blumen.", "tokens": ["Im\u00b7mer", "zu\u00b7erst", ",", "den", "Tod", "wie", "da\u00b7mals", "den", "Tau", ",", "wie", "die", "Blu\u0