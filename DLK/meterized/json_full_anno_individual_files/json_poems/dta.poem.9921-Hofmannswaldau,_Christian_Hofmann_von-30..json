{"dta.poem.9921": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "30.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Allen leuten dien\u2019 ich treulich/ welche meiner d\u00fcrfftig seyn/", "tokens": ["Al\u00b7len", "leu\u00b7ten", "dien'", "ich", "treu\u00b7lich", "/", "wel\u00b7che", "mei\u00b7ner", "d\u00fcr\u00b7ff\u00b7tig", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVINF", "VMFIN", "PPER", "ADJD", "$(", "PRELS", "PPOSAT", "ADJD", "VAINF", "$("], "meter": "+-+-+-+-+-+-+--+", "measure": "iambic.octa.plus.octa.plus.chol"}, "line.2": {"text": "Wo es voll verwirrung stehet/ f\u00fchr ich gleich die ordnung ein/", "tokens": ["Wo", "es", "voll", "ver\u00b7wir\u00b7rung", "ste\u00b7het", "/", "f\u00fchr", "ich", "gleich", "die", "ord\u00b7nung", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "NN", "VVFIN", "$(", "VVFIN", "PPER", "ADV", "ART", "NN", "ART", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Z\u00e4hne hab ich die man siehet/ doch damit verletz ich keinen/", "tokens": ["Z\u00e4h\u00b7ne", "hab", "ich", "die", "man", "sie\u00b7het", "/", "doch", "da\u00b7mit", "ver\u00b7letz", "ich", "kei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ART", "PIS", "VVFIN", "$(", "ADV", "PAV", "VVFIN", "PPER", "PIAT", "$("], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Niemals ess\u2019 ich/ sondern faste/ und doch pfleg ich fett zu scheine\u0303.", "tokens": ["Nie\u00b7mals", "ess'", "ich", "/", "son\u00b7dern", "fas\u00b7te", "/", "und", "doch", "pfleg", "ich", "fett", "zu", "schein\u1ebd", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "KON", "ADJA", "$(", "KON", "ADV", "VVFIN", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}