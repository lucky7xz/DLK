ie\u00b7dern", "Nei\u00b7gun\u00b7gen", "und", "Las\u00b7ter", "fliehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Wenn du zu ihm dich schwingst.", "tokens": ["Wenn", "du", "zu", "ihm", "dich", "schwingst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.27": {"line.1": {"text": "Die Sonne steige nie aus rother Fluth,", "tokens": ["Die", "Son\u00b7ne", "stei\u00b7ge", "nie", "aus", "ro\u00b7ther", "Fluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und sinke nie darein,", "tokens": ["Und", "sin\u00b7ke", "nie", "da\u00b7rein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df du nicht deine Stimm vereinigst mit", "tokens": ["Da\u00df", "du", "nicht", "dei\u00b7ne", "Stimm", "ver\u00b7ei\u00b7nigst", "mit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "PPOSAT", "NN", "VVFIN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Stimme der Natur.", "tokens": ["Der", "Stim\u00b7me", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.28": {"line.1": {"text": "Lob ihn im Regen und in d\u00fcrrer Zeit,", "tokens": ["Lob", "ihn", "im", "Re\u00b7gen", "und", "in", "d\u00fcr\u00b7rer", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "KON", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Im Sonnenschein und Sturm!", "tokens": ["Im", "Son\u00b7nen\u00b7schein", "und", "Sturm", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenns schneyt, wenn Frost aus Wasser Br\u00fccken baut,", "tokens": ["Wenns", "schneyt", ",", "wenn", "Frost", "aus", "Was\u00b7ser", "Br\u00fc\u00b7cken", "baut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "$,", "KOUS", "NN", "APPR", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und wenn die Erde gr\u00fcnt.", "tokens": ["Und", "wenn", "die", "Er\u00b7de", "gr\u00fcnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.29": {"line.1": {"text": "In \u00dcberschwemmungen, in Krieg und Pest", "tokens": ["In", "\u00dc\u00b7bersc\u00b7hwem\u00b7mun\u00b7gen", ",", "in", "Krieg", "und", "Pest"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Trau ihm, und sing ihm Lob!", "tokens": ["Trau", "ihm", ",", "und", "sing", "ihm", "Lob", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KON", "VVFIN", "PPER", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Er sorgt f\u00fcr dich, denn er erschuf zum Gl\u00fcck", "tokens": ["Er", "sorgt", "f\u00fcr", "dich", ",", "denn", "er", "er\u00b7schuf", "zum",