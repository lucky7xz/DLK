{"textgrid.poem.48928": {"metadata": {"author": {"name": "Kleist, Ewald Christian von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gro\u00df ist der Herr! die Himmel ohne Zahl", "genre": "verse", "period": "N.A.", "pub_year": 1737, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gro\u00df ist der Herr! die Himmel ohne Zahl", "tokens": ["Gro\u00df", "ist", "der", "Herr", "!", "die", "Him\u00b7mel", "oh\u00b7ne", "Zahl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$.", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sind seine Wohnungen,", "tokens": ["Sind", "sei\u00b7ne", "Woh\u00b7nun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Sein Wagen, Sturm und donnernde Gew\u00f6lk,", "tokens": ["Sein", "Wa\u00b7gen", ",", "Sturm", "und", "don\u00b7nern\u00b7de", "Ge\u00b7w\u00f6lk", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Blitze sein Gespann.", "tokens": ["Und", "Blit\u00b7ze", "sein", "Ge\u00b7spann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Morgenr\u00f6th' ist nur ein Wiederschein", "tokens": ["Die", "Mor\u00b7gen\u00b7r\u00f6\u00b7th'", "ist", "nur", "ein", "Wie\u00b7der\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Vom Saume seines Kleids,", "tokens": ["Vom", "Sau\u00b7me", "sei\u00b7nes", "Kleids", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und gegen seinen Glanz, ist Demmerung", "tokens": ["Und", "ge\u00b7gen", "sei\u00b7nen", "Glanz", ",", "ist", "Dem\u00b7me\u00b7rung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "$,", "VAFIN", "NN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Der Sonne flammend Licht.", "tokens": ["Der", "Son\u00b7ne", "flam\u00b7mend", "Licht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er sieht mit gn\u00e4dgem Blick zur Erd herab;", "tokens": ["Er", "sieht", "mit", "gn\u00e4d\u00b7gem", "Blick", "zur", "Erd", "her\u00b7ab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sie gr\u00fcnet, bl\u00fcht und lacht.", "tokens": ["Sie", "gr\u00fc\u00b7net", ",", "bl\u00fcht", "und", "lacht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er schilt; es f\u00e4hret Feur von Felsen auf,", "tokens": ["Er", "schilt", ";", "es", "f\u00e4h\u00b7ret", "Feur", "von", "Fel\u00b7sen", "auf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "NN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Meer und Himmel klagt.", "tokens": ["Und", "Meer", "und", "Him\u00b7mel", "klagt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Lobt den gewaltigen, den gn\u00e4dgen Herrn,", "tokens": ["Lobt", "den", "ge\u00b7wal\u00b7ti\u00b7gen", ",", "den", "gn\u00e4d\u00b7gen", "Herrn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ihr Lichter seiner Burg,", "tokens": ["Ihr", "Lich\u00b7ter", "sei\u00b7ner", "Burg", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Sonnenheere! Flammt zu seinem Ruhm!", "tokens": ["Ihr", "Son\u00b7nen\u00b7hee\u00b7re", "!", "Flammt", "zu", "sei\u00b7nem", "Ruhm", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ihr Erden singt sein Lob!", "tokens": ["Ihr", "Er\u00b7den", "singt", "sein", "Lob", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Erhebet ihn ihr Meere! Braust sein Lob!", "tokens": ["Er\u00b7he\u00b7bet", "ihn", "ihr", "Mee\u00b7re", "!", "Braust", "sein", "Lob", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$.", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ihr Fl\u00fcsse rauschet es!", "tokens": ["Ihr", "Fl\u00fcs\u00b7se", "rau\u00b7schet", "es", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es neige sich der Cedern hohes Haupt,", "tokens": ["Es", "nei\u00b7ge", "sich", "der", "Ce\u00b7dern", "ho\u00b7hes", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und jeder Wald f\u00fcr ihn!", "tokens": ["Und", "je\u00b7der", "Wald", "f\u00fcr", "ihn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ihr L\u00f6wen br\u00fcllt zu seiner Ehr im Hayn!", "tokens": ["Ihr", "L\u00f6\u00b7wen", "br\u00fcllt", "zu", "sei\u00b7ner", "Ehr", "im", "Hayn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Singt ihm, ihr V\u00f6gel! singt!", "tokens": ["Singt", "ihm", ",", "ihr", "V\u00f6\u00b7gel", "!", "singt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$.", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Seyd sein Altar ihr Felsen, die er traf,", "tokens": ["Seyd", "sein", "Al\u00b7tar", "ihr", "Fel\u00b7sen", ",", "die", "er", "traf", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPOSAT", "NN", "PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Eur Dampf sey Weyrauch ihm!", "tokens": ["Eur", "Dampf", "sey", "Wey\u00b7rauch", "ihm", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Der Wiederhall lob ihn! Und die Natur", "tokens": ["Der", "Wie\u00b7der\u00b7hall", "lob", "ihn", "!", "Und", "die", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "PPER", "$.", "KON", "ART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sing ihm ein froh Concert!", "tokens": ["Sing", "ihm", "ein", "froh", "Con\u00b7cert", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJD", "NE", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Und du, der Erden Herr, o Mensch! zerflie\u00df", "tokens": ["Und", "du", ",", "der", "Er\u00b7den", "Herr", ",", "o", "Mensch", "!", "zer\u00b7flie\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "PPER", "$,", "ART", "NN", "NN", "$,", "FM", "NN", "$.", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In Harmonien ganz!", "tokens": ["In", "Har\u00b7mo\u00b7ni\u00b7en", "ganz", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Dich hat er, mehr als alles sonst, begl\u00fcckt.", "tokens": ["Dich", "hat", "er", ",", "mehr", "als", "al\u00b7les", "sonst", ",", "be\u00b7gl\u00fcckt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "ADV", "KOUS", "PIS", "ADV", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er gab dir einen Geist,", "tokens": ["Er", "gab", "dir", "ei\u00b7nen", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der durch den Bau des Ganzen dringt und kennt", "tokens": ["Der", "durch", "den", "Bau", "des", "Gan\u00b7zen", "dringt", "und", "kennt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die R\u00e4der der Natur.", "tokens": ["Die", "R\u00e4\u00b7der", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Erheb ihn hoch zu deiner Seeligkeit!", "tokens": ["Er\u00b7heb", "ihn", "hoch", "zu", "dei\u00b7ner", "See\u00b7lig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er braucht kein Lob zum Gl\u00fcck.", "tokens": ["Er", "braucht", "kein", "Lob", "zum", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die niedern Neigungen und Laster fliehn,", "tokens": ["Die", "nie\u00b7dern", "Nei\u00b7gun\u00b7gen", "und", "Las\u00b7ter", "fliehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Wenn du zu ihm dich schwingst.", "tokens": ["Wenn", "du", "zu", "ihm", "dich", "schwingst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Die Sonne steige nie aus rother Fluth,", "tokens": ["Die", "Son\u00b7ne", "stei\u00b7ge", "nie", "aus", "ro\u00b7ther", "Fluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und sinke nie darein,", "tokens": ["Und", "sin\u00b7ke", "nie", "da\u00b7rein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df du nicht deine Stimm vereinigst mit", "tokens": ["Da\u00df", "du", "nicht", "dei\u00b7ne", "Stimm", "ver\u00b7ei\u00b7nigst", "mit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "PPOSAT", "NN", "VVFIN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Stimme der Natur.", "tokens": ["Der", "Stim\u00b7me", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Lob ihn im Regen und in d\u00fcrrer Zeit,", "tokens": ["Lob", "ihn", "im", "Re\u00b7gen", "und", "in", "d\u00fcr\u00b7rer", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "KON", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Im Sonnenschein und Sturm!", "tokens": ["Im", "Son\u00b7nen\u00b7schein", "und", "Sturm", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenns schneyt, wenn Frost aus Wasser Br\u00fccken baut,", "tokens": ["Wenns", "schneyt", ",", "wenn", "Frost", "aus", "Was\u00b7ser", "Br\u00fc\u00b7cken", "baut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "$,", "KOUS", "NN", "APPR", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und wenn die Erde gr\u00fcnt.", "tokens": ["Und", "wenn", "die", "Er\u00b7de", "gr\u00fcnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "In \u00dcberschwemmungen, in Krieg und Pest", "tokens": ["In", "\u00dc\u00b7bersc\u00b7hwem\u00b7mun\u00b7gen", ",", "in", "Krieg", "und", "Pest"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Trau ihm, und sing ihm Lob!", "tokens": ["Trau", "ihm", ",", "und", "sing", "ihm", "Lob", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KON", "VVFIN", "PPER", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Er sorgt f\u00fcr dich, denn er erschuf zum Gl\u00fcck", "tokens": ["Er", "sorgt", "f\u00fcr", "dich", ",", "denn", "er", "er\u00b7schuf", "zum", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "KON", "PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das menschliche Geschlecht.", "tokens": ["Das", "menschli\u00b7che", "Ge\u00b7schlecht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.13": {"line.1": {"text": "Und o wie liebreich sorgt er auch f\u00fcr mich!", "tokens": ["Und", "o", "wie", "lieb\u00b7reich", "sorgt", "er", "auch", "f\u00fcr", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "FM", "KOKOM", "ADJD", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Statt Golds und Ruhms, giebt er", "tokens": ["Statt", "Golds", "und", "Ruhms", ",", "giebt", "er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "NN", "KON", "NN", "$,", "VVFIN", "PPER"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.3": {"text": "Verm\u00f6gen mir die Wahrheit einzusehn,", "tokens": ["Ver\u00b7m\u00f6\u00b7gen", "mir", "die", "Wahr\u00b7heit", "ein\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Freund' und Saytenspiel.", "tokens": ["Und", "Freund'", "und", "Say\u00b7ten\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Erhalte mir, o Herr! was du verleihst;", "tokens": ["Er\u00b7hal\u00b7te", "mir", ",", "o", "Herr", "!", "was", "du", "ver\u00b7leihst", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "FM", "NN", "$.", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mehr brauch ich nicht zum Gl\u00fcck.", "tokens": ["Mehr", "brauch", "ich", "nicht", "zum", "Gl\u00fcck", "."], "token_inf