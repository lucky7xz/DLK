, "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Und irren ans Gestad des Meers, und dich", "tokens": ["Und", "ir\u00b7ren", "ans", "Ge\u00b7stad", "des", "Meers", ",", "und", "dich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "NN", "$,", "KON", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In jeder Woge sehn,", "tokens": ["In", "je\u00b7der", "Wo\u00b7ge", "sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und h\u00f6ren dich im Sturm, bewundern in", "tokens": ["Und", "h\u00f6\u00b7ren", "dich", "im", "Sturm", ",", "be\u00b7wun\u00b7dern", "in"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "$,", "VVFIN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Au Tapeten dich.", "tokens": ["Der", "Au", "Ta\u00b7pe\u00b7ten", "dich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Ich will entz\u00fcckt auf Felsen klimmen, durch", "tokens": ["Ich", "will", "ent\u00b7z\u00fcckt", "auf", "Fel\u00b7sen", "klim\u00b7men", ",", "durch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VMFIN", "VVPP", "APPR", "NN", "VVINF", "$,", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zerri\u00dfne Wolken sehn,", "tokens": ["Zer\u00b7ri\u00df\u00b7ne", "Wol\u00b7ken", "sehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+", "