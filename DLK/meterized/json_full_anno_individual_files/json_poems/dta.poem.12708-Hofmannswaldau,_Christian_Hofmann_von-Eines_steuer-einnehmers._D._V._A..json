{"dta.poem.12708": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Eines steuer-einnehmers.  \n  D.  V. A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die steuern nahm ich ein, und legte rechnung ab,", "tokens": ["Die", "steu\u00b7ern", "nahm", "ich", "ein", ",", "und", "leg\u00b7te", "rech\u00b7nung", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So, wie es sich geb\u00fchrt. Jtzt, da der rest zu heben", "tokens": ["So", ",", "wie", "es", "sich", "ge\u00b7b\u00fchrt", ".", "Jtzt", ",", "da", "der", "rest", "zu", "he\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "PPER", "PRF", "VVPP", "$.", "ADV", "$,", "KOUS", "ART", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und zu erlegen ist, zahl ich selbst mit dem leben:", "tokens": ["Und", "zu", "er\u00b7le\u00b7gen", "ist", ",", "zahl", "ich", "selbst", "mit", "dem", "le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKZU", "VVINF", "VAFIN", "$,", "KOUS", "PPER", "ADV", "APPR", "ART", "VVINF", "$."], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die seele kriegt mein GOtt, und meinen leib das grab.", "tokens": ["Die", "see\u00b7le", "kriegt", "mein", "Gott", ",", "und", "mei\u00b7nen", "leib", "das", "grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,", "KON", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}