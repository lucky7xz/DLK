", "measure": "iambic.tri"}, "line.6": {"text": "Und schaff' sie in Dir neu;", "tokens": ["Und", "schaff'", "sie", "in", "Dir", "neu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mein Denken nimm von hinnen,", "tokens": ["Mein", "Den\u00b7ken", "nimm", "von", "hin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Auf da\u00df es heilig sei.", "tokens": ["Auf", "da\u00df", "es", "hei\u00b7lig", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Mein Glauben, Lieben, Loben,", "tokens": ["Mein", "Glau\u00b7ben", ",", "Lie\u00b7ben", ",", "Lo\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJA", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Hoffen, mein Verstehn,", "tokens": ["Mein", "Hof\u00b7fen", ",", "mein", "Ver\u00b7stehn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das lehre all nach oben", "tokens": ["Das", "leh\u00b7re", "all", "nach", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PIAT", "APPR", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu Dir, mein Heiland, gehn.", "tokens": ["Zu", "Dir", ",", "mein", "Hei\u00b7land", ",", "gehn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPO