{"textgrid.poem.35808": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du kannst mich nicht verlassen,", "genre": "verse", "period": "N.A.", "pub_year": 1818, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du kannst mich nicht verlassen,", "tokens": ["Du", "kannst", "mich", "nicht", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du wirst mich doch befrein,", "tokens": ["Du", "wirst", "mich", "doch", "be\u00b7fr\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du kannst mich doch nicht hassen,", "tokens": ["Du", "kannst", "mich", "doch", "nicht", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du mu\u00dft mir doch verzeihn:", "tokens": ["Du", "mu\u00dft", "mir", "doch", "ver\u00b7zeihn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich liebe Dich von Herzen,", "tokens": ["Ich", "lie\u00b7be", "Dich", "von", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ich sehne mich nach Dir;", "tokens": ["Ich", "seh\u00b7ne", "mich", "nach", "Dir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich suche Dich mit Schmerzen", "tokens": ["Ich", "su\u00b7che", "Dich", "mit", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und weine f\u00fcr und f\u00fcr.", "tokens": ["Und", "wei\u00b7ne", "f\u00fcr", "und", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "KON", "APPR", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und kann Dich doch nicht finden,", "tokens": ["Und", "kann", "Dich", "doch", "nicht", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So weit mein Blicken reicht;", "tokens": ["So", "weit", "mein", "Bli\u00b7cken", "reicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ach! meine vielen S\u00fcnden,", "tokens": ["Ach", "!", "mei\u00b7ne", "vie\u00b7len", "S\u00fcn\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPOSAT", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die haben Dich verscheucht.", "tokens": ["Die", "ha\u00b7ben", "Dich", "ver\u00b7scheucht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Flie\u00dft aus, flie\u00dft aus, ihr Augen,", "tokens": ["Flie\u00dft", "aus", ",", "flie\u00dft", "aus", ",", "ihr", "Au\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wenn ihr Ihn nicht erseht:", "tokens": ["Wenn", "ihr", "Ihn", "nicht", "er\u00b7seht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Was soll das Sehen taugen", "tokens": ["Was", "soll", "das", "Se\u00b7hen", "tau\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dem, der im Finstern geht?", "tokens": ["Dem", ",", "der", "im", "Fins\u00b7tern", "geht", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Du Liebe, einz'ge Liebe,", "tokens": ["Du", "Lie\u00b7be", ",", "einz'\u00b7ge", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du Licht, das brennt und lacht,", "tokens": ["Du", "Licht", ",", "das", "brennt", "und", "lacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PDS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nimm alle meine Triebe", "tokens": ["Nimm", "al\u00b7le", "mei\u00b7ne", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In Deine Hut und Acht.", "tokens": ["In", "Dei\u00b7ne", "Hut", "und", "Acht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "CARD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nimm alle meine Sinnen", "tokens": ["Nimm", "al\u00b7le", "mei\u00b7ne", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und schaff' sie in Dir neu;", "tokens": ["Und", "schaff'", "sie", "in", "Dir", "neu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mein Denken nimm von hinnen,", "tokens": ["Mein", "Den\u00b7ken", "nimm", "von", "hin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Auf da\u00df es heilig sei.", "tokens": ["Auf", "da\u00df", "es", "hei\u00b7lig", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Mein Glauben, Lieben, Loben,", "tokens": ["Mein", "Glau\u00b7ben", ",", "Lie\u00b7ben", ",", "Lo\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJA", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Hoffen, mein Verstehn,", "tokens": ["Mein", "Hof\u00b7fen", ",", "mein", "Ver\u00b7stehn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das lehre all nach oben", "tokens": ["Das", "leh\u00b7re", "all", "nach", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PIAT", "APPR", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu Dir, mein Heiland, gehn.", "tokens": ["Zu", "Dir", ",", "mein", "Hei\u00b7land", ",", "gehn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPOSAT", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nach Dir, Herr! will ich schreien,", "tokens": ["Nach", "Dir", ",", "Herr", "!", "will", "ich", "schrei\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "NN", "$.", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dich lassen will ich nicht,", "tokens": ["Dich", "las\u00b7sen", "will", "ich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VMFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ob tausend Teufel dr\u00e4uen,", "tokens": ["Ob", "tau\u00b7send", "Teu\u00b7fel", "dr\u00e4u\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und ob das Herz mir bricht.", "tokens": ["Und", "ob", "das", "Herz", "mir", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und soll ich Dich nicht haben,", "tokens": ["Und", "soll", "ich", "Dich", "nicht", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PRF", "PTKNEG", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den ich allein erw\u00e4hlt,", "tokens": ["Den", "ich", "al\u00b7lein", "er\u00b7w\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So soll man mich begraben", "tokens": ["So", "soll", "man", "mich", "be\u00b7gra\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PIS", "PRF", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verschm\u00e4ht und unverm\u00e4hlt.", "tokens": ["Ver\u00b7schm\u00e4ht", "und", "un\u00b7ver\u00b7m\u00e4hlt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und sollt' ich Dich nicht sehen,", "tokens": ["Und", "sollt'", "ich", "Dich", "nicht", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mich Dein nicht ewig freu'n,", "tokens": ["Mich", "Dein", "nicht", "e\u00b7wig", "freu'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "M\u00f6cht' ich nicht auferstehen,", "tokens": ["M\u00f6cht'", "ich", "nicht", "auf\u00b7er\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "M\u00f6cht' ich nicht selig sein!", "tokens": ["M\u00f6cht'", "ich", "nicht", "se\u00b7lig", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Du kannst mich nich