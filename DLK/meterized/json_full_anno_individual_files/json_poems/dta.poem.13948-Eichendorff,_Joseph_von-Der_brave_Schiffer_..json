{"dta.poem.13948": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der brave Schiffer .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Und Sturm und Nacht verzogen,", "tokens": ["Und", "Sturm", "und", "Nacht", "ver\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schon blitzt es hier und da,", "tokens": ["Schon", "blitzt", "es", "hier", "und", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Land stieg aus den Wogen", "tokens": ["Das", "Land", "stieg", "aus", "den", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und unter dem Friedensbogen", "tokens": ["Und", "un\u00b7ter", "dem", "Frie\u00b7dens\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Die alte Victoria. \u2014", "tokens": ["Die", "al\u00b7te", "Vic\u00b7to\u00b7ria", "."], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NE", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}