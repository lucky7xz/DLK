{"dta.poem.1056": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Epod.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1618", "urn": "urn:nbn:de:kobv:b4-20628-9", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das derhalb kein vndergang", "tokens": ["Das", "der\u00b7halb", "kein", "vn\u00b7der\u00b7gang"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "PAV", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ewer lob vnd ehr bedecken/", "tokens": ["E\u00b7wer", "lob", "vnd", "ehr", "be\u00b7de\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sondern mit wachsende\u0303 schw\u00e4g", "tokens": ["Son\u00b7dern", "mit", "wach\u00b7sen\u00b7d\u1ebd", "schw\u00e4g"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Sie sich stehts m\u00f6gen au\u00dfstr\u00f6cken/", "tokens": ["Sie", "sich", "stehts", "m\u00f6\u00b7gen", "au\u00df\u00b7str\u00f6\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ist nicht des Golds schwacher schein/", "tokens": ["Ist", "nicht", "des", "Golds", "schwa\u00b7cher", "schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Vnd der zeit-f\u00f6rchtende stein", "tokens": ["Vnd", "der", "zeit\u00b7f\u00f6rch\u00b7ten\u00b7de", "stein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "---+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "In der wolcken weg zusetzen;", "tokens": ["In", "der", "wol\u00b7cken", "weg", "zu\u00b7set\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sondern ewer aug vnd hand", "tokens": ["Son\u00b7dern", "e\u00b7wer", "aug", "vnd", "hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Soll die Poeten er g\u00f6tzen/", "tokens": ["Soll", "die", "Po\u00b7et\u00b7en", "er", "g\u00f6t\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Das sie ewer macht vnd stand", "tokens": ["Das", "sie", "e\u00b7wer", "macht", "vnd", "stand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "ADJD", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Auf die Vnsterblichkeit etzen.", "tokens": ["Auf", "die", "Vns\u00b7terb\u00b7lich\u00b7keit", "et\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}