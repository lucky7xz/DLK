{"textgrid.poem.41593": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Lebendgem Tempel gleicht das Wesen der Natur,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lebendgem Tempel gleicht das Wesen der Natur,", "tokens": ["Le\u00b7bend\u00b7gem", "Tem\u00b7pel", "gleicht", "das", "We\u00b7sen", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Aus seinen S\u00e4ulenreihn t\u00f6nt tief geheimes Fl\u00fcstern,", "tokens": ["Aus", "sei\u00b7nen", "S\u00e4u\u00b7len\u00b7reihn", "t\u00f6nt", "tief", "ge\u00b7hei\u00b7mes", "Fl\u00fcs\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Durch W\u00e4lder geht der Mensch, wo Zeichen ihn umd\u00fcstern,", "tokens": ["Durch", "W\u00e4l\u00b7der", "geht", "der", "Mensch", ",", "wo", "Zei\u00b7chen", "ihn", "um\u00b7d\u00fcs\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$,", "PWAV", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die stillvertrauten Blicks verfolgen seine Spur.", "tokens": ["Die", "still\u00b7ver\u00b7trau\u00b7ten", "Blicks", "ver\u00b7fol\u00b7gen", "sei\u00b7ne", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Geheim verschmelzend wie das Echo fernster Kl\u00fcfte,", "tokens": ["Ge\u00b7heim", "ver\u00b7schmel\u00b7zend", "wie", "das", "E\u00b7cho", "ferns\u00b7ter", "Kl\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "KOKOM", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In gro\u00dfer Einheit und voll dunkeltiefer Macht,", "tokens": ["In", "gro\u00b7\u00dfer", "Ein\u00b7heit", "und", "voll", "dun\u00b7kel\u00b7tie\u00b7fer", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weit wie des \u00c4thers Glanz und die gewaltge Nacht,", "tokens": ["Weit", "wie", "des", "\u00c4\u00b7thers", "Glanz", "und", "die", "ge\u00b7walt\u00b7ge", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Antworten T\u00f6ne rings und Farben sich und D\u00fcfte.", "tokens": ["Ant\u00b7wor\u00b7ten", "T\u00f6\u00b7ne", "rings", "und", "Far\u00b7ben", "sich", "und", "D\u00fcf\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADV", "KON", "NN", "PRF", "KON", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.3": {"line.1": {"text": "Ger\u00fcche sind, wie Duft, der \u00fcber Kindern ruht,", "tokens": ["Ge\u00b7r\u00fc\u00b7che", "sind", ",", "wie", "Duft", ",", "der", "\u00fc\u00b7ber", "Kin\u00b7dern", "ruht", ","], "token_info": ["word", "word", "punct", "word", "word"