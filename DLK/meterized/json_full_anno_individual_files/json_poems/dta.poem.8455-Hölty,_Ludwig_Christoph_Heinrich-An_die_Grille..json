{"dta.poem.8455": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "An die Grille.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Kleines Grillchen, und zirpe deinem Traurer,", "tokens": ["Klei\u00b7nes", "Grill\u00b7chen", ",", "und", "zir\u00b7pe", "dei\u00b7nem", "Trau\u00b7rer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Wie dem Schnitterm\u00e4dchen und Schnitterj\u00fcngling,", "tokens": ["Wie", "dem", "Schnit\u00b7ter\u00b7m\u00e4d\u00b7chen", "und", "Schnit\u00b7ter\u00b7j\u00fcng\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "KON", "NN", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Schlummer entgegen;", "tokens": ["Schlum\u00b7mer", "ent\u00b7ge\u00b7gen", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Wenigen linden Schlummer, liebes Grillchen:", "tokens": ["We\u00b7ni\u00b7gen", "lin\u00b7den", "Schlum\u00b7mer", ",", "lie\u00b7bes", "Grill\u00b7chen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dass die Marter in meiner Seele raste,", "tokens": ["Dass", "die", "Mar\u00b7ter", "in", "mei\u00b7ner", "See\u00b7le", "ras\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und im Traumgesichte mein s\u00fcsses M\u00e4dchen", "tokens": ["Und", "im", "Traum\u00b7ge\u00b7sich\u00b7te", "mein", "s\u00fcs\u00b7ses", "M\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "PPOSAT", "ADJA", "NN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Freude mir l\u00e4chle.", "tokens": ["Freu\u00b7de", "mir", "l\u00e4ch\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}