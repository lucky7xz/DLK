{"dta.poem.14287": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Nachkl\u00e4nge .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Dort auf dem Berge da steht ein Baum,", "tokens": ["Dort", "auf", "dem", "Ber\u00b7ge", "da", "steht", "ein", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Drin jubeln die Wander-G\u00e4ste,", "tokens": ["Drin", "ju\u00b7beln", "die", "Wan\u00b7der\u00b7G\u00e4s\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Er aber, m\u00fcde, r\u00fchrt wie im Traum", "tokens": ["Er", "a\u00b7ber", ",", "m\u00fc\u00b7de", ",", "r\u00fchrt", "wie", "im", "Traum"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "$,", "ADJD", "$,", "VVFIN", "KOKOM", "APPRART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Noch einmal Wipfel und Aeste.", "tokens": ["Noch", "ein\u00b7mal", "Wip\u00b7fel", "und", "A\u00b7es\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}}}}}