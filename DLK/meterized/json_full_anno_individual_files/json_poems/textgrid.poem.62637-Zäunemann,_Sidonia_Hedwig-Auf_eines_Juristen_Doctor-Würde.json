t!", "tokens": ["Kommt", "!", "mer\u00b7ket", "was", "die", "The\u00b7mis", "thut", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "VVFIN", "PWS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie schenket ihm den w\u00fcrdgen Hut.", "tokens": ["Sie", "schen\u00b7ket", "ihm", "den", "w\u00fcrd\u00b7gen", "Hut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So herrlich dien ich euren S\u00f6hnen.", "tokens": ["So", "herr\u00b7lich", "di\u00b7en", "ich", "eu\u00b7ren", "S\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Gleich wich der vorge Streit zur\u00fcck,", "tokens": ["Gleich", "wich", "der", "vor\u00b7ge", "Streit", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Sie w\u00fcnschten ihm, da\u00df dieses Gl\u00fcck,", "tokens": ["Sie", "w\u00fcnschten", "ihm", ",", "da\u00df", "die\u00b7ses", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PDAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Ihn noch mit gr\u00f6\u00dfren m\u00f6ge kr\u00f6nen.", "tokens": ["Ihn", "noch", "mit", "gr\u00f6\u00df\u00b7ren", "m\u00f6\u00b7ge", "kr\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "VVINF", "VMFIN", "VVINF", "$."