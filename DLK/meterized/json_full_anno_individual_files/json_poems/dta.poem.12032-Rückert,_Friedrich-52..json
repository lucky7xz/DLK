{"dta.poem.12032": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "52.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer seiner eigenen Vernunft gehorcht allein,", "tokens": ["Wer", "sei\u00b7ner", "ei\u00b7ge\u00b7nen", "Ver\u00b7nunft", "ge\u00b7horcht", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit der gemeinen gar nichts haben will gemein,", "tokens": ["Mit", "der", "ge\u00b7mei\u00b7nen", "gar", "nichts", "ha\u00b7ben", "will", "ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADV", "PIS", "VAINF", "VMFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ist eben so verkehrt wie wer, um andern nur", "tokens": ["Ist", "e\u00b7ben", "so", "ver\u00b7kehrt", "wie", "wer", ",", "um", "an\u00b7dern", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "KOKOM", "PWS", "$,", "KOUI", "ADJA", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es rechtzumachen, l\u00e4\u00dft die eigene Natur.", "tokens": ["Es", "recht\u00b7zu\u00b7ma\u00b7chen", ",", "l\u00e4\u00dft", "die", "ei\u00b7ge\u00b7ne", "Na\u00b7tur", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So wenig kann die Welt gebrauchen jenen Mann,", "tokens": ["So", "we\u00b7nig", "kann", "die", "Welt", "ge\u00b7brau\u00b7chen", "je\u00b7nen", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VMFIN", "ART", "NN", "VVINF", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als dieser in der Welt sich selber brauchen kann.", "tokens": ["Als", "die\u00b7ser", "in", "der", "Welt", "sich", "sel\u00b7ber", "brau\u00b7chen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "APPR", "ART", "NN", "PRF", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nur da ist etwas Recht's, ob Gro\u00dfes oder Kleines,", "tokens": ["Nur", "da", "ist", "et\u00b7was", "Recht's", ",", "ob", "Gro\u00b7\u00dfes", "o\u00b7der", "Klei\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ADV", "NE", "$,", "KOUS", "NN", "KON", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo ein Besondres ist und auch ein Allgemeines.", "tokens": ["Wo", "ein", "Be\u00b7sond\u00b7res", "ist", "und", "auch", "ein", "All\u00b7ge\u00b7mei\u00b7nes", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VAFIN", "KON", "ADV", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}