des Weisen mir:", "tokens": ["Von", "den", "Lip\u00b7pen", "des", "Wei\u00b7sen", "mir", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PPER", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.15": {"line.1": {"text": "Der, wenn hei\u00dfere Glut ihm in dem Busen gl\u00fcht,", "tokens": ["Der", ",", "wenn", "hei\u00b7\u00dfe\u00b7re", "Glut", "ihm", "in", "dem", "Bu\u00b7sen", "gl\u00fcht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Wie die Sonne so hell, w\u00e4rmend wie sie, und hoch,", "tokens": ["Wie", "die", "Son\u00b7ne", "so", "hell", ",", "w\u00e4r\u00b7mend", "wie", "sie", ",", "und", "hoch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "ADJD", "$,", "ADJD", "KOKOM", "PPER", "$,", "KON", "ADJD", "$,"], "meter": "--+--+---+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Mehr als Nektar der G\u00f6tter", "tokens": ["Mehr", "als", "Nek\u00b7tar", "der", "G\u00f6t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "KOKOM", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In die Seele des H\u00f6rers geu\u00dft.", "tokens": ["In", "die", "See\u00b7le", "des", "H\u00f6\u00b7rers", "geu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.16": {"line.1": {"text": "Schone, schone! denn noch gl\u00fchet die Seele mir", "tokens": ["Scho\u00b7ne", ",", "scho\u00b7ne", "!", "denn", "noch", "gl\u00fc\u