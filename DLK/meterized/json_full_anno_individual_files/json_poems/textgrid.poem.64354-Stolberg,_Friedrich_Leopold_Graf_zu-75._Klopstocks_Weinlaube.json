{"textgrid.poem.64354": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "75. Klopstocks Weinlaube", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcberhangend und frisch, wie an den Wellen des", "tokens": ["\u00dc\u00b7berh\u00b7an\u00b7gend", "und", "frisch", ",", "wie", "an", "den", "Wel\u00b7len", "des"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "KON", "ADJD", "$,", "PWAV", "APPR", "ART", "NN", "ART"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Herzerfreuenden Rheins, schwellen die Reben hier", "tokens": ["Her\u00b7zer\u00b7freu\u00b7en\u00b7den", "Rheins", ",", "schwel\u00b7len", "die", "Re\u00b7ben", "hier"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "VVFIN", "ART", "NN", "ADV"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Die der g\u00f6ttliche S\u00e4nger", "tokens": ["Die", "der", "g\u00f6tt\u00b7li\u00b7che", "S\u00e4n\u00b7ger"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "In sein Zimmer geleitet hat;", "tokens": ["In", "sein", "Zim\u00b7mer", "ge\u00b7lei\u00b7tet", "hat", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "VAFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Streben freudig empor an das erw\u00e4rmende", "tokens": ["Stre\u00b7ben", "freu\u00b7dig", "em\u00b7por", "an", "das", "er\u00b7w\u00e4r\u00b7men\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "PTKVZ", "APPR", "ART", "ADJA"], "meter": "+-+--+-+-+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Fenster, kleiden die Wand und das Gesimse des", "tokens": ["Fens\u00b7ter", ",", "klei\u00b7den", "die", "Wand", "und", "das", "Ge\u00b7sim\u00b7se", "des"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "KON", "ART", "NN", "ART"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Gipses, senken gew\u00f6lbt sich", "tokens": ["Gip\u00b7ses", ",", "sen\u00b7ken", "ge\u00b7w\u00f6lbt", "sich"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "VVINF", "VVFIN", "PRF"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Um die Scheitel des Weisen her;", "tokens": ["Um", "die", "Schei\u00b7tel", "des", "Wei\u00b7sen", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Wehren jeglichem Strahl, welchen die h\u00f6here", "tokens": ["Weh\u00b7ren", "jeg\u00b7li\u00b7chem", "Strahl", ",", "wel\u00b7chen", "die", "h\u00f6\u00b7he\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PIAT", "NN", "$,", "PWAT", "ART", "ADJA"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Sonne sendet, nicht dem, welchen err\u00f6tende", "tokens": ["Son\u00b7ne", "sen\u00b7det", ",", "nicht", "dem", ",", "wel\u00b7chen", "er\u00b7r\u00f6\u00b7ten\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "$,", "PTKNEG", "ART", "$,", "PWAT", "ADJA"], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Morgenschimmer verk\u00fcnden,", "tokens": ["Mor\u00b7gen\u00b7schim\u00b7mer", "ver\u00b7k\u00fcn\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Und dem freundlichen Monde nicht.", "tokens": ["Und", "dem", "freund\u00b7li\u00b7chen", "Mon\u00b7de", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Ihre Schatten sind mir wert wie die Schatten der", "tokens": ["Ih\u00b7re", "Schat\u00b7ten", "sind", "mir", "wert", "wie", "die", "Schat\u00b7ten", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADJD", "KOKOM", "ART", "NN", "ART"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Eiche, k\u00fchl wie das Thal, k\u00fchl wie die Felsenkluft,", "tokens": ["Ei\u00b7che", ",", "k\u00fchl", "wie", "das", "Thal", ",", "k\u00fchl", "wie", "die", "Fel\u00b7sen\u00b7kluft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KOKOM", "ART", "NN", "$,", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Wenn der Finger Aurorens", "tokens": ["Wenn", "der", "Fin\u00b7ger", "Au\u00b7ro\u00b7rens"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NE"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Sie mit bebenden Tropfen schm\u00fcckt.", "tokens": ["Sie", "mit", "be\u00b7ben\u00b7den", "Trop\u00b7fen", "schm\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ihre Trauben sind noch gr\u00fcn wie die Ranke, leicht", "tokens": ["Ih\u00b7re", "Trau\u00b7ben", "sind", "noch", "gr\u00fcn", "wie", "die", "Ran\u00b7ke", ",", "leicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "KOKOM", "ART", "NN", "$,", "ADJD"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Wie das gl\u00e4nzende Blatt; dennoch umschwebet sie", "tokens": ["Wie", "das", "gl\u00e4n\u00b7zen\u00b7de", "Blatt", ";", "den\u00b7noch", "um\u00b7schwe\u00b7bet", "sie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "$.", "ADV", "VVFIN", "PPER"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Schon die Freude, und edel", "tokens": ["Schon", "die", "Freu\u00b7de", ",", "und", "e\u00b7del"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "KON", "ADJD"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Ist wie die Freude des Bechers sie.", "tokens": ["Ist", "wie", "die", "Freu\u00b7de", "des", "Be\u00b7chers", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "NN", "ART", "NN", "PPER", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich empfand es: denn hell strahlte der Mond, und hell", "tokens": ["Ich", "emp\u00b7fand", "es", ":", "denn", "hell", "strahl\u00b7te", "der", "Mond", ",", "und", "hell"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$.", "KON", "ADJD", "VVFIN", "ART", "NN", "$,", "KON", "ADJD"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Durch das hangende Laub Jupiters Auge mir;", "tokens": ["Durch", "das", "han\u00b7gen\u00b7de", "Laub", "Ju\u00b7pi\u00b7ters", "Au\u00b7ge", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN", "PPER", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Heller strahlte die Weisheit", "tokens": ["Hel\u00b7ler", "strahl\u00b7te", "die", "Weis\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Von den Lippen des Weisen mir:", "tokens": ["Von", "den", "Lip\u00b7pen", "des", "Wei\u00b7sen", "mir", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PPER", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "Der, wenn hei\u00dfere Glut ihm in dem Busen gl\u00fcht,", "tokens": ["Der", ",", "wenn", "hei\u00b7\u00dfe\u00b7re", "Glut", "ihm", "in", "dem", "Bu\u00b7sen", "gl\u00fcht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Wie die Sonne so hell, w\u00e4rmend wie sie, und hoch,", "tokens": ["Wie", "die", "Son\u00b7ne", "so", "hell", ",", "w\u00e4r\u00b7mend", "wie", "sie", ",", "und", "hoch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "ADJD", "$,", "ADJD", "KOKOM", "PPER", "$,", "KON", "ADJD", "$,"], "meter": "--+--+---+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Mehr als Nektar der G\u00f6tter", "tokens": ["Mehr", "als", "Nek\u00b7tar", "der", "G\u00f6t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "KOKOM", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In die Seele des H\u00f6rers geu\u00dft.", "tokens": ["In", "die", "See\u00b7le", "des", "H\u00f6\u00b7rers", "geu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.8": {"line.1": {"text": "Schone, schone! denn noch gl\u00fchet die Seele mir", "tokens": ["Scho\u00b7ne", ",", "scho\u00b7ne", "!", "denn", "noch", "gl\u00fc\u00b7het", "die", "See\u00b7le", "mir"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "$.", "KON", "ADV", "VVFIN", "ART", "NN", "PPER"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Vom erhabnen Gesang, den du mir gestern sangst!", "tokens": ["Vom", "er\u00b7hab\u00b7nen", "Ge\u00b7sang", ",", "den", "du", "mir", "ge\u00b7stern", "sangst", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PRELS", "PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Tr\u00e4ufle k\u00fchlere Weisheit", "tokens": ["Tr\u00e4uf\u00b7le", "k\u00fch\u00b7le\u00b7re", "Weis\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "In dem Schimmer des Mondes mir!", "tokens": ["In", "dem", "Schim\u00b7mer", "des", "Mon\u00b7des", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PPER", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.9": {"line.1": {"text": "\u00dcberhangend und frisch, wie an den Wellen des", "tokens": ["\u00dc\u00b7berh\u00b7an\u00b7gend", "und", "frisch", ",", "wie", "an", "den", "Wel\u00b7len", "des"], "token_