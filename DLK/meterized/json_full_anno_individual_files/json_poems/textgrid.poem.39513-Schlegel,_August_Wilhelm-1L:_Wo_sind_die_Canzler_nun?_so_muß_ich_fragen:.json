{"textgrid.poem.39513": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wo sind die Canzler nun? so mu\u00df ich fragen:", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo sind die Canzler nun? so mu\u00df ich fragen:", "tokens": ["Wo", "sind", "die", "Canz\u00b7ler", "nun", "?", "so", "mu\u00df", "ich", "fra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADV", "$.", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo sind die R\u00e4the? wo die Schriftgelehrten?", "tokens": ["Wo", "sind", "die", "R\u00e4\u00b7the", "?", "wo", "die", "Schrift\u00b7ge\u00b7lehr\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$.", "PWAV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie, die mit eitler Weisheit sich bewehrten,", "tokens": ["Sie", ",", "die", "mit", "eit\u00b7ler", "Weis\u00b7heit", "sich", "be\u00b7wehr\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPR", "ADJA", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wu\u00dfte keiner T\u00fcchtiges zu sagen.", "tokens": ["Und", "wu\u00df\u00b7te", "kei\u00b7ner", "T\u00fcch\u00b7ti\u00b7ges", "zu", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das Volk, das euch vertraut, ist hart geschlagen.", "tokens": ["Das", "Volk", ",", "das", "euch", "ver\u00b7traut", ",", "ist", "hart", "ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADJD", "$,", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es sind die K\u00fcnste, die sein Herz verkehrten,", "tokens": ["Es", "sind", "die", "K\u00fcns\u00b7te", ",", "die", "sein", "Herz", "ver\u00b7kehr\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die T\u00e4uschereien, so den Zwiespalt mehrten,", "tokens": ["Die", "T\u00e4u\u00b7sche\u00b7rei\u00b7en", ",", "so", "den", "Zwies\u00b7palt", "mehr\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu Schanden worden in des Schreckens Tagen.", "tokens": ["Zu", "Schan\u00b7den", "wor\u00b7den", "in", "des", "Schre\u00b7ckens", "Ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAPP", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die ihr gebr\u00fctet Basilisken-Eier,", "tokens": ["Die", "ihr", "ge\u00b7br\u00fc\u00b7tet", "Ba\u00b7si\u00b7lis\u00b7ken\u00b7Ei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Spinnwebe wirktet, schwanger giengt mit Strohe,", "tokens": ["Spinn\u00b7we\u00b7be", "wirk\u00b7tet", ",", "schwan\u00b7ger", "giengt", "mit", "Stro\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ADJD", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Stoppeln ohne Halm ans Licht geboren:", "tokens": ["Und", "Stop\u00b7peln", "oh\u00b7ne", "Halm", "ans", "Licht", "ge\u00b7bo\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Helft nun! Die Riesenfl\u00fcgel spreizt der Geier,", "tokens": ["Helft", "nun", "!", "Die", "Rie\u00b7sen\u00b7fl\u00fc\u00b7gel", "spreizt", "der", "Gei\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$.", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er facht im Lande der Verw\u00fcstung Lohe,", "tokens": ["Er", "facht", "im", "Lan\u00b7de", "der", "Ver\u00b7w\u00fcs\u00b7tung", "Lo\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und noch ruft Recht und Wahrheit tauben Ohren.", "tokens": ["Und", "noch", "ruft", "Recht", "und", "Wahr\u00b7heit", "tau\u00b7ben", "Oh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "NN", "KON", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wo sind die Canzler nun? so mu\u00df ich fragen:", "tokens": ["Wo", "sind", "die", "Canz\u00b7ler", "nun", "?", "so", "mu\u00df", "ich", "fra\u00b