{"textgrid.poem.67032": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Erinnern", "genre": "verse", "period": "N.A.", "pub_year": 1785, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Viel, viel sind meiner Tage", "tokens": ["Viel", ",", "viel", "sind", "mei\u00b7ner", "Ta\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch S\u00fcnd entweiht gesunken hinab.", "tokens": ["Durch", "S\u00fcnd", "ent\u00b7weiht", "ge\u00b7sun\u00b7ken", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "O, gro\u00dfer Richter, frage", "tokens": ["O", ",", "gro\u00b7\u00dfer", "Rich\u00b7ter", ",", "fra\u00b7ge"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["NE", "$,", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nicht wie, o lasse ihr Grab", "tokens": ["Nicht", "wie", ",", "o", "las\u00b7se", "ihr", "Grab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "KOKOM", "$,", "FM", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Erbarmende Vergessenheit,", "tokens": ["Er\u00b7bar\u00b7men\u00b7de", "Ver\u00b7ges\u00b7sen\u00b7heit", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.6": {"text": "La\u00df, Vater der Barmherzigkeit,", "tokens": ["La\u00df", ",", "Va\u00b7ter", "der", "Barm\u00b7her\u00b7zig\u00b7keit", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVIM