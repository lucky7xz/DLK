{"dta.poem.7388": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Bajazet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Der L\u00f6we und der Leopard", "tokens": ["Der", "L\u00f6\u00b7we", "und", "der", "Le\u00b7o\u00b7pard"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NE", "KON", "ART", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die singen Wettges\u00e4nge,", "tokens": ["Die", "sin\u00b7gen", "Wett\u00b7ge\u00b7s\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gluts\u00e4ulen heben Wettlauf an,", "tokens": ["Glut\u00b7s\u00e4u\u00b7len", "he\u00b7ben", "Wett\u00b7lauf", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "PTKVZ", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und der Samum ihr Herold.", "tokens": ["Und", "der", "Sa\u00b7mum", "ihr", "He\u00b7rold", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "---+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "O Sonne, birg die Stralen!", "tokens": ["O", "Son\u00b7ne", ",", "birg", "die", "Stra\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Was schleicht dort durch den gelben Sand,", "tokens": ["Was", "schleicht", "dort", "durch", "den", "gel\u00b7ben", "Sand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist es ein wunder Schakal?", "tokens": ["Ist", "es", "ein", "wun\u00b7der", "Scha\u00b7kal", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ist es ein gro\u00dfer Vogel wohl,", "tokens": ["Ist", "es", "ein", "gro\u00b7\u00dfer", "Vo\u00b7gel", "wohl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein schwergetroffner Ibis?", "tokens": ["Ein", "schwer\u00b7ge\u00b7troff\u00b7ner", "I\u00b7bis", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "O Sonne, birg die Stralen!", "tokens": ["O", "Son\u00b7ne", ",", "birg", "die", "Stra\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ein wunder Schakal ist es nicht,", "tokens": ["Ein", "wun\u00b7der", "Scha\u00b7kal", "ist", "es", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein schwergetroffner Vogel,", "tokens": ["Kein", "schwer\u00b7ge\u00b7troff\u00b7ner", "Vo\u00b7gel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es ist der m\u00e4cht'ge Bajazet,", "tokens": ["Es", "ist", "der", "m\u00e4cht'\u00b7ge", "Ba\u00b7ja\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der Reichste in Cairo,", "tokens": ["Der", "Reichs\u00b7te", "in", "Cai\u00b7ro", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er, der die dreizehn Segel hat,", "tokens": ["Er", ",", "der", "die", "drei\u00b7zehn", "Se\u00b7gel", "hat", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "CARD", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die reichbeladnen Schiffe,", "tokens": ["Die", "reich\u00b7be\u00b7lad\u00b7nen", "Schif\u00b7fe", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Auf seiner Achsel liegt der Schlauch,", "tokens": ["Auf", "sei\u00b7ner", "Ach\u00b7sel", "liegt", "der", "Schlauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Stab in seiner Rechten.", "tokens": ["Der", "Stab", "in", "sei\u00b7ner", "Rech\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "O Sonne, birg die Stralen!", "tokens": ["O", "Son\u00b7ne", ",", "birg", "die", "Stra\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u201eweh dir, du ungl\u00fccksel'ges Gold,", "tokens": ["\u201e", "weh", "dir", ",", "du", "un\u00b7gl\u00fcck\u00b7sel'\u00b7ges", "Gold", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PPER", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verr\u00e4therisches Silber!", "tokens": ["Ver\u00b7r\u00e4\u00b7the\u00b7ri\u00b7sches", "Sil\u00b7ber", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und weh dir, Hassan, falscher Freund,", "tokens": ["Und", "weh", "dir", ",", "Has\u00b7san", ",", "fal\u00b7scher", "Freund", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "NE", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du ungetreuer Diener!", "tokens": ["Du", "un\u00b7ge\u00b7treu\u00b7er", "Die\u00b7ner", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nahmst in der Nacht die Zelte mir", "tokens": ["Nahmst", "in", "der", "Nacht", "die", "Zel\u00b7te", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und nahmst mir die Kameele.\u201c", "tokens": ["Und", "nahmst", "mir", "die", "Ka\u00b7mee\u00b7le", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "O Sonne, birg die Stralen!", "tokens": ["O", "Son\u00b7ne", ",", "birg", "die", "Stra\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u201ewie einen Leichnam lie\u00dfest mich,", "tokens": ["\u201e", "wie", "ei\u00b7nen", "Leich\u00b7nam", "lie\u00b7\u00dfest", "mich", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Mumien, verdorrte,", "tokens": ["Wie", "Mu\u00b7mi\u00b7en", ",", "ver\u00b7dorr\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PWAV", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie ein verschmachtetes Kameel,", "tokens": ["Wie", "ein", "ver\u00b7schmach\u00b7te\u00b7tes", "Ka\u00b7me\u00b7el", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie ein Gethier der W\u00fcste!", "tokens": ["Wie", "ein", "Ge\u00b7thier", "der", "W\u00fcs\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und gab dir doch das reiche Gut,", "tokens": ["Und", "gab", "dir", "doch", "das", "rei\u00b7che", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die zwanzigtausend Kori.\u201c", "tokens": ["Die", "zwan\u00b7zig\u00b7tau\u00b7send", "Ko\u00b7ri", ".", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "CARD", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "O Sonne, birg die Stralen!", "tokens": ["O", "Son\u00b7ne", ",", "birg", "die", "Stra\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u201eso fluch' ich denn zu sieben Mal,", "tokens": ["\u201e", "so", "fluch'", "ich", "denn", "zu", "sie\u00b7ben", "Mal", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ADV", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und tausend Mal verfluch' ich:", "tokens": ["Und", "tau\u00b7send", "Mal", "ver\u00b7fluch'", "ich", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df dich verschlingen mag das Meer,", "tokens": ["Da\u00df", "dich", "ver\u00b7schlin\u00b7gen", "mag", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein brennend Haus dich t\u00f6dten!", "tokens": ["Dein", "bren\u00b7nend", "Haus", "dich", "t\u00f6d\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df breche dein Gebein der Leu,", "tokens": ["Da\u00df", "bre\u00b7che", "dein", "Ge\u00b7bein", "der", "Leu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dein Blut der Tiger lecke!", "tokens": ["Dein", "Blut", "der", "Ti\u00b7ger", "le\u00b7cke", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der Beduine pl\u00fcndre dich,", "tokens": ["Der", "Be\u00b7du\u00b7i\u00b7ne", "pl\u00fcnd\u00b7re", "dich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Preis gebe dich der W\u00fcste,", "tokens": ["Preis", "ge\u00b7be", "dich", "der", "W\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.9": {"text": "Da\u00df in dem Sande du versiechst,", "tokens": ["Da\u00df", "in", "dem", "San\u00b7de", "du", "ver\u00b7siechst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Verschmachtend \u2014 h\u00fclflos \u2014 irrend!\u201c", "tokens": ["Ver\u00b7schmach\u00b7tend", "h\u00fcl\u00b7flos", "ir\u00b7rend", "!", "\u201c"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["VVPP", "$(", "ADJD", "$(", "VVPP", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "O Sonne, birg die Stralen!", "tokens": ["O", "Son\u00b7ne", ",", "birg", "die", "Stra\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}