{"dta.poem.12816": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als Herr G. S. Hamm  \n  Anno  1708 im  Octobr.  \n de Atheo \u00e8 structura \u03c4\u0223\u0303 \u1f10\u03b3\u03f0\u03b5\u03c6\u03ac\u03bb\u0223 convin-  \n cendo disputi rte,  \n  Jm namen etl. seiner Landes-leute.  \n G. S.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du steigst, Gelehrter Freund! auf die catheder hin,", "tokens": ["Du", "steigst", ",", "Ge\u00b7lehr\u00b7ter", "Freund", "!", "auf", "die", "cat\u00b7he\u00b7der", "hin", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "NN", "$.", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und weisest, da\u00df dein geist, dein aufgeweckter sinn,", "tokens": ["Und", "wei\u00b7sest", ",", "da\u00df", "dein", "geist", ",", "dein", "auf\u00b7ge\u00b7weck\u00b7ter", "sinn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nicht an der erde klebt, noch wie die bl\u00f6den schnecken", "tokens": ["Nicht", "an", "der", "er\u00b7de", "klebt", ",", "noch", "wie", "die", "bl\u00f6\u00b7den", "schne\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "ART", "NN", "VVFIN", "$,", "ADV", "KOKOM", "ART", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das gantze leben durch sich in sich selbst verstecken,", "tokens": ["Das", "gant\u00b7ze", "le\u00b7ben", "durch", "sich", "in", "sich", "selbst", "ver\u00b7ste\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "APPR", "PRF", "APPR", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und niemals zeigen will; Du bist gantz andrer art.", "tokens": ["Und", "nie\u00b7mals", "zei\u00b7gen", "will", ";", "Du", "bist", "gantz", "an\u00b7drer", "art", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVINF", "VMFIN", "$.", "PPER", "VAFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Indessen hat dein flei\u00df doch keine zeit gespart,", "tokens": ["In\u00b7des\u00b7sen", "hat", "dein", "flei\u00df", "doch", "kei\u00b7ne", "zeit", "ge\u00b7spart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Zu lernen, wie man krafft und witz zusammen fassen,", "tokens": ["Zu", "ler\u00b7nen", ",", "wie", "man", "krafft", "und", "witz", "zu\u00b7sam\u00b7men", "fas\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PWAV", "PIS", "NN", "KON", "VVIMP", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und die gedancken nicht aus ihren schrancken lassen;", "tokens": ["Und", "die", "ge\u00b7dan\u00b7cken", "nicht", "aus", "ih\u00b7ren", "schran\u00b7cken", "las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKNEG", "APPR", "PPOSAT", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nein, sondern in sich selbst offt stille stehen mu\u00df.", "tokens": ["Nein", ",", "son\u00b7dern", "in", "sich", "selbst", "offt", "stil\u00b7le", "ste\u00b7hen", "mu\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KON", "APPR", "PRF", "ADV", "ADV", "ADJA", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Du kennst der tugend bahn, und ehrst der weisheit schlu\u00df.", "tokens": ["Du", "kennst", "der", "tu\u00b7gend", "bahn", ",", "und", "ehrst", "der", "weis\u00b7heit", "schlu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN", "$,", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Niemals zu hause seyn, sich hin und her zerstreuen,", "tokens": ["Nie\u00b7mals", "zu", "hau\u00b7se", "seyn", ",", "sich", "hin", "und", "her", "zer\u00b7streu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VAINF", "$,", "PRF", "PTKVZ", "KON", "ADV", "VVINF", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.12": {"text": "Die still\u2019 und einsamkeit, als einen kercker, scheuen,", "tokens": ["Die", "still'", "und", "ein\u00b7sam\u00b7keit", ",", "als", "ei\u00b7nen", "ker\u00b7cker", ",", "scheu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJD", "KON", "NN", "$,", "KOUS", "ART", "NN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Verr\u00e4th den unverstand; Wer klug studiren will,", "tokens": ["Ver\u00b7r\u00e4\u00b7th", "den", "un\u00b7ver\u00b7stand", ";", "Wer", "klug", "stu\u00b7di\u00b7ren", "will", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "PWS", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Denckt allem, was er liest und h\u00f6ret, in der still", "tokens": ["Denckt", "al\u00b7lem", ",", "was", "er", "liest", "und", "h\u00f6\u00b7ret", ",", "in", "der", "still"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PIS", "$,", "PWS", "PPER", "VVFIN", "KON", "VVFIN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und recht bed\u00e4chtig nach: Er folget dem magnete,", "tokens": ["Und", "recht", "be\u00b7d\u00e4ch\u00b7tig", "nach", ":", "Er", "fol\u00b7get", "dem", "mag\u00b7ne\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "PTKVZ", "$.", "PPER", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Den kein umirrend licht, kein schimmernder planete", "tokens": ["Den", "kein", "um\u00b7ir\u00b7rend", "licht", ",", "kein", "schim\u00b7mern\u00b7der", "pla\u00b7ne\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PIAT", "ADJD", "NN", "$,", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Von seinem pole zieht. Dis ist ein sch\u00f6ner ruhm:", "tokens": ["Von", "sei\u00b7nem", "po\u00b7le", "zieht", ".", "Dis", "ist", "ein", "sch\u00f6\u00b7ner", "ruhm", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$.", "PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Dis ist, Gelehrter Hamm! dein wahres eigenthum,", "tokens": ["Dis", "ist", ",", "Ge\u00b7lehr\u00b7ter", "Hamm", "!", "dein", "wah\u00b7res", "ei\u00b7gen\u00b7thum", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "NN", "NE", "$.", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.19": {"text": "Und soll es auch stets seyn. Man mag sich wol ergetzen;", "tokens": ["Und", "soll", "es", "auch", "stets", "seyn", ".", "Man", "mag", "sich", "wol", "er\u00b7get\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ADV", "VAINF", "$.", "PIS", "VMFIN", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Denn GOtt lie\u00df zu dem tag auch nacht und abend setzen.", "tokens": ["Denn", "Gott", "lie\u00df", "zu", "dem", "tag", "auch", "nacht", "und", "a\u00b7bend", "set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "ART", "NN", "ADV", "NN", "KON", "ADJD", "VVINF", "$."], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.21": {"text": "Wer niemals ruhen will, der schreitet allzuweit,", "tokens": ["Wer", "nie\u00b7mals", "ru\u00b7hen", "will", ",", "der", "schrei\u00b7tet", "all\u00b7zu\u00b7weit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVINF", "VMFIN", "$,", "PRELS", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Beleidigt die natur, und st\u00fcrtzet vor der zeit", "tokens": ["Be\u00b7lei\u00b7digt", "die", "na\u00b7tur", ",", "und", "st\u00fcrt\u00b7zet", "vor", "der", "zeit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "In eine todten-grufft. Best\u00e4ndig m\u00fc\u00dfig gehen", "tokens": ["In", "ei\u00b7ne", "tod\u00b7ten\u00b7grufft", ".", "Be\u00b7st\u00e4n\u00b7dig", "m\u00fc\u00b7\u00dfig", "ge\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$.", "ADJD", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Hei\u00dft gar nichts n\u00fctze seyn: Man mu\u00df im mittel stehen.", "tokens": ["Hei\u00dft", "gar", "nichts", "n\u00fct\u00b7ze", "seyn", ":", "Man", "mu\u00df", "im", "mit\u00b7tel", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIS", "VVFIN", "VAINF", "$.", "PIS", "VMFIN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wer das studiret hat, der hat sehr viel gethan.", "tokens": ["Wer", "das", "stu\u00b7di\u00b7ret", "hat", ",", "der", "hat", "sehr", "viel", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDS", "VVFIN", "VAFIN", "$,", "PRELS", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Das aber, Werther Hamm! das ist die tugend-bahn,", "tokens": ["Das", "a\u00b7ber", ",", "Wert\u00b7her", "Hamm", "!", "das", "ist", "die", "tu\u00b7gen\u00b7dbahn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "$,", "NE", "NE", "$.", "PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Davon kein eitler zug dich auf die seite wendet:", "tokens": ["Da\u00b7von", "kein", "eit\u00b7ler", "zug", "dich", "auf", "die", "sei\u00b7te", "wen\u00b7det", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIAT", "ADJA", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Dis wei\u00df dein Regensburg, so dich hieher gesendet.", "tokens": ["Dis", "wei\u00df", "dein", "Re\u00b7gens\u00b7burg", ",", "so", "dich", "hie\u00b7her", "ge\u00b7sen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$,", "ADV", "PRF", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ursinus, und wer mehr in dir den grund gelegt,", "tokens": ["Ur\u00b7si\u00b7nus", ",", "und", "wer", "mehr", "in", "dir", "den", "grund", "ge\u00b7legt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "PWS", "ADV", "APPR", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.30": {"text": "Bekr\u00e4fftiget dein lob, das unsre feder regt.", "tokens": ["Be\u00b7kr\u00e4ff\u00b7ti\u00b7get", "dein", "lob", ",", "das", "uns\u00b7re", "fe\u00b7der", "reg\u00b7t."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "abbreviation"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "PRELS", "PPOSAT", "NN", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Der grundgelehrte F\u00f6rtsch, aus dessen g\u00f6ldnem munde", "tokens": ["Der", "grund\u00b7ge\u00b7lehr\u00b7te", "F\u00f6rtsch", ",", "aus", "des\u00b7sen", "g\u00f6ld\u00b7nem", "mun\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Du GOttes weisheit lernst: Bey dem du keine stunde", "tokens": ["Du", "Got\u00b7tes", "weis\u00b7heit", "lernst", ":", "Bey", "dem", "du", "kei\u00b7ne", "stun\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "NN", "VVFIN", "$.", "APPR", "PRELS", "PPER", "PIAT", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Vergebens angewandt: f\u00e4llt uns hierinnen bey,", "tokens": ["Ver\u00b7ge\u00b7bens", "an\u00b7ge\u00b7wandt", ":", "f\u00e4llt", "uns", "hie\u00b7rin\u00b7nen", "bey", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$.", "VVFIN", "PPER", "PAV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Buddeus, dem die pest der schn\u00f6den schmeicheley", "tokens": ["Bud\u00b7deus", ",", "dem", "die", "pest", "der", "schn\u00f6\u00b7den", "schmei\u00b7che\u00b7ley"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "ART", "VVFIN", "ART", "ADJA", "NN"], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.35": {"text": "So sehr zuwider ist, als er die wahrheit liebet:", "tokens": ["So", "sehr", "zu\u00b7wi\u00b7der", "ist", ",", "als", "er", "die", "wahr\u00b7heit", "lie\u00b7bet", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VAFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Der alles, was er lehrt, auch thut und selber \u00fcbet:", "tokens": ["Der", "al\u00b7les", ",", "was", "er", "lehrt", ",", "auch", "thut", "und", "sel\u00b7ber", "\u00fc\u00b7bet", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$,", "PWS", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "KON", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Buddeus, dessen treu und lehre wohl besteht,", "tokens": ["Bud\u00b7deus", ",", "des\u00b7sen", "treu", "und", "leh\u00b7re", "wohl", "be\u00b7steht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELAT", "ADJD", "KON", "VVFIN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.38": {"text": "So sehr auch ha\u00df und neid sich wider ihn erh\u00f6ht,", "tokens": ["So", "sehr", "auch", "ha\u00df", "und", "neid", "sich", "wi\u00b7der", "ihn", "er\u00b7h\u00f6ht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVIMP", "KON", "VVFIN", "PRF", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Kennt deinen klugen flei\u00df; Doch da\u00df ihn alle kennen,", "tokens": ["Kennt", "dei\u00b7nen", "klu\u00b7gen", "flei\u00df", ";", "Doch", "da\u00df", "ihn", "al\u00b7le", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$.", "KON", "KOUS", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"t