", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "So schl\u00e4get sie dein witz mit ihren eignen waffen:", "tokens": ["So", "schl\u00e4\u00b7get", "sie", "dein", "witz", "mit", "ih\u00b7ren", "eig\u00b7nen", "waf\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "So kan sie die vernunfft durch deinen kiel bestraffen,", "tokens": ["So", "kan", "sie", "die", "ver\u00b7nunfft", "durch", "dei\u00b7nen", "kiel", "be\u00b7straf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.65": {"text": "Doch auch durch deinen mund: Und also weisest du,", "tokens": ["Doch", "auch", "durch", "dei\u00b7nen", "mund", ":", "Und", "al\u00b7so", "wei\u00b7sest", "du", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "$.", "KON", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.66": {"text": "Was du mit beyden kanst. Wir w\u00fcnschen dir darzu", "tokens": ["Was", "du", "mit", "bey\u00b7den", "kanst", ".", "Wir", "w\u00fcn\u00b7schen", "dir", "dar\u00b7zu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "PIAT", "NN", "$.", "PPER", "VVFIN", "PPER", "PAV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.67": {"text": "Den segen, den ohndem der H\u00f6chste gerne giebet,", "tokens": ["Den", "se\u00b7gen", ",", "den", "ohn\u00b7dem", "der", "H\u00f6chs\u00b7te", "ger\u00b7ne", "gie\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.68": {"text": "Dem, der ihn recht erkennt, und seinen namen liebet,", "tokens": ["Dem", ",", "der", "ihn", "recht", "er\u00b7kennt", ",", "und", "sei\u00b7nen", "na\u00b7men", "lie\u00b7bet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.69": {"text": "Du aber liebest ihn. Wie tieff du ihn erkannt,", "tokens": ["Du", "a\u00b7ber", "lie\u00b7best", "ihn", ".", "Wie", "tieff", "du", "ihn", "er\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "$.", "PWAV", "ADJD", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.70": {"text": "Zeigt itzund hand und mund, und wird das vaterland,", "tokens": ["Zeigt", "it\u00b7zund", "hand", "und", "mund", ",", "und", "wird", "das", "va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "KON", "NN", "$,", "KON", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.71": {"text": "Wenn du es dermaleinst durch reine GOttes-lehren", "tokens": ["Wenn", "du", "es", "der\u00b7ma\u00b7leinst", "durch", "rei\u00b7ne", "Got\u00b7tes\u00b7leh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "