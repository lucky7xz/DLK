{"textgrid.poem.42086": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "5. Cereus Flagelliformis", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Z\u00fcchtigung: dies aber bist Du auch,", "tokens": ["Die", "Z\u00fcch\u00b7ti\u00b7gung", ":", "dies", "a\u00b7ber", "bist", "Du", "auch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PDS", "ADV", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du bist die harte, die umdornte Rute", "tokens": ["Du", "bist", "die", "har\u00b7te", ",", "die", "um\u00b7dorn\u00b7te", "Ru\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aus einem bitteren verfluchten Strauch;", "tokens": ["Aus", "ei\u00b7nem", "bit\u00b7te\u00b7ren", "ver\u00b7fluch\u00b7ten", "Strauch", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wo Du auch triffst, da triffst Du tief im Blute.", "tokens": ["Wo", "Du", "auch", "triffst", ",", "da", "triffst", "Du", "tief", "im", "Blu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du bist was schmerzt. Nichts auf der Welt schmerzt mehr,", "tokens": ["Du", "bist", "was", "schmerzt", ".", "Nichts", "auf", "der", "Welt", "schmerzt", "mehr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "VVFIN", "$.", "PIS", "APPR", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Kein Schmerzendes ist ohne Dich. Bewegst", "tokens": ["Kein", "Schmer\u00b7zen\u00b7des", "ist", "oh\u00b7ne", "Dich", ".", "Be\u00b7wegst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PIAT", "NN", "VAFIN", "APPR", "PPER", "$.", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du Deine Hand, schon \u00fcberh\u00e4ufst Du schwer", "tokens": ["Du", "Dei\u00b7ne", "Hand", ",", "schon", "\u00fc\u00b7berh\u00b7\u00e4ufst", "Du", "schwer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit Schmerz der Leidenschaft. Du schl\u00e4gst", "tokens": ["Mit", "Schmerz", "der", "Lei\u00b7den\u00b7schaft", ".", "Du", "schl\u00e4gst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "ART", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mit Widerhaken tausendfach verschlingend", "tokens": ["Mit", "Wi\u00b7der\u00b7ha\u00b7ken", "tau\u00b7send\u00b7fach", "ver\u00b7schlin\u00b7gend"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ins Fleisch Dich ein, da\u00df, ob Du dort ob hier", "tokens": ["Ins", "Fleisch", "Dich", "ein", ",", "da\u00df", ",", "ob", "Du", "dort", "ob", "hier"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPER", "PTKVZ", "$,", "KOUS", "$,", "KOUS", "PPER", "ADV", "KOUS", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Verweilst, es schmerzt. Und zogen einst sie singend", "tokens": ["Ver\u00b7weilst", ",", "es", "schmerzt", ".", "Und", "zo\u00b7gen", "einst", "sie", "sin\u00b7gend"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "$.", "KON", "VVFIN", "ADV", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Zu ihrem Gott, die blutige Gei\u00dfel schwingend,", "tokens": ["Zu", "ih\u00b7rem", "Gott", ",", "die", "blu\u00b7ti\u00b7ge", "Gei\u00b7\u00dfel", "schwin\u00b7gend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So treibst Du mich, Unruhe \u00fcber mir,", "tokens": ["So", "treibst", "Du", "mich", ",", "Un\u00b7ru\u00b7he", "\u00fc\u00b7ber", "mir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "NN", "APPR", "PPER", "$,"], "meter": "-+-+++-+-+", "measure": "zehnsilber"}, "line.3": {"text": "Nun aus mir fort: ins Unerreichte zwingend.", "tokens": ["Nun", "aus", "mir", "fort", ":", "ins", "Un\u00b7er\u00b7reich\u00b7te", "zwin\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "PTKVZ", "$.", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Die Z\u00fcchtigung: dies aber bist Du auch,", "tokens": ["Die", "Z\u00fcch\u00b7ti\u00b7gung", ":", "dies", "a\u00b7ber", "bist", "Du", "auch", ","], "token_info": ["word", "word", "punct", "word", "word", "word"