{"textgrid.poem.44262": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "[zeuch aus, gefangne Seele]", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zeuch aus, gefangne Seele,", "tokens": ["Zeuch", "aus", ",", "ge\u00b7fang\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Weil Stahl und Kercker bricht,", "tokens": ["Weil", "Stahl", "und", "Ker\u00b7cker", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des Leibes Jammerh\u00f6hle", "tokens": ["Des", "Lei\u00b7bes", "Jam\u00b7mer\u00b7h\u00f6h\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hemmt deine Freyheit nicht;", "tokens": ["Hemmt", "dei\u00b7ne", "Frey\u00b7heit", "nicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das Grab, mein Ruhek\u00fc\u00dfen,", "tokens": ["Das", "Grab", ",", "mein", "Ru\u00b7he\u00b7k\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Begr\u00e4bt die Sclaverey;", "tokens": ["Be\u00b7gr\u00e4bt", "die", "Scla\u00b7ve\u00b7rey", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da nun der Strick zerri\u00dfen,", "tokens": ["Da", "nun", "der", "Strick", "zer\u00b7ri\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So wird der Vogel frey.", "tokens": ["So", "wird", "der", "Vo\u00b7gel", "frey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du lezter meiner Tage,", "tokens": ["Du", "lez\u00b7ter", "mei\u00b7ner", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie sehnlich und wie oft", "tokens": ["Wie", "sehn\u00b7lich", "und", "wie", "oft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "KON", "PWAV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hab ich bey Angst und Plage", "tokens": ["Hab", "ich", "bey", "Angst", "und", "Pla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nicht auf dein Licht gehoft!", "tokens": ["Nicht", "auf", "dein", "Licht", "ge\u00b7hoft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich rief im Ungl\u00fcckswetter:", "tokens": ["Ich", "rief", "im", "Un\u00b7gl\u00fccks\u00b7wet\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ach, Herr, erbarme dich,", "tokens": ["Ach", ",", "Herr", ",", "er\u00b7bar\u00b7me", "dich", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dein Arm sey mein Erretter,", "tokens": ["Dein", "Arm", "sey", "mein", "Er\u00b7ret\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wo nicht, so t\u00f6dte mich!", "tokens": ["Wo", "nicht", ",", "so", "t\u00f6d\u00b7te", "mich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "$,", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Hofnung sieht ihr Ende,", "tokens": ["Die", "Hof\u00b7nung", "sieht", "ihr", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Wuntsch erreicht sein Ziel;", "tokens": ["Mein", "Wunt\u00b7sch", "er\u00b7reicht", "sein", "Ziel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Verwerft, entbundnen H\u00e4nde,", "tokens": ["Ver\u00b7werft", ",", "ent\u00b7bund\u00b7nen", "H\u00e4n\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was euch beschwerlich fiel!", "tokens": ["Was", "euch", "be\u00b7schwer\u00b7lich", "fiel", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Folterbanck der Glieder", "tokens": ["Die", "Fol\u00b7ter\u00b7banck", "der", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zerschmeist der Leichenstein;", "tokens": ["Zer\u00b7schmeist", "der", "Lei\u00b7chen\u00b7stein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Drum m\u00fcst ihr Sterbelieder", "tokens": ["Drum", "m\u00fcst", "ihr", "Ster\u00b7be\u00b7lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mein Halleluja seyn.", "tokens": ["Mein", "Hal\u00b7le\u00b7lu\u00b7ja", "seyn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Nach so viel Marterwochen", "tokens": ["Nach", "so", "viel", "Mar\u00b7ter\u00b7wo\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "PIAT", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Erscheint das Jubeljahr,", "tokens": ["Er\u00b7scheint", "das", "Ju\u00b7bel\u00b7jahr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gott hat das Holz zerbrochen,", "tokens": ["Gott", "hat", "das", "Holz", "zer\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das mir manch Creuz gebahr.", "tokens": ["Das", "mir", "manch", "Creuz", "ge\u00b7bahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mein Fu\u00df verl\u00e4st mit Freuden", "tokens": ["Mein", "Fu\u00df", "ver\u00b7l\u00e4st", "mit", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den Schauplaz dieser Welt,", "tokens": ["Den", "Schau\u00b7plaz", "die\u00b7ser", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wo ich nebst meinem Leiden", "tokens": ["Wo", "ich", "nebst", "mei\u00b7nem", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Rahel vorgestellt.", "tokens": ["Die", "Ra\u00b7hel", "vor\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Mein Ohr vernimmt das Zeichen,", "tokens": ["Mein", "Ohr", "ver\u00b7nimmt", "das", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So mir zu Schife ruft;", "tokens": ["So", "mir", "zu", "Schi\u00b7fe", "ruft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Last nun die Seegel streichen,", "tokens": ["Last", "nun", "die", "See\u00b7gel", "strei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Der Hafen meiner Gruft", "tokens": ["Der", "Ha\u00b7fen", "mei\u00b7ner", "Gruft"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Macht, da\u00df ich nicht mehr strande,", "tokens": ["Macht", ",", "da\u00df", "ich", "nicht", "mehr", "stran\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Der Himmel wird mein Haus;", "tokens": ["Der", "Him\u00b7mel", "wird", "mein", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wohlan, wir sind am Lande,", "tokens": ["Wo\u00b7hlan", ",", "wir", "sind", "am", "Lan\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Steig, m\u00fcder Geist, steig aus!", "tokens": ["Steig", ",", "m\u00fc\u00b7der", "Geist", ",", "steig", "aus", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Du H\u00e4lfte meines Herzens,", "tokens": ["Du", "H\u00e4lf\u00b7te", "mei\u00b7nes", "Her\u00b7zens", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Ehschaz, gute Nacht,", "tokens": ["Mein", "Eh\u00b7schaz", ",", "gu\u00b7te", "Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vergi\u00df des herben Schmerzens,", "tokens": ["Ver\u00b7gi\u00df", "des", "her\u00b7ben", "Schmer\u00b7zens", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der dich zum Wittwer macht;", "tokens": ["Der", "dich", "zum", "Witt\u00b7wer", "macht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das Feuer unsrer Liebe", "tokens": ["Das", "Feu\u00b7er", "uns\u00b7rer", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Verl\u00f6scht kein Thr\u00e4nengu\u00df,", "tokens": ["Ver\u00b7l\u00f6scht", "kein", "Thr\u00e4\u00b7nen\u00b7gu\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Jezt reicht dir deine Riebe", "tokens": ["Jezt", "reicht", "dir", "dei\u00b7ne", "Rie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den lezten Abschiedsku\u00df.", "tokens": ["Den", "lez\u00b7ten", "Ab\u00b7schieds\u00b7ku\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ihr mutterlosen Erben,", "tokens": ["Ihr", "mut\u00b7ter\u00b7lo\u00b7sen", "Er\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die meine Brust ges\u00e4ugt", "tokens": ["Die", "mei\u00b7ne", "Brust", "ge\u00b7s\u00e4ugt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und die mein fr\u00fches Sterben", "tokens": ["Und", "die", "mein", "fr\u00fc\u00b7hes", "Ster\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Fast zu der Erde beugt,", "tokens": ["Fast", "zu", "der", "Er\u00b7de", "beugt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Euch las ich meinen Seegen;", "tokens": ["Euch", "las", "ich", "mei\u00b7nen", "See\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wo er bekleiben soll,", "tokens": ["Wo", "er", "be\u00b7klei\u00b7ben", "soll", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "So geht auf Gottes Wegen;", "tokens": ["So", "geht", "auf", "Got\u00b7tes", "We\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ihr Kinder, lebet wohl!", "tokens": ["Ihr", "Kin\u00b7der", ",", "le\u00b7bet", "wohl", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Du, dem ich, weil ich lebe,", "tokens": ["Du", ",", "dem", "ich", ",", "weil", "ich", "le\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mich selber schuldig bin,", "tokens": ["Mich", "sel\u00b7ber", "schul\u00b7dig", "bin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nimm auch von deiner Rebe", "tokens": ["Nimm", "auch", "von", "dei\u00b7ner", "Re\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Pflicht der Ehrfurcht hin.", "tokens": ["Die", "Pflicht", "der", "Ehr\u00b7furcht", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dein v\u00e4terliches Sorgen", "tokens": ["Dein", "v\u00e4\u00b7ter\u00b7li\u00b7ches", "Sor\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mu\u00df mir das L\u00f6segeld", "tokens": ["Mu\u00df", "mir", "das", "L\u00f6\u00b7se\u00b7geld"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Bis auf den Tag noch borgen,", "tokens": ["Bis", "auf", "den", "Tag", "noch", "bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der allen Rechnung h\u00e4lt.", "tokens": ["Der", "al\u00b7len", "Rech\u00b7nung", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ihr Freunde vom Gebl\u00fcte,", "tokens": ["Ihr", "Freun\u00b7de", "vom", "Ge\u00b7bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die ihr mich Schwester nennt,", "tokens": ["Die", "ihr", "mich", "Schwes\u00b7ter", "nennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ertragt auf meine Bitte,", "tokens": ["Er\u00b7tragt", "auf", "mei\u00b7ne", "Bit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was ihr nicht \u00e4ndern k\u00f6nt.", "tokens": ["Was", "ihr", "nicht", "\u00e4n\u00b7dern", "k\u00f6nt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Erwegt bey meinem Falle,", "tokens": ["Er\u00b7wegt", "bey", "mei\u00b7nem", "Fal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der H\u00f6chste hats gethan.", "tokens": ["Der", "H\u00f6chs\u00b7te", "hats", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ihr folgt mir endlich alle;", "tokens": ["Ihr", "folgt", "mir", "end\u00b7lich", "al\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PIS", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Genung, ich geh voran.", "tokens": ["Ge\u00b7nung", ",", "ich", "geh", "vo\u00b7ran", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Zeuch aus, gefangne Seele,", "tokens": ["Zeuch", "aus", ",", "ge\u00b7fang\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Weil Stahl und Kercker bricht,", "tokens": ["Weil", "Stahl", "und", "Ker\u00b7cker", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des Leibes Jammerh\u00f6hle", "tokens": ["Des", "Lei\u00b7bes", "Jam\u00b7mer\u00b7h\u00f6h\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hemmt deine Freyheit nicht;", "tokens": ["Hemmt", "dei\u00b7ne", "Frey\u00b7heit", "nicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das Grab, mein Ruhek\u00fc\u00dfen,", "tokens": ["Das", "Grab", ",", "mein", "Ru\u00b7he\u00b7k\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Begr\u00e4bt die Sclaverey;", "tokens": ["Be\u00b7gr\u00e4bt", "die", "Scla\u00b7ve\u00b7rey", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da nun der Strick zerri\u00dfen,", "tokens": ["Da", "nun", "der", "Strick", "zer\u00b7ri\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So wird der Vogel frey.", "tokens": ["So", "wird", "der", "Vo\u00b7gel", "frey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Du lezter meiner Tage,", "tokens": ["Du", "lez\u00b7ter", "mei\u00b7ner", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie sehnlich und wie oft", "tokens": ["Wie", "sehn\u00b7lich", "und", "wie", "oft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "KON", "PWAV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hab ich bey Angst und Plage", "tokens": ["Hab", "ich", "bey", "Angst", "und", "Pla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nicht auf dein Licht gehoft!", "tokens": ["Nicht", "auf", "dein", "Licht", "ge\u00b7hoft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich rief im Ungl\u00fcckswetter:", "tokens": ["Ich", "rief", "im", "Un\u00b7gl\u00fccks\u00b7wet\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ach, Herr, erbarme dich,", "tokens": ["Ach", ",", "Herr", ",", "er\u00b7bar\u00b7me", "dich", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dein Arm sey mein Erretter,", "tokens": ["Dein", "Arm", "sey", "mein", "Er\u00b7ret\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wo nicht, so t\u00f6dte mich!", "tokens": ["Wo", "nicht", ",", "so", "t\u00f6d\u00b7te", "mich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "$,", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Die Hofnung sieht ihr Ende,", "tokens": ["Die", "Hof\u00b7nung", "sieht", "ihr", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Wuntsch erreicht sein Ziel;", "tokens": ["Mein", "Wunt\u00b7sch", "er\u00b7reicht", "sein", "Ziel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Verwerft, entbundnen H\u00e4nde,", "tokens": ["Ver\u00b7werft", ",", "ent\u00b7bund\u00b7nen", "H\u00e4n\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was euch beschwerlich fiel!", "tokens": ["Was", "euch", "be\u00b7schwer\u00b7lich", "fiel", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Folterbanck der Glieder", "tokens": ["Die", "Fol\u00b7ter\u00b7banck", "der", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zerschmeist der Leichenstein;", "tokens": ["Zer\u00b7schmeist", "der", "Lei\u00b7chen\u00b7stein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Drum m\u00fcst ihr Sterbelieder", "tokens": ["Drum", "m\u00fcst", "ihr", "Ster\u00b7be\u00b7lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mein Halleluja seyn.", "tokens": ["Mein", "Hal\u00b7le\u00b7lu\u00b7ja", "seyn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Nach so viel Marterwochen", "tokens": ["Nach", "so", "viel", "Mar\u00b7ter\u00b7wo\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "PIAT", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Erscheint das Jubeljahr,", "tokens": ["Er\u00b7scheint", "das", "Ju\u00b7bel\u00b7jahr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gott hat das Holz zerbrochen,", "tokens": ["Gott", "hat", "das", "Holz", "zer\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das mir manch Creuz gebahr.", "tokens": ["Das", "mir", "manch", "Creuz", "ge\u00b7bahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mein Fu\u00df verl\u00e4st mit Freuden", "tokens": ["Mein", "Fu\u00df", "ver\u00b7l\u00e4st", "mit", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den Schauplaz dieser Welt,", "tokens": ["Den", "Schau\u00b7plaz", "die\u00b7ser", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wo ich nebst meinem Leiden", "tokens": ["Wo", "ich", "nebst", "mei\u00b7nem", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Rahel vorgestellt.", "tokens": ["Die", "Ra\u00b7hel", "vor\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Mein Ohr vernimmt das Zeichen,", "tokens": ["Mein", "Ohr", "ver\u00b7nimmt", "das", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So mir zu Schife ruft;", "tokens": ["So", "mir", "zu", "Schi\u00b7fe", "ruft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Last nun die Seegel streichen,", "tokens": ["Last", "nun", "die", "See\u00b7gel", "strei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Der Hafen meiner Gruft", "tokens": ["Der", "Ha\u00b7fen", "mei\u00b7ner", "Gruft"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Macht, da\u00df ich nicht mehr strande,", "tokens": ["Macht", ",", "da\u00df", "ich", "nicht", "mehr", "stran\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Der Himmel wird mein Haus;", "tokens": ["Der", "Him\u00b7mel", "wird", "mein", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wohlan, wir sind am Lande,", "tokens": ["Wo\u00b7hlan", ",", "wir", "sind", "am", "Lan\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Steig, m\u00fcder Geist, steig aus!", "tokens": ["Steig", ",", "m\u00fc\u00b7der", "Geist", ",", "steig", "aus", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Du H\u00e4lfte meines Herzens,", "tokens": ["Du", "H\u00e4lf\u00b7te", "mei\u00b7nes", "Her\u00b7zens", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Ehschaz, gute Nacht,", "tokens": ["Mein", "Eh\u00b7schaz", ",", "gu\u00b7te", "Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vergi\u00df des herben Schmerzens,", "tokens": ["Ver\u00b7gi\u00df", "des", "her\u00b7ben", "Schmer\u00b7zens", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der dich zum Wittwer macht;", "tokens": ["Der", "dich", "zum", "Witt\u00b7wer", "macht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das Feuer unsrer Liebe", "tokens": ["Das", "Feu\u00b7er", "uns\u00b7rer", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Verl\u00f6scht kein Thr\u00e4nengu\u00df,", "tokens": ["Ver\u00b7l\u00f6scht", "kein", "Thr\u00e4\u00b7nen\u00b7gu\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Jezt reicht dir deine Riebe", "tokens": ["Jezt", "reicht", "dir", "dei\u00b7ne", "Rie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den lezten Abschiedsku\u00df.", "tokens": ["Den", "lez\u00b7ten", "Ab\u00b7schieds\u00b7ku\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Ihr mutterlosen Erben,", "tokens": ["Ihr", "mut\u00b7ter\u00b7lo\u00b7sen", "Er\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die meine Brust ges\u00e4ugt", "tokens": ["Die", "mei\u00b7ne", "Brust", "ge\u00b7s\u00e4ugt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und die mein fr\u00fches Sterben", "tokens": ["Und", "die", "mein", "fr\u00fc\u00b7hes", "Ster\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Fast zu der Erde beugt,", "tokens": ["Fast", "zu", "der", "Er\u00b7de", "beugt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Euch las ich meinen Seegen;", "tokens": ["Euch", "las", "ich", "mei\u00b7nen", "See\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wo er bekleiben soll,", "tokens": ["Wo", "er", "be\u00b7klei\u00b7ben", "soll", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "So geht auf Gottes Wegen;", "tokens": ["So", "geht", "auf", "Got\u00b7tes", "We\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ihr Kinder, lebet wohl!", "tokens": ["Ihr", "Kin\u00b7der", ",", "le\u00b7bet", "wohl", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Du, dem ich, weil ich lebe,", "tokens": ["Du", ",", "dem", "ich", ",", "weil", "ich", "le\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mich selber schuldig bin,", "tokens": ["Mich", "sel\u00b7ber", "schul\u00b7dig", "bin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nimm auch von deiner Rebe", "tokens": ["Nimm", "auch", "von", "dei\u00b7ner", "Re\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Pflicht der Ehrfurcht hin.", "tokens": ["Die", "Pflicht", "der", "Ehr\u00b7furcht", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dein v\u00e4terliches Sorgen", "tokens": ["Dein", "v\u00e4\u00b7ter\u00b7li\u00b7ches", "Sor\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mu\u00df mir das L\u00f6segeld", "tokens": ["Mu\u00df", "mir", "das", "L\u00f6\u00b7se\u00b7geld"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Bis auf den Tag noch borgen,", "tokens": ["Bis", "auf", "den", "Tag", "noch", "bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der allen Rechnung h\u00e4lt.", "tokens": ["Der", "al\u00b7len", "Rech\u00b7nung", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Ihr Freunde vom Gebl\u00fcte,", "tokens": ["Ihr", "Freun\u00b7de", "vom", "Ge\u00b7bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die ihr mich Schwester nennt,", "tokens": ["Die", "ihr", "mich", "Schwes\u00b7ter", "nennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ertragt auf meine Bitte,", "tokens": ["Er\u00b7tragt", "auf", "mei\u00b7ne", "Bit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was ihr nicht \u00e4ndern k\u00f6nt.", "tokens": ["Was", "ihr", "nicht", "\u00e4n\u00b7dern", "k\u00f6nt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Erwegt bey meinem Falle,", "tokens": ["Er\u00b7wegt", "bey", "mei\u00b7nem", "Fal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der H\u00f6chste hats gethan.", "tokens": ["Der", "H\u00f6chs\u00b7te", "hats", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ihr folgt mir endlich alle;", "tokens": ["Ihr", "folgt", "mir", "end\u00b7lich", "al\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PIS", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Genung, ich geh voran.", "tokens": ["Ge\u00b7nung", ",", "ich", "geh", "vo\u00b7ran", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}