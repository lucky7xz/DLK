, "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Hilfe! Mideid! Und Beschwerde!", "tokens": ["Hil\u00b7fe", "!", "Mi\u00b7deid", "!", "Und", "Be\u00b7schwer\u00b7de", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "KON", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Zwanzig arme Waisenkinder,", "tokens": ["Zwan\u00b7zig", "ar\u00b7me", "Wai\u00b7sen\u00b7kin\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Streng getrieben, eine Herde", "tokens": ["Streng", "ge\u00b7trie\u00b7ben", ",", "ei\u00b7ne", "Her\u00b7de"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "VVPP", "$,", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Junger Rinder \u2013.", "tokens": ["Jun\u00b7ger", "Rin\u00b7der", "\u2013", "."], "token_info": ["word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$(", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.14": {"line.1": {"text": "Weil mich meine Mutter knufft,", "tokens": ["Weil", "mich", "mei\u00b7ne", "Mut\u00b7ter", "knufft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und um St\u00e4rkres zu vermeiden,", "tokens": ["Und", "um", "St\u00e4r\u00b7kres", "zu", "ver\u00b7mei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Sag ich: \u00bbJa, man l\u00e4\u00dft sie weiden", "tokens": ["Sag", "ich", ":", "\u00bb", "Ja", ",", "m