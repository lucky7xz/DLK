, "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "\u00bbmanche brauchen Mist, Mama,", "tokens": ["\u00bb", "man\u00b7che", "brau\u00b7chen", "Mist", ",", "Ma\u00b7ma", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PIAT", "ADJA", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil sie anderes vermissen,", "tokens": ["Weil", "sie", "an\u00b7de\u00b7res", "ver\u00b7mis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der ist \u2013 wer kann es wissen \u2013", "tokens": ["Und", "der", "ist", "\u2013", "wer", "kann", "es", "wis\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VAFIN", "$(", "PWS", "VMFIN", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hier vielleicht sehr reichlich da.\u00ab", "tokens": ["Hier", "viel\u00b7leicht", "sehr", "reich\u00b7lich", "da", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "ADV", "ADJD", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Meine Mutter ruckt, \u2013 schluckt:", "tokens": ["Mei\u00b7ne", "Mut\u00b7ter", "ruckt", ",", "\u2013", "schluckt", ":"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "$(", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.18": {"line.1": {"text": "\u00bbtreibt mit diesen Engeln Spott!", "tokens": ["\u00bb", "treibt", "mit", "die\u00b7sen", "En\u00b7geln", "Spott", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "APPR", "PDAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mich will er nicht verstehen.", "tokens": ["Und", "mich", "will", "er", "nicht", "ver\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Warte, dir wird's schlimm ergehen!", "tokens": ["War\u00b7te", ",", "dir", "wird's", "schlimm", "er\u00b7ge\u00b7hen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "ADJD", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Und das w\u00fcnsch ich dir. Bei Gott.\u00ab", "tokens": ["Und", "das", "w\u00fcnsch", "ich", "dir", ".", "Bei", "Gott", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPER", "PPER", "$.", "APPR", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.19": {"line.1": {"text": "Meine Mutter dreht", "tokens": ["Mei\u00b7ne", "Mut\u00b7ter", "dreht"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "R\u00fccken zu und geht.", "tokens": ["R\u00fc\u00b7cken", "zu", "und", "geht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.20": {"line.1": {"text": "Und nun sauf ich wo, wo keine", "tokens": ["Und", "nun", "sauf", "ich", "wo", ",", "wo", "kei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PWAV", "$,", "PWAV", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rinder, Blumen, Engel sind,", "tokens": ["Rin\u00b7der", ",", "Blu\u00b7men", ",", "En\u00b7gel", "sind", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NE", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bin f\u00fcr mich oder f\u00fcr meine", "tokens": ["Bin", "f\u00fcr", "mich", "o\u00b7der", "f\u00fcr", "mei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PPER", "KON", "APPR", "PPOSAT"], "meter": "+--++-+-", "measure": "dactylic.init"}, "line.4": {"text": "Mutter Naseweisenkind.", "tokens": ["Mut\u00b7ter", "Na\u00b7se\u00b7wei\u00b7sen\u00b7kind", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}