{"textgrid.poem.42887": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Die Waisenkinder", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwanzig grobe Strohh\u00fcte gehen", "tokens": ["Zwan\u00b7zig", "gro\u00b7be", "Stroh\u00b7h\u00fc\u00b7te", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "ADJA", "NN", "VVINF"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Zwei und zwei wie Milit\u00e4r.", "tokens": ["Zwei", "und", "zwei", "wie", "Mi\u00b7li\u00b7t\u00e4r", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "KON", "CARD", "KOKOM", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zwanzig schwarze Pelerinchen wehen,", "tokens": ["Zwan\u00b7zig", "schwar\u00b7ze", "Pe\u00b7le\u00b7rin\u00b7chen", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Als wenn's zum Begr\u00e4bnis w\u00e4r.", "tokens": ["Als", "wenn's", "zum", "Be\u00b7gr\u00e4b\u00b7nis", "w\u00e4r", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPRART", "NN", "VAFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Magre Lehrerin voraus,", "tokens": ["Mag\u00b7re", "Leh\u00b7re\u00b7rin", "vo\u00b7raus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hinten magre zweite,", "tokens": ["Hin\u00b7ten", "mag\u00b7re", "zwei\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Eine dritte an der Seite,", "tokens": ["Ei\u00b7ne", "drit\u00b7te", "an", "der", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Also zieht aus engem Haus", "tokens": ["Al\u00b7so", "zieht", "aus", "en\u00b7gem", "Haus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Eine Schlange in die Weite.", "tokens": ["Ei\u00b7ne", "Schlan\u00b7ge", "in", "die", "Wei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hilfe! Mideid! Und Beschwerde!", "tokens": ["Hil\u00b7fe", "!", "Mi\u00b7deid", "!", "Und", "Be\u00b7schwer\u00b7de", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "KON", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Zwanzig arme Waisenkinder,", "tokens": ["Zwan\u00b7zig", "ar\u00b7me", "Wai\u00b7sen\u00b7kin\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Streng getrieben, eine Herde", "tokens": ["Streng", "ge\u00b7trie\u00b7ben", ",", "ei\u00b7ne", "Her\u00b7de"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "VVPP", "$,", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Junger Rinder \u2013.", "tokens": ["Jun\u00b7ger", "Rin\u00b7der", "\u2013", "."], "token_info": ["word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$(", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Weil mich meine Mutter knufft,", "tokens": ["Weil", "mich", "mei\u00b7ne", "Mut\u00b7ter", "knufft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und um St\u00e4rkres zu vermeiden,", "tokens": ["Und", "um", "St\u00e4r\u00b7kres", "zu", "ver\u00b7mei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Sag ich: \u00bbJa, man l\u00e4\u00dft sie weiden", "tokens": ["Sag", "ich", ":", "\u00bb", "Ja", ",", "man", "l\u00e4\u00dft", "sie", "wei\u00b7den"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$.", "$(", "PTKANT", "$,", "PIS", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In der frischen, freien Luft.\u00ab", "tokens": ["In", "der", "fri\u00b7schen", ",", "frei\u00b7en", "Luft", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbweiden? \u2013 Dummheit! Siehst du nicht,", "tokens": ["\u00bb", "wei\u00b7den", "?", "\u2013", "Dumm\u00b7heit", "!", "Siehst", "du", "nicht", ","], "token_info": ["punct", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVINF", "$.", "$(", "NN", "$.", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Was hier vorgeht, roher Bengel!", "tokens": ["Was", "hier", "vor\u00b7geht", ",", "ro\u00b7her", "Ben\u00b7gel", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Junge Blumen brauchen Licht,", "tokens": ["Jun\u00b7ge", "Blu\u00b7men", "brau\u00b7chen", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00e4rme, Erde, Wurzel, Stengel \u2013.\u00ab", "tokens": ["W\u00e4r\u00b7me", ",", "Er\u00b7de", ",", "Wur\u00b7zel", ",", "Sten\u00b7gel", "\u2013", ".", "\u00ab"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "punct", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$(", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "\u00bbmanche brauchen Mist, Mama,", "tokens": ["\u00bb", "man\u00b7che", "brau\u00b7chen", "Mist", ",", "Ma\u00b7ma", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PIAT", "ADJA", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil sie anderes vermissen,", "tokens": ["Weil", "sie", "an\u00b7de\u00b7res", "ver\u00b7mis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der ist \u2013 wer kann es wissen \u2013", "tokens": ["Und", "der", "ist", "\u2013", "wer", "kann", "es", "wis\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VAFIN", "$(", "PWS", "VMFIN", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hier vielleicht sehr reichlich da.\u00ab", "tokens": ["Hier", "viel\u00b7leicht", "sehr", "reich\u00b7lich", "da", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "ADV", "ADJD", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Meine Mutter ruckt, \u2013 schluckt:", "tokens": ["Mei\u00b7ne", "Mut\u00b7ter", "ruckt", ",", "\u2013", "schluckt", ":"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "$(", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.8": {"line.1": {"text": "\u00bbtreibt mit diesen Engeln Spott!", "tokens": ["\u00bb", "treibt", "mit", "die\u00b7sen", "En\u00b7geln", "Spott", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "APPR", "PDAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mich will er nicht verstehen.", "tokens": ["Und", "mich", "will", "er", "nicht", "ver\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Warte, dir wird's schlimm ergehen!", "tokens": ["War\u00b7te", ",", "dir", "wird's", "schlimm", "er\u00b7ge\u00b7hen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "ADJD", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Und das w\u00fcnsch ich dir. Bei Gott.\u00ab", "tokens": ["Und", "das", "w\u00fcnsch", "ich", "dir", ".", "Bei", "Gott", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPER", "PPER", "$.", "APPR", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Meine Mutter dreht", "tokens": ["Mei\u00b7ne", "Mut\u00b7ter", "dreht"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "R\u00fccken zu und geht.", "tokens": ["R\u00fc\u00b7cken", "zu", "und", "geht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "Und nun sauf ich wo, wo keine", "tokens": ["Und", "nun", "sauf", "ich", "wo", ",", "wo", "kei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PWAV", "$,", "PWAV", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rinder, Blumen, Engel sind,", "tokens": ["Rin\u00b7der", ",", "Blu\u00b7men", ",", "En\u00b7gel", "sind", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NE", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bin f\u00fcr mich oder f\u00fcr meine", "tokens": ["Bin", "f\u00fcr", "mich", "o\u00b7der", "f\u00fcr", "mei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PPER", "KON", "APPR", "PPOSAT"], "meter": "+--++-+-", "measure": "dactylic.init"}, "line.4": {"text": "Mutter Naseweisenkind.", "tokens": ["Mut\u00b7ter", "Na\u00b7se\u00b7wei\u00b7sen\u00b7kind", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Zwanzig grobe Strohh\u00fcte gehen", "tokens": ["Zwan\u00b7zig", "gro\u00b7be", "Stroh\u00b7h\u00fc\u00b7te", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "ADJA", "NN", "VVINF"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Zwei und zwei wie Milit\u00e4r.", "tokens": ["Zwei", "und", "zwei", "wie", "Mi\u00b7li\u00b7t\u00e4r", "."], "token_info": ["word",