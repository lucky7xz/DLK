{"dta.poem.21353": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XIII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Und als der Morgennebel zerrann,", "tokens": ["Und", "als", "der", "Mor\u00b7gen\u00b7ne\u00b7bel", "zer\u00b7rann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da sah ich am Wege ragen,", "tokens": ["Da", "sah", "ich", "am", "We\u00b7ge", "ra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Im Fr\u00fchrothschein, das Bild des Mann\u2019s,", "tokens": ["Im", "Fr\u00fch\u00b7roth\u00b7schein", ",", "das", "Bild", "des", "Mann's", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der an das Kreuz geschlagen.", "tokens": ["Der", "an", "das", "Kreuz", "ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mit Wehmuth erf\u00fcllt mich jedesmahl", "tokens": ["Mit", "Weh\u00b7muth", "er\u00b7f\u00fcllt", "mich", "je\u00b7des\u00b7mahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Dein Anblick, mein armer Vetter,", "tokens": ["Dein", "An\u00b7blick", ",", "mein", "ar\u00b7mer", "Vet\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der du die Welt erl\u00f6sen gewollt,", "tokens": ["Der", "du", "die", "Welt", "er\u00b7l\u00f6\u00b7sen", "ge\u00b7wollt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVINF", "VMPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Du Narr, du Menschheitsretter!", "tokens": ["Du", "Narr", ",", "du", "Menschheits\u00b7ret\u00b7ter", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PPER", "NN", "$."], "meter": "-+-++-", "measure": "unknown.measure.tri"}}, "stanza.3": {"line.1": {"text": "Sie haben dir \u00fcbel mitgespielt,", "tokens": ["Sie", "ha\u00b7ben", "dir", "\u00fc\u00b7bel", "mit\u00b7ge\u00b7spielt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Herren vom hohen Rathe.", "tokens": ["Die", "Her\u00b7ren", "vom", "ho\u00b7hen", "Ra\u00b7the", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wer hie\u00df dich auch reden so r\u00fccksichtslos", "tokens": ["Wer", "hie\u00df", "dich", "auch", "re\u00b7den", "so", "r\u00fcck\u00b7sichts\u00b7los"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Von der Kirche und vom Staate!", "tokens": ["Von", "der", "Kir\u00b7che", "und", "vom", "Staa\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Zu deinem Malheur war die Buchdruckerey", "tokens": ["Zu", "dei\u00b7nem", "Mal\u00b7heur", "war", "die", "Buch\u00b7dru\u00b7cke\u00b7rey"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Noch nicht in jenen Tagen", "tokens": ["Noch", "nicht", "in", "je\u00b7nen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PTKNEG", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Erfunden; Du h\u00e4ttest geschrieben ein Buch", "tokens": ["Er\u00b7fun\u00b7den", ";", "Du", "h\u00e4t\u00b7test", "ge\u00b7schrie\u00b7ben", "ein", "Buch"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "PPER", "VAFIN", "VVPP", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ueber die Himmelsfragen.", "tokens": ["Ue\u00b7ber", "die", "Him\u00b7mels\u00b7fra\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}}}}