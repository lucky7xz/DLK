{"dta.poem.8792": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XiII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "So ganz der Welt und dir genug zu seyn?", "tokens": ["So", "ganz", "der", "Welt", "und", "dir", "ge\u00b7nug", "zu", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "KON", "PPER", "ADV", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch unbefriedigt schien nur jedes Herz,", "tokens": ["Doch", "un\u00b7be\u00b7frie\u00b7digt", "schien", "nur", "je\u00b7des", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und jedes Wesen, das ich frug, zu seyn;", "tokens": ["Und", "je\u00b7des", "We\u00b7sen", ",", "das", "ich", "frug", ",", "zu", "seyn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein duftig R\u00e4thsel schien die Rose mir,", "tokens": ["Ein", "duf\u00b7tig", "R\u00e4th\u00b7sel", "schien", "die", "Ro\u00b7se", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und jedes Blatt nur auf dem Flug zu seyn;", "tokens": ["Und", "je\u00b7des", "Blatt", "nur", "auf", "dem", "Flug", "zu", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "APPR", "ART", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Des Baumes Schatten, unter dem ich lag,", "tokens": ["Des", "Bau\u00b7mes", "Schat\u00b7ten", ",", "un\u00b7ter", "dem", "ich", "lag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Schien mir ein k\u00f6stlicher Betrug zu seyn;", "tokens": ["Schien", "mir", "ein", "k\u00f6st\u00b7li\u00b7cher", "Be\u00b7trug", "zu", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Gehemmt in Fesseln schien mein eigen Lied,", "tokens": ["Ge\u00b7hemmt", "in", "Fes\u00b7seln", "schien", "mein", "ei\u00b7gen", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "In die ich's wider Willen schlug, zu seyn.", "tokens": ["In", "die", "ich's", "wi\u00b7der", "Wil\u00b7len", "schlug", ",", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "PIS", "APPR", "NN", "VVFIN", "$,", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}