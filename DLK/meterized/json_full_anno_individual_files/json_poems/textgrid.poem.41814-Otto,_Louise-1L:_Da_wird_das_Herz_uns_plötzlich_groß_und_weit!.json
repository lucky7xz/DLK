], "pos": ["APPR", "PDAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbf\u00fcr alle will der Herr sich offenbaren", "tokens": ["\u00bb", "f\u00fcr", "al\u00b7le", "will", "der", "Herr", "sich", "of\u00b7fen\u00b7ba\u00b7ren"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "APPR", "PIS", "VMFIN", "ART", "NN", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In seiner ewigtreuen Liebesmacht;", "tokens": ["In", "sei\u00b7ner", "e\u00b7wig\u00b7treu\u00b7en", "Lie\u00b7bes\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "F\u00fcr alle hat er Noth und Tod befahren", "tokens": ["F\u00fcr", "al\u00b7le", "hat", "er", "Noth", "und", "Tod", "be\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "VAFIN", "PPER", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und der Erl\u00f6sung gro\u00dfes Werk vollbracht,", "tokens": ["Und", "der", "Er\u00b7l\u00f6\u00b7sung", "gro\u00b7\u00dfes", "Werk", "voll\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Das gleich den Gliedern eines Leibes einte", "tokens": ["Das", "gleich", "den", "Glie\u00b7dern", "ei\u00b7nes", "Lei\u00b7bes", "ein\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mit festem Band die gl\u00e4ubige Gemeinde.\u00ab", "tokens": ["Mit", "fes\u00b7tem", "Band", "die", "gl\u0