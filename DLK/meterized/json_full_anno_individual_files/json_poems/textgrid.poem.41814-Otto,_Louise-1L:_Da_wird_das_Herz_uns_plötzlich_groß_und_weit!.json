{"textgrid.poem.41814": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da wird das Herz uns pl\u00f6tzlich gro\u00df und weit!", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da wird das Herz uns pl\u00f6tzlich gro\u00df und weit!", "tokens": ["Da", "wird", "das", "Herz", "uns", "pl\u00f6tz\u00b7lich", "gro\u00df", "und", "weit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "ADJD", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sie k\u00fcnden uns wie mit Drommetendr\u00f6hnen", "tokens": ["Sie", "k\u00fcn\u00b7den", "uns", "wie", "mit", "Drom\u00b7me\u00b7ten\u00b7dr\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "KOKOM", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Siegsgesang der echten Menschlichkeit.", "tokens": ["Den", "Siegs\u00b7ge\u00b7sang", "der", "ech\u00b7ten", "Menschlich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Denn anders ist kein heilig' Werk zu kr\u00f6nen", "tokens": ["Denn", "an\u00b7ders", "ist", "kein", "hei\u00b7lig'", "Werk", "zu", "kr\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PIAT", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und anders nie zu enden Kampf und Streit,", "tokens": ["Und", "an\u00b7ders", "nie", "zu", "en\u00b7den", "Kampf", "und", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Als wenn ein Heil, das in die Welt gekommen", "tokens": ["Als", "wenn", "ein", "Heil", ",", "das", "in", "die", "Welt", "ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der Sonne gleich ", "tokens": ["Der", "Son\u00b7ne", "gleich"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "\u00bb", "tokens": ["\u00bb"], "token_info": ["punct"], "pos": ["$("]}, "line.2": {"text": "In jener gottgeweihten heil'gen Nacht,", "tokens": ["In", "je\u00b7ner", "gott\u00b7ge\u00b7weih\u00b7ten", "heil'\u00b7gen", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbf\u00fcr alle will der Herr sich offenbaren", "tokens": ["\u00bb", "f\u00fcr", "al\u00b7le", "will", "der", "Herr", "sich", "of\u00b7fen\u00b7ba\u00b7ren"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "APPR", "PIS", "VMFIN", "ART", "NN", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In seiner ewigtreuen Liebesmacht;", "tokens": ["In", "sei\u00b7ner", "e\u00b7wig\u00b7treu\u00b7en", "Lie\u00b7bes\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "F\u00fcr alle hat er Noth und Tod befahren", "tokens": ["F\u00fcr", "al\u00b7le", "hat", "er", "Noth", "und", "Tod", "be\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "VAFIN", "PPER", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und der Erl\u00f6sung gro\u00dfes Werk vollbracht,", "tokens": ["Und", "der", "Er\u00b7l\u00f6\u00b7sung", "gro\u00b7\u00dfes", "Werk", "voll\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Das gleich den Gliedern eines Leibes einte", "tokens": ["Das", "gleich", "den", "Glie\u00b7dern", "ei\u00b7nes", "Lei\u00b7bes", "ein\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mit festem Band die gl\u00e4ubige Gemeinde.\u00ab", "tokens": ["Mit", "fes\u00b7tem", "Band", "die", "gl\u00e4u\u00b7bi\u00b7ge", "Ge\u00b7mein\u00b7de", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "\u00bbf\u00fcr alle \u2013\u00ab klang es im Hussitenheere \u2013", "tokens": ["\u00bb", "f\u00fcr", "al\u00b7le", "\u2013", "\u00ab", "klang", "es", "im", "Hus\u00b7si\u00b7ten\u00b7hee\u00b7re", "\u2013"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PIAT", "$(", "$(", "VVFIN", "PPER", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbist auch der Gnade Kelch mit Christi Blut,", "tokens": ["\u00bb", "ist", "auch", "der", "Gna\u00b7de", "Kelch", "mit", "Chris\u00b7ti", "Blut", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ADV", "ART", "NN", "NN", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denn allen ward verk\u00fcndet seine Lehre,", "tokens": ["Denn", "al\u00b7len", "ward", "ver\u00b7k\u00fcn\u00b7det", "sei\u00b7ne", "Leh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die in der Gleichheit aller Menschen ruht,", "tokens": ["Die", "in", "der", "Gleich\u00b7heit", "al\u00b7ler", "Men\u00b7schen", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und Erd' und Himmel hat nicht h\u00f6hre Ehre,", "tokens": ["Und", "Erd'", "und", "Him\u00b7mel", "hat", "nicht", "h\u00f6h\u00b7re", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VAFIN", "PTKNEG", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Als nun uns wird mit dem geweihten Gut!\u00ab", "tokens": ["Als", "nun", "uns", "wird", "mit", "dem", "ge\u00b7weih\u00b7ten", "Gut", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ADV", "PPER", "VAFIN", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Im M\u00e4rtyr'tum, in grauser Todeshalle", "tokens": ["Im", "M\u00e4r\u00b7ty\u00b7r'\u00b7tum", ",", "in", "grau\u00b7ser", "To\u00b7des\u00b7hal\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Ert\u00f6nt es noch: \u00bb", "tokens": ["Er\u00b7t\u00f6nt", "es", "noch", ":", "\u00bb"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "So wu\u00dften sie die Losung recht zu fassen,", "tokens": ["So", "wu\u00df\u00b7ten", "sie", "die", "Lo\u00b7sung", "recht", "zu", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erteilten sie an ", "tokens": ["Er\u00b7teil\u00b7ten", "sie", "an"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Sie wollten nicht das hohe Erbteil lassen,", "tokens": ["Sie", "woll\u00b7ten", "nicht", "das", "ho\u00b7he", "Erb\u00b7teil", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das B\u00fcrgertum im neuen Liebesreich.", "tokens": ["Das", "B\u00fcr\u00b7ger\u00b7tum", "im", "neu\u00b7en", "Lie\u00b7bes\u00b7reich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da gab es keinen Neid mehr und kein Hassen,", "tokens": ["Da", "gab", "es", "kei\u00b7nen", "Neid", "mehr", "und", "kein", "Has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "ADV", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Kein Sklaventum, kein Herschen stark und feig,", "tokens": ["Kein", "Skla\u00b7ven\u00b7tum", ",", "kein", "Her\u00b7schen", "stark", "und", "feig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.8": {"text": "Aus d\u00fcsterm Bann, aus schwerer Knechtschaft Ketten.", "tokens": ["Aus", "d\u00fcs\u00b7term", "Bann", ",", "aus", "schwe\u00b7rer", "Knecht\u00b7schaft", "Ket\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wo wieder aber ward der Ruf vernommen:", "tokens": ["Wo", "wie\u00b7der", "a\u00b7ber", "ward", "der", "Ruf", "ver\u00b7nom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u00bb", "tokens": ["\u00bb"], "token_info": ["punct"], "pos": ["$("]}, "line.3": {"text": "Denn f\u00fcr die ", "tokens": ["Denn", "f\u00fcr", "die"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.4": {"text": "Im Wettersturm der Revolution.", "tokens": ["Im", "Wet\u00b7ter\u00b7sturm", "der", "Re\u00b7vo\u00b7lu\u00b7ti\u00b7on", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Denn schien auch Joch auf Joch hinweggenommen,", "tokens": ["Denn", "schien", "auch", "Joch", "auf", "Joch", "hin\u00b7weg\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und st\u00fcrzte auch in Tr\u00fcmmer Thron um Thron:", "tokens": ["Und", "st\u00fcrz\u00b7te", "auch", "in", "Tr\u00fcm\u00b7mer", "Thron", "um", "Thron", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dem ", "tokens": ["Dem"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}}, "stanza.6": {"line.1": {"text": "Wohl gr\u00fc\u00dften freie M\u00e4nner sich als ", "tokens": ["Wohl", "gr\u00fc\u00df\u00b7ten", "frei\u00b7e", "M\u00e4n\u00b7ner", "sich", "als"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "PRF", "KOUS"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Nur B\u00fcrger gab es, nicht mehr Herr und Knecht;", "tokens": ["Nur", "B\u00fcr\u00b7ger", "gab", "es", ",", "nicht", "mehr", "Herr", "und", "Knecht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "PPER", "$,", "PTKNEG", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wohl sangen sie der Liebe Bundeslieder", "tokens": ["Wohl", "san\u00b7gen", "sie", "der", "Lie\u00b7be", "Bun\u00b7des\u00b7lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und f\u00fchlten sich als ein erneut' Geschlecht.", "tokens": ["Und", "f\u00fchl\u00b7ten", "sich", "als", "ein", "er\u00b7neut'", "Ge\u00b7schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch auf die ", "tokens": ["Doch", "auf", "die"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Blieb von dem Ruf: \u00bbf\u00fcr alle!\u00ab ausgenommen \u2013", "tokens": ["Blieb", "von", "dem", "Ruf", ":", "\u00bb", "f\u00fcr", "al\u00b7le", "!", "\u00ab", "aus\u00b7ge\u00b7nom\u00b7men", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$.", "$(", "APPR", "PIS", "$.", "$(", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ihr mu\u00df erst noch der Tag des Rechtes kommen.", "tokens": ["Ihr", "mu\u00df", "erst", "noch", "der", "Tag", "des", "Rech\u00b7tes", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Der Frauen Schar, die in den Staub getreten,", "tokens": ["Der", "Frau\u00b7en", "Schar", ",", "die", "in", "den", "Staub", "ge\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ward nur erhoben an des Glaubens Hand.", "tokens": ["Ward", "nur", "er\u00b7ho\u00b7ben", "an", "des", "Glau\u00b7bens", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Besten lernten fromm zum Himmel beten,", "tokens": ["Die", "Bes\u00b7ten", "lern\u00b7ten", "fromm", "zum", "Him\u00b7mel", "be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Weil ja die Erdenwelt sie nicht verstand;", "tokens": ["Weil", "ja", "die", "Er\u00b7den\u00b7welt", "sie", "nicht", "ver\u00b7stand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die andern aber lie\u00dfen sich bereden", "tokens": ["Die", "an\u00b7dern", "a\u00b7ber", "lie\u00b7\u00dfen", "sich", "be\u00b7re\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADV", "VVFIN", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sie seien nur bestimmt zu Spiel und Tand,", "tokens": ["Sie", "sei\u00b7en", "nur", "be\u00b7stimmt", "zu", "Spiel", "und", "Tand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Es sei ihr h\u00f6chstes Ziel im s\u00fc\u00dfen Minnen,", "tokens":