{"dta.poem.7100": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Welch ein best\u00e4nd\u2019ger Gnaden-Gu\u00df!", "tokens": ["Welch", "ein", "be\u00b7st\u00e4n\u00b7d'\u00b7ger", "Gna\u00b7den\u00b7Gu\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Welch ein unendlicher und ruh\u2019ger Wollust-Flu\u00df", "tokens": ["Welch", "ein", "un\u00b7end\u00b7li\u00b7cher", "und", "ruh'\u00b7ger", "Wol\u00b7lust\u00b7Flu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ART", "ADJA", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wird alle Seelen \u00fcbersch\u00fctten!", "tokens": ["Wird", "al\u00b7le", "See\u00b7len", "\u00fc\u00b7ber\u00b7sch\u00fct\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Welch ein Entz\u00fccken, welche F\u00fclle,", "tokens": ["Welch", "ein", "Ent\u00b7z\u00fc\u00b7cken", ",", "wel\u00b7che", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "$,", "PWAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Von inniger Zufriedenheit,", "tokens": ["Von", "in\u00b7ni\u00b7ger", "Zu\u00b7frie\u00b7den\u00b7heit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mu\u00df ein verherrlichter Verstand, ein reiner Wille,", "tokens": ["Mu\u00df", "ein", "ver\u00b7herr\u00b7lich\u00b7ter", "Ver\u00b7stand", ",", "ein", "rei\u00b7ner", "Wil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der nichts, als Lust und Herrlichkeit,", "tokens": ["Der", "nichts", ",", "als", "Lust", "und", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$,", "KOUS", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nichts, als unendliche Vollkommenheit", "tokens": ["Nichts", ",", "als", "un\u00b7end\u00b7li\u00b7che", "Voll\u00b7kom\u00b7men\u00b7heit"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PIS", "$,", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Zu seinem Vorwurff hat, empfinden und versp\u00fcren;", "tokens": ["Zu", "sei\u00b7nem", "Vor\u00b7wurff", "hat", ",", "emp\u00b7fin\u00b7den", "und", "ver\u00b7sp\u00fc\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "$,", "VVFIN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wenn GOTT, die ewig seel\u2019ge Liebe,", "tokens": ["Wenn", "GoTT", ",", "die", "e\u00b7wig", "seel'\u00b7ge", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "$,", "PRELS", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Die ewig weise Macht, ihn seelig haben will.", "tokens": ["Die", "e\u00b7wig", "wei\u00b7se", "Macht", ",", "ihn", "see\u00b7lig", "ha\u00b7ben", "will", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,", "PPER", "ADJD", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Was kann und wird doch Der sodann,", "tokens": ["Was", "kann", "und", "wird", "doch", "Der", "so\u00b7dann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "KON", "VAFIN", "ADV", "ART", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Der alles Gute will und kann,", "tokens": ["Der", "al\u00b7les", "Gu\u00b7te", "will", "und", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VMFIN", "KON", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Vor unumschr\u00e4nckte F\u00e4higkeiten,", "tokens": ["Vor", "un\u00b7um\u00b7schr\u00e4nck\u00b7te", "F\u00e4\u00b7hig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Begl\u00fccket und vergn\u00fcgt zu leben,", "tokens": ["Be\u00b7gl\u00fc\u00b7cket", "und", "ver\u00b7gn\u00fcgt", "zu", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Jhm, \u00fcber alles Dencken, geben!", "tokens": ["Jhm", ",", "\u00fc\u00b7ber", "al\u00b7les", "Den\u00b7cken", ",", "ge\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "APPR", "PIS", "VVINF", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Wie kann und wird Er nicht den Geist Jhm zubereiten", "tokens": ["Wie", "kann", "und", "wird", "Er", "nicht", "den", "Geist", "Jhm", "zu\u00b7be\u00b7rei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "KON", "VAFIN", "PPER", "PTKNEG", "ART", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Als eine Tieff\u2019, in der kein Grund zu finden.", "tokens": ["Als", "ei\u00b7ne", "Tief\u00b7f'", ",", "in", "der", "kein", "Grund", "zu", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "APPR", "ART", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Worinn ein Wollust-Meer, das auch nicht zu ergr\u00fcnden,", "tokens": ["Wo\u00b7rinn", "ein", "Wol\u00b7lust\u00b7Meer", ",", "das", "auch", "nicht", "zu", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "ADV", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "In steter Anmuth wallt, worinn kein Tr\u00f6pffchen schwimmet,", "tokens": ["In", "ste\u00b7ter", "An\u00b7muth", "wallt", ",", "wo\u00b7rinn", "kein", "Tr\u00f6pffc\u00b7hen", "schwim\u00b7met", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,", "PWAV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Das nicht zu seiner Lust bestimmet,", "tokens": ["Das", "nicht", "zu", "sei\u00b7ner", "Lust", "be\u00b7stim\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Das er nicht f\u00fchlen kann, das er nicht immer f\u00fchlt,", "tokens": ["Das", "er", "nicht", "f\u00fch\u00b7len", "kann", ",", "das", "er", "nicht", "im\u00b7mer", "f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$,", "PRELS", "PPER", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Das nicht auch mit auf seine Freude zielt.", "tokens": ["Das", "nicht", "auch", "mit", "auf", "sei\u00b7ne", "Freu\u00b7de", "zielt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "ADV", "APPR", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Wie unbegreifflich wird die Lust, die allgemein,", "tokens": ["Wie", "un\u00b7be\u00b7greif\u00b7flich", "wird", "die", "Lust", ",", "die", "all\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Die aus unz\u00e4hlbaren Vergn\u00fcgungen bestehen,", "tokens": ["Die", "aus", "un\u00b7z\u00e4hl\u00b7ba\u00b7ren", "Ver\u00b7gn\u00fc\u00b7gun\u00b7gen", "be\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+--++--+-", "measure": "trochaic.hexa.relaxed"}, "line.26": {"text": "Die alle f\u00fchlbar sind, und tieff aus Hertze gehen;", "tokens": ["Die", "al\u00b7le", "f\u00fchl\u00b7bar", "sind", ",", "und", "tieff", "aus", "Hert\u00b7ze", "ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADJD", "VAFIN", "$,", "KON", "ADJD", "APPR", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "In ihrem g\u00e4ntzlichen Zusammenhang nicht seyn!", "tokens": ["In", "ih\u00b7rem", "g\u00e4ntz\u00b7li\u00b7chen", "Zu\u00b7sam\u00b7men\u00b7hang", "nicht", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKNEG", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}