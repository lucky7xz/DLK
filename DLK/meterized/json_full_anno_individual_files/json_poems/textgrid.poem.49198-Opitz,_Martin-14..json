{"textgrid.poem.49198": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O wol dem, der die rechte Zeit", "tokens": ["O", "wol", "dem", ",", "der", "die", "rech\u00b7te", "Zeit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ART", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In allen Dingen siehet", "tokens": ["In", "al\u00b7len", "Din\u00b7gen", "sie\u00b7het"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und nicht nach dem, was allbereit", "tokens": ["Und", "nicht", "nach", "dem", ",", "was", "all\u00b7be\u00b7reit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PTKNEG", "APPR", "ART", "$,", "PRELS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hinweg ist, sich bem\u00fchet,", "tokens": ["Hin\u00b7weg", "ist", ",", "sich", "be\u00b7m\u00fc\u00b7het", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der kennet, was er lieben soll", "tokens": ["Der", "ken\u00b7net", ",", "was", "er", "lie\u00b7ben", "soll"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "PWS", "PPER", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und was er soll verlassen;", "tokens": ["Und", "was", "er", "soll", "ver\u00b7las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPE