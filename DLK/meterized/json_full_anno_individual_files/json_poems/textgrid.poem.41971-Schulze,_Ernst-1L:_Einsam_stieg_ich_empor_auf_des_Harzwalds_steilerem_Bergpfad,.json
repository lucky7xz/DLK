{"textgrid.poem.41971": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Einsam stieg ich empor auf des Harzwalds steilerem Bergpfad,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einsam stieg ich empor auf des Harzwalds steilerem Bergpfad,", "tokens": ["Ein\u00b7sam", "stieg", "ich", "em\u00b7por", "auf", "des", "Harz\u00b7walds", "stei\u00b7le\u00b7rem", "Berg\u00b7pfad", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PTKVZ", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Nahete dir mich schon, ewiger, alter Granit,", "tokens": ["Na\u00b7he\u00b7te", "dir", "mich", "schon", ",", "e\u00b7wi\u00b7ger", ",", "al\u00b7ter", "Gra\u00b7nit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "$,", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wo hochlodernd einst durch die Nacht vom felsigen Altar", "tokens": ["Wo", "hoch\u00b7lo\u00b7dernd", "einst", "durch", "die", "Nacht", "vom", "fel\u00b7si\u00b7gen", "Al\u00b7tar"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ADV", "APPR", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Hell in's ferne Gefild flammte das Opfer des Mays.", "tokens": ["Hell", "in's", "fer\u00b7ne", "Ge\u00b7fild", "flamm\u00b7te", "das", "Op\u00b7fer", "des", "Mays", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "ART", "NE", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Tr\u00e4umend schritt ich dahin, und es d\u00e4mmerte leise der Vorzeit", "tokens": ["Tr\u00e4u\u00b7mend", "schritt", "ich", "da\u00b7hin", ",", "und", "es", "d\u00e4m\u00b7mer\u00b7te", "lei\u00b7se", "der", "Vor\u00b7zeit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PAV", "$,", "KON", "PPER", "VVFIN", "ADJD", "ART", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Riesengebild mit des Wahns Wundergestalten umher.", "tokens": ["Rie\u00b7sen\u00b7ge\u00b7bild", "mit", "des", "Wahns", "Wun\u00b7der\u00b7ge\u00b7stal\u00b7ten", "um\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Schaurig scholl, wie die Sagen entschwundener Zeit, das Ges\u00e4usel", "tokens": ["Schau\u00b7rig", "scholl", ",", "wie", "die", "Sa\u00b7gen", "ent\u00b7schwun\u00b7de\u00b7ner", "Zeit", ",", "das", "Ge\u00b7s\u00e4u\u00b7sel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "ADJD", "$,", "PWAV", "ART", "NN", "ADJA", "NN", "$,", "ART", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Fl\u00fcsternder Tannen, und fern rauschte der Bach des Gesteins.", "tokens": ["Fl\u00fcs\u00b7tern\u00b7der", "Tan\u00b7nen", ",", "und", "fern", "rauschte", "der", "Bach", "des", "Ge\u00b7steins", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "ADJD", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.9": {"text": "Geyer umflatterten Felsen und Wald lautkr\u00e4chzend, und graunvoll", "tokens": ["Ge\u00b7yer", "um\u00b7flat\u00b7ter\u00b7ten", "Fel\u00b7sen", "und", "Wald", "laut\u00b7kr\u00e4ch\u00b7zend", ",", "und", "graun\u00b7voll"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ADJA", "NN", "KON", "NN", "ADJD", "$,", "KON", "ADJD"], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Schwieg, um Th\u00e4ler und H\u00f6hn starrend, das Haidegefild.", "tokens": ["Schwieg", ",", "um", "Th\u00e4\u00b7ler", "und", "H\u00f6hn", "star\u00b7rend", ",", "das", "Hai\u00b7de\u00b7ge\u00b7fild", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "KOUI", "NN", "KON", "NN", "VVPP", "$,", "ART", "NN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Sieh, da nahetest du mit munterem Schritt aus des Waldes", "tokens": ["Sieh", ",", "da", "na\u00b7he\u00b7test", "du", "mit", "mun\u00b7te\u00b7rem", "Schritt", "aus", "des", "Wal\u00b7des"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.12": {"text": "Heiliger Nacht, und ich stand staunend und schaute dich an.", "tokens": ["Hei\u00b7li\u00b7ger", "Nacht", ",", "und", "ich", "stand", "stau\u00b7nend", "und", "schau\u00b7te", "dich", "an", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "PPER", "VVFIN", "ADJD", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.13": {"text": "Hell umflo\u00df dich das wei\u00dfe Gewand, frisch gr\u00fcnte des Epheus", "tokens": ["Hell", "um\u00b7flo\u00df", "dich", "das", "wei\u00b7\u00dfe", "Ge\u00b7wand", ",", "frisch", "gr\u00fcn\u00b7te", "des", "E\u00b7pheus"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "ADJD", "VVFIN", "ART", "NN"], "meter": "--+--+--+-+-+-+", "measure": "anapaest.tri.plus"}, "line.14": {"text": "Kranz um die Stirn, und es hielt Nelken die zierliche Hand.", "tokens": ["Kranz", "um", "die", "Stirn", ",", "und", "es", "hielt", "Nel\u00b7ken", "die", "zier\u00b7li\u00b7che", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "KON", "PPER", "VVFIN", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.15": {"text": "Ach, wohl w\u00e4hnt' ich ein Wunder zu sehn aus des fr\u00fcheren Glaubens", "tokens": ["Ach", ",", "wohl", "w\u00e4hnt'", "ich", "ein", "Wun\u00b7der", "zu", "sehn", "aus", "des", "fr\u00fc\u00b7he\u00b7ren", "Glau\u00b7bens"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "PTKZU", "VVINF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Zaubergebiet; denn nicht schienst du ein sterbliches Bild.", "tokens": ["Zau\u00b7ber\u00b7ge\u00b7biet", ";", "denn", "nicht", "schienst", "du", "ein", "sterb\u00b7li\u00b7ches", "Bild", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "PTKNEG", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.17": {"text": "Leuchtete nicht im Blick dir der Hoheit Ernst, und erschien nicht", "tokens": ["Leuch\u00b7te\u00b7te", "nicht", "im", "Blick", "dir", "der", "Ho\u00b7heit", "Ernst", ",", "und", "er\u00b7schien", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PTKNEG", "APPRART", "NN", "PPER", "ART", "NN", "NE", "$,", "KON", "VVFIN", "PTKNEG"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.18": {"text": "Geistig, um Wangen und Mund bl\u00fchend, die Rose der Scham?", "tokens": ["Geis\u00b7tig", ",", "um", "Wan\u00b7gen", "und", "Mund", "bl\u00fc\u00b7hend", ",", "die", "Ro\u00b7se", "der", "Scham", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUI", "NN", "KON", "NN", "VVPP", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.19": {"text": "Lieh dein Nahn nicht Licht und Gesang der ver\u00f6deten Waldflur?", "tokens": ["Lieh", "dein", "Nahn", "nicht", "Licht", "und", "Ge\u00b7sang", "der", "ver\u00b7\u00f6\u00b7de\u00b7ten", "Wald\u00b7flur", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKNEG", "NN", "KON", "NN", "ART", "ADJA", "NN", "$."], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Bl\u00fcheten nicht ringsum Blumen aus Haid' und Geklipp?", "tokens": ["Bl\u00fc\u00b7he\u00b7ten", "nicht", "ring\u00b7sum", "Blu\u00b7men", "aus", "Haid'", "und", "Ge\u00b7klipp", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "NN", "APPR", "NE", "KON", "NN", "$."], "meter": "+---+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.21": {"text": "Und doch lie\u00dfest du hold mich nahn, und der zagenden Rede", "tokens": ["Und", "doch", "lie\u00b7\u00dfest", "du", "hold", "mich", "nahn", ",", "und", "der", "za\u00b7gen\u00b7den", "Re\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD", "PPER", "ADJA", "$,", "KON", "ART", "ADJA", "NN"], "meter": "--+-++-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Standest du gern und gabst freundlich dem Worte das Wort.", "tokens": ["Stan\u00b7dest", "du", "gern", "und", "gabst", "freund\u00b7lich", "dem", "Wor\u00b7te", "das", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "KON", "VVFIN", "ADJD", "ART", "NN", "ART", "NN", "$."], "meter": "+--+-++--+--+", "measure": "iambic.hexa.invert"}, "line.23": {"text": "Denn noch h\u00fcllte der Unschuld Flor dir die t\u00e4uschende Welt ein,", "tokens": ["Denn", "noch", "h\u00fcll\u00b7te", "der", "Un\u00b7schuld", "Flor", "dir", "die", "t\u00e4u\u00b7schen\u00b7de", "Welt", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "NN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.24": {"text": "Und nur Engel zu sehn w\u00e4hnte der Engel in dir.", "tokens": ["Und", "nur", "En\u00b7gel", "zu", "sehn", "w\u00e4hn\u00b7te", "der", "En\u00b7gel", "in", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "PTKZU", "VVINF", "VVFIN", "ART", "NN", "APPR", "PPER", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.25": {"text": "Traulich entwandelten wir, und bald nun nahte dem Jagdschlo\u00df,", "tokens": ["Trau\u00b7lich", "ent\u00b7wan\u00b7del\u00b7ten", "wir", ",", "und", "bald", "nun", "nah\u00b7te", "dem", "Jagd\u00b7schlo\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "KON", "ADV", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+-+-+--++", "measure": "dactylic.di.plus"}, "line.26": {"text": "Das im finsteren Hain gl\u00e4nzte, das kosende Paar.", "tokens": ["Das", "im", "fins\u00b7te\u00b7ren", "Hain", "gl\u00e4nz\u00b7te", ",", "das", "ko\u00b7sen\u00b7de", "Paar", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "APPRART", "ADJA", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.27": {"text": "Kundig zeigtest du jetzt mir die einfach edlen Gem\u00e4cher,", "tokens": ["Kun\u00b7dig", "zeig\u00b7test", "du", "jetzt", "mir", "die", "ein\u00b7fach", "ed\u00b7len", "Ge\u00b7m\u00e4\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "PPER", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.28": {"text": "Spartest Sch\u00f6neres noch stets nach dem Sch\u00f6nen mir auf;", "tokens": ["Spar\u00b7test", "Sch\u00f6\u00b7ne\u00b7res", "noch", "stets", "nach", "dem", "Sch\u00f6\u00b7nen", "mir", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "ADV", "APPR", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.29": {"text": "Ach, l\u00e4ngst hatt' ich das Sch\u00f6nste gesehn; dein Auge nur sucht' ich,", "tokens": ["Ach", ",", "l\u00e4ngst", "hatt'", "ich", "das", "Sch\u00f6ns\u00b7te", "ge\u00b7sehn", ";", "dein", "Au\u00b7ge", "nur", "sucht'", "ich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$.", "PPOSAT", "NN", "ADV", "VVFIN", "PPER", "$,"], "meter": "--+--+--+-+-+-+", "measure": "anapaest.tri.plus"}, "line.30": {"text": "Wenn du mir Farb' und Glanz r\u00fchmtest und heiteren Reiz.", "tokens": ["Wenn", "du", "mir", "Fa\u00b7rb'", "und", "Glanz", "r\u00fchm\u00b7test", "und", "hei\u00b7te\u00b7ren", "Reiz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "NN", "KON", "NN", "VVFIN", "KON", "ADJA", "NN", "$."], "meter": "-+-++-++--+--+", "measure": "iambic.septa.relaxed"}, "line.31": {"text": "Doch nicht zittertest du vor dem k\u00fchneren Blick; jungfr\u00e4ulich", "tokens": ["Doch", "nicht", "zit\u00b7ter\u00b7test", "du", "vor", "dem", "k\u00fch\u00b7ne\u00b7ren", "Blick", ";", "jung\u00b7fr\u00e4u\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PTKNEG", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$.", "ADJD"], "meter": "--+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.32": {"text": "Standest du da; nie naht Z\u00fcchtigen niedrer Verdacht.", "tokens": ["Stan\u00b7dest", "du", "da", ";", "nie", "naht", "Z\u00fcch\u00b7ti\u00b7gen", "nie\u00b7drer", "Ver\u00b7dacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$.", "ADV", "VVFIN", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+--++-+", "measure": "trochaic.septa.relaxed"}, "line.33": {"text": "Huldigend beugte mein Herz sich dir; ich zagte der Hoheit", "tokens": ["Hul\u00b7di\u00b7gend", "beug\u00b7te", "mein", "Herz", "sich", "dir", ";", "ich", "zag\u00b7te", "der", "Ho\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "PRF", "PPER", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.34": {"text": "Leuchtendem Strahl, und schnell rief ich das staunende Wort:", "tokens": ["Leuch\u00b7ten\u00b7dem", "Strahl", ",", "und", "schnell", "rief", "ich", "das", "stau\u00b7nen\u00b7de", "Wort", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "ADJD", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+--+-++--+--+", "measure": "iambic.hexa.invert"}, "line.35": {"text": "K\u00f6nigin solltest du seyn: nicht still in des hohen Gebirges", "tokens": ["K\u00f6\u00b7ni\u00b7gin", "soll\u00b7test", "du", "seyn", ":", "nicht", "still", "in", "des", "ho\u00b7hen", "Ge\u00b7bir\u00b7ges"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "VAINF", "$.", "PTKNEG", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.36": {"text": "Oede verbl\u00fchn, nicht fern prangen vom Preise der Welt!", "tokens": ["O\u00b7e\u00b7de", "ver\u00b7bl\u00fchn", ",", "nicht", "fern", "pran\u00b7gen", "vom", "Prei\u00b7se", "der", "Welt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "PTKNEG", "ADJD", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+--+", "measure": "amphibrach.penta.plus"}, "line.37": {"text": "L\u00e4chelnd sahst du mich an und sprachst, aufhebend der Nelken", "tokens": ["L\u00e4\u00b7chelnd", "sahst", "du", "mich", "an", "und", "sprachst", ",", "auf\u00b7he\u00b7bend", "der", "Nel\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "PTKVZ", "KON", "VVFIN", "$,", "VVPP", "ART", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.38": {"text": "Duftigen Strau\u00df und sanft l\u00fcft