{"dta.poem.6028": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Sonne, den Planeten,  \n und vom Firmament.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es mu\u00df ein Jrr-Stern stets in seinem Himmel gehn:", "tokens": ["Es", "mu\u00df", "ein", "Jrr\u00b7S\u00b7tern", "stets", "in", "sei\u00b7nem", "Him\u00b7mel", "gehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er mu\u00df sich um sich selbst, als eine Kugel, drehn.", "tokens": ["Er", "mu\u00df", "sich", "um", "sich", "selbst", ",", "als", "ei\u00b7ne", "Ku\u00b7gel", ",", "drehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "PRF", "ADV", "$,", "KOUS", "ART", "NN", "$,", "CARD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie diese C\u00f6rper nun sich nicht verm\u00f6gen", "tokens": ["Wie", "die\u00b7se", "C\u00f6r\u00b7per", "nun", "sich", "nicht", "ver\u00b7m\u00f6\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PDAT", "NN", "ADV", "PRF", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So schnell, als wie die Himmels-Lufft, zu regen,", "tokens": ["So", "schnell", ",", "als", "wie", "die", "Him\u00b7mels\u00b7Lufft", ",", "zu", "re\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "KOKOM", "ART", "NN", "$,", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die \u00fcber sie best\u00e4ndig sich bewegt;", "tokens": ["Die", "\u00fc\u00b7ber", "sie", "be\u00b7st\u00e4n\u00b7dig", "sich", "be\u00b7wegt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "ADJD", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}