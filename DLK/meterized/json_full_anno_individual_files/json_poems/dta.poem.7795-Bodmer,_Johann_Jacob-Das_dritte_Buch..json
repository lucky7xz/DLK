{"dta.poem.7795": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Das dritte Buch.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198429", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es hatte das Ger\u00fccht von diesen Wundersiegen,", "tokens": ["Es", "hat\u00b7te", "das", "Ge\u00b7r\u00fccht", "von", "die\u00b7sen", "Wun\u00b7der\u00b7sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von der Philister Flucht, von Ecrons Unterliegen", "tokens": ["Von", "der", "Phi\u00b7lis\u00b7ter", "Flucht", ",", "von", "E\u00b7crons", "Un\u00b7ter\u00b7lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "$,", "APPR", "NE", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das gantze Land erf\u00fcllt, gantz Jsrael h\u00f6rt an", "tokens": ["Das", "gant\u00b7ze", "Land", "er\u00b7f\u00fcllt", ",", "gantz", "Js\u00b7rael", "h\u00f6rt", "an"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,", "ADV", "NE", "VVFIN", "PTKVZ"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Verwundert und erfreut, was Gott bey sie gethan.", "tokens": ["Ver\u00b7wun\u00b7dert", "und", "er\u00b7freut", ",", "was", "Gott", "bey", "sie", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "$,", "PRELS", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein jeder Stamm verlangt zu stillen sein Verlangen,", "tokens": ["Ein", "je\u00b7der", "Stamm", "ver\u00b7langt", "zu", "stil\u00b7len", "sein", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVPP", "PTKZU", "VVINF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Den K\u00f6nig und das Heer mit Jauchtzen zu empfangen,", "tokens": ["Den", "K\u00f6\u00b7nig", "und", "das", "Heer", "mit", "Jaucht\u00b7zen", "zu", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Theils gehn nach Gibea den Einzug anzusehn,", "tokens": ["Theils", "gehn", "nach", "Gi\u00b7bea", "den", "Ein\u00b7zug", "an\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NE", "ART", "NN", "VVIZU", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.8": {"text": "Theils gehn dem Lager zu um selber zu verstehn", "tokens": ["Theils", "gehn", "dem", "La\u00b7ger", "zu", "um", "sel\u00b7ber", "zu", "ver\u00b7stehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKZU", "APPR", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Was man von David spricht, von dessen Wunderstreiten,", "tokens": ["Was", "man", "von", "Da\u00b7vid", "spricht", ",", "von", "des\u00b7sen", "Wun\u00b7der\u00b7strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "APPR", "NE", "VVFIN", "$,", "APPR", "PRELAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Von dessen fremden Kampf. Sein Lob weit auszubreiten", "tokens": ["Von", "des\u00b7sen", "frem\u00b7den", "Kampf", ".", "Sein", "Lob", "weit", "aus\u00b7zu\u00b7brei\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "ADJA", "NN", "$.", "PPOSAT", "NN", "ADJD", "VVIZU"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ist jeder Stamm bem\u00fcht, doch Juda f\u00fcr sie all,", "tokens": ["Ist", "je\u00b7der", "Stamm", "be\u00b7m\u00fcht", ",", "doch", "Ju\u00b7da", "f\u00fcr", "sie", "all", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "VVFIN", "$,", "ADV", "NE", "APPR", "PPER", "PIAT", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Weil er ihr Bruder war, und er sie von der Qual", "tokens": ["Weil", "er", "ihr", "Bru\u00b7der", "war", ",", "und", "er", "sie", "von", "der", "Qual"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VAFIN", "$,", "KON", "PPER", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der Dienstbarkeit befreyt, sie traffe es f\u00fcr allen,", "tokens": ["Der", "Dienst\u00b7bar\u00b7keit", "be\u00b7freyt", ",", "sie", "traf\u00b7fe", "es", "f\u00fcr", "al\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PPER", "VVFIN", "PPER", "APPR", "PIAT", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Drum muste ihre Freud nun in die Luft erschallen.", "tokens": ["Drum", "mus\u00b7te", "ih\u00b7re", "Freud", "nun", "in", "die", "Luft", "er\u00b7schal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die B\u00fcrger zu Damin, Asecka und Socho", "tokens": ["Die", "B\u00fcr\u00b7ger", "zu", "Da\u00b7min", ",", "As\u00b7ec\u00b7ka", "und", "So\u00b7cho"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NE", "$,", "NE", "KON", "NE"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Die waren da\u00df sie so befreyet mehr als froh.", "tokens": ["Die", "wa\u00b7ren", "da\u00df", "sie", "so", "be\u00b7fre\u00b7yet", "mehr", "als", "froh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "KOUS", "PPER", "ADV", "VVFIN", "PIS", "KOKOM", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Sie hatten wieder Fried, kein Schrecken mehr sie schreckte,", "tokens": ["Sie", "hat\u00b7ten", "wie\u00b7der", "Fried", ",", "kein", "Schre\u00b7cken", "mehr", "sie", "schreck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "$,", "PIAT", "NN", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die Sicherheit war da, die Davids Steg erweckte,", "tokens": ["Die", "Si\u00b7cher\u00b7heit", "war", "da", ",", "die", "Da\u00b7vids", "Steg", "er\u00b7weck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "PRELS", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Das Feld ward wieder wach, der Landmann f\u00fchrt den Pflug,", "tokens": ["Das", "Feld", "ward", "wie\u00b7der", "wach", ",", "der", "Land\u00b7mann", "f\u00fchrt", "den", "Pflug", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKVZ", "$,", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Man hatte am Geschrey der Krieger \u00fcbrig gnug.", "tokens": ["Man", "hat\u00b7te", "am", "Ge\u00b7schrey", "der", "Krie\u00b7ger", "\u00fcb\u00b7rig", "gnug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPRART", "NN", "ART", "NN", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}