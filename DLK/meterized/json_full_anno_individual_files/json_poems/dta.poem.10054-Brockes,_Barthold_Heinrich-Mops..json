{"dta.poem.10054": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Mops.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Um mich, nach vollbrachter Arbeit, wieder etwas zu er-", "tokens": ["Um", "mich", ",", "nach", "voll\u00b7brach\u00b7ter", "Ar\u00b7beit", ",", "wie\u00b7der", "et\u00b7was", "zu", "er"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUI", "PPER", "$,", "APPR", "ADJA", "NN", "$,", "ADV", "PIS", "APPR", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "frischen,", "tokens": ["fri\u00b7schen", ","], "token_info": ["word", "punct"], "pos": ["ADJA", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Setzt\u2019 ich mich zur Fr\u00fchlings-Zeit j\u00fcngst bey bl\u00fchnden Ro-", "tokens": ["Setzt'", "ich", "mich", "zur", "Fr\u00fch\u00b7lings\u00b7Zeit", "j\u00fcngst", "bey", "bl\u00fchn\u00b7den", "Ro"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "APPRART", "NN", "ADV", "APPR", "ADJA", "TRUNC"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "sen-B\u00fcschen:", "tokens": ["sen\u00b7B\u00fc\u00b7schen", ":"], "token_info": ["word", "punct"], "pos": ["VAINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Und es setzte Mops, mein Hund, sich von ungefehr zu mir.", "tokens": ["Und", "es", "setz\u00b7te", "Mops", ",", "mein", "Hund", ",", "sich", "von", "un\u00b7ge\u00b7fehr", "zu", "mir", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NN", "$,", "PPOSAT", "NN", "$,", "PRF", "APPR", "ADJD", "APPR", "PPER", "$."], "meter": "--+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Ich ergetzte mich von Hertzen an der sch\u00f6nen Staude Zier,", "tokens": ["Ich", "er\u00b7getz\u00b7te", "mich", "von", "Hert\u00b7zen", "an", "der", "sch\u00f6\u00b7nen", "Stau\u00b7de", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Brach von allen eine Rose, deren Farb\u2019 am sch\u00f6nsten spielte,", "tokens": ["Brach", "von", "al\u00b7len", "ei\u00b7ne", "Ro\u00b7se", ",", "de\u00b7ren", "Fa\u00b7rb'", "am", "sch\u00f6ns\u00b7ten", "spiel\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIS", "ART", "NN", "$,", "PRELAT", "NN", "APPRART", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-+-+--+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.8": {"text": "Mit vergn\u00fcgten Fingern ab.", "tokens": ["Mit", "ver\u00b7gn\u00fcg\u00b7ten", "Fin\u00b7gern", "ab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Wie ich nun von ungefehr Mops sie vor die Augen hielte,", "tokens": ["Wie", "ich", "nun", "von", "un\u00b7ge\u00b7fehr", "Mops", "sie", "vor", "die", "Au\u00b7gen", "hiel\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "APPR", "ADJD", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Und sie ihm zu riechen gab;", "tokens": ["Und", "sie", "ihm", "zu", "rie\u00b7chen", "gab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Kehrt er Kopf und Schnautze weg. Ach! fiel mir hier\u00fc-", "tokens": ["Kehrt", "er", "Kopf", "und", "Schnaut\u00b7ze", "weg", ".", "Ach", "!", "fiel", "mir", "hier\u00fc"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "PTKVZ", "$.", "ITJ", "$.", "VVFIN", "PPER", "TRUNC"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.12": {"text": "ber ein:", "tokens": ["ber", "ein", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.13": {"text": "Handeltest du, lieber Mops, so mit Bluhmen doch allein!", "tokens": ["Han\u00b7del\u00b7test", "du", ",", "lie\u00b7ber", "Mops", ",", "so", "mit", "Bluh\u00b7men", "doch", "al\u00b7lein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "ADV", "NN", "$,", "ADV", "APPR", "NN", "ADV", "ADV", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "Aber so l\u00e4sst mancher Mensch der Gesch\u00f6pfe Schmuck und", "tokens": ["A\u00b7ber", "so", "l\u00e4sst", "man\u00b7cher", "Mensch", "der", "Ge\u00b7sch\u00f6p\u00b7fe", "Schmuck", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PIAT", "NN", "ART", "NN", "NN", "KON"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.15": {"text": "Pracht,", "tokens": ["Pracht", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.16": {"text": "Mit nicht minder schneller Abkehr seiner Sinnen aus der", "tokens": ["Mit", "nicht", "min\u00b7der", "schnel\u00b7ler", "Ab\u00b7kehr", "sei\u00b7ner", "Sin\u00b7nen", "aus", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PTKNEG", "ADV", "ADJA", "NN", "PPOSAT", "NN", "APPR", "ART"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.17": {"text": "Acht;", "tokens": ["Acht", ";"], "token_info": ["word", "punct"], "pos": ["CARD", "$."], "meter": "-", "measure": "single.down"}, "line.18": {"text": "Wollt ihr denn, vern\u00fcnftge Menschen, GOttes Wercke, die", "tokens": ["Wollt", "ihr", "denn", ",", "ver\u00b7n\u00fcnft\u00b7ge", "Men\u00b7schen", ",", "Got\u00b7tes", "Wer\u00b7cke", ",", "die"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VMFIN", "PPER", "ADV", "$,", "ADJA", "NN", "$,", "NN", "NN", "$,", "PRELS"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.19": {"text": "so sch\u00f6n,", "tokens": ["so", "sch\u00f6n", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.20": {"text": "Anders nicht, als wie die Hunde, riechen, h\u00f6ren, schmecken,", "tokens": ["An\u00b7ders", "nicht", ",", "als", "wie", "die", "Hun\u00b7de", ",", "rie\u00b7chen", ",", "h\u00f6\u00b7ren", ",", "schme\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "PTKNEG", "$,", "KOUS", "KOKOM", "ART", "NN", "$,", "VVFIN", "$,", "VVINF", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.21": {"text": "sehn?", "tokens": ["sehn", "?"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+", "measure": "single.up"}}}}}