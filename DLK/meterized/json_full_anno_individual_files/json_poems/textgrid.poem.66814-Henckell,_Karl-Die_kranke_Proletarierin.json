{"textgrid.poem.66814": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Die kranke Proletarierin", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O wende mir dein bleiches Haupt", "tokens": ["O", "wen\u00b7de", "mir", "dein", "blei\u00b7ches", "Haupt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit mildem Schwesterblicke zu!", "tokens": ["Mit", "mil\u00b7dem", "Schwes\u00b7ter\u00b7bli\u00b7cke", "zu", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich bin so lust- und gl\u00fcckberaubt", "tokens": ["Ich", "bin", "so", "lust", "und", "gl\u00fcck\u00b7be\u00b7raubt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "TRUNC", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie du, gequ\u00e4ltes Weib, wie du.", "tokens": ["Wie", "du", ",", "ge\u00b7qu\u00e4l\u00b7tes", "Weib", ",", "wie", "du", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "ADJA", "NN", "$,", "PWAV", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das Gift, das durch die Brust dir g\u00e4rt,", "tokens": ["Das", "Gift", ",", "das", "durch", "die", "Brust", "dir", "g\u00e4rt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Siechtumsschlange, die dich bi\u00df,", "tokens": ["Die", "Siech\u00b7tums\u00b7schlan\u00b7ge", ",", "die", "dich", "bi\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sie hat mit Leid auch mich gen\u00e4hrt,", "tokens": ["Sie", "hat", "mit", "Leid", "auch", "mich", "ge\u00b7n\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Getr\u00e4nkt auch mich mit Bitternis.", "tokens": ["Ge\u00b7tr\u00e4nkt", "auch", "mich", "mit", "Bit\u00b7ter\u00b7nis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADV", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O sieh mich nicht so jammervoll,", "tokens": ["O", "sieh", "mich", "nicht", "so", "jam\u00b7mer\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So ohne Ma\u00dfen traurig an!", "tokens": ["So", "oh\u00b7ne", "Ma\u00b7\u00dfen", "trau\u00b7rig", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich will bes\u00e4nftigen deinen Groll,", "tokens": ["Ich", "will", "be\u00b7s\u00e4nf\u00b7ti\u00b7gen", "dei\u00b7nen", "Groll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Will tr\u00f6sten, was ich tr\u00f6sten kann.", "tokens": ["Will", "tr\u00f6s\u00b7ten", ",", "was", "ich", "tr\u00f6s\u00b7ten", "kann", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$,", "PWS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auf deinem Leben lag die Not", "tokens": ["Auf", "dei\u00b7nem", "Le\u00b7ben", "lag", "die", "Not"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit schwarzem Fittich ausgespannt,", "tokens": ["Mit", "schwar\u00b7zem", "Fit\u00b7tich", "aus\u00b7ge\u00b7spannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nun winkt dir der Erl\u00f6ser Tod", "tokens": ["Nun", "winkt", "dir", "der", "Er\u00b7l\u00f6\u00b7ser", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit seiner bleichen Schattenhand.", "tokens": ["Mit", "sei\u00b7ner", "blei\u00b7chen", "Schat\u00b7ten\u00b7hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du warbst, dem holden Licht entr\u00fcckt,", "tokens": ["Du", "warbst", ",", "dem", "hol\u00b7den", "Licht", "ent\u00b7r\u00fcckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Eltern Brot, ein schw\u00e4chlich Kind,", "tokens": ["Den", "El\u00b7tern", "Brot", ",", "ein", "schw\u00e4ch\u00b7lich", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In dunkle Winkel hingedr\u00fcckt,", "tokens": ["In", "dunk\u00b7le", "Win\u00b7kel", "hin\u00b7ge\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wobst du die blauen Augen blind.", "tokens": ["Wobst", "du", "die", "blau\u00b7en", "Au\u00b7gen", "blind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit deinem Manne Tag f\u00fcr Tag", "tokens": ["Mit", "dei\u00b7nem", "Man\u00b7ne", "Tag", "f\u00fcr", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hast du gek\u00e4mpft, ein treues Weib,", "tokens": ["Hast", "du", "ge\u00b7k\u00e4mpft", ",", "ein", "treu\u00b7es", "Weib", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der F\u00e4den Schlag und Gegenschlag", "tokens": ["Der", "F\u00e4\u00b7den", "Schlag", "und", "Ge\u00b7gen\u00b7schlag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "War euer Flitterzeitvertreib.", "tokens": ["War", "eu\u00b7er", "Flit\u00b7ter\u00b7zeit\u00b7ver\u00b7treib", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O weine nicht! o weine nicht!", "tokens": ["O", "wei\u00b7ne", "nicht", "!", "o", "wei\u00b7ne", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "$.", "FM", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun hat der Groll mich selbst gepackt.", "tokens": ["Nun", "hat", "der", "Groll", "mich", "selbst", "ge\u00b7packt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn so das Gl\u00fcck in Scherben bricht,", "tokens": ["Wenn", "so", "das", "Gl\u00fcck", "in", "Scher\u00b7ben", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sch\u00e4umt auf der Zornflut Katarakt.", "tokens": ["Sch\u00e4umt", "auf", "der", "Zorn\u00b7flut", "Ka\u00b7ta\u00b7rakt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Der Vater deiner Kinder sank", "tokens": ["Der", "Va\u00b7ter", "dei\u00b7ner", "Kin\u00b7der", "sank"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Verst\u00fcmmelt in ein schaurig Grab,", "tokens": ["Ver\u00b7st\u00fcm\u00b7melt", "in", "ein", "schau\u00b7rig", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da schafftest du, bis matt und krank", "tokens": ["Da", "schaff\u00b7test", "du", ",", "bis", "matt", "und", "krank"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dir Gott, der Herr, den Abschied gab.", "tokens": ["Dir", "Gott", ",", "der", "Herr", ",", "den", "Ab\u00b7schied", "gab", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "O gib zum Abschied mir die Hand!", "tokens": ["O", "gib", "zum", "Ab\u00b7schied", "mir", "die", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVIMP", "APPRART", "NN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Adern blau Gewebe zuckt,", "tokens": ["Der", "A\u00b7dern", "blau", "Ge\u00b7we\u00b7be", "zuckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Abendd\u00e4mmrung leckt die Wand,", "tokens": ["Die", "A\u00b7bend\u00b7d\u00e4mm\u00b7rung", "leckt", "die", "Wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gleich hat sie dich und mich verschluckt.", "tokens": ["Gleich", "hat", "sie", "dich", "und", "mich", "ver\u00b7schluckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "KON", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Geh du zum tiefen Schlummer ein", "tokens": ["Geh", "du", "zum", "tie\u00b7fen", "Schlum\u00b7mer", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "APPRART", "ADJA", "NN", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und st\u00e4rke deine schwache Brust", "tokens": ["Und", "st\u00e4r\u00b7ke", "dei\u00b7ne", "schwa\u00b7che", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit diesem Ungarfeuerwein", "tokens": ["Mit", "die\u00b7sem", "Un\u00b7gar\u00b7feu\u00b7er\u00b7wein"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und h\u00f6re, was du tr\u00e4umen mu\u00dft:", "tokens": ["Und", "h\u00f6\u00b7re", ",", "was", "du", "tr\u00e4u\u00b7men", "mu\u00dft", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der Knabe, den dein Leib gebar,", "tokens": ["Der", "Kna\u00b7be", ",", "den", "dein", "Leib", "ge\u00b7bar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den du mit Kummer aufges\u00e4ugt,", "tokens": ["Den", "du", "mit", "Kum\u00b7mer", "auf\u00b7ge\u00b7s\u00e4ugt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zieht hoch voran der Heldenschar,", "tokens": ["Zieht", "hoch", "vo\u00b7ran", "der", "Hel\u00b7den\u00b7schar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die alle Not von hinnen scheucht.", "tokens": ["Die", "al\u00b7le", "Not", "von", "hin\u00b7nen", "scheucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sein blaues Auge gl\u00e4nzt voll Kraft", "tokens": ["Sein", "blau\u00b7es", "Au\u00b7ge", "gl\u00e4nzt", "voll", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ins Lichtmeer einer sch\u00f6nern Zeit,", "tokens": ["Ins", "Licht\u00b7meer", "ei\u00b7ner", "sch\u00f6\u00b7nern", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Eisenhand umspannt den Schaft", "tokens": ["Die", "Ei\u00b7sen\u00b7hand", "um\u00b7spannt", "den", "Schaft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der purpurnen Gerechtigkeit.", "tokens": ["Der", "pur\u00b7pur\u00b7nen", "Ge\u00b7rech\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "O wende mir dein bleiches Haupt", "tokens": ["O", "wen\u00b7de", "mir", "dein", "blei\u00b7ches", "Haupt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit mildem Schwesterblicke zu!