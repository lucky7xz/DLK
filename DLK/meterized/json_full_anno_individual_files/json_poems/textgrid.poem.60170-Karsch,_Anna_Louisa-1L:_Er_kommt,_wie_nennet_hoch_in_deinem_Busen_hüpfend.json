{"textgrid.poem.60170": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Er kommt, wie nennet hoch in deinem Busen h\u00fcpfend", "genre": "verse", "period": "N.A.", "pub_year": 1763, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er kommt, wie nennet hoch in deinem Busen h\u00fcpfend", "tokens": ["Er", "kommt", ",", "wie", "nen\u00b7net", "hoch", "in", "dei\u00b7nem", "Bu\u00b7sen", "h\u00fcp\u00b7fend"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein z\u00e4rtlich Herze, den, den alles Sieger hei\u00dft:", "tokens": ["Dein", "z\u00e4rt\u00b7lich", "Her\u00b7ze", ",", "den", ",", "den", "al\u00b7les", "Sie\u00b7ger", "hei\u00dft", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "VVFIN", "$,", "ART", "$,", "ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein Bruder lispelst du, die ganze Seele kn\u00fcpfend", "tokens": ["Mein", "Bru\u00b7der", "lis\u00b7pelst", "du", ",", "die", "gan\u00b7ze", "See\u00b7le", "kn\u00fcp\u00b7fend"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "An seinen grossen Heldengeist.", "tokens": ["An", "sei\u00b7nen", "gros\u00b7sen", "Hel\u00b7den\u00b7geist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {