{"textgrid.poem.38072": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Des Bauerwirths Heimkehr", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es wollt ein Fuhrmann \u00fcber Land fahren,", "tokens": ["Es", "wollt", "ein", "Fuhr\u00b7mann", "\u00fc\u00b7ber", "Land", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Er wollt drey Eimer Wein aufladen,", "tokens": ["Er", "wollt", "drey", "Ei\u00b7mer", "Wein", "auf\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "CARD", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein s\u00fcssen und ein sauern,", "tokens": ["Ein", "s\u00fcs\u00b7sen", "und", "ein", "sau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Altemeralte ein s\u00fcssen und ein sauern.", "tokens": ["Al\u00b7te\u00b7me\u00b7ral\u00b7te", "ein", "s\u00fcs\u00b7sen", "und", "ein", "sau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "KON", "ART", "VVINF", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Und da er \u00fcber die Br\u00fcck hinein fuhr,", "tokens": ["Und", "da", "er", "\u00fc\u00b7ber", "die", "Br\u00fcck", "hin\u00b7ein", "fuhr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "APZR", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da brach ihm sein Geissel und auch sein Schnur,", "tokens": ["Da", "brach", "ihm", "sein", "Geis\u00b7sel", "und", "auch", "sein", "Schnur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "KON", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Schwarzbraune lie\u00df er laufen:", "tokens": ["Schwarz\u00b7brau\u00b7ne", "lie\u00df", "er", "lau\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Altemeralte u.s.w.", "tokens": ["Al\u00b7te\u00b7me\u00b7ral\u00b7te", "u.", "s.", "w."], "token_info": ["word", "abbreviation", "abbreviation", "abbreviation"], "pos": ["NN", "APPR", "VVIMP", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Er kam wohl vor der Frau Wirthin ihr Haus,", "tokens": ["Er", "kam", "wohl", "vor", "der", "Frau", "Wirt\u00b7hin", "ihr", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Frau Wirthin schauet oben heraus", "tokens": ["Frau", "Wirt\u00b7hin", "schau\u00b7et", "o\u00b7ben", "he\u00b7raus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NN", "VVFIN", "ADV", "APZR"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Mit ihren schwarzbraunen Augen:", "tokens": ["Mit", "ih\u00b7ren", "schwarz\u00b7brau\u00b7nen", "Au\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Altemeralte u.s.w.", "tokens": ["Al\u00b7te\u00b7me\u00b7ral\u00b7te", "u.", "s.", "w."], "token_info": ["word", "abbreviation", "abbreviation", "abbreviation"], "pos": ["NN", "APPR", "VVIMP", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Frau Wirthin habt ihr nicht soviel Gewalt,", "tokens": ["Frau", "Wirt\u00b7hin", "habt", "ihr", "nicht", "so\u00b7viel", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "PPER", "PTKNEG", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df ihr ein'n Fuhrmann \u00fcber Nacht behalt,", "tokens": ["Da\u00df", "ihr", "ein'n", "Fuhr\u00b7mann", "\u00fc\u00b7ber", "Nacht", "be\u00b7halt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dazu vier Ro\u00df und Wagen:", "tokens": ["Da\u00b7zu", "vier", "Ro\u00df", "und", "Wa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "CARD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Altemeralte u.s.w.", "tokens": ["Al\u00b7te\u00b7me\u00b7ral\u00b7te", "u.", "s.", "w."], "token_info": ["word", "abbreviation", "abbreviation", "abbreviation"], "pos": ["NN", "APPR", "VVIMP", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Ey so viel Gewalt, das hab ich wol,", "tokens": ["Ey", "so", "viel", "Ge\u00b7walt", ",", "das", "hab", "ich", "wol", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PIAT", "NN", "$,", "PDS", "VAFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Ich wei\u00df nur nicht wie ich mich halten soll,", "tokens": ["Ich", "wei\u00df", "nur", "nicht", "wie", "ich", "mich", "hal\u00b7ten", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "PWAV", "PPER", "PRF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mein Mann ist nicht daheime:", "tokens": ["Mein", "Mann", "ist", "nicht", "da\u00b7hei\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Altemeralte u.s.w.", "tokens": ["Al\u00b7te\u00b7me\u00b7ral\u00b7te", "u.", "s.", "w."], "token_info": ["word", "abbreviation", "abbreviation", "abbreviation"], "pos": ["NN", "APPR", "VVIMP", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Er ist fort, ist \u00fcber Feld,", "tokens": ["Er", "ist", "fort", ",", "ist", "\u00fc\u00b7ber", "Feld", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKVZ", "$,", "VAFIN", "APPR", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Er hat einen Beutel, darin ist kein Geld,", "tokens": ["Er", "hat", "ei\u00b7nen", "Beu\u00b7tel", ",", "da\u00b7rin", "ist", "kein", "Geld", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PAV", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Er wird bald wieder kommen:", "tokens": ["Er", "wird", "bald", "wie\u00b7der", "kom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Altemeralte u.s.w.", "tokens": ["Al\u00b7te\u00b7me\u00b7ral\u00b7te", "u.", "s.", "w."], "token_info": ["word", "abbreviation", "abbreviation", "abbreviation"], "pos": ["NN", "APPR", "VVIMP", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "Und da das Wirthlein heime kam,", "tokens": ["Und", "da", "das", "Wirt\u00b7hlein", "hei\u00b7me", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Frau Wirthin hatt' einen andern Mann,", "tokens": ["Frau", "Wirt\u00b7hin", "hatt'", "ei\u00b7nen", "an\u00b7dern", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Sehr \u00fcbel th\u00e4t er sie schlagen:", "tokens": ["Sehr", "\u00fc\u00b7bel", "th\u00e4t", "er", "sie", "schla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Altemeralte u.s.w.", "tokens": ["Al\u00b7te\u00b7me\u00b7ral\u00b7te", "u.", "s.", "w."], "token_info": ["word", "abbreviation", "abbreviation", "abbreviation"], "pos": ["NN", "APPR", "VVIMP", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.8": {"line.1": {"text": "\u00bbey wollt ihr mich so \u00fcbel schlagen,", "tokens": ["\u00bb", "ey", "wollt", "ihr", "mich", "so", "\u00fc\u00b7bel", "schla\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "VMFIN", "PPER", "PRF", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So will ichs meinem Vater sagen.", "tokens": ["So", "will", "ichs", "mei\u00b7nem", "Va\u00b7ter", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dazu will ichs ja wagen\u00ab:", "tokens": ["Da\u00b7zu", "will", "ichs", "ja", "wa\u00b7gen", "\u00ab", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PAV", "VMFIN", "PIS", "ADV", "VVINF", "$(", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Altemeralte u.s.w.", "tokens": ["Al\u00b7te\u00b7me\u00b7ral\u00b7te", "u.", "s.", "w."], "token_info": ["word", "abbreviation", "abbreviation", "abbreviation"], "pos": ["NN", "APPR", "VVIMP", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.9": {"line.1": {"text": "Willt fort, willt nimmer wieder kommen,", "tokens": ["Willt", "fort", ",", "willt", "nim\u00b7mer", "wie\u00b7der", "kom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "VMFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So la\u00df du mir die Schl\u00fcssel zukommen,", "tokens": ["So", "la\u00df", "du", "mir", "die", "Schl\u00fcs\u00b7sel", "zu\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Schl\u00fcssel zu deinen K\u00e4sten:", "tokens": ["Die", "Schl\u00fcs\u00b7sel", "zu", "dei\u00b7nen", "K\u00e4s\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Altemeralte u.s.w.", "tokens": ["Al\u00b7te\u00b7me\u00b7ral\u00b7te", "u.", "s.", "w."], "token_info": ["word", "abbreviation", "abbreviation", "abbreviation"], "pos": ["NN", "APPR", "VVIMP", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.10": {"line.1": {"text": "Frau Wirthin war so voller List,", "tokens": ["Frau", "Wirt\u00b7hin", "war", "so", "vol\u00b7ler", "List", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie schiebet die Schl\u00fcssel wohl zwischen die Br\u00fcst,", "tokens": ["Sie", "schie\u00b7bet", "die", "Schl\u00fcs\u00b7sel", "wohl", "zwi\u00b7schen", "die", "Br\u00fcst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sie sprach, sie habs verloren:", "tokens": ["Sie", "sprach", ",", "sie", "habs", "ver\u00b7lo\u00b7ren", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Altemeralte u.s.w.", "tokens": ["Al\u00b7te\u00b7me\u00b7ral\u00b7te", "u.", "s.", "w."], "token_info": ["word", "abbreviation", "abbreviation", "abbreviation"], "pos": ["NN", "APPR", "VVIMP", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.11": {"line.1": {"text": "Ey hast du dann die Schl\u00fcssel verloren,", "tokens": ["Ey", "hast", "du", "dann", "die", "Schl\u00fcs\u00b7sel", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So haben wir gute Aexte und Bohrer,", "tokens": ["So", "ha\u00b7ben", "wir", "gu\u00b7te", "A\u00b7ex\u00b7te", "und", "Boh\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Die Kiste k\u00f6nnen wir aufhauen:", "tokens": ["Die", "Kis\u00b7te", "k\u00f6n\u00b7nen", "wir", "auf\u00b7hau\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Altemeralte u.s.w.", "tokens": ["Al\u00b7te\u00b7me\u00b7ral\u00b7te", "u.", "s.", "w."], "token_info": ["word", "abbreviation", "abbreviation", "abbreviation"], "pos": ["NN", "APPR", "VVIMP", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.12": {"line.1": {"text": "Und da die Kiste offen war,", "tokens": ["Und", "da", "die", "Kis\u00b7te", "of\u00b7fen", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Darinnen war ein junger Knab,", "tokens": ["Da\u00b7rin\u00b7nen", "war", "ein", "jun\u00b7ger", "Knab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er bl\u00fcht, als wie die Rosen:", "tokens": ["Er", "bl\u00fcht", ",", "als", "wie", "die", "Ro\u00b7sen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Altemeralte u.s.w.", "tokens": ["Al\u00b7te\u00b7me\u00b7ral\u00b7te", "u.", "s.