{"textgrid.poem.64299": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "18. Lied eines alten schw\u00e4bischen Ritters an seinen Sohn, aus dem zw\u00f6lften Jahrhundert", "genre": "verse", "period": "N.A.", "pub_year": 1774, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sohn, da hast du meinen Speer,", "tokens": ["Sohn", ",", "da", "hast", "du", "mei\u00b7nen", "Speer", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Meinem Arm wird er zu schwer,", "tokens": ["Mei\u00b7nem", "Arm", "wird", "er", "zu", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "PTKA", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nimm den Schild und dies Gescho\u00df,", "tokens": ["Nimm", "den", "Schild", "und", "dies", "Ge\u00b7scho\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "KON", "PDS", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Tummle du forthin mein Ro\u00df!", "tokens": ["Tumm\u00b7le", "du", "for\u00b7thin", "mein", "Ro\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Siehe, dies nun wei\u00dfe Haar", "tokens": ["Sie\u00b7he", ",", "dies", "nun", "wei\u00b7\u00dfe", "Haar"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "PDS", "ADV", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Deckt der Helm schon funfzig Jahr;", "tokens": ["Deckt", "der", "Helm", "schon", "funf\u00b7zig", "Jahr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "CARD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jedes Jahr hat eine Schlacht", "tokens": ["Je\u00b7des", "Jahr", "hat", "ei\u00b7ne", "Schlacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwert und Streitaxt stumpf gemacht.", "tokens": ["Schwert", "und", "Strei\u00b7taxt", "stumpf", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Herzog Rudolf hat dies Schwert,", "tokens": ["Her\u00b7zog", "Ru\u00b7dolf", "hat", "dies", "Schwert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "PDS", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Axt und Kolbe mir verehrt,", "tokens": ["Axt", "und", "Kol\u00b7be", "mir", "ver\u00b7ehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn ich blieb dem Herzog hold", "tokens": ["Denn", "ich", "blieb", "dem", "Her\u00b7zog", "hold"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NE", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und verschm\u00e4hte Heinrichs Sold.", "tokens": ["Und", "ver\u00b7schm\u00e4h\u00b7te", "Hein\u00b7richs", "Sold", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "F\u00fcr die Freiheit flo\u00df das Blut", "tokens": ["F\u00fcr", "die", "Frei\u00b7heit", "flo\u00df", "das", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Seiner Rechten! Rudolfs Mut", "tokens": ["Sei\u00b7ner", "Rech\u00b7ten", "!", "Ru\u00b7dolfs", "Mut"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "NE", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "That mit seiner linken Hand", "tokens": ["That", "mit", "sei\u00b7ner", "lin\u00b7ken", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch dem Franken Widerstand.", "tokens": ["Noch", "dem", "Fran\u00b7ken", "Wi\u00b7der\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Nimm die Wehr, und wappne dich!", "tokens": ["Nimm", "die", "Wehr", ",", "und", "wapp\u00b7ne", "dich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "KON", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kaiser Konrad r\u00fcstet sich!", "tokens": ["Kai\u00b7ser", "Kon\u00b7rad", "r\u00fcs\u00b7tet", "sich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sohn, entlaste mich des Harms", "tokens": ["Sohn", ",", "ent\u00b7las\u00b7te", "mich", "des", "Harms"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PRF", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ob der Schw\u00e4che meines Arms.", "tokens": ["Ob", "der", "Schw\u00e4\u00b7che", "mei\u00b7nes", "Arms", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Z\u00fccke nie umsonst dies Schwert", "tokens": ["Z\u00fc\u00b7cke", "nie", "um\u00b7sonst", "dies", "Schwert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ADV", "PDS", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fcr der V\u00e4ter freien Herd!", "tokens": ["F\u00fcr", "der", "V\u00e4\u00b7ter", "frei\u00b7en", "Herd", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sei behutsam auf der Wacht,", "tokens": ["Sei", "be\u00b7hut\u00b7sam", "auf", "der", "Wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sei ein Wetter in der Schlacht!", "tokens": ["Sei", "ein", "Wet\u00b7ter", "in", "der", "Schlacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Immer sei zum Kampf bereit!", "tokens": ["Im\u00b7mer", "sei", "zum", "Kampf", "be\u00b7reit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Suche stets den w\u00e4rmsten Streit!", "tokens": ["Su\u00b7che", "stets", "den", "w\u00e4rms\u00b7ten", "Streit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schone de\u00df, der wehrlos fleht!", "tokens": ["Scho\u00b7ne", "de\u00df", ",", "der", "wehr\u00b7los", "fleht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Haue den, der widersteht!", "tokens": ["Hau\u00b7e", "den", ",", "der", "wi\u00b7der\u00b7steht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "$,", "PRELS", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Wenn dein Hause wankend steht,", "tokens": ["Wenn", "dein", "Hau\u00b7se", "wan\u00b7kend", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihm umsonst das F\u00e4hnlein weht,", "tokens": ["Ihm", "um\u00b7sonst", "das", "F\u00e4hn\u00b7lein", "weht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Trotze dann, ein fester Turm,", "tokens": ["Trot\u00b7ze", "dann", ",", "ein", "fes\u00b7ter", "Turm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der vereinten Feinde Sturm!", "tokens": ["Der", "ver\u00b7ein\u00b7ten", "Fein\u00b7de", "Sturm", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Deine Br\u00fcder fra\u00df das Schwert,", "tokens": ["Dei\u00b7ne", "Br\u00fc\u00b7der", "fra\u00df", "das", "Schwert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sieben Knaben, Deutschlands wert;", "tokens": ["Sie\u00b7ben", "Kna\u00b7ben", ",", "Deutschlands", "wert", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "NE", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Deine Mutter h\u00e4rmte sich,", "tokens": ["Dei\u00b7ne", "Mut\u00b7ter", "h\u00e4rm\u00b7te", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Stumm und starrend, und erblich.", "tokens": ["Stumm", "und", "star\u00b7rend", ",", "und", "er\u00b7blich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Einsam bin ich nun und schwach;", "tokens": ["Ein\u00b7sam", "bin", "ich", "nun", "und", "schwach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber, Knabe, deine Schmach", "tokens": ["A\u00b7ber", ",", "Kna\u00b7be", ",", "dei\u00b7ne", "Schmach"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "$,", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00e4r' mir herber tausendmal,", "tokens": ["W\u00e4r'", "mir", "her\u00b7ber", "tau\u00b7send\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn der sieben andern Fall.", "tokens": ["Denn", "der", "sie\u00b7ben", "an\u00b7dern", "Fall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "CARD", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Drum so scheue nicht den Tod,", "tokens": ["Drum", "so", "scheu\u00b7e", "nicht", "den", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und vertraue deinem Gott!", "tokens": ["Und", "ver\u00b7trau\u00b7e", "dei\u00b7nem", "Gott", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So du k\u00e4mpfest ritterlich,", "tokens": ["So", "du", "k\u00e4mp\u00b7fest", "rit\u00b7ter\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADJD", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Freut dein alter Vater sich!", "tokens": ["Freut", "dein", "al\u00b7ter", "Va\u00b7ter", "sich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Sohn, da hast du meinen Speer,", "tokens": ["Sohn", ",", "da", "hast", "du", "mei\u00b7nen", "Speer", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Meinem Arm wird er zu schwer,", "tokens": ["Mei\u00b