{"textgrid.poem.42030": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "Die Heimkehr", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vielleicht, da\u00df mich der schlanke Schaft der Birke trug?!", "tokens": ["Viel\u00b7leicht", ",", "da\u00df", "mich", "der", "schlan\u00b7ke", "Schaft", "der", "Bir\u00b7ke", "trug", "?!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nun wurzelt Fu\u00df ins Wurzelwerk der Brombeerranken.", "tokens": ["Nun", "wur\u00b7zelt", "Fu\u00df", "ins", "Wur\u00b7zel\u00b7werk", "der", "Brom\u00b7beer\u00b7ran\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auf blauem Teiche meines Blicks: Libellenflug,", "tokens": ["Auf", "blau\u00b7em", "Tei\u00b7che", "mei\u00b7nes", "Blicks", ":", "Li\u00b7bel\u00b7len\u00b7flug", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "NN", "$.", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gr\u00e4ser im S\u00fcdwind meines Atems wanken", "tokens": ["Gr\u00e4\u00b7ser", "im", "S\u00fcd\u00b7wind", "mei\u00b7nes", "A\u00b7tems", "wan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "PPOSAT", "NN", "VVINF"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Und das in sanften Wogen wehend flie\u00dft:", "tokens": ["Und", "das", "in", "sanf\u00b7ten", "Wo\u00b7gen", "we\u00b7hend", "flie\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Kornfeld des blonden Haares, erdgeneigt.", "tokens": ["Korn\u00b7feld", "des", "blon\u00b7den", "Haa\u00b7res", ",", "erd\u00b7ge\u00b7neigt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "VVPP", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Ein K\u00e4fertraum an Fingerstengeln sprie\u00dft,", "tokens": ["Ein", "K\u00e4\u00b7fer\u00b7traum", "an", "Fin\u00b7gers\u00b7ten\u00b7geln", "sprie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Grille, die aus umlaubter Achselh\u00f6hle geigt \u2013", "tokens": ["Gril\u00b7le", ",", "die", "aus", "um\u00b7laub\u00b7ter", "Ach\u00b7sel\u00b7h\u00f6h\u00b7le", "geigt", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.9": {"text": "Oh, ich ward Tiefe, Weite und der Wald,", "tokens": ["Oh", ",", "ich", "ward", "Tie\u00b7fe", ",", "Wei\u00b7te", "und", "der", "Wald", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "NN", "$,", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Der m\u00fcdverh\u00e4ngter Wimper gr\u00fcn entd\u00e4mmert,", "tokens": ["Der", "m\u00fcd\u00b7ver\u00b7h\u00e4ng\u00b7ter", "Wim\u00b7per