{"textgrid.poem.60232": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein armer Ehegatte,", "genre": "verse", "period": "N.A.", "pub_year": 1756, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein armer Ehegatte,", "tokens": ["Ein", "ar\u00b7mer", "E\u00b7he\u00b7gat\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der ohne seine Schuld", "tokens": ["Der", "oh\u00b7ne", "sei\u00b7ne", "Schuld"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die H\u00f6ll' auf Erden hatte,", "tokens": ["Die", "H\u00f6ll'", "auf", "Er\u00b7den", "hat\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ward endlich der Geduld", "tokens": ["Ward", "end\u00b7lich", "der", "Ge\u00b7duld"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nach langen Jahren m\u00fcde,", "tokens": ["Nach", "lan\u00b7gen", "Jah\u00b7ren", "m\u00fc\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und schaffte schnell und klug", "tokens": ["Und", "schaff\u00b7te", "schnell", "und", "klug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sich vor dem Engel Friede,", "tokens": ["Sich", "vor", "dem", "En\u00b7gel", "Frie\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der ihn mit F\u00e4usten schlug.", "tokens": ["Der", "ihn", "mit", "F\u00e4us\u00b7ten", "schlug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sein Weib war bitterb\u00f6se,", "tokens": ["Sein", "Weib", "war", "bit\u00b7ter\u00b7b\u00f6\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Tobsucht rief aus ihr,", "tokens": ["Die", "Tob\u00b7sucht", "rief", "aus", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bey manchem Zankget\u00f6se:", "tokens": ["Bey", "man\u00b7chem", "Zank\u00b7ge\u00b7t\u00f6\u00b7se", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Leides thu ich mir!", "tokens": ["Ein", "Lei\u00b7des", "thu", "ich", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ja ja, du Weiberhasser,", "tokens": ["Ja", "ja", ",", "du", "Wei\u00b7ber\u00b7has\u00b7ser", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "PTKANT", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Du Teufel, der du bist,", "tokens": ["Du", "Teu\u00b7fel", ",", "der", "du", "bist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PRELS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich springe noch ins Wasser,", "tokens": ["Ich", "sprin\u00b7ge", "noch", "ins", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wo es am tiefsten ist.", "tokens": ["Wo", "es", "am", "tiefs\u00b7ten", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "ADJA", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie sprachs zu tausendmalen,", "tokens": ["Sie", "sprachs", "zu", "tau\u00b7send\u00b7ma\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und sprang ins Wasser nie.", "tokens": ["Und", "sprang", "ins", "Was\u00b7ser", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf neue M\u00e4nnerqualen", "tokens": ["Auf", "neu\u00b7e", "M\u00e4n\u00b7ner\u00b7qua\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dacht ihre Seele fr\u00fch,", "tokens": ["Dacht", "ih\u00b7re", "See\u00b7le", "fr\u00fch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sobald der Tag erwachte.", "tokens": ["So\u00b7bald", "der", "Tag", "er\u00b7wach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ihr D\u00e4mon, schwarz und klein,", "tokens": ["Ihr", "D\u00e4\u00b7mon", ",", "schwarz", "und", "klein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Blies ihr im Traum bey Nachte", "tokens": ["Blies", "ihr", "im", "Traum", "bey", "Nach\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den Stoff zum Zanken ein.", "tokens": ["Den", "Stoff", "zum", "Zan\u00b7ken", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Einst fieng beym Abendtische", "tokens": ["Einst", "fi\u00b7eng", "beym", "A\u00b7bend\u00b7ti\u00b7sche"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN"], "meter": "+-+-+---", "measure": "unknown.measure.tri"}, "line.2": {"text": "Ihr Zorn zu donnern an,", "tokens": ["Ihr", "Zorn", "zu", "don\u00b7nern", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und still, wie stumme Fische,", "tokens": ["Und", "still", ",", "wie", "stum\u00b7me", "Fi\u00b7sche", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Blieb ihr geplagter Mann;", "tokens": ["Blieb", "ihr", "ge\u00b7plag\u00b7ter", "Mann", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Lie\u00df ihrer frechen Zunge", "tokens": ["Lie\u00df", "ih\u00b7rer", "fre\u00b7chen", "Zun\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den Z\u00fcgel \u2013 gab ihr nach,", "tokens": ["Den", "Z\u00fc\u00b7gel", "\u2013", "gab", "ihr", "nach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Bis sie vom Wassersprunge", "tokens": ["Bis", "sie", "vom", "Was\u00b7ser\u00b7sprun\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit blauen Lefzen sprach.", "tokens": ["Mit", "blau\u00b7en", "Lef\u00b7zen", "sprach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da warf der Mann sein Messer", "tokens": ["Da", "warf", "der", "Mann", "sein", "Mes\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Tief in den Tisch, und ri\u00df", "tokens": ["Tief", "in", "den", "Tisch", ",", "und", "ri\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "$,", "KON", "VVFIN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Das Weib an ein Gew\u00e4sser.", "tokens": ["Das", "Weib", "an", "ein", "Ge\u00b7w\u00e4s\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hier, sprach er: Thue die\u00df", "tokens": ["Hier", ",", "sprach", "er", ":", "Thue", "die\u00df"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "VVFIN", "PPER", "$.", "VVFIN", "PDS"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Was du zu thun beschlossen.", "tokens": ["Was", "du", "zu", "thun", "be\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hier springe mir hinab. \u2013", "tokens": ["Hier", "sprin\u00b7ge", "mir", "hin\u00b7ab", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Hier sah sie, furchtbegossen,", "tokens": ["Hier", "sah", "sie", ",", "furcht\u00b7be\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ins grause Wassergrab.", "tokens": ["Ins", "grau\u00b7se", "Was\u00b7ser\u00b7grab", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Sie hieng an seinen Armen", "tokens": ["Sie", "hieng", "an", "sei\u00b7nen", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und f\u00fchlte Todesquaal;", "tokens": ["Und", "f\u00fchl\u00b7te", "To\u00b7des\u00b7qua\u00b7al", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er aber, ohn Erbarmen,", "tokens": ["Er", "a\u00b7ber", ",", "ohn", "Er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "KOUI", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er tauchte siebenmal", "tokens": ["Er", "tauch\u00b7te", "sie\u00b7ben\u00b7mal"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie unter mit dem Kopfe,", "tokens": ["Sie", "un\u00b7ter", "mit", "dem", "Kop\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bis sie die Luft verlor:", "tokens": ["Bis", "sie", "die", "Luft", "ver\u00b7lor", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und hub sie drauf beym Zopfe", "tokens": ["Und", "hub", "sie", "drauf", "beym", "Zop\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PAV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Stark aus der Fluth empor.", "tokens": ["Stark", "aus", "der", "Fluth", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.7": {"line.1": {"text": "Das Mittel half geschwinde;", "tokens": ["Das", "Mit\u00b7tel", "half", "ge\u00b7schwin\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie seufzte leichenbla\u00df:", "tokens": ["Sie", "seufz\u00b7te", "lei\u00b7chen\u00b7bla\u00df", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ach! M\u00e4nnchen, sey gelinde,", "tokens": ["Ach", "!", "M\u00e4nn\u00b7chen", ",", "sey", "ge\u00b7lin\u00b7de", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "NN", "$,", "VAFIN", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ach! liebes M\u00e4nnchen, la\u00df", "tokens": ["Ach", "!", "lie\u00b7bes", "M\u00e4nn\u00b7chen", ",", "la\u00df"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["ITJ", "$.", "ADJA", "NN", "$,", "XY"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mich diesesmal nur leben,", "tokens": ["Mich", "die\u00b7ses\u00b7mal", "nur", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und ende meine Pein,", "tokens": ["Und", "en\u00b7de", "mei\u00b7ne", "Pein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich will mich gern bestreben,", "tokens": ["Ich", "will", "mich", "gern", "be\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Recht l\u00e4mmerfromm zu seyn.", "tokens": ["Recht", "l\u00e4m\u00b7mer\u00b7fromm", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Der Mann lie\u00df sich bedingen,", "tokens": ["Der", "Mann", "lie\u00df", "sich", "be\u00b7din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Weib ward zahm gemacht,", "tokens": ["Das", "Weib", "ward", "zahm", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und an kein Wasserspringen", "tokens": ["Und", "an", "kein", "Was\u00b7ser\u00b7sprin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ward k\u00fcnftig mehr gedacht.", "tokens": ["Ward", "k\u00fcnf\u00b7tig", "mehr", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie lebten, sanft wie Tauben,", "tokens": ["Sie", "leb\u00b7ten", ",", "sanft", "wie", "Tau\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von keinem Zank gequ\u00e4lt,", "tokens": ["Von", "kei\u00b7nem", "Zank", "ge\u00b7qu\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und alle Welt wirds glauben", "tokens": ["Und", "al\u00b7le", "Welt", "wirds", "glau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VAFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Weil es ein Weib erz\u00e4hlt.", "tokens": ["Weil", "es", "ein", "Weib", "er\u00b7z\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ein armer Ehegatte,", "tokens": ["Ein", "ar\u00b7mer", "E\u00b7he\u00b7gat\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der ohne seine Schuld", "tokens": ["Der", "oh\u00b7ne", "sei\u00b7ne", "Schuld"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die H\u00f6ll' auf Erden hatte,", "tokens": ["Die", "H\u00f6ll'", "auf", "Er\u00b7den", "hat\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ward endlich der Geduld", "tokens": ["Ward", "end\u00b7lich", "der", "Ge\u00b7duld"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nach langen Jahren m\u00fcde,", "tokens": ["Nach", "lan\u00b7gen", "Jah\u00b7ren", "m\u00fc\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und schaffte schnell und klug", "tokens": ["Und", "schaff\u00b7te", "schnell", "und", "klug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sich vor dem Engel Friede,", "tokens": ["Sich", "vor", "dem", "En\u00b7gel", "Frie\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der ihn mit F\u00e4usten schlug.", "tokens": ["Der", "ihn", "mit", "F\u00e4us\u00b7ten", "schlug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Sein Weib war bitterb\u00f6se,", "tokens": ["Sein", "Weib", "war", "bit\u00b7ter\u00b7b\u00f6\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Tobsucht rief aus ihr,", "tokens": ["Die", "Tob\u00b7sucht", "rief", "aus", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bey manchem Zankget\u00f6se:", "tokens": ["Bey", "man\u00b7chem", "Zank\u00b7ge\u00b7t\u00f6\u00b7se", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Leides thu ich mir!", "tokens": ["Ein", "Lei\u00b7des", "thu", "ich", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ja ja, du Weiberhasser,", "tokens": ["Ja", "ja", ",", "du", "Wei\u00b7ber\u00b7has\u00b7ser", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "PTKANT", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Du Teufel, der du bist,", "tokens": ["Du", "Teu\u00b7fel", ",", "der", "du", "bist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PRELS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich springe noch ins Wasser,", "tokens": ["Ich", "sprin\u00b7ge", "noch", "ins", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wo es am tiefsten ist.", "tokens": ["Wo", "es", "am", "tiefs\u00b7ten", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "ADJA", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Sie sprachs zu tausendmalen,", "tokens": ["Sie", "sprachs", "zu", "tau\u00b7send\u00b7ma\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und sprang ins Wasser nie.", "tokens": ["Und", "sprang", "ins", "Was\u00b7ser", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf neue M\u00e4nnerqualen", "tokens": ["Auf", "neu\u00b7e", "M\u00e4n\u00b7ner\u00b7qua\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dacht ihre Seele fr\u00fch,", "tokens": ["Dacht", "ih\u00b7re", "See\u00b7le", "fr\u00fch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sobald der Tag erwachte.", "tokens": ["So\u00b7bald", "der", "Tag", "er\u00b7wach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ihr D\u00e4mon, schwarz und klein,", "tokens": ["Ihr", "D\u00e4\u00b7mon", ",", "schwarz", "und", "klein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Blies ihr im Traum bey Nachte", "tokens": ["Blies", "ihr", "im", "Traum", "bey", "Nach\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den Stoff zum Zanken ein.", "tokens": ["Den", "Stoff", "zum", "Zan\u00b7ken", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Einst fieng beym Abendtische", "tokens": ["Einst", "fi\u00b7eng", "beym", "A\u00b7bend\u00b7ti\u00b7sche"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN"], "meter": "+-+-+---", "measure": "unknown.measure.tri"}, "line.2": {"text": "Ihr Zorn zu donnern an,", "tokens": ["Ihr", "Zorn", "zu", "don\u00b7nern", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und still, wie stumme Fische,", "tokens": ["Und", "still", ",", "wie", "stum\u00b7me", "Fi\u00b7sche", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Blieb ihr geplagter Mann;", "tokens": ["Blieb", "ihr", "ge\u00b7plag\u00b7ter", "Mann", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Lie\u00df ihrer frechen Zunge", "tokens": ["Lie\u00df", "ih\u00b7rer", "fre\u00b7chen", "Zun\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den Z\u00fcgel \u2013 gab ihr nach,", "tokens": ["Den", "Z\u00fc\u00b7gel", "\u2013", "gab", "ihr", "nach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Bis sie vom Wassersprunge", "tokens": ["Bis", "sie", "vom", "Was\u00b7ser\u00b7sprun\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit blauen Lefzen sprach.", "tokens": ["Mit", "blau\u00b7en", "Lef\u00b7zen", "sprach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Da warf der Mann sein Messer", "tokens": ["Da", "warf", "der", "Mann", "sein", "Mes\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Tief in den Tisch, und ri\u00df", "tokens": ["Tief", "in", "den", "Tisch", ",", "und", "ri\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "$,", "KON", "VVFIN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Das Weib an ein Gew\u00e4sser.", "tokens": ["Das", "Weib", "an", "ein", "Ge\u00b7w\u00e4s\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hier, sprach er: Thue die\u00df", "tokens": ["Hier", ",", "sprach", "er", ":", "Thue", "die\u00df"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "VVFIN", "PPER", "$.", "VVFIN", "PDS"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Was du zu thun beschlossen.", "tokens": ["Was", "du", "zu", "thun", "be\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hier springe mir hinab. \u2013", "tokens": ["Hier", "sprin\u00b7ge", "mir", "hin\u00b7ab", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Hier sah sie, furchtbegossen,", "tokens": ["Hier", "sah", "sie", ",", "furcht\u00b7be\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ins grause Wassergrab.", "tokens": ["Ins", "grau\u00b7se", "Was\u00b7ser\u00b7grab", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Sie hieng an seinen Armen", "tokens": ["Sie", "hieng", "an", "sei\u00b7nen", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und f\u00fchlte Todesquaal;", "tokens": ["Und", "f\u00fchl\u00b7te", "To\u00b7des\u00b7qua\u00b7al", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er aber, ohn Erbarmen,", "tokens": ["Er", "a\u00b7ber", ",", "ohn", "Er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "KOUI", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er tauchte siebenmal", "tokens": ["Er", "tauch\u00b7te", "sie\u00b7ben\u00b7mal"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie unter mit dem Kopfe,", "tokens": ["Sie", "un\u00b7ter", "mit", "dem", "Kop\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bis sie die Luft verlor:", "tokens": ["Bis", "sie", "die", "Luft", "ver\u00b7lor", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und hub sie drauf beym Zopfe", "tokens": ["Und", "hub", "sie", "drauf", "beym", "Zop\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PAV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Stark aus der Fluth empor.", "tokens": ["Stark", "aus", "der", "Fluth", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.15": {"line.1": {"text": "Das Mittel half geschwinde;", "tokens": ["Das", "Mit\u00b7tel", "half", "ge\u00b7schwin\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie seufzte leichenbla\u00df:", "tokens": ["Sie", "seufz\u00b7te", "lei\u00b7chen\u00b7bla\u00df", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ach! M\u00e4nnchen, sey gelinde,", "tokens": ["Ach", "!", "M\u00e4nn\u00b7chen", ",", "sey", "ge\u00b7lin\u00b7de", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "NN", "$,", "VAFIN", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ach! liebes M\u00e4nnchen, la\u00df", "tokens": ["Ach", "!", "lie\u00b7bes", "M\u00e4nn\u00b7chen", ",", "la\u00df"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["ITJ", "$.", "ADJA", "NN", "$,", "XY"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mich diesesmal nur leben,", "tokens": ["Mich", "die\u00b7ses\u00b7mal", "nur", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und ende meine Pein,", "tokens": ["Und", "en\u00b7de", "mei\u00b7ne", "Pein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich will mich gern bestreben,", "tokens": ["Ich", "will", "mich", "gern", "be\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Recht l\u00e4mmerfromm zu seyn.", "tokens": ["Recht", "l\u00e4m\u00b7mer\u00b7fromm", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Der Mann lie\u00df sich bedingen,", "tokens": ["Der", "Mann", "lie\u00df", "sich", "be\u00b7din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Weib ward zahm gemacht,", "tokens": ["Das", "Weib", "ward", "zahm", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und an kein Wasserspringen", "tokens": ["Und", "an", "kein", "Was\u00b7ser\u00b7sprin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ward k\u00fcnftig mehr gedacht.", "tokens": ["Ward", "k\u00fcnf\u00b7tig", "mehr", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie lebten, sanft wie Tauben,", "tokens": ["Sie", "leb\u00b7ten", ",", "sanft", "wie", "Tau\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von keinem Zank gequ\u00e4lt,", "tokens": ["Von", "kei\u00b7nem", "Zank", "ge\u00b7qu\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und alle Welt wirds glauben", "tokens": ["Und", "al\u00b7le", "Welt", "wirds", "glau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VAFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Weil es ein Weib erz\u00e4hlt.", "tokens": ["Weil", "es", "ein", "Weib", "er\u00b7z\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}