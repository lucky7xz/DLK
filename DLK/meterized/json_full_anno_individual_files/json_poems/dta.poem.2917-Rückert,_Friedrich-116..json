{"dta.poem.2917": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "116.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was dir mit Einem Mund bewundernd alle preisen,", "tokens": ["Was", "dir", "mit", "Ei\u00b7nem", "Mund", "be\u00b7wun\u00b7dernd", "al\u00b7le", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ART", "NN", "ADJD", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Woran sich dir nichts will Bewundernswerthes weisen;", "tokens": ["Wo\u00b7ran", "sich", "dir", "nichts", "will", "Be\u00b7wun\u00b7derns\u00b7wert\u00b7hes", "wei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PPER", "PIS", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Es mu\u00df doch etwas seyn daran, wonach sie rennen,", "tokens": ["Es", "mu\u00df", "doch", "et\u00b7was", "seyn", "da\u00b7ran", ",", "wo\u00b7nach", "sie", "ren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PIS", "VAINF", "PAV", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du aber raste nicht dasselbe zu erkennen;", "tokens": ["Du", "a\u00b7ber", "ras\u00b7te", "nicht", "das\u00b7sel\u00b7be", "zu", "er\u00b7ken\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PTKNEG", "PDAT", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nicht, um es selber nun in gleichem Schein zu sehn,", "tokens": ["Nicht", ",", "um", "es", "sel\u00b7ber", "nun", "in", "glei\u00b7chem", "Schein", "zu", "sehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "KOUI", "PPER", "ADV", "ADV", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur die Bewundrung als vern\u00fcnftig einzusehn.", "tokens": ["Nur", "die", "Be\u00b7wund\u00b7rung", "als", "ver\u00b7n\u00fcnf\u00b7tig", "ein\u00b7zu\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KOKOM", "ADJD", "VVINF", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}}}}