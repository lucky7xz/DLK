{"textgrid.poem.31593": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: So viel / als der Igel Stachel / darff ich Waffen widers Gl\u00fcck:", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So viel / als der Igel Stachel / darff ich Waffen widers Gl\u00fcck:", "tokens": ["So", "viel", "/", "als", "der", "I\u00b7gel", "Sta\u00b7chel", "/", "darff", "ich", "Waf\u00b7fen", "wi\u00b7ders", "Gl\u00fcck", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$(", "KOUS", "ART", "NN", "NE", "$(", "VMFIN", "PPER", "NN", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "da\u00df ich aller Ort und End mich vor seinem Anlauf schutze /", "tokens": ["da\u00df", "ich", "al\u00b7ler", "Ort", "und", "End", "mich", "vor", "sei\u00b7nem", "An\u00b7lauf", "schut\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "KON", "NN", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "und die k\u00fchn Verletzungs-Hand / vor dem Tugend Angriff stutze.", "tokens": ["und", "die", "k\u00fchn", "Ver\u00b7let\u00b7zungs\u00b7Hand", "/", "vor", "dem", "Tu\u00b7gend", "An\u00b7griff", "stut\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$(", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Ich hab mich mit ihm zu schlagen / alle Stund und Augenblick.", "tokens": ["Ich", "hab", "mich", "mit", "ihm", "zu", "schla\u00b7gen", "/", "al\u00b7le", "Stund", "und", "Au\u00b7gen\u00b7blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "PPER", "PTKZU", "VVINF", "$(", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.5": {"text": "Es steckt wohl / als wie der Igel / voller Stachel / voller T\u00fcck.", "tokens": ["Es", "steckt", "wohl", "/", "als", "wie", "der", "I\u00b7gel", "/", "vol\u00b7ler", "Sta\u00b7chel", "/", "vol\u00b7ler", "T\u00fcck", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$(", "KOUS", "KOKOM", "ART", "NN", "$(", "ADJA", "NN", "$(", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "Doch mu\u00df nach dem Tugend-Klang es mir danzen ihm zu Trutze /", "tokens": ["Doch", "mu\u00df", "nach", "dem", "Tu\u00b7gen\u00b7dKlang", "es", "mir", "dan\u00b7zen", "ihm", "zu", "Trut\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "ART", "NN", "PPER", "PPER", "VVFIN", "PPER", "APPR", "NN", "$("], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "wann ich ihm den Vnbestand / und das schnell vergehn aufmutze.", "tokens": ["wann", "ich", "ihm", "den", "Vn\u00b7be\u00b7stand", "/", "und", "das", "schnell", "ver\u00b7gehn", "auf\u00b7mut\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ART", "NN", "$(", "KON", "ART", "ADJD", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"te