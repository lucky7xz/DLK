{"textgrid.poem.35770": {"metadata": {"author": {"name": "Tiedge, Christoph August", "birth": "N.A.", "death": "N.A."}, "title": "10. An die Deutschen!", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6rt, welch ein Ruf! der mit dem Lerchenschlage", "tokens": ["H\u00f6rt", ",", "welch", "ein", "Ruf", "!", "der", "mit", "dem", "Ler\u00b7chen\u00b7schla\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAT", "ART", "NN", "$.", "ART", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Fern her die blaue Fr\u00fchlingsluft erf\u00fcllt,", "tokens": ["Fern", "her", "die", "blau\u00b7e", "Fr\u00fch\u00b7lings\u00b7luft", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APZR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und im Gem\u00fct der nachtverh\u00fcllten Klage", "tokens": ["Und", "im", "Ge\u00b7m\u00fct", "der", "nacht\u00b7ver\u00b7h\u00fcll\u00b7ten", "Kla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Lichtblick neuer Hoffnungen enth\u00fcllt!", "tokens": ["Den", "Licht\u00b7blick", "neu\u00b7er", "Hoff\u00b7nun\u00b7gen", "ent\u00b7h\u00fcllt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Verk\u00fcndet er den festlichsten der Tage,", "tokens": ["Ver\u00b7k\u00fcn\u00b7det", "er", "den", "fest\u00b7lichs\u00b7ten", "der", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "ART", "NN", "$,"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Den Richttag Gottes, der die Zeit erf\u00fcllt?", "tokens": ["Den", "Richt\u00b7tag", "Got\u00b7tes", ",", "der", "die", "Zeit", "er\u00b7f\u00fcllt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Es t\u00f6net, wie mit langverhaltnem Grimme;", "tokens": ["Es", "t\u00f6\u00b7net", ",", "wie", "mit", "lang\u00b7ver\u00b7halt\u00b7nem", "Grim\u00b7me", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Vom Donaustrom her\u00fcber schallt die Stimme.", "tokens": ["Vom", "Do\u00b7naus\u00b7trom", "her\u00b7\u00fc\u00b7ber", "schallt", "die", "Stim\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da spiegelt sich das neue Morgenrot.", "tokens": ["Da", "spie\u00b7gelt", "sich", "das", "neu\u00b7e", "Mor\u00b7gen\u00b7rot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf, deutsche S\u00f6hne, wagt, euch zu erheben!", "tokens": ["Auf", ",", "deut\u00b7sche", "S\u00f6h\u00b7ne", ",", "wagt", ",", "euch", "zu", "er\u00b7he\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "ADJA", "NN", "$,", "VVFIN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Unwillig braust der Rhein durch seine Reben,", "tokens": ["Un\u00b7wil\u00b7lig", "braust", "der", "Rhein", "durch", "sei\u00b7ne", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NE", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "L\u00f6st ihn und euch vom fremden Machtgebot.", "tokens": ["L\u00f6st", "ihn", "und", "euch", "vom", "frem\u00b7den", "Macht\u00b7ge\u00b7bot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Sklave lebt nur halb, und halbes Leben,", "tokens": ["Der", "Skla\u00b7ve", "lebt", "nur", "halb", ",", "und", "hal\u00b7bes", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$,", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nichts weiter ist's, als ein gef\u00fchlter Tod.", "tokens": ["Nichts", "wei\u00b7ter", "ist's", ",", "als", "ein", "ge\u00b7f\u00fchl\u00b7ter", "Tod", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VAFIN", "$,", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "O, richtet euch mit frischem Herzensschlage", "tokens": ["O", ",", "rich\u00b7tet", "euch", "mit", "fri\u00b7schem", "Her\u00b7zens\u00b7schla\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Empor zum gro\u00dfen Auferstehungstage!", "tokens": ["Em\u00b7por", "zum", "gro\u00b7\u00dfen", "Auf\u00b7er\u00b7ste\u00b7hungs\u00b7ta\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nur Wollen gilt's; im Wollen ruht die Kraft,", "tokens": ["Nur", "Wol\u00b7len", "gilt's", ";", "im", "Wol\u00b7len", "ruht", "die", "Kraft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "$.", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nur Wollen gilt's, um Felsen zu zersplittern;", "tokens": ["Nur", "Wol\u00b7len", "gilt's", ",", "um", "Fel\u00b7sen", "zu", "zer\u00b7split\u00b7tern", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "$,", "KOUI", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und deutsche F\u00fcrsten sollten in der Haft", "tokens": ["Und", "deut\u00b7sche", "F\u00fcrs\u00b7ten", "soll\u00b7ten", "in", "der", "Haft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VMFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Kettenschmach vor einem Gaukler zittern? \u2013", "tokens": ["Der", "Ket\u00b7ten\u00b7schmach", "vor", "ei\u00b7nem", "Gauk\u00b7ler", "zit\u00b7tern", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Brecht st\u00fcrmend auf, gleich brausenden Gewittern!", "tokens": ["Brecht", "st\u00fcr\u00b7mend", "auf", ",", "gleich", "brau\u00b7sen\u00b7den", "Ge\u00b7wit\u00b7tern", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PTKVZ", "$,", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Vers\u00f6hnt den Geist der alten Heldenschaft,", "tokens": ["Ver\u00b7s\u00f6hnt", "den", "Geist", "der", "al\u00b7ten", "Hel\u00b7den\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und reicht von S\u00fcd und Nord euch treu die H\u00e4nde,", "tokens": ["Und", "reicht", "von", "S\u00fcd", "und", "Nord", "euch", "treu", "die", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NE", "PPER", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df keine Schmach das Heiligste mehr sch\u00e4nde!", "tokens": ["Da\u00df", "kei\u00b7ne", "Schmach", "das", "Hei\u00b7ligs\u00b7te", "mehr", "sch\u00e4n\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nur Wollen gilt's! da seht! die L\u00fcgenbrut,", "tokens": ["Nur", "Wol\u00b7len", "gilt's", "!", "da", "seht", "!", "die", "L\u00fc\u00b7gen\u00b7brut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "$.", "ADV", "VVFIN", "$.", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ob sie auch prunkend Sieg auf Sieg entf\u00fchrte,", "tokens": ["Ob", "sie", "auch", "prun\u00b7kend", "Sieg", "auf", "Sieg", "ent\u00b7f\u00fchr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bekennt durch sich, da\u00df ihr kein Sieg geb\u00fchrte.", "tokens": ["Be\u00b7kennt", "durch", "sich", ",", "da\u00df", "ihr", "kein", "Sieg", "ge\u00b7b\u00fchr\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PRF", "$,", "KOUS", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Geist der Wahrheit sei mit eurem Mut,", "tokens": ["Der", "Geist", "der", "Wahr\u00b7heit", "sei", "mit", "eu\u00b7rem", "Mut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Den ungebeugt die Knechtschaft nicht ber\u00fchrte.", "tokens": ["Den", "un\u00b7ge\u00b7beugt", "die", "Knecht\u00b7schaft", "nicht", "be\u00b7r\u00fchr\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Am Schrei der Not entz\u00fcndet eure Glut!", "tokens": ["Am", "Schrei", "der", "Not", "ent\u00b7z\u00fcn\u00b7det", "eu\u00b7re", "Glut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Vernehmt, Geborene von deutschen M\u00fcttern,", "tokens": ["Ver\u00b7nehmt", ",", "Ge\u00b7bo\u00b7re\u00b7ne", "von", "deut\u00b7schen", "M\u00fct\u00b7tern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.8": {"text": "Vernehmt den Ruf, um euch empor zu sch\u00fcttern!", "tokens": ["Ver\u00b7nehmt", "den", "Ruf", ",", "um", "euch", "em\u00b7por", "zu", "sch\u00fct\u00b7tern", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "KOUI", "PPER", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Seht die Gestalt, mit Fesseln an der Hand,", "tokens": ["Seht", "die", "Ge\u00b7stalt", ",", "mit", "Fes\u00b7seln", "an", "der", "Hand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da liegend, wie ein Opfertier gebunden,", "tokens": ["Da", "lie\u00b7gend", ",", "wie", "ein", "Op\u00b7fer\u00b7tier", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "PWAV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aus dem schon halb das Leben weggeschwunden:", "tokens": ["Aus", "dem", "schon", "halb", "das", "Le\u00b7ben", "weg\u00b7ge\u00b7schwun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das ist, entsetzt euch! euer Vaterland!", "tokens": ["Das", "ist", ",", "ent\u00b7setzt", "euch", "!", "eu\u00b7er", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "VVFIN", "PPER", "$.", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und welch ein Vampir saugt an seinen Wunden?", "tokens": ["Und", "welch", "ein", "Vam\u00b7pir", "saugt", "an", "sei\u00b7nen", "Wun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Das ist der Friede, der das Opfer band.", "tokens": ["Das", "ist", "der", "Frie\u00b7de", ",", "der", "das", "Op\u00b7fer", "band", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So ganz ist er zur H\u00f6llenkunst geworden,", "tokens": ["So", "ganz", "ist", "er", "zur", "H\u00f6l\u00b7len\u00b7kunst", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "APPRART", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die halb erw\u00fcrgt, um l\u00e4nger zu ermorden.", "tokens": ["Die", "halb", "er\u00b7w\u00fcrgt", ",", "um", "l\u00e4n\u00b7ger", "zu", "er\u00b7mor\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVPP", "$,", "KOUI", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Brecht r\u00fcstig auf, und fraget nicht das Gl\u00fcck!", "tokens": ["Brecht", "r\u00fcs\u00b7tig", "auf", ",", "und", "fra\u00b7get", "nicht", "das", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PTKVZ", "$,", "KON", "VVFIN", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Euch f\u00fchren Helden, st\u00e4rkt euch durch Vertrauen!", "tokens": ["Euch", "f\u00fch\u00b7ren", "Hel\u00b7den", ",", "st\u00e4rkt", "euch", "durch", "Ver\u00b7trau\u00b7en", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "NN", "$,", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "La\u00dft hinter euch das alte Mi\u00dfgeschick!", "tokens": ["La\u00dft", "hin\u00b7ter", "euch", "das", "al\u00b7te", "Mi\u00df\u00b7ge\u00b7schick", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie Wasserfluten brauset durch die Auen!", "tokens": ["Wie", "Was\u00b7ser\u00b7flu\u00b7ten", "brau\u00b7set", "durch", "die", "Au\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Glaubt an euch selbst, und rei\u00dfet aus den Klauen", "tokens": ["Glaubt", "an", "euch", "selbst", ",", "und", "rei\u00b7\u00dfet", "aus", "den", "Klau\u00b7en"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPER", "ADV", "$,", "KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Des Galliers das Vaterland zur\u00fcck.", "tokens": ["Des", "Gal\u00b7liers", "das", "Va\u00b7ter\u00b7land", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "