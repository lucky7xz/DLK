{"dta.poem.18827": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Dichter .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Welch ein Zustand! Herr, so sp\u00e4te", "tokens": ["Welch", "ein", "Zu\u00b7stand", "!", "Herr", ",", "so", "sp\u00e4\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PIAT", "ART", "NN", "$.", "NN", "$,", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schleichst du heut aus deiner Kammer;", "tokens": ["Schleichst", "du", "heut", "aus", "dei\u00b7ner", "Kam\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Perser nennen\u2019s Bidamag buden,", "tokens": ["Per\u00b7ser", "nen\u00b7nen's", "Bi\u00b7da\u00b7mag", "bu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Deutsche sagen Katzenjammer.", "tokens": ["Deut\u00b7sche", "sa\u00b7gen", "Kat\u00b7zen\u00b7jam\u00b7mer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Lass mich jetzt, geliebter Knabe,", "tokens": ["Lass", "mich", "jetzt", ",", "ge\u00b7lieb\u00b7ter", "Kna\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir will nicht die Welt gefallen,", "tokens": ["Mir", "will", "nicht", "die", "Welt", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht der Schein, der Duft der Rose,", "tokens": ["Nicht", "der", "Schein", ",", "der", "Duft", "der", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht der Sang der Nachtigallen.", "tokens": ["Nicht", "der", "Sang", "der", "Nach\u00b7ti\u00b7gal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Eben das will ich behandeln,", "tokens": ["E\u00b7ben", "das", "will", "ich", "be\u00b7han\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Und ich denk\u2019, es soll mir klecken,", "tokens": ["Und", "ich", "denk'", ",", "es", "soll", "mir", "kle\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hier! geniess die frischen Mandeln", "tokens": ["Hier", "!", "ge\u00b7niess", "die", "fri\u00b7schen", "Man\u00b7deln"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$.", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Wein wird wieder schmecken.", "tokens": ["Und", "der", "Wein", "wird", "wie\u00b7der", "schme\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Dann will ich auf der Terrasse", "tokens": ["Dann", "will", "ich", "auf", "der", "Ter\u00b7ras\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dich mit frischen L\u00fcften tr\u00e4nken,", "tokens": ["Dich", "mit", "fri\u00b7schen", "L\u00fcf\u00b7ten", "tr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie ich dich in\u2019s Auge fasse", "tokens": ["Wie", "ich", "dich", "in's", "Au\u00b7ge", "fas\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Giebst du einen Kuss dem Schenken.", "tokens": ["Giebst", "du", "ei\u00b7nen", "Kuss", "dem", "Schen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Schau! die Welt ist keine H\u00f6hle,", "tokens": ["Schau", "!", "die", "Welt", "ist", "kei\u00b7ne", "H\u00f6h\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Immer reich an Brut und Nestern,", "tokens": ["Im\u00b7mer", "reich", "an", "Brut", "und", "Nes\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Rosenduft und Rosen\u00f6le!", "tokens": ["Ro\u00b7sen\u00b7duft", "und", "Ro\u00b7sen\u00b7\u00f6\u00b7le", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bulbul auch, sie singt wie gestern.", "tokens": ["Bul\u00b7bul", "auch", ",", "sie", "singt", "wie", "ge\u00b7stern", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "PPER", "VVFIN", "KOKOM", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}