{"dta.poem.18781": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Komm Liebchen, komm! umwinde mir die", "tokens": ["Komm", "Lieb\u00b7chen", ",", "komm", "!", "um\u00b7win\u00b7de", "mir", "die"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "NN", "$,", "VVFIN", "$.", "VVFIN", "PPER", "ART"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "M\u00fctze", "tokens": ["M\u00fct\u00b7ze"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Aus deiner Hand nur ist der Tulbend sch\u00f6n.", "tokens": ["Aus", "dei\u00b7ner", "Hand", "nur", "ist", "der", "Tul\u00b7bend", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hat Abbas doch, auf Irans h\u00f6chstem Sitze,", "tokens": ["Hat", "Ab\u00b7bas", "doch", ",", "auf", "I\u00b7rans", "h\u00f6chs\u00b7tem", "Sit\u00b7ze", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "ADV", "$,", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sein Haupt nicht zierlicher umwinden sehn.", "tokens": ["Sein", "Haupt", "nicht", "zier\u00b7li\u00b7cher", "um\u00b7win\u00b7den", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein Tulbend war das Band, das Alexandern", "tokens": ["Ein", "Tul\u00b7bend", "war", "das", "Band", ",", "das", "A\u00b7lex\u00b7an\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In Schleifen sch\u00f6n vom Haupte fiel", "tokens": ["In", "Schlei\u00b7fen", "sch\u00f6n", "vom", "Haup\u00b7te", "fiel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und allen Folgeherrschern, jenen Andern,", "tokens": ["Und", "al\u00b7len", "Fol\u00b7ge\u00b7herr\u00b7schern", ",", "je\u00b7nen", "An\u00b7dern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PDAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als K\u00f6nigszierde wohlgefiel.", "tokens": ["Als", "K\u00f6\u00b7nigs\u00b7zier\u00b7de", "wohl\u00b7ge\u00b7fiel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein Tulbend ist\u2019s der unsern Kaiser schm\u00fccket,", "tokens": ["Ein", "Tul\u00b7bend", "ist's", "der", "un\u00b7sern", "Kai\u00b7ser", "schm\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie nennen\u2019s Krone. Name geht wohl hin!", "tokens": ["Sie", "nen\u00b7nen's", "Kro\u00b7ne", ".", "Na\u00b7me", "geht", "wohl", "hin", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$.", "NN", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Juweel und Perle! sey das Aug\u2019 entz\u00fccket!", "tokens": ["Ju\u00b7weel", "und", "Per\u00b7le", "!", "sey", "das", "Aug'", "ent\u00b7z\u00fc\u00b7cket", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$.", "VAFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der sch\u00f6nste Schmuck ist stets der Mousselin.", "tokens": ["Der", "sch\u00f6ns\u00b7te", "Schmuck", "ist", "stets", "der", "Mous\u00b7se\u00b7lin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und diesen hier, ganz rein und silberstreifig,", "tokens": ["Und", "die\u00b7sen", "hier", ",", "ganz", "rein", "und", "sil\u00b7ber\u00b7strei\u00b7fig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADV", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Umwinde Liebchen um die Stirn umher.", "tokens": ["Um\u00b7win\u00b7de", "Lieb\u00b7chen", "um", "die", "Stirn", "um\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was ist denn Hoheit? Mir ist sie gel\u00e4ufig!", "tokens": ["Was", "ist", "denn", "Ho\u00b7heit", "?", "Mir", "ist", "sie", "ge\u00b7l\u00e4u\u00b7fig", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "NN", "$.", "PPER", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du schaust mich an, ich bin so gross als Er.", "tokens": ["Du", "schaust", "mich", "an", ",", "ich", "bin", "so", "gross", "als", "Er", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VAFIN", "ADV", "ADJD", "KOKOM", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}