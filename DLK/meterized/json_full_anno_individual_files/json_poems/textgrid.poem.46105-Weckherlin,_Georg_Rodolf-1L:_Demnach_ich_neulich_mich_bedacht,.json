{"textgrid.poem.46105": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Demnach ich neulich mich bedacht,", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Demnach ich neulich mich bedacht,", "tokens": ["Dem\u00b7nach", "ich", "neu\u00b7lich", "mich", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "wie ich doch m\u00f6cht geb\u00fchrlich lehren", "tokens": ["wie", "ich", "doch", "m\u00f6cht", "ge\u00b7b\u00fchr\u00b7lich", "leh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "VMFIN", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "der Musen eine, die mein pracht,", "tokens": ["der", "Mu\u00b7sen", "ei\u00b7ne", ",", "die", "mein", "pracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "$,", "PRELS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "euch dises b\u00fcchlein zu verehren;", "tokens": ["euch", "di\u00b7ses", "b\u00fcch\u00b7lein", "zu", "ver\u00b7eh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PDAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da kam ein jede bald zu mir,", "tokens": ["Da", "kam", "ein", "je\u00b7de", "bald", "zu", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "PIAT", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "vermeinend die ehr zu 