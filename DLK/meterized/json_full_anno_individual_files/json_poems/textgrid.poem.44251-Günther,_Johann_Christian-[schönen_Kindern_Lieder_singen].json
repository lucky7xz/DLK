{"textgrid.poem.44251": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "[sch\u00f6nen Kindern Lieder singen]", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6nen Kindern Lieder singen,", "tokens": ["Sch\u00f6\u00b7nen", "Kin\u00b7dern", "Lie\u00b7der", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist das Amt der Poesie,", "tokens": ["Ist", "das", "Amt", "der", "Poe\u00b7sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und vor sie die Laute zwingen", "tokens": ["Und", "vor", "sie", "die", "Lau\u00b7te", "zwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "ART", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Nichts als angenehme M\u00fch;", "tokens": ["Nichts", "als", "an\u00b7ge\u00b7neh\u00b7me", "M\u00fch", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn der Strahl von ihren Kerzen", "tokens": ["Denn", "der", "Strahl", "von", "ih\u00b7ren", "Ker\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Z\u00fcndet Blut und Geister an,", "tokens": ["Z\u00fcn\u00b7det", "Blut", "und", "Geis\u00b7ter", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df man bey galantem Scherzen", "tokens": ["Da\u00df", "man", "bey", "ga\u00b7lan\u00b7tem", "Scher\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Desto netter spielen kan.", "tokens": ["Des\u00b7to", "net\u00b7ter", "spie\u00b7len", "kan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Jezt erweckstu meine Fl\u00f6then,", "tokens": ["Jezt", "er\u00b7wecks\u00b7tu", "mei\u00b7ne", "Fl\u00f6\u00b7then", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du, o hofnungsvolles Kind.", "tokens": ["Du", ",", "o", "hof\u00b7nungs\u00b7vol\u00b7les", "Kind", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "FM", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sp\u00f6tter sprechen, da\u00df Poeten", "tokens": ["Sp\u00f6t\u00b7ter", "spre\u00b7chen", ",", "da\u00df", "Po\u00b7et\u00b7en"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVINF", "$,", "KOUS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur galante L\u00fcgner sind;", "tokens": ["Nur", "ga\u00b7lan\u00b7te", "L\u00fcg\u00b7ner", "sind", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Diesen Saz zu widerlegen,", "tokens": ["Die\u00b7sen", "Saz", "zu", "wi\u00b7der\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Braucht es nichts als dies mein Blat,", "tokens": ["Braucht", "es", "nichts", "als", "dies", "mein", "Blat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "KOKOM", "PDS", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Welches blos der Warheit wegen", "tokens": ["Wel\u00b7ches", "blos", "der", "War\u00b7heit", "we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ART", "NN", "APPR"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Seine Sch\u00f6nheit von dir hat.", "tokens": ["Sei\u00b7ne", "Sch\u00f6n\u00b7heit", "von", "dir", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie an schlancken Cederst\u00e4mmen", "tokens": ["Wie", "an", "schlan\u00b7cken", "Ce\u00b7der\u00b7st\u00e4m\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zweig und Gipfel munter stehn", "tokens": ["Zweig", "und", "Gip\u00b7fel", "mun\u00b7ter", "stehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "ADJD", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und, die Kiefern zu verd\u00e4mmen,", "tokens": ["Und", ",", "die", "Kie\u00b7fern", "zu", "ver\u00b7d\u00e4m\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "T\u00e4glich st\u00e4rck- und h\u00f6her gehn,", "tokens": ["T\u00e4g\u00b7lich", "st\u00e4r\u00b7ck", "und", "h\u00f6\u00b7her", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "TRUNC", "KON", "ADJD", "VVINF", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "So erhebt dich in der Menge", "tokens": ["So", "er\u00b7hebt", "dich", "in", "der", "Men\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Vieler Sch\u00f6nen unsrer Zeit", "tokens": ["Vie\u00b7ler", "Sch\u00f6\u00b7nen", "uns\u00b7rer", "Zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Die so wohl gestallte L\u00e4nge", "tokens": ["Die", "so", "wohl", "ge\u00b7stall\u00b7te", "L\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und der Glieder Artigkeit.", "tokens": ["Und", "der", "Glie\u00b7der", "Ar\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie der Sonnen fr\u00fches Blizen,", "tokens": ["Wie", "der", "Son\u00b7nen", "fr\u00fc\u00b7hes", "Bli\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn der Thau das Erdreich k\u00fchlt,", "tokens": ["Wenn", "der", "Thau", "das", "Er\u00b7dreich", "k\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf den halbgebrochnen Spizen", "tokens": ["Auf", "den", "halb\u00b7ge\u00b7broch\u00b7nen", "Spi\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Junger Rosenkn\u00f6pfe spielt,", "tokens": ["Jun\u00b7ger", "Ro\u00b7sen\u00b7kn\u00f6p\u00b7fe", "spielt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Also spielt auf Stirn und Wangen", "tokens": ["Al\u00b7so", "spielt", "auf", "Stirn", "und", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eine blumenreiche Pracht,", "tokens": ["Ei\u00b7ne", "blu\u00b7men\u00b7rei\u00b7che", "Pracht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Die schon manchem ein Verlangen", "tokens": ["Die", "schon", "man\u00b7chem", "ein", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "PIAT", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie den Eltern Freude macht.", "tokens": ["Wie", "den", "El\u00b7tern", "Freu\u00b7de", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "In der Augen Farb und Flammen", "tokens": ["In", "der", "Au\u00b7gen", "Farb", "und", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spiegelt sich des Himmels Bild,", "tokens": ["Spie\u00b7gelt", "sich", "des", "Him\u00b7mels", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Milch und Blut fliest da zusammen,", "tokens": ["Milch", "und", "Blut", "fliest", "da", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo der K\u00fc\u00dfe Nectar quillt.", "tokens": ["Wo", "der", "K\u00fc\u00b7\u00dfe", "Nec\u00b7tar", "quillt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Fe\u00dfel an das Herz zu legen,", "tokens": ["Fe\u00b7\u00dfel", "an", "das", "Herz", "zu", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Brauchstu nur ein einzig Glied,", "tokens": ["Brauchs\u00b7tu", "nur", "ein", "ein\u00b7zig", "Glied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "Das durch artiges Bewegen", "tokens": ["Das", "durch", "ar\u00b7ti\u00b7ges", "Be\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Aller Neigung an sich zieht.", "tokens": ["Al\u00b7ler", "Nei\u00b7gung", "an", "sich", "zieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "B\u00e4ume ziert so Laub als Bl\u00fcthe,", "tokens": ["B\u00e4u\u00b7me", "ziert", "so", "Laub", "als", "Bl\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "NN", "KOUS", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch dies ist nur halber Schein,", "tokens": ["Doch", "dies", "ist", "nur", "hal\u00b7ber", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Von der innerlichen G\u00fcte", "tokens": ["Von", "der", "in\u00b7ner\u00b7li\u00b7chen", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Mu\u00dfen Fr\u00fcchte Zeugen seyn:", "tokens": ["Mu\u00b7\u00dfen", "Fr\u00fcch\u00b7te", "Zeu\u00b7gen", "seyn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Deines Leibes holde Gaben", "tokens": ["Dei\u00b7nes", "Lei\u00b7bes", "hol\u00b7de", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lehren in des Alters May,", "tokens": ["Leh\u00b7ren", "in", "des", "Al\u00b7ters", "May", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df ein Geist, den wenig haben,", "tokens": ["Da\u00df", "ein", "Geist", ",", "den", "we\u00b7nig", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "PIS", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Seiner Sch\u00f6nheit Sch\u00f6nheit sey.", "tokens": ["Sei\u00b7ner", "Sch\u00f6n\u00b7heit", "Sch\u00f6n\u00b7heit", "sey", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Sitten, Mienen, Wort und Blicke", "tokens": ["Sit\u00b7ten", ",", "Mie\u00b7nen", ",", "Wort", "und", "Bli\u00b7cke"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zeigen Sanftmuth, Wiz und Kunst,", "tokens": ["Zei\u00b7gen", "Sanft\u00b7muth", ",", "Wiz", "und", "Kunst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Drum verspricht dir auch das Gl\u00fccke", "tokens": ["Drum", "ver\u00b7spricht", "dir", "auch", "das", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den Bestand von seiner Gunst.", "tokens": ["Den", "Be\u00b7stand", "von", "sei\u00b7ner", "Gunst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Las viel stolze M\u00e4gdgen h\u00f6hnen", "tokens": ["Las", "viel", "stol\u00b7ze", "M\u00e4gd\u00b7gen", "h\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PIAT", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und aus blinder Misgunst schm\u00e4hn,", "tokens": ["Und", "aus", "blin\u00b7der", "Mis\u00b7gunst", "schm\u00e4hn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dir verbleibt der Ruhm der Sch\u00f6nen,", "tokens": ["Dir", "ver\u00b7bleibt", "der", "Ruhm", "der", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die auf guten Wandel sehn.", "tokens": ["Die", "auf", "gu\u00b7ten", "Wan\u00b7del", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "O welch z\u00e4rtliches Entz\u00fccken,", "tokens": ["O", "welch", "z\u00e4rt\u00b7li\u00b7ches", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "O welch sanfter Keuschheitszoll", "tokens": ["O", "welch", "sanf\u00b7ter", "Keuschheits\u00b7zoll"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "PIAT", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wird einmahl den Mund erquicken,", "tokens": ["Wird", "ein\u00b7mahl", "den", "Mund", "er\u00b7qui\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dem dein Erstling werden soll;", "tokens": ["Dem", "dein", "Erst\u00b7ling", "wer\u00b7den", "soll", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VAINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "O was wird der Mutter Liebe", "tokens": ["O", "was", "wird", "der", "Mut\u00b7ter", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "PWS", "VAFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Vor vergn\u00fcgte Stunden sehn,", "tokens": ["Vor", "ver\u00b7gn\u00fcg\u00b7te", "Stun\u00b7den", "sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn so manches Freyers Triebe", "tokens": ["Wenn", "so", "man\u00b7ches", "Frey\u00b7ers", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PIAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nach der liebsten Tochter flehn.", "tokens": ["Nach", "der", "liebs\u00b7ten", "Toch\u00b7ter", "flehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Dieser Tag, der dich der Erden", "tokens": ["Die\u00b7ser", "Tag", ",", "der", "dich", "der", "Er\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "$,", "PRELS", "PRF", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als ein Kleinod erst geschenckt,", "tokens": ["Als", "ein", "Klei\u00b7nod", "erst", "ge\u00b7schenckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Soll so lang ein Festtag werden,", "tokens": ["Soll", "so", "lang", "ein", "Fest\u00