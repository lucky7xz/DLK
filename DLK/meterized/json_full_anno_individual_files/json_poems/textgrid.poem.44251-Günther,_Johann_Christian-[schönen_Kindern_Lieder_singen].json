], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Junger Rosenkn\u00f6pfe spielt,", "tokens": ["Jun\u00b7ger", "Ro\u00b7sen\u00b7kn\u00f6p\u00b7fe", "spielt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Also spielt auf Stirn und Wangen", "tokens": ["Al\u00b7so", "spielt", "auf", "Stirn", "und", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eine blumenreiche Pracht,", "tokens": ["Ei\u00b7ne", "blu\u00b7men\u00b7rei\u00b7che", "Pracht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Die schon manchem ein Verlangen", "tokens": ["Die", "schon", "man\u00b7chem", "ein", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "PIAT", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie den Eltern Freude macht.", "tokens": ["Wie", "den", "El\u00b7tern", "Freu\u00b7de", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "In der Augen Farb und Flammen", "tokens": ["In", "der", "Au\u00b7gen", "Farb", "und", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spiegelt sich des Himmels Bild,", "tokens": ["Spie\u00b7gelt", "sich", "des", "Him\u00b7mels", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFI