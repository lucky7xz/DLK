sure": "iambic.tri"}, "line.6": {"text": "Bis da\u00df sein Bierherz bricht.", "tokens": ["Bis", "da\u00df", "sein", "Bier\u00b7herz", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Er geht nicht mehr von hinnen,", "tokens": ["Er", "geht", "nicht", "mehr", "von", "hin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "APPR", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Poussiert die Kellnerinnen", "tokens": ["Pous\u00b7siert", "die", "Kell\u00b7ne\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["NE", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vor Gram und \u00dcberdru\u00df.", "tokens": ["Vor", "Gram", "und", "\u00dc\u00b7berd\u00b7ru\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und wer hat das verschuldet?", "tokens": ["Und", "wer", "hat", "das", "ver\u00b7schul\u00b7det", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "PDS", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der, den man leider duldet,", "tokens": ["Der", ",", "den", "man", "lei\u00b7der", "dul\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Simplicissimus!", "tokens": ["Der", "Sim\u00b7pli\u00b7cis\u00b7si\u00b7mus", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}