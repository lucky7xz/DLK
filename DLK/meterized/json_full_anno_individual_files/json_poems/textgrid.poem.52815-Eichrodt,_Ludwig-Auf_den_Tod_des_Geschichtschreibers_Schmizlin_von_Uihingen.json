{"textgrid.poem.52815": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Auf den Tod des Geschichtschreibers Schmizlin von Uihingen", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lasset heut' die Leichenglocke schallen", "tokens": ["Las\u00b7set", "heut'", "die", "Lei\u00b7chen\u00b7glo\u00b7cke", "schal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In der Harfe Todtenmelodien,", "tokens": ["In", "der", "Har\u00b7fe", "Tod\u00b7ten\u00b7me\u00b7lo\u00b7di\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ach, ein gro\u00dfer Mann ist uns entfallen,", "tokens": ["Ach", ",", "ein", "gro\u00b7\u00dfer", "Mann", "ist", "uns", "ent\u00b7fal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "ADJA", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Schwabens Zierde, Schmizlin, ist dahin!", "tokens": ["Schwa\u00b7bens", "Zier\u00b7de", ",", "Sch\u00b7miz\u00b7lin", ",", "ist", "da\u00b7hin", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "NE", "$,", "VAFIN", "PAV", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Ewig soll im Tempel der Geschichte", "tokens": ["E\u00b7wig", "soll", "im", "Tem\u00b7pel", "der", "Ge\u00b7schich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Ihres Priesters Name aufrecht stehn,", "tokens": ["Ih\u00b7res", "Pries\u00b7ters", "Na\u00b7me", "auf\u00b7recht", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Und im Lied, im Drama, im Gedichte", "tokens": ["Und", "im", "Lied", ",", "im", "Dra\u00b7ma", ",", "im", "Ge\u00b7dich\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPRART", "NN", "$,", "APPRART", "NN", "$,", "APPRART", "NN"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Soll die Nachwelt ihn verherrlichen.", "tokens": ["Soll", "die", "Nach\u00b7welt", "ihn", "ver\u00b7herr\u00b7li\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Mit dem Geist der alt' und neuen Zeiten", "tokens": ["Mit", "dem", "Geist", "der", "alt'", "und", "neu\u00b7en", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "KON", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "War der Hingetretene vertraut,", "tokens": ["War", "der", "Hin\u00b7ge\u00b7tre\u00b7te\u00b7ne", "ver\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Er enth\u00fcllte die Begebenheiten", "tokens": ["Er", "ent\u00b7h\u00fcll\u00b7te", "die", "Be\u00b7ge\u00b7ben\u00b7hei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Mit der Wahrheit unverzagtem Laut.", "tokens": ["Mit", "der", "Wahr\u00b7heit", "un\u00b7ver\u00b7zag\u00b7tem", "Laut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "O Verh\u00e4ngni\u00df, warum diese H\u00e4rte", "tokens": ["O", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", ",", "wa\u00b7rum", "die\u00b7se", "H\u00e4r\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PWAV", "PDAT", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Ueber's Vaterland, war's nicht genug,", "tokens": ["Ue\u00b7ber's", "Va\u00b7ter\u00b7land", ",", "wa\u00b7r's", "nicht", "ge\u00b7nug", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VAFIN", "PTKNEG", "ADV", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Da\u00df der Krieg mit seinem Heidenschwerte", "tokens": ["Da\u00df", "der", "Krieg", "mit", "sei\u00b7nem", "Hei\u00b7den\u00b7schwer\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Ihm erst j\u00fcngst so manchen Sohn erschlug?", "tokens": ["Ihm", "erst", "j\u00fcngst", "so", "man\u00b7chen", "Sohn", "er\u00b7schlug", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Warum ist das Loos der Feuerk\u00f6pfe", "tokens": ["Wa\u00b7rum", "ist", "das", "Loos", "der", "Feu\u00b7er\u00b7k\u00f6p\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schweres Ungl\u00fcck oder fr\u00fcher Tod?", "tokens": ["Schwe\u00b7res", "Un\u00b7gl\u00fcck", "o\u00b7der", "fr\u00fc\u00b7her", "Tod", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJD", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "W\u00e4hrend die gew\u00f6hnlichen Gesch\u00f6pfe", "tokens": ["W\u00e4h\u00b7rend", "die", "ge\u00b7w\u00f6hn\u00b7li\u00b7chen", "Ge\u00b7sch\u00f6p\u00b7fe"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Finden lebenslang ihr t\u00e4glich Brot.", "tokens": ["Fin\u00b7den", "le\u00b7bens\u00b7lang", "ihr", "t\u00e4g\u00b7lich", "Brot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Ward nur darum diesem selt'nen Manne", "tokens": ["Ward", "nur", "da\u00b7rum", "die\u00b7sem", "selt'\u00b7nen", "Man\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PAV", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "So im Nu dies arge Lebensziel?", "tokens": ["So", "im", "Nu", "dies", "ar\u00b7ge", "Le\u00b7bens\u00b7ziel", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADV", "PDS", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Fiel er darum wie die hohe Tanne,", "tokens": ["Fiel", "er", "da\u00b7rum", "wie", "die", "ho\u00b7he", "Tan\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Und ist Tannensturz ein Kinderspiel?", "tokens": ["Und", "ist", "Tan\u00b7nen\u00b7sturz", "ein", "Kin\u00b7der\u00b7spiel", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Aber besser so, wie es gekommen,", "tokens": ["A\u00b7ber", "bes\u00b7ser", "so", ",", "wie", "es", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "$,", "PWAV", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Als wenn Schlimmeres noch w\u00e4r' geschehn.", "tokens": ["Als", "wenn", "Schlim\u00b7me\u00b7res", "noch", "w\u00e4r'", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "NN", "ADV", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Kann Gebrest und Altersschw\u00e4che frommen", "tokens": ["Kann", "Ge\u00b7brest", "und", "Al\u00b7ters\u00b7schw\u00e4\u00b7che", "from\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "NN", "KON", "NN", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Einem Geiste aus den Himmelsh\u00f6hn?", "tokens": ["Ei\u00b7nem", "Geis\u00b7te", "aus", "den", "Him\u00b7mels\u00b7h\u00f6hn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Welcher zu den Menschen kam hernieder,", "tokens": ["Wel\u00b7cher", "zu", "den", "Men\u00b7schen", "kam", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "APPR", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Zu belehren dieses Publikum,", "tokens": ["Zu", "be\u00b7leh\u00b7ren", "die\u00b7ses", "Pub\u00b7li\u00b7kum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Solche Geister kommen nicht gleich wieder,", "tokens": ["Sol\u00b7che", "Geis\u00b7ter", "kom\u00b7men", "nicht", "gleich", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PTKNEG", "ADV", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Wenn sie scheiden, wissen sie warum!", "tokens": ["Wenn", "sie", "schei\u00b7den", ",", "wis\u00b7sen", "sie", "wa\u00b7rum", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "$,", "VVFIN", "PPER", "PWAV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Darum sch\u00e4m' ich mich, mit l\u00e4ngern Klagen", "tokens": ["Da\u00b7rum", "sch\u00e4m'", "ich", "mich", ",", "mit", "l\u00e4n\u00b7gern", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Zu entsetzen euer friedlich' Ohr;", "tokens": ["Zu", "ent\u00b7set\u00b7zen", "eu\u00b7er", "fried\u00b7lich'", "Ohr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "An der gro\u00dfen M\u00e4nner Sarkophagen", "tokens": ["An", "der", "gro\u00b7\u00dfen", "M\u00e4n\u00b7ner", "Sar\u00b7ko\u00b7pha\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ziemt sich blos f\u00fcr Dankbarkeit ein Chor.", "tokens": ["Ziemt", "sich", "blos", "f\u00fcr", "Dank\u00b7bar\u00b7keit", "ein", "Chor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "APPR", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Auf darum, und la\u00dft uns diesen singen!", "tokens": ["Auf", "da\u00b7rum", ",", "und", "la\u00dft", "uns", "die\u00b7sen", "sin\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PAV", "$,", "KON", "VVIMP", "PPER", "PDS", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Ewig bleiben wir davon entz\u00fcckt,", "tokens": ["E\u00b7wig", "blei\u00b7ben", "wir", "da\u00b7von", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PAV", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Da\u00df der sel'ge Schmizlin von Uihingen", "tokens": ["Da\u00df", "der", "sel'\u00b7ge", "Sch\u00b7miz\u00b7lin", "von", "Ui\u00b7hin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "NN"], "meter": "+-+-+-+-++-", "measure": "unknown.measure.hexa"}, "line.8": {"text": "Uns mit seiner Gegenwart begl\u00fcckt!", "tokens": ["Uns", "mit", "sei\u00b7ner", "Ge\u00b7gen\u00b7wart", "be\u00b7gl\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Lasset heut' die Leichenglocke schallen", "tokens": ["Las\u00b7set", "heut'", "die", "Lei\u00b7chen\u00b7glo\u00b7cke", "