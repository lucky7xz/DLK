{"dta.poem.7411": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Reiterlied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die bange Nacht ist nun herum,", "tokens": ["Die", "ban\u00b7ge", "Nacht", "ist", "nun", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir reiten still, wir reiten stumm,", "tokens": ["Wir", "rei\u00b7ten", "still", ",", "wir", "rei\u00b7ten", "stumm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und reiten in's Verderben.", "tokens": ["Und", "rei\u00b7ten", "in's", "Ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie weht so scharf der Morgenwind!", "tokens": ["Wie", "weht", "so", "scharf", "der", "Mor\u00b7gen\u00b7wind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Frau Wirthin, noch ein Glas geschwind", "tokens": ["Frau", "Wirt\u00b7hin", ",", "noch", "ein", "Glas", "ge\u00b7schwind"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NN", "$,", "ADV", "ART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vorm Sterben, vorm Sterben.", "tokens": ["Vorm", "Ster\u00b7ben", ",", "vorm", "Ster\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Du junges Gras, was stehst so gr\u00fcn?", "tokens": ["Du", "jun\u00b7ges", "Gras", ",", "was", "stehst", "so", "gr\u00fcn", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PWS", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mu\u00dft bald wie lauter R\u00f6slein bl\u00fch'n,", "tokens": ["Mu\u00dft", "bald", "wie", "lau\u00b7ter", "R\u00f6s\u00b7lein", "bl\u00fch'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "KOKOM", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Blut ja soll Dich f\u00e4rben.", "tokens": ["Mein", "Blut", "ja", "soll", "Dich", "f\u00e4r\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den ersten Schluck, an's Schwert die Hand,", "tokens": ["Den", "ers\u00b7ten", "Schluck", ",", "an's", "Schwert", "die", "Hand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den trink' ich, f\u00fcr das Vaterland", "tokens": ["Den", "trink'", "ich", ",", "f\u00fcr", "das", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zu sterben, zu sterben.", "tokens": ["Zu", "ster\u00b7ben", ",", "zu", "ster\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Und schnell den zweiten hinterdrein,", "tokens": ["Und", "schnell", "den", "zwei\u00b7ten", "hin\u00b7ter\u00b7drein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "ADJA", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und der soll f\u00fcr die Freiheit sein,", "tokens": ["Und", "der", "soll", "f\u00fcr", "die", "Frei\u00b7heit", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VMFIN", "APPR", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der zweite Schluck vom Herben!", "tokens": ["Der", "zwei\u00b7te", "Schluck", "vom", "Her\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Di\u00df Restchen \u2014 nun, wem bring' ich's gleich?", "tokens": ["Di\u00df", "Rest\u00b7chen", "nun", ",", "wem", "bring'", "ich's", "gleich", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "$(", "ADV", "$,", "PWS", "VVFIN", "PIS", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Di\u00df Restchen Dir, o r\u00f6misch Reich,", "tokens": ["Di\u00df", "Rest\u00b7chen", "Dir", ",", "o", "r\u00f6\u00b7misch", "Reich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "PPER", "$,", "FM", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zum Sterben, zum Sterben!", "tokens": ["Zum", "Ster\u00b7ben", ",", "zum", "Ster\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Dem Liebchen \u2014 doch das Glas ist leer,", "tokens": ["Dem", "Lieb\u00b7chen", "doch", "das", "Glas", "ist", "leer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Kugel saust, es blitzt der Speer;", "tokens": ["Die", "Ku\u00b7gel", "saust", ",", "es", "blitzt", "der", "Speer", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bringt meinem Kind die Scherben!", "tokens": ["Bringt", "mei\u00b7nem", "Kind", "die", "Scher\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf! in den Feind wie Wetterschlag!", "tokens": ["Auf", "!", "in", "den", "Feind", "wie", "Wet\u00b7ter\u00b7schlag", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "APPR", "ART", "NN", "KOKOM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O Reiterlust, am fr\u00fchen Tag", "tokens": ["O", "Rei\u00b7ter\u00b7lust", ",", "am", "fr\u00fc\u00b7hen", "Tag"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zu sterben, zu sterben!", "tokens": ["Zu", "ster\u00b7ben", ",", "zu", "ster\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}