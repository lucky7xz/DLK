{"dta.poem.5388": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die jetzt aus allen Dingen qvillt,", "tokens": ["Die", "jetzt", "aus", "al\u00b7len", "Din\u00b7gen", "qvillt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Erde, Luft und alles f\u00fcllt,", "tokens": ["Und", "Er\u00b7de", ",", "Luft", "und", "al\u00b7les", "f\u00fcllt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "KON", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erf\u00fcllet dann auch billig mein Gem\u00fcthe,", "tokens": ["Er\u00b7f\u00fcl\u00b7let", "dann", "auch", "bil\u00b7lig", "mein", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df es nicht nur versp\u00fchrt und sieht", "tokens": ["Da\u00df", "es", "nicht", "nur", "ver\u00b7sp\u00fchrt", "und", "sieht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "VVPP", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Sch\u00f6nheit in der bunten Bl\u00fcht,", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "in", "der", "bun\u00b7ten", "Bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Lieblichkeit im frischen Gr\u00fcnen;", "tokens": ["Die", "Lieb\u00b7lich\u00b7keit", "im", "fri\u00b7schen", "Gr\u00fc\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Nein, da\u00df es, gleich den regen Bienen,", "tokens": ["Nein", ",", "da\u00df", "es", ",", "gleich", "den", "re\u00b7gen", "Bie\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Aus allem gleichfals Honig zieht.", "tokens": ["Aus", "al\u00b7lem", "gleich\u00b7fals", "Ho\u00b7nig", "zieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der Honig, welcher geistig ist", "tokens": ["Der", "Ho\u00b7nig", ",", "wel\u00b7cher", "geis\u00b7tig", "ist"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und eine rechte Seelen-Speise,", "tokens": ["Und", "ei\u00b7ne", "rech\u00b7te", "See\u00b7len\u00b7Spei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Ist, wenn man, unserm GOtt zum Preise,", "tokens": ["Ist", ",", "wenn", "man", ",", "un\u00b7serm", "Gott", "zum", "Prei\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOUS", "PIS", "$,", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Sein\u2019 Allmacht, seine Lieb\u2019 ermi\u00dft,", "tokens": ["Sein'", "All\u00b7macht", ",", "sei\u00b7ne", "Lieb'", "er\u00b7mi\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und wenn man fr\u00f6lich \u00fcberleget,", "tokens": ["Und", "wenn", "man", "fr\u00f6\u00b7lich", "\u00fc\u00b7ber\u00b7le\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Da\u00df was der Welt-Kreis herrlichs heget", "tokens": ["Da\u00df", "was", "der", "Welt\u00b7Kreis", "herr\u00b7lichs", "he\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "NN", "APZR", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Allein der GOttheit weise Kraft", "tokens": ["Al\u00b7lein", "der", "Got\u00b7theit", "wei\u00b7se", "Kraft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Und seine Liebe schuf und schaft.", "tokens": ["Und", "sei\u00b7ne", "Lie\u00b7be", "schuf", "und", "schaft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "So sucht die Kraft der regen Seelen,", "tokens": ["So", "sucht", "die", "Kraft", "der", "re\u00b7gen", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Wenn der Gesch\u00f6pfe Pracht sie r\u00fchrt,", "tokens": ["Wenn", "der", "Ge\u00b7sch\u00f6p\u00b7fe", "Pracht", "sie", "r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Und sie darinn den Sch\u00f6pfer sp\u00fchrt,", "tokens": ["Und", "sie", "da\u00b7rinn", "den", "Sch\u00f6p\u00b7fer", "sp\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Nicht nur die Wunder zu erzehlen;", "tokens": ["Nicht", "nur", "die", "Wun\u00b7der", "zu", "er\u00b7zeh\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Sie ist, von Innbrunst hei\u00df, bem\u00fcht,", "tokens": ["Sie", "ist", ",", "von", "I\u00b7nnbrunst", "hei\u00df", ",", "be\u00b7m\u00fcht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "APPR", "ADV", "ADJD", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Wenn sie im Werck den Sch\u00f6pfer sieht,", "tokens": ["Wenn", "sie", "im", "Werck", "den", "Sch\u00f6p\u00b7fer", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Durch ein empfindlichs Ueberdencken", "tokens": ["Durch", "ein", "emp\u00b7find\u00b7lichs", "Ue\u00b7ber\u00b7den\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Sich ins allgegenw\u00e4rt\u2019ge Meer", "tokens": ["Sich", "ins", "all\u00b7ge\u00b7gen\u00b7w\u00e4rt'\u00b7ge", "Meer"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "APPRART", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.25": {"text": "Der GOttheit gleichsam zu versencken.", "tokens": ["Der", "Got\u00b7theit", "gleich\u00b7sam", "zu", "ver\u00b7sen\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Sie w\u00fcnscht, zu ihres Sch\u00f6pfers Ehr,", "tokens": ["Sie", "w\u00fcnscht", ",", "zu", "ih\u00b7res", "Sch\u00f6p\u00b7fers", "Ehr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "In einem durch der Wercke Pracht", "tokens": ["In", "ei\u00b7nem", "durch", "der", "Wer\u00b7cke", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Geschm\u00fcckten Andacht-vollen Dencken,", "tokens": ["Ge\u00b7schm\u00fcck\u00b7ten", "An\u00b7dacht\u00b7vol\u00b7len", "Den\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Sich selber ihrem GOtt zu schencken.", "tokens": ["Sich", "sel\u00b7ber", "ih\u00b7rem", "Gott", "zu", "schen\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}