{"dta.poem.16068": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "122.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du bist nur halb, o Mensch, wie dich hervorgebracht", "tokens": ["Du", "bist", "nur", "halb", ",", "o", "Mensch", ",", "wie", "dich", "her\u00b7vor\u00b7ge\u00b7bracht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "FM", "NN", "$,", "PWAV", "PPER", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat die Natur, und halb, wie du dich selbst gemacht.", "tokens": ["Hat", "die", "Na\u00b7tur", ",", "und", "halb", ",", "wie", "du", "dich", "selbst", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "KON", "ADJD", "$,", "PWAV", "PPER", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sie hat den festen Grund gelegt, an den du r\u00fchren", "tokens": ["Sie", "hat", "den", "fes\u00b7ten", "Grund", "ge\u00b7legt", ",", "an", "den", "du", "r\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,", "APPR", "ART", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht darfst, dir aber bleibt der Bau drauf auszuf\u00fchren.", "tokens": ["Nicht", "darfst", ",", "dir", "a\u00b7ber", "bleibt", "der", "Bau", "drauf", "aus\u00b7zu\u00b7f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PAV", "$,", "PPER", "ADV", "VVFIN", "ART", "NN", "PAV", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Bei jenem kanst du nichts, bei diesem alles thun,", "tokens": ["Bei", "je\u00b7nem", "kanst", "du", "nichts", ",", "bei", "die\u00b7sem", "al\u00b7les", "thun", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VMFIN", "PPER", "PIS", "$,", "APPR", "PDAT", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dieses ist genug, um tr\u00e4ge nie zu ruhn.", "tokens": ["Und", "die\u00b7ses", "ist", "ge\u00b7nug", ",", "um", "tr\u00e4\u00b7ge", "nie", "zu", "ruhn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADV", "$,", "KOUI", "VVFIN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nie ruhe, bis du gut das was du schlecht gemacht", "tokens": ["Nie", "ru\u00b7he", ",", "bis", "du", "gut", "das", "was", "du", "schlecht", "ge\u00b7macht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "PPER", "ADJD", "PDS", "PRELS", "PPER", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An dir, und was du falsch gemacht, hast recht gemacht.", "tokens": ["An", "dir", ",", "und", "was", "du", "falsch", "ge\u00b7macht", ",", "hast", "recht", "ge\u00b7macht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "KON", "PWS", "PPER", "ADJD", "VVPP", "$,", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Dazu ists nie zu fr\u00fch, dazu ists nie zu sp\u00e4t;", "tokens": ["Da\u00b7zu", "ists", "nie", "zu", "fr\u00fch", ",", "da\u00b7zu", "ists", "nie", "zu", "sp\u00e4t", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ADV", "PTKA", "ADJD", "$,", "PAV", "VAFIN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn stets im Werden, bist du nie geworden st\u00e4t.", "tokens": ["Denn", "stets", "im", "Wer\u00b7den", ",", "bist", "du", "nie", "ge\u00b7wor\u00b7den", "st\u00e4t", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "$,", "VAFIN", "PPER", "ADV", "VAPP", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}