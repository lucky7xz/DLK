{"dta.poem.1222": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die todten Farben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Weil mich die Liebe zwingt zu gehen in den Tod/", "tokens": ["Weil", "mich", "die", "Lie\u00b7be", "zwingt", "zu", "ge\u00b7hen", "in", "den", "Tod", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Soll dieser Todten-Brieff auch Tod und Liebe weisen:", "tokens": ["Soll", "die\u00b7ser", "Tod\u00b7ten\u00b7Brieff", "auch", "Tod", "und", "Lie\u00b7be", "wei\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDAT", "NN", "ADV", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Beyeln Farbe zeigt die harte Liebes-Noth/", "tokens": ["Der", "Be\u00b7yeln", "Far\u00b7be", "zeigt", "die", "har\u00b7te", "Lie\u00b7bes\u00b7Noth", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Todten-Bl\u00e4tter/ da\u00df ich mu\u00df zum Tode reisen.", "tokens": ["Der", "Tod\u00b7ten\u00b7Bl\u00e4t\u00b7ter", "/", "da\u00df", "ich", "mu\u00df", "zum", "To\u00b7de", "rei\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "KOUS", "PPE