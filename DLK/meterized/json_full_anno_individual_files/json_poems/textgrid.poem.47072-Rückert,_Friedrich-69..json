{"textgrid.poem.47072": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "69.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du wei\u00dfe, sch\u00f6n gewaschne Hemdenkrause,", "tokens": ["Du", "wei\u00b7\u00dfe", ",", "sch\u00f6n", "ge\u00b7waschne", "Hem\u00b7den\u00b7krau\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zur Trocknung hier am Ofen aufgehangen,", "tokens": ["Zur", "Trock\u00b7nung", "hier", "am", "O\u00b7fen", "auf\u00b7ge\u00b7han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du siehst mir eben aus, als ob zu prangen", "tokens": ["Du", "siehst", "mir", "e\u00b7ben", "aus", ",", "als", "ob", "zu", "pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "KOKOM", "KOUS", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du habest Lust bei einem Hochzeitschmause.", "tokens": ["Du", "ha\u00b7best", "Lust", "bei", "ei\u00b7nem", "Hoch\u00b7zeit\u00b7schmau\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "\u00bbja, Hochzeit gibt's auch n\u00e4chstens hier im Hause.\u00ab", "tokens": ["\u00bb", "ja", ",", "Hoch\u00b7zeit", "gibt's", "auch", "n\u00e4chs\u00b7tens", "hier", "im", "Hau\u00b7se", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKANT", "$,", "NN", "VVFIN", "ADV", "ADV", "ADV", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Weh' mir, zum Eintritt machst du schlimm mich bangen.", "tokens": ["Weh'", "mir", ",", "zum", "Ein\u00b7tritt", "machst", "du", "schlimm", "mich", "ban\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPRART", "NN", "VVFIN", "PPER", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbnicht doch! der gilt's, die dich nichts angegangen;", "tokens": ["\u00bb", "nicht", "doch", "!", "der", "gilt's", ",", "die", "dich", "nichts", "an\u00b7ge\u00b7gan\u00b7gen", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKNEG", "ADV", "$.", "ART", "VVFIN", "$,", "PRELS", "PPER", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die j\u00fcngre bleibt vorerst noch in der Klause.\u00ab", "tokens": ["Die", "j\u00fcng\u00b7re", "bleibt", "vor\u00b7erst", "noch", "in", "der", "Klau\u00b7se", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Weh' mir, auch so verlier' ich eine Schanze.", "tokens": ["Weh'", "mir", ",", "auch", "so", "ver\u00b7lier'", "ich", "ei\u00b7ne", "Schan\u00b7ze", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADV", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbwie so das?\u00ab Ach! vorm w\u00fcsten Freierhaufen", "tokens": ["\u00bb", "wie", "so", "das", "?", "\u00ab", "Ach", "!", "vorm", "w\u00fcs\u00b7ten", "Frei\u00b7er\u00b7hau\u00b7fen"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word"], "pos": ["$(", "KOKOM", "ADV", "PDS", "$.", "$(", "ITJ", "$.", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "That die mir gute Dienste allerwegen", "tokens": ["That", "die", "mir", "gu\u00b7te", "Diens\u00b7te", "al\u00b7ler\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "PPER", "ADJA", "NN", "VVINF"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "Als Au\u00dfenwerk; ist das weg, wird der ganze", "tokens": ["Als", "Au\u00b7\u00dfen\u00b7werk", ";", "ist", "das", "weg", ",", "wird", "der", "gan\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "NN", "$.", "VAFIN", "PDS", "PTKVZ", "$,", "VAFIN", "ART", "ADJA"], "meter": "-+-+--++-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Verwegne Schwarm mir nun ans Hauptwerk laufen.", "tokens": ["Ver\u00b7weg\u00b7ne", "Schwarm", "mir", "nun", "ans", "Haupt\u00b7werk", "lau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbda mu\u00dft du denn zwiefach ins Zeug dich legen.\u00ab", "tokens": ["\u00bb", "da", "mu\u00dft", "du", "denn", "zwie\u00b7fach", "ins", "Zeug", "dich", "le\u00b7gen", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "AD