{"dta.poem.11602": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Seufzer   .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Dein Liebesfeuer,", "tokens": ["Dein", "Lie\u00b7bes\u00b7feu\u00b7er", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Ach Herr! wie theuer", "tokens": ["Ach", "Herr", "!", "wie", "theu\u00b7er"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ITJ", "NN", "$.", "PWAV", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wollt' ich es hegen,", "tokens": ["Wollt'", "ich", "es", "he\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Wollt' ich es pflegen!", "tokens": ["Wollt'", "ich", "es", "pfle\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Hab's nicht geheget", "tokens": ["Hab's", "nicht", "ge\u00b7he\u00b7get"], "token_info": ["word", "word", "word"], "pos": ["NE", "PTKNEG", "VVFIN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Und nicht gepfleget,", "tokens": ["Und", "nicht", "ge\u00b7pfle\u00b7get", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "War Eis im Herzen \u2014", "tokens": ["War", "Eis", "im", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPRART", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "O H\u00f6llenschmerzen!", "tokens": ["O", "H\u00f6l\u00b7len\u00b7schmer\u00b7zen", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}