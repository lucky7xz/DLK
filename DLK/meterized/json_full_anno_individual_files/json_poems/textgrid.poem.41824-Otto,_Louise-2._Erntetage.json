{"textgrid.poem.41824": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "2. Erntetage", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Aehren golden Gl\u00e4nzen", "tokens": ["Der", "A\u00b7eh\u00b7ren", "gol\u00b7den", "Gl\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "L\u00e4dt schon der Schnitter Scharen ein", "tokens": ["L\u00e4dt", "schon", "der", "Schnit\u00b7ter", "Scha\u00b7ren", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ART", "NN", "NN", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu frohen Erntet\u00e4nzen.", "tokens": ["Zu", "fro\u00b7hen", "Ern\u00b7te\u00b7t\u00e4n\u00b7zen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Zur Arbeit, wie zur Freude ruft", "tokens": ["Zur", "Ar\u00b7beit", ",", "wie", "zur", "Freu\u00b7de", "ruft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PWAV", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Sommer allerwegen,", "tokens": ["Der", "Som\u00b7mer", "al\u00b7ler\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und Vogelsang und Blumenduft", "tokens": ["Und", "Vo\u00b7gel\u00b7sang", "und", "Blu\u00b7men\u00b7duft"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Versch\u00f6nen seinen Segen.", "tokens": ["Ver\u00b7sch\u00f6\u00b7nen", "sei\u00b7nen", "Se\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Bald aber streift ein k\u00fchler Wind", "tokens": ["Bald", "a\u00b7ber", "streift", "ein", "k\u00fch\u00b7ler", "Wind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ob leeren Stoppelfeldern,", "tokens": ["Ob", "lee\u00b7ren", "Stop\u00b7pel\u00b7fel\u00b7dern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die V\u00f6glein still geworden sind", "tokens": ["Die", "V\u00f6\u00b7glein", "still", "ge\u00b7wor\u00b7den", "sind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VAPP", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In B\u00fcschen und in W\u00e4ldern.", "tokens": ["In", "B\u00fc\u00b7schen", "und", "in", "W\u00e4l\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Langsam im Rebengarten,", "tokens": ["Lang\u00b7sam", "im", "Re\u00b7beng\u00b7ar\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Es l\u00e4\u00dft des Herbstes sch\u00f6nstes Bild", "tokens": ["Es", "l\u00e4\u00dft", "des", "Herbs\u00b7tes", "sch\u00f6ns\u00b7tes", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gern lange sich erwarten.", "tokens": ["Gern", "lan\u00b7ge", "sich", "er\u00b7war\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wer m\u00f6chte tadeln sie darum?", "tokens": ["Wer", "m\u00f6ch\u00b7te", "ta\u00b7deln", "sie", "da\u00b7rum", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "VVINF", "PPER", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist erst auch sie genommen,", "tokens": ["Ist", "erst", "auch", "sie", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dann wird es einsam um und um,", "tokens": ["Dann", "wird", "es", "ein\u00b7sam", "um", "und", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dann droht des Winters Kommen.", "tokens": ["Dann", "droht", "des", "Win\u00b7ters", "Kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Dann sind die V\u00f6glein all' verjagt,", "tokens": ["Dann", "sind", "die", "V\u00f6\u00b7glein", "all'", "ver\u00b7jagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Schwalben fortgeflogen,", "tokens": ["Die", "Schwal\u00b7ben", "fort\u00b7ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Des Laubes Fallen traurig klagt,", "tokens": ["Des", "Lau\u00b7bes", "Fal\u00b7len", "trau\u00b7rig", "klagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Rot und Gold durchzogen.", "tokens": ["Von", "Rot", "und", "Gold", "durch\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Drum segnen wir die letzte Frucht", "tokens": ["Drum", "seg\u00b7nen", "wir", "die", "letz\u00b7te", "Frucht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als k\u00f6stlichste von allen,", "tokens": ["Als", "k\u00f6st\u00b7lichs\u00b7te", "von", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "APPR", "PIAT", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von sonn'ger H\u00f6he bis zur Schlucht", "tokens": ["Von", "sonn'\u00b7ger", "H\u00f6\u00b7he", "bis", "zur", "Schlucht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Dankeslieder schallen.", "tokens": ["Ihr", "Dan\u00b7kes\u00b7lie\u00b7der", "schal\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ob's \u00bbHerbsten\u00ab hei\u00dft im Volkesmund,", "tokens": ["Ob's", "\u00bb", "Herbs\u00b7ten", "\u00ab", "hei\u00dft", "im", "Vol\u00b7kes\u00b7mund", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "NN", "$(", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ob \u00bbes wird Wein gelesen\u00ab,", "tokens": ["Ob", "\u00bb", "es", "wird", "Wein", "ge\u00b7le\u00b7sen", "\u00ab", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "$(", "PPER", "VAFIN", "NN", "VVPP", "$(", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es thut sich allw\u00e4rts jauchzend kund", "tokens": ["Es", "thut", "sich", "all\u00b7w\u00e4rts", "jauch\u00b7zend", "kund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ADJD", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein frisch und fr\u00f6hlich Wesen.", "tokens": ["Ein", "frisch", "und", "fr\u00f6h\u00b7lich", "We\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und weithin durch die L\u00fcfte dr\u00f6hnt's", "tokens": ["Und", "weit\u00b7hin", "durch", "die", "L\u00fcf\u00b7te", "dr\u00f6hnt's"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus Flinten und aus B\u00f6llern.", "tokens": ["Aus", "Flin\u00b7ten", "und", "aus", "B\u00f6l\u00b7lern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Anwortend gl\u00e4nzend noch versch\u00f6nt's", "tokens": ["An\u00b7wor\u00b7tend", "gl\u00e4n\u00b7zend", "noch", "ver\u00b7sch\u00f6nt's"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "ADV", "NE"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Buntfeuer von den S\u00f6llern.", "tokens": ["Bunt\u00b7feu\u00b7er", "von", "den", "S\u00f6l\u00b7lern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Das ist die letzte Erntezeit \u2013", "tokens": ["Das", "ist", "die", "letz\u00b7te", "Ern\u00b7te\u00b7zeit", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn Trauben Most geworden:", "tokens": ["Wenn", "Trau\u00b7ben", "Most", "ge\u00b7wor\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "VAPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dann h\u00e4ngt der ", "tokens": ["Dann", "h\u00e4ngt", "der"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Still an des ", "tokens": ["Still", "an", "des"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "APPR", "ART"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.11": {"line.1": {"text": "Doch Scheuern, Keller heimsten ein", "tokens": ["Doch", "Scheu\u00b7ern", ",", "Kel\u00b7ler", "heims\u00b7ten", "ein"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "$,", "NN", "VVFIN", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Sommers h\u00f6chste Gaben:", "tokens": ["Des", "Som\u00b7mers", "h\u00f6chs\u00b7te", "Ga\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So sei gesegnet Korn und Wein,", "tokens": ["So", "sei", "ge\u00b7seg\u00b7net", "Korn", "und", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn wir geerntet haben.", "tokens": ["Wenn", "wir", "ge\u00b7ern\u00b7tet", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Gesegnet sei in Bl\u00fct' und Frucht", "tokens": ["Ge\u00b7seg\u00b7net", "sei", "in", "Bl\u00fct'", "und", "Frucht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vor allen Gottesgaben!", "tokens": ["Vor", "al\u00b7len", "Got\u00b7tes\u00b7ga\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mag nun des Winters Sturm und Wucht", "tokens": ["Mag", "nun", "des", "Win\u00b7ters", "Sturm", "und", "Wucht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das letzte Blatt begraben.", "tokens": ["Das", "letz\u00b7te", "Blatt", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Sein Scheiden zu ertragen;", "tokens": ["Sein", "Schei\u00b7den", "zu", "er\u00b7tra\u00b7gen", ";"], "token_i