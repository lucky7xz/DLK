{"dta.poem.9715": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An eine hoff\u00e4rtige he\u00dfliche.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.85", "en:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht bilde dir was ein/ Licene/ theurstes bild/", "tokens": ["Nicht", "bil\u00b7de", "dir", "was", "ein", "/", "Li\u00b7ce\u00b7ne", "/", "theurs\u00b7tes", "bild", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "PIS", "ART", "$(", "NN", "$(", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wann dein beruffner prei\u00df die gantze stadt erf\u00fcllt.", "tokens": ["Wann", "dein", "be\u00b7ruff\u00b7ner", "prei\u00df", "die", "gant\u00b7ze", "stadt", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wei\u00dft du woher es kommt/ du niedlichs ungeheuer?", "tokens": ["Wei\u00dft", "du", "wo\u00b7her", "es", "kommt", "/", "du", "nied\u00b7lichs", "un\u00b7ge\u00b7heu\u00b7er", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PWAV", "PPER", "VVFIN", "$(", "PPER", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die kleidung macht dich nur/ und nicht die sch\u00f6nheit theuer.", "tokens": ["Die", "klei\u00b7dung", "macht", "dich", "nur", "/", "und", "nicht", "die", "sch\u00f6n\u00b7heit", "theu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word