{"dta.poem.6727": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Gesicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sie sprechen, da\u00df ein Vorwurff, den man sieht,", "tokens": ["Sie", "spre\u00b7chen", ",", "da\u00df", "ein", "Vor\u00b7wurff", ",", "den", "man", "sieht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein unempfindlich Bild in n\u00e4h\u2019re Lufft formiret,", "tokens": ["Ein", "un\u00b7emp\u00b7find\u00b7lich", "Bild", "in", "n\u00e4h'\u00b7re", "Lufft", "for\u00b7mi\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das Staffelweise sich gem\u00e4hlig aufw\u00e4rts f\u00fchret,", "tokens": ["Das", "Staf\u00b7fel\u00b7wei\u00b7se", "sich", "ge\u00b7m\u00e4h\u00b7lig", "auf\u00b7w\u00e4rts", "f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bis es sich in den Grund von unserm Auge zieht.", "tokens": ["Bis", "es", "sich", "in", "den", "Grund", "von", "un\u00b7serm", "Au\u00b7ge", "zieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexand