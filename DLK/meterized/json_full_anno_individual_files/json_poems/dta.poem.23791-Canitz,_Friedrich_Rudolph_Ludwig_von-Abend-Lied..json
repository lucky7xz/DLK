{"dta.poem.23791": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Abend-Lied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1700", "urn": "urn:nbn:de:kobv:b4-200905197532", "language": ["de:0.99"], "booktitle": "[Canitz, Friedrich Rudolph Ludwig von]: Neben-Stunden Unterschiedener Gedichte. [Hrsg. v. Joachim Lange]. Berlin, 1700."}, "poem": {"stanza.1": {"line.1": {"text": "GoTT du l\u00e4ssest mich erreichen", "tokens": ["GoTT", "du", "l\u00e4s\u00b7sest", "mich", "er\u00b7rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Abermahl die Abend-Zeit/", "tokens": ["A\u00b7ber\u00b7mahl", "die", "A\u00b7ben\u00b7dZeit", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das ist mir ein neues Zeichen", "tokens": ["Das", "ist", "mir", "ein", "neu\u00b7es", "Zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Deiner Lieb und G\u00fctigkeit/", "tokens": ["Dei\u00b7ner", "Lieb", "und", "G\u00fc\u00b7tig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00df jetzund mein schlechtes Singen", "tokens": ["La\u00df", "je\u00b7tzund", "mein", "schlech\u00b7tes", "Sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Durch die tr\u00fcbe Wolcken dringen/", "tokens": ["Durch", "die", "tr\u00fc\u00b7be", "Wol\u00b7cken", "drin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und sey gegen diese Nacht", "tokens": ["Und", "sey", "ge\u00b7gen", "die\u00b7se", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "PDAT", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Ferner auf mein Heyl bedacht.", "tokens": ["Fer\u00b7ner", "auf", "mein", "Heyl", "be\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Neige dich zu meinem Bitten/", "tokens": ["Nei\u00b7ge", "dich", "zu", "mei\u00b7nem", "Bit\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sto\u00df nicht dis mein Opffer weg.", "tokens": ["Sto\u00df", "nicht", "dis", "mein", "Opf\u00b7fer", "weg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "PDS", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hab\u2019 ich gleich oft \u00fcberschritten", "tokens": ["Hab'", "ich", "gleich", "oft", "\u00fc\u00b7bersc\u00b7hrit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deiner Warheit heilgen Steg/", "tokens": ["Dei\u00b7ner", "War\u00b7heit", "heil\u00b7gen", "Steg", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "So verfluch ich meine S\u00fcnden/", "tokens": ["So", "ver\u00b7fluch", "ich", "mei\u00b7ne", "S\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und wil mich mit dir verbinden/", "tokens": ["Und", "wil", "mich", "mit", "dir", "ver\u00b7bin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "APPR", "PPER", "VVINF", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Rei\u00df du nur aus meiner Brust", "tokens": ["Rei\u00df", "du", "nur", "aus", "mei\u00b7ner", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Alle Wurtzel b\u00f6ser Lust.", "tokens": ["Al\u00b7le", "Wurt\u00b7zel", "b\u00f6\u00b7ser", "Lust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Herr es sey mein Leib und Leben/", "tokens": ["Herr", "es", "sey", "mein", "Leib", "und", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VAFIN", "PPOSAT", "NN", "KON", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und was du mir hast geschenckt/", "tokens": ["Und", "was", "du", "mir", "hast", "ge\u00b7schenckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PPER", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deiner Allmacht \u00fcbergeben/", "tokens": ["Dei\u00b7ner", "All\u00b7macht", "\u00fc\u00b7ber\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die den Himmel selbst beschr\u00e4nckt.", "tokens": ["Die", "den", "Him\u00b7mel", "selbst", "be\u00b7schr\u00e4nckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00df \u00fcm mich und \u00fcm die Meinen", "tokens": ["La\u00df", "\u00fcm", "mich", "und", "\u00fcm", "die", "Mei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "VVFIN", "PPER", "KON", "ADJD", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Einen Strahl der Gottheit scheinen/", "tokens": ["Ei\u00b7nen", "Strahl", "der", "Got\u00b7theit", "schei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Der was deinen Namen tr\u00e4gt/", "tokens": ["Der", "was", "dei\u00b7nen", "Na\u00b7men", "tr\u00e4gt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PWS", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Als dein Guth zu sch\u00e4tzen pflegt.", "tokens": ["Als", "dein", "Guth", "zu", "sch\u00e4t\u00b7zen", "pflegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "La\u00df mich mildiglich bethauen/", "tokens": ["La\u00df", "mich", "mil\u00b7dig\u00b7lich", "be\u00b7thau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deines Seegens Uberflu\u00df/", "tokens": ["Dei\u00b7nes", "See\u00b7gens", "U\u00b7ber\u00b7flu\u00df", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schirme mich f\u00fcr Angst und Grauen/", "tokens": ["Schir\u00b7me", "mich", "f\u00fcr", "Angst", "und", "Grau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "NN", "KON", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wende Schaden und Verdru\u00df/", "tokens": ["Wen\u00b7de", "Scha\u00b7den", "und", "Ver\u00b7dru\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Brand und sonst betr\u00fcbte F\u00e4lle.", "tokens": ["Brand", "und", "sonst", "be\u00b7tr\u00fcb\u00b7te", "F\u00e4l\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zeichne meines Hauses Schwelle/", "tokens": ["Zeich\u00b7ne", "mei\u00b7nes", "Hau\u00b7ses", "Schwel\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df hier keinen nicht der Schlag", "tokens": ["Da\u00df", "hier", "kei\u00b7nen", "nicht", "der", "Schlag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PIAT", "PTKNEG", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Des Verderbers treffen mag.", "tokens": ["Des", "Ver\u00b7der\u00b7bers", "tref\u00b7fen", "mag."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["ART", "NN", "VVINF", "VMFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Wircke du in meinen Sinnen/", "tokens": ["Wir\u00b7cke", "du", "in", "mei\u00b7nen", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wohne mir im Schatten bey/", "tokens": ["Woh\u00b7ne", "mir", "im", "Schat\u00b7ten", "bey", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "APPR", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df mein schlaffendes Beginnen", "tokens": ["Da\u00df", "mein", "schlaf\u00b7fen\u00b7des", "Be\u00b7gin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dir auch nicht zu wider sey.", "tokens": ["Dir", "auch", "nicht", "zu", "wi\u00b7der", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKNEG", "PTKZU", "PTKVZ", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schaffe da\u00df ich schon auf Erden", "tokens": ["Schaf\u00b7fe", "da\u00df", "ich", "schon", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOUS", "PPER", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mag ein solcher Tempel werden/", "tokens": ["Mag", "ein", "sol\u00b7cher", "Tem\u00b7pel", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "PIAT", "NN", "VAINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Der nur dir/ und nicht der Welt/", "tokens": ["Der", "nur", "dir", "/", "und", "nicht", "der", "Welt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "$(", "KON", "PTKNEG", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ewig Licht und Feuer h\u00e4lt.", "tokens": ["E\u00b7wig", "Licht", "und", "Feu\u00b7er", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Geht ihr meine m\u00fcde Glieder/", "tokens": ["Geht", "ihr", "mei\u00b7ne", "m\u00fc\u00b7de", "Glie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Geht und senckt euch in die Ruh/", "tokens": ["Geht", "und", "senckt", "euch", "in", "die", "Ruh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn ihr euch regt morgen wieder/", "tokens": ["Wenn", "ihr", "euch", "regt", "mor\u00b7gen", "wie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "ADV", "ADV", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Schreibt es eurem Sch\u00f6pffer zu/", "tokens": ["Schreibt", "es", "eu\u00b7rem", "Sch\u00f6pf\u00b7fer", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PTKZU", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der so treue Wacht gehalten.", "tokens": ["Der", "so", "treu\u00b7e", "Wacht", "ge\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn ihr aber m\u00fc\u00dft erkalten/", "tokens": ["Wenn", "ihr", "a\u00b7ber", "m\u00fc\u00dft", "er\u00b7kal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VMFIN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wird des bittern Todes-Pein", "tokens": ["Wird", "des", "bit\u00b7tern", "To\u00b7des\u00b7Pein"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Doch der Seelen Vortheil seyn.", "tokens": ["Doch", "der", "See\u00b7len", "Vor\u00b7theil", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}