{"dta.poem.18246": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "25.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Ein ew'ger Wechsel ist die Welt;", "tokens": ["Ein", "ew'\u00b7ger", "Wech\u00b7sel", "ist", "die", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der kleinste Stein lehrt's dich auf Erden,", "tokens": ["Der", "kleins\u00b7te", "Stein", "lehrt's", "dich", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er w\u00e4chst, er dauert und zerschellt.", "tokens": ["Er", "w\u00e4chst", ",", "er", "dau\u00b7ert", "und", "zer\u00b7schellt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "KON", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und dennoch willst du sie verklagen,", "tokens": ["Und", "den\u00b7noch", "willst", "du", "sie", "ver\u00b7kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Parze, die den Faden spinnt,", "tokens": ["Die", "Par\u00b7ze", ",", "die", "den", "Fa\u00b7den", "spinnt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und meinst du k\u00f6nntest es nicht tragen,", "tokens": ["Und", "meinst", "du", "k\u00f6nn\u00b7test", "es", "nicht", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn dir ein Traum in Nichts zerrinnt?", "tokens": ["Wenn", "dir", "ein", "Traum", "in", "Nichts", "zer\u00b7rinnt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O sieh, wie hoch sich dir zu H\u00e4upten", "tokens": ["O", "sieh", ",", "wie", "hoch", "sich", "dir", "zu", "H\u00e4up\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PTKVZ", "$,", "PWAV", "ADJD", "PRF", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Sterne drehn im Sph\u00e4rentanz;", "tokens": ["Die", "Ster\u00b7ne", "drehn", "im", "Sph\u00e4\u00b7ren\u00b7tanz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "CARD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer wei\u00df, ob sie nicht l\u00e4ngst zerst\u00e4ubten,", "tokens": ["Wer", "wei\u00df", ",", "ob", "sie", "nicht", "l\u00e4ngst", "zer\u00b7st\u00e4ub\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und dennoch blendet dich ihr Glanz.", "tokens": ["Und", "den\u00b7noch", "blen\u00b7det", "dich", "ihr", "Glanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So l\u00e4\u00dft Erinnrung auch im Busen", "tokens": ["So", "l\u00e4\u00dft", "E\u00b7rinn\u00b7rung", "auch", "im", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dich dein versunknes Eden sehn", "tokens": ["Dich", "dein", "ver\u00b7sunk\u00b7nes", "E\u00b7den", "sehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und durch die goldne Gunst der Musen", "tokens": ["Und", "durch", "die", "gold\u00b7ne", "Gunst", "der", "Mu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In Liedern wieder auferstehn.", "tokens": ["In", "Lie\u00b7dern", "wie\u00b7der", "auf\u00b7er\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}