{"dta.poem.15776": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "36.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So breit geworden ist nun Kunst und Wissenschaft,", "tokens": ["So", "breit", "ge\u00b7wor\u00b7den", "ist", "nun", "Kunst", "und", "Wis\u00b7sen\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAPP", "VAFIN", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Umfassen k\u00f6nnten sie nur Arme riesenhaft.", "tokens": ["Um\u00b7fas\u00b7sen", "k\u00f6nn\u00b7ten", "sie", "nur", "Ar\u00b7me", "rie\u00b7sen\u00b7haft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Man sollte sie der Welt in kurzen Auszug bringen;", "tokens": ["Man", "soll\u00b7te", "sie", "der", "Welt", "in", "kur\u00b7zen", "Aus\u00b7zug", "brin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch in die Knospe wer kann Rosen wieder zwingen?", "tokens": ["Doch", "in", "die", "Knos\u00b7pe", "wer", "kann", "Ro\u00b7sen", "wie\u00b7der", "zwin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PWS", "VMFIN", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Am besten wenigstens g\u00e4bst du, statt neuen Lenz,", "tokens": ["Am", "bes\u00b7ten", "we\u00b7nigs\u00b7tens", "g\u00e4bst", "du", ",", "statt", "neu\u00b7en", "Lenz", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "ADV", "VVFIN", "PPER", "$,", "KOUI", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O Dichter, uns von dir sogleich die Quintessenz.", "tokens": ["O", "Dich\u00b7ter", ",", "uns", "von", "dir", "sog\u00b7leich", "die", "Quin\u00b7tes\u00b7senz", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "APPR", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch nur die Zeit \u2014 o zieh du deinen Flor indessen", "tokens": ["Doch", "nur", "die", "Zeit", "o", "zieh", "du", "dei\u00b7nen", "Flor", "in\u00b7des\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "$(", "FM", "VVFIN", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Kann kochen Rosen\u00f6l und Rosenwasser pressen.", "tokens": ["Kann", "ko\u00b7chen", "Ro\u00b7sen\u00b7\u00f6l", "und", "Ro\u00b7sen\u00b7was\u00b7ser", "pres\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJA", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}