{"textgrid.poem.56208": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "8", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich mu\u00df den Zweig, den b\u00f6sen Rosenzweig", "tokens": ["Ich", "mu\u00df", "den", "Zweig", ",", "den", "b\u00f6\u00b7sen", "Ro\u00b7sen\u00b7zweig"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verklagen.", "tokens": ["Ver\u00b7kla\u00b7gen", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Er bat so sanft, wie sollt ich den ihm gleich", "tokens": ["Er", "bat", "so", "sanft", ",", "wie", "sollt", "ich", "den", "ihm", "gleich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "PWAV", "VMFIN", "PPER", "ART", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Versagen?", "tokens": ["Ver\u00b7sa\u00b7gen", "?"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Doch war's, da\u00df ich ihn selbst zum Strauch gef\u00fchrt,", "tokens": ["Doch", "wa\u00b7r's", ",", "da\u00df", "ich", "ihn", "selbst", "zum", "Strauch", "ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "KOUS", "PPER", "PPER", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Nicht weise,", "tokens": ["Nicht", "wei\u00b7se", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Wo seine Hand die meinige ber\u00fchrt,", "tokens": ["Wo", "sei\u00b7ne", "Hand", "die", "mei\u00b7ni\u00b7ge", "be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ART", "PPOSS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So leise.", "tokens": ["So", "lei\u00b7se", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Und als er z\u00f6gernd aus dem Garten war", "tokens": ["Und", "als", "er", "z\u00f6\u00b7gernd", "aus", "dem", "Gar\u00b7ten", "war"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADJD", "APPR", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gegangen,", "tokens": ["Ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Stand zitternd ich, als h\u00e4tt ich B\u00f6ses gar", "tokens": ["Stand", "zit\u00b7ternd", "ich", ",", "als", "h\u00e4tt", "ich", "B\u00f6\u00b7ses", "gar"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "$,", "KOKOM", "VAFIN", "PPER", "NE", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Begangen.", "tokens": ["Be\u00b7gan\u00b7gen", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "O h\u00e4tt ich seiner holden Rede nicht", "tokens": ["O", "h\u00e4tt", "ich", "sei\u00b7ner", "hol\u00b7den", "Re\u00b7de", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": 