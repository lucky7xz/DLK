{"dta.poem.18512": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die in das Paradie\u00df gehende Eva/  \n bey beerdigung Jungfer Eva Maria  \n Rivinin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wje schwer und traurig mu\u00df der gang gewesen seyn/", "tokens": ["Wie", "schwer", "und", "trau\u00b7rig", "mu\u00df", "der", "gang", "ge\u00b7we\u00b7sen", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VMFIN", "ART", "NN", "VAPP", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den einst das erste weib/ die Eva/ hat erlitten/", "tokens": ["Den", "einst", "das", "ers\u00b7te", "weib", "/", "die", "E\u00b7va", "/", "hat", "er\u00b7lit\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "$(", "ART", "NN", "$(", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als ihre f\u00fcsse sind zu ihrer gr\u00f6sten pein", "tokens": ["Als", "ih\u00b7re", "f\u00fcs\u00b7se", "sind", "zu", "ih\u00b7rer", "gr\u00f6s\u00b7ten", "pein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "VAFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Aus jenem lust-revier und paradies geschritten.", "tokens": ["Aus", "je\u00b7nem", "lust\u00b7re\u00b7vier", "und", "pa\u00b7ra\u00b7dies", "ge\u00b7schrit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Kaum hatte sie den schmuck von Eden recht gesehn/", "tokens": ["Kaum", "hat\u00b7te", "sie", "den", "schmuck", "von", "E\u00b7den", "recht", "ge\u00b7sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "APPR", "NN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Kaum war der apffel-bi\u00df verbotner frucht genossen/", "tokens": ["Kaum", "war", "der", "apf\u00b7fel\u00b7bi\u00df", "ver\u00b7bot\u00b7ner", "frucht", "ge\u00b7nos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So war es wieder schon um den besitz geschehn;", "tokens": ["So", "war", "es", "wie\u00b7der", "schon", "um", "den", "be\u00b7sitz", "ge\u00b7schehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "APPR", "ART", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sie muste fl\u00fcchtig seyn/ der garten ward verschlossen.", "tokens": ["Sie", "mus\u00b7te", "fl\u00fcch\u00b7tig", "seyn", "/", "der", "gar\u00b7ten", "ward", "ver\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "VAINF", "$(", "ART", "VVFIN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ach! sagte sie vielleicht/ ihr b\u00e4ume gute nacht/", "tokens": ["Ach", "!", "sag\u00b7te", "sie", "viel\u00b7leicht", "/", "ihr", "b\u00e4u\u00b7me", "gu\u00b7te", "nacht", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "ADV", "$(", "PPER", "VVFIN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Jhr blumen lebet wohl/ lebt wohl begl\u00fcckten auen/", "tokens": ["Ihr", "blu\u00b7men", "le\u00b7bet", "wohl", "/", "lebt", "wohl", "be\u00b7gl\u00fcck\u00b7ten", "au\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$(", "VVFIN", "ADV", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ich werd\u2019/ ich soll und mu\u00df nun leider eure pracht/", "tokens": ["Ich", "werd'", "/", "ich", "soll", "und", "mu\u00df", "nun", "lei\u00b7der", "eu\u00b7re", "pracht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "PPER", "VMFIN", "KON", "VMFIN", "ADV", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die mich so sehr vergn\u00fcgt/ nicht ferner wieder schauen.", "tokens": ["Die", "mich", "so", "sehr", "ver\u00b7gn\u00fcgt", "/", "nicht", "fer\u00b7ner", "wie\u00b7der", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VVPP", "$(", "PTKNEG", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ein apfel raubet mir den gantzen garten hin/", "tokens": ["Ein", "ap\u00b7fel", "rau\u00b7bet", "mir", "den", "gant\u00b7zen", "gar\u00b7ten", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "ADJA", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Es warten schon auff mich viel schmertzliche beschwerden/", "tokens": ["Es", "war\u00b7ten", "schon", "auff", "mich", "viel", "schmertz\u00b7li\u00b7che", "be\u00b7schwer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "PIAT", "ADJA", "VVINF", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.15": {"text": "Noth/ arbeit und verdru\u00df bleibt k\u00fcnfftig mein gewinn/", "tokens": ["Noth", "/", "ar\u00b7beit", "und", "ver\u00b7dru\u00df", "bleibt", "k\u00fcnff\u00b7tig", "mein", "ge\u00b7winn", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "KON", "NN", "VVFIN", "ADJD", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Lebt wohl/ mein paradi\u00df mu\u00df mir zur w\u00fcste werden.", "tokens": ["Lebt", "wohl", "/", "mein", "pa\u00b7ra\u00b7di\u00df", "mu\u00df", "mir", "zur", "w\u00fcs\u00b7te", "wer\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$(", "PPOSAT", "NN", "VMFIN", "PPER", "APPRART", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Hoch-selige/ du hast den nahmen auch gef\u00fchrt/", "tokens": ["Hoch\u00b7se\u00b7li\u00b7ge", "/", "du", "hast", "den", "nah\u00b7men", "auch", "ge\u00b7f\u00fchrt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PPER", "VAFIN", "ART", "VVFIN", "ADV", "VVPP", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.18": {"text": "Den Eva vormahls hat im paradie\u00df bekommen/", "tokens": ["Den", "E\u00b7va", "vor\u00b7mahls", "hat", "im", "pa\u00b7ra\u00b7die\u00df", "be\u00b7kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und hast/ wie Even einst der ursprung hat geziert/", "tokens": ["Und", "hast", "/", "wie", "E\u00b7ven", "einst", "der", "ur\u00b7sprung", "hat", "ge\u00b7ziert", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$(", "KOKOM", "NE", "ADV", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Den anfang gleichsam auch im paradie\u00df genommen.", "tokens": ["Den", "an\u00b7fang", "gleich\u00b7sam", "auch", "im", "pa\u00b7ra\u00b7die\u00df", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Denn deines vaters hau\u00df war Edens garten gleich/", "tokens": ["Denn", "dei\u00b7nes", "va\u00b7ters", "hau\u00df", "war", "E\u00b7dens", "gar\u00b7ten", "gleich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VAFIN", "NE", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Aus dem so mancher bach", "tokens": ["Aus", "dem", "so", "man\u00b7cher", "bach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Die/ weil sie allerseits an gold der wei\u00dfheit reich/", "tokens": ["Die", "/", "weil", "sie", "al\u00b7ler\u00b7seits", "an", "gold", "der", "wei\u00df\u00b7heit", "reich", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "KOUS", "PPER", "ADV", "APPR", "NN", "ART", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Sich in die weite welt mit tausend nutz ergossen.", "tokens": ["Sich", "in", "die", "wei\u00b7te", "welt", "mit", "tau\u00b7send", "nutz", "er\u00b7gos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "APPR", "CARD", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Zu dem so ist hier auch das werthe Gottes-hau\u00df", "tokens": ["Zu", "dem", "so", "ist", "hier", "auch", "das", "wert\u00b7he", "Got\u00b7tes\u00b7hau\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "VAFIN", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Dir an vergn\u00fcgungen ein paradie\u00df gewesen;", "tokens": ["Dir", "an", "ver\u00b7gn\u00fc\u00b7gun\u00b7gen", "ein", "pa\u00b7ra\u00b7die\u00df", "ge\u00b7we\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "VVFIN", "ART", "NN", "VAPP", "$."], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.27": {"text": "In dieses giengest du mit freuden ein und aus/", "tokens": ["In", "die\u00b7ses", "gien\u00b7gest", "du", "mit", "freu\u00b7den", "ein", "und", "aus", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVFIN", "PPER", "APPR", "NN", "PTKVZ", "KON", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und hast vom lebens-baum erlaubte frucht gelesen.", "tokens": ["Und", "hast", "vom", "le\u00b7bens\u00b7baum", "er\u00b7laub\u00b7te", "frucht", "ge\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPRART", "NN", "VVFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Jtzt zahlst du zwar die schuld/ die Eva hat gemacht/", "tokens": ["Jtzt", "zahlst", "du", "zwar", "die", "schuld", "/", "die", "E\u00b7va", "hat", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJD", "$(", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und wirst dem kalten heer der todten zugef\u00fcget/", "tokens": ["Und", "wirst", "dem", "kal\u00b7ten", "heer", "der", "tod\u00b7ten", "zu\u00b7ge\u00b7f\u00fc\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Du giebst dem theuren paar der br\u00fcder gute nacht/", "tokens": ["Du", "giebst", "dem", "theu\u00b7ren", "paar", "der", "br\u00fc\u00b7der", "gu\u00b7te", "nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und meidest/ was dich sonst hat auf der welt vergn\u00fcget.", "tokens": ["Und", "mei\u00b7dest", "/", "was", "dich", "sonst", "hat", "auf", "der", "welt", "ver\u00b7gn\u00fc\u00b7get", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "PWS", "PPER", "ADV", "VAFIN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Jedoch du bist begl\u00fcckt: denn dein erl\u00f6ster geist", "tokens": ["Je\u00b7doch", "du", "bist", "be\u00b7gl\u00fcckt", ":", "denn", "dein", "er\u00b7l\u00f6s\u00b7ter", "geist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VAFIN", "VVPP", "$.", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Hat einen bessern tausch als Eva einst getroffen.", "tokens": ["Hat", "ei\u00b7nen", "bes\u00b7sern", "tausch", "als", "E\u00b7va", "einst", "ge\u00b7trof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "ADJD", "KOKOM", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Denn nun du aus der welt als einer w\u00fcste reist/", "tokens": ["Denn", "nun", "du", "aus", "der", "welt", "als", "ei\u00b7ner", "w\u00fcs\u00b7te", "reist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "APPR", "ART", "NN", "KOKOM", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Steht dir das paradie\u00df des hohen himmels offen.", "tokens": ["Steht", "dir", "das", "pa\u00b7ra\u00b7die\u00df", "des", "ho\u00b7hen", "him\u00b7mels", "of\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Wohlan! ich lasse hier noch meine letzte pflicht/", "tokens": ["Wo\u00b7hlan", "!", "ich", "las\u00b7se", "hier", "noch", "mei\u00b7ne", "letz\u00b7te", "pflicht", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VVFIN", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Die ich dir schuldig bin/ bey deiner baare schauen/", "tokens": ["Die", "ich", "dir", "schul\u00b7dig", "bin", "/", "bey", "dei\u00b7ner", "baa\u00b7re", "schau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "ADJD", "VAFIN", "$(", "APPR", "PPOSAT", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Und will di\u00df/ was bereits der nachruhm von dir spricht/", "tokens": ["Und", "will", "di\u00df", "/", "was", "be\u00b7reits", "der", "nach\u00b7ruhm", "von", "dir", "spricht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PDS", "$(", "PWS", "ADV", "ART", "NN", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.40": {"text": "Auf deinen leichen-stein an statt der grabschrifft hauen:", "tokens": ["Auf", "dei\u00b7nen", "lei\u00b7chen\u00b7stein", "an", "statt", "der", "grab\u00b7schrifft", "hau\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Hie ruhet Evens leib/ die zu dem br\u00e4utigam", "tokens": ["Hie", "ru\u00b7het", "E\u00b7vens", "leib", "/", "die", "zu", "dem", "br\u00e4u\u00b7ti\u00b7gam"], "token_info": ["word"