{"textgrid.poem.58683": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "115.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Himmel herrschen Heilige, auf Erden gro\u00dfe Tiere,", "tokens": ["Im", "Him\u00b7mel", "herr\u00b7schen", "Hei\u00b7li\u00b7ge", ",", "auf", "Er\u00b7den", "gro\u00b7\u00dfe", "Tie\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADJA", "$,", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Wer hier will gro\u00df und m\u00e4chtig sein, ist eines dieser viere;", "tokens": ["Wer", "hier", "will", "gro\u00df", "und", "m\u00e4ch\u00b7tig", "sein", ",", "ist", "ei\u00b7nes", "die\u00b7ser", "vie\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VMFIN", "ADJD", "KON", "ADJD", "VAINF", "$,", "VAFIN", "PIS", "PDAT", "ADJA", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Ein Heil'ger lebt hier unbekannt in schlechter Knechtsgestalt,", "tokens": ["Ein", "Heil'\u00b7ger", "lebt", "hier", "un\u00b7be\u00b7kannt", "in", "schlech\u00b7ter", "Knechts\u00b7ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Das Reich der Welt kommt und vergeht, ", "tokens": ["Das", "Reich", "der", "Welt", "kommt", "und", "ver\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"],