{"dta.poem.5319": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Augen als Spiegel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Des reinen Wassers klare Flut", "tokens": ["Des", "rei\u00b7nen", "Was\u00b7sers", "kla\u00b7re", "Flut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Stellt nicht nur Kr\u00e4uter, B\u00fcsch\u2019 und H\u00fcgel,", "tokens": ["Stellt", "nicht", "nur", "Kr\u00e4u\u00b7ter", ",", "B\u00fcsch'", "und", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Als wie ein glatt-polirter Spiegel,", "tokens": ["Als", "wie", "ein", "glat\u00b7tpo\u00b7lir\u00b7ter", "Spie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ju noch vermehrter Sch\u00f6nheit f\u00fcr;", "tokens": ["Ju", "noch", "ver\u00b7mehr\u00b7ter", "Sch\u00f6n\u00b7heit", "f\u00fcr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJA", "NN", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie zeigt uns nebst der Sonnen Glut", "tokens": ["Sie", "zeigt", "uns", "nebst", "der", "Son\u00b7nen", "Glut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des gantzen Himmels helle Zier.", "tokens": ["Des", "gant\u00b7zen", "Him\u00b7mels", "hel\u00b7le", "Zier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ach m\u00f6gte gleichfals dir und mir", "tokens": ["Ach", "m\u00f6g\u00b7te", "gleich\u00b7fals", "dir", "und", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "ADV", "PPER", "KON", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das Wasser, so in unsern Augen,", "tokens": ["Das", "Was\u00b7ser", ",", "so", "in", "un\u00b7sern", "Au\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Bey dem so sch\u00f6nen Schmuck des Himmels und der Erden,", "tokens": ["Bey", "dem", "so", "sch\u00f6\u00b7nen", "Schmuck", "des", "Him\u00b7mels", "und", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Zum Welt- und Himmels-Spiegel werden!", "tokens": ["Zum", "Welt", "und", "Him\u00b7mels\u00b7Spie\u00b7gel", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "TRUNC", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Ach m\u00f6gt\u2019 auch die\u00df die Welt zu bilden taugen!", "tokens": ["Ach", "m\u00f6gt'", "auch", "die\u00df", "die", "Welt", "zu", "bil\u00b7den", "tau\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADV", "PDS", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Ach dr\u00fcckte doch der wundersch\u00f6ne Schein", "tokens": ["Ach", "dr\u00fcck\u00b7te", "doch", "der", "wun\u00b7der\u00b7sch\u00f6\u00b7ne", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Von aller Creaturen Pracht,", "tokens": ["Von", "al\u00b7ler", "Crea\u00b7tu\u00b7ren", "Pracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "Zu dessen Ruhm, der sie gemacht,", "tokens": ["Zu", "des\u00b7sen", "Ruhm", ",", "der", "sie", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Durch ihre klare Fluth sich unsern Seelen ein!", "tokens": ["Durch", "ih\u00b7re", "kla\u00b7re", "Fluth", "sich", "un\u00b7sern", "See\u00b7len", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PRF", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So w\u00fcrden sie dadurch, wie uns das Wasser, sch\u00f6n", "tokens": ["So", "w\u00fcr\u00b7den", "sie", "da\u00b7durch", ",", "wie", "uns", "das", "Was\u00b7ser", ",", "sch\u00f6n"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VAFIN", "PPER", "PAV", "$,", "PWAV", "PPER", "ART", "NN", "$,", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Auch andern Geistern anzusehn,", "tokens": ["Auch", "an\u00b7dern", "Geis\u00b7tern", "an\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Und, lieblich ausgeschm\u00fcckt, mit Lust betrachtet seyn.", "tokens": ["Und", ",", "lieb\u00b7lich", "aus\u00b7ge\u00b7schm\u00fcckt", ",", "mit", "Lust", "be\u00b7trach\u00b7tet", "seyn", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJD", "VVPP", "$,", "APPR", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}