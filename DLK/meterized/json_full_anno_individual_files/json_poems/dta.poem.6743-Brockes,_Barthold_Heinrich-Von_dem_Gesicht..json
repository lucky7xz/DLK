{"dta.poem.6743": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Gesicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zur selben Zeit, wenn das Gesicht", "tokens": ["Zur", "sel\u00b7ben", "Zeit", ",", "wenn", "das", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf alle Staffel dieser Weite", "tokens": ["Auf", "al\u00b7le", "Staf\u00b7fel", "die\u00b7ser", "Wei\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Krafft mit allem Ernst gericht;", "tokens": ["Die", "Krafft", "mit", "al\u00b7lem", "Ernst", "ge\u00b7richt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIS", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Formirt die Seel\u2019 ihr Urtheil durch die Schl\u00fcsse,", "tokens": ["For\u00b7mirt", "die", "Seel'", "ihr", "Ur\u00b7theil", "durch", "die", "Schl\u00fcs\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}