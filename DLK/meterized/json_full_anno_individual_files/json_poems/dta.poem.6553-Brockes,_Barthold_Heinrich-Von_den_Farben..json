{"dta.poem.6553": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Farben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das weisse Tisch-Tuch zeigt uns einen rothen Schein,", "tokens": ["Das", "weis\u00b7se", "TischTuch", "zeigt", "uns", "ei\u00b7nen", "ro\u00b7then", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sieht man beym Licht darauf ein Gla\u00df mit rothem Wein;", "tokens": ["Sieht", "man", "beym", "Licht", "da\u00b7rauf", "ein", "Gla\u00df", "mit", "ro\u00b7them", "Wein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPRART", "NN", "PAV", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In Farben kehren sich die Theilchen von dem Licht,", "tokens": ["In", "Far\u00b7ben", "keh\u00b7ren", "sich", "die", "Theil\u00b7chen", "von", "dem", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PRF", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und durch den Wider-Schein erregt er das Gesicht.", "tokens": ["Und", "durch", "den", "Wi\u00b7der\u00b7Schein", "er\u00b7regt", "er", "das", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wir f\u00fchlen ihren Druck sich so ins Auge dringen,", "tokens": ["Wir", "f\u00fch\u00b7len", "ih\u00b7ren", "Druck", "sich", "so", "ins", "Au\u00b7ge", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PRF", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie sie die Ordnungen erst durch das Glas empfingen.", "tokens": ["Wie", "sie", "die", "Ord\u00b7nun\u00b7gen", "erst", "durch", "das", "Glas", "emp\u00b7fin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}