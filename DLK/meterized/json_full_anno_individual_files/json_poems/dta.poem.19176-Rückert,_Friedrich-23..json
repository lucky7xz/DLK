{"dta.poem.19176": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "23.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als Bl\u00fctenalter ist die Jugend wol bekannt,", "tokens": ["Als", "Bl\u00fc\u00b7ten\u00b7al\u00b7ter", "ist", "die", "Ju\u00b7gend", "wol", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VAFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mir aber sei hinfort das Alter so genannt.", "tokens": ["Mir", "a\u00b7ber", "sei", "hin\u00b7fort", "das", "Al\u00b7ter", "so", "ge\u00b7nannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADV", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die junge Pflanz' ist gr\u00fcn; wielang mu\u00df sie sich m\u00fchn", "tokens": ["Die", "jun\u00b7ge", "Pflanz'", "ist", "gr\u00fcn", ";", "wie\u00b7lang", "mu\u00df", "sie", "sich", "m\u00fchn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "$.", "VVFIN", "VMFIN", "PPER", "PRF", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch Blatt und Zweig hindurch, bis ihr gelingt zu bl\u00fchn!", "tokens": ["Durch", "Blatt", "und", "Zweig", "hin\u00b7durch", ",", "bis", "ihr", "ge\u00b7lingt", "zu", "bl\u00fchn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PAV", "$,", "KOUS", "PPER", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ihr letztes ist das Bl\u00fchn, nicht erstes, zweifelsohne;", "tokens": ["Ihr", "letz\u00b7tes", "ist", "das", "Bl\u00fchn", ",", "nicht", "ers\u00b7tes", ",", "zwei\u00b7fel\u00b7soh\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VAFIN", "ART", "NN", "$,", "PTKNEG", "ADJA", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dann stirbt sie wann sie aufgesetzt die Bl\u00fctenkrone.", "tokens": ["Dann", "stirbt", "sie", "wann", "sie", "auf\u00b7ge\u00b7setzt", "die", "Bl\u00fc\u00b7ten\u00b7kro\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PWAV", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wie in der Jugend auch als Raupe kriecht, im Alter", "tokens": ["Wie", "in", "der", "Ju\u00b7gend", "auch", "als", "Rau\u00b7pe", "kriecht", ",", "im", "Al\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ADV", "KOUS", "NN", "VVFIN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die bl\u00fctengleiche Schwing' entfaltet der Zwiefalter.", "tokens": ["Die", "bl\u00fc\u00b7ten\u00b7glei\u00b7che", "Schwing'", "ent\u00b7fal\u00b7tet", "der", "Zwie\u00b7fal\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Doch fragst du wo denn sei des Alters Schwing' und Bl\u00fcte?", "tokens": ["Doch", "fragst", "du", "wo", "denn", "sei", "des", "Al\u00b7ters", "Schwing'", "und", "Bl\u00fc\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PWAV", "ADV", "VAFIN", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So sag' ich: au\u00dfen nicht, doch innen im Gem\u00fcte.", "tokens": ["So", "sag'", "ich", ":", "au\u00b7\u00dfen", "nicht", ",", "doch", "in\u00b7nen", "im", "Ge\u00b7m\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "ADV", "PTKNEG", "$,", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Das ist die Bl\u00fcte, die hier athmet Seelenduft,", "tokens": ["Das", "ist", "die", "Bl\u00fc\u00b7te", ",", "die", "hier", "ath\u00b7met", "See\u00b7len\u00b7duft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dis Silfenfl\u00fcgelpaar tr\u00e4gt \u00fcber Welt und Gruft.", "tokens": ["Dis", "Sil\u00b7fen\u00b7fl\u00fc\u00b7gel\u00b7paar", "tr\u00e4gt", "\u00fc\u00b7ber", "Welt", "und", "Gruft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}