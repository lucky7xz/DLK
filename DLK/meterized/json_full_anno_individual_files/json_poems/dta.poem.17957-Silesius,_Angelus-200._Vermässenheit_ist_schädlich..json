{"dta.poem.17957": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "200.  Verm\u00e4ssenheit ist sch\u00e4dlich.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Vermi\u00df dich Jungfrau nicht/ wer in Gefahr sich giebt/", "tokens": ["Ver\u00b7mi\u00df", "dich", "Jung\u00b7frau", "nicht", "/", "wer", "in", "Ge\u00b7fahr", "sich", "giebt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "PTKNEG", "$(", "PWS", "APPR", "NN", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der wird gemeiniglich gef\u00e4hret und betr\u00fcbt.", "tokens": ["Der", "wird", "ge\u00b7mei\u00b7nig\u00b7lich", "ge\u00b7f\u00e4h\u00b7ret", "und", "be\u00b7tr\u00fcbt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}