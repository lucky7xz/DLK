{"textgrid.poem.48399": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Gro\u00dfer, denke nicht an meinen faulen Sinn,", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Gro\u00dfer, denke nicht an meinen faulen Sinn,", "tokens": ["O", "Gro\u00b7\u00dfer", ",", "den\u00b7ke", "nicht", "an", "mei\u00b7nen", "fau\u00b7len", "Sinn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "der nichts als Unrecht tut und von dir f\u00e4llet hin,", "tokens": ["der", "nichts", "als", "Un\u00b7recht", "tut", "und", "von", "dir", "f\u00e4l\u00b7let", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "KOKOM", "NN", "VVFIN", "KON", "APPR", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "der dich verl\u00e4\u00dft und irrt! Du bist der Sonnen Zier,", "tokens": ["der", "dich", "ver\u00b7l\u00e4\u00dft", "und", "irrt", "!", "Du", "bist", "der", "Son\u00b7nen", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "KON", "VVFIN", "$.", "PPER", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "die auf- und niedergeht. Verwundre dich in dir!", "tokens": ["die", "auf", "und", "nie\u00b7der\u00b7geht", ".", "Ver\u00b7wund\u00b7re", "dich", "in", "dir", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "