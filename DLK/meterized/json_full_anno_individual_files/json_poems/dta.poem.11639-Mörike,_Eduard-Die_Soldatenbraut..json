{"dta.poem.11639": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Die Soldatenbraut.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Wie wacker mein Schatzelein ist!", "tokens": ["Wie", "wa\u00b7cker", "mein", "Schat\u00b7zel\u00b7ein", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "F\u00fcr den K\u00f6nig, da lie\u00df er sein Blut,", "tokens": ["F\u00fcr", "den", "K\u00f6\u00b7nig", ",", "da", "lie\u00df", "er", "sein", "Blut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "F\u00fcr mich aber eben so gut.", "tokens": ["F\u00fcr", "mich", "a\u00b7ber", "e\u00b7ben", "so", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Mein Schatz hat kein Band und kein' Stern,", "tokens": ["Mein", "Schatz", "hat", "kein", "Band", "und", "kein'", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Kein Kreuz wie die vornehmen Herrn,", "tokens": ["Kein", "Kreuz", "wie", "die", "vor\u00b7neh\u00b7men", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Schatz wird auch kein General:", "tokens": ["Mein", "Schatz", "wird", "auch", "kein", "Ge\u00b7ne\u00b7ral", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00e4tt' er nur seinen Abschied einmal!", "tokens": ["H\u00e4tt'", "er", "nur", "sei\u00b7nen", "Ab\u00b7schied", "ein\u00b7mal", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Es scheinen drei Sterne so hell", "tokens": ["Es", "schei\u00b7nen", "drei", "Ster\u00b7ne", "so", "hell"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "CARD", "NN", "ADV", "ADJD"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Dort \u00fcber Marien-Kapell;", "tokens": ["Dort", "\u00fc\u00b7ber", "Ma\u00b7ri\u00b7en\u00b7Ka\u00b7pell", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da kn\u00fcpft uns ein rosenroth Band,", "tokens": ["Da", "kn\u00fcpft", "uns", "ein", "ro\u00b7sen\u00b7roth", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJD", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Und ein Hauskreuz ist auch bei der Hand.", "tokens": ["Und", "ein", "Haus\u00b7kreuz", "ist", "auch", "bei", "der", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}}}}}