{"dta.poem.23505": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Vlysses vnd Si-  \n rene.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1619", "urn": "urn:nbn:de:kobv:b4-20627-4", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Den lust sich lassen \u00fcber winden:", "tokens": ["Den", "lust", "sich", "las\u00b7sen", "\u00fc\u00b7ber", "win\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "VVINF", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dan eben nach m\u00fch vnd verdru\u00df", "tokens": ["Dan", "e\u00b7ben", "nach", "m\u00fch", "vnd", "ver\u00b7dru\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ADJD", "KON", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Kan man der ruh besser gen\u00fcessen;", "tokens": ["Kan", "man", "der", "ruh", "bes\u00b7ser", "ge\u00b7n\u00fces\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.4": {"text": "So kan auch des lusts \u00fcberflu\u00df", "tokens": ["So", "kan", "auch", "des", "lusts", "\u00fc\u00b7berf\u00b7lu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vns sowol als die m\u00fch verdriessen.", "tokens": ["Vns", "so\u00b7wol", "als", "die", "m\u00fch", "ver\u00b7dries\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "KOUS", "ART", "ADJD", "VVPP", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}}}}