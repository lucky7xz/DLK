{"dta.poem.8231": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Die Stadt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Und seitab liegt die Stadt;", "tokens": ["Und", "seit\u00b7ab", "liegt", "die", "Stadt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Der Nebel deckt die D\u00e4cher schwer,", "tokens": ["Der", "Ne\u00b7bel", "deckt", "die", "D\u00e4\u00b7cher", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und durch die Stille braust das Meer", "tokens": ["Und", "durch", "die", "Stil\u00b7le", "braust", "das", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Eint\u00f6nig um die Stadt.", "tokens": ["Ein\u00b7t\u00f6\u00b7nig", "um", "die", "Stadt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Es rauscht kein Wald, es schl\u00e4gt im Mai", "tokens": ["Es", "rauscht", "kein", "Wald", ",", "es", "schl\u00e4gt", "im", "Mai"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Vogel ohn' Unterla\u00df;", "tokens": ["Kein", "Vo\u00b7gel", "ohn'", "Un\u00b7ter\u00b7la\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Wandergans mit hartem Schrei", "tokens": ["Die", "Wan\u00b7der\u00b7gans", "mit", "har\u00b7tem", "Schrei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur fliegt in Herbstesnacht vorbei,", "tokens": ["Nur", "fliegt", "in", "Herbs\u00b7tes\u00b7nacht", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Am Strande weht das Gras.", "tokens": ["Am", "Stran\u00b7de", "weht", "das", "Gras", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch h\u00e4ngt mein ganzes Herz an dir,", "tokens": ["Doch", "h\u00e4ngt", "mein", "gan\u00b7zes", "Herz", "an", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du graue Stadt am Meer;", "tokens": ["Du", "grau\u00b7e", "Stadt", "am", "Meer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Jugend Zauber f\u00fcr und f\u00fcr", "tokens": ["Der", "Ju\u00b7gend", "Zau\u00b7ber", "f\u00fcr", "und", "f\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "APPR", "KON", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ruht l\u00e4chelnd doch auf dir, auf dir,", "tokens": ["Ruht", "l\u00e4\u00b7chelnd", "doch", "auf", "dir", ",", "auf", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "APPR", "PPER", "$,", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du graue Stadt am Meer.", "tokens": ["Du", "grau\u00b7e", "Stadt", "am", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}