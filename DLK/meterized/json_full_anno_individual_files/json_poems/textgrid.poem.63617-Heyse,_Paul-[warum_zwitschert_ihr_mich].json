{"textgrid.poem.63617": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "[warum zwitschert ihr mich]", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Warum zwitschert ihr mich", "tokens": ["Wa\u00b7rum", "zwit\u00b7schert", "ihr", "mich"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Um meinen Morgenschlaf", "tokens": ["Um", "mei\u00b7nen", "Mor\u00b7gen\u00b7schlaf"], "token_info": ["word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit scharfem Weckruf,", "tokens": ["Mit", "schar\u00b7fem", "Weck\u00b7ruf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Grausame V\u00f6gel!", "tokens": ["Grau\u00b7sa\u00b7me", "V\u00f6\u00b7gel", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Ach, ihr scheuchet", "tokens": ["Ach", ",", "ihr", "scheu\u00b7chet"], "token_info": ["word", "punct", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Mir von der Seite", "tokens": ["Mir", "von", "der", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Den einz'gen Freund und Erbarmer,", "tokens": ["Den", "einz'\u00b7gen", "Freund", "und", "Er\u00b7bar\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der bei mir aushielt,", "tokens": ["Der", "bei", "mir", "aus\u00b7hielt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Da vom Haupte", "tokens": ["Da", "vom", "Haup\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPRART", "NN"], "meter": "--+-", "measure": "anapaest.init"}, "line.6": {"text": "Des G\u00f6tterverfemten", "tokens": ["Des", "G\u00f6t\u00b7ter\u00b7ver\u00b7fem\u00b7ten"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Entsetzt hinwegflohn", "tokens": ["Ent\u00b7setzt", "hin\u00b7weg\u00b7flohn"], "token_info": ["word", "word"], "pos": ["FM.la", "FM.la"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Alle guten Geister.", "tokens": ["Al\u00b7le", "gu\u00b7ten", "Geis\u00b7ter", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Wie qualvoll lang", "tokens": ["Wie", "qual\u00b7voll", "lang"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJD", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Im purpurnen Abgrund der Nacht,", "tokens": ["Im", "pur\u00b7pur\u00b7nen", "Ab\u00b7grund", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Zu dem hinunter", "tokens": ["Zu", "dem", "hin\u00b7un\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Kein Strahl des Friedens tauchte,", "tokens": ["Kein", "Strahl", "des", "Frie\u00b7dens", "tauch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Lag ich mit fieberbangen Sinnen,", "tokens": ["Lag", "ich", "mit", "fie\u00b7ber\u00b7ban\u00b7gen", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Aus furchtbarn Tr\u00e4umen", "tokens": ["Aus", "furcht\u00b7barn", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Zur\u00fcckgeschreckt", "tokens": ["Zu\u00b7r\u00fcck\u00b7ge\u00b7schreckt"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Ins schreckenvollere", "tokens": ["Ins", "schre\u00b7cken\u00b7vol\u00b7le\u00b7re"], "token_info": ["word", "word"], "pos": ["APPRART", "ADJA"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Wache Bewu\u00dftsein", "tokens": ["Wa\u00b7che", "Be\u00b7wu\u00df\u00b7tsein"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.10": {"text": "Meines Ungl\u00fccks,", "tokens": ["Mei\u00b7nes", "Un\u00b7gl\u00fccks", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Bis endlich nachgab", "tokens": ["Bis", "end\u00b7lich", "nach\u00b7gab"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADV", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Der leidermattete Leib", "tokens": ["Der", "lei\u00b7der\u00b7mat\u00b7te\u00b7te", "Leib"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.13": {"text": "Und ein Tropfe Vergessen", "tokens": ["Und", "ein", "Trop\u00b7fe", "Ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.14": {"text": "Auf die lechzende Seele taute.", "tokens": ["Auf", "die", "lech\u00b7zen\u00b7de", "See\u00b7le", "tau\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Den mi\u00dfg\u00f6nnet ihr mir,", "tokens": ["Den", "mi\u00df\u00b7g\u00f6n\u00b7net", "ihr", "mir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PPER", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Schadenfrohe V\u00f6gel!", "tokens": ["Scha\u00b7den\u00b7fro\u00b7he", "V\u00f6\u00b7gel", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Ach, vorzeiten", "tokens": ["Ach", ",", "vor\u00b7zei\u00b7ten"], "token_info": ["word", "punct", "word"], "pos": ["ITJ", "$,", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Meintet ihr's gut,", "tokens": ["Mein\u00b7tet", "ih\u00b7r's", "gut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wenn ihr den schlummerberauschten", "tokens": ["Wenn", "ihr", "den", "schlum\u00b7mer\u00b7be\u00b7rauschten"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Knaben und Mann", "tokens": ["Kna\u00b7ben", "und", "Mann"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Hinaus in die lodernde", "tokens": ["Hin\u00b7aus", "in", "die", "lo\u00b7dern\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.6": {"text": "Pracht des Morgens riefet.", "tokens": ["Pracht", "des", "Mor\u00b7gens", "rie\u00b7fet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADV", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Da war Welt und Leben", "tokens": ["Da", "war", "Welt", "und", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "NN", "KON", "NN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Des Wachens wert.", "tokens": ["Des", "Wa\u00b7chens", "wert", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Jetzt ist der dichteste Schleier,", "tokens": ["Jetzt", "ist", "der", "dich\u00b7tes\u00b7te", "Schlei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Den Tr\u00e4ume weben,", "tokens": ["Den", "Tr\u00e4u\u00b7me", "we\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Nur wie ein Spinnweb,", "tokens": ["Nur", "wie", "ein", "Spinn\u00b7web", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Gelegt auf frische Wunde:", "tokens": ["Ge\u00b7legt", "auf", "fri\u00b7sche", "Wun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nur leicht das Blut", "tokens": ["Nur", "leicht", "das", "Blut"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Zu hemmen vermag's;", "tokens": ["Zu", "hem\u00b7men", "ver\u00b7mag's", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Doch voll durchtr\u00e4nkt", "tokens": ["Doch", "voll", "durch\u00b7tr\u00e4nkt"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VVPP"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Mit dem quellenden Na\u00df,", "tokens": ["Mit", "dem", "quel\u00b7len\u00b7den", "Na\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Wird das Gespinst", "tokens": ["Wird", "das", "Ge\u00b7spinst"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Wieder hinweggesp\u00fclt,", "tokens": ["Wie\u00b7der", "hin\u00b7weg\u00b7ge\u00b7sp\u00fclt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Und hei\u00dfer rieselt die Welle", "tokens": ["Und", "hei\u00b7\u00dfer", "rie\u00b7selt", "die", "Wel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "VVFIN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Am grauen Morgen.", "tokens": ["Am", "grau\u00b7en", "Mor\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Da\u00df ein Morgen k\u00e4me,", "tokens": ["Da\u00df", "ein", "Mor\u00b7gen",