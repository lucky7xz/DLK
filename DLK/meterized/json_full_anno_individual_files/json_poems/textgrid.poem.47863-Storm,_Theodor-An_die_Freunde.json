{"textgrid.poem.47863": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "An die Freunde", "genre": "verse", "period": "N.A.", "pub_year": 1852, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wieder einmal ausgeflogen,", "tokens": ["Wie\u00b7der", "ein\u00b7mal", "aus\u00b7ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wieder einmal heimgekehrt;", "tokens": ["Wie\u00b7der", "ein\u00b7mal", "heim\u00b7ge\u00b7kehrt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fand ich doch die alten Freunde", "tokens": ["Fand", "ich", "doch", "die", "al\u00b7ten", "Freun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Herzen unversehrt.", "tokens": ["Und", "die", "Her\u00b7zen", "un\u00b7ver\u00b7sehrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wird uns wieder wohl vereinen", "tokens": ["Wird", "uns", "wie\u00b7der", "wohl", "ver\u00b7ei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Frischer Ost und frischer West?", "tokens": ["Fri\u00b7scher", "Ost", "und", "fri\u00b7scher", "West", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Au