{"dta.poem.12770": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "B egr\u00e4bni\u00df- G edichte.  \n Trauer-ode  \n  Bey gehaltener leichen-rede  \n Ulric\u00e4 Eleonor\u00e4, K\u00f6nigin in  \n Schweden rc.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr Gothen-kinder! weinet blut,", "tokens": ["Ihr", "Go\u00b7then\u00b7kin\u00b7der", "!", "wei\u00b7net", "blut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Last eurer mutter tod euch recht zu hertzen gehen,", "tokens": ["Last", "eu\u00b7rer", "mut\u00b7ter", "tod", "euch", "recht", "zu", "hert\u00b7zen", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "NN", "PPER", "ADJD", "PTKZU", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die trauer-fackel schl\u00e4gt um cronen eine glut,", "tokens": ["Die", "trau\u00b7e\u00b7rfa\u00b7ckel", "schl\u00e4gt", "um", "cro\u00b7nen", "ei\u00b7ne", "glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df scepter, helm und thron in voller flamme stehen:", "tokens": ["Da\u00df", "scep\u00b7ter", ",", "helm", "und", "thron", "in", "vol\u00b7ler", "flam\u00b7me", "ste\u00b7hen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "$,", "NN", "KON", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es wird der Schwedsche purpur bleich:", "tokens": ["Es", "wird", "der", "Schwed\u00b7sche", "pur\u00b7pur", "bleich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es zittert und erstaunt das welt-geprie\u00dfne Norden:", "tokens": ["Es", "zit\u00b7tert", "und", "er\u00b7staunt", "das", "welt\u00b7ge\u00b7prie\u00df\u00b7ne", "Nor\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sein licht ist schwartzen n\u00e4chten gleich,", "tokens": ["Sein", "licht", "ist", "schwart\u00b7zen", "n\u00e4ch\u00b7ten", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVINF", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Weil ihm die Sonne selbst so bald verfinstert worden.", "tokens": ["Weil", "ihm", "die", "Son\u00b7ne", "selbst", "so", "bald", "ver\u00b7fins\u00b7tert", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADV", "ADV", "VVPP", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Kaum war der Pindus hier bem\u00fcht,", "tokens": ["Kaum", "war", "der", "Pin\u00b7dus", "hier", "be\u00b7m\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das frohe jubel-jahr in Ost und West zu senden;", "tokens": ["Das", "fro\u00b7he", "ju\u00b7bel\u00b7jahr", "in", "Ost", "und", "West", "zu", "sen\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nun giebt der harffen-klang ein traurig klage-lied,", "tokens": ["Nun", "giebt", "der", "ha\u00b7rf\u00b7fen\u00b7klang", "ein", "trau\u00b7rig", "kla\u00b7ge\u00b7lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJD", "VVFIN", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Es stirbt die K\u00f6nigin der Schweden, Gothen, Wenden,", "tokens": ["Es", "stirbt", "die", "K\u00f6\u00b7ni\u00b7gin", "der", "Schwe\u00b7den", ",", "Go\u00b7then", ",", "Wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NE", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wofern ihr nur nicht tieger seyd,", "tokens": ["Wo\u00b7fern", "ihr", "nur", "nicht", "tie\u00b7ger", "seyd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "M\u00fcst ihr bey solchem fall dem L\u00f6wen helffen klagen;", "tokens": ["M\u00fcst", "ihr", "bey", "sol\u00b7chem", "fall", "dem", "L\u00f6\u00b7wen", "helf\u00b7fen", "kla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "NN", "ART", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die reiser trifft zugleich das leyd,", "tokens": ["Die", "rei\u00b7ser", "trifft", "zu\u00b7gleich", "das", "leyd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn blitz und donner-keil nach hohen cedern schlagen.", "tokens": ["Wenn", "blitz", "und", "don\u00b7ner\u00b7keil", "nach", "ho\u00b7hen", "ce\u00b7dern", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "KON", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Es f\u00e4llt ins grab die marmel-brust,", "tokens": ["Es", "f\u00e4llt", "ins", "grab", "die", "mar\u00b7mel\u00b7brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So reiner gottesfurcht ist ein altar gewesen,", "tokens": ["So", "rei\u00b7ner", "got\u00b7tes\u00b7furcht", "ist", "ein", "al\u00b7tar", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VAFIN", "ART", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jhr andacht-volles hertz hat sie mit tausend lust", "tokens": ["Ihr", "an\u00b7dacht\u00b7vol\u00b7les", "hertz", "hat", "sie", "mit", "tau\u00b7send", "lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PPER", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem himmel noch allhier zum opffer auserlesen:", "tokens": ["Dem", "him\u00b7mel", "noch", "all\u00b7hier", "zum", "opf\u00b7fer", "au\u00b7ser\u00b7le\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jhr beten war gleich einer glut,", "tokens": ["Ihr", "be\u00b7ten", "war", "gleich", "ei\u00b7ner", "glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und kont\u2019 aus heissem trieb bis an die wolcken steigen;", "tokens": ["Und", "kont'", "aus", "heis\u00b7sem", "trieb", "bis", "an", "die", "wol\u00b7cken", "stei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "ADJA", "VVFIN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Drum wird nach vieler jahre flut", "tokens": ["Drum", "wird", "nach", "vie\u00b7ler", "jah\u00b7re", "flut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "APPR", "PIAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Auch noch die fromme welt vor ihr die h\u00e4upter neigen.", "tokens": ["Auch", "noch", "die", "from\u00b7me", "welt", "vor", "ihr", "die", "h\u00e4up\u00b7ter", "nei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "APPR", "PPER", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Hat iemals klugheit und verstand", "tokens": ["Hat", "ie\u00b7mals", "klug\u00b7heit", "und", "ver\u00b7stand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als weise seide sich um cronen-gold gewunden,", "tokens": ["Als", "wei\u00b7se", "sei\u00b7de", "sich", "um", "cro\u00b7nen\u00b7gold", "ge\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVFIN", "PRF", "APPR", "NE", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So wurde dieser schmuck, der seelen perlen-band,", "tokens": ["So", "wur\u00b7de", "die\u00b7ser", "schmuck", ",", "der", "see\u00b7len", "per\u00b7len\u00b7band", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PDAT", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bey dieser K\u00f6nigin in sch\u00f6nster pracht gefunden,", "tokens": ["Bey", "die\u00b7ser", "K\u00f6\u00b7ni\u00b7gin", "in", "sch\u00f6ns\u00b7ter", "pracht", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und weil ihr geist recht himmlisch war,", "tokens": ["Und", "weil", "ihr", "geist", "recht", "himm\u00b7lisch", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Lie\u00df sie, nach sternen art, bedr\u00e4ngten heyl geniessen,", "tokens": ["Lie\u00df", "sie", ",", "nach", "ster\u00b7nen", "art", ",", "be\u00b7dr\u00e4ng\u00b7ten", "heyl", "ge\u00b7nies\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Denn k\u00f6n\u2019ge stellen berge dar,", "tokens": ["Denn", "k\u00f6n'\u00b7ge", "stel\u00b7len", "ber\u00b7ge", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "VVINF", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wovon genad und h\u00fclff als reiche str\u00f6me fliessen.", "tokens": ["Wo\u00b7von", "ge\u00b7nad", "und", "h\u00fclff", "als", "rei\u00b7che", "str\u00f6\u00b7me", "flies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "KOKOM", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Jtzt, leyder! bricht ihr thron entzwey,", "tokens": ["Jtzt", ",", "ley\u00b7der", "!", "bricht", "ihr", "thron", "ent\u00b7zwey", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "$.", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ach da\u00df den scepter auch, wurm, zeit und moder fressen;", "tokens": ["Ach", "da\u00df", "den", "scep\u00b7ter", "auch", ",", "wurm", ",", "zeit", "und", "mo\u00b7der", "fres\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "ART", "ADJA", "ADV", "$,", "VAFIN", "$,", "NN", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Doch, Schweden! fasse dich, und dencke di\u00df dabey:", "tokens": ["Doch", ",", "Schwe\u00b7den", "!", "fas\u00b7se", "dich", ",", "und", "den\u00b7cke", "di\u00df", "da\u00b7bey", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NE", "$.", "VVFIN", "PPER", "$,", "KON", "VVFIN", "PDS", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df deine K\u00f6nigin noch nie so hoch gesessen,", "tokens": ["Da\u00df", "dei\u00b7ne", "K\u00f6\u00b7ni\u00b7gin", "noch", "nie", "so", "hoch", "ge\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Weil sie vier theure printzen schon", "tokens": ["Weil", "sie", "vier", "theu\u00b7re", "print\u00b7zen", "schon"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "CARD", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vor sich vorangeschickt aus diesem welt-get\u00fcmmel;", "tokens": ["Vor", "sich", "vor\u00b7an\u00b7ge\u00b7schickt", "aus", "die\u00b7sem", "welt\u00b7ge\u00b7t\u00fcm\u00b7mel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "VVPP", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sind auch vier cronen nun ihr lohn;", "tokens": ["Sind", "auch", "vier", "cro\u00b7nen", "nun", "ihr", "lohn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "CARD", "NN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Denn dreye trug sie hier, die vierdte giebt der himmel.", "tokens": ["Denn", "drey\u00b7e", "trug", "sie", "hier", ",", "die", "vierd\u00b7te", "giebt", "der", "him\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "VVFIN", "PPER", "ADV", "$,", "PRELS", "PIS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Du aber, Nordens gro\u00dfes Licht!", "tokens": ["Du", "a\u00b7ber", ",", "Nor\u00b7dens", "gro\u00b7\u00dfes", "Licht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du Schwedscher Gideon! regiere noch viel jahre,", "tokens": ["Du", "Schwed\u00b7scher", "Gi\u00b7de\u00b7on", "!", "re\u00b7gie\u00b7re", "noch", "viel", "jah\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "NE", "$.", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df, wenn dein Jsrael vor land und glauben ficht,", "tokens": ["Da\u00df", ",", "wenn", "dein", "Js\u00b7rael", "vor", "land", "und", "glau\u00b7ben", "ficht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPOSAT", "NE", "APPR", "NN", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Sich \u00f6l und palmen-zweig mit ihren waffen paare;", "tokens": ["Sich", "\u00f6l", "und", "pal\u00b7men\u00b7zweig", "mit", "ih\u00b7ren", "waf\u00b7fen", "paa\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "KON", "NE", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wann dann der tap