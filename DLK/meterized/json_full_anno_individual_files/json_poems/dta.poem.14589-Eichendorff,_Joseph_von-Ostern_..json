{"dta.poem.14589": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Ostern .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Vom M\u00fcnster Trauer-Glocken klingen,", "tokens": ["Vom", "M\u00fcns\u00b7ter", "Trau\u00b7e\u00b7rGlo\u00b7cken", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Thal ein Jauchzen schallt herauf.", "tokens": ["Vom", "Thal", "ein", "Jauch\u00b7zen", "schallt", "her\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zur Ruh sie dort dem Todten singen,", "tokens": ["Zur", "Ruh", "sie", "dort", "dem", "Tod\u00b7ten", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Lerchen jubeln: wache auf!", "tokens": ["Die", "Ler\u00b7chen", "ju\u00b7beln", ":", "wa\u00b7che", "auf", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$.", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit Erde sie ihn still bedecken,", "tokens": ["Mit", "Er\u00b7de", "sie", "ihn", "still", "be\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das Gr\u00fcn aus allen Gr\u00e4bern bricht,", "tokens": ["Das", "Gr\u00fcn", "aus", "al\u00b7len", "Gr\u00e4\u00b7bern", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Str\u00f6me hell durch's Land sich strecken,", "tokens": ["Die", "Str\u00f6\u00b7me", "hell", "durch's", "Land", "sich", "stre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPRART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Wald ernst wie in Tr\u00e4umen spricht,", "tokens": ["Der", "Wald", "ernst", "wie", "in", "Tr\u00e4u\u00b7men", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KOKOM", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und bei den Kl\u00e4ngen, Jauchzen, Trauern,", "tokens": ["Und", "bei", "den", "Kl\u00e4n\u00b7gen", ",", "Jauch\u00b7zen", ",", "Trau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "So weit in's Land man schauen mag,", "tokens": ["So", "weit", "in's", "Land", "man", "schau\u00b7en", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Es ist ein tiefes Fr\u00fchlingsschauern", "tokens": ["Es", "ist", "ein", "tie\u00b7fes", "Fr\u00fch\u00b7lings\u00b7schau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Als wie ein Auferstehungstag.", "tokens": ["Als", "wie", "ein", "Auf\u00b7er\u00b7ste\u00b7hungs\u00b7tag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}