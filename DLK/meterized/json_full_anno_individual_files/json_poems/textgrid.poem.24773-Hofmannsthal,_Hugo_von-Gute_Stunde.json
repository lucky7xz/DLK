{"textgrid.poem.24773": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "Gute Stunde", "genre": "verse", "period": "N.A.", "pub_year": 1901, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier lieg ich, mich d\u00fcnkt es der Gipfel der Welt,", "tokens": ["Hier", "lieg", "ich", ",", "mich", "d\u00fcnkt", "es", "der", "Gip\u00b7fel", "der", "Welt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Hier hab ich kein Haus, und hier hab ich kein Zelt!", "tokens": ["Hier", "hab", "ich", "kein", "Haus", ",", "und", "hier", "hab", "ich", "kein", "Zelt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "$,", "KON", "ADV", "VAFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Wege der Menschen sind um mich her,", "tokens": ["Die", "We\u00b7ge", "der", "Men\u00b7schen", "sind", "um", "mich", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Hinauf zu den Bergen und nieder zum Meer:", "tokens": ["Hin\u00b7auf", "zu", "den", "Ber\u00b7gen", "und", "nie\u00b7der", "zum", "Meer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "KON", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Sie tragen die Ware, die ihnen gef\u00e4llt,", "tokens": ["Sie", "tra\u00b7gen", "die", "Wa\u00b7re", ",", "die", "ih\u00b7nen", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Unwissend, da\u00df jede mein Leben enth\u00e4lt.", "tokens": ["Un\u00b7wis\u00b7send", ",", "da\u00df", "je\u00b7de", "mein", "Le\u00b7ben", "ent\u00b7h\u00e4lt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "PIAT", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Sie bringen in Schwingen aus Binsen und Gras", "tokens": ["Sie", "brin\u00b7gen", "in", "Schwin\u00b7gen", "aus", "Bin\u00b7sen", "und", "Gras"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die Fr\u00fcchte, von denen ich lange nicht a\u00df:", "tokens": ["Die", "Fr\u00fcch\u00b7te", ",", "von", "de\u00b7nen", "ich", "lan\u00b7ge", "nicht", "a\u00df", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "Die Feige erkenn ich, nun sp\u00fcr ich den Ort,", "tokens": ["Die", "Fei\u00b7ge", "er\u00b7kenn", "ich", ",", "nun", "sp\u00fcr", "ich", "den", "Ort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Doch lebte der lange Vergessene fort!", "tokens": ["Doch", "leb\u00b7te", "der", "lan\u00b7ge", "Ver\u00b7ges\u00b7se\u00b7ne", "fort", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.6": {"line.1": {"text": "Und war mir das Leben, das sch\u00f6ne, entwandt,", "tokens": ["Und", "war", "mir", "das", "Le\u00b7ben", ",", "das", "sch\u00f6\u00b7ne", ",", "ent\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ART", "NN", "$,", "ART", "ADJA", "$,", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Es hielt sich im Meer, und es hielt sich im Land!", "tokens": ["Es", "hielt", "sich", "im", "Meer", ",", "und", "es", "hielt", "sich", "im", "Land", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN", "$,", "KON", "PPER", "VVFIN", "PRF", "APPRART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.7": {"line.1": {"text": "Hier lieg ich, mich d\u00fcnkt es der Gipfel der Welt,", "tokens": ["Hier", "lieg", "ich", ",", "mich", "d\u00fcnkt", "es", "der", "Gip\u00b7fel", "der", "Welt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Hier hab ich kein Haus, und hier hab ich kein Zelt!", "tokens": ["Hier", "hab", "ich", "kein", "Haus", ",", "und", "hier", "hab", "ich", "kein", "Zelt", "!"], "token_info": ["word", "word", "