{"textgrid.poem.59451": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Waldgespr\u00e4ch", "genre": "verse", "period": "N.A.", "pub_year": 1812, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbes ist schon sp\u00e4t, es wird schon kalt,", "tokens": ["\u00bb", "es", "ist", "schon", "sp\u00e4t", ",", "es", "wird", "schon", "kalt", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "ADJD", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was reitst du einsam durch den Wald?", "tokens": ["Was", "reitst", "du", "ein\u00b7sam", "durch", "den", "Wald", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Wald ist lang, du bist allein,", "tokens": ["Der", "Wald", "ist", "lang", ",", "du", "bist", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "PPER", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du sch\u00f6ne Braut! Ich f\u00fchr dich heim!\u00ab", "tokens": ["Du", "sch\u00f6\u00b7ne", "Braut", "!", "Ich", "f\u00fchr", "dich", "heim", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "PPER", "VVFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbgro\u00df ist der M\u00e4nner Trug und List,", "tokens": ["\u00bb", "gro\u00df", "ist", "der", "M\u00e4n\u00b7ner", "Trug", "und", "List", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VAFIN", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vor Schmerz mein Herz gebrochen ist,", "tokens": ["Vor", "Schmerz", "mein", "Herz", "ge\u00b7bro\u00b7chen", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wohl irrt das Waldhorn her und hin,", "tokens": ["Wohl", "irrt", "das", "Wald\u00b7horn", "her", "und", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O flieh! Du wei\u00dft nicht, wer ich bin.\u00ab", "tokens": ["O", "flieh", "!", "Du", "wei\u00dft", "nicht", ",", "wer", "ich", "bin", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "$.", "PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VAFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So reich geschm\u00fcckt ist Ro\u00df und Weib,", "tokens": ["So", "reich", "ge\u00b7schm\u00fcckt", "ist", "Ro\u00df", "und", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So wundersch\u00f6n der junge Leib,", "tokens": ["So", "wun\u00b7der\u00b7sch\u00f6n", "der", "jun\u00b7ge", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbjetzt kenn ich dich \u2013 Gott steh mir bei!", "tokens": ["\u00bb", "jetzt", "kenn", "ich", "dich", "\u2013", "Gott", "steh", "mir", "bei", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "PRF", "$(", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du bist die Hexe Lorelei.\u00ab", "tokens": ["Du", "bist", "die", "He\u00b7xe", "Lo\u00b7re\u00b7lei", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbdu kennst mich wohl \u2013 von hohem Stein", "tokens": ["\u00bb", "du", "kennst", "mich", "wohl", "\u2013", "von", "ho\u00b7hem", "Stein"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ADV", "$(", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schaut still mein Schlo\u00df tief in den Rhein.", "tokens": ["Schaut", "still", "mein", "Schlo\u00df", "tief", "in", "den", "Rhein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "ADJD", "APPR", "ART", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es ist schon sp\u00e4t, es wird schon kalt,", "tokens": ["Es", "ist", "schon", "sp\u00e4t", ",", "es", "wird", "schon", "kalt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kommst nimmermehr aus diesem Wald!\u00ab", "tokens": ["Kommst", "nim\u00b7mer\u00b7mehr", "aus", "