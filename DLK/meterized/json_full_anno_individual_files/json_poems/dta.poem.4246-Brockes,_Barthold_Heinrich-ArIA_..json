{"dta.poem.4246": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "ArIA .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sollten wohl vern\u00fcnft\u2019ge Seelen", "tokens": ["Soll\u00b7ten", "wohl", "ver\u00b7n\u00fcnft'\u00b7ge", "See\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das Get\u00fcmmel dieser Welt,", "tokens": ["Das", "Ge\u00b7t\u00fcm\u00b7mel", "die\u00b7ser", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das so bald vergeht, zerf\u00e4llt,", "tokens": ["Das", "so", "bald", "ver\u00b7geht", ",", "zer\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jhr Get\u00f6se, Lerm, Gebr\u00fclle,", "tokens": ["Ihr", "Ge\u00b7t\u00f6\u00b7se", ",", "Lerm", ",", "Ge\u00b7br\u00fcl\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "F\u00fcr die seel\u2019ge Ruh und Stille,", "tokens": ["F\u00fcr", "die", "seel'\u00b7ge", "Ruh", "und", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die dort ewig w\u00e4hret, w\u00e4hlen?", "tokens": ["Die", "dort", "e\u00b7wig", "w\u00e4h\u00b7ret", ",", "w\u00e4h\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VVFIN", "$,", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Macht euch mit dem stillen Frieden,", "tokens": ["Macht", "euch", "mit", "dem", "stil\u00b7len", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den ihr in den W\u00e4ldern sp\u00fchrt,", "tokens": ["Den", "ihr", "in", "den", "W\u00e4l\u00b7dern", "sp\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der uns \u00f6fters innig r\u00fchrt,", "tokens": ["Der", "uns", "\u00f6f\u00b7ters", "in\u00b7nig", "r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mehr bekannt, um euch beyzeiten", "tokens": ["Mehr", "be\u00b7kannt", ",", "um", "euch", "bey\u00b7zei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "KOUI", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Zu der Stille zu bereiten,", "tokens": ["Zu", "der", "Stil\u00b7le", "zu", "be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die uns allen dort beschieden.", "tokens": ["Die", "uns", "al\u00b7len", "dort", "be\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIAT", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}