{"dta.poem.17090": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "191.  GOtt liebt man nie zuviel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Wer GOtt recht lieben wil/ der thu\u2019 s ohn ma\u00df un\u0304 Ziehl/", "tokens": ["Wer", "Gott", "recht", "lie\u00b7ben", "wil", "/", "der", "thu'", "s", "ohn", "ma\u00df", "un\u0304", "Ziehl", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "ADJD", "VVINF", "VMFIN", "$(", "ART", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Er ist so s\u00fc\u00df\u2019 und gutt/ man liebt jhn nie zu viel.", "tokens": ["Er", "ist", "so", "s\u00fc\u00df'", "und", "gutt", "/", "man", "liebt", "jhn", "nie", "zu", "viel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$(", "PIS", "VVFIN", "PPER", "ADV", "PTKA", "PIS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}