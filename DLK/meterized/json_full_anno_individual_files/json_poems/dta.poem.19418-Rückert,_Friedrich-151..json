{"dta.poem.19418": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "151.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Willst du dem Irrenden klar seinen Irrthum machen,", "tokens": ["Willst", "du", "dem", "Ir\u00b7ren\u00b7den", "klar", "sei\u00b7nen", "Irr\u00b7thum", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ADJD", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+++-+-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "So sieh, von welcher Seit' er angesehn die Sachen.", "tokens": ["So", "sieh", ",", "von", "wel\u00b7cher", "Seit'", "er", "an\u00b7ge\u00b7sehn", "die", "Sa\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "APPR", "PWAT", "NN", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "R\u00e4um' ein, die Sache sei von dieser Seite wahr,", "tokens": ["R\u00e4um'", "ein", ",", "die", "Sa\u00b7che", "sei", "von", "die\u00b7ser", "Sei\u00b7te", "wahr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "ART", "NN", "VAFIN", "APPR", "PDAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mach' ihm nebenbei die andern Seiten klar.", "tokens": ["Und", "mach'", "ihm", "ne\u00b7ben\u00b7bei", "die", "an\u00b7dern", "Sei\u00b7ten", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+