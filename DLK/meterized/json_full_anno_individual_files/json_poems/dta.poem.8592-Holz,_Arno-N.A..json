{"dta.poem.8592": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1899", "urn": "urn:nbn:de:kobv:b4-200905192690", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zw\u00f6lf!", "tokens": ["Zw\u00f6lf", "!"], "token_info": ["word", "punct"], "pos": ["CARD", "$."], "meter": "+", "measure": "single.up"}}, "stanza.2": {"line.1": {"text": "Durch die Gardinen in den Weihnachtsbaum", "tokens": ["Durch", "die", "Gar\u00b7di\u00b7nen", "in", "den", "Weih\u00b7nachts\u00b7baum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "scheint der Mond.", "tokens": ["scheint", "der", "Mond", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Alle Engelchen glitzern.", "tokens": ["Al\u00b7le", "En\u00b7gel\u00b7chen", "glit\u00b7zern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Im weissen Kleidchen schluchzt die Braut,", "tokens": ["Im", "weis\u00b7sen", "Kleid\u00b7chen", "schluchzt", "die", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "wir halten Hochzeit.", "tokens": ["wir", "hal\u00b7ten", "Hoch\u00b7zeit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Katerlieschen ist unsre Grossmama,", "tokens": ["Ka\u00b7ter\u00b7lie\u00b7schen", "ist", "uns\u00b7re", "Gross\u00b7ma\u00b7ma", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "unser Grosspapa heisst Rumpelstilzchen.", "tokens": ["un\u00b7ser", "Gross\u00b7pa\u00b7pa", "heisst", "Rum\u00b7pel\u00b7stilz\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NE", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Eine uralte Familie!", "tokens": ["Ei\u00b7ne", "ur\u00b7al\u00b7te", "Fa\u00b7mi\u00b7lie", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.7": {"line.1": {"text": "Die ganze Sippschaft,", "tokens": ["Die", "gan\u00b7ze", "Sipp\u00b7schaft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "alle Marzipanschweinchen sind geladen.", "tokens": ["al\u00b7le", "Mar\u00b7zi\u00b7pan\u00b7schwein\u00b7chen", "sind", "ge\u00b7la\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "Leise,", "tokens": ["Lei\u00b7se", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "knisternd,", "tokens": ["knis\u00b7ternd", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "seinen Segen,", "tokens": ["sei\u00b7nen", "Se\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "singt der Weihnachtsstern.", "tokens": ["singt", "der", "Weih\u00b7nachts\u00b7stern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}