{"dta.poem.21792": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "Vi.  \n  Der verbrannte Amor.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.71", "nl:0.28"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Solt' Amor wol gefl\u00fcgelt sein?", "tokens": ["Solt'", "A\u00b7mor", "wol", "ge\u00b7fl\u00fc\u00b7gelt", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "ich bild es mir nicht ein.", "tokens": ["ich", "bild", "es", "mir", "nicht", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "L\u00e4ngst ist er bey mir eingezogen", "tokens": ["L\u00e4ngst", "ist", "er", "bey", "mir", "ein\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "nie ist er wieder fort geflogen/", "tokens": ["nie", "ist", "er", "wie\u00b7der", "fort", "ge\u00b7flo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PTKVZ", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "ich bild\u2019 es mir nicht ein.", "tokens": ["ich", "bild'", "es", "mir", "nicht", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es macht uns zwar Apelles Hand", "tokens": ["Es", "macht", "uns", "zwar", "A\u00b7pel\u00b7les", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "den Amor so bekand.", "tokens": ["den", "A\u00b7mor", "so", "be\u00b7kand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "H\u00e4tt\u2019 aber er ie k\u00f6nnen fliegen", "tokens": ["H\u00e4tt'", "a\u00b7ber", "er", "ie", "k\u00f6n\u00b7nen", "flie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPER", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "er w\u00fcrde so nicht stille liegen.", "tokens": ["er", "w\u00fcr\u00b7de", "so", "nicht", "stil\u00b7le", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ja wol gefl\u00fcgelt sein", "tokens": ["Ja", "wol", "ge\u00b7fl\u00fc\u00b7gelt", "sein"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKANT", "ADV", "VVPP", "VAINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "ich bild\u2019 es mir nicht ein.", "tokens": ["ich", "bild'", "es", "mir", "nicht", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch/ st! aniezt besinn\u2019 ich mich:", "tokens": ["Doch", "/", "st", "!", "an\u00b7iezt", "be\u00b7sinn'", "ich", "mich", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "er hatte was an sich", "tokens": ["er", "hat\u00b7te", "was", "an", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIS", "APPR", "PRF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als er zu mir kahm eingefahren/", "tokens": ["Als", "er", "zu", "mir", "kahm", "ein\u00b7ge\u00b7fah\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ADJD", "VVINF", "$("], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "mich d\u00fcnket/ da\u00df es Federn waren.", "tokens": ["mich", "d\u00fcn\u00b7ket", "/", "da\u00df", "es", "Fe\u00b7dern", "wa\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPER", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Er mu\u00df wol halten Stand", "tokens": ["Er", "mu\u00df", "wol", "hal\u00b7ten", "Stand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "die Federn sind verbrannt.", "tokens": ["die", "Fe\u00b7dern", "sind", "ver\u00b7brannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er aber hat selbst Schuld daran", "tokens": ["Er", "a\u00b7ber", "hat", "selbst", "Schuld", "da\u00b7ran"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "ADV", "NN", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "da\u00df er nicht weiter kan.", "tokens": ["da\u00df", "er", "nicht", "wei\u00b7ter", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er hat ein Feur in meinem Herzen", "tokens": ["Er", "hat", "ein", "Feur", "in", "mei\u00b7nem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "entz\u00fcndet mit der Liebes-Kerzen.", "tokens": ["ent\u00b7z\u00fcn\u00b7det", "mit", "der", "Lie\u00b7bes\u00b7Ker\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "ich g\u00f6nnt\u2019 es/ Amor/ dir/", "tokens": ["ich", "g\u00f6nnt'", "es", "/", "A\u00b7mor", "/", "dir", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$(", "NE", "$(", "PPER", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "w\u00e4rstu nur erst au\u00df mir.", "tokens": ["w\u00e4rs\u00b7tu", "nur", "erst", "au\u00df", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}