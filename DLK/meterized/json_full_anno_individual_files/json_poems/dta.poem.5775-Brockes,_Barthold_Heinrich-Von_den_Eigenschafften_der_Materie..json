{"dta.poem.5775": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Eigenschafften der  \n Materie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So viel und grosse Werck aus der Materie,", "tokens": ["So", "viel", "und", "gros\u00b7se", "Werck", "aus", "der", "Ma\u00b7te\u00b7rie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die unerm\u00e4\u00dflich ist, hervorgebracht,", "tokens": ["Die", "un\u00b7er\u00b7m\u00e4\u00df\u00b7lich", "ist", ",", "her\u00b7vor\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sind all\u2019 aus C\u00f6rperchen, so nicht zu sehn, gemacht:", "tokens": ["Sind", "all'", "aus", "C\u00f6r\u00b7per\u00b7chen", ",", "so", "nicht", "zu", "sehn", ",", "ge\u00b7macht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PIS", "APPR", "NE", "$,", "ADV", "PTKNEG", "PTKZU", "VVINF", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und die aufs neu, wenn sie sich mischen und verfliegen,", "tokens": ["Und", "die", "aufs", "neu", ",", "wenn", "sie", "sich", "mi\u00b7schen", "und", "ver\u00b7flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPRART", "ADJD", "$,", "KOUS", "PPER", "PRF", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sich abermahl zum ersten Klumpen f\u00fcgen.", "tokens": ["Sich", "a\u00b7ber\u00b7mahl", "zum", "ers\u00b7ten", "Klum\u00b7pen", "f\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Von allen unterschiednen Dingen", "tokens": ["Von", "al\u00b7len", "un\u00b7ter\u00b7schied\u00b7nen", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "F\u00e4llt jedes unvermerckt das, drauf es w\u00fcrcket, an,", "tokens": ["F\u00e4llt", "je\u00b7des", "un\u00b7ver\u00b7merckt", "das", ",", "drauf", "es", "w\u00fcr\u00b7cket", ",", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJD", "PDS", "$,", "PAV", "PPER", "VVFIN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und suchet ihm den Untergang zu bringen.", "tokens": ["Und", "su\u00b7chet", "ihm", "den", "Un\u00b7ter\u00b7gang", "zu", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ein jedes z\u00f6gert nicht, und nimmt, wenn sie sich reiben,", "tokens": ["Ein", "je\u00b7des", "z\u00f6\u00b7gert", "nicht", ",", "und", "nimmt", ",", "wenn", "sie", "sich", "rei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "VVFIN", "PTKNEG", "$,", "KON", "VVFIN", "$,", "KOUS", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So zarte Theilchen weg, die man nicht sehen kan.", "tokens": ["So", "zar\u00b7te", "Theil\u00b7chen", "weg", ",", "die", "man", "nicht", "se\u00b7hen", "kan", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PTKVZ", "$,", "PRELS", "PIS", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die Lufft, so leicht sie ist, durchgr\u00e4bt den h\u00e4rtsten Stein:", "tokens": ["Die", "Lufft", ",", "so", "leicht", "sie", "ist", ",", "durch\u00b7gr\u00e4bt", "den", "h\u00e4rts\u00b7ten", "Stein", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADJD", "PPER", "VAFIN", "$,", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die feuchte Nacht, des frohen Tages Schein", "tokens": ["Die", "feuch\u00b7te", "Nacht", ",", "des", "fro\u00b7hen", "Ta\u00b7ges", "Schein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Die haben unbekannte Spitzen,", "tokens": ["Die", "ha\u00b7ben", "un\u00b7be\u00b7kann\u00b7te", "Spit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Wodurch sie alles abzun\u00fctzen", "tokens": ["Wo\u00b7durch", "sie", "al\u00b7les", "ab\u00b7zu\u00b7n\u00fct\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PIS", "VVIZU"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Geschickt und f\u00e4hig seyn.", "tokens": ["Ge\u00b7schickt", "und", "f\u00e4\u00b7hig", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Ohn allen Knall vom Donner und von Blitzen", "tokens": ["Ohn", "al\u00b7len", "Knall", "vom", "Don\u00b7ner", "und", "von", "Blit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "APPRART", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Vermag,", "tokens": ["Ver\u00b7mag", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.18": {"text": "Jhr stet- und unsichtbarer Schlag,", "tokens": ["Ihr", "stet", "und", "un\u00b7sicht\u00b7ba\u00b7rer", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "TRUNC", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Was noch so fest gegr\u00fcndet stund,", "tokens": ["Was", "noch", "so", "fest", "ge\u00b7gr\u00fcn\u00b7det", "stund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "ADJD", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Bis auf den Grund", "tokens": ["Bis", "auf", "den", "Grund"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.21": {"text": "Herum zu st\u00fcrzen, zu verst\u00f6hren.", "tokens": ["He\u00b7rum", "zu", "st\u00fcr\u00b7zen", ",", "zu", "ver\u00b7st\u00f6h\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}