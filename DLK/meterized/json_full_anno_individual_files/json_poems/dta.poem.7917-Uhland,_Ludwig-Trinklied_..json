{"dta.poem.7917": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Trinklied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Wir sind nicht mehr am ersten Glas,", "tokens": ["Wir", "sind", "nicht", "mehr", "am", "ers\u00b7ten", "Glas", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drum denken wir gern an dies und das,", "tokens": ["Drum", "den\u00b7ken", "wir", "gern", "an", "dies", "und", "das", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "APPR", "PDS", "KON", "PDS", "$,"], "meter": "-+-++-+-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Was rauschet und was brauset.", "tokens": ["Was", "rau\u00b7schet", "und", "was", "brau\u00b7set", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KON", "PWS", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "So denken wir an den wilden Wald,", "tokens": ["So", "den\u00b7ken", "wir", "an", "den", "wil\u00b7den", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Darin die St\u00fcrme sausen,", "tokens": ["Da\u00b7rin", "die", "St\u00fcr\u00b7me", "sau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir h\u00f6ren, wie das Jagdhorn schallt,", "tokens": ["Wir", "h\u00f6\u00b7ren", ",", "wie", "das", "Jagd\u00b7horn", "schallt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Ross\u2019 und Hunde brausen,", "tokens": ["Die", "Ross'", "und", "Hun\u00b7de", "brau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und wie der Hirsch durch\u2019s Wasser setzt,", "tokens": ["Und", "wie", "der", "Hirsch", "durch's", "Was\u00b7ser", "setzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Fluten rauschen und wallen,", "tokens": ["Die", "Flu\u00b7ten", "rau\u00b7schen", "und", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und wie der J\u00e4ger ruft und hetzt,", "tokens": ["Und", "wie", "der", "J\u00e4\u00b7ger", "ruft", "und", "hetzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Sch\u00fcsse schmetternd fallen.", "tokens": ["Die", "Sch\u00fcs\u00b7se", "schmet\u00b7ternd", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wir sind nicht mehr am ersten Glas,", "tokens": ["Wir", "sind", "nicht", "mehr", "am", "ers\u00b7ten", "Glas", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drum denken wir gern an dies und das,", "tokens": ["Drum", "den\u00b7ken", "wir", "gern", "an", "dies", "und", "das", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "APPR", "PDS", "KON", "PDS", "$,"], "meter": "-+-++-+-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Was rauschet und was brauset.", "tokens": ["Was", "rau\u00b7schet", "und", "was", "brau\u00b7set", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KON", "PWS", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "So denken wir an das wilde Meer,", "tokens": ["So", "den\u00b7ken", "wir", "an", "das", "wil\u00b7de", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und h\u00f6ren die Wogen brausen,", "tokens": ["Und", "h\u00f6\u00b7ren", "die", "Wo\u00b7gen", "brau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Donner rollen dr\u00fcberher,", "tokens": ["Die", "Don\u00b7ner", "rol\u00b7len", "dr\u00fc\u00b7ber\u00b7her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Wirbelwinde sausen.", "tokens": ["Die", "Wir\u00b7bel\u00b7win\u00b7de", "sau\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ha! wie das Schifflein schwankt und dr\u00f6hnt,", "tokens": ["Ha", "!", "wie", "das", "Schif\u00b7flein", "schwankt", "und", "dr\u00f6hnt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Wie Mast und Stange splittern,", "tokens": ["Wie", "Mast", "und", "Stan\u00b7ge", "split\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und wie der Nothschu\u00df dumpf ert\u00f6nt,", "tokens": ["Und", "wie", "der", "Noth\u00b7schu\u00df", "dumpf", "er\u00b7t\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Schiffer fluchen und zittern!", "tokens": ["Die", "Schif\u00b7fer", "flu\u00b7chen", "und", "zit\u00b7tern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Wir sind nicht mehr am ersten Glas,", "tokens": ["Wir", "sind", "nicht", "mehr", "am", "ers\u00b7ten", "Glas", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drum denken wir gern an dies und das,", "tokens": ["Drum", "den\u00b7ken", "wir", "gern", "an", "dies", "und", "das", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "APPR", "PDS", "KON", "PDS", "$,"], "meter": "-+-++-+-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Was rauschet und was brauset.", "tokens": ["Was", "rau\u00b7schet", "und", "was", "brau\u00b7set", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KON", "PWS", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "So denken wir an die wilde Schlacht,", "tokens": ["So", "den\u00b7ken", "wir", "an", "die", "wil\u00b7de", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da fechten die deutschen M\u00e4nner,", "tokens": ["Da", "fech\u00b7ten", "die", "deut\u00b7schen", "M\u00e4n\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das Schwerdt erklirrt, die Lanze kracht,", "tokens": ["Das", "Schwerdt", "er\u00b7klirrt", ",", "die", "Lan\u00b7ze", "kracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es schnauben die muth\u2019gen Renner.", "tokens": ["Es", "schnau\u00b7ben", "die", "muth'\u00b7gen", "Ren\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Mit Trommelwirbel, Trommetenschall,", "tokens": ["Mit", "Trom\u00b7mel\u00b7wir\u00b7bel", ",", "Trom\u00b7me\u00b7ten\u00b7schall", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "So zieht das Heer zum Sturme;", "tokens": ["So", "zieht", "das", "Heer", "zum", "Stur\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Hin st\u00fcrzet von Kanonenknall", "tokens": ["Hin", "st\u00fcr\u00b7zet", "von", "Ka\u00b7no\u00b7nen\u00b7knall"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Mauer sammt dem Thurme.", "tokens": ["Die", "Mau\u00b7er", "sammt", "dem", "Thur\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wir sind nicht mehr am ersten Glas,", "tokens": ["Wir", "sind", "nicht", "mehr", "am", "ers\u00b7ten", "Glas", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drum denken wir gern an dies und das,", "tokens": ["Drum", "den\u00b7ken", "wir", "gern", "an", "dies", "und", "das", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "APPR", "PDS", "KON", "PDS", "$,"], "meter": "-+-++-+-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Was rauschet und was brauset.", "tokens": ["Was", "rau\u00b7schet", "und", "was", "brau\u00b7set", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KON", "PWS", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "So denken wir an den j\u00fcngsten Tag,", "tokens": ["So", "den\u00b7ken", "wir", "an", "den", "j\u00fcng\u00b7sten", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und h\u00f6ren Posaunen schallen,", "tokens": ["Und", "h\u00f6\u00b7ren", "Po\u00b7sau\u00b7nen", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Gr\u00e4ber springen von Donnerschlag,", "tokens": ["Die", "Gr\u00e4\u00b7ber", "sprin\u00b7gen", "von", "Don\u00b7ner\u00b7schlag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Sterne vom Himmel fallen.", "tokens": ["Die", "Ster\u00b7ne", "vom", "Him\u00b7mel", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Es braust die offne H\u00f6llenkluft", "tokens": ["Es", "braust", "die", "off\u00b7ne", "H\u00f6l\u00b7len\u00b7kluft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit wildem Flammenmeere,", "tokens": ["Mit", "wil\u00b7dem", "Flam\u00b7men\u00b7mee\u00b7re", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und oben in der goldnen Luft,", "tokens": ["Und", "o\u00b7ben", "in", "der", "gold\u00b7nen", "Luft", ","], "tok