{"textgrid.poem.58523": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "561. S\u00fcnde, die Ursache aller Leiden", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mensch, lern es tief zu Herzen fassen:", "tokens": ["Mensch", ",", "lern", "es", "tief", "zu", "Her\u00b7zen", "fas\u00b7sen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADJD", "APPR", "NN", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "So bitter ist es, Gott verlassen,", "tokens": ["So", "bit\u00b7ter", "ist", "es", ",", "Gott", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "$,", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bekenn es recht, gib Gott die Ehr'", "tokens": ["Be\u00b7kenn", "es", "recht", ",", "gib", "Gott", "die", "Ehr'"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "$,", "VVIMP", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sei doch nicht so t\u00f6richt mehr,", "tokens": ["Und", "sei", "doch", "nicht", "so", "t\u00f6\u00b7richt", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PTKNEG", "ADV", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "La\u00df dich die Schlange nicht betr\u00fcgen,", "tokens": ["La\u00df", "dich", "die", "Schlan\u00b7ge", "nicht", "be\u00b7tr\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "S\u00fcnd' ist ein kurzes Scheinvergn\u00fcgen!", "tokens": ["S\u00fcnd'", "ist", "ein", "kur\u00b7zes", "Schein\u00b7ver\u00b7gn\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Auf kleine Lust folgt gro\u00dfe Last,", "tokens": ["Auf", "klei\u00b7ne", "Lust", "folgt", "gro\u00b7\u00dfe", "Last", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wer S\u00fcnde tut, sich selber ha\u00dft;", "tokens": ["Wer", "S\u00fcn\u00b7de", "tut", ",", "sich", "sel\u00b7ber", "ha\u00dft", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "$,", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Herr, la\u00df mich dir im Kreuz ankleben!", "tokens": ["Herr", ",", "la\u00df", "mich", "dir", "im", "Kreuz", "an\u00b7kle\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "K\u00f6nnt' S\u00fcnde mir den Himmel geben", "tokens": ["K\u00f6nnt'", "S\u00fcn\u00b7de", "mir", "den", "Him\u00b7mel", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "NN", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und deine Liebe solche Pein,", "tokens": ["Und", "dei\u00b7ne", "Lie\u00b7be", "sol\u00b7che", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"]