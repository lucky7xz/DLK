{"dta.poem.16179": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "233.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df in denselben Flu\u00df du kannst nicht zweimal steigen,", "tokens": ["Da\u00df", "in", "den\u00b7sel\u00b7ben", "Flu\u00df", "du", "kannst", "nicht", "zwei\u00b7mal", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PDAT", "NN", "PPER", "VMFIN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil jeden Augenblick ihm andre Flut ist eigen,", "tokens": ["Weil", "je\u00b7den", "Au\u00b7gen\u00b7blick", "ihm", "and\u00b7re", "Flut", "ist", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PPER", "ADJA", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und da\u00df du selber auch, dir selber nicht getreuer,", "tokens": ["Und", "da\u00df", "du", "sel\u00b7ber", "auch", ",", "dir", "sel\u00b7ber", "nicht", "ge\u00b7treu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "$,", "PPER", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bist jeden Augenblick ein anderer und neuer;", "tokens": ["Bist", "je\u00b7den", "Au\u00b7gen\u00b7blick", "ein", "an\u00b7de\u00b7rer", "und", "neu\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ART", "ADJA", "KON", "ADJA", "$."], 