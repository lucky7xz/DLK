{"textgrid.poem.57637": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Poeta laureatus", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zeitalter gab's, wo Lorbeer", "tokens": ["Zei\u00b7tal\u00b7ter", "gab's", ",", "wo", "Lor\u00b7beer"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "$,", "PWAV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Dichter hat gekr\u00f6nt,", "tokens": ["Den", "Dich\u00b7ter", "hat", "ge\u00b7kr\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Prediger der Wahrheit,", "tokens": ["Den", "Pre\u00b7di\u00b7ger", "der", "Wahr\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der heute wird verh\u00f6hnt,", "tokens": ["Der", "heu\u00b7te", "wird", "ver\u00b7h\u00f6hnt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wer jetzt die Wahrheit singet,", "tokens": ["Wer", "jetzt", "die", "Wahr\u00b7heit", "sin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den steiniget man tot,", "tokens": ["Den", "stei\u00b7ni\u00b7get", "man", "tot", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIS", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sie bieten Gold dem armen Volke,", "tokens": ["Sie", "bie\u00b7ten", "Gold", "dem", "ar\u00b7men", "Vol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Man weigert ihnen Dach und Brot.", "tokens": ["Man", "wei\u00b7gert", "ih\u00b7nen", "Dach", "und", "Brot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wer schmeicheln nicht und kriechen", "tokens": ["Wer", "schmei\u00b7cheln", "nicht", "und", "krie\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PTKNEG", "KON", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und sich verleugnen kann,", "tokens": ["Und", "sich", "ver\u00b7leug\u00b7nen", "kann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den morden die Federhelden,", "tokens": ["Den", "mor\u00b7den", "die", "Fe\u00b7der\u00b7hel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Den liest kein Biedermann,", "tokens": ["Den", "liest", "kein", "Bie\u00b7der\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nur himmelblaue Liedlein", "tokens": ["Nur", "him\u00b7mel\u00b7blau\u00b7e", "Lied\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und Sittsamsimpelei", "tokens": ["Und", "Sitt\u00b7sam\u00b7sim\u00b7pe\u00b7lei"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ist Geistesbrot dem Volk der Denker,", "tokens": ["Ist", "Geis\u00b7tes\u00b7brot", "dem", "Volk", "der", "Den\u00b7ker", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Doch nicht der Wahrheit Hungerschrei.", "tokens": ["Doch", "nicht", "der", "Wahr\u00b7heit", "Hun\u00b7ger\u00b7schrei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich ruf' euch alle zusammen,", "tokens": ["Ich", "ruf'", "euch", "al\u00b7le", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "PTKVZ", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ihr S\u00e4nger unsrer Zeit,", "tokens": ["Ihr", "S\u00e4n\u00b7ger", "uns\u00b7rer", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zum Kampfe f\u00fcr der Wahrheit", "tokens": ["Zum", "Kamp\u00b7fe", "f\u00fcr", "der", "Wahr\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Beschmutzte Heiligkeit,", "tokens": ["Be\u00b7schmutz\u00b7te", "Hei\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mit wilden Liedern r\u00fcttelt", "tokens": ["Mit", "wil\u00b7den", "Lie\u00b7dern", "r\u00fct\u00b7telt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das taube Deutschland wach,", "tokens": ["Das", "tau\u00b7be", "Deutschland", "wach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Und handelt wie ein strenger Vater,", "tokens": ["Und", "han\u00b7delt", "wie", "ein", "stren\u00b7ger", "Va\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dem schlechten Kind frommt jeder Schlag!", "tokens": ["Dem", "schlech\u00b7ten", "Kind", "frommt", "je\u00b7der", "Schlag", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Singt ewig nicht von Liebe,", "tokens": ["Singt", "e\u00b7wig", "nicht", "von", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Stimmt an das Lied vom Ha\u00df,", "tokens": ["Stimmt", "an", "das", "Lied", "vom", "Ha\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Fort Wein und Lust \u2013 besinget", "tokens": ["Fort", "Wein", "und", "Lust", "\u2013", "be\u00b7sin\u00b7get"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NN", "NN", "KON", "NN", "$(", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des Schwei\u00dfes heilig Na\u00df,", "tokens": ["Des", "Schwei\u00b7\u00dfes", "hei\u00b7lig", "Na\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Legt blo\u00df am deutschen Stamme", "tokens": ["Legt", "blo\u00df", "am", "deut\u00b7schen", "Stam\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Was wurmfaul ist und krank,", "tokens": ["Was", "wurm\u00b7faul", "ist", "und", "krank", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wenn man euch heut auch schm\u00e4ht und l\u00e4stert,", "tokens": ["Wenn", "man", "euch", "heut", "auch", "schm\u00e4ht", "und", "l\u00e4s\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADV", "ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Schon morgen bl\u00fcht euch hei\u00dfer Dank.", "tokens": ["Schon", "mor\u00b7gen", "bl\u00fcht", "euch", "hei\u00b7\u00dfer", "Dank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Rei\u00dft der Gesellschaftsl\u00fcge", "tokens": ["Rei\u00dft", "der", "Ge\u00b7sell\u00b7schafts\u00b7l\u00fc\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Die Maske vom Gesicht,", "tokens": ["Die", "Mas\u00b7ke", "vom", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und spritzt auch Blut und Eiter,", "tokens": ["Und", "spritzt", "auch", "Blut", "und", "Ei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Furcht kennt der Dichter nicht,", "tokens": ["Furcht", "kennt", "der", "Dich\u00b7ter", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und weigert sich der Kranke,", "tokens": ["Und", "wei\u00b7gert", "sich", "der", "Kran\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zu nehmen die Arznei,", "tokens": ["Zu", "neh\u00b7men", "die", "Arz\u00b7nei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mitleidig darf der Arzt nicht sein", "tokens": ["Mit\u00b7lei\u00b7dig", "darf", "der", "Arzt", "nicht", "sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VMFIN", "ART", "NN", "PTKNEG", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und achten nicht auf jeden Schrei.", "tokens": ["Und", "ach\u00b7ten", "nicht", "auf", "je\u00b7den", "Schrei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Dr\u00fcckt selber euch die Kr\u00e4nze", "tokens": ["Dr\u00fcckt", "sel\u00b7ber", "euch", "die", "Kr\u00e4n\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von Lorbeer auf das Haupt", "tokens": ["Von", "Lor\u00b7beer", "auf", "das", "Haupt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und schmettert den zu Boden,", "tokens": ["Und", "schmet\u00b7tert", "den", "zu", "Bo\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der euch die Krone raubt,", "tokens": ["Der", "euch", "die", "Kro\u00b7ne", "raubt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Leier in der Linken", "tokens": ["Die", "Lei\u00b7er", "in", "der", "Lin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und rechts das scharfe Schwert,", "tokens": ["Und", "rechts", "das", "schar\u00b7fe", "Schwert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Gewaffnet und zum Kampf ger\u00fcstet,", "tokens": ["Ge\u00b7waff\u00b7net", "und", "zum", "Kampf", "ge\u00b7r\u00fcs\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So seid ihr erst der Neuzeit wert.", "tokens": ["So", "seid", "ihr", "erst", "der", "Neu\u00b7zeit", "wert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Werft ab den grauen Mantel", "tokens": ["Werft", "ab", "den", "grau\u00b7en", "Man\u00b7tel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der stillen Bescheidenheit,", "tokens": ["Der", "stil\u00b7len", "Be\u00b7schei\u00b7den\u00b7heit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Lautklirrend mu\u00df erschallen", "tokens": ["Laut\u00b7klir\u00b7rend", "mu\u00df", "er\u00b7schal\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Lied im hei\u00dfen Streit \u2013", "tokens": ["Das", "Lied", "im", "hei\u00b7\u00dfen", "Streit", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sonst bleibt es bei den Spenden,", "tokens": ["Sonst", "bleibt", "es", "bei", "den", "Spen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die man euch fr\u00fcher gab:", "tokens": ["Die", "man", "euch", "fr\u00fc\u00b7her", "gab", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Nur Hohn und Spott im Dichterleben,", "tokens": ["Nur", "Hohn", "und", "Spott", "im", "Dich\u00b7ter\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Kranz aufs fr\u00fche Dichtergrab!", "tokens": ["Ein", "Kranz", "aufs", "fr\u00fc\u00b7he", "Dich\u00b7ter\u00b7grab", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Zeitalter gab's, wo Lorbeer", "tokens": ["Zei\u00b7tal\u00b7ter", "gab's", ",", "wo", "Lor\u00b7beer"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "$,", "PWAV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Dichter hat gekr\u00f6nt,", "tokens": ["Den", "Dich\u00b7ter", "hat", "ge\u00b7kr\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Prediger der Wahrheit,", "tokens": ["Den", "Pre\u00b7di\u00b7ger", "der", "Wahr\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der heute wird verh\u00f6hnt,", "tokens": ["Der", "heu\u00b7te", "wird", "ver\u00b7h\u00f6hnt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wer jetzt die Wahrheit singet,", "tokens": ["Wer", "jetzt", "die", "Wahr\u00b7heit", "sin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den steiniget man tot,", "tokens": ["Den", "stei\u00b7ni\u00b7get", "man", "tot", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIS"