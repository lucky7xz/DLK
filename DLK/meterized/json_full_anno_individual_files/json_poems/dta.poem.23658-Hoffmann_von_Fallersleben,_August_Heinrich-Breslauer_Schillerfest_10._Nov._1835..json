{"dta.poem.23658": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Breslauer Schillerfest 10. Nov. 1835.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es leben die Poeten!", "tokens": ["Es", "le\u00b7ben", "die", "Po\u00b7et\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die erhabenen begrabenen", "tokens": ["Die", "er\u00b7ha\u00b7be\u00b7nen", "be\u00b7gra\u00b7be\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Und die strebenden lebenden,", "tokens": ["Und", "die", "stre\u00b7ben\u00b7den", "le\u00b7ben\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "sinnig waltenden,", "tokens": ["sin\u00b7nig", "wal\u00b7ten\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.5": {"text": "innig entfaltenden,", "tokens": ["in\u00b7nig", "ent\u00b7fal\u00b7ten\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "minnig gestaltenden,", "tokens": ["min\u00b7nig", "ge\u00b7stal\u00b7ten\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$,"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.7": {"text": "klangentz\u00fcckten entz\u00fcndenden,", "tokens": ["klan\u00b7gent\u00b7z\u00fcck\u00b7ten", "ent\u00b7z\u00fcn\u00b7den\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VVPP", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.8": {"text": "sangbegl\u00fcckten begl\u00fcckenden,", "tokens": ["sang\u00b7be\u00b7gl\u00fcck\u00b7ten", "be\u00b7gl\u00fc\u00b7cken\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VVINF", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.9": {"text": "bei Erlebnissen,", "tokens": ["bei", "Er\u00b7leb\u00b7nis\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "bei Begebnissen,", "tokens": ["bei", "Be\u00b7geb\u00b7nis\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.11": {"text": "bei Begr\u00e4bnissen,", "tokens": ["bei", "Be\u00b7gr\u00e4b\u00b7nis\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.12": {"text": "bei Hoch-", "tokens": ["bei", "Hoch"], "token_info": ["word", "word"], "pos": ["APPR", "TRUNC"], "meter": "-+", "measure": "iambic.single"}, "line.13": {"text": "und bei noch", "tokens": ["und", "bei", "noch"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.14": {"text": "anderen Zeiten", "tokens": ["an\u00b7de\u00b7ren", "Zei\u00b7ten"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "und Gelegenheiten \u2014", "tokens": ["und", "Ge\u00b7le\u00b7gen\u00b7hei\u00b7ten"], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$("], "meter": "--+-+-", "measure": "anapaest.init"}, "line.16": {"text": "Es leben ", "tokens": ["Es", "le\u00b7ben"], "token_info": ["word", "word"], "pos": ["PPER", "VVINF"], "meter": "-+-", "measure": "amphibrach.single"}, "line.17": {"text": "Die's heute schon sind oder morgen noch werden!", "tokens": ["Die's", "heu\u00b7te", "schon", "sind", "o\u00b7der", "mor\u00b7gen", "noch", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "VAFIN", "KON", "ADV", "ADV", "VAINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}}}}