{"dta.poem.18761": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Betr\u00fcbt euch nicht ihr guten Seelen!", "tokens": ["Be\u00b7tr\u00fcbt", "euch", "nicht", "ihr", "gu\u00b7ten", "See\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Denn wer nicht fehlt weiss wohl wenn andre fehlen;", "tokens": ["Denn", "wer", "nicht", "fehlt", "weiss", "wohl", "wenn", "and\u00b7re", "feh\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PTKNEG", "VVFIN", "ADV", "ADV", "KOUS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Allein wer fehlt der ist erst recht daran,", "tokens": ["Al\u00b7lein", "wer", "fehlt", "der", "ist", "erst", "recht", "da\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VVFIN", "ART", "VAFIN", "ADV", "ADJD", "PAV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Er weiss nun deutlich wie sie wohl gethan.", "tokens": ["Er", "weiss", "nun", "deut\u00b7lich", "wie", "sie", "wohl", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "KOKOM", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}