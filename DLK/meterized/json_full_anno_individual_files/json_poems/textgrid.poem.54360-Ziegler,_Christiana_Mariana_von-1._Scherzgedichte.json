in", "frem\u00b7der", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wohin dich Gl\u00fcck und Schicksal ruft.", "tokens": ["Wo\u00b7hin", "dich", "Gl\u00fcck", "und", "Schick\u00b7sal", "ruft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.36": {"line.1": {"text": "Wir schneiden, kanns nicht anders seyn,", "tokens": ["Wir", "schnei\u00b7den", ",", "kanns", "nicht", "an\u00b7ders", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VMFIN", "PTKNEG", "ADV", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Namen in die Linden ein;", "tokens": ["Den", "Na\u00b7men", "in", "die", "Lin\u00b7den", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Weil deine Tugend wohl verdient,", "tokens": ["Weil", "dei\u00b7ne", "Tu\u00b7gend", "wohl", "ver\u00b7dient", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df dein Ged\u00e4chtnis immer gr\u00fcnt.", "tokens": ["Da\u00df", "dein", "Ge\u00b7d\u00e4cht\u00b7nis", "im\u00b7mer", "gr\u00fcnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}