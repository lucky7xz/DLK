{"textgrid.poem.54360": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1. Scherzgedichte", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da, wo die fahle Pleisse flie\u00dft,", "tokens": ["Da", ",", "wo", "die", "fah\u00b7le", "Pleis\u00b7se", "flie\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sich mit sanftem Fall ergie\u00dft,", "tokens": ["Und", "sich", "mit", "sanf\u00b7tem", "Fall", "er\u00b7gie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da fanden sich bey fr\u00fchem Schein", "tokens": ["Da", "fan\u00b7den", "sich", "bey", "fr\u00fc\u00b7hem", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aurorens sehr viel Nymphen ein.", "tokens": ["Au\u00b7ro\u00b7rens", "sehr", "viel", "Nym\u00b7phen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es ward ein froher Tanz bestellt,", "tokens": ["Es", "ward", "ein", "fro\u00b7her", "Tanz", "be\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem jegliche sich zugesellt;", "tokens": ["Dem", "jeg\u00b7li\u00b7che", "sich", "zu\u00b7ge\u00b7sellt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Weil sich ein solch Gestirne wies,", "tokens": ["Weil", "sich", "ein", "solch", "Ge\u00b7stir\u00b7ne", "wies", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Das sie die Reyhen schliessen hie\u00df.", "tokens": ["Das", "sie", "die", "Rey\u00b7hen", "schlies\u00b7sen", "hie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Das Gl\u00fccke flocht in frischem Glanz", "tokens": ["Das", "Gl\u00fc\u00b7cke", "flocht", "in", "fri\u00b7schem", "Glanz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In ihres Damons Wiegenkranz", "tokens": ["In", "ih\u00b7res", "Da\u00b7mons", "Wie\u00b7gen\u00b7kranz"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein neues Lorberbl\u00e4ttchen ein;", "tokens": ["Ein", "neu\u00b7es", "Lor\u00b7ber\u00b7bl\u00e4tt\u00b7chen", "ein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dies muste ja gefeyret seyn.", "tokens": ["Dies", "mus\u00b7te", "ja", "ge\u00b7fey\u00b7ret", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Hier war die gr\u00f6ste Herrlichkeit,", "tokens": ["Hier", "war", "die", "gr\u00f6s\u00b7te", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie sprungen, tanzten h\u00f6chst erfreut,", "tokens": ["Sie", "sprun\u00b7gen", ",", "tanz\u00b7ten", "h\u00f6chst", "er\u00b7freut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und stimmten solche Lieder an,", "tokens": ["Und", "stimm\u00b7ten", "sol\u00b7che", "Lie\u00b7der", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die man nicht netter h\u00f6ren kann.", "tokens": ["Die", "man", "nicht", "net\u00b7ter", "h\u00f6\u00b7ren", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "PTKNEG", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch mitten in der gr\u00f6sten Lust,", "tokens": ["Doch", "mit\u00b7ten", "in", "der", "gr\u00f6s\u00b7ten", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drang was durch der Gespielen Brust,", "tokens": ["Drang", "was", "durch", "der", "Ge\u00b7spie\u00b7len", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRELS", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-++-+-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Und in der Sch\u00e4ferinnen Ohr,", "tokens": ["Und", "in", "der", "Sch\u00e4\u00b7fe\u00b7rin\u00b7nen", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df sich Gesang und Scherz verlohr.", "tokens": ["Da\u00df", "sich", "Ge\u00b7sang", "und", "Scherz", "ver\u00b7lohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die Schreckenspost erscholl sogleich.", "tokens": ["Die", "Schre\u00b7ckens\u00b7post", "er\u00b7scholl", "sog\u00b7leich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schweigt, Kinder, hie\u00df es, trennet euch,", "tokens": ["Schweigt", ",", "Kin\u00b7der", ",", "hie\u00df", "es", ",", "tren\u00b7net", "euch", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach! euer Sch\u00e4fer, welchen ihr", "tokens": ["Ach", "!", "eu\u00b7er", "Sch\u00e4\u00b7fer", ",", "wel\u00b7chen", "ihr"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "$.", "PPOSAT", "NN", "$,", "PWAT", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So wehrt gehalten, flieht von hier.", "tokens": ["So", "wehrt", "ge\u00b7hal\u00b7ten", ",", "flieht", "von", "hier", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,", "VVFIN", "APPR", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Er l\u00e4\u00dft euch auf den Auen stehn,", "tokens": ["Er", "l\u00e4\u00dft", "euch", "auf", "den", "Au\u00b7en", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Um andern Fluhren nachzugehn.", "tokens": ["Um", "an\u00b7dern", "Fluh\u00b7ren", "nach\u00b7zu\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "VVIZU", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sein Hirtenstock hat, wie man h\u00f6rt,", "tokens": ["Sein", "Hir\u00b7ten\u00b7stock", "hat", ",", "wie", "man", "h\u00f6rt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich in den Wanderstab verkehrt.", "tokens": ["Sich", "in", "den", "Wan\u00b7der\u00b7stab", "ver\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Hier fiel den Nymphen Muth und Sinn", "tokens": ["Hier", "fiel", "den", "Nym\u00b7phen", "Muth", "und", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und alle Freud auf einmal hin;", "tokens": ["Und", "al\u00b7le", "Freud", "auf", "ein\u00b7mal", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie liessen H\u00e4nd und Reyhen los.", "tokens": ["Sie", "lies\u00b7sen", "H\u00e4nd", "und", "Rey\u00b7hen", "los", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn ihr Verlust war gar zu gro\u00df.", "tokens": ["Denn", "ihr", "Ver\u00b7lust", "war", "gar", "zu", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "O! sprachen sie, geht dieser fort,", "tokens": ["O", "!", "spra\u00b7chen", "sie", ",", "geht", "die\u00b7ser", "fort", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "PPER", "$,", "VVFIN", "PDS", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sucht Quartier an fremdem Ort,", "tokens": ["Und", "sucht", "Quar\u00b7tier", "an", "frem\u00b7dem", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So ists um unsre Lust geschehn,", "tokens": ["So", "ists", "um", "uns\u00b7re", "Lust", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er wird doch uns nicht wieder sehn.", "tokens": ["Er", "wird", "doch", "uns", "nicht", "wie\u00b7der", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPER", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Der Kummer \u00fcberh\u00e4ufte sie;", "tokens": ["Der", "Kum\u00b7mer", "\u00fc\u00b7berh\u00b7\u00e4uf\u00b7te", "sie", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und doch gab sich die eine M\u00fch,", "tokens": ["Und", "doch", "gab", "sich", "die", "ei\u00b7ne", "M\u00fch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PRF", "ART", "ART", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Zu dem Orakel hinzugehn,", "tokens": ["Zu", "dem", "O\u00b7ra\u00b7kel", "hin\u00b7zu\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es um den Ausspruch anzuflehn.", "tokens": ["Es", "um", "den", "Aus\u00b7spruch", "an\u00b7zu\u00b7flehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Sie fragt, und dies nicht ohne Pein:", "tokens": ["Sie", "fragt", ",", "und", "dies", "nicht", "oh\u00b7ne", "Pein", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "PDS", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird Damon auch so redlich seyn,", "tokens": ["Wird", "Da\u00b7mon", "auch", "so", "red\u00b7lich", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "ADV", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df er an unsre Pleisse denkt,", "tokens": ["Da\u00df", "er", "an", "uns\u00b7re", "Pleis\u00b7se", "denkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.4": {"text": "Und ihr noch manchmal Seufzer schenkt?", "tokens": ["Und", "ihr", "noch", "manch\u00b7mal", "Seuf\u00b7zer", "schenkt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Ach schlechter Trost: denn dieses sprach:", "tokens": ["Ach", "schlech\u00b7ter", "Trost", ":", "denn", "die\u00b7ses", "sprach", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "$.", "KON", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Seht ihm, verweiste Nymphen, nach;", "tokens": ["Seht", "ihm", ",", "ver\u00b7weis\u00b7te", "Nym\u00b7phen", ",", "nach", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "NN", "$,", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hofft nicht, da\u00df er zur\u00fccke sieht,", "tokens": ["Hofft", "nicht", ",", "da\u00df", "er", "zu\u00b7r\u00fc\u00b7cke", "sieht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weil ihm ein gr\u00f6ssres Gl\u00fccke bl\u00fcht.", "tokens": ["Weil", "ihm", "ein", "gr\u00f6ss\u00b7res", "Gl\u00fc\u00b7cke", "bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "So bald er R \u2013 \u2013 \u2013 erblickt.", "tokens": ["So", "bald", "er", "R", "\u2013", "\u2013", "\u2013", "er\u00b7blickt", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "NE", "$(", "$(", "$(", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und von der Post den Fu\u00df ger\u00fcckt,", "tokens": ["Und", "von", "der", "Post", "den", "Fu\u00df", "ge\u00b7r\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So beut ihm alles, was nur kann,", "tokens": ["So", "beut", "ihm", "al\u00b7les", ",", "was", "nur", "kann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "$,", "PRELS", "ADV", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Quartier und Herz freywillig an.", "tokens": ["Quar\u00b7tier", "und", "Herz", "frey\u00b7wil\u00b7lig", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Man l\u00e4\u00dft ihm weder Fried noch Ruh;", "tokens": ["Man", "l\u00e4\u00dft", "ihm", "we\u00b7der", "Fried", "noch", "Ruh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "KON", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zw\u00f6lf Nymphen reichen hier nicht zu,", "tokens": ["Zw\u00f6lf", "Nym\u00b7phen", "rei\u00b7chen", "hier", "nicht", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD",