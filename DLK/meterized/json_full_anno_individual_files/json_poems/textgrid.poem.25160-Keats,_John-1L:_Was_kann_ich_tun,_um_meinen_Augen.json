{"textgrid.poem.25160": {"metadata": {"author": {"name": "Keats, John", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was kann ich tun, um meinen Augen", "genre": "verse", "period": "N.A.", "pub_year": 1819, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was kann ich tun, um meinen Augen", "tokens": ["Was", "kann", "ich", "tun", ",", "um", "mei\u00b7nen", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "VVINF", "$,", "KOUI", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erinnrung zu entziehn? Warst du doch nah;", "tokens": ["E\u00b7rinn\u00b7rung", "zu", "ent\u00b7ziehn", "?", "Warst", "du", "doch", "nah", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$.", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Erst eine Stunde ging, seit ich dich sah,", "tokens": ["Erst", "ei\u00b7ne", "Stun\u00b7de", "ging", ",", "seit", "ich", "dich", "sah", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit durstigem Blick dein Bildnis aufzusaugen.", "tokens": ["Mit", "durs\u00b7ti\u00b7gem", "Blick", "dein", "Bild\u00b7nis", "auf\u00b7zu\u00b7sau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Ber\u00fchrung hat Ged\u00e4chtnis! Lieb, o sage,", "tokens": ["Be\u00b7r\u00fch\u00b7rung", "hat", "Ge\u00b7d\u00e4cht\u00b7nis", "!", "Lieb", ",", "o", "sa\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$.", "NN", "$,", "FM", "FM", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wie kann ich das ert\u00f6ten?", "tokens": ["Wie", "kann", "ich", "das", "er\u00b7t\u00f6\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PDS", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wie rett ich mich aus diesen tiefen N\u00f6ten,", "tokens": ["Wie", "rett", "ich", "mich", "aus", "die\u00b7sen", "tie\u00b7fen", "N\u00f6\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df ich in aller Freiheit wieder rage?", "tokens": ["Da\u00df", "ich", "in", "al\u00b7ler", "Frei\u00b7heit", "wie\u00b7der", "ra\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Wenn jeder Sch\u00f6nen, die ich sah, mein Fang", "tokens": ["Wenn", "je\u00b7der", "Sch\u00f6\u00b7nen", ",", "die", "ich", "sah", ",", "mein", "Fang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Geschickt gelang,", "tokens": ["Ge\u00b7schickt", "ge\u00b7lang", ","], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "So ri\u00df doch bald die schlechtgewebte Schlinge,", "tokens": ["So", "ri\u00df", "doch", "bald", "die", "schlecht\u00b7ge\u00b7web\u00b7te", "Schlin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und ich entsprang!", "tokens": ["Und", "ich", "ent\u00b7sprang", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Ob d\u00fcrftige, ob farbenbunte Dinge \u2013", "tokens": ["Ob", "d\u00fcrf\u00b7ti\u00b7ge", ",", "ob", "far\u00b7ben\u00b7bun\u00b7te", "Din\u00b7ge", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "$,", "KOUS", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Ich f\u00fchlte meiner Muse Fl\u00fcgel,", "tokens": ["Ich", "f\u00fchl\u00b7te", "mei\u00b7ner", "Mu\u00b7se", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Ich hielt die Z\u00fcgel!", "tokens": ["Ich", "hielt", "die", "Z\u00fc\u00b7gel", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.16": {"text": "Und stets war ihre Kraft bereit", "tokens": ["Und", "stets", "war", "ih\u00b7re", "Kraft", "be\u00b7reit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Sich meinem Wunsch zu schenken,", "tokens": ["Sich", "mei\u00b7nem", "Wunsch", "zu", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Der ohne nachzudenken", "tokens": ["Der", "oh\u00b7ne", "nach\u00b7zu\u00b7den\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["ART", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Doch thronte in erhabner G\u00f6ttlichkeit.", "tokens": ["Doch", "thron\u00b7te", "in", "er\u00b7hab\u00b7ner", "G\u00f6tt\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "In G\u00f6ttlichkeit! \u2013 Der Vogel, den sein Flug", "tokens": ["In", "G\u00f6tt\u00b7lich\u00b7keit", "!", "\u2013", "Der", "Vo\u00b7gel", ",", "den", "sein", "Flug"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$.", "$(", "ART", "NE", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Hintanzend \u00fcber Meeresrauschen trug,", "tokens": ["Hin\u00b7tan\u00b7zend", "\u00fc\u00b7ber", "Mee\u00b7res\u00b7rau\u00b7schen", "trug", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Wird er im heitren Steigen, Neigen, Senken \u2013", "tokens": ["Wird", "er", "im", "hei\u00b7tren", "Stei\u00b7gen", ",", "Nei\u00b7gen", ",", "Sen\u00b7ken", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "ADJA", "NN", "$,", "NN", "$,", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Ein Philosoph \u2013 an Ziel und Absicht denken?", "tokens": ["Ein", "Phi\u00b7lo\u00b7soph", "\u2013", "an", "Ziel", "und", "Ab\u00b7sicht", "den\u00b7ken", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie soll ich tun,", "tokens": ["Wie", "soll", "ich", "tun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Von neuem nun.", "tokens": ["Von", "neu\u00b7em", "nun", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Verlorne Federn wiederzuempfangen,", "tokens": ["Ver\u00b7lor\u00b7ne", "Fe\u00b7dern", "wie\u00b7der\u00b7zu\u00b7em\u00b7pfan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Empor, empor,", "tokens": ["Em\u00b7por", ",", "em\u00b7por", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Bis drunten Amors Flattern sich verlor,", "tokens": ["Bis", "drun\u00b7ten", "A\u00b7mors", "Flat\u00b7tern", "sich", "ver\u00b7lor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "NE", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In ewigreinen \u00c4ther zu gelangen? \u2013", "tokens": ["In", "e\u00b7wig\u00b7rei\u00b7nen", "\u00c4\u00b7ther", "zu", "ge\u00b7lan\u00b7gen", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Berausche dich in Wein! \u2013", "tokens": ["Be\u00b7rau\u00b7sche", "dich", "in", "Wein", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PRF", "APPR", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Das ist gemein,", "tokens": ["Das", "ist", "ge\u00b7mein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Ist S\u00fcnde, Ketzerei,", "tokens": ["Ist", "S\u00fcn\u00b7de", ",", "Ket\u00b7ze\u00b7rei", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Die das Gesetz der Liebe schm\u00e4hlich sch\u00e4ndet.", "tokens": ["Die", "das", "Ge\u00b7setz", "der", "Lie\u00b7be", "schm\u00e4h\u00b7lich", "sch\u00e4n\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Nein, \u2013 nur den Frohen macht das Trinken frei,", "tokens": ["Nein", ",", "\u2013", "nur", "den", "Fro\u00b7hen", "macht", "das", "Trin\u00b7ken", "frei", ","], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "$(", "ADV", "ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Doch mir ist ", "tokens": ["Doch", "mir", "ist"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPER", "VAFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "Wie soll ich wissen, wo mein Friede sei?", "tokens": ["Wie", "soll", "ich", "wis\u00b7sen", ",", "wo", "mein", "Frie\u00b7de", "sei", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "VVINF", "$,", "PWAV", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Und wie mich st\u00e4hlen, jenem grausigen Lande,", "tokens": ["Und", "wie", "mich", "st\u00e4h\u00b7len", ",", "je\u00b7nem", "grau\u00b7si\u00b7gen", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Dem Kerker meiner Freude, fern zu bleiben:", "tokens": ["Dem", "Ker\u00b7ker", "mei\u00b7ner", "Freu\u00b7de", ",", "fern", "zu", "blei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Dem eklen Strande,", "tokens": ["Dem", "ek\u00b7len", "Stran\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.17": {"text": "An dem sie scheiterten und haltlos treiben;", "tokens": ["An", "dem", "sie", "schei\u00b7ter\u00b7ten", "und", "halt\u00b7los", "trei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Der f\u00fcrchterlichen Welt, wo tr\u00fcbe Fl\u00fcsse", "tokens": ["Der", "f\u00fcrch\u00b7ter\u00b7li\u00b7chen", "Welt", ",", "wo", "tr\u00fc\u00b7be", "Fl\u00fcs\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Die schmutzigen Wellen an die Ufer sp\u00fclen", "tokens": ["Die", "schmut\u00b7zi\u00b7gen", "Wel\u00b7len", "an", "die", "U\u00b7fer", "sp\u00fc\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "VVINF"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Und nie die N\u00e4he heitrer G\u00f6tter f\u00fchlen \u2013", "tokens": ["Und", "nie", "die", "N\u00e4\u00b7he", "hei\u00b7trer", "G\u00f6t\u00b7ter", "f\u00fch\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Wo rauher Wind beeiste Ruten schwingt", "tokens": ["Wo", "rau\u00b7her", "Wind", "be\u00b7eis\u00b7te", "Ru\u00b7ten", "schwingt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Und Gei\u00dfelhiebe bringt", "tokens": ["Und", "Gei\u00b7\u00df\u00b7el\u00b7hie\u00b7be", "bringt"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.23": {"text": "Und wilden Schmerz als einzige Gen\u00fcsse \u2013", "tokens": ["Und", "wil\u00b7den", "Schmerz", "als", "ein\u00b7zi\u00b7ge", "Ge\u00b7n\u00fcs\u00b7se", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KOUS", "ADJA", "NN", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.24": {"text": "Wo blind und schwarz erfrorne W\u00e4lder ragen,", "tokens": ["Wo", "blind", "und", "schwarz", "er\u00b7fror\u00b7ne", "W\u00e4l\u00b7der", "ra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Dryaden schreckend \u2013, wo verdorrtes Gras,", "tokens": ["Dry\u00b7a\u00b7den", "schre\u00b7ckend", "\u2013", ",", "wo", "ver\u00b7dorr\u00b7tes", "Gras", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$(", "$,", "PWAV", "ADJA", "NN", "$,"], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.26": {"text": "Des d\u00fcrren Ochsen widerlicher Fra\u00df,", "tokens": ["Des", "d\u00fcr\u00b7ren", "Och\u00b7sen", "wi\u00b7der\u00b7li\u00b7cher", "Fra\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Die Wiesen deckt, die keine Blumen tragen \u2013", "tokens": ["Die", "Wie\u00b7sen", "deckt", ",", "die", "kei\u00b7ne", "Blu\u00b7men", "tra\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Wo niemals lockt ein lieber Vogelruf:", "tokens": ["Wo", "nie\u00b7mals", "lockt", "ein", "lie\u00b7ber", "Vo\u00b7gel\u00b7ruf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Dem Land, das die Natur im Zorn erschuf!", "tokens": ["Dem", "Land", ",", "das", "die", "Na\u00b7tur", "im", "Zorn", "er\u00b7schuf", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O da\u00df ein Wunder k\u00e4me!", "tokens": ["O", "da\u00df", "ein", "Wun\u00b7der", "k\u00e4\u00b7me", "!"], "token_info": ["word", "w