{"textgrid.poem.47981": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Gewitterwind", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von den H\u00f6hen braust der Gewitterwind,", "tokens": ["Von", "den", "H\u00f6\u00b7hen", "braust", "der", "Ge\u00b7wit\u00b7ter\u00b7wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "und die B\u00e4ume wirbeln und schwanken;", "tokens": ["und", "die", "B\u00e4u\u00b7me", "wir\u00b7beln", "und", "schwan\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "KON", "VVINF", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "wie die wehenden Bl\u00e4tter im Winde sind", "tokens": ["wie", "die", "we\u00b7hen\u00b7den", "Bl\u00e4t\u00b7ter", "im", "Win\u00b7de", "sind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPRART", "NN", "VAFIN"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "im Haupte mir die Gedanken. \u2013", "tokens": ["im", "Haup\u00b7te", "mir", "die", "Ge\u00b7dan\u00b7ken", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und es war eine Zeit, da leis und lind", "tokens": ["Und", "es", "war", "ei\u00b7ne", "Zeit", ",", "da", "leis", "und", "lind"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "$,", "KOUS