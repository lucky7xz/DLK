{"dta.poem.22502": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "52. Grabschrifft der H. Jungfrauen  \n  Gertrudis.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Glaub hier in diesem Grab ligt nur ein blosser schein/", "tokens": ["Glaub", "hier", "in", "die\u00b7sem", "Grab", "ligt", "nur", "ein", "blos\u00b7ser", "schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PDAT", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es kan ", "tokens": ["Es", "kan"], "token_info": ["word", "word"], "pos": ["PPER", "VMFIN"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Wo sie nicht solt jhr Grab im Hertzen JEsu haben/", "tokens": ["Wo", "sie", "nicht", "solt", "jhr", "Grab", "im", "Hert\u00b7zen", "Je\u00b7su", "ha\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "VMFIN", "PPOSAT", "NN", "APPRART", "NN", "NE", "VAFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So muste JEsus seyn au\u00df jhrem au\u00dfgegraben.", "tokens": ["So", "mus\u00b7te", "Je\u00b7sus", "seyn", "au\u00df", "jhrem", "au\u00df\u00b7ge\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NE", "PPOSAT", "APPR", "PPOSAT", "VVPP", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}}}}