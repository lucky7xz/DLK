{"dta.poem.23342": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Hausorden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dem Verdienste seine Kronen!", "tokens": ["Dem", "Ver\u00b7diens\u00b7te", "sei\u00b7ne", "Kro\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Also denket mancher Mann,", "tokens": ["Al\u00b7so", "den\u00b7ket", "man\u00b7cher", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und er will sich selbst belohnen,", "tokens": ["Und", "er", "will", "sich", "selbst", "be\u00b7loh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PRF", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn kein andrer denkt daran.", "tokens": ["Denn", "kein", "an\u00b7drer", "denkt", "da\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "VVFIN", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und wie gro\u00dfe Potentaten", "tokens": ["Und", "wie", "gro\u00b7\u00dfe", "Po\u00b7ten\u00b7ta\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heckt er einen Orden aus", "tokens": ["Heckt", "er", "ei\u00b7nen", "Or\u00b7den", "aus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zur Belohnung seiner Thaten", "tokens": ["Zur", "Be\u00b7loh\u00b7nung", "sei\u00b7ner", "Tha\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur f\u00fcr sich und f\u00fcr sein Haus.", "tokens": ["Nur", "f\u00fcr", "sich", "und", "f\u00fcr", "sein", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRF", "KON", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und er theilet dann in Klassen", "tokens": ["Und", "er", "thei\u00b7let", "dann", "in", "Klas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Diesen Orden seiner Wahl,", "tokens": ["Die\u00b7sen", "Or\u00b7den", "sei\u00b7ner", "Wahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und er will damit umfassen", "tokens": ["Und", "er", "will", "da\u00b7mit", "um\u00b7fas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "PAV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der Verdienste gro\u00dfe Zahl.", "tokens": ["Der", "Ver\u00b7diens\u00b7te", "gro\u00b7\u00dfe", "Zahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ehekreuz das ist die erste,", "tokens": ["E\u00b7he\u00b7kreuz", "das", "ist", "die", "ers\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "VAFIN", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hauskreuz mu\u00df die zweite sein,", "tokens": ["Haus\u00b7kreuz", "mu\u00df", "die", "zwei\u00b7te", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "ADJA", "VAINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und dann kommt die sch\u00f6nst' und hehrste,", "tokens": ["Und", "dann", "kommt", "die", "sch\u00f6nst'", "und", "hehrs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "VVFIN", "KON", "ADJA", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Todtenkreuz noch hinterdrein.", "tokens": ["Tod\u00b7ten\u00b7kreuz", "noch", "hin\u00b7ter\u00b7drein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Seit die Orden sind geworden", "tokens": ["Seit", "die", "Or\u00b7den", "sind", "ge\u00b7wor\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAFIN", "VAPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jedem Stand' ein Liebespfand,", "tokens": ["Je\u00b7dem", "Stand'", "ein", "Lie\u00b7be\u00b7spfand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nun, so hascht man auch nach Orden", "tokens": ["Nun", ",", "so", "hascht", "man", "auch", "nach", "Or\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "VVFIN", "PIS", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem heil'gen Ehestand.", "tokens": ["In", "dem", "heil'\u00b7gen", "E\u00b7hes\u00b7tand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wenn dich drum der Staat nicht ehret,", "tokens": ["Wenn", "dich", "drum", "der", "Staat", "nicht", "eh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Werde gleich ein Ehemann,", "tokens": ["Wer\u00b7de", "gleich", "ein", "E\u00b7he\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dir wird ein Kreuz bescheret,", "tokens": ["Und", "dir", "wird", "ein", "Kreuz", "be\u00b7sche\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df du denkst zeitlebens dran.", "tokens": ["Da\u00df", "du", "denkst", "zeit\u00b7le\u00b7bens", "dran", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "+-++-+-", "measure": "unknown.measure.tetra"}}}}}