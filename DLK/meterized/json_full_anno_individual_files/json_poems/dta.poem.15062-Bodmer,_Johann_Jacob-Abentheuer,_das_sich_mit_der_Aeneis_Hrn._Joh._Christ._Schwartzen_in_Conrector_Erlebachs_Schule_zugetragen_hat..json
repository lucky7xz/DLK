{"dta.poem.15062": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Abentheuer, das sich mit der Aeneis  \n Hrn. Joh. Christ. Schwartzen in Conrector  \n Erlebachs Schule zugetragen hat.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198476", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da ich mein Vaterland und Trojens Port verlie\u00df,", "tokens": ["Da", "ich", "mein", "Va\u00b7ter\u00b7land", "und", "Tro\u00b7jens", "Port", "ver\u00b7lie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "KON", "NE", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da weint ich; und mein Sohn, und die so bey uns waren", "tokens": ["Da", "weint", "ich", ";", "und", "mein", "Sohn", ",", "und", "die", "so", "bey", "uns", "wa\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "KON", "PPOSAT", "NN", "$,", "KON", "ART", "ADV", "APPR", "PPER", "VAFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sind mit der G\u00f6tter Schutz ins Elend abgefahren.", "tokens": ["Sind", "mit", "der", "G\u00f6t\u00b7ter", "Schutz", "ins", "E\u00b7lend", "ab\u00b7ge\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}