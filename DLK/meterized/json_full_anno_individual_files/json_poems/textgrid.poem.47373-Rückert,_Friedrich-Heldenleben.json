{"textgrid.poem.47373": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Heldenleben", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ist des alten Heldenlebens Geist,", "tokens": ["Das", "ist", "des", "al\u00b7ten", "Hel\u00b7den\u00b7le\u00b7bens", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df, wie du immer ihm entfremdet seist,", "tokens": ["Da\u00df", ",", "wie", "du", "im\u00b7mer", "ihm", "ent\u00b7frem\u00b7det", "seist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWAV", "PPER", "ADV", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du dich ergriffen von der Herrlichkeit,", "tokens": ["Du", "dich", "er\u00b7grif\u00b7fen", "von", "der", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ersch\u00fcttert f\u00fchlst, erhoben und geweiht,", "tokens": ["Er\u00b7sch\u00fct\u00b7tert", "f\u00fchlst", ",", "er\u00b7ho\u00b7ben", "und", "ge\u00b7weiht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Zugleich erkennest, da\u00df, wie frisch und stark,", "tokens": ["Zu\u00b7gleich", "er\u00b7ken\u00b7nest", ",", "da\u00df", ",", "wie", "frisch", "und", "stark", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", 