{"dta.poem.21112": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Abends unter der Linde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Woher, o namenloses Sehnen,               ", "tokens": ["Wo\u00b7her", ",", "o", "na\u00b7men\u00b7lo\u00b7ses", "Seh\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "FM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das den beklemmten Busen presst?", "tokens": ["Das", "den", "be\u00b7klemm\u00b7ten", "Bu\u00b7sen", "presst", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Woher, ihr bitters\u00fcssen Thr\u00e4nen,", "tokens": ["Wo\u00b7her", ",", "ihr", "bit\u00b7ter\u00b7s\u00fcs\u00b7sen", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die ihr das Auge d\u00e4mmernd n\u00e4sst?", "tokens": ["Die", "ihr", "das", "Au\u00b7ge", "d\u00e4m\u00b7mernd", "n\u00e4sst", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O Abendroth, o Mondenblitz,", "tokens": ["O", "A\u00b7ben\u00b7droth", ",", "o", "Mon\u00b7den\u00b7blitz", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Flimmt blasser um den Lindensitz!", "tokens": ["Flimmt", "blas\u00b7ser", "um", "den", "Lin\u00b7den\u00b7sitz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es s\u00e4uselt in dem Laub der Linde;", "tokens": ["Es", "s\u00e4u\u00b7selt", "in", "dem", "Laub", "der", "Lin\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es flistert im Akazienstrauch.", "tokens": ["Es", "flis\u00b7tert", "im", "A\u00b7ka\u00b7zi\u00b7en\u00b7strauch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Mir schmeichelt s\u00fcss, mir schmeichelt linde", "tokens": ["Mir", "schmei\u00b7chelt", "s\u00fcss", ",", "mir", "schmei\u00b7chelt", "lin\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "PPER", "VVFIN", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des grauen Abends lauer Hauch.", "tokens": ["Des", "grau\u00b7en", "A\u00b7bends", "lau\u00b7er", "Hauch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es spricht um mich, wie Geistergruss;", "tokens": ["Es", "spricht", "um", "mich", ",", "wie", "Geis\u00b7ter\u00b7gruss", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "PWAV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es weht mich an, wie Engelkuss.", "tokens": ["Es", "weht", "mich", "an", ",", "wie", "En\u00b7gel\u00b7kuss", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "PWAV", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es gl\u00e4nzt, es gl\u00e4nzt im Nachtgefilde.", "tokens": ["Es", "gl\u00e4nzt", ",", "es", "gl\u00e4nzt", "im", "Nacht\u00b7ge\u00b7fil\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Linde graue Scheitel bebt \u2014", "tokens": ["Der", "Lin\u00b7de", "grau\u00b7e", "Schei\u00b7tel", "bebt"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verkl\u00e4rte himmlische Gebilde,", "tokens": ["Ver\u00b7kl\u00e4r\u00b7te", "himm\u00b7li\u00b7sche", "Ge\u00b7bil\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Seyd ihr es, die ihr mich umschwebt?", "tokens": ["Seyd", "ihr", "es", ",", "die", "ihr", "mich", "um\u00b7schwebt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "PPER", "$,", "PRELS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich f\u00fchle eures Athems Kuss,", "tokens": ["Ich", "f\u00fch\u00b7le", "eu\u00b7res", "A\u00b7thems", "Kuss", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O Julie! o Emilius!", "tokens": ["O", "Ju\u00b7lie", "!", "o", "E\u00b7mi\u00b7lius", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "FM", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Bleibt, Sel'ge, bleibt in eurem Eden!", "tokens": ["Bleibt", ",", "Sel'\u00b7ge", ",", "bleibt", "in", "eu\u00b7rem", "E\u00b7den", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Lebens Hauch bl\u00e4st schwer und schw\u00fcl", "tokens": ["Des", "Le\u00b7bens", "Hauch", "bl\u00e4st", "schwer", "und", "schw\u00fcl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch stumme leichenvolle \u00d6den.", "tokens": ["Durch", "stum\u00b7me", "lei\u00b7chen\u00b7vol\u00b7le", "\u00d6\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Elisium ist mild und k\u00fchl.", "tokens": ["E\u00b7li\u00b7si\u00b7um", "ist", "mild", "und", "k\u00fchl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Elisium ist wonnevoll \u2014", "tokens": ["E\u00b7li\u00b7si\u00b7um", "ist", "won\u00b7ne\u00b7voll"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Fahrt wohl, ihr Trauten! fahret wohl!", "tokens": ["Fahrt", "wohl", ",", "ihr", "Trau\u00b7ten", "!", "fah\u00b7ret", "wohl", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPOSAT", "NN", "$.", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}