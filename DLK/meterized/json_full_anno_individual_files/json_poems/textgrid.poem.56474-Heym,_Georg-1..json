{"textgrid.poem.56474": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie Feuerregen f\u00fcllt den Ozean", "tokens": ["Wie", "Feu\u00b7er\u00b7re\u00b7gen", "f\u00fcllt", "den", "O\u00b7ze\u00b7an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der schwarze Gram. Die gro\u00dfen Wogen t\u00fcrmt", "tokens": ["Der", "schwar\u00b7ze", "Gram", ".", "Die", "gro\u00b7\u00dfen", "Wo\u00b7gen", "t\u00fcrmt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der S\u00fcdwind auf, der in die Segel st\u00fcrmt,", "tokens": ["Der", "S\u00fcd\u00b7wind", "auf", ",", "der", "in", "die", "Se\u00b7gel", "st\u00fcrmt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die schwarz und riesig flattern im Orkan.", "tokens": ["Die", "schwarz", "und", "rie\u00b7sig", "flat\u00b7tern", "im", "Or\u00b7kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "VVINF", "APPRART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ein Vogel fliegt voraus. Sein langes Haar", "tokens": ["Ein", "Vo\u00b7gel", "fliegt", "vo\u00b7raus", ".", "Sein", "lan\u00b7ges", "Haar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "PTKVZ", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Str\u00e4ubt von den Winden um das Haupt ihm gro\u00df.", "tokens": ["Str\u00e4ubt", "von", "den", "Win\u00b7den", "um", "das", "Haupt", "ihm", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPR", "ART", "NN", "PPER", "ADJD", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Der Wasser Dunkelheit, die meilenlos,", "tokens": ["Der", "Was\u00b7ser", "Dun\u00b7kel\u00b7heit", ",", "die", "mei\u00b7len\u00b7los", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Umarmt er riesig mit dem Schwingenpaar.", "tokens": ["Um\u00b7armt", "er", "rie\u00b7sig", "mit", "dem", "Schwin\u00b7gen\u00b7paar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Vorbei an China, wo das gelbe Meer", "tokens": ["Vor\u00b7bei", "an", "Chi\u00b7na", ",", "wo", "das", "gel\u00b7be", "Meer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NE", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Die Drachendschunken vor den St\u00e4dten wiegt,", "tokens": ["Die", "Dra\u00b7chend\u00b7schun\u00b7ken", "vor", "den", "St\u00e4d\u00b7ten", "wiegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo Feuerwerk die Himmel \u00fcberfliegt", "tokens": ["Wo", "Feu\u00b7er\u00b7werk", "die", "Him\u00b7mel", "\u00fc\u00b7berf\u00b7liegt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Trommeln schlagen um die Tempel her.", "tokens": ["Und", "Trom\u00b7meln", "schla\u00b7gen", "um", "die", "Tem\u00b7pel", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der Regen jagt, der sp\u00e4rlich niedertropft", "tokens": ["Der", "Re\u00b7gen", "jagt", ",", "der", "sp\u00e4r\u00b7lich", "nie\u00b7der\u00b7tropft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "ADJD", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf seinen Mantel, der im Sturme bl\u00e4ht.", "tokens": ["Auf", "sei\u00b7nen", "Man\u00b7tel", ",", "der", "im", "Stur\u00b7me", "bl\u00e4ht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Im Mast, der hinter seinem R\u00fccken steht,", "tokens": ["Im", "Mast", ",", "der", "hin\u00b7ter", "sei\u00b7nem", "R\u00fc\u00b7cken", "steht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "H\u00f6rt er die Totenuhr, die ruhlos klopft.", "tokens": ["H\u00f6rt", "er", "die", "To\u00b7te\u00b7nuhr", ",", "die", "ruh\u00b7los", "klopft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Die Larve einer toten Ewigkeit", "tokens": ["Die", "Lar\u00b7ve", "ei\u00b7ner", "to\u00b7ten", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hat sein Gesicht mit Leere \u00fcbereist.", "tokens": ["Hat", "sein", "Ge\u00b7sicht", "mit", "Lee\u00b7re", "\u00fc\u00b7be\u00b7reist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "D\u00fcrr, wie ein Wald, durch den ein Feuer reist.", "tokens": ["D\u00fcrr", ",", "wie", "ein", "Wald", ",", "durch", "den", "ein", "Feu\u00b7er", "reist", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "$,", "APPR", "ART", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Wie tr\u00fcber Staub umflackert es die Zeit.", "tokens": ["Wie", "tr\u00fc\u00b7ber", "Staub", "um\u00b7fla\u00b7ckert", "es", "die", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Die Jahre graben sich der Stirne ein,", "tokens": ["Die", "Jah\u00b7re", "gra\u00b7ben", "sich", "der", "Stir\u00b7ne", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die wie ein alter Baum die Borke tr\u00e4gt.", "tokens": ["Die", "wie", "ein", "al\u00b7ter", "Baum", "die", "Bor\u00b7ke", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sein wei\u00dfes Haar, das Wintersturmwind fegt,", "tokens": ["Sein", "wei\u00b7\u00dfes", "Haar", ",", "das", "Win\u00b7ter\u00b7sturm\u00b7wind", "fegt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Steht wie ein Feuer um der Schl\u00e4fen Stein.", "tokens": ["Steht", "wie", "ein", "Feu\u00b7er", "um", "der", "Schl\u00e4\u00b7fen", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Die Schiffer an den Rudern sind verdorrt,", "tokens": ["Die", "Schif\u00b7fer", "an", "den", "Ru\u00b7dern", "sind", "ver\u00b7dorrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Als Mumien schlafen sie auf ihrer Bank.", "tokens": ["Als", "Mu\u00b7mi\u00b7en", "schla\u00b7fen", "sie", "auf", "ih\u00b7rer", "Bank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und ihre H\u00e4nde sind wie Wurzeln lang", "tokens": ["Und", "ih\u00b7re", "H\u00e4n\u00b7de", "sind", "wie", "Wur\u00b7zeln", "lang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "KOKOM", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hereingewachsen in den morschen Bord.", "tokens": ["Her\u00b7ein\u00b7ge\u00b7wach\u00b7sen", "in", "den", "mor\u00b7schen", "Bord", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Ihr Schifferzopf wand sich wie ein Barett", "tokens": ["Ihr", "Schif\u00b7fer\u00b7zopf", "wand", "sich", "wie", "ein", "Ba\u00b7rett"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "KOKOM", "ART", "NN"], "meter": "-+-++-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Um ihren Kopf herum, der schwankt im Wind.", "tokens": ["Um", "ih\u00b7ren", "Kopf", "he\u00b7rum", ",", "der", "schwankt", "im", "Wind", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "PTKVZ", "$,", "PRELS", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und auf den H\u00e4lsen, die wie R\u00f6hren sind,", "tokens": ["Und", "auf", "den", "H\u00e4l\u00b7sen", ",", "die", "wie", "R\u00f6h\u00b7ren", "sind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "KOKOM", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "H\u00e4ngt jedem noch ein gro\u00dfes Amulett.", "tokens": ["H\u00e4ngt", "je\u00b7dem", "noch", "ein", "gro\u00b7\u00dfes", "A\u00b7mu\u00b7lett", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Er ruft sie an, sie h\u00f6ren nimmermehr.", "tokens": ["Er", "ruft", "sie", "an", ",", "sie", "h\u00f6\u00b7ren", "nim\u00b7mer\u00b7mehr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Herbst hat Moos in ihrem Ohr gepflanzt,", "tokens": ["Der", "Herbst", "hat", "Moos", "in", "ih\u00b7rem", "Ohr", "ge\u00b7pflanzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das gr\u00fcnlich h\u00e4ngt und in dem Winde tanzt", "tokens": ["Das", "gr\u00fcn\u00b7lich", "h\u00e4ngt", "und", "in", "dem", "Win\u00b7de", "tanzt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADJD", "VVFIN", "KON", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Um ihre welken Backen hin und her.", "tokens": ["Um", "ih\u00b7re", "wel\u00b7ken", "Ba\u00b7cken", "hin", "und", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Wie Feuerregen f\u00fcllt den Ozean", "tokens": ["Wie", "Feu\u00b7er\u00b7re\u00b7gen", "f\u00fcllt", "den", "O\u00b7ze\u00b7an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der schwarze Gram. Die gro\u00dfen Wogen t\u00fcrmt", "tokens"