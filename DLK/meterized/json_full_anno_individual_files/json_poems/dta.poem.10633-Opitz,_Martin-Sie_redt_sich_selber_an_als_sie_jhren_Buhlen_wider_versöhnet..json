{"dta.poem.10633": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Sie redt sich selber an/ als sie jhren Buhlen  \n wider vers\u00f6hnet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.85", "nl:0.14"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Dv hochgeborne Fraw/ die du so reich gezieret", "tokens": ["Dv", "hoch\u00b7ge\u00b7bor\u00b7ne", "Fraw", "/", "die", "du", "so", "reich", "ge\u00b7zie\u00b7ret"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$(", "PRELS", "PPER", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Gaben vnd Geschenck de\u00df Himmels manigfalt/", "tokens": ["Mit", "Ga\u00b7ben", "vnd", "Ge\u00b7schenck", "de\u00df", "Him\u00b7mels", "ma\u00b7nig\u00b7falt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der dich mit treffligkeit der edelen gestalt", "tokens": ["Der", "dich", "mit", "tref\u00b7flig\u00b7keit", "der", "e\u00b7de\u00b7len", "ge\u00b7stalt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Begabt/ da\u00df seine Macht werd\u2019 in dir recht gesp\u00fcret/", "tokens": ["Be\u00b7gabt", "/", "da\u00df", "sei\u00b7ne", "Macht", "werd'", "in", "dir", "recht", "ge\u00b7sp\u00fc\u00b7ret", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "KOUS", "PPOSAT", "NN", "VAFIN", "APPR", "PPER", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ins k\u00fcnfftig wetter nicht zuklagen dir geb\u00fcret", "tokens": ["Ins", "k\u00fcnff\u00b7tig", "wet\u00b7ter", "nicht", "zu\u00b7kla\u00b7gen", "dir", "ge\u00b7b\u00fc\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJD", "NN", "PTKNEG", "VVFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "In deinem hohen Sinn/ der Ha\u00df vnd die Gewalt", "tokens": ["In", "dei\u00b7nem", "ho\u00b7hen", "Sinn", "/", "der", "Ha\u00df", "vnd", "die", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$(", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ist au\u00df/ die zwar dein Feindt/ doch Trost vnd auffenthalt/", "tokens": ["Ist", "au\u00df", "/", "die", "zwar", "dein", "Feindt", "/", "doch", "Trost", "vnd", "auf\u00b7fent\u00b7halt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "$(", "PRELS", "ADV", "PPOSAT", "NN", "$(", "ADV", "NN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mehr als zu lange Zeit hat wider dich gef\u00fcret.", "tokens": ["Mehr", "als", "zu", "lan\u00b7ge", "Zeit", "hat", "wi\u00b7der", "dich", "ge\u00b7f\u00fc\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "APPR", "ADJA", "NN", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "In einem Augenblick wirdt dir das Gl\u00fcck geneiget/", "tokens": ["In", "ei\u00b7nem", "Au\u00b7gen\u00b7blick", "wirdt", "dir", "das", "Gl\u00fcck", "ge\u00b7nei\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die Sonn hat sich bi\u00dfher de\u00dfwegen tr\u00fcb erzeiget/", "tokens": ["Die", "Sonn", "hat", "sich", "bi\u00df\u00b7her", "de\u00df\u00b7we\u00b7gen", "tr\u00fcb", "er\u00b7zei\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PRF", "ADV", "PAV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Auff da\u00df sie dein Gem\u00fcth recht zuerkennen krieg\u2019", "tokens": ["Auff", "da\u00df", "sie", "dein", "Ge\u00b7m\u00fcth", "recht", "zu\u00b7er\u00b7ken\u00b7nen", "krieg'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "PPOSAT", "NN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Jetz ist jhr heller Schein/ jhr klares Liecht ernewet/", "tokens": ["Jetz", "ist", "jhr", "hel\u00b7ler", "Schein", "/", "jhr", "kla\u00b7res", "Liecht", "er\u00b7ne\u00b7wet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "$(", "PPOSAT", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.13": {"text": "Vnd hat jhn seines Sinns vnd h\u00e4rtigkeit gerewet/", "tokens": ["Vnd", "hat", "jhn", "sei\u00b7nes", "Sinns", "vnd", "h\u00e4r\u00b7tig\u00b7keit", "ge\u00b7re\u00b7wet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PPOSAT", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.14": {"text": "Je gr\u00f6sser Vbel war/ je sch\u00f6ner ist der Sieg.", "tokens": ["Je", "gr\u00f6s\u00b7ser", "Vbel", "war", "/", "je", "sch\u00f6\u00b7ner", "ist", "der", "Sieg", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VAFIN", "$(", "ADV", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}}}}