{"dta.poem.10880": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Eine Viole Matronal  \n im Herbst.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie ein kleiner weisser Stral,", "tokens": ["Wie", "ein", "klei\u00b7ner", "weis\u00b7ser", "Stral", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der durch gr\u00fcne Zweige bricht,", "tokens": ["Der", "durch", "gr\u00fc\u00b7ne", "Zwei\u00b7ge", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fiel mir j\u00fcngst ein nettes Sp\u00e4tling der Violen Matronal,", "tokens": ["Fiel", "mir", "j\u00fcngst", "ein", "net\u00b7tes", "Sp\u00e4t\u00b7ling", "der", "Vi\u00b7o\u00b7len", "Mat\u00b7ro\u00b7nal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Sp\u00e4t im Herbst, noch im October, unvermuthet ins Gesicht.", "tokens": ["Sp\u00e4t", "im", "Herbst", ",", "noch", "im", "Oc\u00b7to\u00b7ber", ",", "un\u00b7ver\u00b7mu\u00b7thet", "ins", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$,", "ADV", "APPRART", "NN", "$,", "ADJD", "APPRART", "NN", "$."], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Stutzt ich nun den Glanz zu sehen: Stutzt ich wirklich ja so sehr,", "tokens": ["Stutzt", "ich", "nun", "den", "Glanz", "zu", "se\u00b7hen", ":", "Stutzt", "ich", "wirk\u00b7lich", "ja", "so", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "PTKZU", "VVINF", "$.", "VVFIN", "PPER", "ADJD", "ADV", "ADV", "ADV", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Und vergn\u00fcgt, ergetzt, erquickte mich, an selbe fast noch mehr,", "tokens": ["Und", "ver\u00b7gn\u00fcgt", ",", "er\u00b7getzt", ",", "er\u00b7quick\u00b7te", "mich", ",", "an", "sel\u00b7be", "fast", "noch", "mehr", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "$,", "VVPP", "$,", "VVFIN", "PPER", "$,", "APPR", "ADJA", "ADV", "ADV", "ADV", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Durch den Balsam des Geruchs, den ich wunderstark versp\u00fcrte,", "tokens": ["Durch", "den", "Bal\u00b7sam", "des", "Ge\u00b7ruchs", ",", "den", "ich", "wun\u00b7der\u00b7stark", "ver\u00b7sp\u00fcr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Da er mir, nicht nur die Nase, wirklich Hirn und Seele, r\u00fchrte,", "tokens": ["Da", "er", "mir", ",", "nicht", "nur", "die", "Na\u00b7se", ",", "wirk\u00b7lich", "Hirn", "und", "See\u00b7le", ",", "r\u00fchr\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "$,", "PTKNEG", "ADV", "ART", "NN", "$,", "ADJD", "NN", "KON", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.9": {"text": "So da\u00df ich, vor Gott, im Geiste, ehrerbietig niedersank,", "tokens": ["So", "da\u00df", "ich", ",", "vor", "Gott", ",", "im", "Geis\u00b7te", ",", "ehr\u00b7er\u00b7bie\u00b7tig", "nie\u00b7der\u00b7sank", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "$,", "APPR", "NN", "$,", "APPRART", "NN", "$,", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Alle Kr\u00e4fte meiner Seelen auf Jhn, als den Sch\u00f6pfer, lenkte,", "tokens": ["Al\u00b7le", "Kr\u00e4f\u00b7te", "mei\u00b7ner", "See\u00b7len", "auf", "Jhn", ",", "als", "den", "Sch\u00f6p\u00b7fer", ",", "lenk\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "PPOSAT", "NN", "APPR", "PPER", "$,", "KOUS", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+--+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.11": {"text": "Und ein von vergn\u00fcgter Andacht angef\u00fclltes Herz Jhm schenkte,", "tokens": ["Und", "ein", "von", "ver\u00b7gn\u00fcg\u00b7ter", "An\u00b7dacht", "an\u00b7ge\u00b7f\u00fcll\u00b7tes", "Herz", "Jhm", "schenk\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "ADJA", "NN", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.12": {"text": "Nebst dem Ausbruch meiner Lippen: ", "tokens": ["Nebst", "dem", "Aus\u00b7bruch", "mei\u00b7ner", "Lip\u00b7pen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Preis und Dank.", "tokens": ["Preis", "und", "Dank", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}