{"dta.poem.8232": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Bettlerliebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Und hangen stumm an deinem Blick;", "tokens": ["Und", "han\u00b7gen", "stumm", "an", "dei\u00b7nem", "Blick", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du bist so jung, du bist so sch\u00f6n,", "tokens": ["Du", "bist", "so", "jung", ",", "du", "bist", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus deinen Augen lacht das Gl\u00fcck.", "tokens": ["Aus", "dei\u00b7nen", "Au\u00b7gen", "lacht", "das", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und ich so arm, so m\u00fcde schon,", "tokens": ["Und", "ich", "so", "arm", ",", "so", "m\u00fc\u00b7de", "schon", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADJD", "$,", "ADV", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich habe nichts, was dich gewinnt.", "tokens": ["Ich", "ha\u00b7be", "nichts", ",", "was", "dich", "ge\u00b7winnt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O w\u00e4r' ich doch ein K\u00f6nigssohn,", "tokens": ["O", "w\u00e4r'", "ich", "doch", "ein", "K\u00f6\u00b7nigs\u00b7sohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und du ein arm' verlornes Kind!", "tokens": ["Und", "du", "ein", "arm'", "ver\u00b7lor\u00b7nes", "Kind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}