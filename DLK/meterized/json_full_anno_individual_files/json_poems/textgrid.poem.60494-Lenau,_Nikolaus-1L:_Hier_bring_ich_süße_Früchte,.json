{"textgrid.poem.60494": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hier bring ich s\u00fc\u00dfe Fr\u00fcchte,", "genre": "verse", "period": "N.A.", "pub_year": 1832, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier bring ich s\u00fc\u00dfe Fr\u00fcchte,", "tokens": ["Hier", "bring", "ich", "s\u00fc\u00b7\u00dfe", "Fr\u00fcch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die auf gar ferner Au,", "tokens": ["Die", "auf", "gar", "fer\u00b7ner", "Au", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADV", "ADV", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dort unter jenem Himmel", "tokens": ["Dort", "un\u00b7ter", "je\u00b7nem", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gereift, der ewig blau.", "tokens": ["Ge\u00b7reift", ",", "der", "e\u00b7wig", "blau", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADJD", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wenn du sie wirst genie\u00dfen,", "tokens": ["Wenn", "du", "sie", "wirst", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So werden sie dir gern", "tokens": ["So", "wer\u00b7den", "sie", "dir", "gern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Den freien Blick erschlie\u00dfen", "tokens": ["Den", "frei\u00b7en", "Blick", "er\u00b7schlie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In weite L\u00e4nderfern.", "tokens": ["In", "wei\u00b7te", "L\u00e4n\u00b7der\u00b7fern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du denke dir die B\u00e4ume,", "tokens": ["Du", "den\u00b7ke", "dir", "die", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die sie erzogen gro\u00df,", "tokens": ["Die", "sie", "er\u00b7zo\u00b7gen", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das saftig-dunkelgr\u00fcne", "tokens": ["Das", "saf\u00b7tig\u00b7dun\u00b7kel\u00b7gr\u00fc\u00b7ne"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Laubwerk, das sie umschlo\u00df,", "tokens": ["Laub\u00b7werk", ",", "das", "sie", "um\u00b7schlo\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Wie sie wohl mochten winken", "tokens": ["Wie", "sie", "wohl", "moch\u00b7ten", "win\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hell aus der Bl\u00e4tternacht,", "tokens": ["Hell", "aus", "der", "Bl\u00e4t\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Wie Edelsteine blinken", "tokens": ["Wie", "E\u00b7del\u00b7stei\u00b7ne", "blin\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Aus dunklem Bergesschacht.", "tokens": ["Aus", "dunk\u00b7lem", "Ber\u00b7ges\u00b7schacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Du denk dir die Olive,", "tokens": ["Du", "denk", "dir", "die", "O\u00b7li\u00b7ve", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wie sie ihr Gr\u00fcn, so licht,", "tokens": ["Wie", "sie", "ihr", "Gr\u00fcn", ",", "so", "licht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit der Zypresse Dunkel", "tokens": ["Mit", "der", "Zyp\u00b7res\u00b7se", "Dun\u00b7kel"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Zu buntem Kranze flicht.", "tokens": ["Zu", "bun\u00b7tem", "Kran\u00b7ze", "flicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du denke dir die Pinien,", "tokens": ["Du", "den\u00b7ke", "dir", "die", "Pi\u00b7ni\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Gewaltig, breit und dicht,", "tokens": ["Ge\u00b7wal\u00b7tig", ",", "breit", "und", "dicht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Pappeln schlanke Linien", "tokens": ["Der", "Pap\u00b7peln", "schlan\u00b7ke", "Li\u00b7ni\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zum Himmel aufgericht.", "tokens": ["Zum", "Him\u00b7mel", "auf\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Rebe, die die St\u00e4mme", "tokens": ["Die", "Re\u00b7be", ",", "die", "die", "St\u00e4m\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit s\u00fc\u00dfem Netz umringt,", "tokens": ["Mit", "s\u00fc\u00b7\u00dfem", "Netz", "um\u00b7ringt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die leicht von Baum zu Baume", "tokens": ["Die", "leicht", "von", "Baum", "zu", "Bau\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPR", "NE", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Liebesketten schlingt.", "tokens": ["Die", "Lie\u00b7bes\u00b7ket\u00b7ten", "schlingt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Denk dir die Rosen gl\u00fchend", "tokens": ["Denk", "dir", "die", "Ro\u00b7sen", "gl\u00fc\u00b7hend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "NN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Im sch\u00f6nsten Purpurschein", "tokens": ["Im", "sch\u00f6ns\u00b7ten", "Pur\u00b7pur\u00b7schein"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und s\u00fc\u00dfe D\u00fcfte spr\u00fchend", "tokens": ["Und", "s\u00fc\u00b7\u00dfe", "D\u00fcf\u00b7te", "spr\u00fc\u00b7hend"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Durch n\u00e4chtlich dunkeln Hain.", "tokens": ["Durch", "n\u00e4cht\u00b7lich", "dun\u00b7keln", "Hain", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Denk dir die Pracht des Kaktus,", "tokens": ["Denk", "dir", "die", "Pracht", "des", "Kak\u00b7tus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die bl\u00fchnde Aloe", "tokens": ["Die", "bl\u00fchn\u00b7de", "A\u00b7loe"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+---", "measure": "dactylic.init"}, "line.3": {"text": "Und dr\u00fcber hin die Palme,", "tokens": ["Und", "dr\u00fc\u00b7ber", "hin", "die", "Pal\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Strebend hinauf zur H\u00f6h!", "tokens": ["Stre\u00b7bend", "hin\u00b7auf", "zur", "H\u00f6h", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPRART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Sieh, Schmetterlinge fliegen", "tokens": ["Sieh", ",", "Schmet\u00b7ter\u00b7lin\u00b7ge", "flie\u00b7gen"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Durch all die Blumen hin", "tokens": ["Durch", "all", "die", "Blu\u00b7men", "hin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ART", "NN", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Eidechsen, die sich wiegen", "tokens": ["Ei\u00b7dech\u00b7sen", ",", "die", "sich", "wie\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "PRF", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Auf Rosen, goldengr\u00fcn.", "tokens": ["Auf", "Ro\u00b7sen", ",", "gol\u00b7den\u00b7gr\u00fcn", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Denk dir durch dieses alles", "tokens": ["Denk", "dir", "durch", "die\u00b7ses", "al\u00b7les"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "PDAT", "PIS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der L\u00fcfte leisen Tanz", "tokens": ["Der", "L\u00fcf\u00b7te", "lei\u00b7sen", "Tanz"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und \u00fcber diesem allem", "tokens": ["Und", "\u00fc\u00b7ber", "die\u00b7sem", "al\u00b7lem"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PDAT", "PIS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des Mondes Zauberglanz,", "tokens": ["Des", "Mon\u00b7des", "Zau\u00b7ber\u00b7glanz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der wandelnd still und milde", "tokens": ["Der", "wan\u00b7delnd", "still", "und", "mil\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "PTKVZ", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Im \u00c4ther, wolkenlos,", "tokens": ["Im", "\u00c4\u00b7ther", ",", "wol\u00b7ken\u00b7los", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sich unten schau im Bilde", "tokens": ["Sich", "un\u00b7ten", "schau", "im", "Bil\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "ADV", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Aus blauem Meeresscho\u00df.", "tokens": ["Aus", "blau\u00b7em", "Mee\u00b7res\u00b7scho\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und durch die See hin fahre", "tokens": ["Und", "durch", "die", "See", "hin", "fah\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Nachen, fischervoll,", "tokens": ["Ein", "Na\u00b7chen", ",", "fi\u00b7scher\u00b7voll", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aus dem die Barkarole", "tokens": ["Aus", "dem", "die", "Bar\u00b7ka\u00b7ro\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "NN"], "meter": "-+-+-+-", "measure"