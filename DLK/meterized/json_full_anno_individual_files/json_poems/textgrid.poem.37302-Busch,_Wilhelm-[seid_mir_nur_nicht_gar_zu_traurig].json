{"textgrid.poem.37302": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[seid mir nur nicht gar zu traurig]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seid mir nur nicht gar zu traurig,", "tokens": ["Seid", "mir", "nur", "nicht", "gar", "zu", "trau\u00b7rig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "ADV", "PTKNEG", "ADV", "PTKA", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df die sch\u00f6ne Zeit entflieht,", "tokens": ["Da\u00df", "die", "sch\u00f6\u00b7ne", "Zeit", "ent\u00b7flieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df die Welle k\u00fchl und schaurig", "tokens": ["Da\u00df", "die", "Wel\u00b7le", "k\u00fchl", "und", "schau\u00b7rig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Uns in ihre Wirbel zieht;", "tokens": ["Uns", "in", "ih\u00b7re", "Wir\u00b7bel", "zieht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Da\u00df des Herzens s\u00fc\u00dfe Regung,", "tokens": ["Da\u00df", "des", "Her\u00b7zens", "s\u00fc\u00b7\u00dfe", "Re\u00b7gung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df der Liebe Hochgenu\u00df,", "tokens": ["Da\u00df", "der", "Lie\u