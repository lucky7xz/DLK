{"dta.poem.11757": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "43.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Am Rand des Stromes sitzt ein Angler um zu angeln,", "tokens": ["Am", "Rand", "des", "Stro\u00b7mes", "sitzt", "ein", "Ang\u00b7ler", "um", "zu", "an\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "ART", "NN", "APPR", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und l\u00e4\u00dfts an keiner Kunst, den Fisch zu locken, mangeln.", "tokens": ["Und", "l\u00e4\u00dfts", "an", "kei\u00b7ner", "Kunst", ",", "den", "Fisch", "zu", "lo\u00b7cken", ",", "man\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Lockung l\u00e4sset er am feinsten Faden schweben,", "tokens": ["Die", "Lo\u00b7ckung", "l\u00e4s\u00b7set", "er", "am", "feins\u00b7ten", "Fa\u00b7den", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Ruth' ist stark genug den schwersten Fang zu heben.", "tokens": ["Die", "Ruth'", "ist", "stark", "ge\u00b7nug", "den", "schwers\u00b7ten", "Fang", "zu", "he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "ADV", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch munter spielt der Fisch in seinem Element,", "tokens": ["Doch", "mun\u00b7ter", "spielt", "der", "Fisch", "in", "sei\u00b7nem", "E\u00b7le\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und achtets seinen Tod, wenn man davon ihn trennt.", "tokens": ["Und", "ach\u00b7tets", "sei\u00b7nen", "Tod", ",", "wenn", "man", "da\u00b7von", "ihn", "trennt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "$,", "KOUS", "PIS", "PAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "So \u00fcberm Sinnenmeer, in das versenkt wir sind,", "tokens": ["So", "\u00fc\u00b7berm", "Sin\u00b7nen\u00b7meer", ",", "in", "das", "ver\u00b7senkt", "wir", "sind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "APPR", "PDS", "VVFIN", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sitzt dort ein Angler auch und lockt das Menschenkind.", "tokens": ["Sitzt", "dort", "ein", "Ang\u00b7ler", "auch", "und", "lockt", "das", "Men\u00b7schen\u00b7kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADV", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Der Angel Nektar schwebt an goldnem Sonnenfaden,", "tokens": ["Der", "An\u00b7gel", "Nek\u00b7tar", "schwebt", "an", "gold\u00b7nem", "Son\u00b7nen\u00b7fa\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Uns aus der bittern Flut zur s\u00fc\u00dfen Kost zu laden.", "tokens": ["Uns", "aus", "der", "bit\u00b7tern", "Flut", "zur", "s\u00fc\u00b7\u00dfen", "Kost", "zu", "la\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Doch wollen sie nicht recht der Himmelsladung achten,", "tokens": ["Doch", "wol\u00b7len", "sie", "nicht", "recht", "der", "Him\u00b7mels\u00b7la\u00b7dung", "ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie f\u00fcrchten wie der Fisch im Aether zu verschmachten.", "tokens": ["Sie", "f\u00fcrch\u00b7ten", "wie", "der", "Fisch", "im", "A\u00b7e\u00b7ther", "zu", "ver\u00b7schmach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Doch jeder ist zuletzt gefangen unwillk\u00fchrlich;", "tokens": ["Doch", "je\u00b7der", "ist", "zu\u00b7letzt", "ge\u00b7fan\u00b7gen", "un\u00b7will\u00b7k\u00fchr\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ADV", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Komm, stirb der Welt im Geist, eh du ihr stirbst nat\u00fcrlich!", "tokens": ["Komm", ",", "stirb", "der", "Welt", "im", "Geist", ",", "eh", "du", "ihr", "stirbst", "na\u00b7t\u00fcr\u00b7lich", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "ART", "NN", "APPRART", "NN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Der Mensch, solang er lebt, ist meist ein Doppelleber,", "tokens": ["Der", "Mensch", ",", "so\u00b7lang", "er", "lebt", ",", "ist", "meist", "ein", "Dop\u00b7pel\u00b7le\u00b7ber", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur wen'ge sind ganz Fisch, noch wen'ger Himmelschweber.", "tokens": ["Nur", "wen'\u00b7ge", "sind", "ganz", "Fisch", ",", "noch", "wen'\u00b7ger", "Him\u00b7mel\u00b7schwe\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VAFIN", "ADV", "ADJD", "$,", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}