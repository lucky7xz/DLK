{"textgrid.poem.53392": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Christliches Trost-Liedchen", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Himmel und auff Erden", "tokens": ["Im", "Him\u00b7mel", "und", "auff", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dem gossen Welt-Geb\u00e4w", "tokens": ["Dem", "gos\u00b7sen", "Welt\u00b7Ge\u00b7b\u00e4w"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kan nichts gefunden werden", "tokens": ["Kan", "nichts", "ge\u00b7fun\u00b7den", "wer\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "VVPP", "VAINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das nicht des Herren sey:", "tokens": ["Das", "nicht", "des", "Her\u00b7ren", "sey", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was unser Auge schaut,", "tokens": ["Was", "un\u00b7ser", "Au\u00b7ge", "schaut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Vns kan zu Hertzen dringen", "tokens": ["Vns", "kan", "zu", "Hert\u00b7zen", "drin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Den Geist von allen Dingen", "tokens": ["Den", "Geist", "von", "al\u00b7len", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Hat er aus nichts gebawt.", "tokens": ["Hat", "er", "aus", "nichts", "ge\u00b7bawt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PIS", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Als ich bin nichts gewesen,", "tokens": ["Als", "ich", "bin", "nichts", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "PIS", "VAPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wer hat mich da bereit", "tokens": ["Wer", "hat", "mich", "da", "be\u00b7reit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aus seinem Buch gelesen", "tokens": ["Aus", "sei\u00b7nem", "Buch", "ge\u00b7le\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vnd meine gantze Zeit?", "tokens": ["Vnd", "mei\u00b7ne", "gant\u00b7ze", "Zeit", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vnd da ich Anfang nam", "tokens": ["Vnd", "da", "ich", "An\u00b7fang", "nam"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Tieff in der Erden Schatten,", "tokens": ["Tieff", "in", "der", "Er\u00b7den", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wer war der mir zu statten", "tokens": ["Wer", "war", "der", "mir", "zu", "stat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In meiner Bildung kam?", "tokens": ["In", "mei\u00b7ner", "Bil\u00b7dung", "kam", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wer zog mich damals eben", "tokens": ["Wer", "zog", "mich", "da\u00b7mals", "e\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit Haut und Adern an?", "tokens": ["Mit", "Haut", "und", "A\u00b7dern", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer go\u00df in mich das Leben,", "tokens": ["Wer", "go\u00df", "in", "mich", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hab' ich es selbst gethan?", "tokens": ["Hab'", "ich", "es", "selbst", "ge\u00b7than", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wer zog mich aus dem Koht", "tokens": ["Wer", "zog", "mich", "aus", "dem", "Koht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Da\u00df ich zu sehn begonnen", "tokens": ["Da\u00df", "ich", "zu", "sehn", "be\u00b7gon\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKZU", "VVINF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Das Licht der rohten Sonnen?", "tokens": ["Das", "Licht", "der", "roh\u00b7ten", "Son\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Wunderbare Gott.", "tokens": ["Der", "Wun\u00b7der\u00b7ba\u00b7re", "Gott", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er hat mich auffgenommen", "tokens": ["Er", "hat", "mich", "auff\u00b7ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch trewe Pfleg' und Hut", "tokens": ["Durch", "tre\u00b7we", "Pfleg'", "und", "Hut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Zu Kr\u00e4fften lassen kommen", "tokens": ["Zu", "Kr\u00e4ff\u00b7ten", "las\u00b7sen", "kom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "VVINF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mehr als ein Vater thut.", "tokens": ["Mehr", "als", "ein", "Va\u00b7ter", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er macht noch t\u00e4glich sat", "tokens": ["Er", "macht", "noch", "t\u00e4g\u00b7lich", "sat"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Vnd f\u00fcllt mit Wolgefallen", "tokens": ["Vnd", "f\u00fcllt", "mit", "Wol\u00b7ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Gantz reichlich mich sampt allen", "tokens": ["Gantz", "reich\u00b7lich", "mich", "sampt", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "VVFIN", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vnd was sonst Athem hat.", "tokens": ["Vnd", "was", "sonst", "A\u00b7them", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Was hie ein Mensch kan haben", "tokens": ["Was", "hie", "ein", "Mensch", "kan", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ART", "NN", "VMFIN", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "K\u00f6mpt blo\u00df aus seiner Hand,", "tokens": ["K\u00f6mpt", "blo\u00df", "aus", "sei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es sind nur seine Gaben", "tokens": ["Es", "sind", "nur", "sei\u00b7ne", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Weib, Kinder, Gut und Standt,", "tokens": ["Weib", ",", "Kin\u00b7der", ",", "Gut", "und", "Standt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADJD", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die thut er uns zwar ein", "tokens": ["Die", "thut", "er", "uns", "zwar", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "PRF", "ADV", "ART"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Da\u00df wir sie brauchen sollen,", "tokens": ["Da\u00df", "wir", "sie", "brau\u00b7chen", "sol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Nicht aber wie wir wollen,", "tokens": ["Nicht", "a\u00b7ber", "wie", "wir", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "KOKOM", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das Recht dazu ist sein.", "tokens": ["Das", "Recht", "da\u00b7zu", "ist", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "VAFIN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Legt er die meinen nieder", "tokens": ["Legt", "er", "die", "mei\u00b7nen", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "VVFIN", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch einen fr\u00fchen Tod,", "tokens": ["Durch", "ei\u00b7nen", "fr\u00fc\u00b7hen", "Tod", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er nimmt das seine wieder;", "tokens": ["Er", "nimmt", "das", "sei\u00b7ne", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "PPOSAT", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was schrey ich \u00fcber Noht", "tokens": ["Was", "schrey", "ich", "\u00fc\u00b7ber", "Noht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vnd komm' umb alle Ruh?", "tokens": ["Vnd", "komm'", "umb", "al\u00b7le", "Ruh", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Es steht in seinen m\u00e4chten,", "tokens": ["Es", "steht", "in", "sei\u00b7nen", "m\u00e4ch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wer wolte mit ihm rechten", "tokens": ["Wer", "wol\u00b7te", "mit", "ihm", "rech\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "APPR", "PPER", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vnd fragen was er thu?", "tokens": ["Vnd", "fra\u00b7gen", "was", "er", "thu", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ihr die Ihr Gott bekennet,", "tokens": ["Ihr", "die", "Ihr", "Gott", "be\u00b7ken\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Seyd Christen in der That", "tokens": ["Seyd", "Chris\u00b7ten", "in", "der", "That"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAIMP", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und nicht allein genennet,", "tokens": ["Und", "nicht", "al\u00b7lein", "ge\u00b7nen\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Rh\u00fcmt seinen weisen Raht,", "tokens": ["Rh\u00fcmt", "sei\u00b7nen", "wei\u00b7sen", "Raht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vnd sprechet in gemein:", "tokens": ["Vnd", "spre\u00b7chet", "in", "ge\u00b7mein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Er kan in jenem Leben", "tokens": ["Er", "kan", "in", "je\u00b7nem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "(vertraut nur seiner Macht)", "tokens": ["(", "ver\u00b7traut", "nur", "sei\u00b7ner", "Macht", ")"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd wird auch wieder geben", "tokens": ["Vnd", "wird", "auch", "wie\u00b7der", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die\u00df was er weggebracht", "tokens": ["Die\u00df", "was", "er", "weg\u00b7ge\u00b7bracht"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vollkommen, herrlich sch\u00f6n.", "tokens": ["Voll\u00b7kom\u00b7men", ",", "herr\u00b7lich", "sch\u00f6n", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Was hie in Angst und Leiden", "tokens": ["Was", "hie", "in", "Angst", "und", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Geseet wird, wird in Frewden", "tokens": ["Ge\u00b7seet", "wird", ",", "wird", "in", "Frew\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "$,", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "An jenem Tag' erstehn.", "tokens": ["An", "je\u00b7nem", "Tag'", "er\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "O Gott bild unsern Sinnen", "tokens": ["O", "Gott", "bild", "un\u00b7sern", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NN", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die selig' Hoffnung ein,", "tokens": ["Die", "se\u00b7lig'", "Hoff\u00b7nung", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Vnd la\u00df uns Trost gewinnen", "tokens": ["Vnd", "la\u00df", "uns", "Trost", "ge\u00b7win\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In aller unser Pein.", "tokens": ["In", "al\u00b7ler", "un\u00b7ser", "Pein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Insonderheit wenn wir", "tokens": ["In\u00b7son\u00b7der\u00b7heit", "wenn", "wir"], "token_info": ["word", "word", "word"], "pos": ["ADV", "KOUS", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Selbst mit dem Tode streiten,", "tokens": ["Selbst", "mit", "dem", "To\u00b7de", "strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "So steh auff unsrer Seiten", "tokens": ["So", "steh", "auff", "uns\u00b7rer", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vnd nimm uns auff zu dir.", "tokens": ["Vnd", "nimm", "uns", "auff", "zu", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "APPR", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Im Himmel und auff Erden", "tokens": ["Im", "Him\u00b7mel", "und", "auff", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dem gossen Welt-Geb\u00e4w", "tokens": ["Dem", "gos\u00b7sen", "Welt\u00b7Ge\u00b7b\u00e4w"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kan nichts gefunden werden", "tokens": ["Kan", "nichts", "ge\u00b7fun\u00b7den", "wer\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "VVPP", "VAINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das nicht des Herren sey:", "tokens": ["Das", "nicht", "des", "Her\u00b7ren", "sey", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was unser Auge schaut,", "tokens": ["Was", "un\u00b7ser", "Au\u00b7ge", "schaut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Vns kan zu Hertzen dringen", "tokens": ["Vns", "kan", "zu", "Hert\u00b7zen", "drin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Den Geist von allen Dingen", "tokens": ["Den", "Geist", "von", "al\u00b7len", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Hat er aus nichts gebawt.", "tokens": ["Hat", "er", "aus", "nichts", "ge\u00b7bawt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PIS", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Als ich bin nichts gewesen,", "tokens": ["Als", "ich", "bin", "nichts", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "PIS", "VAPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wer hat mich da bereit", "tokens": ["Wer", "hat", "mich", "da", "be\u00b7reit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aus seinem Buch gelesen", "tokens": ["Aus", "sei\u00b7nem", "Buch", "ge\u00b7le\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vnd meine gantze Zeit?", "tokens": ["Vnd", "mei\u00b7ne", "gant\u00b7ze", "Zeit", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vnd da ich Anfang nam", "tokens": ["Vnd", "da", "ich", "An\u00b7fang", "nam"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Tieff in der Erden Schatten,", "tokens": ["Tieff", "in", "der", "Er\u00b7den", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wer war der mir zu statten", "tokens": ["Wer", "war", "der", "mir", "zu", "stat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In meiner Bildung kam?", "tokens": ["In", "mei\u00b7ner", "Bil\u00b7dung", "kam", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Wer zog mich damals eben", "tokens": ["Wer", "zog", "mich", "da\u00b7mals", "e\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit Haut und Adern an?", "tokens": ["Mit", "Haut", "und", "A\u00b7dern", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer go\u00df in mich das Leben,", "tokens": ["Wer", "go\u00df", "in", "mich", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hab' ich es selbst gethan?", "tokens": ["Hab'", "ich", "es", "selbst", "ge\u00b7than", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wer zog mich aus dem Koht", "tokens": ["Wer", "zog", "mich", "aus", "dem", "Koht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Da\u00df ich zu sehn begonnen", "tokens": ["Da\u00df", "ich", "zu", "sehn", "be\u00b7gon\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKZU", "VVINF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Das Licht der rohten Sonnen?", "tokens": ["Das", "Licht", "der", "roh\u00b7ten", "Son\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Wunderbare Gott.", "tokens": ["Der", "Wun\u00b7der\u00b7ba\u00b7re", "Gott", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Er hat mich auffgenommen", "tokens": ["Er", "hat", "mich", "auff\u00b7ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch trewe Pfleg' und Hut", "tokens": ["Durch", "tre\u00b7we", "Pfleg'", "und", "Hut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Zu Kr\u00e4fften lassen kommen", "tokens": ["Zu", "Kr\u00e4ff\u00b7ten", "las\u00b7sen", "kom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "VVINF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mehr als ein Vater thut.", "tokens": ["Mehr", "als", "ein", "Va\u00b7ter", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er macht noch t\u00e4glich sat", "tokens": ["Er", "macht", "noch", "t\u00e4g\u00b7lich", "sat"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Vnd f\u00fcllt mit Wolgefallen", "tokens": ["Vnd", "f\u00fcllt", "mit", "Wol\u00b7ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Gantz reichlich mich sampt allen", "tokens": ["Gantz", "reich\u00b7lich", "mich", "sampt", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "VVFIN", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vnd was sonst Athem hat.", "tokens": ["Vnd", "was", "sonst", "A\u00b7them", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Was hie ein Mensch kan haben", "tokens": ["Was", "hie", "ein", "Mensch", "kan", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ART", "NN", "VMFIN", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "K\u00f6mpt blo\u00df aus seiner Hand,", "tokens": ["K\u00f6mpt", "blo\u00df", "aus", "sei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es sind nur seine Gaben", "tokens": ["Es", "sind", "nur", "sei\u00b7ne", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Weib, Kinder, Gut und Standt,", "tokens": ["Weib", ",", "Kin\u00b7der", ",", "Gut", "und", "Standt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADJD", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die thut er uns zwar ein", "tokens": ["Die", "thut", "er", "uns", "zwar", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "PRF", "ADV", "ART"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Da\u00df wir sie brauchen sollen,", "tokens": ["Da\u00df", "wir", "sie", "brau\u00b7chen", "sol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Nicht aber wie wir wollen,", "tokens": ["Nicht", "a\u00b7ber", "wie", "wir", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "KOKOM", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das Recht dazu ist sein.", "tokens": ["Das", "Recht", "da\u00b7zu", "ist", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "VAFIN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Legt er die meinen nieder", "tokens": ["Legt", "er", "die", "mei\u00b7nen", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "VVFIN", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch einen fr\u00fchen Tod,", "tokens": ["Durch", "ei\u00b7nen", "fr\u00fc\u00b7hen", "Tod", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er nimmt das seine wieder;", "tokens": ["Er", "nimmt", "das", "sei\u00b7ne", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "PPOSAT", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was schrey ich \u00fcber Noht", "tokens": ["Was", "schrey", "ich", "\u00fc\u00b7ber", "Noht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vnd komm' umb alle Ruh?", "tokens": ["Vnd", "komm'", "umb", "al\u00b7le", "Ruh", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Es steht in seinen m\u00e4chten,", "tokens": ["Es", "steht", "in", "sei\u00b7nen", "m\u00e4ch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wer wolte mit ihm rechten", "tokens": ["Wer", "wol\u00b7te", "mit", "ihm", "rech\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "APPR", "PPER", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vnd fragen was er thu?", "tokens": ["Vnd", "fra\u00b7gen", "was", "er", "thu", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Ihr die Ihr Gott bekennet,", "tokens": ["Ihr", "die", "Ihr", "Gott", "be\u00b7ken\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Seyd Christen in der That", "tokens": ["Seyd", "Chris\u00b7ten", "in", "der", "That"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAIMP", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und nicht allein genennet,", "tokens": ["Und", "nicht", "al\u00b7lein", "ge\u00b7nen\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Rh\u00fcmt seinen weisen Raht,", "tokens": ["Rh\u00fcmt", "sei\u00b7nen", "wei\u00b7sen", "Raht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vnd sprechet in gemein:", "tokens": ["Vnd", "spre\u00b7chet", "in", "ge\u00b7mein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Er kan in jenem Leben", "tokens": ["Er", "kan", "in", "je\u00b7nem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "(vertraut nur seiner Macht)", "tokens": ["(", "ver\u00b7traut", "nur", "sei\u00b7ner", "Macht", ")"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd wird auch wieder geben", "tokens": ["Vnd", "wird", "auch", "wie\u00b7der", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die\u00df was er weggebracht", "tokens": ["Die\u00df", "was", "er", "weg\u00b7ge\u00b7bracht"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vollkommen, herrlich sch\u00f6n.", "tokens": ["Voll\u00b7kom\u00b7men", ",", "herr\u00b7lich", "sch\u00f6n", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Was hie in Angst und Leiden", "tokens": ["Was", "hie", "in", "Angst", "und", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Geseet wird, wird in Frewden", "tokens": ["Ge\u00b7seet", "wird", ",", "wird", "in", "Frew\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "$,", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "An jenem Tag' erstehn.", "tokens": ["An", "je\u00b7nem", "Tag'", "er\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "O Gott bild unsern Sinnen", "tokens": ["O", "Gott", "bild", "un\u00b7sern", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NN", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die selig' Hoffnung ein,", "tokens": ["Die", "se\u00b7lig'", "Hoff\u00b7nung", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Vnd la\u00df uns Trost gewinnen", "tokens": ["Vnd", "la\u00df", "uns", "Trost", "ge\u00b7win\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In aller unser Pein.", "tokens": ["In", "al\u00b7ler", "un\u00b7ser", "Pein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Insonderheit wenn wir", "tokens": ["In\u00b7son\u00b7der\u00b7heit", "wenn", "wir"], "token_info": ["word", "word", "word"], "pos": ["ADV", "KOUS", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Selbst mit dem Tode streiten,", "tokens": ["Selbst", "mit", "dem", "To\u00b7de", "strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "So steh auff unsrer Seiten", "tokens": ["So", "steh", "auff", "uns\u00b7rer", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vnd nimm uns auff zu dir.", "tokens": ["Vnd", "nimm", "uns", "auff", "zu", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "APPR", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}