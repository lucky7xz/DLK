{"textgrid.poem.45885": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[lieb' und Leid im leichten Leben]", "genre": "verse", "period": "N.A.", "pub_year": 1802, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lieb' und Leid im leichten Leben", "tokens": ["Lieb'", "und", "Leid", "im", "leich\u00b7ten", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich erheben, abw\u00e4rts schweben,", "tokens": ["Sich", "er\u00b7he\u00b7ben", ",", "ab\u00b7w\u00e4rts", "schwe\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "VVINF", "$,", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles will das Herz umfangen", "tokens": ["Al\u00b7les", "will", "das", "Herz", "um\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur verlangen, nie erlangen,", "tokens": ["Nur", "ver\u00b7lan\u00b7gen", ",", "nie", "er\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "In den Spiegel all ihr Bilder", "tokens": ["In", "den", "Spie\u00b7gel", "all", "ihr", "Bil\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PIAT", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blicket milder, blicket wilder", "tokens": ["Bli\u00b7cket", "mil\u00b7der", ",", "bli\u00b7cket", "wil\u00b7der"], "token_info": ["word", "word", "punct", 