{"dta.poem.13934": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Ii .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Welch' Glanz hat mich umflogen", "tokens": ["Welch'", "Glanz", "hat", "mich", "um\u00b7flo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und f\u00fcllt das ganze Haus,", "tokens": ["Und", "f\u00fcllt", "das", "gan\u00b7ze", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als pfeilerten die Bogen", "tokens": ["Als", "pfei\u00b7ler\u00b7ten", "die", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In's Himmelreich hinaus?!", "tokens": ["In's", "Him\u00b7mel\u00b7reich", "hin\u00b7aus", "?!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}