{"dta.poem.13005": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Leander an Floretten, als sie von  \n ihm einen langen brief verlangt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du forderst, Sch\u00f6nes Kind! von mir ein langes schreiben,", "tokens": ["Du", "for\u00b7derst", ",", "Sch\u00f6\u00b7nes", "Kind", "!", "von", "mir", "ein", "lan\u00b7ges", "schrei\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "$.", "APPR", "PPER", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich aber zweiffle sehr, da\u00df ich es lieffern kan.", "tokens": ["Ich", "a\u00b7ber", "zweiff\u00b7le", "sehr", ",", "da\u00df", "ich", "es", "lief\u00b7fern", "kan", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "$,", "KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn wer die poesie so selten pflegt zu treiben,", "tokens": ["Denn", "wer", "die", "po\u00b7e\u00b7sie", "so", "sel\u00b7ten", "pflegt", "zu", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "ADV", "ADJD", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der bringt nach vieler m\u00fch kaum etwas auf die bahn.", "tokens": ["Der", "bringt", "nach", "vie\u00b7ler", "m\u00fch", "kaum", "et\u00b7was", "auf", "die", "bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PIAT", "ADJD", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du hast mir ohne dem die feder l\u00e4ngst verschnitten,", "tokens": ["Du", "hast", "mir", "oh\u00b7ne", "dem", "die", "fe\u00b7der", "l\u00e4ngst", "ver\u00b7schnit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "PRELS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nachdem ich dir nicht mehr von liebe schreiben darff.", "tokens": ["Nach\u00b7dem", "ich", "dir", "nicht", "mehr", "von", "lie\u00b7be", "schrei\u00b7ben", "darff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "ADV", "APPR", "ADJA", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Drum ist Leanders kiel noch wohl bey dir gelidten,", "tokens": ["Drum", "ist", "Le\u00b7an\u00b7ders", "kiel", "noch", "wohl", "bey", "dir", "ge\u00b7lid\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "NE", "VVFIN", "ADV", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "So sey ins k\u00fcnfftige nicht gegen ihn so scharff.", "tokens": ["So", "sey", "ins", "k\u00fcnff\u00b7ti\u00b7ge", "nicht", "ge\u00b7gen", "ihn", "so", "scharff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "ADJA", "PTKNEG", "APPR", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Indessen weil du doch das harte schweigen brechen", "tokens": ["In\u00b7des\u00b7sen", "weil", "du", "doch", "das", "har\u00b7te", "schwei\u00b7gen", "bre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und deinen freund aufs neu mit briefen ehren wilft,", "tokens": ["Und", "dei\u00b7nen", "freund", "aufs", "neu", "mit", "brie\u00b7fen", "eh\u00b7ren", "wilft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPRART", "ADJD", "APPR", "VVINF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So la\u00df die dornen nicht durch deine rosen stechen,", "tokens": ["So", "la\u00df", "die", "dor\u00b7nen", "nicht", "durch", "dei\u00b7ne", "ro\u00b7sen", "ste\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ART", "NN", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Damit du allezeit sonst deine schreiben f\u00fcllst.", "tokens": ["Da\u00b7mit", "du", "al\u00b7le\u00b7zeit", "sonst", "dei\u00b7ne", "schrei\u00b7ben", "f\u00fcllst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "PPOSAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Es darff kein Oedipus dir dieses r\u00e4tzel l\u00f6sen:", "tokens": ["Es", "darff", "kein", "O\u00b7e\u00b7di\u00b7pus", "dir", "die\u00b7ses", "r\u00e4t\u00b7zel", "l\u00f6\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "PPER", "PDAT", "NN", "VVINF", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Dein ungemeiner witz geht tieffer als man denckt.", "tokens": ["Dein", "un\u00b7ge\u00b7mei\u00b7ner", "witz", "geht", "tief\u00b7fer", "als", "man", "denckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADJD", "KOKOM", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Gieb keinem argwohn platz, dieweil er nur durch b\u00f6sen", "tokens": ["Gieb", "kei\u00b7nem", "arg\u00b7wohn", "platz", ",", "die\u00b7weil", "er", "nur", "durch", "b\u00f6\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PIAT", "NN", "PTKVZ", "$,", "KOUS", "PPER", "ADV", "APPR", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und nichtigen verdacht die zarte seele kr\u00e4nckt.", "tokens": ["Und", "nich\u00b7ti\u00b7gen", "ver\u00b7dacht", "die", "zar\u00b7te", "see\u00b7le", "kr\u00e4nckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "La\u00df eher diesen satz in seiner tieffe stecken,", "tokens": ["La\u00df", "e\u00b7her", "die\u00b7sen", "satz", "in", "sei\u00b7ner", "tief\u00b7fe", "ste\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PDAT", "NN", "APPR", "PPOSAT", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wofern er dunckel scheint, und lie\u00df, was kl\u00e4rer ist.", "tokens": ["Wo\u00b7fern", "er", "dun\u00b7ckel", "scheint", ",", "und", "lie\u00df", ",", "was", "kl\u00e4\u00b7rer", "ist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "$,", "KON", "VVFIN", "$,", "PWS", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und darff mein vorwitz sich bi\u00df auf dein thun erstrecken,", "tokens": ["Und", "darff", "mein", "vor\u00b7witz", "sich", "bi\u00df", "auf", "dein", "thun", "er\u00b7stre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPOSAT", "NN", "PRF", "ADV", "APPR", "PPOSAT", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "So sage: was itzund dein kluges auge liest.", "tokens": ["So", "sa\u00b7ge", ":", "was", "it\u00b7zund", "dein", "klu\u00b7ges", "au\u00b7ge", "liest", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "PWS", "ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Was macht die Clelia? spreugt sie noch durch die Tyber?", "tokens": ["Was", "macht", "die", "Cle\u00b7lia", "?", "spreugt", "sie", "noch", "durch", "die", "Ty\u00b7ber", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NE", "$.", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "F\u00e4llt dem Hesiodus kein deutscher dichter ein?", "tokens": ["F\u00e4llt", "dem", "He\u00b7sio\u00b7dus", "kein", "deut\u00b7scher", "dich\u00b7ter", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NE", "PIAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.23": {"text": "Geht das beschlo\u00dfne Rom nicht an Porsennen \u00fcber?", "tokens": ["Geht", "das", "be\u00b7schlo\u00df\u00b7ne", "Rom", "nicht", "an", "Por\u00b7sen\u00b7nen", "\u00fc\u00b7ber", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NE", "PTKNEG", "APPR", "NN", "APPR", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Und wird Aruntius bald in dem hafen seyn?", "tokens": ["Und", "wird", "Ar\u00b7un\u00b7ti\u00b7us", "bald", "in", "dem", "ha\u00b7fen", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NE", "ADV", "APPR", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Will Celer nicht einmal die s\u00fc\u00dfe schwermuth lassen?", "tokens": ["Will", "Ce\u00b7ler", "nicht", "ein\u00b7mal", "die", "s\u00fc\u00b7\u00dfe", "schwer\u00b7muth", "las\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "PTKNEG", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Schilt Berelisens mund noch auf die untreu los?", "tokens": ["Schilt", "Be\u00b7re\u00b7li\u00b7sens", "mund", "noch", "auf", "die", "un\u00b7treu", "los", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "ADV", "APPR", "ART", "ADJD", "PTKVZ", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.27": {"text": "Will denn Andronodor die einsamkeit umfassen?", "tokens": ["Will", "denn", "An\u00b7dro\u00b7no\u00b7dor", "die", "ein\u00b7sam\u00b7keit", "um\u00b7fas\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.28": {"text": "Und macht Herminius nur stille flammen gros?", "tokens": ["Und", "macht", "Her\u00b7mi\u00b7nius", "nur", "stil\u00b7le", "flam\u00b7men", "gros", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "ADV", "ADJA", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.29": {"text": "Was spricht Xenocrates? nennt er die sanfften bande", "tokens": ["Was", "spricht", "Xe\u00b7no\u00b7cra\u00b7tes", "?", "nennt", "er", "die", "sanff\u00b7ten", "ban\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "NN", "$.", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-++-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.30": {"text": "Der liebe, nach wie vor, ein gro\u00dfes ungemach?", "tokens": ["Der", "lie\u00b7be", ",", "nach", "wie", "vor", ",", "ein", "gro\u00b7\u00dfes", "un\u00b7ge\u00b7mach", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "APPR", "KOKOM", "APPR", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wo bleibt der kluge ri\u00df von dem gelobten lande,", "tokens": ["Wo", "bleibt", "der", "klu\u00b7ge", "ri\u00df", "von", "dem", "ge\u00b7lob\u00b7ten", "lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Das den Horatius so in die augen stach?", "tokens": ["Das", "den", "Ho\u00b7ra\u00b7ti\u00b7us", "so", "in", "die", "au\u00b7gen", "stach", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NE", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.33": {"text": "Florette l\u00e4\u00dft du mich nach dieser carte reisen,", "tokens": ["Flo\u00b7ret\u00b7te", "l\u00e4\u00dft", "du", "mich", "nach", "die\u00b7ser", "car\u00b7te", "rei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PRF", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Die Clelia so nett und sinnreich aufgesetzt,", "tokens": ["Die", "Cle\u00b7lia", "so", "nett", "und", "sinn\u00b7reich", "auf\u00b7ge\u00b7setzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADV", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.35": {"text": "So wirst du mir den weg in einen hafen weisen,", "tokens": ["So", "wirst", "du", "mir", "den", "weg", "in", "ei\u00b7nen", "ha\u00b7fen", "wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ART", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Da mich nach tieffer angst die h\u00f6chste lust ergetzt.", "tokens": ["Da", "mich", "nach", "tief\u00b7fer", "angst", "die", "h\u00f6chs\u00b7te", "lust", "er\u00b7getzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Mi\u00dff\u00e4llt dir dieser reim; so sey er weggestrichen.", "tokens": ["Mi\u00df\u00b7f\u00e4llt", "dir", "die\u00b7ser", "reim", ";", "so", "sey", "er", "weg\u00b7ge\u00b7stri\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "NN", "$.", "ADV", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Ich suche keine lust, die dir nicht auch beliebt.", "tokens": ["Ich", "su\u00b7che", "kei\u00b7ne", "lust", ",", "die", "dir", "nicht", "auch", "be\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "PRELS", "PPER", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Drum wend\u2019 ich mich dahin, wovon ich abgewichen,", "tokens": ["Drum", "wend'", "ich", "mich", "da\u00b7hin", ",", "wo\u00b7von", "ich", "ab\u00b7ge\u00b7wi\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "PAV", "$,", "PWAV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und was dir, Werthes Licht! so viel vergn\u00fcgen giebt.", "tokens": ["Und", "was", "dir", ",", "Wert\u00b7hes", "Licht", "!", "so", "viel", "ver\u00b7gn\u00fc\u00b7gen", "giebt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "$,", "ADJA", "NN", "$.", "ADV", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Spielt Dison noch den mann? liebt Cimber noch verholen?", "tokens": ["Spielt", "Di\u00b7son", "noch", "den", "mann", "?", "liebt", "Cim\u00b7ber", "noch", "ver\