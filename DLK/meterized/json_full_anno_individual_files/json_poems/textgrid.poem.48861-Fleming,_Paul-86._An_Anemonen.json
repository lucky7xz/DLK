{"textgrid.poem.48861": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "86. An Anemonen", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich meint', ich h\u00e4tte dir mein ganzes Herz entdeckt,", "tokens": ["Ich", "meint'", ",", "ich", "h\u00e4t\u00b7te", "dir", "mein", "gan\u00b7zes", "Herz", "ent\u00b7deckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "PPER", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "mein Lassen und mein Tun, mein Wollen und Beginnen,", "tokens": ["mein", "Las\u00b7sen", "und", "mein", "Tun", ",", "mein", "Wol\u00b7len", "und", "Be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "so da\u00df ich mich mir selbst nicht besser \u00f6ffnen k\u00f6nnen.", "tokens": ["so", "da\u00df", "ich", "mich", "mir", "selbst", "nicht", "bes\u00b7ser", "\u00f6ff\u00b7nen", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PRF", "PPER", "ADV", "PTKNEG", "ADJD", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich war nun nicht in mir; ich war in dich versteckt.", "tokens": ["Ich", "war", "nun", "nicht", "in", "mir", ";", "ich", "war", "in", "dich", "ver\u00b7steckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "APPR", "PPER", "$.", "PPER", "VAFIN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was hat denn diesen Ha\u00df so bald auf mich erweckt,", "tokens": ["Was", "hat", "denn", "die\u00b7sen", "Ha\u00df", "so", "bald", "auf", "mich", "er\u00b7weckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "PDAT", "NN", "ADV", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "da\u00df du mir itzund auch ein Auge nicht wilst g\u00f6nnen?", "tokens": ["da\u00df", "du", "mir", "it\u00b7zund", "auch", "ein", "Au\u00b7ge", "nicht", "wilst", "g\u00f6n\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADV", "ART", "NN", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Besinne dich doch, Lieb, wo du was kanst besinnen,", "tokens": ["Be\u00b7sin\u00b7ne", "dich", "doch", ",", "Lieb", ",", "wo", "du", "was", "kanst", "be\u00b7sin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "NN", "$,", "PWAV", "PPER", "PIS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "wie hoch mich dieses schmerzt, wie sehr mich di\u00df erschreckt.", "tokens": ["wie", "hoch", "mich", "die\u00b7ses", "schmerzt", ",", "wie", "sehr", "mich", "di\u00df", "er\u00b7schreckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "PDS", "VVFIN", "$,", "PWAV", "ADV", "PPER", "PDS", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Gedenke doch an dich, wilst du an mich nicht denken.", "tokens": ["Ge\u00b7den\u00b7ke", "doch", "an", "dich", ",", "wilst", "du", "an", "mich", "nicht", "den\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPER", "$,", "VMFIN", "PPER", "APPR", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sei mir Feind und nicht dir, dieweil es Zeit ist noch.", "tokens": ["Sei", "mir", "Feind", "und", "nicht", "dir", ",", "die\u00b7weil", "es", "Zeit", "ist", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "KON", "PTKNEG", "PPER", "$,", "KOUS", "PPER", "NN", "VAFIN", "ADV", "$."], "meter": "+-+----+-+-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Wilst du mich richten hin, so schone deiner doch,", "tokens": ["Wilst", "du", "mich", "rich\u00b7ten", "hin", ",", "so", "scho\u00b7ne", "dei\u00b7ner", "doch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVFIN", "PTKVZ", "$,", "ADV", "VVFIN", "PPOSAT", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "als die um meinen Tod zu Tode sich wird kr\u00e4nken.", "tokens": ["als", "die", "um", "mei\u00b7nen", "Tod", "zu", "To\u00b7de", "sich", "wird", "kr\u00e4n\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "APPR", "PPOSAT", "NN", "APPR", "NN", "PRF", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2