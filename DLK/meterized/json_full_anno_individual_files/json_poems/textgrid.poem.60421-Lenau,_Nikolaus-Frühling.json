n hinnen zieht.", "tokens": ["Und", "bang", "das", "Herz", "von", "hin\u00b7nen", "zieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Ich wandle irr, dem Himmel nach,", "tokens": ["Ich", "wand\u00b7le", "irr", ",", "dem", "Him\u00b7mel", "nach", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Der rauschend auf mich niederbrach;", "tokens": ["Der", "rau\u00b7schend", "auf", "mich", "nie\u00b7der\u00b7brach", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "O Fr\u00fchling! trunken bin ich dein!", "tokens": ["O", "Fr\u00fch\u00b7ling", "!", "trun\u00b7ken", "bin", "ich", "dein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "ADJD", "VAFIN", "PPER", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "O Fr\u00fchling! ewig bist du mein!", "tokens": ["O", "Fr\u00fch\u00b7ling", "!", "e\u00b7wig", "bist", "du", "mein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "ADJD", "VAFIN", "PPER", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}