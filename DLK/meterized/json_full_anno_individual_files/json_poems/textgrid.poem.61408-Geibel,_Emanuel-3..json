{"textgrid.poem.61408": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcber dem schlummernden Kind, dem erg\u00f6tzlichen Spiele des Knaben", "tokens": ["\u00dc\u00b7ber", "dem", "schlum\u00b7mern\u00b7den", "Kind", ",", "dem", "er\u00b7g\u00f6tz\u00b7li\u00b7chen", "Spie\u00b7le", "des", "Kna\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "H\u00e4lt mit l\u00e4chelnder Stirn schirmend ein Genius Wacht;", "tokens": ["H\u00e4lt", "mit", "l\u00e4\u00b7cheln\u00b7der", "Stirn", "schir\u00b7mend", "ein", "Ge\u00b7nius", "Wacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "ADJD", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Liebreich g\u00f6nnet dem redlichen Sinn, dem beschr\u00e4nkten, der Zufall,", "tokens": ["Lieb\u00b7reich", "g\u00f6n\u00b7net", "dem", "red\u00b7li\u00b7chen", "Sinn", ",", "dem", "be\u00b7schr\u00e4nk\u00b7ten", ",", "der", "Zu\u00b7fall", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "$,", "ART", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Was er bedarf, und im Spiel ebnet er gern ihm die Bahn.", "tokens": ["Was", "er", "be\u00b7darf", ",", "und", "im", "Spiel", "eb\u00b7net", "er", "gern", "ihm", "die", "Bahn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "KON", "APPRART", "NN", "VVFIN", "PPER", "ADV", "PPER", "ART", "NN", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Doch nur selten erscheint aus den Wolken ein Helfer dem Gro\u00dfen;", "tokens": ["Doch", "nur", "sel\u00b7ten", "er\u00b7scheint", "aus", "den", "Wol\u00b7ken", "ein", "Hel\u00b7fer", "dem", "Gro\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.6": {"text": "Denn ihm gab die Natur selber ein Auge zu schaun,", "tokens": ["Denn", "ihm", "gab", "die", "Na\u00b7tur", "sel\u00b7ber", "ein", "Au\u00b7ge", "zu", "schaun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ADV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Schuf ihm Fl\u00fcgel, die Welt zu beherrschen, und senkt' ihm der Ahnung", "tokens": ["Schuf", "ihm", "Fl\u00fc\u00b7gel", ",", "die", "Welt", "zu", "be\u00b7herr\u00b7schen", ",", "und", "senkt'", "ihm", "der", "Ah\u00b7nung"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,", "KON", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "G\u00f6ttliche Kraft in die Brust, da\u00df sie ein Steuer ihm sei.", "tokens": ["G\u00f6tt\u00b7li\u00b7che", "Kraft", "in", "die", "Brust", ",", "da\u00df", "sie", "ein", "Steu\u00b7er", "ihm", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "PPER", "VAFIN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.9": {"text": "Wohl ihm, ehrt er das hohe Geschenk! Doch tr\u00fcbt er es frevelnd:", "tokens": ["Wohl", "ihm", ",", "ehrt", "er", "das", "ho\u00b7he", "Ge\u00b7schenk", "!", "Doch", "tr\u00fcbt", "er", "es", "fre\u00b7velnd", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "VVFIN", "PPER", "ART", "ADJA", "NN", "$.", "KON", "VVFIN", "PPER", "PPER", "VVPP", "$."], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.10": {"text": "Leicht, ein erblindeter Aar, schwankt er hinab ins Gekl\u00fcft.", "tokens": ["Leicht", ",", "ein", "er\u00b7blin\u00b7de\u00b7ter", "Aar", ",", "schwankt", "er", "hin\u00b7ab", "ins", "Ge\u00b7kl\u00fcft", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ART", "ADJA", "NN", "$,", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Ach, drum sehn wir so oft vom Sturm die Heroen verschlagen", "tokens": ["Ach", ",", "drum", "sehn", "wir", "so", "oft", "vom", "Sturm", "die", "He\u00b7roen", "ver\u00b7schla\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PAV", "VVFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "ART", "NN", "VVINF"], "meter": "--+--+-+--+-+-", "measure": "anapaest.di.plus"}, "line.12": {"text": "Und das gefeierte Haupt schwer von den Blitzen versengt.", "tokens": ["Und", "das", "ge\u00b7fei\u00b7er\u00b7te", "Haupt", "schwer", "von", "den", "Blit\u00b7zen", "ver\u00b7sengt", "."],