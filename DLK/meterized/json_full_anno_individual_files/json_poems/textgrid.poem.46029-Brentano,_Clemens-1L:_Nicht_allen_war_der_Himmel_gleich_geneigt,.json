{"textgrid.poem.46029": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nicht allen war der Himmel gleich geneigt,", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht allen war der Himmel gleich geneigt,", "tokens": ["Nicht", "al\u00b7len", "war", "der", "Him\u00b7mel", "gleich", "ge\u00b7neigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIS", "VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und jeglichem ist andre Pflicht gegeben", "tokens": ["Und", "jeg\u00b7li\u00b7chem", "ist", "and\u00b7re", "Pflicht", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VAFIN", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie mancher betet an, indes die Lippe schweigt,", "tokens": ["Wie", "man\u00b7cher", "be\u00b7tet", "an", ",", "in\u00b7des", "die", "Lip\u00b7pe", "schweigt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "PTKVZ", "$,", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der andere darf nur die Blicke heben,", "tokens": ["Der", "an\u00b7de\u00b7re", "darf", "nur", "die", "Bli\u00b7cke", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der K\u00f6nig Gold der Weise Mirrhen reicht", "tokens": ["Der", "K\u00f6\u00b7nig", "Gold", "der", "Wei\u00b7se", "Mirr\u00b7hen", "reicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und Weihrauchwolken l\u00e4\u00dft der Melchior schweben,", "tokens": ["Und", "Weih\u00b7rauch\u00b7wol\u00b7ken", "l\u00e4\u00dft", "der", "Melc\u00b7hi\u00b7or", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Der Kinder Lallen und der Liebe Stammeln,", "tokens": ["Der", "Kin\u00b7der", "Lal\u00b7len", "und", "der", "Lie\u00b7be", "Stam\u00b7meln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Des S\u00e4ngers Lied mu\u00df sich zum Dienste sammlen.", "tokens": ["Des", "S\u00e4n\u00b7gers", "Lied", "mu\u00df", "sich", "zum", "Diens\u00b7te", "samm\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "PRF", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es hat der Herr sich eine Welt erbaut,", "tokens": ["Es", "hat", "der", "Herr", "sich", "ei\u00b7ne", "Welt", "er\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er hat sie mit der Sch\u00f6nheit ausgeschm\u00fccket,", "tokens": ["Er", "hat", "sie", "mit", "der", "Sch\u00f6n\u00b7heit", "aus\u00b7ge\u00b7schm\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er hat sie dem Gesetze anvertraut,", "tokens": ["Er", "hat", "sie", "dem", "Ge\u00b7set\u00b7ze", "an\u00b7ver\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sein Siegel auf des Menschen Stirn gedr\u00fccket,", "tokens": ["Sein", "Sie\u00b7gel", "auf", "des", "Men\u00b7schen", "Stirn", "ge\u00b7dr\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "O selig wer in solche Augen schaut,", "tokens": ["O", "se\u00b7lig", "wer", "in", "sol\u00b7che", "Au\u00b7gen", "schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PWS", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die solche Seligkeit der Welt entz\u00fccket,", "tokens": ["Die", "sol\u00b7che", "Se\u00b7lig\u00b7keit", "der", "Welt", "ent\u00b7z\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ihm ist der Herr, ihm ist das Reich erschienen", "tokens": ["Ihm", "ist", "der", "Herr", ",", "ihm", "ist", "das", "Reich", "er\u00b7schie\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Er wei\u00df, er wei\u00df, wo's lieblich ist zu dienen.", "tokens": ["Er", "wei\u00df", ",", "er", "wei\u00df", ",", "wo's", "lieb\u00b7lich", "ist", "zu", "die\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PWAV", "ADJD", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie g\u00fctig ist der Herr, der \u00fcberall,", "tokens": ["Wie", "g\u00fc\u00b7tig", "ist", "der", "Herr", ",", "der", "\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da wo ich bin, da will er mir erscheinen,", "tokens": ["Da", "wo", "ich", "bin", ",", "da", "will", "er", "mir", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "PPER", "VAFIN", "$,", "KOUS", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wo ich singe gr\u00fc\u00dfet ihn der Widerhall", "tokens": ["Und", "wo", "ich", "sin\u00b7ge", "gr\u00fc\u00b7\u00dfet", "ihn", "der", "Wi\u00b7der\u00b7hall"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und wo ich denke kann ich ihn nur meinen,", "tokens": ["Und", "wo", "ich", "den\u00b7ke", "kann", "ich", "ihn", "nur", "mei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "VMFIN", "PPER", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ihn lob' ich lachend mit der Freude Schall", "tokens": ["Ihn", "lob'", "ich", "la\u00b7chend", "mit", "der", "Freu\u00b7de", "Schall"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ihn ehrt der Trauer stillbescheidnes Weinen,", "tokens": ["Ihn", "ehrt", "der", "Trau\u00b7er", "still\u00b7be\u00b7scheid\u00b7nes", "Wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und was mich r\u00fchrte, darf ich stolz auch singen,", "tokens": ["Und", "was", "mich", "r\u00fchr\u00b7te", ",", "darf", "ich", "stolz", "auch", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "VMFIN", "PPER", "ADJD", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Denn nur zu ihm erheben sich die Schwingen.", "tokens": ["Denn", "nur", "zu", "ihm", "er\u00b7he\u00b7ben", "sich", "die", "Schwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPER", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Mir ward ein Aug', was herrlich ist, zu sehen", "tokens": ["Mir", "ward", "ein", "Aug'", ",", "was", "herr\u00b7lich", "ist", ",", "zu", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "ADJD", "VAFIN", "$,", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Herz ward mir, was w\u00fcrdig ist zu hegen,", "tokens": ["Ein", "Herz", "ward", "mir", ",", "was", "w\u00fcr\u00b7dig", "ist", "zu", "he\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "$,", "PRELS", "ADJD", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Sonne will mir auf und untergehen,", "tokens": ["Die", "Son\u00b7ne", "will", "mir", "auf", "und", "un\u00b7ter\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PTKVZ", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Anmut geh' ich treu und fromm entgegen,", "tokens": ["Der", "An\u00b7mut", "geh'", "ich", "treu", "und", "fromm", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Vor dir du sch\u00f6ner Mensch mag gern ich stehn,", "tokens": ["Vor", "dir", "du", "sch\u00f6\u00b7ner", "Mensch", "mag", "gern", "ich", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPER", "ADJA", "NN", "VMFIN", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Dir, mir zu lieb nicht, nein nur Gottes Wegen.", "tokens": ["Dir", ",", "mir", "zu", "lieb", "nicht", ",", "nein", "nur", "Got\u00b7tes", "We\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "PTKA", "ADJD", "PTKNEG", "$,", "PTKANT", "ADV", "NN", "NN", "$."], "meter": "-+-+-+-