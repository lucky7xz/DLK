.7": {"text": "Ihm ist der Herr, ihm ist das Reich erschienen", "tokens": ["Ihm", "ist", "der", "Herr", ",", "ihm", "ist", "das", "Reich", "er\u00b7schie\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Er wei\u00df, er wei\u00df, wo's lieblich ist zu dienen.", "tokens": ["Er", "wei\u00df", ",", "er", "wei\u00df", ",", "wo's", "lieb\u00b7lich", "ist", "zu", "die\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PWAV", "ADJD", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Wie g\u00fctig ist der Herr, der \u00fcberall,", "tokens": ["Wie", "g\u00fc\u00b7tig", "ist", "der", "Herr", ",", "der", "\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da wo ich bin, da will er mir erscheinen,", "tokens": ["Da", "wo", "ich", "bin", ",", "da", "will", "er", "mir", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "PPER", "VAFIN", "$,", "KOUS", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wo ich singe gr\u00fc\u00dfet ihn der Widerhall", "tokens": ["Und", "wo", "ich", "sin\u00b7ge", "gr\u00fc\u00b7\u00dfet", "ihn", "der", "Wi\u00b7der\u00b7hall"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "V