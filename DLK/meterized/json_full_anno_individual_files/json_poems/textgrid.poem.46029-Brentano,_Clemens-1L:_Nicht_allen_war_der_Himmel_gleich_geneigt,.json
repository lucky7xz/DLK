, "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Mir ward ein Aug', was herrlich ist, zu sehen", "tokens": ["Mir", "ward", "ein", "Aug'", ",", "was", "herr\u00b7lich", "ist", ",", "zu", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "ADJD", "VAFIN", "$,", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Herz ward mir, was w\u00fcrdig ist zu hegen,", "tokens": ["Ein", "Herz", "ward", "mir", ",", "was", "w\u00fcr\u00b7dig", "ist", "zu", "he\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "$,", "PRELS", "ADJD", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Sonne will mir auf und untergehen,", "tokens": ["Die", "Son\u00b7ne", "will", "mir", "auf", "und", "un\u00b7ter\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PTKVZ", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Anmut geh' ich treu und fromm entgegen,", "tokens": ["Der", "An\u00b7mut", "geh'", "ich", "treu", "und", "fromm", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Vor dir du sch\u00f6ner Mensch mag gern ich stehn,", "tokens": ["Vor", "dir", "du", "sch\u00f6\u00b7ner", "Mensch", "mag", "gern", "ich", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPER", "ADJA", "NN", "VMFIN", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Dir, mir zu lieb nicht, nein nur Gottes Wegen.", "tokens": ["Dir", ",", "mir", "zu", "lieb", "nicht", ",", "nein", "nur", "Got\u00b7tes", "We\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "PTKA", "ADJD", "PTKNEG", "$,", "PTKANT", "ADV", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Sei irdisch Himmel mir, und himmlisch Erde", "tokens": ["Sei", "ir\u00b7disch", "Him\u00b7mel", "mir", ",", "und", "himm\u00b7lisch", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAIMP", "ADJD", "NN", "PPER", "$,", "KON", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df Freundesdienst ein Gottesdienst mir werde.", "tokens": ["Da\u00df", "Freun\u00b7des\u00b7dienst", "ein", "Got\u00b7tes\u00b7dienst", "mir", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}