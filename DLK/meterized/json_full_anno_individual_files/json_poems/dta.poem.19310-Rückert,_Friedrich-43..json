{"dta.poem.19310": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "43.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was ist der kleine Mensch in der Unendlichkeit!", "tokens": ["Was", "ist", "der", "klei\u00b7ne", "Mensch", "in", "der", "Un\u00b7end\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So eng ist sein Begriff, ihr Umfang ist so weit.", "tokens": ["So", "eng", "ist", "sein", "Be\u00b7griff", ",", "ihr", "Um\u00b7fang", "ist", "so", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mit Schrecken siehst du dich in einen Kreis gestellt,", "tokens": ["Mit", "Schre\u00b7cken", "siehst", "du", "dich", "in", "ei\u00b7nen", "Kreis", "ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der r\u00fccksichtlos auf dich, den ew'gen Umschwung h\u00e4lt.", "tokens": ["Der", "r\u00fcck\u00b7sicht\u00b7los", "auf", "dich", ",", "den", "ew'\u00b7gen", "Um\u00b7schwung", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPER", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ein Kreis, des Mittelpunkt, wenn er ist irgendwo,", "tokens": ["Ein", "Kreis", ",", "des", "Mit\u00b7tel\u00b7punkt", ",", "wenn", "er", "ist", "ir\u00b7gend\u00b7wo", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "KOUS", "PPER", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur \u00fcberall ist, und sein Umkreis nirgendwo.", "tokens": ["Nur", "\u00fc\u00b7be\u00b7rall", "ist", ",", "und", "sein", "Um\u00b7kreis", "nir\u00b7gend\u00b7wo", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "$,", "KON", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wenn dieser Mittelpunkt denn allerorten ist,", "tokens": ["Wenn", "die\u00b7ser", "Mit\u00b7tel\u00b7punkt", "denn", "al\u00b7le\u00b7ror\u00b7ten", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "KON", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So ist er ja, o Mensch, am Ort auch wo du bist.", "tokens": ["So", "ist", "er", "ja", ",", "o", "Mensch", ",", "am", "Ort", "auch", "wo", "du", "bist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "FM", "NN", "$,", "APPRART", "NN", "ADV", "PWAV", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Du stellest k\u00fchn dich hin als Mittelpunkt der Welt,", "tokens": ["Du", "stel\u00b7lest", "k\u00fchn", "dich", "hin", "als", "Mit\u00b7tel\u00b7punkt", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPER", "ADV", "KOUS", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und siehst wie sie um dich den ew'gen Umschwung h\u00e4lt.", "tokens": ["Und", "siehst", "wie", "sie", "um", "dich", "den", "ew'\u00b7gen", "Um\u00b7schwung", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "PPER", "APPR", "PRF", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "So klar ist ihr Gesetz, da\u00df du's erkennen kannst,", "tokens": ["So", "klar", "ist", "ihr", "Ge\u00b7setz", ",", "da\u00df", "du's", "er\u00b7ken\u00b7nen", "kannst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$,", "KOUS", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und durch die Einsicht selbst am Weltplan Theil gewannst.", "tokens": ["Und", "durch", "die", "Ein\u00b7sicht", "selbst", "am", "Welt\u00b7plan", "Theil", "ge\u00b7wannst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "APPRART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Wie du es siehst durchs Rohr, so kreist der Sf\u00e4ren Chor,", "tokens": ["Wie", "du", "es", "siehst", "durchs", "Rohr", ",", "so", "kreist", "der", "Sf\u00e4\u00b7ren", "Chor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVFIN", "APPRART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als zeichnetest du selbst ihm seine T\u00e4nze vor.", "tokens": ["Als", "zeich\u00b7ne\u00b7test", "du", "selbst", "ihm", "sei\u00b7ne", "T\u00e4n\u00b7ze", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ADV", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Nur kannst du das Gesetz nicht \u00e4ndern zum Vergn\u00fcgen,", "tokens": ["Nur", "kannst", "du", "das", "Ge\u00b7setz", "nicht", "\u00e4n\u00b7dern", "zum", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mu\u00dft ins gegebene erkannte sch\u00f6n dich f\u00fcgen.", "tokens": ["Mu\u00dft", "ins", "ge\u00b7ge\u00b7be\u00b7ne", "er\u00b7kann\u00b7te", "sch\u00f6n", "dich", "f\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPRART", "ADJA", "VVFIN", "ADJD", "PPER", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.9": {"line.1": {"text": "O Mensch, dis ist dein Lo\u00df, dich in Selbst\u00e4ndigkeit", "tokens": ["O", "Mensch", ",", "dis", "ist", "dein", "Lo\u00df", ",", "dich", "in", "Selb\u00b7st\u00e4n\u00b7dig\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "$,", "PRF", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu f\u00fcgen frei und gro\u00df der Weltnothwendigkeit.", "tokens": ["Zu", "f\u00fc\u00b7gen", "frei", "und", "gro\u00df", "der", "Welt\u00b7noth\u00b7wen\u00b7dig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADJD", "KON", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}