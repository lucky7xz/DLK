{"dta.poem.7287": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am Thurme.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Ich steh' auf hohem Balkone am Thurm,", "tokens": ["Ich", "steh'", "auf", "ho\u00b7hem", "Bal\u00b7ko\u00b7ne", "am", "Thurm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.2": {"text": "Umstrichen vom schreienden Staare,", "tokens": ["Um\u00b7stri\u00b7chen", "vom", "schrei\u00b7en\u00b7den", "Staa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und la\u00df' gleich einer M\u00e4nade den Sturm", "tokens": ["Und", "la\u00df'", "gleich", "ei\u00b7ner", "M\u00e4\u00b7na\u00b7de", "den", "Sturm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mir w\u00fchlen im flatternden Haare;", "tokens": ["Mir", "w\u00fch\u00b7len", "im", "flat\u00b7tern\u00b7den", "Haa\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "O wilder Geselle, o toller Fant,", "tokens": ["O", "wil\u00b7der", "Ge\u00b7sel\u00b7le", ",", "o", "tol\u00b7ler", "Fant", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "FM", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Ich m\u00f6chte dich kr\u00e4ftig umschlingen,", "tokens": ["Ich", "m\u00f6ch\u00b7te", "dich", "kr\u00e4f\u00b7tig", "um\u00b7schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Und, Sehne an Sehne, zwei Schritte vom Rand", "tokens": ["Und", ",", "Seh\u00b7ne", "an", "Seh\u00b7ne", ",", "zwei", "Schrit\u00b7te", "vom", "Rand"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "VVFIN", "APPR", "NN", "$,", "CARD", "NN", "APPRART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Auf Tod und Leben dann ringen!", "tokens": ["Auf", "Tod", "und", "Le\u00b7ben", "dann", "rin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Und drunten seh' ich am Strand, so frisch", "tokens": ["Und", "drun\u00b7ten", "seh'", "ich", "am", "Strand", ",", "so", "frisch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPRART", "NN", "$,", "ADV", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie spielende Doggen, die Wellen", "tokens": ["Wie", "spie\u00b7len\u00b7de", "Dog\u00b7gen", ",", "die", "Wel\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "$,", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Sich tummeln rings mit Geklaff und Gezisch,", "tokens": ["Sich", "tum\u00b7meln", "rings", "mit", "Ge\u00b7klaff", "und", "Ge\u00b7zisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und gl\u00e4nzende Flocken schnellen.", "tokens": ["Und", "gl\u00e4n\u00b7zen\u00b7de", "Flo\u00b7cken", "schnel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "O, springen m\u00f6cht' ich hinein alsbald,", "tokens": ["O", ",", "sprin\u00b7gen", "m\u00f6cht'", "ich", "hin\u00b7ein", "als\u00b7bald", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVINF", "VMFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Recht in die tobende Meute,", "tokens": ["Recht", "in", "die", "to\u00b7ben\u00b7de", "Meu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "Und jagen durch den korallenen Wald", "tokens": ["Und", "ja\u00b7gen", "durch", "den", "ko\u00b7ral\u00b7le\u00b7nen", "Wald"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-----+", "measure": "zehnsilber"}, "line.8": {"text": "Das Wallro\u00df, die lustige Beute!", "tokens": ["Das", "Wall\u00b7ro\u00df", ",", "die", "lus\u00b7ti\u00b7ge", "Beu\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Und dr\u00fcben seh' ich ein Wimpel wehn", "tokens": ["Und", "dr\u00fc\u00b7ben", "seh'", "ich", "ein", "Wim\u00b7pel", "wehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So keck wie eine Standarte,", "tokens": ["So", "keck", "wie", "ei\u00b7ne", "Stand\u00b7ar\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Seh auf und nieder den Kiel sich drehn", "tokens": ["Seh", "auf", "und", "nie\u00b7der", "den", "Kiel", "sich", "drehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "KON", "ADV", "ART", "NE", "PRF", "VVINF"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Von meiner luftigen Warte;", "tokens": ["Von", "mei\u00b7ner", "luf\u00b7ti\u00b7gen", "War\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "O, sitzen m\u00f6cht' ich im k\u00e4mpfenden Schiff,", "tokens": ["O", ",", "sit\u00b7zen", "m\u00f6cht'", "ich", "im", "k\u00e4mp\u00b7fen\u00b7den", "Schiff", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVINF", "VMFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Das Steuerruder ergreifen,", "tokens": ["Das", "Steu\u00b7er\u00b7ru\u00b7der", "er\u00b7grei\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und zischend \u00fcber das brandende Riff", "tokens": ["Und", "zi\u00b7schend", "\u00fc\u00b7ber", "das", "bran\u00b7den\u00b7de", "Riff"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Wie eine Seem\u00f6ve streifen.", "tokens": ["Wie", "ei\u00b7ne", "See\u00b7m\u00f6\u00b7ve", "strei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "War ich ein J\u00e4ger auf freier Flur,", "tokens": ["War", "ich", "ein", "J\u00e4\u00b7ger", "auf", "frei\u00b7er", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein St\u00fcck nur von einem Soldaten,", "tokens": ["Ein", "St\u00fcck", "nur", "von", "ei\u00b7nem", "Sol\u00b7da\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "W\u00e4r ich ein Mann doch mindestens nur,", "tokens": ["W\u00e4r", "ich", "ein", "Mann", "doch", "min\u00b7des\u00b7tens", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "So w\u00fcrde der Himmel mir rathen;", "tokens": ["So", "w\u00fcr\u00b7de", "der", "Him\u00b7mel", "mir", "ra\u00b7then", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Nun mu\u00df ich sitzen so fein und klar,", "tokens": ["Nun", "mu\u00df", "ich", "sit\u00b7zen", "so", "fein", "und", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Gleich einem artigen Kinde,", "tokens": ["Gleich", "ei\u00b7nem", "ar\u00b7ti\u00b7gen", "Kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und darf nur heimlich l\u00f6sen mein Haar,", "tokens": ["Und", "darf", "nur", "heim\u00b7lich", "l\u00f6\u00b7sen", "mein", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Und lassen es flattern im Winde!", "tokens": ["Und", "las\u00b7sen", "es", "flat\u00b7tern", "im", "Win\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVINF", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}