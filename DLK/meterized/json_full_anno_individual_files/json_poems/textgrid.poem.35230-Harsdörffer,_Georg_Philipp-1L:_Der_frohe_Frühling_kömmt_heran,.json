{"textgrid.poem.35230": {"metadata": {"author": {"name": "Harsd\u00f6rffer, Georg Philipp", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der frohe Fr\u00fchling k\u00f6mmt heran,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der frohe Fr\u00fchling k\u00f6mmt heran,", "tokens": ["Der", "fro\u00b7he", "Fr\u00fch\u00b7ling", "k\u00f6mmt", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Schnee dem Klee entweichet;", "tokens": ["Der", "Schnee", "dem", "Klee", "ent\u00b7wei\u00b7chet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Lenz, der bunte Blumenmann,", "tokens": ["Der", "Lenz", ",", "der", "bun\u00b7te", "Blu\u00b7men\u00b7mann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit linden Winden h\u00e4uchet.", "tokens": ["Mit", "lin\u00b7den", "Win\u00b7den", "h\u00e4u\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Erd' er\u00f6ffnet ihre Brust,", "tokens": ["Die", "Erd'", "er\u00b7\u00f6ff\u00b7net", "ih\u00b7re", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Saft und Kraft erf\u00fcllet;", "tokens": ["Mit", "Saft", "und", "Kraft", "er\u00b7f\u00fcl\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der zarte West, der Felder Lust,", "tokens": ["Der", "zar\u00b7te", "West", ",", "der", "Fel\u00b7der", "Lust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Hat nun den Nord gestillet.", "tokens": ["Hat", "nun", "den", "Nord", "ge\u00b7stil\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es hat der silberklare Bach", "tokens": ["Es", "hat", "der", "sil\u00b7ber\u00b7kla\u00b7re", "Bach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Harnisch ausgezogen,", "tokens": ["Den", "Har\u00b7nisch", "aus\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es jagt die Fluth der Fluthe nach,", "tokens": ["Es", "jagt", "die", "Fluth", "der", "Flut\u00b7he", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch bunten Kies gesogen.", "tokens": ["Durch", "bun\u00b7ten", "Kies", "ge\u00b7so\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das Thauen nun die Auen frischt,", "tokens": ["Das", "Thau\u00b7en", "nun", "die", "Au\u00b7en", "frischt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die wei\u00dfe Wollenheerde", "tokens": ["Die", "wei\u00b7\u00dfe", "Wol\u00b7len\u00b7heer\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Auf neubegr\u00fcntem Teppich tischt", "tokens": ["Auf", "neu\u00b7be\u00b7gr\u00fcn\u00b7tem", "Tep\u00b7pich", "tischt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und tanzet auf der Erde.", "tokens": ["Und", "tan\u00b7zet", "auf", "der", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Man h\u00f6rt die heisre Turteltaub',", "tokens": ["Man", "h\u00f6rt", "die", "heis\u00b7re", "Tur\u00b7tel\u00b7taub'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Schwalb' und Nachtigallen.", "tokens": ["Die", "Schwalb'", "und", "Nach\u00b7ti\u00b7gal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das gr\u00fcnlichwei\u00dfe Bl\u00fcthenlaub", "tokens": ["Das", "gr\u00fcn\u00b7lich\u00b7wei\u00b7\u00dfe", "Bl\u00fc\u00b7then\u00b7laub"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mu\u00df aus den Knospen fallen", "tokens": ["Mu\u00df", "aus", "den", "Knos\u00b7pen", "fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "ART", "NN", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Und bauen diesen Schattenthron", "tokens": ["Und", "bau\u00b7en", "die\u00b7sen", "Schat\u00b7ten\u00b7thron"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Luft- und Felderg\u00e4sten.", "tokens": ["Den", "Luft", "und", "Fel\u00b7der\u00b7g\u00e4s\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die Rose hebt die Dornenkron'", "tokens": ["Die", "Ro\u00b7se", "hebt", "die", "Dor\u00b7nen\u00b7kron'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Auf schwachen Stachel\u00e4sten.", "tokens": ["Auf", "schwa\u00b7chen", "Sta\u00b7chel\u00b7\u00e4s\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Sonne wieder st\u00e4rker scheint", "tokens": ["Die", "Son\u00b7ne", "wie\u00b7der", "st\u00e4r\u00b7ker", "scheint"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und machet fr\u00fcher wachen.", "tokens": ["Und", "ma\u00b7chet", "fr\u00fc\u00b7her", "wa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Allein die d\u00fcrre Rebe weint,", "tokens": ["Al\u00b7lein", "die", "d\u00fcr\u00b7re", "Re\u00b7be", "weint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wann Feld und W\u00e4lder lachen.", "tokens": ["Wann", "Feld", "und", "W\u00e4l\u00b7der", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die hochgesch\u00e4tzte Tulipan,", "tokens": ["Die", "hoch\u00b7ge\u00b7sch\u00e4tz\u00b7te", "Tu\u00b7li\u00b7pan", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Sinnbild auf dem Beete,", "tokens": ["Das", "Sinn\u00b7bild", "auf", "dem", "Bee\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Zieht ihre fremden Kleider an", "tokens": ["Zieht", "ih\u00b7re", "frem\u00b7den", "Klei\u00b7der", "an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und pranget in die Wette.", "tokens": ["Und", "pran\u00b7get", "in", "die", "Wet\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ach Gott, der du mit so viel Gut", "tokens": ["Ach", "Gott", ",", "der", "du", "mit", "so", "viel", "Gut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "NN", "$,", "PRELS", "PPER", "APPR", "ADV", "PIAT", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Bekr\u00f6nst des Jahres Zeiten,", "tokens": ["Be\u00b7kr\u00f6nst", "des", "Jah\u00b7res", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df uns auch mit erfreutem Muth", "tokens": ["La\u00df", "uns", "auch", "mit", "er\u00b7freu\u00b7tem", "Muth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Paradies bereiten,", "tokens": ["Zum", "Pa\u00b7ra\u00b7dies", "be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da wir dich werden f\u00fcr und f\u00fcr,", "tokens": ["Da", "wir", "dich", "wer\u00b7den", "f\u00fcr", "und", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VAFIN", "APPR", "KON", "APPR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die h\u00f6chste Sch\u00f6nheit, finden,", "tokens": ["Die", "h\u00f6chs\u00b7te", "Sch\u00f6n\u00b7heit", ",", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Dagegen diese schn\u00f6de Zier", "tokens": ["Da\u00b7ge\u00b7gen", "die\u00b7se", "schn\u00f6\u00b7de", "Zier"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ist eitler Staub der S\u00fcnden.", "tokens": ["Ist", "eit\u00b7ler", "Staub", "der", "S\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der frohe Fr\u00fchling k\u00f6mmt heran,", "tokens": ["Der", "fro\u00b7he", "Fr\u00fch\u00b7ling", "k\u00f6mmt", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Schnee dem Klee entweichet;", "tokens": ["Der", "Schnee", "dem", "Klee", "ent\u00b7wei\u00b7chet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Lenz, der bunte Blumenmann,", "tokens": ["Der", "Lenz", ",", "der", "bun\u00b7te", "Blu\u00b7men\u00b7mann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit linden Winden h\u00e4uchet.", "tokens": ["Mit", "lin\u00b7den", "Win\u00b7den", "h\u00e4u\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Erd' er\u00f6ffnet ihre Brust,", "tokens": ["Die", "Erd'", "er\u00b7\u00f6ff\u00b7net", "ih\u00b7re", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Saft und Kraft erf\u00fcllet;", "tokens": ["Mit", "Saft", "und", "Kraft", "er\u00b7f\u00fcl\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der zarte West, der Felder Lust,", "tokens": ["Der", "zar\u00b7te", "West", ",", "der", "Fel\u00b7der", "Lust", ","], "token_info": ["word", "word",