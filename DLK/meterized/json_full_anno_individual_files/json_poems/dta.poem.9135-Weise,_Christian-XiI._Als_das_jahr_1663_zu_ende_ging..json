{"dta.poem.9135": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "XiI.  \n Als das jahr 1663 zu ende ging.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Du liebes jahr/ so eilst du nun von hinnen/", "tokens": ["Du", "lie\u00b7bes", "jahr", "/", "so", "eilst", "du", "nun", "von", "hin\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$(", "ADV", "VVFIN", "PPER", "ADV", "APPR", "ADV", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und nimmst mit dir die freude meiner sinnen/", "tokens": ["Und", "nimmst", "mit", "dir", "die", "freu\u00b7de", "mei\u00b7ner", "sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ja freylich ist di\u00df schon der letzte tag/", "tokens": ["Ja", "frey\u00b7lich", "ist", "di\u00df", "schon", "der", "letz\u00b7te", "tag", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "VAFIN", "PDS", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da ich noch drey zu sechsen setzen mag.", "tokens": ["Da", "ich", "noch", "drey", "zu", "sech\u00b7sen", "set\u00b7zen", "mag."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KOUS", "PPER", "ADV", "CARD", "PTKZU", "VVINF", "VVFIN", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "2. Verg\u00f6nne mir du auszug meiner jahre", "tokens": ["Ver\u00b7g\u00f6n\u00b7ne", "mir", "du", "aus\u00b7zug", "mei\u00b7ner", "jah\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df ich mein hertz der feder offenbare", "tokens": ["Da\u00df", "ich", "mein", "hertz", "der", "fe\u00b7der", "of\u00b7fen\u00b7ba\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "La\u00df mich einmahl auff dich zur\u00fccke sehn/", "tokens": ["La\u00df", "mich", "ein\u00b7mahl", "auff", "dich", "zu\u00b7r\u00fc\u00b7cke", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "PPER", "VVFIN", "VVINF", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Es wird mir doch nicht mehr so wol geschehn.", "tokens": ["Es", "wird", "mir", "doch", "nicht", "mehr", "so", "wol", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PTKNEG", "ADV", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "3. Du hast mir erst die jugend und das leben", "tokens": ["Du", "hast", "mir", "erst", "die", "ju\u00b7gend", "und", "das", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ART", "NN", "KON", "PDS", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch eine krafft der s\u00fcssigkeit gegeben/", "tokens": ["Durch", "ei\u00b7ne", "krafft", "der", "s\u00fcs\u00b7sig\u00b7keit", "ge\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich war noch tod/ du hast mich aufgeweckt/", "tokens": ["Ich", "war", "noch", "tod", "/", "du", "hast", "mich", "auf\u00b7ge\u00b7weckt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "$(", "PPER", "VAFIN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich brannte nicht/ du hast mich angesteckt.", "tokens": ["Ich", "brann\u00b7te", "nicht", "/", "du", "hast", "mich", "an\u00b7ge\u00b7steckt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$(", "PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "4. Du hast mich erst den sanfften weg gef\u00fchret/", "tokens": ["Du", "hast", "mich", "erst", "den", "sanff\u00b7ten", "weg", "ge\u00b7f\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ART", "ADJA", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und meine brust durch liebes-gluth ber\u00fchret/", "tokens": ["Und", "mei\u00b7ne", "brust", "durch", "lie\u00b7bes\u00b7gluth", "be\u00b7r\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du sch\u00f6ne zeit du hast mir kund gethan/", "tokens": ["Du", "sch\u00f6\u00b7ne", "zeit", "du", "hast", "mir", "kund", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PPER", "VAFIN", "PPER", "PTKVZ", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Was der verstand vertrauter freundschafft kan.", "tokens": ["Was", "der", "ver\u00b7stand", "ver\u00b7trau\u00b7ter", "freund\u00b7schafft", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "VVFIN", "ADJA", "NN", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "5. Ich konte nicht mein hertz mein liebgen sprechen", "tokens": ["Ich", "kon\u00b7te", "nicht", "mein", "hertz", "mein", "lieb\u00b7gen", "spre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "PPOSAT", "NN", "PPOSAT", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und muste mir den kopf umsonst zerbrechen/", "tokens": ["Und", "mus\u00b7te", "mir", "den", "kopf", "um\u00b7sonst", "zer\u00b7bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ART", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch deine gunst versetzte mich zur ruh", "tokens": ["Doch", "dei\u00b7ne", "gunst", "ver\u00b7setz\u00b7te", "mich", "zur", "ruh"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und f\u00fchrte mir die sch\u00f6nheit selber zu.", "tokens": ["Und", "f\u00fchr\u00b7te", "mir", "die", "sch\u00f6n\u00b7heit", "sel\u00b7ber", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "6. Da war ein sinn/ ein willen/ ein belieben/", "tokens": ["Da", "war", "ein", "sinn", "/", "ein", "wil\u00b7len", "/", "ein", "be\u00b7lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$(", "ART", "NN", "$(", "ART", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schertz sonder schertz/ vergn\u00fcgung ohn betr\u00fcben/", "tokens": ["Schertz", "son\u00b7der", "schertz", "/", "ver\u00b7gn\u00fc\u00b7gung", "ohn", "be\u00b7tr\u00fc\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$(", "NN", "APPR", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie war erfreut und lustig wann ich kam/", "tokens": ["Sie", "war", "er\u00b7freut", "und", "lus\u00b7tig", "wann", "ich", "kam", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "ADJD", "PWAV", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und kr\u00e4nckte sich/ so bald ich abschied nahm.", "tokens": ["Und", "kr\u00e4nck\u00b7te", "sich", "/", "so", "bald", "ich", "ab\u00b7schied", "nahm", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "$(", "ADV", "ADV", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "7. Jedennoch hab ich was von dir bekommen", "tokens": ["Je\u00b7den\u00b7noch", "hab", "ich", "was", "von", "dir", "be\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So hastu all\u2019s auch wieder weggenommen", "tokens": ["So", "has\u00b7tu", "all's", "auch", "wie\u00b7der", "weg\u00b7ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du hast mir nur den schein/ der uns betreugt/", "tokens": ["Du", "hast", "mir", "nur", "den", "schein", "/", "der", "uns", "be\u00b7treugt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ART", "NN", "$(", "PRELS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Durch einen traum/ und durch ein bild gezeigt.", "tokens": ["Durch", "ei\u00b7nen", "traum", "/", "und", "durch", "ein", "bild", "ge\u00b7zeigt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$(", "KON", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "8. Ich werde wohl nicht in dergleichen freuden", "tokens": ["Ich", "wer\u00b7de", "wohl", "nicht", "in", "derg\u00b7lei\u00b7chen", "freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "APPR", "PIS", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein angesicht bey solcher sch\u00f6nheit weiden/", "tokens": ["Mein", "an\u00b7ge\u00b7sicht", "bey", "sol\u00b7cher", "sch\u00f6n\u00b7heit", "wei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du altes jahr du nimbst den alten sinn/", "tokens": ["Du", "al\u00b7tes", "jahr", "du", "nimbst", "den", "al\u00b7ten", "sinn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die alte lust/ die alte hoffnung hin.", "tokens": ["Die", "al\u00b7te", "lust", "/", "die", "al\u00b7te", "hoff\u00b7nung", "hin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "9. Ich will mich nicht de\u00dfhalben mit dir zancken/", "tokens": ["Ich", "will", "mich", "nicht", "de\u00df\u00b7hal\u00b7ben", "mit", "dir", "zan\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "PAV", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich werde dir vielmehr vor dieses dancken/", "tokens": ["Ich", "wer\u00b7de", "dir", "viel\u00b7mehr", "vor", "die\u00b7ses", "dan\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPR", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df ich itzund durch dein und meinen flei\u00df", "tokens": ["Da\u00df", "ich", "it\u00b7zund", "durch", "dein", "und", "mei\u00b7nen", "flei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPOSAT", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den schlauen gang der stillen liebe wei\u00df.", "tokens": ["Den", "schlau\u00b7en", "gang", "der", "stil\u00b7len", "lie\u00b7be", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "10. Ich mu\u00df nunmehr in diesem neuen jahre/", "tokens": ["Ich", "mu\u00df", "nun\u00b7mehr", "in", "die\u00b7sem", "neu\u00b7en", "jah\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Beflissen seyn/ wie ich die stunde spare", "tokens": ["Be\u00b7flis\u00b7sen", "seyn", "/", "wie", "ich", "die", "stun\u00b7de", "spa\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VAINF", "$(", "PWAV", "PPER", "ART", "ADJA", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die ich zu viel auf zeit-vertreib gelegt/", "tokens": ["Die", "ich", "zu", "viel", "auf", "zeit\u00b7ver\u00b7treib", "ge\u00b7legt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKA", "PIS", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Darzu mich sie und ihre lust bewegt.", "tokens": ["Dar\u00b7zu", "mich", "sie", "und", "ih\u00b7re", "lust", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PPER", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "11. Das gl\u00fccke mag am besten f\u00fcr mich sorgen/", "tokens": ["Das", "gl\u00fc\u00b7cke", "mag", "am", "bes\u00b7ten", "f\u00fcr", "mich", "sor\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VMFIN", "PTKA", "ADJD", "APPR", "PPER", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du altes jahr zeuch fort/ ich denck an morgen/", "tokens": ["Du", "al\u00b7tes", "jahr", "zeuch", "fort", "/", "ich", "denck", "an", "mor\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VVIMP", "PTKVZ", "$(", "PPER", "VVFIN", "APPR", "ADV", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Daselbsten wird der neue sonnenschein", "tokens": ["Da\u00b7selbs\u00b7ten", "wird", "der", "neu\u00b7e", "son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mein neuer trost und neue wollust seyn.", "tokens": ["Mein", "neu\u00b7er", "trost", "und", "neu\u00b7e", "wol\u00b7lust", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}