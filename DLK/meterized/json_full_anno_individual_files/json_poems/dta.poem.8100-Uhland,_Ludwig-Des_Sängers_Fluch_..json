{"dta.poem.8100": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Des S\u00e4ngers Fluch .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Es stand in alten Zeiten ein Schlo\u00df, so hoch und hehr,", "tokens": ["Es", "stand", "in", "al\u00b7ten", "Zei\u00b7ten", "ein", "Schlo\u00df", ",", "so", "hoch", "und", "hehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Weit gl\u00e4nzt\u2019 es \u00fcber die Lande bis an das blaue Meer,", "tokens": ["Weit", "gl\u00e4nzt'", "es", "\u00fc\u00b7ber", "die", "Lan\u00b7de", "bis", "an", "das", "blau\u00b7e", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und rings von duft\u2019gen G\u00e4rten ein bl\u00fcthenreicher Kranz,", "tokens": ["Und", "rings", "von", "duft'\u00b7gen", "G\u00e4r\u00b7ten", "ein", "bl\u00fc\u00b7then\u00b7rei\u00b7cher", "Kranz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Drin sprangen frische Brunnen in Regenbogenglanz.", "tokens": ["Drin", "spran\u00b7gen", "fri\u00b7sche", "Brun\u00b7nen", "in", "Re\u00b7gen\u00b7bo\u00b7gen\u00b7glanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Dort sa\u00df ein stolzer K\u00f6nig, an Land und Siegen reich,", "tokens": ["Dort", "sa\u00df", "ein", "stol\u00b7zer", "K\u00f6\u00b7nig", ",", "an", "Land", "und", "Sie\u00b7gen", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "APPR", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Er sa\u00df auf seinem Throne so finster und so bleich;", "tokens": ["Er", "sa\u00df", "auf", "sei\u00b7nem", "Thro\u00b7ne", "so", "fins\u00b7ter", "und", "so", "bleich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Denn was er sinnt, ist Schrecken, und was er blickt, ist Wuth,", "tokens": ["Denn", "was", "er", "sinnt", ",", "ist", "Schre\u00b7cken", ",", "und", "was", "er", "blickt", ",", "ist", "Wuth", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "VAFIN", "NN", "$,", "KON", "PWS", "PPER", "VVFIN", "$,", "VAFIN", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und was er spricht, ist Gei\u00dfel, und was er schreibt, ist Blut.", "tokens": ["Und", "was", "er", "spricht", ",", "ist", "Gei\u00b7\u00dfel", ",", "und", "was", "er", "schreibt", ",", "ist", "Blut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "VAFIN", "NN", "$,", "KON", "PWS", "PPER", "VVFIN", "$,", "VAFIN", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Einst zog nach diesem Schlosse ein edles S\u00e4ngerpaar,", "tokens": ["Einst", "zog", "nach", "die\u00b7sem", "Schlos\u00b7se", "ein", "ed\u00b7les", "S\u00e4n\u00b7ger\u00b7paar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PDAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der Ein\u2019 in goldnen Locken, der Andre grau von Haar;", "tokens": ["Der", "Ein'", "in", "gold\u00b7nen", "Lo\u00b7cken", ",", "der", "And\u00b7re", "grau", "von", "Haar", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,", "PRELS", "PIS", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der Alte mit der Harfe, der sa\u00df auf schmuckem Ro\u00df,", "tokens": ["Der", "Al\u00b7te", "mit", "der", "Har\u00b7fe", ",", "der", "sa\u00df", "auf", "schmu\u00b7ckem", "Ro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "PRELS", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Es schritt ihm frisch zur Seite der bl\u00fchende Geno\u00df.", "tokens": ["Es", "schritt", "ihm", "frisch", "zur", "Sei\u00b7te", "der", "bl\u00fc\u00b7hen\u00b7de", "Ge\u00b7no\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+---+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Der Alte sprach zum Jungen: \u201enun sey bereit, mein Sohn!", "tokens": ["Der", "Al\u00b7te", "sprach", "zum", "Jun\u00b7gen", ":", "\u201e", "nun", "sey", "be\u00b7reit", ",", "mein", "Sohn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$.", "$(", "ADV", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Denk unsrer tiefsten Lieder, stimm an den vollsten Ton,", "tokens": ["Denk", "uns\u00b7rer", "tiefs\u00b7ten", "Lie\u00b7der", ",", "stimm", "an", "den", "volls\u00b7ten", "Ton", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "ADJA", "NN", "$,", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Nimm alle Kraft zusammen, die Lust und auch den Schmerz!", "tokens": ["Nimm", "al\u00b7le", "Kraft", "zu\u00b7sam\u00b7men", ",", "die", "Lust", "und", "auch", "den", "Schmerz", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "PTKVZ", "$,", "ART", "NN", "KON", "ADV", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Es gilt uns heut, zu r\u00fchren des K\u00f6nigs steinern Herz.\u201c", "tokens": ["Es", "gilt", "uns", "heut", ",", "zu", "r\u00fch\u00b7ren", "des", "K\u00f6\u00b7nigs", "stei\u00b7nern", "Herz", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PTKZU", "VVINF", "ART", "NN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Schon stehn die beiden S\u00e4nger im hohen S\u00e4ulensaal", "tokens": ["Schon", "stehn", "die", "bei\u00b7den", "S\u00e4n\u00b7ger", "im", "ho\u00b7hen", "S\u00e4u\u00b7len\u00b7saal"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "PIAT", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und auf dem Throne sitzen der K\u00f6nig und sein Gemahl;", "tokens": ["Und", "auf", "dem", "Thro\u00b7ne", "sit\u00b7zen", "der", "K\u00f6\u00b7nig", "und", "sein", "Ge\u00b7mahl", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der K\u00f6nig, furchtbar pr\u00e4chtig, wie blut\u2019ger Nordlichtschein,", "tokens": ["Der", "K\u00f6\u00b7nig", ",", "furcht\u00b7bar", "pr\u00e4ch\u00b7tig", ",", "wie", "blut'\u00b7ger", "Nord\u00b7licht\u00b7schein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "ADJD", "$,", "PWAV", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die K\u00f6nigin, s\u00fc\u00df und milde, als blickte Vollmond drein.", "tokens": ["Die", "K\u00f6\u00b7ni\u00b7gin", ",", "s\u00fc\u00df", "und", "mil\u00b7de", ",", "als", "blick\u00b7te", "Voll\u00b7mond", "drein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,", "KOUS", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Da schlug der Greis die Saiten, er schlug sie wundervoll,", "tokens": ["Da", "schlug", "der", "Greis", "die", "Sai\u00b7ten", ",", "er", "schlug", "sie", "wun\u00b7der\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da\u00df reicher, immer reicher der Klang zum Ohre schwoll,", "tokens": ["Da\u00df", "rei\u00b7cher", ",", "im\u00b7mer", "rei\u00b7cher", "der", "Klang", "zum", "Oh\u00b7re", "schwoll", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "$,", "ADV", "ADJD", "ART", "NN", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Dann str\u00f6mte himmlisch helle des J\u00fcnglings Stimme vor,", "tokens": ["Dann", "str\u00f6m\u00b7te", "himm\u00b7lisch", "hel\u00b7le", "des", "J\u00fcng\u00b7lings", "Stim\u00b7me", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ADJA", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Des Alten Sang dazwischen, wie dumpfer Geisterchor.", "tokens": ["Des", "Al\u00b7ten", "Sang", "da\u00b7zwi\u00b7schen", ",", "wie", "dum\u00b7pfer", "Geis\u00b7ter\u00b7chor", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "$,", "PWAV", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Sie singen von Lenz und Liebe, von sel\u2019ger goldner Zeit,", "tokens": ["Sie", "sin\u00b7gen", "von", "Lenz", "und", "Lie\u00b7be", ",", "von", "sel'\u00b7ger", "gold\u00b7ner", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Von Freiheit, M\u00e4nnerw\u00fcrde, von Treu und Heiligkeit;", "tokens": ["Von", "Frei\u00b7heit", ",", "M\u00e4n\u00b7ner\u00b7w\u00fcr\u00b7de", ",", "von", "Treu", "und", "Hei\u00b7lig\u00b7keit", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Sie singen von allem S\u00fc\u00dfen, was Menschenbrust durchbebt,", "tokens": ["Sie", "sin\u00b7gen", "von", "al\u00b7lem", "S\u00fc\u00b7\u00dfen", ",", "was", "Men\u00b7schen\u00b7brust", "durch\u00b7bebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIS", "NN", "$,", "PWS", "NN", "VVFIN", "$,"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Sie singen von allem Hohen, was Menschenherz erhebt.", "tokens": ["Sie", "sin\u00b7gen", "von", "al\u00b7lem", "Ho\u00b7hen", ",", "was", "Men\u00b7schen\u00b7herz", "er\u00b7hebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIS", "NN", "$,", "PWS", "NN", "VVFIN", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "Die H\u00f6flingsschaar im Kreise verlernet jeden Spott,", "tokens": ["Die", "H\u00f6f\u00b7lings\u00b7schaar", "im", "Krei\u00b7se", "ver\u00b7ler\u00b7net", "je\u00b7den", "Spott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Des K\u00f6nigs trotz\u2019ge Krieger, sie beugen sich vor Gott,", "tokens": ["Des", "K\u00f6\u00b7nigs", "trotz'\u00b7ge", "Krie\u00b7ger", ",", "sie", "beu\u00b7gen", "sich", "vor", "Gott", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "PPER", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die K\u00f6nigin, zerflossen in Wehmuth und in Lust,", "tokens": ["Die", "K\u00f6\u00b7ni\u00b7gin", ",", "zer\u00b7flos\u00b7sen", "in", "Weh\u00b7muth", "und", "in", "Lust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Sie wirft den S\u00e4ngern nieder die Rose von ihrer Brust.", "tokens": ["Sie", "wirft", "den", "S\u00e4n\u00b7gern", "nie\u00b7der", "die", "Ro\u00b7se", "von", "ih\u00b7rer", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "\u201eihr habt mein Volk verf\u00fchret, verlockt ihr nun mein Weib?\u201c", "tokens": ["\u201e", "ihr", "habt", "mein", "Volk", "ver\u00b7f\u00fch\u00b7ret", ",", "ver\u00b7lockt", "ihr", "nun", "mein", "Weib", "?", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der K\u00f6nig schreit es w\u00fcthend, er bebt am ganzen Leib,", "tokens": ["Der", "K\u00f6\u00b7nig", "schreit", "es", "w\u00fct\u00b7hend", ",", "er", "bebt", "am", "gan\u00b7zen", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$,", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Er wirft sein Schwerdt, das blitzend des J\u00fcnglings Brust", "tokens": ["Er", "wirft", "sein", "Schwerdt", ",", "das", "blit\u00b7zend", "des", "J\u00fcng\u00b7lings", "Brust"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PRELS", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "durchdringt,", "tokens": ["durch\u00b7dringt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Draus, statt der goldnen Lieder, ein Blutstral hochauf springt.", "tokens": ["Draus", ",", "statt", "der", "gold\u00b7nen", "Lie\u00b7der", ",", "ein", "Bluts\u00b7tral", "hoch\u00b7auf", "springt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "KOUI", "ART", "ADJA", "NN", "$,", "ART", "NN", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "Und wie vom Sturm zerstoben ist all der H\u00f6rer Schwarm,", "tokens": ["Und", "wie", "vom", "Sturm", "zer\u00b7sto\u00b7ben", "ist", "all", "der", "H\u00f6\u00b7rer", "Schwarm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPRART", "NN", "VVPP", "VAFIN", "PIAT", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der J\u00fcngling hat verr\u00f6chelt in seines Meisters Arm,", "tokens": ["Der", "J\u00fcng\u00b7ling", "hat", "ver\u00b7r\u00f6\u00b7chelt", "in", "sei\u00b7nes", "Meis\u00b7ters", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der schl\u00e4gt um ihn den Mantel und setzt ihn auf das Ro\u00df,", "tokens": ["Der", "schl\u00e4gt", "um", "ihn", "den", "Man\u00b7tel", "und", "setzt", "ihn", "auf", "das", "Ro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPER", "ART", "NN", "KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Er bindt ihn aufrecht feste, verl\u00e4\u00dft mit ihm das Schlo\u00df.", "tokens": ["Er", "bindt", "ihn", "auf\u00b7recht", "fes\u00b7te", ",", "ver\u00b7l\u00e4\u00dft", "mit", "ihm", "das", "Schlo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ADJA", "$,", "VVFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.11": {"line.1": {"text": "Doch vor dem hohen Thore, da h\u00e4lt der S\u00e4ngergreis,", "tokens": ["Doch", "vor", "dem", "ho\u00b7hen", "Tho\u00b7re", ",", "da", "h\u00e4lt", "der", "S\u00e4n\u00b7ger\u00b7greis", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da fa\u00dft er seine Harfe, sie aller Harfen Preis,", "tokens": ["Da", "fa\u00dft", "er", "sei\u00b7ne", "Har\u00b7fe", ",", "sie", "al\u00b7ler", "Har\u00b7fen", "Preis", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "PPER", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "An einer Marmors\u00e4ule, da hat er sie zerschellt,", "tokens": ["An", "ei\u00b7ner", "Mar\u00b7mor\u00b7s\u00e4u\u00b7le", ",", "da", "hat", "er", "sie", "zer\u00b7schellt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Dann ruft er, da\u00df es schaurig durch Schlo\u00df und G\u00e4rten gellt:", "tokens": ["Dann", "ruft", "er", ",", "da\u00df", "es", "schau\u00b7rig", "durch", "Schlo\u00df", "und", "G\u00e4r\u00b7ten", "gellt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADJD", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.12": {"line.1": {"text": "\u201eweh euch, ihr stolzen Hallen! nie t\u00f6ne s\u00fc\u00dfer Klang", "tokens": ["\u201e", "weh", "euch", ",", "ihr", "stol\u00b7zen", "Hal\u00b7len", "!", "nie", "t\u00f6\u00b7ne", "s\u00fc\u00b7\u00dfer", "Klang"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "VVIMP", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$.", "ADV", "ADJA", "ADJA", "NN"], "meter": "---+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Durch eure R\u00e4ume wieder, nie Saite noch Gesang,", "tokens": ["Durch", "eu\u00b7re", "R\u00e4u\u00b7me", "wie\u00b7der", ",", "nie", "Sai\u00b7te", "noch", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$,", "ADV", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Nein! Seufzer nur und St\u00f6hnen, und scheuer Sklavenschritt,", "tokens": ["Nein", "!", "Seuf\u00b7zer", "nur", "und", "St\u00f6h\u00b7nen", ",", "und", "scheu\u00b7er", "Skla\u00b7ven\u00b7schritt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "NN", "ADV", "KON", "NN", "$,", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Bis euch zu Schutt und Moder der Rachegeist zertritt!", "tokens": ["Bis", "euch", "zu", "Schutt", "und", "Mo\u00b7der", "der", "Ra\u00b7che\u00b7geist", "zer\u00b7tritt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "NN", "KON", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.13": {"line.1": {"text": "Weh euch, ihr duft\u2019gen G\u00e4rten im holden Maienlicht!", "tokens": ["Weh", "euch", ",", "ihr", "duft'\u00b7gen", "G\u00e4r\u00b7ten", "im", "hol\u00b7den", "Mai\u00b7en\u00b7licht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Euch zeig\u2019 ich dieses Todten entstelltes Angesicht,", "tokens": ["Euch", "zeig'", "ich", "die\u00b7ses", "Tod\u00b7ten", "ent\u00b7stell\u00b7tes", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PDAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Da\u00df ihr darob verdorret, da\u00df jeder Quell versiegt,", "tokens": ["Da\u00df", "ihr", "da\u00b7rob", "ver\u00b7dor\u00b7ret", ",", "da\u00df", "je\u00b7der", "Quell", "ver\u00b7siegt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "VVFIN", "$,", "KOUS", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Da\u00df ihr in k\u00fcnft\u2019gen Tagen versteint, ver\u00f6det liegt.", "tokens": ["Da\u00df", "ihr", "in", "k\u00fcnft'\u00b7gen", "Ta\u00b7gen", "ver\u00b7steint", ",", "ver\u00b7\u00f6\u00b7det", "liegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$,", "VVPP", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.14": {"line.1": {"text": "Weh dir, verruchter M\u00f6rder! du Fluch des S\u00e4ngerthums!", "tokens": ["Weh", "dir", ",", "ver\u00b7ruch\u00b7ter", "M\u00f6r\u00b7der", "!", "du", "Fluch", "des", "S\u00e4n\u00b7ger\u00b7thums", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ADJA", "NN", "$.", "PPER", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Umsonst sey all dein Ringen nach Kr\u00e4nzen blut\u2019gen Ruhms,", "tokens": ["Um\u00b7sonst", "sey", "all", "dein", "Rin\u00b7gen", "nach", "Kr\u00e4n\u00b7zen", "blut'\u00b7gen", "Ruhms", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "PPOSAT", "NN", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Dein Name sey vergessen, in ew\u2019ge Nacht getaucht,", "tokens": ["Dein", "Na\u00b7me", "sey", "ver\u00b7ges\u00b7sen", ",", "in", "ew'\u00b7ge", "Nacht", "ge\u00b7taucht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Sey, wie ein letztes R\u00f6cheln, in leere Luft verhaucht!\u201c", "tokens": ["Sey", ",", "wie", "ein", "letz\u00b7tes", "R\u00f6\u00b7cheln", ",", "in", "lee\u00b7re", "Luft", "ver\u00b7haucht", "!", "\u201c"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "$,", "PWAV", "ART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.15": {"line.1": {"text": "Der Alte hat\u2019s gerufen, der Himmel hat\u2019s geh\u00f6rt,", "tokens": ["Der", "Al\u00b7te", "hat's", "ge\u00b7ru\u00b7fen", ",", "der", "Him\u00b7mel", "hat's", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die Mauern liegen nieder, die Hallen sind zerst\u00f6rt,", "tokens": ["Die", "Mau\u00b7ern", "lie\u00b7gen", "nie\u00b7der", ",", "die", "Hal\u00b7len", "sind", "zer\u00b7st\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Noch Eine hohe S\u00e4ule zeugt von verschwundner Pracht,", "tokens": ["Noch", "Ei\u00b7ne", "ho\u00b7he", "S\u00e4u\u00b7le", "zeugt", "von", "ver\u00b7schwund\u00b7ner", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Auch diese, schon geborsten, kann st\u00fcrzen \u00fcber Nacht.", "tokens": ["Auch", "die\u00b7se", ",", "schon", "ge\u00b7bors\u00b7ten", ",", "kann", "st\u00fcr\u00b7zen", "\u00fc\u00b7ber", "Nacht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "$,", "ADV", "VVPP", "$,", "VMFIN", "VVINF", "APPR", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.16": {"line.1": {"text": "Und rings, statt duft\u2019ger G\u00e4rten, ein \u00f6des Haideland,", "tokens": ["Und", "rings", ",", "statt", "duft'\u00b7ger", "G\u00e4r\u00b7ten", ",", "ein", "\u00f6\u00b7des", "Hai\u00b7de\u00b7land", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUI", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Kein Baum verstreuet Schatten, kein Quell durchdringt den", "tokens": ["Kein", "Baum", "ver\u00b7streu\u00b7et", "Schat\u00b7ten", ",", "kein", "Quell", "durch\u00b7dringt", "den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "NN", "$,", "PIAT", "NN", "VVFIN", "ART"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sand,", "tokens": ["Sand", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Des K\u00f6nigs Namen meldet kein Lied, kein Heldenbuch;", "tokens": ["Des", "K\u00f6\u00b7nigs", "Na\u00b7men", "mel\u00b7det", "kein", "Lied", ",", "kein", "Hel\u00b7den\u00b7buch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PIAT", "NN", "$,", "PIAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Versunken und vergessen! das ist des S\u00e4ngers Fluch.", "tokens": ["Ver\u00b7sun\u00b7ken", "und", "ver\u00b7ges\u00b7sen", "!", "das", "ist", "des", "S\u00e4n\u00b7gers", "Fluch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "$.", "PDS", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}