{"dta.poem.20598": {"metadata": {"author": {"name": "Hebel, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Auf einem Grabe .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1803", "urn": "urn:nbn:de:kobv:b4-200905192133", "language": ["de:0.99"], "booktitle": "[Hebel, Johann Peter]: Allemannische Gedichte. Karlsruhe, 1803."}, "poem": {"stanza.1": {"line.1": {"text": "Schlof wohl, schlof wohl im ch\u00fcele Bett!               ", "tokens": ["Schlof", "wohl", ",", "schlof", "wohl", "im", "ch\u00fce\u00b7le", "Bett", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "De ligsch zwor hert uf Sand und Chies;", "tokens": ["De", "ligsch", "zwor", "hert", "uf", "Sand", "und", "Chies", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "VVFIN", "APPR", "NN", "KON", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "doch sp\u00fcrts di m\u00fcede Rucke nit.", "tokens": ["doch", "sp\u00fcrts", "di", "m\u00fce\u00b7de", "Ru\u00b7cke", "nit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NE", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schlof sanft und wohl!", "tokens": ["Schlof", "sanft", "und", "wohl", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "KON", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Und \u2019s Deckbett lit der, dick und schwer", "tokens": ["Und", "'s", "Deck\u00b7bett", "lit", "der", ",", "dick", "und", "schwer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "NN", "VVFIN", "ART", "$,", "ADJD", "KON", "ADJD"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "in d\u2019 H\u00f6chi gsch\u00fcttlet, uffem Herz;", "tokens": ["in", "d'", "H\u00f6\u00b7chi", "gsch\u00fctt\u00b7let", ",", "uf\u00b7fem", "Herz", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Doch schlofsch im Friede, \u2019s druckt di nit.", "tokens": ["Doch", "schlof\u00b7sch", "im", "Frie\u00b7de", ",", "'s", "druckt", "di", "nit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "$,", "PPER", "VVFIN", "NE", "PTKNEG", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Schlof sanft und wohl!", "tokens": ["Schlof", "sanft", "und", "wohl", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "KON", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "De schlofsch und h\u00f6rsch mi Bh\u00fctdi Gott,", "tokens": ["De", "schlof\u00b7sch", "und", "h\u00f6rsch", "mi", "Bh\u00fct\u00b7di", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "KON", "ADJD", "NE", "NE", "NN", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "de h\u00f6rsch mi sehnli Chlage nit.", "tokens": ["de", "h\u00f6rsch", "mi", "sehn\u00b7li", "Chla\u00b7ge", "nit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "W\u00e4rs besser, wenn de \u2019s h\u00f6re ch\u00f6nntsch?", "tokens": ["W\u00e4rs", "bes\u00b7ser", ",", "wenn", "de", "'s", "h\u00f6\u00b7re", "ch\u00f6nntsch", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "KOUS", "NE", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nei, weger nei!", "tokens": ["Nei", ",", "we\u00b7ger", "nei", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "O \u2019s isch der wohl, es isch der wohl!", "tokens": ["O", "'s", "isch", "der", "wohl", ",", "es", "isch", "der", "wohl", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADJD", "ART", "ADV", "$,", "PPER", "ADJD", "ART", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Und wenni numme by der w\u00e4r,", "tokens": ["Und", "wen\u00b7ni", "num\u00b7me", "by", "der", "w\u00e4r", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "FM", "FM", "FM", "ART", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "se w\u00e4r scho alles recht und gut.", "tokens": ["se", "w\u00e4r", "scho", "al\u00b7les", "recht", "und", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PIS", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mer toltenis!", "tokens": ["Mer", "tol\u00b7te\u00b7nis", "!"], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "De schlofsch und achtisch \u2019s Unrneih nit", "tokens": ["De", "schlof\u00b7sch", "und", "ach\u00b7tisch", "'s", "U\u00b7nrneih", "nit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "KON", "ADJD", "PPER", "NN", "PTKNEG"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "in Chilche-Thurn die langi Nacht,", "tokens": ["in", "Chil\u00b7che\u00b7Thurn", "die", "lan\u00b7gi", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und wenn der W\u00e4chter ", "tokens": ["und", "wenn", "der", "W\u00e4ch\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "im stille Dorf.", "tokens": ["im", "stil\u00b7le", "Dorf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Und wenns am schwarze Himmel blizt,", "tokens": ["Und", "wenns", "am", "schwar\u00b7ze", "Him\u00b7mel", "blizt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und Gw\u00fclch an Gw\u00fclch im Donner chracht", "tokens": ["und", "Gw\u00fclch", "an", "Gw\u00fclch", "im", "Don\u00b7ner", "chracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "NN", "APPRART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "se fahrtder \u2019s Wetter \u00fcber\u2019s Grab,", "tokens": ["se", "fahrt\u00b7der", "'s", "Wet\u00b7ter", "\u00fc\u00b7ber's", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "PPER", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "und weckt di nit.", "tokens": ["und", "weckt", "di", "nit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Und was di fr\u00fceih im Morgeroth", "tokens": ["Und", "was", "di", "fr\u00fc\u00b7eih", "im", "Mor\u00b7ge\u00b7roth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "NE", "ADJD", "APPRART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "bis spot in d\u2019 Mittnacht bch\u00fcmmert het,", "tokens": ["bis", "spot", "in", "d'", "Mitt\u00b7nacht", "bch\u00fcm\u00b7mert", "het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NE", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+++-+-", "measure": "unknown.measure.penta"}, "line.3": {"text": "Gottlob, es ficht di n\u00fcmmen a", "tokens": ["Gott\u00b7lob", ",", "es", "ficht", "di", "n\u00fcm\u00b7men", "a"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "im stille Grab.", "tokens": ["im", "stil\u00b7le", "Grab", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Es isch der wohl, o \u2019s isch der wohl!", "tokens": ["Es", "isch", "der", "wohl", ",", "o", "'s", "isch", "der", "wohl", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ART", "ADV", "$,", "FM", "PPER", "ADJD", "ART", "ADV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "und alles was de glitte hesch,", "tokens": ["und", "al\u00b7les", "was", "de", "glit\u00b7te", "hesch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PWS", "NE", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gottlob und Dank, im ch\u00fcele Grund", "tokens": ["Gott\u00b7lob", "und", "Dank", ",", "im", "ch\u00fce\u00b7le", "Grund"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "thuts n\u00fcmme weh.", "tokens": ["thuts", "n\u00fcm\u00b7me", "weh", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Drum, wenni numme by der w\u00e4r,", "tokens": ["Drum", ",", "wen\u00b7ni", "num\u00b7me", "by", "der", "w\u00e4r", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "FM", "FM", "FM", "ART", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "se w\u00e4r io alles recht und gut;", "tokens": ["se", "w\u00e4r", "i\u00b7o", "al\u00b7les", "recht", "und", "gut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PIS", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "iez sitzi do, und wei\u00df kei Trost", "tokens": ["iez", "sit\u00b7zi", "do", ",", "und", "wei\u00df", "kei", "Trost"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "$,", "KON", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "mi\u2019m tiefe Schmerz.", "tokens": ["mi'm", "tie\u00b7fe", "Schmerz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.10": {"line.1": {"text": "Doch \u00f6bbe bald, wenns Gottswill isch,", "tokens": ["Doch", "\u00f6b\u00b7be", "bald", ",", "wenns", "Gotts\u00b7will", "isch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "KOUS", "NE", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "se chunnt mi Samstig z\u2019 oben au,", "tokens": ["se", "chunnt", "mi", "Sams\u00b7tig", "z'", "o\u00b7ben", "au", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "ADV", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "und druf, se grabt der Nochber Chlaus", "tokens": ["und", "druf", ",", "se", "grabt", "der", "Noch\u00b7ber", "Chlaus"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "mir au ne Bett.", "tokens": ["mir", "au", "ne", "Bett", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "NE", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.11": {"line.1": {"text": "Und wenni lig, und n\u00fcmme schnuuf,", "tokens": ["Und", "wen\u00b7ni", "lig", ",", "und", "n\u00fcm\u00b7me", "schnuuf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADJD", "$,", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und wenn sie \u2019s Schloflied gsunge hen,", "tokens": ["und", "wenn", "sie", "'s", "Schlo\u00b7flied", "gsun\u00b7ge", "hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "NN", "ADJA", "NN", "$,"], "meter": "-+-+++-+-", "measure": "unknown.measure.penta"}, "line.3": {"text": "se sch\u00fcttle sie mer \u2019s Deckbett uf,", "tokens": ["se", "sch\u00fctt\u00b7le", "sie", "mer", "'s", "Deck\u00b7bett", "uf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPER", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "und \u2014 Bh\u00fctdi Gott!", "tokens": ["und", "Bh\u00fct\u00b7di", "Gott", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["KON", "$(", "NE", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.12": {"line.1": {"text": "J schlof derno so sanft wie du,", "tokens": ["J", "schlof", "der\u00b7no", "so", "sanft", "wie", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ADV", "ADJD", "KOKOM", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und h\u00f6r\u2019 im Chilch-Thurn \u2019s Unrueth nit!", "tokens": ["und", "h\u00f6r'", "im", "Chilch\u00b7Thurn", "'s", "Un\u00b7ru\u00b7eth", "nit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PPER", "NN", "PTKNEG", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "mer schlofe, bis am Sunntig fr\u00fceih", "tokens": ["mer", "schlo\u00b7fe", ",", "bis", "am", "Sunn\u00b7tig", "fr\u00fc\u00b7eih"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "PTKA", "ADJD", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "der Morge thaut.", "tokens": ["der", "Mor\u00b7ge", "thaut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.13": {"line.1": {"text": "Und wenn emol der Sunntig tagt,", "tokens": ["Und", "wenn", "e\u00b7mol", "der", "Sunn\u00b7tig", "tagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ART", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und d\u2019 Engel singe \u2019s Morgelied,", "tokens": ["und", "d'", "En\u00b7gel", "sin\u00b7ge", "'s", "Mor\u00b7ge\u00b7lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "VVFIN", "PPER", "NN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "se st\u00f6hn mer mit enanderno uf,", "tokens": ["se", "st\u00f6hn", "mer", "mit", "en\u00b7an\u00b7der\u00b7no", "uf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "erquickt und gsund.", "tokens": ["er\u00b7quickt", "und", "gsund", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.14": {"line.1": {"text": "Und \u2019s stoht e neui Chilche do,", "tokens": ["Und", "'s", "stoht", "e", "neu\u00b7i", "Chil\u00b7che", "do", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "FM", "FM", "FM", "FM", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "hel funklet sie im Morgeroth.", "tokens": ["hel", "funk\u00b7let", "sie", "im", "Mor\u00b7ge\u00b7roth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mer g\u00f6hn, und singen am Altar", "tokens": ["Mer", "g\u00f6hn", ",", "und", "sin\u00b7gen", "am", "Al\u00b7tar"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "KON", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4":