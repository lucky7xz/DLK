{"dta.poem.11552": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Agnes.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Rosenzeit! wie schnell vorbei,", "tokens": ["Ro\u00b7sen\u00b7zeit", "!", "wie", "schnell", "vor\u00b7bei", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWAV", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schnell vorbei,", "tokens": ["Schnell", "vor\u00b7bei", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Bist du doch gegangen!", "tokens": ["Bist", "du", "doch", "ge\u00b7gan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "W\u00e4r' mein Lieb nur blieben treu,", "tokens": ["W\u00e4r'", "mein", "Lieb", "nur", "blie\u00b7ben", "treu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Blieben treu,", "tokens": ["Blie\u00b7ben", "treu", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Sollte mir nicht bangen.", "tokens": ["Soll\u00b7te", "mir", "nicht", "ban\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Um die Ernte wohlgemuth,", "tokens": ["Um", "die", "Ern\u00b7te", "wohl\u00b7ge\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wohlgemuth,", "tokens": ["Wohl\u00b7ge\u00b7muth", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Schnitterinnen singen.", "tokens": ["Schnit\u00b7te\u00b7rin\u00b7nen", "sin\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Aber, ach! mir kranken Blut,", "tokens": ["A\u00b7ber", ",", "ach", "!", "mir", "kran\u00b7ken", "Blut", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ITJ", "$.", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mir kranken Blut,", "tokens": ["Mir", "kran\u00b7ken", "Blut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Will nichts mehr gelingen.", "tokens": ["Will", "nichts", "mehr", "ge\u00b7lin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Schleiche so durch's Wiesenthal,", "tokens": ["Schlei\u00b7che", "so", "durch's", "Wie\u00b7sent\u00b7hal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "So durch's Thal,", "tokens": ["So", "durch's", "Thal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Als im Traum verloren,", "tokens": ["Als", "im", "Traum", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Nach dem Berg, da tausend Mal", "tokens": ["Nach", "dem", "Berg", ",", "da", "tau\u00b7send", "Mal"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "CARD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Tausend Mal,", "tokens": ["Tau\u00b7send", "Mal", ","], "token_info": ["word", "word", "punct"], "pos": ["CARD", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Er mir Treu geschworen.", "tokens": ["Er", "mir", "Treu", "ge\u00b7schwo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Oben auf des H\u00fcgels Rand,", "tokens": ["O\u00b7ben", "auf", "des", "H\u00fc\u00b7gels", "Rand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Abgewandt,", "tokens": ["Ab\u00b7ge\u00b7wandt", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Wein' ich bei der Linde,", "tokens": ["Wein'", "ich", "bei", "der", "Lin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ART", "NE", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "An dem Hut mein Rosenband,", "tokens": ["An", "dem", "Hut", "mein", "Ro\u00b7sen\u00b7band", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Von seiner Hand,", "tokens": ["Von", "sei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Spielet in dem Winde.", "tokens": ["Spie\u00b7let", "in", "dem", "Win\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}