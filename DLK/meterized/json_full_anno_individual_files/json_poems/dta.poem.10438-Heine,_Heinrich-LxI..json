{"dta.poem.10438": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "LxI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Der Traumgott bracht' mich in ein Riesenschlo\u00df,", "tokens": ["Der", "Traum\u00b7gott", "bracht'", "mich", "in", "ein", "Rie\u00b7sen\u00b7schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo schw\u00fcler Zauberduft und Lichterschimmer,", "tokens": ["Wo", "schw\u00fc\u00b7ler", "Zau\u00b7ber\u00b7duft", "und", "Lich\u00b7ter\u00b7schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und bunte Menschenwoge sich ergo\u00df", "tokens": ["Und", "bun\u00b7te", "Men\u00b7schen\u00b7wo\u00b7ge", "sich", "er\u00b7go\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Durch labyrinthisch vielverschlungne Zimmer.", "tokens": ["Durch", "la\u00b7by\u00b7rin\u00b7thisch", "viel\u00b7ver\u00b7schlung\u00b7ne", "Zim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Ausgangspforte sucht der bleiche Tro\u00df,", "tokens": ["Die", "Aus\u00b7gangs\u00b7pfor\u00b7te", "sucht", "der", "blei\u00b7che", "Tro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mit H\u00e4nderingen und mit Angstgewimmer.", "tokens": ["Mit", "H\u00e4n\u00b7de\u00b7rin\u00b7gen", "und", "mit", "Angst\u00b7ge\u00b7wim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Jungfrau'n und Ritter ragen aus der Menge,", "tokens": ["Jung\u00b7frau'n", "und", "Rit\u00b7ter", "ra\u00b7gen", "aus", "der", "Men\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ich selbst bin fortgezogen im Gedr\u00e4nge.", "tokens": ["Ich", "selbst", "bin", "fort\u00b7ge\u00b7zo\u00b7gen", "im", "Ge\u00b7dr\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch pl\u00f6tzlich steh' ich ganz allein, und seh',", "tokens": ["Doch", "pl\u00f6tz\u00b7lich", "steh'", "ich", "ganz", "al\u00b7lein", ",", "und", "seh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADV", "ADV", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und staun', wie schnell die Menge konnt' verschwinden,", "tokens": ["Und", "staun'", ",", "wie", "schnell", "die", "Men\u00b7ge", "konnt'", "ver\u00b7schwin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ADJD", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wandre fort allein, und eil', und geh'", "tokens": ["Und", "wand\u00b7re", "fort", "al\u00b7lein", ",", "und", "eil'", ",", "und", "geh'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PTKVZ", "ADV", "$,", "KON", "VVFIN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Durch die Gem\u00e4cher, die sich seltsam winden.", "tokens": ["Durch", "die", "Ge\u00b7m\u00e4\u00b7cher", ",", "die", "sich", "selt\u00b7sam", "win\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PRF", "ADJD", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Mein Fu\u00df wird Blei, im Herzen Angst und Weh,", "tokens": ["Mein", "Fu\u00df", "wird", "Blei", ",", "im", "Her\u00b7zen", "Angst", "und", "Weh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$,", "APPRART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Verzweifl' ich fast den Ausgang je zu finden.", "tokens": ["Ver\u00b7zweifl'", "ich", "fast", "den", "Aus\u00b7gang", "je", "zu", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Da komm' ich endlich an das letzte Thor;", "tokens": ["Da", "komm'", "ich", "end\u00b7lich", "an", "das", "letz\u00b7te", "Thor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ich will hinaus \u2014 o Gott, wer steht davor!", "tokens": ["Ich", "will", "hin\u00b7aus", "o", "Gott", ",", "wer", "steht", "da\u00b7vor", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APZR", "$(", "FM", "NN", "$,", "PWS", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Es war die Liebste, die am Thore stand,", "tokens": ["Es", "war", "die", "Liebs\u00b7te", ",", "die", "am", "Tho\u00b7re", "stand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Schmerz um die Lippen, Sorge auf der Stirne.", "tokens": ["Schmerz", "um", "die", "Lip\u00b7pen", ",", "Sor\u00b7ge", "auf", "der", "Stir\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Ich soll zur\u00fcckgehn, winkt sie mit der Hand;", "tokens": ["Ich", "soll", "zu\u00b7r\u00fcck\u00b7gehn", ",", "winkt", "sie", "mit", "der", "Hand", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich wei\u00df nicht ob sie warne oder z\u00fcrne.", "tokens": ["Ich", "wei\u00df", "nicht", "ob", "sie", "war\u00b7ne", "o\u00b7der", "z\u00fcr\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "KOUS", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch aus den Augen bricht ein s\u00fc\u00dfer Brand,", "tokens": ["Doch", "aus", "den", "Au\u00b7gen", "bricht", "ein", "s\u00fc\u00b7\u00dfer", "Brand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der mir durchzuckt das Herz und das Gehirne.", "tokens": ["Der", "mir", "durch\u00b7zuckt", "das", "Herz", "und", "das", "Ge\u00b7hir\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wie sie mich ansah, streng und wunderlich,", "tokens": ["Wie", "sie", "mich", "an\u00b7sah", ",", "streng", "und", "wun\u00b7der\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVFIN", "$,", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und doch so liebevoll, erwachte ich.", "tokens": ["Und", "doch", "so", "lie\u00b7be\u00b7voll", ",", "er\u00b7wach\u00b7te", "ich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}