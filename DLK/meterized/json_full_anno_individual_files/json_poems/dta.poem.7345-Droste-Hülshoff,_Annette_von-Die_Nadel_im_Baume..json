{"dta.poem.7345": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Die Nadel im Baume.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Vor Zeiten, ich war schon gro\u00df genug,", "tokens": ["Vor", "Zei\u00b7ten", ",", "ich", "war", "schon", "gro\u00df", "ge\u00b7nug", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PPER", "VAFIN", "ADV", "ADJD", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hatt' die Kinderschuhe vertreten,", "tokens": ["Hatt'", "die", "Kin\u00b7der\u00b7schu\u00b7he", "ver\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Nicht alt war ich, doch eben im Zug'", "tokens": ["Nicht", "alt", "war", "ich", ",", "doch", "e\u00b7ben", "im", "Zug'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "VAFIN", "PPER", "$,", "ADV", "ADV", "APPRART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Zu Sankt Andreas zu beten,", "tokens": ["Zu", "Sankt", "A\u00b7ndre\u00b7as", "zu", "be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Da bin ich gewandelt, Tag f\u00fcr Tag,", "tokens": ["Da", "bin", "ich", "ge\u00b7wan\u00b7delt", ",", "Tag", "f\u00fcr", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$,", "NN", "APPR", "NN", "$,"], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.6": {"text": "Das Feld entlang mit der Kathi;", "tokens": ["Das", "Feld", "ent\u00b7lang", "mit", "der", "Ka\u00b7thi", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPO", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ob etwas Liebes im Wege lag?", "tokens": ["Ob", "et\u00b7was", "Lie\u00b7bes", "im", "We\u00b7ge", "lag", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Und in dem Haideland stand ein Baum,", "tokens": ["Und", "in", "dem", "Hai\u00b7de\u00b7land", "stand", "ein", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "---+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Eine schlanke schm\u00e4chtige Erle,", "tokens": ["Ei\u00b7ne", "schlan\u00b7ke", "schm\u00e4ch\u00b7ti\u00b7ge", "Er\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Da sa\u00dfen wir oft in wachendem Traum,", "tokens": ["Da", "sa\u00b7\u00dfen", "wir", "oft", "in", "wa\u00b7chen\u00b7dem", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die hatte ihr struppiges Nest gebaut,", "tokens": ["Die", "hat\u00b7te", "ihr", "strup\u00b7pi\u00b7ges", "Nest", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+++-+-+", "measure": "zehnsilber"}, "line.5": {"text": "Grad in der schwankenden Krone,", "tokens": ["Grad", "in", "der", "schwan\u00b7ken\u00b7den", "Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Und hat so keck hernieder geschaut", "tokens": ["Und", "hat", "so", "keck", "her\u00b7nie\u00b7der", "ge\u00b7schaut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "PTKVZ", "VVPP"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Wie ein Gr\u00e4flein vom winzigen Throne.", "tokens": ["Wie", "ein", "Gr\u00e4f\u00b7lein", "vom", "win\u00b7zi\u00b7gen", "Thro\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.3": {"line.1": {"text": "Wir kosten so viel und gingen so lang',", "tokens": ["Wir", "kos\u00b7ten", "so", "viel", "und", "gin\u00b7gen", "so", "lang'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "KON", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da\u00df dr\u00fcber der Sommer verflossen;", "tokens": ["Da\u00df", "dr\u00fc\u00b7ber", "der", "Som\u00b7mer", "ver\u00b7flos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PAV", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Dann hie\u00df es: \u201eScheiden, o weh wie bang!\u201c", "tokens": ["Dann", "hie\u00df", "es", ":", "\u201e", "Schei\u00b7den", ",", "o", "weh", "wie", "bang", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "$(", "NN", "$,", "FM", "ADJD", "KOKOM", "ADJD", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Viel Thr\u00e4nen wurden vergossen;", "tokens": ["Viel", "Thr\u00e4\u00b7nen", "wur\u00b7den", "ver\u00b7gos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Die H\u00e4nde hielten wir stumm gepre\u00dft,", "tokens": ["Die", "H\u00e4n\u00b7de", "hiel\u00b7ten", "wir", "stumm", "ge\u00b7pre\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Da zog ich aus flatternder Binde", "tokens": ["Da", "zog", "ich", "aus", "flat\u00b7tern\u00b7der", "Bin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Eine blanke Nadel, und dr\u00fcckte fest", "tokens": ["Ei\u00b7ne", "blan\u00b7ke", "Na\u00b7del", ",", "und", "dr\u00fcck\u00b7te", "fest"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "VVFIN", "PTKVZ"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Sie, fest in die saftige Rinde;", "tokens": ["Sie", ",", "fest", "in", "die", "saf\u00b7ti\u00b7ge", "Rin\u00b7de", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Und drunter merkte ich Tag und Stund',", "tokens": ["Und", "drun\u00b7ter", "merk\u00b7te", "ich", "Tag", "und", "Stund'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dann sind wir f\u00fcrder gezogen,", "tokens": ["Dann", "sind", "wir", "f\u00fcr\u00b7der", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "So kl\u00e4glich schluchzend aus Herzensgrund,", "tokens": ["So", "kl\u00e4g\u00b7lich", "schluch\u00b7zend", "aus", "Her\u00b7zens\u00b7grund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da\u00df schreiend die Merle entflogen;", "tokens": ["Da\u00df", "schrei\u00b7end", "die", "Mer\u00b7le", "ent\u00b7flo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "O junge Seelen sind K\u00f6nigen gleich,", "tokens": ["O", "jun\u00b7ge", "See\u00b7len", "sind", "K\u00f6\u00b7ni\u00b7gen", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VAFIN", "NN", "ADV", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Sie k\u00f6nnen ein Peru vergeuden,", "tokens": ["Sie", "k\u00f6n\u00b7nen", "ein", "Pe\u00b7ru", "ver\u00b7geu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Im braunen Haid, unter'm gr\u00fcnen Zweig,", "tokens": ["Im", "brau\u00b7nen", "Haid", ",", "un\u00b7ter'm", "gr\u00fc\u00b7nen", "Zweig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Ein Peru an Lieben und Leiden.", "tokens": ["Ein", "Pe\u00b7ru", "an", "Lie\u00b7ben", "und", "Lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "Die Jahre verglitten mit schleichendem Gang,", "tokens": ["Die", "Jah\u00b7re", "ver\u00b7glit\u00b7ten", "mit", "schlei\u00b7chen\u00b7dem", "Gang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Verrannen gleich duftiger Wolke,", "tokens": ["Ver\u00b7ran\u00b7nen", "gleich", "duf\u00b7ti\u00b7ger", "Wol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und wieder zog ich das Feld entlang", "tokens": ["Und", "wie\u00b7der", "zog", "ich", "das", "Feld", "ent\u00b7lang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "APPO"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit jungem lustigen Volke;", "tokens": ["Mit", "jun\u00b7gem", "lus\u00b7ti\u00b7gen", "Vol\u00b7ke", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Die schleuderten St\u00e4be, und schrieen \u201eHalloh!\u201c", "tokens": ["Die", "schleu\u00b7der\u00b7ten", "St\u00e4\u00b7be", ",", "und", "schri\u00b7een", "\u201e", "Hal\u00b7loh", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "VVFIN", "$(", "ITJ", "$.", "$("], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Die sprudelten Witze wie Schlossen,", "tokens": ["Die", "spru\u00b7del\u00b7ten", "Wit\u00b7ze", "wie", "Schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KOKOM", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Mir ward's im Herzen gar keck und froh,", "tokens": ["Mir", "ward's", "im", "Her\u00b7zen", "gar", "keck", "und", "froh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Muthwillig wie unter Genossen.", "tokens": ["Mut\u00b7hwil\u00b7lig", "wie", "un\u00b7ter", "Ge\u00b7nos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "APPR", "NN", "$."], "meter": "++--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Da pl\u00f6tzlich rauscht' es im dichten Gezweig,", "tokens": ["Da", "pl\u00f6tz\u00b7lich", "rauscht'", "es", "im", "dich\u00b7ten", "Ge\u00b7zweig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u201eeine Merle\u201c, rief's, \u201eeine Merle!\u201c", "tokens": ["\u201e", "ei\u00b7ne", "Mer\u00b7le", "\u201c", ",", "rie\u00b7f's", ",", "\u201e", "ei\u00b7ne", "Mer\u00b7le", "!", "\u201c"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "$(", "$,", "VVFIN", "$,", "$(", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ich fuhr empor \u2014 ward ich etwa bleich?", "tokens": ["Ich", "fuhr", "em\u00b7por", "ward", "ich", "et\u00b7wa", "bleich", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$(", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich stand an der alternden Erle;", "tokens": ["Ich", "stand", "an", "der", "al\u00b7tern\u00b7den", "Er\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Und r\u00fcckw\u00e4rts zog mir's den Schleier vom Haar,", "tokens": ["Und", "r\u00fcck\u00b7w\u00e4rts", "zog", "mir's", "den", "Schlei\u00b7er", "vom", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "NE", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ach Gott, ich ergl\u00fchte wie Flamme,", "tokens": ["Ach", "Gott", ",", "ich", "er\u00b7gl\u00fch\u00b7te", "wie", "Flam\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "PPER", "VVFIN", "KOKOM", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Als ich sah, da\u00df die alte Nadel es war,", "tokens": ["Als", "ich", "sah", ",", "da\u00df", "die", "al\u00b7te", "Na\u00b7del", "es", "war", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KOUS", "ART", "ADJA", "NN", "PPER", "VAFIN", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Meine rostige Nadel im Stamme!", "tokens": ["Mei\u00b7ne", "ros\u00b7ti\u00b7ge", "Na\u00b7del", "im", "Stam\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Drauf hab' ich genommen ganz still in Schau", "tokens": ["Drauf", "hab'", "ich", "ge\u00b7nom\u00b7men", "ganz", "still", "in", "Schau"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "VVPP", "ADV", "ADJD", "APPR", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die Inschrift, zu eigenem Frommen,", "tokens": ["Die", "In\u00b7schrift", ",", "zu", "ei\u00b7ge\u00b7nem", "From\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und f\u00fchlte dann pl\u00f6tzlich, es steige der Thau,", "tokens": ["Und", "f\u00fchl\u00b7te", "dann", "pl\u00f6tz\u00b7lich", ",", "es", "stei\u00b7ge", "der", "Thau", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und werde mir schwerlich bekommen.", "tokens": ["Und", "wer\u00b7de", "mir", "schwer\u00b7lich", "be\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphi