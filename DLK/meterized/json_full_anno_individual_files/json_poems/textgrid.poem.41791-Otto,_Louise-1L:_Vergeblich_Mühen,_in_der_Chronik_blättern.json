{"textgrid.poem.41791": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vergeblich M\u00fchen, in der Chronik bl\u00e4ttern", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vergeblich M\u00fchen, in der Chronik bl\u00e4ttern", "tokens": ["Ver\u00b7geb\u00b7lich", "M\u00fc\u00b7hen", ",", "in", "der", "Chro\u00b7nik", "bl\u00e4t\u00b7tern"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "$,", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Drin Deutschlands beste Namen sind gebucht \u2013", "tokens": ["Drin", "Deutschlands", "bes\u00b7te", "Na\u00b7men", "sind", "ge\u00b7bucht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "ADJA", "NN", "VAFIN", "VVPP", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wohl steht da \u00bbFrauenlob\u00ab mit gro\u00dfen Lettern,", "tokens": ["Wohl", "steht", "da", "\u00bb", "Frau\u00b7en\u00b7lob", "\u00ab", "mit", "gro\u00b7\u00dfen", "Let\u00b7tern", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$(", "NN", "$(", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch weiter find' ich nicht, was ich gesucht:", "tokens": ["Doch", "wei\u00b7ter", "find'", "ich", "nicht", ",", "was", "ich", "ge\u00b7sucht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PTKNEG", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie er gelebt, der also sch\u00f6n gesungen", "tokens": ["Wie", "er", "ge\u00b7lebt", ",", "der", "al\u00b7so", "sch\u00f6n", "ge\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVPP", "$,", "PRELS", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Den Meistersang, das deutsche Minnelied?", "tokens": ["Den", "Meis\u00b7ter\u00b7sang", ",", "das", "deut\u00b7sche", "Min\u00b7ne\u00b7lied", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sein Leben in Vergessenheit geriet,", "tokens": ["Sein", "Le\u00b7ben", "in", "Ver\u00b7ges\u00b7sen\u00b7heit", "ge\u00b7riet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Indes sein Lied doch bis zu uns gedrungen,", "tokens": ["In\u00b7des", "sein", "Lied", "doch", "bis", "zu", "uns", "ge\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADV", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ein Klang der die Jahrhunderte durchzieht.", "tokens": ["Ein", "Klang", "der", "die", "Jahr\u00b7hun\u00b7der\u00b7te", "durch\u00b7zieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ART", "NN", "VVFIN", "$."], "meter": "-+--+----+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Von seinem Tod allein wird uns berichtet:", "tokens": ["Von", "sei\u00b7nem", "Tod", "al\u00b7lein", "wird", "uns", "be\u00b7rich\u00b7tet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Er starb zu Mainz \u2013 und der der Frauen Lob", "tokens": ["Er", "starb", "zu", "Mainz", "\u2013", "und", "der", "der", "Frau\u00b7en", "Lob"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NE", "$(", "KON", "ART", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In holden Versen also sch\u00f6n gedichtet,", "tokens": ["In", "hol\u00b7den", "Ver\u00b7sen", "al\u00b7so", "sch\u00f6n", "ge\u00b7dich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Frauen Dank auf zarte Schultern hob:", "tokens": ["Der", "Frau\u00b7en", "Dank", "auf", "zar\u00b7te", "Schul\u00b7tern", "hob", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es ward sein Sarg von Frauen nur getragen", "tokens": ["Es", "ward", "sein", "Sarg", "von", "Frau\u00b7en", "nur", "ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "APPR", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Geschm\u00fcckt mit Blumen und mit Eichenlaub,", "tokens": ["Ge\u00b7schm\u00fcckt", "mit", "Blu\u00b7men", "und", "mit", "Ei\u00b7chen\u00b7laub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Kein Aug' blieb trocken und kein Ohr blieb taub,", "tokens": ["Kein", "Aug'", "blieb", "tro\u00b7cken", "und", "kein", "Ohr", "blieb", "taub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADJD", "KON", "PIAT", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Bei ihren Trauers\u00e4ngen, ihren Klagen;", "tokens": ["Bei", "ih\u00b7ren", "Trau\u00b7er\u00b7s\u00e4n\u00b7gen", ",", "ih\u00b7ren", "Kla\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Im hohen Dom ruht noch des S\u00e4ngers Staub.", "tokens": ["Im", "ho\u00b7hen", "Dom", "ruht", "noch", "des", "S\u00e4n\u00b7gers", "Staub", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wohl mag ich solchen edlen Landsmann preisen,", "tokens": ["Wohl", "mag", "ich", "sol\u00b7chen", "ed\u00b7len", "Lands\u00b7mann", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der einst die deutschen Schwestern so geehrt,", "tokens": ["Der", "einst", "die", "deut\u00b7schen", "Schwes\u00b7tern", "so", "ge\u00b7ehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und dessen Namen: \u00bbHeinerich von Mei\u00dfen\u00ab", "tokens": ["Und", "des\u00b7sen", "Na\u00b7men", ":", "\u00bb", "Hei\u00b7ne\u00b7rich", "von", "Mei\u00b7\u00dfen", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "$.", "$(", "NE", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu \u00bbHeinrich Frauenlob\u00ab die Zeit verkl\u00e4rt.", "tokens": ["Zu", "\u00bb", "Hein\u00b7rich", "Frau\u00b7en\u00b7lob", "\u00ab", "die", "Zeit", "ver\u00b7kl\u00e4rt", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$(", "NE", "NN", "$(", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nach seinem Schicksal gilt es nicht zu fragen,", "tokens": ["Nach", "sei\u00b7nem", "Schick\u00b7sal", "gilt", "es", "nicht", "zu", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Es ist erz\u00e4hlt von seinem Leichenzug:", "tokens": ["Es", "ist", "er\u00b7z\u00e4hlt", "von", "sei\u00b7nem", "Lei\u00b7chen\u00b7zug", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ein Dichter war er, dem im Busen schlug", "tokens": ["Ein", "Dich\u00b7ter", "war", "er", ",", "dem", "im", "Bu\u00b7sen", "schlug"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "$,", "PRELS", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ein flammend Herz, das ein Eliaswagen,", "tokens": ["Ein", "flam\u00b7mend", "Herz", ",", "das", "ein", "E\u00b7li\u00b7as\u00b7wa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "PRELS", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Die Erde feiernd, doch zum Himmel trug.", "tokens": ["Die", "Er\u00b7de", "fei\u00b7ernd", ",", "doch", "zum", "Him\u00b7mel", "trug", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wer so der Frauen Huldigung empfangen,", "tokens": ["Wer", "so", "der", "Frau\u00b7en", "Hul\u00b7di\u00b7gung", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hat auch verstanden edler Frauen Herz,", "tokens": ["Hat", "auch", "ver\u00b7stan\u00b7den", "ed\u00b7ler", "Frau\u00b7en", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJA", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Verstanden einer Frauenseele Bangen", "tokens": ["Ver\u00b7stan\u00b7den", "ei\u00b7ner", "Frau\u00b7en\u00b7see\u00b7le", "Ban\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und tief gef\u00fchlt der Liebe Lust und Schmerz", "tokens": ["Und", "tief", "ge\u00b7f\u00fchlt", "der", "Lie\u00b7be", "Lust", "und", "Schmerz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVPP", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der wandelte in Fr\u00f6mmigkeit und Sitte,", "tokens": ["Der", "wan\u00b7del\u00b7te", "in", "Fr\u00f6m\u00b7mig\u00b7keit", "und", "Sit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der war ein freier Mann, ein starker Held,", "tokens": ["Der", "war", "ein", "frei\u00b7er", "Mann", ",", "ein", "star\u00b7ker", "Held", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Bereit zum Kampfe wider eine Welt,", "tokens": ["Be\u00b7reit", "zum", "Kamp\u00b7fe", "wi\u00b7der", "ei\u00b7ne", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Sang gerne auch in zarter Frauen Mitte,", "tokens": ["Sang", "ger\u00b7ne", "auch", "in", "zar\u00b7ter", "Frau\u00b7en", "Mit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.9": {"text": "Sein Lied am Herde wie im Kriegerzelt.", "tokens": ["Sein", "Lied", "am", "Her\u00b7de", "wie", "im", "Krie\u00b7ger\u00b7zelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "KOKOM", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Der schlang um zarte Stirnen Rosenkronen,", "tokens": ["Der", "schlang", "um", "zar\u00b7te", "Stir\u00b7nen", "Ro\u00b7sen\u00b7kro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erwarb sich selbst den Lorber in der Schlacht,", "tokens": ["Er\u00b7warb", "sich", "selbst", "den", "Lor\u00b7ber", "in", "der", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von holden Frauen lie\u00df er gern sich lohnen,", "tokens": ["Von", "hol\u00b7den", "Frau\u00b7en", "lie\u00df", "er", "gern", "sich", "loh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ADV", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ob er gek\u00e4mpfet, ob im Lied gedacht.", "tokens": ["Ob", "er", "ge\u00b7k\u00e4mp\u00b7fet", ",", "ob", "im", "Lied", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$,", "KOUS", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So zog durch's Leben er im Dienst der Minne,", "tokens": ["So", "zog", "durch's", "Le\u00b7ben", "er", "im", "Dienst", "der", "Min\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "PPER", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und seine Herrin hat ihn hoch begl\u00fcckt,", "tokens": ["Und", "sei\u00b7ne", "Her\u00b7rin", "hat", "ihn", "hoch", "be\u00b7gl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ja ihn zu aller Frauen Preis entz\u00fcckt,", "tokens": ["Ja", "ihn", "zu", "al\u00b7ler", "Frau\u00b7en", "Preis", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "APPR", "PIAT", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Sie, die so treu und von so hohem Sinne,", "tokens": ["Sie", ",", "die", "so", "treu", "und", "von", "so", "ho\u00b7hem", "Sin\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ADV", "ADJD", "KON", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Sein Lied mit holder Anmut ausgeschm\u00fcckt.", "tokens": ["Sein", "Lied", "mit", "hol\u00b7der", "An\u00b7mut", "aus\u00b7ge\u00b7schm\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Maguntia! ich knie in deinem Dome,", "tokens": ["Ma\u00b7gun\u00b7tia", "!", "ich", "knie", "in", "dei\u00b7nem", "Do\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "An Deinem Grabe, Heinrich Frauenlob,", "tokens": ["An", "Dei\u00b7nem", "Gra\u00b7be", ",", "Hein\u00b7rich", "Frau\u00b7en\u00b7lob", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "NE", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich gr\u00fc\u00dfe Dich am edlen Rheinesstrome,", "tokens": ["Ich", "gr\u00fc\u00b7\u00dfe", "Dich", "am", "ed\u00b7len", "Rhei\u