{"dta.poem.4130": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Januar.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein warmes Haus, erhellt von tausend Kerzen,", "tokens": ["Ein", "war\u00b7mes", "Haus", ",", "er\u00b7hellt", "von", "tau\u00b7send", "Ker\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bewohnt von bunten Spielen, T\u00e4nzen, Scherzen,", "tokens": ["Be\u00b7wohnt", "von", "bun\u00b7ten", "Spie\u00b7len", ",", "T\u00e4n\u00b7zen", ",", "Scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von Amoretten auch, die Pfeile senden.", "tokens": ["Von", "A\u00b7mo\u00b7ret\u00b7ten", "auch", ",", "die", "Pfei\u00b7le", "sen\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sie flattern auf und ab an allen Enden,", "tokens": ["Sie", "flat\u00b7tern", "auf", "und", "ab", "an", "al\u00b7len", "En\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "PTKVZ", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Jungfrau schaut besorgt nach ihrem Herzen,", "tokens": ["Die", "Jung\u00b7frau", "schaut", "be\u00b7sorgt", "nach", "ih\u00b7rem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Andre schon nach Einem, der den Schmerzen", "tokens": ["Die", "And\u00b7re", "schon", "nach", "Ei\u00b7nem", ",", "der", "den", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PIS", "ADV", "APPR", "PIS", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Wunde m\u00f6chte s\u00fc\u00dfen Balsam spenden.", "tokens": ["Der", "Wun\u00b7de", "m\u00f6ch\u00b7te", "s\u00fc\u00b7\u00dfen", "Bal\u00b7sam", "spen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Als h\u00fclfreich hab' ich immer dich erfunden,", "tokens": ["Als", "h\u00fclf\u00b7reich", "hab'", "ich", "im\u00b7mer", "dich", "er\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VAFIN", "PPER", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vor Allem, wo es gilt den schwachen Sch\u00f6nen,", "tokens": ["Vor", "Al\u00b7lem", ",", "wo", "es", "gilt", "den", "schwa\u00b7chen", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PWAV", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Drum, denk' ich, wird sie nicht bis morgen klagen.", "tokens": ["Drum", ",", "denk'", "ich", ",", "wird", "sie", "nicht", "bis", "mor\u00b7gen", "kla\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "VVFIN", "PPER", "$,", "VAFIN", "PPER", "PTKNEG", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Bald sind verrauscht des Festes hei\u00dfe Stunden,", "tokens": ["Bald", "sind", "ver\u00b7rauscht", "des", "Fes\u00b7tes", "hei\u00b7\u00dfe", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schon h\u00f6r' ich Hufschlag vor dem Thore dr\u00f6hnen:", "tokens": ["Schon", "h\u00f6r'", "ich", "Huf\u00b7schlag", "vor", "dem", "Tho\u00b7re", "dr\u00f6h\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Reich' ihr den Arm und f\u00fchre sie zum Wagen!", "tokens": ["Reich'", "ihr", "den", "Arm", "und", "f\u00fch\u00b7re", "sie", "zum", "Wa\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "KON", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}