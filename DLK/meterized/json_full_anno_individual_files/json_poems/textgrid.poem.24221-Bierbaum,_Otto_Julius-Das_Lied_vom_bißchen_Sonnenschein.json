{"textgrid.poem.24221": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Das Lied vom bi\u00dfchen Sonnenschein", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein bi\u00dfchen Sonnenschein", "tokens": ["Es", "ist", "ein", "bi\u00df\u00b7chen", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf meinen Weg gefallen,", "tokens": ["Auf", "mei\u00b7nen", "Weg", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da h\u00f6rt ich gleich des Gl\u00fccks Schalmein", "tokens": ["Da", "h\u00f6rt", "ich", "gleich", "des", "Gl\u00fccks", "Schal\u00b7mein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus allen Himmeln hallen", "tokens": ["Aus", "al\u00b7len", "Him\u00b7meln", "hal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und glaubte gleich,", "tokens": ["Und", "glaub\u00b7te", "gleich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Das Himmelreich,", "tokens": ["Das", "Him\u00b7mel\u00b7reich", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Das Himmelreich sei mein.", "tokens": ["Das", "Him\u00b7mel\u00b7reich", "sei", "mein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Sonnenschein ist weggegl\u00e4nzt,", "tokens": ["Der", "Son\u00b7nen\u00b7schein", "ist", "weg\u00b7ge\u00b7gl\u00e4nzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er galt nicht meinem Wege,", "tokens": ["Er", "galt", "nicht", "mei\u00b7nem", "We\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich habe mich zu fr\u00fch bekr\u00e4nzt,", "tokens": ["Ich", "ha\u00b7be", "mich", "zu", "fr\u00fch", "be\u00b7kr\u00e4nzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKA", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nun kreischt des Grames S\u00e4ge:", "tokens": ["Nun", "kreischt", "des", "Gra\u00b7mes", "S\u00e4\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Winter naht,", "tokens": ["Der", "Win\u00b7ter", "naht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Der Potentat,", "tokens": ["Der", "Po\u00b7ten\u00b7tat", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Es hat sich ausgelenzt.", "tokens": ["Es", "hat", "sich", "aus\u00b7ge\u00b7lenzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "