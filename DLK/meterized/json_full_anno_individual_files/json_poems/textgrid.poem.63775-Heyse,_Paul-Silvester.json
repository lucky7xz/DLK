{"textgrid.poem.63775": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Silvester", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie feierten Silvester im Ges\u00f9", "tokens": ["Sie", "fei\u00b7er\u00b7ten", "Sil\u00b7ves\u00b7ter", "im", "Ge\u00b7s\u00f9"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "APPRART", "NN"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Mit Kerzenglanz und festlichem Gepr\u00e4nge;", "tokens": ["Mit", "Ker\u00b7zen\u00b7glanz", "und", "fest\u00b7li\u00b7chem", "Ge\u00b7pr\u00e4n\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Orgel dr\u00f6hnt', es brausten Chorges\u00e4nge \u2013", "tokens": ["Die", "Or\u00b7gel", "dr\u00f6hnt'", ",", "es", "braus\u00b7ten", "Chor\u00b7ge\u00b7s\u00e4n\u00b7ge", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mir ging's zu bunt und laut und lustig zu.", "tokens": ["Mir", "ging's", "zu", "bunt", "und", "laut", "und", "lus\u00b7tig", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKA", "ADJD", "KON", "ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dem b\u00f6sen Jahre w\u00fcnscht' ich gute Ruh", "tokens": ["Dem", "b\u00f6\u00b7sen", "Jah\u00b7re", "w\u00fcnscht'", "ich", "gu\u00b7te", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und floh hinaus und wand mich durch die Menge", "tokens": ["Und", "floh", "hin\u00b7aus", "und", "wand", "mich", "durch", "die", "Men\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKVZ", "KON", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zum Kapitol hinan die sanften H\u00e4nge,", "tokens": ["Zum", "Ka\u00b7pi\u00b7tol", "hi\u00b7nan", "die", "sanf\u00b7ten", "H\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In d\u00fcstrem Mut. Wohl hatt' ich Grund dazu.", "tokens": ["In", "d\u00fcst\u00b7rem", "Mut", ".", "Wohl", "hatt'", "ich", "Grund", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "ADV", "VAFIN", "PPER", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da sah ich, eng im K\u00e4fig eingegittert,", "tokens": ["Da", "sah", "ich", ",", "eng", "im", "K\u00e4\u00b7fig", "ein\u00b7ge\u00b7git\u00b7tert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die hagre W\u00f6lfin neidisch mich be\u00e4ugen,", "tokens": ["Die", "hag\u00b7re", "W\u00f6l\u00b7fin", "nei\u00b7disch", "mich", "be\u00b7\u00e4u\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als spr\u00e4ch sie: Du bist frei und kannst noch klagen?", "tokens": ["Als", "spr\u00e4ch", "sie", ":", "Du", "bist", "frei", "und", "kannst", "noch", "kla\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "$.", "PPER", "VAFIN", "ADJD", "KON", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sieh mich! Ich werd als Wappentier gef\u00fcttert!", "tokens": ["Sieh", "mich", "!", "Ich", "werd", "als", "Wap\u00b7pen\u00b7tier", "ge\u00b7f\u00fct\u00b7tert", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$.", "PPER", "VAFIN", "KOKOM", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das ist der Dank, wenn Zwillinge wir s\u00e4ugen", "tokens": ["Das", "ist", "der", "Dank", ",", "wenn", "Zwil\u00b7lin\u00b7ge", "wir", "s\u00e4u\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "KOUS", "NN", "PPER", "VVFIN"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und gegen Menschen menschlich uns betragen!", "tokens": ["Und", "ge\u00b7gen", "Men\u00b7schen", "menschlich", "uns", "be\u00b7tra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Sie feierten Silvester im Ges\u00f9", "tokens": ["Sie", "fei\u00b7er\u00b7ten", "Sil\u00b7ves\u00b7ter", "im", "Ge\u00b7s\u00f9"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "APPRART", "NN"], "meter": "-+---+-+-+", "measure": "dactylic.init"},