{"dta.poem.7596": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "La\u00df nur den tollen Spuk der Zeit vor\u00fcberflirren!", "tokens": ["La\u00df", "nur", "den", "tol\u00b7len", "Spuk", "der", "Zeit", "vor\u00b7\u00fc\u00b7berf\u00b7lir\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ergetzen kann er dich, er kann dich nicht verwirren.", "tokens": ["Er\u00b7get\u00b7zen", "kann", "er", "dich", ",", "er", "kann", "dich", "nicht", "ver\u00b7wir\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PRF", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch wenn dem Schwindel trotzt dein Geist mit fester Stirne;", "tokens": ["Doch", "wenn", "dem", "Schwin\u00b7del", "trotzt", "dein", "Geist", "mit", "fes\u00b7ter", "Stir\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bedenke da\u00df es gibt auch schw\u00e4chere Gehirne.", "tokens": ["Be\u00b7den\u00b7ke", "da\u00df", "es", "gibt", "auch", "schw\u00e4\u00b7che\u00b7re", "Ge\u00b7hir\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOUS", "PPER", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Den Wirbel mehre nicht, worin sie trunken drehn;", "tokens": ["Den", "Wir\u00b7bel", "meh\u00b7re", "nicht", ",", "wo\u00b7rin", "sie", "trun\u00b7ken", "drehn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zeig' ihnen ehr den Punkt, worauf man fest kann stehn.", "tokens": ["Zeig'", "ih\u00b7nen", "ehr", "den", "Punkt", ",", "wo\u00b7rauf", "man", "fest", "kann", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "ART", "NN", "$,", "PWAV", "PIS", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}