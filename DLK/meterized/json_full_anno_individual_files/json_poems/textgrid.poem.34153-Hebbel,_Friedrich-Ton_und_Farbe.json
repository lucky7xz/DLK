{"textgrid.poem.34153": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Ton und Farbe", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo die Natur den Ton verleiht, da versagt sie die Farbe,", "tokens": ["Wo", "die", "Na\u00b7tur", "den", "Ton", "ver\u00b7leiht", ",", "da", "ver\u00b7sagt", "sie", "die", "Far\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wo sie die Farbe gew\u00e4hrt, weigert sie immer den Ton.", "tokens": ["Wo", "sie", "die", "Far\u00b7be", "ge\u00b7w\u00e4hrt", ",", "wei\u00b7gert", "sie", "im\u00b7mer", "den", "Ton", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVPP", "$,", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Denkt der Nachtigall und denkt des Flamingo, so seht ihr's;", "tokens": ["Denkt", "der", "Nach\u00b7ti\u00b7gall", "und", "denkt", "des", "Fla\u00b7min\u00b7go", ",", "so", "seht", "ih\u00b7r's", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "PIS", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Aber das gleiche Gesetz waltet im Reiche der Kunst.", "tokens": ["A\u00b7ber", "das", "glei\u00b7che", "Ge\u00b7setz", "wal\u00b7tet", "im", "Rei\u00b7che", "der", "Kunst", "."], "token_info": ["word", "word", "wor