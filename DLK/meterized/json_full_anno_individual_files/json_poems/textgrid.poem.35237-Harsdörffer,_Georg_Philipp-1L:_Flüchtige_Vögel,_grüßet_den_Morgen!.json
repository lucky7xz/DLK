{"textgrid.poem.35237": {"metadata": {"author": {"name": "Harsd\u00f6rffer, Georg Philipp", "birth": "N.A.", "death": "N.A."}, "title": "1L: Fl\u00fcchtige V\u00f6gel, gr\u00fc\u00dfet den Morgen!", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fl\u00fcchtige V\u00f6gel, gr\u00fc\u00dfet den Morgen!", "tokens": ["Fl\u00fcch\u00b7ti\u00b7ge", "V\u00f6\u00b7gel", ",", "gr\u00fc\u00b7\u00dfet", "den", "Mor\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wecket der Menschen t\u00e4gliche Sorgen!", "tokens": ["We\u00b7cket", "der", "Men\u00b7schen", "t\u00e4g\u00b7li\u00b7che", "Sor\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Singet und klinget dem H\u00f6chsten ein Lied,", "tokens": ["Sin\u00b7get", "und", "klin\u00b7get", "dem", "H\u00f6chs\u00b7ten", "ein", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Welcher uns schenket Segen und Fried'!", "tokens": ["Wel\u00b7cher", "uns", "schen\u00b7ket", "Se\u00b7gen", "und", "Fried'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Danket dem Herren, lobet ihn alle,", "tokens": ["Dan\u00b7ket", "dem", "Her\u00b7ren", ",", "lo\u00b7bet", "ihn", "al\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "PPER", "PIS", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Stimmet mit gleich erhabenem Schalle!", "tokens": ["Stim\u00b7met", "mit", "gleich", "er\u00b7ha\u00b7be\u00b7nem", "Schal\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Echo der Th\u00e4ler gegen euch halle!", "tokens": ["E\u00b7cho", "der", "Th\u00e4\u00b7ler", "ge\u00b7gen", "euch", "hal\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Nachtigall, f\u00fchre der V\u00f6gelein Reihen!", "tokens": ["Nach\u00b7ti\u00b7gall", ",", "f\u00fch\u00b7re", "der", "V\u00f6\u00b7ge\u00b7lein", "Rei\u00b7hen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "T\u00f6ne, wann Andere freien im Maien!", "tokens": ["T\u00f6\u00b7ne", ",", "wann", "An\u00b7de\u00b7re", "frei\u00b7en", "im", "Mai\u00b7en", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PIS", "VVFIN", "APPRART", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Schwinge dich h\u00f6her, liebliche Lerch',", "tokens": ["Schwin\u00b7ge", "dich", "h\u00f6\u00b7her", ",", "lieb\u00b7li\u00b7che", "Ler\u00b7ch'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Z\u00e4hle d