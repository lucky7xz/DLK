{"textgrid.poem.51155": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "7. Ich halte dich in meinem Arm", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich halte dich in meinem Arm, du h\u00e4ltst die Rose zart,", "tokens": ["Ich", "hal\u00b7te", "dich", "in", "mei\u00b7nem", "Arm", ",", "du", "h\u00e4ltst", "die", "Ro\u00b7se", "zart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Und eine junge Biene tief in sich die Rose wahrt;", "tokens": ["Und", "ei\u00b7ne", "jun\u00b7ge", "Bie\u00b7ne", "tief", "in", "sich", "die", "Ro\u00b7se", "wahrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADJD", "APPR", "PRF", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "So reihen wir uns perlenhaft an ", "tokens": ["So", "rei\u00b7hen", "wir", "uns", "per\u00b7len\u00b7haft", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJD", "PTKVZ"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "So freun wir uns, wie Blatt an Blatt sich an der Rose schart.", "tokens": ["So", "freun", "wir", "uns", ",", "wie", "Blatt", "an", "Blatt", "sich", "an", "der", "Ro\u00b7se", "schart", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "PWAV", "NN", "APPR", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Und gl\u00fcht mein Ku\u00df auf deinem Mund, so zuckt die Flammenspur", "tokens": ["Und", "gl\u00fcht", "mein", "Ku\u00df", "auf", "dei\u00b7nem", "Mund", ",", "so", "zuckt", "die", "Flam\u00b7men\u00b7spur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "Bis in der Biene Herz, das sich dem Kelch der Rose paart!", "tokens": ["Bis", "in", "der", "Bie\u00b7ne", "Herz", ",", "das", "sich", "dem", "Kelch", "der", "Ro\u00b7se", "paart", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$,", "PRELS", "PRF", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.2": {"line.1": {"text": "Ich halte dich in meinem Arm, du h\u00e4ltst die Rose zart,", "tokens": ["Ich", "hal\u00b7te", "dich", "in", "mei\u00b7nem", "Arm", ",", "du", "h\u00e4ltst", "die", "Ro\u00b7se", "zart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Und eine junge Biene tief in sich die Rose wahrt;", "tokens": ["Und", "ei\u00b7ne", "jun\u00b7ge", "Bie\u00b7ne", "tief", "in", "sich", "die", "Ro\u00b7se", "wahrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADJD", "APPR", "PRF", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+", "