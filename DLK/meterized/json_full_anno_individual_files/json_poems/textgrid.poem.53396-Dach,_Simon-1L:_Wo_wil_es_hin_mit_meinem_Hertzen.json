{"textgrid.poem.53396": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wo wil es hin mit meinem Hertzen", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo wil es hin mit meinem Hertzen", "tokens": ["Wo", "wil", "es", "hin", "mit", "mei\u00b7nem", "Hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem merklich alle Kr\u00e4fft' entgehn?", "tokens": ["Dem", "merk\u00b7lich", "al\u00b7le", "Kr\u00e4fft'", "ent\u00b7gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich sehe nichts in diesen Schmertzen", "tokens": ["Ich", "se\u00b7he", "nichts", "in", "die\u00b7sen", "Schmert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als Tod und Grawen vor mir stehn:", "tokens": ["Als", "Tod", "und", "Gra\u00b7wen", "vor", "mir", "stehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nichts ist zu hoffen, nichts zu werben,", "tokens": ["Nichts", "ist", "zu", "hof\u00b7fen", ",", "nichts", "zu", "wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PTKZU", "VVINF", "$,", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Beschick dein Hau\u00df, Mensch, du must sterben.", "tokens": ["Be\u00b7schick", "dein", "Hau\u00df", ",", "Mensch", ",", "du", "must", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "$,", "NN", "$,", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wo la\u00df ich mich nach diesem Leben?", "tokens": ["Wo", "la\u00df", "ich", "mich", "nach", "die\u00b7sem", "Le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVIMP", "PPER", "PRF", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Leib geh\u00f6rt der Erden zu,", "tokens": ["Der", "Leib", "ge\u00b7h\u00f6rt", "der", "Er\u00b7den", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der sey er wieder \u00fcbergeben,", "tokens": ["Der", "sey", "er", "wie\u00b7der", "\u00fc\u00b7ber\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Daselbst empfind er seine Ruh,", "tokens": ["Da\u00b7selbst", "emp\u00b7find", "er", "sei\u00b7ne", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn ihn die W\u00fcrme gleich verzehren,", "tokens": ["Wenn", "ihn", "die", "W\u00fcr\u00b7me", "gleich", "ver\u00b7zeh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich kan es jhnen doch nicht wehren.", "tokens": ["Ich", "kan", "es", "jh\u00b7nen", "doch", "nicht", "weh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wo wird mein' arme Seele bleiben?", "tokens": ["Wo", "wird", "mein'", "ar\u00b7me", "See\u00b7le", "blei\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die sieht durch eine breite Bahn", "tokens": ["Die", "sieht", "durch", "ei\u00b7ne", "brei\u00b7te", "Bahn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die in der Hellen Abgrund treiben", "tokens": ["Die", "in", "der", "Hel\u00b7len", "Ab\u00b7grund", "trei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die b\u00f6ses busselos gethan,", "tokens": ["Die", "b\u00f6\u00b7ses", "bus\u00b7se\u00b7los", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Christo hie ergeben waren", "tokens": ["Die", "Chris\u00b7to", "hie", "er\u00b7ge\u00b7ben", "wa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NE", "ADV", "VVPP", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den engen Steig gen Himmel fahren.", "tokens": ["Den", "en\u00b7gen", "Steig", "gen", "Him\u00b7mel", "fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Gott der du anfangs meine Seele", "tokens": ["Gott", "der", "du", "an\u00b7fangs", "mei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "PPER", "ADV", "PPOSAT", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Tieff unter m\u00fctterlichem Scho\u00df", "tokens": ["Tieff", "un\u00b7ter", "m\u00fct\u00b7ter\u00b7li\u00b7chem", "Scho\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gesenckt in dieses Leibes H\u00f6le", "tokens": ["Ge\u00b7senckt", "in", "die\u00b7ses", "Lei\u00b7bes", "H\u00f6\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "APPR", "PDAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd machst sie jetzt auch wieder lo\u00df,", "tokens": ["Vnd", "machst", "sie", "jetzt", "auch", "wie\u00b7der", "lo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Weil wir dich Tod und Leben sehen", "tokens": ["Weil", "wir", "dich", "Tod", "und", "Le\u00b7ben", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Stets auff gerechten H\u00e4nden drehen,", "tokens": ["Stets", "auff", "ge\u00b7rech\u00b7ten", "H\u00e4n\u00b7den", "dre\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Vergib und la\u00df seyn ungerochen", "tokens": ["Ver\u00b7gib", "und", "la\u00df", "seyn", "un\u00b7ge\u00b7ro\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "KON", "VVIMP", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch deines Sohnes Blut und Tod", "tokens": ["Durch", "dei\u00b7nes", "Soh\u00b7nes", "Blut", "und", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die\u00df was ich hie und da verbrochen,", "tokens": ["Die\u00df", "was", "ich", "hie", "und", "da", "ver\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRELS", "PPER", "ADV", "KON", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sieh meines Hertzens grosse Noht,", "tokens": ["Sieh", "mei\u00b7nes", "Hert\u00b7zens", "gros\u00b7se", "Noht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vnd mein ge\u00e4ngstetes Gewissen,", "tokens": ["Vnd", "mein", "ge\u00b7\u00e4ngs\u00b7te\u00b7tes", "Ge\u00b7wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das Rew und Busse gantz zerrissen.", "tokens": ["Das", "Rew", "und", "Bus\u00b7se", "gantz", "zer\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wend ab des Sathans List und Pfeile", "tokens": ["Wend", "ab", "des", "Sa\u00b7thans", "List", "und", "Pfei\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "NE", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vnd thu ihm starcken Widerstandt,", "tokens": ["Vnd", "thu", "ihm", "star\u00b7cken", "Wi\u00b7der\u00b7standt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vnd weil ich jetzt von hinnen eile,", "tokens": ["Vnd", "weil", "ich", "jetzt", "von", "hin\u00b7nen", "ei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nimm meinen Geist in deine Hand,", "tokens": ["Nimm", "mei\u00b7nen", "Geist", "in", "dei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den Christus tewer ihm erworben,", "tokens": ["Den", "Chris\u00b7tus", "te\u00b7wer", "ihm", "er\u00b7wor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NE", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Als er unschuldig ist gestorben.", "tokens": ["Als", "er", "un\u00b7schul\u00b7dig", "ist", "ge\u00b7stor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Thust du es? ja, so komm behende,", "tokens": ["Thust", "du", "es", "?", "ja", ",", "so", "komm", "be\u00b7hen\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$.", "PTKANT", "$,", "ADV", "VVFIN", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Komm, Tod, und k\u00fcrtz mir meine Pein,", "tokens": ["Komm", ",", "Tod", ",", "und", "k\u00fcrtz", "mir", "mei\u00b7ne", "Pein", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "KON", "VVIMP", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Brich an mit einem seelign Ende,", "tokens": ["Brich", "an", "mit", "ei\u00b7nem", "see\u00b7lign", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich wil nur auffgel\u00f6set seyn.", "tokens": ["Ich", "wil", "nur", "auff\u00b7ge\u00b7l\u00f6\u00b7set", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wer Christo gl\u00e4ubig sich ergeben", "tokens": ["Wer", "Chris\u00b7to", "gl\u00e4u\u00b7big", "sich", "er\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "NE", "ADJD", "PRF", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der stirbt und wird ihm ewig leben.", "tokens": ["Der", "stirbt", "und", "wird", "ihm", "e\u00b7wig", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VAFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {