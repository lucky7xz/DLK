{"dta.poem.18581": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "A ls sie ihren  N ahmens- T ag  \n anderwerts begangen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Dein Nahmens-Tag/ du holder Augen Sonne!", "tokens": ["Dein", "Nah\u00b7mens\u00b7Tag", "/", "du", "hol\u00b7der", "Au\u00b7gen", "Son\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PPER", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verschliesset zwar vorl\u00e4ngsten seinen Schein;", "tokens": ["Ver\u00b7schlies\u00b7set", "zwar", "vor\u00b7l\u00e4ngs\u00b7ten", "sei\u00b7nen", "Schein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch wie mein Hertz die Lust entfernt gewonne/", "tokens": ["Doch", "wie", "mein", "Hertz", "die", "Lust", "ent\u00b7fernt", "ge\u00b7won\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "NN", "ART", "NN", "VVPP", "VAPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die damahls mir nicht konte nahe seyn:", "tokens": ["Die", "da\u00b7mahls", "mir", "nicht", "kon\u00b7te", "na\u00b7he", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "PTKNEG", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So l\u00e4sset Mund und Hand", "tokens": ["So", "l\u00e4s\u00b7set", "Mund", "und", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Nach frohen Wiedersehen/", "tokens": ["Nach", "fro\u00b7hen", "Wie\u00b7der\u00b7se\u00b7hen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die Opffer-Glut geschehen/", "tokens": ["Die", "Opf\u00b7fer\u00b7Glut", "ge\u00b7sche\u00b7hen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die dir zuvor im Geiste war entbrandt.", "tokens": ["Die", "dir", "zu\u00b7vor", "im", "Geis\u00b7te", "war", "ent\u00b7brandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPRART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es z\u00fcnden sich nun tausend Freuden-Kertzen/", "tokens": ["Es", "z\u00fcn\u00b7den", "sich", "nun", "tau\u00b7send", "Freu\u00b7den\u00b7Kert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "CARD", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ach glaub' es nur in meiner Seelen an/", "tokens": ["Ach", "glaub'", "es", "nur", "in", "mei\u00b7ner", "See\u00b7len", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Man nennt dich kaum/ so regt sich schon im Hertzen", "tokens": ["Man", "nennt", "dich", "kaum", "/", "so", "regt", "sich", "schon", "im", "Hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "$(", "ADV", "VVFIN", "PRF", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Di\u00df/ was dein Aug' im Spiegel sehen kan.", "tokens": ["Di\u00df", "/", "was", "dein", "Aug'", "im", "Spie\u00b7gel", "se\u00b7hen", "kan", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$(", "PWS", "PPOSAT", "NN", "APPRART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Gebr\u00e4uche Weichen hier", "tokens": ["Ge\u00b7br\u00e4u\u00b7che", "Wei\u00b7chen", "hier"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und der Calender Zeiten/", "tokens": ["Und", "der", "Ca\u00b7len\u00b7der", "Zei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$("], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Denn deine Lieblichkeiten/", "tokens": ["Denn", "dei\u00b7ne", "Lieb\u00b7lich\u00b7kei\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Begehen stets dein Nahmens-Fest in mir.", "tokens": ["Be\u00b7ge\u00b7hen", "stets", "dein", "Nah\u00b7mens\u00b7Fest", "in", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ach meine nicht/ da\u00df blosse Schmeicheleyen", "tokens": ["Ach", "mei\u00b7ne", "nicht", "/", "da\u00df", "blos\u00b7se", "Schmei\u00b7che\u00b7le\u00b7yen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "PTKNEG", "$(", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Der Worte Glantz und falscher Firni\u00df seyn/", "tokens": ["Der", "Wor\u00b7te", "Glantz", "und", "fal\u00b7scher", "Fir\u00b7ni\u00df", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "ADJA", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nein/ wahrer Ruhm mu\u00df Sch\u00f6nheit nur erfreuen/", "tokens": ["Nein", "/", "wah\u00b7rer", "Ruhm", "mu\u00df", "Sch\u00f6n\u00b7heit", "nur", "er\u00b7freu\u00b7en", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "ADJA", "NN", "VMFIN", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die He\u00dfliche streicht Complimenten ein.", "tokens": ["Die", "He\u00df\u00b7li\u00b7che", "streicht", "Com\u00b7pli\u00b7men\u00b7ten", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "PTKVZ", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Es mu\u00df die gantze Welt", "tokens": ["Es", "mu\u00df", "die", "gant\u00b7ze", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mit einer Sonne prahlen/", "tokens": ["Mit", "ei\u00b7ner", "Son\u00b7ne", "prah\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und deiner Augen Strahlen", "tokens": ["Und", "dei\u00b7ner", "Au\u00b7gen", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sind nur ein Blitz/ der tausend Hertzen f\u00e4llt.", "tokens": ["Sind", "nur", "ein", "Blitz", "/", "der", "tau\u00b7send", "Hert\u00b7zen", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$(", "ART", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Du hast die Kunst an meiner Brust erwiesen/", "tokens": ["Du", "hast", "die", "Kunst", "an", "mei\u00b7ner", "Brust", "er\u00b7wie\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gesteh' es nur/ wie starck du mich ger\u00fchrt.", "tokens": ["Ge\u00b7steh'", "es", "nur", "/", "wie", "starck", "du", "mich", "ge\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$(", "PWAV", "ADJD", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Helena hat Griechenland gepriesen/", "tokens": ["Die", "He\u00b7le\u00b7na", "hat", "Grie\u00b7chen\u00b7land", "ge\u00b7prie\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "NE", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und ward hernach durch sie in Brandt gef\u00fchrt.", "tokens": ["Und", "ward", "her\u00b7nach", "durch", "sie", "in", "Brandt", "ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "APPR", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Du bist auch wundersch\u00f6n;", "tokens": ["Du", "bist", "auch", "wun\u00b7der\u00b7sch\u00f6n", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Doch leschen deine H\u00e4nde", "tokens": ["Doch", "le\u00b7schen", "dei\u00b7ne", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Nicht meiner Liebe Br\u00e4nde/", "tokens": ["Nicht", "mei\u00b7ner", "Lie\u00b7be", "Br\u00e4n\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wird man an mir ein ander Troja sehn.", "tokens": ["Wird", "man", "an", "mir", "ein", "an\u00b7der", "Tro\u00b7ja", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "APPR", "PPER", "ART", "ADJD", "NE", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Vergn\u00fcge dich indessen in der Seelen/", "tokens": ["Ver\u00b7gn\u00fc\u00b7ge", "dich", "in\u00b7des\u00b7sen", "in", "der", "See\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und k\u00fcsse stets das sch\u00f6nste Gl\u00fcck der Welt.", "tokens": ["Und", "k\u00fcs\u00b7se", "stets", "das", "sch\u00f6ns\u00b7te", "Gl\u00fcck", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Flora darff dir keine Rosen zehlen/", "tokens": ["Die", "Flo\u00b7ra", "darff", "dir", "kei\u00b7ne", "Ro\u00b7sen", "zeh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dein Antlitz ist ein Blumen-reiches Feld.", "tokens": ["Dein", "Ant\u00b7litz", "ist", "ein", "Blu\u00b7men\u00b7rei\u00b7ches", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Gr\u00fcnt nun dein Fr\u00fchlings-Schein", "tokens": ["Gr\u00fcnt", "nun", "dein", "Fr\u00fch\u00b7lings\u00b7Schein"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "In steter Sch\u00f6nheit-Bl\u00fcthe/", "tokens": ["In", "ste\u00b7ter", "Sch\u00f6n\u00b7heit\u00b7B\u00b7l\u00fc\u00b7the", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "So la\u00df auch deine G\u00fcte", "tokens": ["So", "la\u00df", "auch", "dei\u00b7ne", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Annehmlichste! mein sch\u00f6ner Fr\u00fchling seyn.", "tokens": ["An\u00b7nehm\u00b7lichs\u00b7te", "!", "mein", "sch\u00f6\u00b7ner", "Fr\u00fch\u00b7ling", "seyn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPOSAT", "ADJA", "NN", "VAINF", "$."], "meter": "-----+-+-+", "measure": "unknown.measure.tri"}}}}}