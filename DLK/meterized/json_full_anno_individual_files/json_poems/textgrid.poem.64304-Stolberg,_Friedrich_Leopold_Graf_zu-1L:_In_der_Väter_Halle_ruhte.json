{"textgrid.poem.64304": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: In der V\u00e4ter Halle ruhte", "genre": "verse", "period": "N.A.", "pub_year": 1774, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In der V\u00e4ter Halle ruhte", "tokens": ["In", "der", "V\u00e4\u00b7ter", "Hal\u00b7le", "ruh\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NE", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ritter Rudolfs Heldenarm!", "tokens": ["Rit\u00b7ter", "Ru\u00b7dolfs", "Hel\u00b7de\u00b7narm", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rudolf, den die Schlacht erfreute,", "tokens": ["Ru\u00b7dolf", ",", "den", "die", "Schlacht", "er\u00b7freu\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Rudolf, welchen Frankreich scheute,", "tokens": ["Ru\u00b7dolf", ",", "wel\u00b7chen", "Fran\u00b7kreich", "scheu\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAT", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und der Sarazenen Schwarm.", "tokens": ["Und", "der", "Sa\u00b7ra\u00b7ze\u00b7nen", "Schwarm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Er, der letzte seines Stammes,", "tokens": ["Er", ",", "der", "letz\u00b7te", "sei\u00b7nes", "Stam\u00b7mes", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "ADJA", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weinte seiner S\u00f6hne Fall;", "tokens": ["Wein\u00b7te", "sei\u00b7ner", "S\u00f6h\u00b7ne", "Fall", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zwischen moosbewachs'nen Mauren", "tokens": ["Zwi\u00b7schen", "moos\u00b7be\u00b7wachs'\u00b7nen", "Mau\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "T\u00f6nte seiner Klage Trauren", "tokens": ["T\u00f6n\u00b7te", "sei\u00b7ner", "Kla\u00b7ge", "Trau\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In der Zellen Wiederhall.", "tokens": ["In", "der", "Zel\u00b7len", "Wie\u00b7der\u00b7hall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Agnes mit den goldnen Locken", "tokens": ["Ag\u00b7nes", "mit", "den", "gold\u00b7nen", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "War des Greises Trost und Stab.", "tokens": ["War", "des", "Grei\u00b7ses", "Trost", "und", "Stab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sanft wie Tauben, wei\u00df wie Schw\u00e4ne,", "tokens": ["Sanft", "wie", "Tau\u00b7ben", ",", "wei\u00df", "wie", "Schw\u00e4\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "NN", "$,", "VVFIN", "KOKOM", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00fc\u00dfte sie des Vaters Thr\u00e4ne", "tokens": ["K\u00fc\u00df\u00b7te", "sie", "des", "Va\u00b7ters", "Thr\u00e4\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Von den grauen Wimpern ab.", "tokens": ["Von", "den", "grau\u00b7en", "Wim\u00b7pern", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach! sie weinte selbst im stillen,", "tokens": ["Ach", "!", "sie", "wein\u00b7te", "selbst", "im", "stil\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "ADV", "APPRART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn der Mond ins Fenster schien;", "tokens": ["Wenn", "der", "Mond", "ins", "Fens\u00b7ter", "schien", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Albrecht mit der offnen Stirne", "tokens": ["Al\u00b7brecht", "mit", "der", "off\u00b7nen", "Stir\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "ADJA", "NN"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "Brannte f\u00fcr die edle Dirne,", "tokens": ["Brann\u00b7te", "f\u00fcr", "die", "ed\u00b7le", "Dir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und die Dirne liebte ihn.", "tokens": ["Und", "die", "Dir\u00b7ne", "lieb\u00b7te", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Aber Horst, der hundert Krieger", "tokens": ["A\u00b7ber", "Horst", ",", "der", "hun\u00b7dert", "Krie\u00b7ger"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NE", "$,", "PRELS", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unterhielt im eignen Sold,", "tokens": ["Un\u00b7ter\u00b7hielt", "im", "eig\u00b7nen", "Sold", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "R\u00fchmte seines Stammes Ahnen,", "tokens": ["R\u00fchm\u00b7te", "sei\u00b7nes", "Stam\u00b7mes", "Ah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Prangte mit erfochtnen Fahnen,", "tokens": ["Prang\u00b7te", "mit", "er\u00b7focht\u00b7nen", "Fah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und der Vater war ihm hold", "tokens": ["Und", "der", "Va\u00b7ter", "war", "ihm", "hold"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Einst beim freien Mahle k\u00fc\u00dfte", "tokens": ["Einst", "beim", "frei\u00b7en", "Mah\u00b7le", "k\u00fc\u00df\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Albrecht ihre weiche Hand;", "tokens": ["Al\u00b7brecht", "ih\u00b7re", "wei\u00b7che", "Hand", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihre sanften Augen strebten", "tokens": ["Ih\u00b7re", "sanf\u00b7ten", "Au\u00b7gen", "streb\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihn zu strafen, ach! da bebten", "tokens": ["Ihn", "zu", "stra\u00b7fen", ",", "ach", "!", "da", "beb\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "ITJ", "$.", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Thr\u00e4nen auf das Busenband.", "tokens": ["Thr\u00e4\u00b7nen", "auf", "das", "Bu\u00b7sen\u00b7band", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Horst entbrannte, blickte seitw\u00e4rts", "tokens": ["Horst", "ent\u00b7brann\u00b7te", ",", "blick\u00b7te", "seit\u00b7w\u00e4rts"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "$,", "VVFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf sein schweres Mordgewehr;", "tokens": ["Auf", "sein", "schwe\u00b7res", "Mord\u00b7ge\u00b7wehr", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf des Ritters Wange gl\u00fchten", "tokens": ["Auf", "des", "Rit\u00b7ters", "Wan\u00b7ge", "gl\u00fch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zorn und Liebe, Funken spr\u00fchten", "tokens": ["Zorn", "und", "Lie\u00b7be", ",", "Fun\u00b7ken", "spr\u00fch\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aus den Augen wild umher.", "tokens": ["Aus", "den", "Au\u00b7gen", "wild", "um\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Drohend warf er seinen Handschuh", "tokens": ["Dro\u00b7hend", "warf", "er", "sei\u00b7nen", "Hand\u00b7schuh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der Agnes keuschen Scho\u00df:", "tokens": ["In", "der", "Ag\u00b7nes", "keu\u00b7schen", "Scho\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Albrecht, nimm! Zu dieser Stunde", "tokens": ["Al\u00b7brecht", ",", "nimm", "!", "Zu", "die\u00b7ser", "Stun\u00b7de"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "VVIMP", "$.", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Harr' ich dein im M\u00fchlengrunde!", "tokens": ["Harr'", "ich", "dein", "im", "M\u00fch\u00b7len\u00b7grun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kaum gesagt, schon flog sein Ro\u00df.", "tokens": ["Kaum", "ge\u00b7sagt", ",", "schon", "flog", "sein", "Ro\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Albrecht nahm das Fehdezeichen", "tokens": ["Al\u00b7brecht", "nahm", "das", "Feh\u00b7de\u00b7zei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruhig, und bestieg sein Ro\u00df;", "tokens": ["Ru\u00b7hig", ",", "und", "be\u00b7stieg", "sein", "Ro\u00df", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Freute sich des M\u00e4dchens Z\u00e4hre,", "tokens": ["Freu\u00b7te", "sich", "des", "M\u00e4d\u00b7chens", "Z\u00e4h\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die der Lieb' und ihm zur Ehre", "tokens": ["Die", "der", "Lieb'", "und", "ihm", "zur", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "KON", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aus dem blauen Auge flo\u00df.", "tokens": ["Aus", "dem", "blau\u00b7en", "Au\u00b7ge", "flo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "R\u00f6tlich schimmerte die R\u00fcstung", "tokens": ["R\u00f6t\u00b7lich", "schim\u00b7mer\u00b7te", "die", "R\u00fcs\u00b7tung"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der Abendsonne Strahl;", "tokens": ["In", "der", "A\u00b7bend\u00b7son\u00b7ne", "Strahl", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Von den Hufen ihrer Pferde", "tokens": ["Von", "den", "Hu\u00b7fen", "ih\u00b7rer", "Pfer\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "T\u00f6nte weit umher die Erde,", "tokens": ["T\u00f6n\u00b7te", "weit", "um\u00b7her", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PTKVZ", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und die Hirsche flohn ins Thal.", "tokens": ["Und", "die", "Hir\u00b7sche", "flohn", "ins", "Thal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Auf des S\u00f6llers Gitter lehnte", "tokens": ["Auf", "des", "S\u00f6l\u00b7lers", "Git\u00b7ter", "lehn\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die bet\u00e4ubte Agnes sich,", "tokens": ["Die", "be\u00b7t\u00e4ub\u00b7te", "Ag\u00b7nes", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NE", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sah die blanken Speere blinken,", "tokens": ["Sah", "die", "blan\u00b7ken", "Spee\u00b7re", "blin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sah den edlen Albrecht sinken \u2013", "tokens": ["Sah", "den", "ed\u00b7len", "Al\u00b7brecht", "sin\u00b7ken", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sank wie Albrecht, und erblich.", "tokens": ["Sank", "wie", "Al\u00b7brecht", ",", "und", "er\u00b7blich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "NE", "$,", "KON", "ADJD", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.12": {"line.1": {"text": "Bang' von leiser Ahndung spornte", "tokens": ["Bang'", "von", "lei\u00b7ser", "Ahn\u00b7dung", "sporn\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Horst sein schaumbedecktes Pferd;", "tokens": ["Horst", "sein", "schaum\u00b7be\u00b7deck\u00b7tes", "Pferd", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6ret nun des Hauses Jammer,", "tokens": ["H\u00f6\u00b7ret", "nun", "des", "Hau\u00b7ses", "Jam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eilet in der Sch\u00f6nen Kammer,", "tokens": ["Ei\u00b7let", "in", "der", "Sch\u00f6\u00b7nen", "Kam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "troc