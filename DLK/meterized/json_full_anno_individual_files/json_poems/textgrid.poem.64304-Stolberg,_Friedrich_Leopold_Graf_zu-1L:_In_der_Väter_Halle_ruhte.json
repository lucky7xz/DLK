die", "kal\u00b7te", "Toch\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In den v\u00e4terlichen Arm;", "tokens": ["In", "den", "v\u00e4\u00b7ter\u00b7li\u00b7chen", "Arm", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hielt sie so zween lange Tage,", "tokens": ["Hielt", "sie", "so", "zween", "lan\u00b7ge", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "VVFIN", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Thr\u00e4nenlos und ohne Klage,", "tokens": ["Thr\u00e4\u00b7nen\u00b7los", "und", "oh\u00b7ne", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und verschied im stummen Harm.", "tokens": ["Und", "ver\u00b7schied", "im", "stum\u00b7men", "Harm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}