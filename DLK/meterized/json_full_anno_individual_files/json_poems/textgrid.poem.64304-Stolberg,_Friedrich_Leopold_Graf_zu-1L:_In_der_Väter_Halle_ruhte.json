info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruhig, und bestieg sein Ro\u00df;", "tokens": ["Ru\u00b7hig", ",", "und", "be\u00b7stieg", "sein", "Ro\u00df", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Freute sich des M\u00e4dchens Z\u00e4hre,", "tokens": ["Freu\u00b7te", "sich", "des", "M\u00e4d\u00b7chens", "Z\u00e4h\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die der Lieb' und ihm zur Ehre", "tokens": ["Die", "der", "Lieb'", "und", "ihm", "zur", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "KON", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aus dem blauen Auge flo\u00df.", "tokens": ["Aus", "dem", "blau\u00b7en", "Au\u00b7ge", "flo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.23": {"line.1": {"text": "R\u00f6tlich schimmerte die R\u00fcstung", "tokens": ["R\u00f6t\u00b7lich", "schim\u00b7mer\u00b7te", "die", "R\u00fcs\u00b7tung"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der Abendsonne Strahl;", "tokens": ["In", "der", "A\u00b7bend\u00b7son\u00b7ne", "Strahl", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Von den Hufen ihrer Pferde", "toke