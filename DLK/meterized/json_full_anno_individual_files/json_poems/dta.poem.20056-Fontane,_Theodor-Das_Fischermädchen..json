{"dta.poem.20056": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Das Fischerm\u00e4dchen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Steht auf sand\u2019gem D\u00fcnenr\u00fccken", "tokens": ["Steht", "auf", "san\u00b7d'\u00b7gem", "D\u00fc\u00b7nen\u00b7r\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Eine Fischerh\u00fctt\u2019 am Strand;", "tokens": ["Ei\u00b7ne", "Fi\u00b7scher\u00b7h\u00fctt'", "am", "Strand", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Abendroth und Netze schm\u00fccken", "tokens": ["A\u00b7ben\u00b7droth", "und", "Net\u00b7ze", "schm\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wunderlich die Giebelwand.", "tokens": ["Wun\u00b7der\u00b7lich", "die", "Gie\u00b7bel\u00b7wand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Drinnen schnurrt das Spinner\u00e4dchen,", "tokens": ["Drin\u00b7nen", "schnurrt", "das", "Spin\u00b7ne\u00b7r\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bla\u00df der Mond in\u2019s Fenster scheint,", "tokens": ["Bla\u00df", "der", "Mond", "in's", "Fens\u00b7ter", "scheint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Still am Herd das Fischerm\u00e4dchen", "tokens": ["Still", "am", "Herd", "das", "Fi\u00b7scher\u00b7m\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denkt des letzten Sturms und \u2014 weint.", "tokens": ["Denkt", "des", "letz\u00b7ten", "Sturms", "und", "weint", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "KON", "$(", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und es klagen ihre Thr\u00e4nen:", "tokens": ["Und", "es", "kla\u00b7gen", "ih\u00b7re", "Thr\u00e4\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u201eweit der Himmel, tief die See, \u2014", "tokens": ["\u201e", "weit", "der", "Him\u00b7mel", ",", "tief", "die", "See", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "ART", "NN", "$,", "ADJD", "ART", "NN", "$,", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch noch weiter geht mein Sehnen,", "tokens": ["Doch", "noch", "wei\u00b7ter", "geht", "mein", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und noch tiefer ist mein Weh.\u201c", "tokens": ["Und", "noch", "tie\u00b7fer", "ist", "mein", "Weh", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}