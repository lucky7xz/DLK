{"textgrid.poem.67065": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An die Ruhe", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vom Gru\u00df des Hahns, vom Sichelget\u00f6n erweckt,", "tokens": ["Vom", "Gru\u00df", "des", "Hahns", ",", "vom", "Si\u00b7chel\u00b7ge\u00b7t\u00f6n", "er\u00b7weckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "APPRART", "NE", "VVPP", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Gelobt ich dir, Begl\u00fcckerin! Lobgesang,", "tokens": ["Ge\u00b7lobt", "ich", "dir", ",", "Be\u00b7gl\u00fc\u00b7cke\u00b7rin", "!", "Lob\u00b7ge\u00b7sang", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "NN", "$.", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und siehe da, am heitern Mittag", "tokens": ["Und", "sie\u00b7he", "da", ",", "am", "hei\u00b7tern", "Mit\u00b7tag"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVIMP", "ADV", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schl\u00e4get sie mir, der Begeistrung Stunde.", "tokens": ["Schl\u00e4\u00b7get", "sie", "mir", ",", "der", "Be\u00b7geis\u00b7trung", "Stun\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "ART", "NN", "NN", "$."], "meter": "+--++-+-+-", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Erquicklich, wie die heimische Ruhebank", "tokens": ["Er\u00b7quick\u00b7lich", ",", "wie", "die", "hei\u00b7mi\u00b7sche", "Ru\u00b7he\u00b7bank"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Im fernen Schlachtget\u00fcmmel dem Krieger deucht,", "tokens": ["Im", "fer\u00b7nen", "Schlacht\u00b7ge\u00b7t\u00fcm\u00b7mel", "dem", "Krie\u00b7ger", "deucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wenn die zerfleischten Arme sinken,", "tokens": ["Wenn", "die", "zer\u00b7fleischten", "Ar\u00b7me", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und der geschmetterte Stahl im Blut liegt \u2013", "tokens": ["Und", "der", "ge\u00b7schmet\u00b7ter\u00b7te", "Stahl", "im", "Blut", "liegt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "So bist du, Ruhe! freundliche Tr\u00f6sterin!", "tokens": ["So", "bist", "du", ",", "Ru\u00b7he", "!", "freund\u00b7li\u00b7che", "Tr\u00f6s\u00b7te\u00b7rin", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "NN", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Du schenkest Riesenkraft dem Verachteten;", "tokens": ["Du", "schen\u00b7kest", "Rie\u00b7sen\u00b7kraft", "dem", "Ver\u00b7ach\u00b7te\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Er h\u00f6hnet Dominiksgesichtern,", "tokens": ["Er", "h\u00f6h\u00b7net", "Do\u00b7mi\u00b7niks\u00b7ge\u00b7sich\u00b7tern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00f6hnet der zischenden Natterzunge.", "tokens": ["H\u00f6h\u00b7net", "der", "zi\u00b7schen\u00b7den", "Nat\u00b7ter\u00b7zun\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Im Veilchental, vom d\u00e4mmernden Hain umbraust,", "tokens": ["Im", "Veil\u00b7chen\u00b7tal", ",", "vom", "d\u00e4m\u00b7mern\u00b7den", "Hain", "um\u00b7braust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Entschlummert er, von s\u00fc\u00dfen Begeistrungen", "tokens": ["Ent\u00b7schlum\u00b7mert", "er", ",", "von", "s\u00fc\u00b7\u00dfen", "Be\u00b7geis\u00b7trun\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Zukunft trunken, von der Unschuld", "tokens": ["Der", "Zu\u00b7kunft", "trun\u00b7ken", ",", "von", "der", "Un\u00b7schuld"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Spielen im flatternden Fl\u00fcgelkleide.", "tokens": ["Spie\u00b7len", "im", "flat\u00b7tern\u00b7den", "Fl\u00fc\u00b7gel\u00b7klei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Da weiht der Ruhe Zauber den Schlummernden,", "tokens": ["Da", "weiht", "der", "Ru\u00b7he", "Zau\u00b7ber", "den", "Schlum\u00b7mern\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit Mut zu schwingen im Labyrinth sein Licht,", "tokens": ["Mit", "Mut", "zu", "schwin\u00b7gen", "im", "La\u00b7by\u00b7rinth", "sein", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Fahne rasch voranzutragen,", "tokens": ["Die", "Fah\u00b7ne", "rasch", "vor\u00b7an\u00b7zu\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo sich der D\u00fcnkel entgegenstemmet.", "tokens": ["Wo", "sich", "der", "D\u00fcn\u00b7kel", "ent\u00b7ge\u00b7gens\u00b7tem\u00b7met", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}}, "stanza.6": {"line.1": {"text": "Auf springt er, wandelt ernster den Bach hinab", "tokens": ["Auf", "springt", "er", ",", "wan\u00b7delt", "erns\u00b7ter", "den", "Bach", "hin\u00b7ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "VVFIN", "PPER", "$,", "VVFIN", "ADJD", "ART", "NN", "PTKVZ"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nach seiner H\u00fctte. Siehe! das G\u00f6tterwerk,", "tokens": ["Nach", "sei\u00b7ner", "H\u00fct\u00b7te", ".", "Sie\u00b7he", "!", "das", "G\u00f6t\u00b7ter\u00b7werk", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "VVIMP", "$.", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Es keimet in der gro\u00dfen Seele.", "tokens": ["Es", "kei\u00b7met", "in", "der", "gro\u00b7\u00dfen", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wieder ein Lenz, \u2013 und es ist vollendet.", "tokens": ["Wie\u00b7der", "ein", "Lenz", ",", "\u2013", "und", "es", "ist", "voll\u00b7en\u00b7det", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "$(", "KON", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "An jener St\u00e4tte bauet der Herrliche", "tokens": ["An", "je\u00b7ner", "St\u00e4t\u00b7te", "bau\u00b7et", "der", "Herr\u00b7li\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dir, gottgesandte Ruhe! den Dankaltar.", "tokens": ["Dir", ",", "gott\u00b7ge\u00b7sand\u00b7te", "Ru\u00b7he", "!", "den", "Dan\u00b7kal\u00b7tar", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$.", "ART", "NN", "$."], "meter": "---+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dort harrt er, wonnel\u00e4chlend, wie die", "tokens": ["Dort", "harrt", "er", ",", "won\u00b7ne\u00b7l\u00e4ch\u00b7lend", ",", "wie", "die"], "token_inf