{"textgrid.poem.29804": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "18.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Chloris, Doris, Iris, Ciris liebten einen Hirten alle;", "tokens": ["Chlo\u00b7ris", ",", "Do\u00b7ris", ",", "I\u00b7ris", ",", "Ci\u00b7ris", "lieb\u00b7ten", "ei\u00b7nen", "Hir\u00b7ten", "al\u00b7le", ";"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "NE", "$,", "NE", "VVFIN", "ART", "NN", "PIS", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Ihm zu weisen mit dem Wercke, da\u00df er ieden wol gefalle,", "tokens": ["Ihm", "zu", "wei\u00b7sen", "mit", "dem", "Wer\u00b7cke", ",", "da\u00df", "er", "ie\u00b7den", "wol", "ge\u00b7fal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "APPR", "ART", "NN", "$,", "KOUS", "PPER", "PIAT", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Kr\u00f6nte Chloris ihn mit Blumen; Doris bracht ihm Honig-Schnidte;", "tokens": ["Kr\u00f6n\u00b7te", "Chlo\u00b7ris", "ihn", "mit", "Blu\u00b7men", ";", "Do\u00b7ris", "bracht", "ihm", "Ho\u00b7nig\u00b7Sch\u00b7nid\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "PPER", "APPR", "NN", "$.", "NE", "VVFIN", "PPER", "NN", "$."], "meter": "+-+-+-+-+-+-+--+-", "measure": "trochaic.octa.plus.relaxed"}, "line.4": {"text": "Iris gr\u00fcsset ihn mit l\u00e4cheln; Ciris fasst ihn in der Mitte,", "tokens": ["I\u00b7ris", "gr\u00fcs\u00b7set", "ihn", "mit", "l\u00e4\u00b7cheln", ";", "Ci\u00b7ris", "fasst", "ihn", "in", "der", "Mit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "VVFIN", "$.", "NE", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.5": {"text": "K\u00fcste seinen Mund-Rubin. Ihm behagte nur das k\u00fcssen,", "tokens": ["K\u00fcs\u00b7te", "sei\u00b7nen", "Mun\u00b7dRu\u00b7bin", ".", "Ihm", "be\u00b7hag\u00b7te", "nur", "das", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "ADV", "PDS", "VVINF", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Nam von sich und gab der Ciris Krone, Honig und das gr\u00fcssen.", "tokens": ["Nam", "von", "sich", "und", "gab", "der", "Ci\u00b7ris", "Kro\u00b7ne", ",", "Ho\u00b7nig", "und", "das", "gr\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PRF", "KON", "VVFIN", "ART", "NE", "NE", "$,", "NN", "KON", "PDS", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Chloris, Doris, Iris, Ciris liebten einen Hirten alle;", "tokens": ["Chlo\u00b7ris", ",", "Do\u00b7ris", ",", "I\u00b7ris", ",", "Ci\u00b7ris", "lieb\u00b7ten", "ei\u00b7nen", "Hir\u00b7ten", "al\u00b7le", ";"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "NE", "$,", "NE", "VVFIN", "ART", "NN", "PIS", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Ihm zu weisen mit dem Wercke, da\u00df er ieden wol gefalle,", "tokens": ["Ihm", "zu", "wei\u00b7sen", "