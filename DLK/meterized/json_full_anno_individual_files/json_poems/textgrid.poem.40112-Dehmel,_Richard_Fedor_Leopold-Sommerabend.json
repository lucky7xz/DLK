{"textgrid.poem.40112": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Sommerabend", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Klar ruhn die L\u00fcfte auf der stillen Flur;", "tokens": ["Klar", "ruhn", "die", "L\u00fcf\u00b7te", "auf", "der", "stil\u00b7len", "Flur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "fern dampft der See; in D\u00fcnsten goldig flimmernd", "tokens": ["fern", "dampft", "der", "See", ";", "in", "D\u00fcns\u00b7ten", "gol\u00b7dig", "flim\u00b7mernd"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$.", "APPR", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "verschwimmt der Sonne letzte rote Spur;", "tokens": ["ver\u00b7schwimmt", "der", "Son\u00b7ne", "letz\u00b7te", "ro\u00b7te", "Spur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "die zarten Wolken wallen hoch und schimmernd.", "tokens": ["die", "zar\u00b7ten", "Wol\u00b7ken", "wal\u00b7len", "hoch", "und", "schim\u00b7mernd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "In laue D\u00e4mmrung h\u00fcllt sich das Gefild,", "tokens": ["In", "lau\u00b7e", "D\u00e4mm\u00b7rung", "h\u00fcllt", "sich", "das", "Ge\u00b7fild", ",