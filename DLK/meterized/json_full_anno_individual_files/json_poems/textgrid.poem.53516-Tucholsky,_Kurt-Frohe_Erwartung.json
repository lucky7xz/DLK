{"textgrid.poem.53516": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Frohe Erwartung", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vater Wrangel, jener alte gute", "tokens": ["Va\u00b7ter", "Wran\u00b7gel", ",", "je\u00b7ner", "al\u00b7te", "gu\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NE", "$,", "PDAT", "ADJA", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "General von Anno dazumal,", "tokens": ["Ge\u00b7ne\u00b7ral", "von", "An\u00b7no", "da\u00b7zu\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "zog beim Klange einer Aufstands-Tute", "tokens": ["zog", "beim", "Klan\u00b7ge", "ei\u00b7ner", "Auf\u00b7stands\u00b7Tu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "aus Berlin, weil man es so befahl.", "tokens": ["aus", "Ber\u00b7lin", ",", "weil", "man", "es", "so", "be\u00b7fahl", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "KOUS", "PIS", "PPER", "ADV", "VVFIN", "$."], "meter": "-++-+-+-+", "measure": "unknown.measure.penta"}, "line.5": {"text": "Und sie drohten ihm sein Haus zu sengen,", "tokens": ["Und", "sie", "droh\u00b7ten", "ihm", "sein", "Haus", "zu", "sen\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "seine Frau Gemahlin zu erh\u00e4ngen,", "tokens": ["sei\u00b7ne", "Frau", "Ge\u00b7mah\u00b7lin", "zu", "er\u00b7h\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "bis er dann zu gro\u00dfem Gram", "tokens": ["bis", "er", "dann", "zu", "gro\u00b7\u00dfem", "Gram"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "der Rebellen wiederkam.", "tokens": ["der", "Re\u00b7bel\u00b7len", "wie\u00b7der\u00b7kam", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.9": {"text": "Heftig blasend ritt man durch die Linden,", "tokens": ["Hef\u00b7tig", "bla\u00b7send", "ritt", "man", "durch", "die", "Lin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VVFIN", "PIS", "APPR", "ART", "NE", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "voller Sehnsucht, seine Frau zu finden.", "tokens": ["vol\u00b7ler", "Sehn\u00b7sucht", ",", "sei\u00b7ne", "Frau", "zu", "fin\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Weich und lind entfuhrs dem alten Knaben:", "tokens": ["Weich", "und", "lind", "ent\u00b7fuhrs", "dem", "al\u00b7ten", "Kna\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "\u00bbob sie ihr wohl uffjehangen haben?\u00ab", "tokens": ["\u00bb", "ob", "sie", "ihr", "wohl", "uff\u00b7je\u00b7han\u00b7gen", "ha\u00b7ben", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KOUS", "PPER", "PPER", "ADV", "VVPP", "VAINF", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Nimmer will mich dieses Wort verlassen.", "tokens": ["Nim\u00b7mer", "will", "mich", "die\u00b7ses", "Wort", "ver\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PRF", "PDAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Heut noch lebt die alte Reaktion.", "tokens": ["Heut", "noch", "lebt", "die", "al\u00b7te", "Re\u00b7ak\u00b7ti\u00b7on", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Heute noch ist sie so schwer zu fassen \u2013", "tokens": ["Heu\u00b7te", "noch", "ist", "sie", "so", "schwer", "zu", "fas\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADV", "ADJD", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Brennglas, der versuchte es ja schon.", "tokens": ["Brenn\u00b7glas", ",", "der", "ver\u00b7such\u00b7te", "es", "ja", "schon", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "So viel Jahre steck ich schon im Kriege,", "tokens": ["So", "viel", "Jah\u00b7re", "steck", "ich", "schon", "im", "Krie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "denke an die Panke meiner Wiege,", "tokens": ["den\u00b7ke", "an", "die", "Pan\u00b7ke", "mei\u00b7ner", "Wie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "an mein Preu\u00dfen, an Berlin", "tokens": ["an", "mein", "Preu\u00b7\u00dfen", ",", "an", "Ber\u00b7lin"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "NE"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "und die Junker von Malchin.", "tokens": ["und", "die", "Jun\u00b7ker", "von", "Mal\u00b7chin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "NE", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "Nie verge\u00df ich in dem fremden Lande", "tokens": ["Nie", "ver\u00b7ge\u00df", "ich", "in", "dem", "frem\u00b7den", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Mutter Reaktion und ihre Schande.", "tokens": ["Mut\u00b7ter", "Re\u00b7ak\u00b7ti\u00b7on", "und", "ih\u00b7re", "Schan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.11": {"text": "Voller Hoffnung sinn ich oft im Graben:", "tokens": ["Vol\u00b7ler", "Hoff\u00b7nung", "sinn", "ich", "oft", "im", "Gra\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "\u00bbob sie ihr wohl uffjehangen haben?\u00ab", "tokens": ["\u00bb", "ob", "sie", "ihr", "wohl", "uff\u00b7je\u00b7han\u00b7gen", "ha\u00b7ben", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KOUS", "PPER", "PPER", "ADV", "VVPP", "VAINF", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Da zu Haus, bei Vatern auf dem Boden,", "tokens": ["Da", "zu", "Haus", ",", "bei", "Va\u00b7tern", "auf", "dem", "Bo\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "liegt ein gro\u00dfes buntes Fahnentuch,", "tokens": ["liegt", "ein", "gro\u00b7\u00dfes", "bun\u00b7tes", "Fah\u00b7nen\u00b7tuch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "mitten im Ger\u00fcmpel der Kommoden,", "tokens": ["mit\u00b7ten", "im", "Ge\u00b7r\u00fcm\u00b7pel", "der", "Kom\u00b7mo\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "in dem Schummer voller Staubgeruch . . .", "tokens": ["in", "dem", "Schum\u00b7mer", "vol\u00b7ler", "Staub\u00b7ge\u00b7ruch", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$.", "$.", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Und beim Urlaub sagte mir der Alte,", "tokens": ["Und", "beim", "Ur\u00b7laub", "sag\u00b7te", "mir", "der", "Al\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "oben h\u00e4ngt er durch die Bodenspalte", "tokens": ["o\u00b7ben", "h\u00e4ngt", "er", "durch", "die", "Bo\u00b7dens\u00b7pal\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "seine Fahne in den Wind,", "tokens": ["sei\u00b7ne", "Fah\u00b7ne", "in", "den", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "wenn wir erst zu Hause sind.", "tokens": ["wenn", "wir", "erst", "zu", "Hau\u00b7se", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Das war F\u00fcnfzehn. Und bei jedem frischen", "tokens": ["Das", "war", "F\u00fcnf\u00b7zehn", ".", "Und", "bei", "je\u00b7dem", "fri\u00b7schen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "NN", "$.", "KON", "APPR", "PIAT", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Wechsel an den deutschen gr\u00fcnen Tischen", "tokens": ["Wech\u00b7sel", "an", "den", "deut\u00b7schen", "gr\u00fc\u00b7nen", "Ti\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "bitt ich um die sch\u00f6nste aller Gaben:", "tokens": ["bitt", "ich", "um", "die", "sch\u00f6ns\u00b7te", "al\u00b7ler", "Ga\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "\u00bbob sie ihr wohl uffjehangen haben?\u00ab", "tokens": ["\u00bb", "ob", "sie", "ihr", "wohl", "uff\u00b7je\u00b7han\u00b7gen", "ha\u00b7ben", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KOUS", "PPER", "PPER", "ADV", "VVPP", "VAINF", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Vater Wrangel, jener alte gute", "tokens": ["Va\u00b7ter", "Wran\u00b7gel", ",", "je\u00b7ner", "al\u00b7te", "gu\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NE", "$,", "PDAT", "ADJA", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "General von Anno dazumal,", "tokens": ["Ge\u00b7ne\u00b7ral", "von", "An\u00b7no", "da\u00b7zu\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "zog beim Klange einer Aufstands-Tute", "tokens": ["zog", "beim", "Klan\u00b7ge", "ei\u00b7ner", "Auf\u00b7stands\u00b7Tu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "aus Berlin, weil man es so befahl.", "tokens": ["aus", "Ber\u00b7lin", ",", "weil", "man", "es", "so", "be\u00b7fahl", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "KOUS", "PIS", "PPER", "ADV", "VVFIN", "$."], "meter": "-++-+-+-+", "measure": "unknown.measure.penta"}, "line.5": {"text": "Und sie drohten ihm sein Haus zu sengen,", 