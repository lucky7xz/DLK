{"textgrid.poem.47375": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Dichterselbstlob", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin K\u00f6nig eines stillen Volks von Tr\u00e4umen,", "tokens": ["Ich", "bin", "K\u00f6\u00b7nig", "ei\u00b7nes", "stil\u00b7len", "Volks", "von", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Herrscher in der Phantasieen Himmelsr\u00e4umen.", "tokens": ["Herr\u00b7scher", "in", "der", "Phan\u00b7ta\u00b7si\u00b7e\u00b7en", "Him\u00b7mels\u00b7r\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Kaiserkron' und K\u00f6nigskerze mir zu F\u00fc\u00dfen", "tokens": ["Kai\u00b7ser\u00b7kron'", "und", "K\u00f6\u00b7nigs\u00b7ker\u00b7ze", "mir", "zu", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Bl\u00fchen auf, mich, ihren Oberherrn, zu gr\u00fc\u00dfen.", "tokens": ["Bl\u00fc\u00b7hen", "auf", ",", "mich", ",", "ih\u00b7ren", "O\u00b7ber\u00b7herrn", ",", "zu", "gr\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPER", "$,", "PPOSAT", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Um die dunklen Locken farb'ge Wolkenbogen", "tokens": ["Um", "die", "dunk\u00b7len", "Lo\u00b7cken", "fa\u00b7rb'\u00b7ge", "Wol\u00b7ken\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "ADJA", "NN", "ADJA", "NN"], "meter": "--+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Sind, ein buntgesteintes Diadem, gezogen.", "tokens": ["Sind", ",", "ein", "bunt\u00b7ge\u00b7stein\u00b7tes", "Di\u00b7a\u00b7dem", ",", "ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "$,", "ART", "ADJA", "NN", "$,", "VVPP", "$."], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.7": {"text": "Alle Fr\u00fchlingsblumen kommen, vorzutragen", "tokens": ["Al\u00b7le", "Fr\u00fch\u00b7lings\u00b7blu\u00b7men", "kom\u00b7men", ",", "vor\u00b7zu\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PIAT", "NN", "VVINF", "$,", "VVIZU"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.8": {"text": "Meinen Ohren ihre ew'gen Liebesklagen.", "tokens": ["Mei\u00b7nen", "Oh\u00b7ren", "ih\u00b7re", "ew'\u00b7gen", "Lie\u00b7bes\u00b7kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.9": {"text": "Alle Bronnen aus der Sch\u00f6pfung Tiefen brechen,", "tokens": ["Al\u00b7le", "Bron\u00b7nen", "aus", "der", "Sch\u00f6p\u00b7fung", "Tie\u00b7fen", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.10": {"text": "Von Geheimnissen mit mir sich zu besprechen.", "tokens": ["Von", "Ge\u00b7heim\u00b7nis\u00b7sen", "mit", "mir", "sich", "zu", "be\u00b7spre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPER", "PRF", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.11": {"text": "An der Linken trag' ich Salomonis Siegel,", "tokens": ["An", "der", "Lin\u00b7ken", "trag'", "ich", "Sa\u00b7lo\u00b7mo\u00b7nis", "Sie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "NE", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.12": {"text": "Mit der Rechten heb' ich Dschemschids Weltenspiegel.", "tokens": ["Mit", "der", "Rech\u00b7ten", "heb'", "ich", "Dschem\u00b7schids", "Wel\u00b7ten\u00b7spie\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "NE", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.13": {"text": "Alle Geister sind des Siegels Unterthanen,", "tokens": ["Al\u00b7le", "Geis\u00b7ter", "sind", "des", "Sie\u00b7gels", "Un\u00b7ter\u00b7tha\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.14": {"text": "Und die Sch\u00f6pfung schw\u00f6rt zu meinen Sonnenfahnen.", "tokens": ["Und", "die", "Sch\u00f6p\u00b7fung", "schw\u00f6rt", "zu", "mei\u00b7nen", "Son\u00b7nen\u00b7fah\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.15": {"text": "Gegen Nacht und Finsternis in Kampfesschranken", "tokens": ["Ge\u00b7gen", "Nacht", "und", "Fins\u00b7ter\u00b7nis", "in", "Kamp\u00b7fes\u00b7schran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.16": {"text": "F\u00fchr' ich eine Schar von leuchtenden Gedanken.", "tokens": ["F\u00fchr'", "ich", "ei\u00b7ne", "Schar", "von", "leuch\u00b7ten\u00b7den", "Ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.17": {"text": "Kommt und helft, den Himmel auf der Erde stiften,", "tokens": ["Kommt", "und", "helft", ",", "den", "Him\u00b7mel", "auf", "der", "Er\u00b7de", "stif\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "$,", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.18": {"text": "Helft den Tod mir t\u00f6ten und das Gift entgiften.", "tokens": ["Helft", "den", "Tod", "mir", "t\u00f6\u00b7ten", "und", "das", "Gift", "ent\u00b7gif\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "VVFIN", "KON", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.19": {"text": "Jeden Baum des Lebens soll mein Hauch bebl\u00e4ttern,", "tokens": ["Je\u00b7den", "Baum", "des", "Le\u00b7bens", "soll", "mein", "Hauch", "be\u00b7bl\u00e4t\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.20": {"text": "Und die Schlang' am Stamme soll mein Arm zerschmettern.", "tokens": ["Und", "die", "Schlang'", "am", "Stam\u00b7me", "soll", "mein", "Arm", "zer\u00b7schmet\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.21": {"text": "Morgenwinde, gehet aus auf allen Pfaden,", "tokens": ["Mor\u00b7gen\u00b7win\u00b7de", ",", "ge\u00b7het", "aus", "auf", "al\u00b7len", "Pfa\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.22": {"text": "Mir zum neuen Paradies die Welt zu laden.", "tokens": ["Mir", "zum", "neu\u00b7en", "Pa\u00b7ra\u00b7dies", "die", "Welt", "zu", "la\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.23": {"text": "Wer dem Druck der Tyrannei mu\u00df drau\u00dfen weichen,", "tokens": ["Wer", "dem", "Druck", "der", "Ty\u00b7ran\u00b7nei", "mu\u00df", "drau\u00b7\u00dfen", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ART", "NN", "VMFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.24": {"text": "Eine Freistatt biet' ich ihm in meinen Reichen.", "tokens": ["Ei\u00b7ne", "Freis\u00b7tatt", "biet'", "ich", "ihm", "in", "mei\u00b7nen", "Rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.25": {"text": "Dort ist M\u00fchsal, Drang, Verfolgung, Not und Kummer;", "tokens": ["Dort", "ist", "M\u00fch\u00b7sal", ",", "Drang", ",", "Ver\u00b7fol\u00b7gung", ",", "Not", "und", "Kum\u00b7mer", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.26": {"text": "Hier ist Frieden, Eintracht, Stille, Ruh' und Schlummer.", "tokens": ["Hier", "ist", "Frie\u00b7den", ",", "Ein\u00b7tracht", ",", "Stil\u00b7le", ",", "Ruh'", "und", "Schlum\u00b7mer", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.27": {"text": "Ihr Bewohner Dschinnistans, Peris und Dschinnen,", "tokens": ["Ihr", "Be\u00b7woh\u00b7ner", "Dschin\u00b7nis\u00b7tans", ",", "Pe\u00b7ris", "und", "Dschin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "$,", "NE", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.28": {"text": "Baut mir hier ein Wunderschlo\u00df mit goldnen Zinnen.", "tokens": ["Baut", "mir", "hier", "ein", "Wun\u00b7der\u00b7schlo\u00df", "mit", "gold\u00b7nen", "Zin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.29": {"text": "Bauet mir den Weltpalast mit vielen Zimmern,", "tokens": ["Bau\u00b7et", "mir", "den", "Welt\u00b7pa\u00b7last", "mit", "vie\u00b7len", "Zim\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.30": {"text": "Wo vereint die Herrlichkeit der Welt soll schimmern.", "tokens": ["Wo", "ver\u00b7eint", "die", "Herr\u00b7lich\u00b7keit", "der", "Welt", "soll", "schim\u00b7mern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "ART", "NN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.31": {"text": "Bauet so viel Zimmer mir als Nationen;", "tokens": ["Bau\u00b7et", "so", "viel", "Zim\u00b7mer", "mir", "als", "Na\u00b7ti\u00b7o\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIAT", "NN", "PPER", "KOUS", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.32": {"text": "Jede soll mit ihrer Lust in einem wohnen.", "tokens": ["Je\u00b7de", "soll", "mit", "ih\u00b7rer", "Lust", "in", "ei\u00b7nem", "woh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "PPOSAT", "NN", "APPR", "ART", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.33": {"text": "Bauet so viel D\u00e4cher mir als Himmelszonen;", "tokens": ["Bau\u00b7et", "so", "viel", "D\u00e4\u00b7cher", "mir", "als", "Him\u00b7mels\u00b7zo\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIAT", "NN", "PPER", "KOUS", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.34": {"text": "Jede soll mit ihrer Pracht auf einem thronen.", "tokens": ["Je\u00b7de", "soll", "mit", "ih\u00b7rer", "Pracht", "auf", "ei\u00b7nem", "thro\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.35": {"text": "In der sieben Prunkgem\u00e4cher Tepp'che wirken", "tokens": ["In", "der", "sie\u00b7ben", "Prunk\u00b7ge\u00b7m\u00e4\u00b7cher", "Tepp'\u00b7che", "wir\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "CARD", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.36": {"text": "Soll man Wunderwerk' aus sieben Weltbezirken.", "tokens": ["Soll", "man", "Wun\u00b7der\u00b7werk'", "aus", "sie\u00b7ben", "Welt\u00b7be\u00b7zir\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "NN", "APPR", "CARD", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.37": {"text": "Malerei soll Fr\u00fchlingsglanz an W\u00e4nden weben,", "tokens": ["Ma\u00b7le\u00b7rei", "soll", "Fr\u00fch\u00b7lings\u00b7glanz", "an", "W\u00e4n\u00b7den", "we\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.38": {"text": "In den Nischen sollen Marmorbilder leben.", "tokens": ["In", "den", "Ni\u00b7schen", "sol\u00b7len", "Mar\u00b7mor\u00b7bil\u00b7der", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.39": {"text": "Und Musik soll mit den ew'gen Sph\u00e4rent\u00f6nen", "tokens": ["Und", "Mu\u00b7sik", "soll", "mit", "den", "ew'\u00b7gen", "Sph\u00e4\u00b7ren\u00b7t\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VMFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+----+-+-+-", "measure": "dactylic.init"}, "line.40": {"text": "Alle Lebensstimmen der Natur vers\u00f6hnen.", "tokens": ["Al\u00b7le", "Le\u00b7bens\u00b7stim\u00b7men", "der", "Na\u00b7tur", "ver\u00b7s\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.41": {"text": "O ihr Geister, um das Zauberschlo\u00df den Garten", "tokens": ["O", "ihr", "Geis\u00b7ter", ",", "um", "das", "Zau\u00b7ber\u00b7schlo\u00df", "den", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "$,", "KOUI", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.42": {"text": "Pflanzt mit B\u00e4umen und Gew\u00e4chsen aller Arten.", "tokens": ["Pflanzt", "mit", "B\u00e4u\u00b7men", "und", "Ge\u00b7w\u00e4ch\u00b7sen", "al\u00b7ler", "Ar\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.43": {"text": "Nachtigallen aller Zonen mit den Rosen", "tokens": ["Nach\u00b7ti\u00b7gal\u00b7len", "al\u00b7ler", "Zo\u00b7nen", "mit", "den", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PIAT", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.44": {"text": "Aller Himmel lasset mir zusammen kosen.", "tokens": ["Al\u00b7ler", "Him\u00b7mel", "las\u00b7set", "mir", "zu\u00b7sam\u00b7men", "ko\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.45": {"text": "O ihr G\u00f6tter Hindostans, die ihr in Blumen-", "tokens": ["O", "ihr", "G\u00f6t\u00b7ter", "Hin\u00b7dos\u00b7tans", ",", "die", "ihr", "in", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "NE", "$,", "PRELS", "PPER", "APPR", "TRUNC"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.46": {"text": "Kelchen wohnet, komm