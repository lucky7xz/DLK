{"textgrid.poem.37820": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Rewelge", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbdes Morgens zwischen dreyn und vieren", "tokens": ["\u00bb", "des", "Mor\u00b7gens", "zwi\u00b7schen", "dreyn", "und", "vie\u00b7ren"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "ADV", "APPR", "CARD", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da m\u00fcssen wir Soldaten marschieren", "tokens": ["Da", "m\u00fcs\u00b7sen", "wir", "Sol\u00b7da\u00b7ten", "mar\u00b7schie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Das G\u00e4\u00dflein auf und ab;", "tokens": ["Das", "G\u00e4\u00df\u00b7lein", "auf", "und", "ab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Tralali, Tralaley, Tralala,", "tokens": ["Tra\u00b7la\u00b7li", ",", "Tra\u00b7la\u00b7ley", ",", "Tra\u00b7la\u00b7la", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Mein Sch\u00e4tzel sieht herab.", "tokens": ["Mein", "Sch\u00e4t\u00b7zel", "sieht", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ach Bruder jetzt bin ich geschossen,", "tokens": ["Ach", "Bru\u00b7der", "jetzt", "bin", "ich", "ge\u00b7schos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Kugel hat mich schwer getroffen,", "tokens": ["Die", "Ku\u00b7gel", "hat", "mich", "schwer", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Trag mich in mein Quartier,", "tokens": ["Trag", "mich", "in", "mein", "Quar\u00b7tier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Tralali, Tralaley, Tralala,", "tokens": ["Tra\u00b7la\u00b7li", ",", "Tra\u00b7la\u00b7ley", ",", "Tra\u00b7la\u00b7la", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Es ist nicht weit von hier.", "tokens": ["Es", "ist", "nicht", "weit", "von", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "APPR", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ach Bruder ich kann dich nicht tragen,", "tokens": ["Ach", "Bru\u00b7der", "ich", "kann", "dich", "nicht", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die Feinde haben uns geschlagen,", "tokens": ["Die", "Fein\u00b7de", "ha\u00b7ben", "uns", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Helf dir der liebe Gott;", "tokens": ["Helf", "dir", "der", "lie\u00b7be", "Gott", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Tralali, Tralaley, Tralala,", "tokens": ["Tra\u00b7la\u00b7li", ",", "Tra\u00b7la\u00b7ley", ",", "Tra\u00b7la\u00b7la", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Ich mu\u00df marschieren in Tod.", "tokens": ["Ich", "mu\u00df", "mar\u00b7schie\u00b7ren", "in", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "APPR", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Ach Br\u00fcder! ihr geht ja vor\u00fcber,", "tokens": ["Ach", "Br\u00fc\u00b7der", "!", "ihr", "geht", "ja", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "PPER", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als w\u00e4r es mit mir schon vor\u00fcber,", "tokens": ["Als", "w\u00e4r", "es", "mit", "mir", "schon", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "APPR", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr Lumpenfeind seyd da;", "tokens": ["Ihr", "Lum\u00b7pen\u00b7feind", "seyd", "da", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Tralali, Tralaley, Tralala,", "tokens": ["Tra\u00b7la\u00b7li", ",", "Tra\u00b7la\u00b7ley", ",", "Tra\u00b7la\u00b7la", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Ihr tretet mir zu nah.", "tokens": ["Ihr", "tre\u00b7tet", "mir", "zu", "nah", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich mu\u00df wohl meine Trommel r\u00fchren,", "tokens": ["Ich", "mu\u00df", "wohl", "mei\u00b7ne", "Trom\u00b7mel", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sonst werde ich mich ganz verlieren;", "tokens": ["Sonst", "wer\u00b7de", "ich", "mich", "ganz", "ver\u00b7lie\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Br\u00fcder dick ges\u00e4et,", "tokens": ["Die", "Br\u00fc\u00b7der", "dick", "ge\u00b7s\u00e4et", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Tralali, Tralaley, Tralala,", "tokens": ["Tra\u00b7la\u00b7li", ",", "Tra\u00b7la\u00b7ley", ",", "Tra\u00b7la\u00b7la", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Sie liegen wie gem\u00e4ht.\u00ab", "tokens": ["Sie", "lie\u00b7gen", "wie", "ge\u00b7m\u00e4ht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "VVPP", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Er schl\u00e4gt die Trommel auf und nieder,", "tokens": ["Er", "schl\u00e4gt", "die", "Trom\u00b7mel", "auf", "und", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er wecket seine stillen Br\u00fcder,", "tokens": ["Er", "we\u00b7cket", "sei\u00b7ne", "stil\u00b7len", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie schlagen ihren Feind,", "tokens": ["Sie", "schla\u00b7gen", "ih\u00b7ren", "Feind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Tralali, Tralaley, Tralala,", "tokens": ["Tra\u00b7la\u00b7li", ",", "Tra\u00b7la\u00b7ley", ",", "Tra\u00b7la\u00b7la", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Ein Schrecken schl\u00e4gt den Feind.", "tokens": ["Ein", "Schre\u00b7cken", "schl\u00e4gt", "den", "Feind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Er schl\u00e4gt die Trommel auf und nieder,", "tokens": ["Er", "schl\u00e4gt", "die", "Trom\u00b7mel", "auf", "und", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN"