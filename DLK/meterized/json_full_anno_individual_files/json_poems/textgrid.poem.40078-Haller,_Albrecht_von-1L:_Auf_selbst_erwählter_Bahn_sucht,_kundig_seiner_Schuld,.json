{"textgrid.poem.40078": {"metadata": {"author": {"name": "Haller, Albrecht von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf selbst erw\u00e4hlter Bahn sucht, kundig seiner Schuld,", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf selbst erw\u00e4hlter Bahn sucht, kundig seiner Schuld,", "tokens": ["Auf", "selbst", "er\u00b7w\u00e4hl\u00b7ter", "Bahn", "sucht", ",", "kun\u00b7dig", "sei\u00b7ner", "Schuld", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "VVFIN", "$,", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der unbekehrte Mensch des gro\u00dfen Sch\u00f6pfers Huld.", "tokens": ["Der", "un\u00b7be\u00b7kehr\u00b7te", "Mensch", "des", "gro\u00b7\u00dfen", "Sch\u00f6p\u00b7fers", "Huld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Umsonst wird er zu dir befleckte H\u00e4nde heben,", "tokens": ["Um\u00b7sonst", "wird", "er", "zu", "dir", "be\u00b7fleck\u00b7te", "H\u00e4n\u00b7de", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPER", "VVFIN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Herr! dein ist ja die Welt, was bleibt ihm, dir zu geben?", "tokens": ["Herr", "!", "dein", "ist", "ja", "die", "Welt", ",", "was", "bleibt", "ihm", ",", "dir", "zu", "ge\u00b7ben", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "po