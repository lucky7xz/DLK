{"dta.poem.11747": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "33.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie einst des Geiz'gen Aug' erschlossen Zaubersalben,", "tokens": ["Wie", "einst", "des", "Gei\u00b7z'\u00b7gen", "Aug'", "er\u00b7schlos\u00b7sen", "Zau\u00b7ber\u00b7sal\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da\u00df ihm verborgne Sch\u00e4tz' erschienen allenthalben;", "tokens": ["Da\u00df", "ihm", "ver\u00b7borg\u00b7ne", "Sch\u00e4tz'", "er\u00b7schie\u00b7nen", "al\u00b7len\u00b7thal\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die ganze Welt gewebt aus Gold und Edelstein;", "tokens": ["Die", "gan\u00b7ze", "Welt", "ge\u00b7webt", "aus", "Gold", "und", "E\u00b7del\u00b7stein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nur zu sch\u00e4rfen dient es ihm der Habsucht Pein.", "tokens": ["Und", "nur", "zu", "sch\u00e4r\u00b7fen", "dient", "es", "ihm", "der", "Hab\u00b7sucht", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKZU", "VVINF", "VVFIN", "PPER", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So ward erschlossen auch mein Blick von Wundersalben,", "tokens": ["So", "ward", "er\u00b7schlos\u00b7sen", "auch", "mein", "Blick", "von", "Wun\u00b7der\u00b7sal\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "ADV", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ungeahnte Sch\u00e4tz' erblick' ich allenthalben;", "tokens": ["Und", "un\u00b7ge\u00b7ahn\u00b7te", "Sch\u00e4tz'", "er\u00b7blick'", "ich", "al\u00b7len\u00b7thal\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die ganze Welt gewebt aus Sonn- und Blumenschein;", "tokens": ["Die", "gan\u00b7ze", "Welt", "ge\u00b7webt", "aus", "Sonn", "und", "Blu\u00b7men\u00b7schein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "APPR", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und zur Befriedigung gereicht es mir allein.", "tokens": ["Und", "zur", "Be\u00b7frie\u00b7di\u00b7gung", "ge\u00b7reicht", "es", "mir", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Zufrieden seh' ich, da\u00df ich niemals kann ausbeuten", "tokens": ["Zu\u00b7frie\u00b7den", "seh'", "ich", ",", "da\u00df", "ich", "nie\u00b7mals", "kann", "aus\u00b7beu\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Sch\u00f6pfung Schacht, und nie ihr R\u00e4thselspiel ausdeuten.", "tokens": ["Der", "Sch\u00f6p\u00b7fung", "Schacht", ",", "und", "nie", "ihr", "R\u00e4th\u00b7sel\u00b7spiel", "aus\u00b7deu\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "KON", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Der Schacht, in dem das Erz nachw\u00e4chst aus innrer Kraft;", "tokens": ["Der", "Schacht", ",", "in", "dem", "das", "Erz", "nach\u00b7w\u00e4chst", "aus", "inn\u00b7rer", "Kraft", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das R\u00e4thsel, das, gel\u00f6s't, wird doppelt r\u00e4thselhaft.", "tokens": ["Das", "R\u00e4th\u00b7sel", ",", "das", ",", "ge\u00b7l\u00f6s't", ",", "wird", "dop\u00b7pelt", "r\u00e4th\u00b7sel\u00b7haft", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "$,", "VVPP", "$,", "VAFIN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Und l\u00f6sen wir mit Gl\u00fcck, was wir zur Zeit aufhaben,", "tokens": ["Und", "l\u00f6\u00b7sen", "wir", "mit", "Gl\u00fcck", ",", "was", "wir", "zur", "Zeit", "auf\u00b7ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schon aufgegeben sind der Folgezeit Aufgaben.", "tokens": ["Schon", "auf\u00b7ge\u00b7ge\u00b7ben", "sind", "der", "Fol\u00b7ge\u00b7zeit", "Auf\u00b7ga\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Und was zu l\u00f6sen wir die Hoffnung jetzt aufgaben,", "tokens": ["Und", "was", "zu", "l\u00f6\u00b7sen", "wir", "die", "Hoff\u00b7nung", "jetzt", "auf\u00b7ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PTKZU", "VVFIN", "PPER", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das l\u00f6sen leicht einst, die zu l\u00f6sen das aufhaben.", "tokens": ["Das", "l\u00f6\u00b7sen", "leicht", "einst", ",", "die", "zu", "l\u00f6\u00b7sen", "das", "auf\u00b7ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "ADV", "$,", "PRELS", "PTKZU", "VVINF", "PDS", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Ich aber freue mich, nach Lust hervorzuholen,", "tokens": ["Ich", "a\u00b7ber", "freu\u00b7e", "mich", ",", "nach", "Lust", "her\u00b7vor\u00b7zu\u00b7ho\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "$,", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und f\u00fcrchte nicht, zuletzt zu finden taube Kohlen.", "tokens": ["Und", "f\u00fcrch\u00b7te", "nicht", ",", "zu\u00b7letzt", "zu", "fin\u00b7den", "tau\u00b7be", "Koh\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "ADV", "PTKZU", "VVINF", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Und was ich selber Lust nicht hab' hervorzuholen,", "tokens": ["Und", "was", "ich", "sel\u00b7ber", "Lust", "nicht", "hab'", "her\u00b7vor\u00b7zu\u00b7ho\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "NN", "PTKNEG", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sei einem lustigern Geschlecht von mir empfohlen.", "tokens": ["Sei", "ei\u00b7nem", "lus\u00b7ti\u00b7gern", "Ge\u00b7schlecht", "von", "mir", "emp\u00b7foh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Noch lange wird die Axt den Urwald nicht ausreuten,", "tokens": ["Noch", "lan\u00b7ge", "wird", "die", "Axt", "den", "Ur\u00b7wald", "nicht", "aus\u00b7reu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch lange Bienenflei\u00df den Fr\u00fchling nicht ausbeuten:", "tokens": ["Noch", "lan\u00b7ge", "Bie\u00b7nen\u00b7flei\u00df", "den", "Fr\u00fch\u00b7ling", "nicht", "aus\u00b7beu\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Solang in Gott und Welt sich Herzen still ausfreuten,", "tokens": ["So\u00b7lang", "in", "Gott", "und", "Welt", "sich", "Her\u00b7zen", "still", "aus\u00b7freu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "PRF", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Maienglocken sacht des Lenzes Sieg ausbeuten:", "tokens": ["Und", "Mai\u00b7en\u00b7glo\u00b7cken", "sacht", "des", "Len\u00b7zes", "Sieg", "aus\u00b7beu\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Solang wird frohe Kunst die Wunder nur ausdeuten,", "tokens": ["So\u00b7lang", "wird", "fro\u00b7he", "Kunst", "die", "Wun\u00b7der", "nur", "aus\u00b7deu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJA", "NN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die eines K\u00fcnstlers H\u00e4nd' auf die Natur ausstreuten.", "tokens": ["Die", "ei\u00b7nes", "K\u00fcnst\u00b7lers", "H\u00e4nd'", "auf", "die", "Na\u00b7tur", "aus\u00b7streu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "APPR", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.14": {"line.1": {"text": "Er gebe Leben mir, Gesundheit, innre Lust!", "tokens": ["Er", "ge\u00b7be", "Le\u00b7ben", "mir", ",", "Ge\u00b7sund\u00b7heit", ",", "inn\u00b7re", "Lust", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PPER", "$,", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn noch zur H\u00e4lft' ist nicht der Schatz in meiner Brust.", "tokens": ["Denn", "noch", "zur", "H\u00e4lft'", "ist", "nicht", "der", "Schatz", "in", "mei\u00b7ner", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "VAFIN", "PTKNEG", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.15": {"line.1": {"text": "Nicht l\u00e4ngstes Leben reicht ihn vollends auszubeuten,", "tokens": ["Nicht", "l\u00e4ngs\u00b7tes", "Le\u00b7ben", "reicht", "ihn", "vol\u00b7lends", "aus\u00b7zu\u00b7beu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "VVFIN", "PPER", "ADV", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob Tochtert\u00f6chter ich ausstattete zu Br\u00e4uten.", "tokens": ["Ob", "Toch\u00b7ter\u00b7t\u00f6ch\u00b7ter", "ich", "aus\u00b7stat\u00b7te\u00b7te", "zu", "Br\u00e4u\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.16": {"line.1": {"text": "Weh, Reim, du hast im Klang ein Bild mir aufgedrungen,", "tokens": ["Weh", ",", "Reim", ",", "du", "hast", "im", "Klang", "ein", "Bild", "mir", "auf\u00b7ge\u00b7drun\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PPER", "VAFIN", "APPRART", "NN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch dessen Weh sind hier die Saiten abgesprungen.", "tokens": ["Durch", "des\u00b7sen", "Weh", "sind", "hier", "die", "Sai\u00b7ten", "ab\u00b7ge\u00b7sprun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "VAFIN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}