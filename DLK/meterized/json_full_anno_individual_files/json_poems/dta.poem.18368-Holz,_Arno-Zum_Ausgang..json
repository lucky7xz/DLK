{"dta.poem.18368": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Zum Ausgang.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.85", "cy:0.14"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "O bleicher Tod, o wolle mir nicht nahn,", "tokens": ["O", "blei\u00b7cher", "Tod", ",", "o", "wol\u00b7le", "mir", "nicht", "nahn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "FM", "VMFIN", "PPER", "PTKNEG", "ADJA", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Eh ich f\u00fcr mein Geschlecht etwas gethan,", "tokens": ["Eh", "ich", "f\u00fcr", "mein", "Ge\u00b7schlecht", "et\u00b7was", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich bete auf zu meinem Gott und Herrn:", "tokens": ["Ich", "be\u00b7te", "auf", "zu", "mei\u00b7nem", "Gott", "und", "Herrn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fcr eine sch\u00f6ne Sache st\u00fcrb ich gern.", "tokens": ["F\u00fcr", "ei\u00b7ne", "sch\u00f6\u00b7ne", "Sa\u00b7che", "st\u00fcrb", "ich", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "F\u00fcrs Recht der Menschheit und der neuen Zeit,", "tokens": ["F\u00fcrs", "Recht", "der", "Menschheit", "und", "der", "neu\u00b7en", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ART", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie w\u00e4r es sch\u00f6n, zu fallen in dem Streit;", "tokens": ["Wie", "w\u00e4r", "es", "sch\u00f6n", ",", "zu", "fal\u00b7len", "in", "dem", "Streit", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "$,", "PTKZU", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bei dem gebrochnen Erz der Tyrannei,", "tokens": ["Bei", "dem", "ge\u00b7broch\u00b7nen", "Erz", "der", "Ty\u00b7ran\u00b7nei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie st\u00fcrb es sich im Felde froh und frei!", "tokens": ["Wie", "st\u00fcrb", "es", "sich", "im", "Fel\u00b7de", "froh", "und", "frei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O, l\u00e4g ich dann wie heut in selger Pein", "tokens": ["O", ",", "l\u00e4g", "ich", "dann", "wie", "heut", "in", "sel\u00b7ger", "Pein"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "PPER", "ADV", "KOKOM", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf einem gr\u00fcnen B\u00fchl im Abendschein", "tokens": ["Auf", "ei\u00b7nem", "gr\u00fc\u00b7nen", "B\u00fchl", "im", "A\u00b7bend\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und d\u00fcrfte sprechen, auf der Brust die Hand:", "tokens": ["Und", "d\u00fcrf\u00b7te", "spre\u00b7chen", ",", "auf", "der", "Brust", "die", "Hand", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "VVINF", "$,", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Du kennst dein Kind, mein sch\u00f6nes Vaterland!", "tokens": ["Du", "kennst", "dein", "Kind", ",", "mein", "sch\u00f6\u00b7nes", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}