{"textgrid.poem.52207": {"metadata": {"author": {"name": "Gryphius, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Ungereimtes Sonnett", "genre": "verse", "period": "N.A.", "pub_year": 1677, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ob gleich Cloridalis auf ihre Marmor-Kugeln,", "tokens": ["Ob", "gleich", "Clo\u00b7ri\u00b7da\u00b7lis", "auf", "ih\u00b7re", "Mar\u00b7mor\u00b7Ku\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NE", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die, wie ein ieder sagt, der Himmel selbst gew\u00f6lbt,", "tokens": ["Die", ",", "wie", "ein", "ie\u00b7der", "sagt", ",", "der", "Him\u00b7mel", "selbst", "ge\u00b7w\u00f6lbt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "ART", "PIS", "VVFIN", "$,", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und auf ihr Angesicht, das Sternen gleichet, trozt,", "tokens": ["Und", "auf", "ihr", "An\u00b7ge\u00b7sicht", ",", "das", "Ster\u00b7nen", "glei\u00b7chet", ",", "trozt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "$,", "ART", "NN", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ob schon, wie sie vermeynt, des Paris goldner Apfel", "tokens": ["Ob", "schon", ",", "wie", "sie", "ver\u00b7meynt", ",", "des", "Pa\u00b7ris", "gold\u00b7ner", "Ap\u00b7fel"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "$,", "PWAV", "PPER", "VVPP", "$,", "ART", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Vor sie allein gemacht, ob gleich viel altes Silber", "tokens": ["Vor", "sie", "al\u00b7lein", "ge\u00b7macht", ",", "ob", "gleich", "viel", "al\u00b7tes", "Sil\u00b7ber"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ADV", "VVPP", "$,", "KOUS", "ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "In ihrem Kasten ruht, doch ists ein eitler Wurf,", "tokens": ["In", "ih\u00b7rem", "Kas\u00b7ten", "ruht", ",", "doch", "ists", "ein", "eit\u00b7ler", "Wurf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,", "ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Den sie nach mir gethan; ich bin gleichwie ein Fel\u00df,", "tokens": ["Den", "sie", "nach", "mir", "ge\u00b7than", ";", "ich", "bin", "gleich\u00b7wie", "ein", "Fel\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPER", "VVPP", "$.", "PPER", "VAFIN", "KON", "ART", "NN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Und lieb ein kluges Buch mehr als der Venus G\u00fcrtel.", "tokens": ["Und", "lieb", "ein", "klu\u00b7ges", "Buch", "mehr", "als", "der", "Ve\u00b7nus", "G\u00fcr\u00b7tel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "ADJA", "NN", "PIAT", "KOKOM", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Liebe reimet sich so wenig mit Minerven,", "tokens": ["Die", "Lie\u00b7be", "rei\u00b7met", "sich", "so", "we\u00b7nig", "mit", "Mi\u00b7ner\u00b7ven", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "PIS", "APPR", "NN", "$,"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Als eine Sterbe-Kunst zu Karten und zu W\u00fcrffeln,", "tokens": ["Als", "ei\u00b7ne", "Ster\u00b7be\u00b7Kunst", "zu", "Kar\u00b7ten", "und", "zu", "W\u00fcrf\u00b7feln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das Braut-Bett in die Gruft, Schalmeyen zu der Orgel,", "tokens": ["Das", "Braut\u00b7Bett", "in", "die", "Gruft", ",", "Schal\u00b7me\u00b7yen", "zu", "der", "Or\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "NN", "APPR", "ART", "NN", "$,"], "meter": "-++--++-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ein M\u00e4gdchen und ein Grei\u00df, als Pferde zu den Eseln,", "tokens": ["Ein", "M\u00e4gd\u00b7chen", "und", "ein", "Grei\u00df", ",", "als", "Pfer\u00b7de", "zu", "den", "E\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,", "KOUS", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Als Me\u00dfing zum Smaragd, als Rosen zu den Disteln,", "tokens": ["Als", "Me\u00b7\u00dfing", "zum", "Sma\u00b7ragd", ",", "als", "Ro\u00b7sen", "zu", "den", "Dis\u00b7teln", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPRART",