{"dta.poem.9987": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "96.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Leser/ du kanst hier mich finden/ nichts verhindert dich daran/", "tokens": ["Le\u00b7ser", "/", "du", "kanst", "hier", "mich", "fin\u00b7den", "/", "nichts", "ver\u00b7hin\u00b7dert", "dich", "da\u00b7ran", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PPER", "VMFIN", "ADV", "PPER", "VVINF", "$(", "PIS", "VVFIN", "PRF", "PAV", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Ich steh frey vor deinen augen/ keine deck ist vorgethan.", "tokens": ["Ich", "steh", "frey", "vor", "dei\u00b7nen", "au\u00b7gen", "/", "kei\u00b7ne", "deck", "ist", "vor\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$(", "PIAT", "PDS", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Welcher mich erlanget hat/ kan nicht mehr auf erden leben/", "tokens": ["Wel\u00b7cher", "mich", "er\u00b7lan\u00b7get", "hat", "/", "kan", "nicht", "mehr", "auf", "er\u00b7den", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "VAFIN", "$(", "VMFIN", "PTKNEG", "ADV", "APPR", "NN", "VVINF", "$("], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Mu\u00df entweder in der h\u00f6ll oder in dem himmel schweben.", "tokens": ["Mu\u00df", "ent\u00b7we\u00b7der", "in", "der", "h\u00f6ll", "o\u00b7der", "in", "dem", "him\u00b7mel", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "KON", "APPR", "ART", "NN", "KON", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+--+-+-", "measure": "trochaic.septa.relaxed"}}}}}