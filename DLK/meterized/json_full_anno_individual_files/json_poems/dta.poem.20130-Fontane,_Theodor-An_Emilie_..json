{"dta.poem.20130": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "An Emilie .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Da drau\u00dfen schneit es: Schneegeflimmer", "tokens": ["Da", "drau\u00b7\u00dfen", "schneit", "es", ":", "Schnee\u00b7ge\u00b7flim\u00b7mer"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$.", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wies heute mir den Weg zu Dir;", "tokens": ["Wies", "heu\u00b7te", "mir", "den", "Weg", "zu", "Dir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Eintret\u2019 ich in Dein traulich Zimmer,", "tokens": ["Ein\u00b7tret'", "ich", "in", "Dein", "trau\u00b7lich", "Zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und warm an\u2019s Herze fliegst Du mir \u2014", "tokens": ["Und", "warm", "an's", "Her\u00b7ze", "fliegst", "Du", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "VVFIN", "PPER", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Absch\u00fcttl\u2019 ich jetzt die Winterflocken,", "tokens": ["Ab\u00b7sch\u00fcttl'", "ich", "jetzt", "die", "Win\u00b7ter\u00b7flo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Absch\u00fcttl\u2019 ich hinterdrein die Welt, \u2014", "tokens": ["Ab\u00b7sch\u00fcttl'", "ich", "hin\u00b7ter\u00b7drein", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "ADV", "ART", "NN", "$,", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Nur leise noch von Schlittenglocken", "tokens": ["Nur", "lei\u00b7se", "noch", "von", "Schlit\u00b7ten\u00b7glo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein ferner Klang her\u00fcbergellt.", "tokens": ["Ein", "fer\u00b7ner", "Klang", "her\u00b7\u00fc\u00b7ber\u00b7gellt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nun ist es still; nun la\u00df uns kosen:", "tokens": ["Nun", "ist", "es", "still", ";", "nun", "la\u00df", "uns", "ko\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKVZ", "$.", "ADV", "VVIMP", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du legst Dein Haupt auf meinen Schoo\u00df,", "tokens": ["Du", "legst", "Dein", "Haupt", "auf", "mei\u00b7nen", "Schoo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich aber kn\u00fcpf\u2019 in leichtem, losen", "tokens": ["Ich", "a\u00b7ber", "kn\u00fcpf'", "in", "leich\u00b7tem", ",", "lo\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "ADJA", "$,", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Get\u00e4ndel Dir die Flechten los.", "tokens": ["Ge\u00b7t\u00e4n\u00b7del", "Dir", "die", "Flech\u00b7ten", "los", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du z\u00fcrnst; warum? Du glaubst zu m\u00fcssen,", "tokens": ["Du", "z\u00fcrnst", ";", "wa\u00b7rum", "?", "Du", "glaubst", "zu", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PWAV", "$.", "PPER", "VVFIN", "PTKZU", "VMINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und schw\u00f6rst: \u201enie wieder einen Ku\u00df!\u201c", "tokens": ["Und", "schw\u00f6rst", ":", "\u201e", "nie", "wie\u00b7der", "ei\u00b7nen", "Ku\u00df", "!", "\u201c"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "ADV", "ADV", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da wei\u00df ich, da\u00df ich rasch mit K\u00fcssen", "tokens": ["Da", "wei\u00df", "ich", ",", "da\u00df", "ich", "rasch", "mit", "K\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die krause Stirn Dir gl\u00e4tten mu\u00df.", "tokens": ["Die", "krau\u00b7se", "Stirn", "Dir", "gl\u00e4t\u00b7ten", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "\u201enun aber komm, nun la\u00df uns plaudern", "tokens": ["\u201e", "nun", "a\u00b7ber", "komm", ",", "nun", "la\u00df", "uns", "plau\u00b7dern"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ADV", "ADV", "VVFIN", "$,", "ADV", "VVIMP", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom eignen Herd, von Hof und Haus!\u201c", "tokens": ["Vom", "eig\u00b7nen", "Herd", ",", "von", "Hof", "und", "Haus", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "NE", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da baust Du lachend, ohne Zaudern,", "tokens": ["Da", "baust", "Du", "la\u00b7chend", ",", "oh\u00b7ne", "Zau\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "KOUI", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bis unter\u2019s Dach die Zukunft aus;", "tokens": ["Bis", "un\u00b7ter's", "Dach", "die", "Zu\u00b7kunft", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du h\u00e4ngst an meines Zimmers W\u00e4nde", "tokens": ["Du", "h\u00e4ngst", "an", "mei\u00b7nes", "Zim\u00b7mers", "W\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "All meine Lieblingsschilderein, \u2014", "tokens": ["All", "mei\u00b7ne", "Lieb\u00b7lings\u00b7schil\u00b7de\u00b7rein", ","], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich seh\u2019s und streck danach die H\u00e4nde,", "tokens": ["Ich", "seh's", "und", "streck", "da\u00b7nach", "die", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "KON", "VVFIN", "PAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Als m\u00fcss\u2019 es wahr und wirklich sein.", "tokens": ["Als", "m\u00fcss'", "es", "wahr", "und", "wirk\u00b7lich", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "ADJD", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So flieht des Abends sch\u00f6ne Stunde,", "tokens": ["So", "flieht", "des", "A\u00b7bends", "sch\u00f6\u00b7ne", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom fernen Thurm t\u00f6nt\u2019s Mitternacht,", "tokens": ["Vom", "fer\u00b7nen", "Thurm", "t\u00f6nt's", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Mutter schl\u00e4ft, in stiller Runde", "tokens": ["Die", "Mut\u00b7ter", "schl\u00e4ft", ",", "in", "stil\u00b7ler", "Run\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur noch die Wanduhr pickt und wacht.", "tokens": ["Nur", "noch", "die", "Wan\u00b7duhr", "pickt", "und", "wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u201eade, mein Lieb!\u201c von warmen Lippen", "tokens": ["\u201e", "a\u00b7de", ",", "mein", "Lieb", "!", "\u201c", "von", "war\u00b7men", "Lip\u00b7pen"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["$(", "ADV", "$,", "PPOSAT", "NN", "$.", "$(", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Ein Ku\u00df noch, \u2014 dann in Nacht hinein:", "tokens": ["Ein", "Ku\u00df", "noch", ",", "dann", "in", "Nacht", "hin\u00b7ein", ":"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "$(", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "\u201e", "tokens": ["\u201e"], "token_info": ["punct"], "pos": ["$("]}, "line.8": {"text": "Klippen,", "tokens": ["Klip\u00b7pen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Nur Steurer mu\u00df die Liebe sein.&#8220;", "tokens": ["Nur", "Steu\u00b7rer", "mu\u00df", "die", "Lie\u00b7be", "sein", ".", "&#8220;"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "XML_entity"], "pos": ["ADV", "NN", "VMFIN", "ART", "NN", "VAINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}