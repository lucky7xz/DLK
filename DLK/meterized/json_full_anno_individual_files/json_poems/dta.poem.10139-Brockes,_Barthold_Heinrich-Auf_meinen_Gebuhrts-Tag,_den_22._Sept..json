{"dta.poem.10139": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf meinen Gebuhrts-Tag,  \n den 22. Sept.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Heut j\u00e4hrig hast Du mich auf Erden,", "tokens": ["Heut", "j\u00e4h\u00b7rig", "hast", "Du", "mich", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein GOTT, gebohren lassen werden.", "tokens": ["Mein", "GoTT", ",", "ge\u00b7boh\u00b7ren", "las\u00b7sen", "wer\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVPP", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du hast mir gar, in diesem Leben,", "tokens": ["Du", "hast", "mir", "gar", ",", "in", "die\u00b7sem", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nunmehr schon funfzig Jahr gegeben,", "tokens": ["Nun\u00b7mehr", "schon", "funf\u00b7zig", "Jahr", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "CARD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und in der gantzen Lebens-Zeit", "tokens": ["Und", "in", "der", "gant\u00b7zen", "Le\u00b7bens\u00b7Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mich wunder-wunderbar gef\u00fchret:", "tokens": ["Mich", "wun\u00b7der\u00b7wun\u00b7der\u00b7bar", "ge\u00b7f\u00fch\u00b7ret", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wof\u00fcr, Quell aller G\u00fctigkeit,", "tokens": ["Wo\u00b7f\u00fcr", ",", "Quell", "al\u00b7ler", "G\u00fc\u00b7tig\u00b7keit", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "PIAT", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Nur Dir Lob, Ehr und Preis geb\u00fchret.", "tokens": ["Nur", "Dir", "Lob", ",", "Ehr", "und", "Preis", "ge\u00b7b\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "NN", "$,", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie wenig unter vielen seyn,", "tokens": ["Wie", "we\u00b7nig", "un\u00b7ter", "vie\u00b7len", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "PIAT", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die an Gem\u00fcths- und Leibs-St\u00e4rck und Gesundheits-Gaben", "tokens": ["Die", "an", "Ge\u00b7m\u00fcths", "und", "Leibs\u00b7St\u00e4rck", "und", "Ge\u00b7sund\u00b7heits\u00b7Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "TRUNC", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gl\u00fcck, G\u00fcter, Ehre, Dir allein,", "tokens": ["Gl\u00fcck", ",", "G\u00fc\u00b7ter", ",", "Eh\u00b7re", ",", "Dir", "al\u00b7lein", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O GOTT, so viel zu dancken haben!", "tokens": ["O", "GoTT", ",", "so", "viel", "zu", "dan\u00b7cken", "ha\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADV", "PIS", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mein Eh-Gemahl, so viele Kinder,", "tokens": ["Mein", "Eh\u00b7Ge\u00b7mahl", ",", "so", "vie\u00b7le", "Kin\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die all\u2019, an Seel\u2019 und Leib, nicht minder,", "tokens": ["Die", "all'", ",", "an", "Seel'", "und", "Leib", ",", "nicht", "min\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PIS", "$,", "APPR", "NN", "KON", "NN", "$,", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Als ich, gesund sind, zeigens an;", "tokens": ["Als", "ich", ",", "ge\u00b7sund", "sind", ",", "zei\u00b7gens", "an", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADJD", "VAFIN", "$,", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Absonderlich da\u00df mein Gem\u00fcthe", "tokens": ["Ab\u00b7son\u00b7der\u00b7lich", "da\u00df", "mein", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Von Deiner Weisheit, Macht und G\u00fcte", "tokens": ["Von", "Dei\u00b7ner", "Weis\u00b7heit", ",", "Macht", "und", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "So viel gesehn, und sehen kann.", "tokens": ["So", "viel", "ge\u00b7sehn", ",", "und", "se\u00b7hen", "kann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$,", "KON", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Da\u00df ich, o HERR, f\u00fcr Deine Wercke", "tokens": ["Da\u00df", "ich", ",", "o", "HeRR", ",", "f\u00fcr", "Dei\u00b7ne", "Wer\u00b7cke"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "FM", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Ein aufmercksames Aug\u2019 empfing;", "tokens": ["Ein", "auf\u00b7merck\u00b7sa\u00b7mes", "Aug'", "emp\u00b7fing", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Da\u00df Deiner Lieb\u2019 und Weisheit St\u00e4rcke", "tokens": ["Da\u00df", "Dei\u00b7ner", "Lieb'", "und", "Weis\u00b7heit", "St\u00e4r\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "KON", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Mir offt durchs Aug\u2019 ans Hertze ging;", "tokens": ["Mir", "offt", "durchs", "Aug'", "ans", "Hert\u00b7ze", "ging", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "APPRART", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "So da\u00df ich, inniglich ger\u00fchret,", "tokens": ["So", "da\u00df", "ich", ",", "in\u00b7nig\u00b7lich", "ge\u00b7r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Auch andern durch den Druck gezeigt,", "tokens": ["Auch", "an\u00b7dern", "durch", "den", "Druck", "ge\u00b7zeigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Wie man durch sie zu Dir gef\u00fchret,", "tokens": ["Wie", "man", "durch", "sie", "zu", "Dir", "ge\u00b7f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Und durchs Gesch\u00f6pf zum Sch\u00f6pfer steigt.", "tokens": ["Und", "durchs", "Ge\u00b7sch\u00f6pf", "zum", "Sch\u00f6p\u00b7fer", "steigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O ew\u2019ge Liebe! willst Du mich", "tokens": ["O", "ew'\u00b7ge", "Lie\u00b7be", "!", "willst", "Du", "mich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$.", "VMFIN", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch l\u00e4nger hier auf Erden lassen;", "tokens": ["Noch", "l\u00e4n\u00b7ger", "hier", "auf", "Er\u00b7den", "las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So gieb, da\u00df ich, in ihnen, Dich", "tokens": ["So", "gieb", ",", "da\u00df", "ich", ",", "in", "ih\u00b7nen", ",", "Dich"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "VVIMP", "$,", "KOUS", "PPER", "$,", "APPR", "PPER", "$,", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch immer mehr vermag zu fassen!", "tokens": ["Noch", "im\u00b7mer", "mehr", "ver\u00b7mag", "zu", "fas\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Verleihe mir Gelegenheit!", "tokens": ["Ver\u00b7lei\u00b7he", "mir", "Ge\u00b7le\u00b7gen\u00b7heit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gieb Weisheit! sch\u00e4rffe meine Sinnen!", "tokens": ["Gieb", "Weis\u00b7heit", "!", "sch\u00e4rf\u00b7fe", "mei\u00b7ne", "Sin\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "$.", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Gieb Eifer! gieb Bequemlichkeit,", "tokens": ["Gieb", "Ei\u00b7fer", "!", "gieb", "Be\u00b7quem\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "NN", "$.", "VVIMP", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und segne ferner mein beginnen!", "tokens": ["Und", "seg\u00b7ne", "fer\u00b7ner", "mein", "be\u00b7gin\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Damit, von allen Undancks-Wust", "tokens": ["Da\u00b7mit", ",", "von", "al\u00b7len", "Un\u00b7dancks\u00b7Wust"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PAV", "$,", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wir uns ie mehr und mehr entfernen,", "tokens": ["Wir", "uns", "ie", "mehr", "und", "mehr", "ent\u00b7fer\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "ADV", "ADV", "KON", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und Dich, mit Lieb\u2019 erf\u00fcllter Brust,", "tokens": ["Und", "Dich", ",", "mit", "Lieb'", "er\u00b7f\u00fcll\u00b7ter", "Brust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Als wahren GOTT, erkennen lernen!", "tokens": ["Als", "wah\u00b7ren", "GoTT", ",", "er\u00b7ken\u00b7nen", "ler\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$,", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Soll aber, HERR, nach Deinem Raht,", "tokens": ["Soll", "a\u00b7ber", ",", "HeRR", ",", "nach", "Dei\u00b7nem", "Raht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "$,", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich bald vergehen und erkalten;", "tokens": ["Ich", "bald", "ver\u00b7ge\u00b7hen", "und", "er\u00b7kal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So la\u00df mit Worten, Schrift und That", "tokens": ["So", "la\u00df", "mit", "Wor\u00b7ten", ",", "Schrift", "und", "That"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVIMP", "APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Meinigen mein Ampt verwalten!", "tokens": ["Die", "Mei\u00b7ni\u00b7gen", "mein", "Ampt", "ver\u00b7wal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSS", "PPOSAT", "NN", "VVINF", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Und mich in jenen seelgen H\u00f6hn,", "tokens": ["Und", "mich", "in", "je\u00b7nen", "seel\u00b7gen", "H\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wozu uns Deine Gnade rieffe,", "tokens": ["Wo\u00b7zu", "uns", "Dei\u00b7ne", "Gna\u00b7de", "rief\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die unergr\u00fcndlich tieffe Tieffe", "tokens": ["Die", "un\u00b7er\u00b7gr\u00fcnd\u00b7lich", "tief\u00b7fe", "Tief\u00b7fe"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der wahren Gottheit ewig sehn!", "tokens": ["Der", "wah\u00b7ren", "Got\u00b7theit", "e\u00b7wig", "sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}