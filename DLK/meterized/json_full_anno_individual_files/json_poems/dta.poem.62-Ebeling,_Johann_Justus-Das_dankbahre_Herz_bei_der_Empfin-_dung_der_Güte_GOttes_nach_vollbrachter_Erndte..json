{"dta.poem.62": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Das  \n dankbahre Herz bei der Empfin-  \n dung der G\u00fcte GOttes nach  \n vollbrachter Erndte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198797", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Grosser Sch\u00f6pfer! deine Gaben,", "tokens": ["Gros\u00b7ser", "Sch\u00f6p\u00b7fer", "!", "dei\u00b7ne", "Ga\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Sprossen deiner G\u00fct", "tokens": ["Und", "die", "Spros\u00b7sen", "dei\u00b7ner", "G\u00fct"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die die Zungen reichlich laben,", "tokens": ["Die", "die", "Zun\u00b7gen", "reich\u00b7lich", "la\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die erfreuen das Gem\u00fcth,", "tokens": ["Die", "er\u00b7freu\u00b7en", "das", "Ge\u00b7m\u00fcth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Treiben mich in holden Singen,", "tokens": ["Trei\u00b7ben", "mich", "in", "hol\u00b7den", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ein Dankopfer Dir zu bringen.", "tokens": ["Ein", "Dan\u00b7kop\u00b7fer", "Dir", "zu", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Welch ein \u00fcberreicher Seegen,", "tokens": ["Welch", "ein", "\u00fc\u00b7berr\u00b7ei\u00b7cher", "See\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deiner ewgen G\u00fctigkeit,", "tokens": ["Dei\u00b7ner", "ew\u00b7gen", "G\u00fc\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat uns durch ein wallend Regen,", "tokens": ["Hat", "uns", "durch", "ein", "wal\u00b7lend", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fetter Frucht, das Herz erfreut!", "tokens": ["Fet\u00b7ter", "Frucht", ",", "das", "Herz", "er\u00b7freut", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da der Felder dichte Halmen,", "tokens": ["Da", "der", "Fel\u00b7der", "dich\u00b7te", "Hal\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Uns bewegt zu Lob und Psalmen!", "tokens": ["Uns", "be\u00b7wegt", "zu", "Lob", "und", "Psal\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nunmehr sind sie eingescheuret,", "tokens": ["Nun\u00b7mehr", "sind", "sie", "ein\u00b7ge\u00b7scheu\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der Mund geniesset schon,", "tokens": ["Und", "der", "Mund", "ge\u00b7nies\u00b7set", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was die Allmacht hat verneuret:", "tokens": ["Was", "die", "All\u00b7macht", "hat", "ver\u00b7neu\u00b7ret", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nunmehr sol mein Jubelthon,", "tokens": ["Nun\u00b7mehr", "sol", "mein", "Ju\u00b7bel\u00b7thon", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aus vergn\u00fcgter Seele fliessen,", "tokens": ["Aus", "ver\u00b7gn\u00fcg\u00b7ter", "See\u00b7le", "flies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da wir deines Guts geniessen.", "tokens": ["Da", "wir", "dei\u00b7nes", "Guts", "ge\u00b7nies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Vater! solte nicht die Seele,", "tokens": ["Va\u00b7ter", "!", "sol\u00b7te", "nicht", "die", "See\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VMFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Satt von deiner G\u00fcte seyn,", "tokens": ["Satt", "von", "dei\u00b7ner", "G\u00fc\u00b7te", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da durch leibliche Can\u00e4le", "tokens": ["Da", "durch", "leib\u00b7li\u00b7che", "Ca\u00b7n\u00e4\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deines Seegens Gnadenschein,", "tokens": ["Dei\u00b7nes", "See\u00b7gens", "Gna\u00b7den\u00b7schein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da durch k\u00f6rperliche Sinnen,", "tokens": ["Da", "durch", "k\u00f6r\u00b7per\u00b7li\u00b7che", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "--++--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "So viel Wolluststr\u00f6me rinnen?", "tokens": ["So", "viel", "Wol\u00b7lust\u00b7str\u00f6\u00b7me", "rin\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wir geniessen lauter Wunder,", "tokens": ["Wir", "ge\u00b7nies\u00b7sen", "lau\u00b7ter", "Wun\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die die weise Allmacht zeigt,", "tokens": ["Die", "die", "wei\u00b7se", "All\u00b7macht", "zeigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und daraus entbrennt der Zunder", "tokens": ["Und", "da\u00b7raus", "ent\u00b7brennt", "der", "Zun\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "VVFIN", "ART", "NN"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "Der das Herz zur Liebe neigt:", "tokens": ["Der", "das", "Herz", "zur", "Lie\u00b7be", "neigt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Du giebst uns das Korngerichte,", "tokens": ["Du", "giebst", "uns", "das", "Korn\u00b7ge\u00b7rich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Nim davor der Andacht Fr\u00fcchte.", "tokens": ["Nim", "da\u00b7vor", "der", "An\u00b7dacht", "Fr\u00fcch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PAV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "So viel K\u00f6rner, so viel Halmen,", "tokens": ["So", "viel", "K\u00f6r\u00b7ner", ",", "so", "viel", "Hal\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Uns die Erndte hat beschert;", "tokens": ["Uns", "die", "Ernd\u00b7te", "hat", "be\u00b7schert", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So viel Lob und Dank und Psalmen,", "tokens": ["So", "viel", "Lob", "und", "Dank", "und", "Psal\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Verdient der, der uns ern\u00e4hrt!", "tokens": ["Ver\u00b7di\u00b7ent", "der", ",", "der", "uns", "er\u00b7n\u00e4hrt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "W\u00e4ren alle Glieder Zungen,", "tokens": ["W\u00e4\u00b7ren", "al\u00b7le", "Glie\u00b7der", "Zun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "W\u00fcrdest Du nicht gnug besungen.", "tokens": ["W\u00fcr\u00b7dest", "Du", "nicht", "gnug", "be\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "So viel Nahrungsreiche Bissen,", "tokens": ["So", "viel", "Nah\u00b7rungs\u00b7rei\u00b7che", "Bis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus der K\u00f6rner Mark entstehn,", "tokens": ["Aus", "der", "K\u00f6r\u00b7ner", "Mark", "ent\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wir dieses Jahr geniessen:", "tokens": ["Und", "wir", "die\u00b7ses", "Jahr", "ge\u00b7nies\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PDAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So viel mu\u00df man dich erh\u00f6hn!", "tokens": ["So", "viel", "mu\u00df", "man", "dich", "er\u00b7h\u00f6hn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PIS", "PRF", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "M\u00f6chte doch der Zungen Schmekken,", "tokens": ["M\u00f6ch\u00b7te", "doch", "der", "Zun\u00b7gen", "Schmek\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dazu meinen Geist erwekken!", "tokens": ["Da\u00b7zu", "mei\u00b7nen", "Geist", "er\u00b7wek\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Auf mein Herz! stets zu bedenken,", "tokens": ["Auf", "mein", "Herz", "!", "stets", "zu", "be\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was die Vorsicht hat gethan;", "tokens": ["Was", "die", "Vor\u00b7sicht", "hat", "ge\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So wird sie die Triebe lenken,", "tokens": ["So", "wird", "sie", "die", "Trie\u00b7be", "len\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da sie alles lenken kan,", "tokens": ["Da", "sie", "al\u00b7les", "len\u00b7ken", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df ich k\u00f6nn aus reiner Seelen,", "tokens": ["Da\u00df", "ich", "k\u00f6nn", "aus", "rei\u00b7ner", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Was mein Mund geschmekt, erz\u00e4hlen!", "tokens": ["Was", "mein", "Mund", "ge\u00b7schmekt", ",", "er\u00b7z\u00e4h\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVPP", "$,", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}