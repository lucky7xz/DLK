{"textgrid.poem.56734": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der alte, ehrw\u00fcrdige Herr", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der alte, ehrw\u00fcrdige Herr", "tokens": ["Der", "al\u00b7te", ",", "ehr\u00b7w\u00fcr\u00b7di\u00b7ge", "Herr"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "mit dem gro\u00dfen Bart", "tokens": ["mit", "dem", "gro\u00b7\u00dfen", "Bart"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "war heute bei mir.", "tokens": ["war", "heu\u00b7te", "bei", "mir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPER", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "\u00bbich habe dich gestern gerettet!\u00ab", "tokens": ["\u00bb", "ich", "ha\u00b7be", "dich", "ge\u00b7stern", "ge\u00b7ret\u00b7tet", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "sagte er freundlich.", "tokens": ["sag\u00b7te", "er", "freund\u00b7lich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "\u00bbden Einfall, die Arme", "tokens": ["\u00bb", "den", "Ein\u00b7fall", ",", "die", "Ar\u00b7me"], "token_info": ["punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "ART", "NN", "$,", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "zur Kreuzform zu strecken,", "tokens": ["zur", "Kreuz\u00b7form", "zu", "stre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "hab ich dir gesteckt.\u00ab", "tokens": ["hab", "ich", "dir", "ge\u00b7steckt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "PPER", "VVPP", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Ich sch\u00fcttelte dankbar", "tokens": ["Ich", "sch\u00fct\u00b7tel\u00b7te", "dank\u00b7bar"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "die biedere Rechte.", "tokens": ["die", "bie\u00b7de\u00b7re", "Rech\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Er aber drohte mir", "tokens": ["Er", "a\u00b7ber", "droh\u00b7te", "mir"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "mit dem Finger:", "tokens": ["mit", "dem", "Fin\u00b7ger", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "--+-", "measure": "anapaest.init"}, "line.13": {"text": "\u00bbein Schelm bleibst du doch!", "tokens": ["\u00bb", "ein", "Schelm", "bleibst", "du", "doch", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-++-+", "measure": "unknown.measure.tri"}, "line.14": {"text": "Ich traue dir nicht.", "tokens": ["Ich", "trau\u00b7e", "dir", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.15": {"text": "Doch h\u00f6re!\u00ab", "tokens": ["Doch", "h\u00f6\u00b7re", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "$.", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.16": {"text": "Und er kniff mir den Arm", "tokens": ["Und", "er", "kniff", "mir", "den", "Arm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ART", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.17": {"text": "und zeigte mir rings", "tokens": ["und", "zeig\u00b7te", "mir", "rings"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.18": {"text": "die Lande \u2013:", "tokens": ["die", "Lan\u00b7de", "\u2013", ":"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ART", "NN", "$(", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.19": {"text": "\u00bbdies alles soll dein sein,", "tokens": ["\u00bb", "dies", "al\u00b7les", "soll", "dein", "sein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "PIS", "VMFIN", "PPOSAT", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "wenn du hier hinf\u00e4llst", "tokens": ["wenn", "du", "hier", "hin\u00b7f\u00e4llst"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.21": {"text": "und mich anbetest.\u00ab", "tokens": ["und", "mich", "an\u00b7be\u00b7test", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.22": {"text": "Der Arme, er wu\u00dfte nicht,", "tokens": ["Der", "Ar\u00b7me", ",", "er", "wu\u00df\u00b7te", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "PTKNEG", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.23": {"text": "da\u00df Erde und Himmel", "tokens": ["da\u00df", "Er\u00b7de", "und", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.24": {"text": "durch Phanta l\u00e4ngst mein war.", "tokens": ["durch", "Phan\u00b7ta", "l\u00e4ngst", "mein", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "PPOSAT", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "\u00bbnun, willst du nicht?\u00ab", "tokens": ["\u00bb", "nun", ",", "willst", "du", "nicht", "?", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "$,", "VMFIN", "PPER", "PTKNEG", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.26": {"text": "rief er halb \u00e4ngstlich", "tokens": ["rief", "er", "halb", "\u00e4ngst\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "ADJD"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.27": {"text": "halb \u00e4rgerlich.", "tokens": ["halb", "\u00e4r\u00b7ger\u00b7lich", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ich aber machte ihm", "tokens": ["Ich", "a\u00b7ber", "mach\u00b7te", "ihm"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "schnell eine kalte Kompresse", "tokens": ["schnell", "ei\u00b7ne", "kal\u00b7te", "Kom\u00b7pres\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "um die erhitzten Schl\u00e4fen", "tokens": ["um", "die", "er\u00b7hitz\u00b7ten", "Schl\u00e4\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "und f\u00fchrte ihn sorgsam", "tokens": ["und", "f\u00fchr\u00b7te", "ihn", "sorg\u00b7sam"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "den Berg hinunter.", "tokens": ["den", "Berg", "hin\u00b7un\u00b7ter", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Auf halber H\u00f6he", "tokens": ["Auf", "hal\u00b7ber", "H\u00f6\u00b7he"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "traf ich den gro\u00dfen Pan.", "tokens": ["traf", "ich", "den", "gro\u00b7\u00dfen", "Pan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.8": {"text": "Er wollte gerade", "tokens": ["Er", "woll\u00b7te", "ge\u00b7ra\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "eine Windhosen-Orgel bauen.", "tokens": ["ei\u00b7ne", "Wind\u00b7ho\u00b7sen\u00b7Or\u00b7gel", "bau\u00b7en", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "Doch ich entri\u00df ihn", "tokens": ["Doch", "ich", "ent\u00b7ri\u00df", "ihn"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "dem k\u00fchnen Projekte", "tokens": ["dem", "k\u00fch\u00b7nen", "Pro\u00b7jek\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "und stellte ihm", "tokens": ["und", "stell\u00b7te", "ihm"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "seinen greisen Kollegen vor.", "tokens": ["sei\u00b7nen", "grei\u00b7sen", "Kol\u00b7le\u00b7gen", "vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.14": {"text": "\u00bbalte Bekanntschaft!\u00ab rief Pan", "tokens": ["\u00bb", "al\u00b7te", "Be\u00b7kannt\u00b7schaft", "!", "\u00ab", "rief", "Pan"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word"], "pos": ["$(", "ADJA", "NN", "$.", "$(", "VVFIN", "NE"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.15": {"text": "und zog die krumme Nase", "tokens": ["und", "zog", "die", "krum\u00b7me", "Na\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "mi\u00dfmutig noch kr\u00fcmmer.", "tokens": ["mi\u00df\u00b7mu\u00b7tig", "noch", "kr\u00fcm\u00b7mer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ADJD", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.17": {"text": "\u00bbvielleicht hilft er dir", "tokens": ["\u00bb", "viel\u00b7leicht", "hilft", "er", "dir"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VVFIN", "PPER", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.18": {"text": "bei der Windhosen-Orgel!\u00ab", "tokens": ["bei", "der", "Wind\u00b7ho\u00b7sen\u00b7Or\u00b7gel", "!", "\u00ab"], "