{"dta.poem.21031": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "XcV.  \n Einer unbest\u00e4ndigen Jungfrauen.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Leben \u00e4ndert\u2019 ich nach meiner Augenblicke/", "tokens": ["Mein", "Le\u00b7ben", "\u00e4n\u00b7dert'", "ich", "nach", "mei\u00b7ner", "Au\u00b7gen\u00b7bli\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Unbest\u00e4ndigkeit besch\u00e4mt ich das Gel\u00fccke.", "tokens": ["Mit", "Un\u00b7be\u00b7st\u00e4n\u00b7dig\u00b7keit", "be\u00b7sch\u00e4mt", "ich", "das", "Ge\u00b7l\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die gr\u00f6ste B\u00fcberey hat mir der Tod gethan/", "tokens": ["Die", "gr\u00f6s\u00b7te", "B\u00fc\u00b7be\u00b7rey", "hat", "mir", "der", "Tod", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dieweil ich dieses Grab auch nicht ver\u00e4ndern", "tokens": ["Die\u00b7weil", "ich", "die\u00b7ses", "Grab", "auch", "nicht", "ver\u00b7\u00e4n\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PDAT", "NN", "ADV", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "kan.", "tokens": ["kan", "."], "token_info": ["word", "punct"], "pos": ["VMFIN", "$."], "meter": "-", "measure": "single.down"}}}}}