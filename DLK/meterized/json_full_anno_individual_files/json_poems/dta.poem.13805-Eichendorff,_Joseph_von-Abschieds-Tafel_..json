{"dta.poem.13805": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Abschieds-Tafel .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Nun, Morgenlohe, h\u00fclle,", "tokens": ["Nun", ",", "Mor\u00b7gen\u00b7lo\u00b7he", ",", "h\u00fcl\u00b7le", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Glorien Dein Reich!", "tokens": ["In", "Glo\u00b7ri\u00b7en", "Dein", "Reich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dir, tieflebend'ge F\u00fclle,", "tokens": ["Dir", ",", "tief\u00b7le\u00b7ben\u00b7d'\u00b7ge", "F\u00fcl\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Schleud'r' ich das Glas hinunter,", "tokens": ["Schleu\u00b7d'\u00b7r'", "ich", "das", "Glas", "hin\u00b7un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mir schwindeln alle Sinnen,", "tokens": ["Mir", "schwin\u00b7deln", "al\u00b7le", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So wend' ich mich geblendet,", "tokens": ["So", "wend'", "ich", "mich", "ge\u00b7blen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Gott segne Dich und Euch!", "tokens": ["Gott", "seg\u00b7ne", "Dich", "und", "Euch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}