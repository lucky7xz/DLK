{"dta.poem.8433": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Tod.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Gottmensch, wann die seligste der Stunden,", "tokens": ["Gott\u00b7mensch", ",", "wann", "die", "se\u00b7ligs\u00b7te", "der", "Stun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "ADJA", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Welche Kronen auf der Wage hat,", "tokens": ["Wel\u00b7che", "Kro\u00b7nen", "auf", "der", "Wa\u00b7ge", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "APPR", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Meinem Sterbebette naht!", "tokens": ["Mei\u00b7nem", "Ster\u00b7be\u00b7bet\u00b7te", "naht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dann beschatte mich, o Ruh, mit linden", "tokens": ["Dann", "be\u00b7schat\u00b7te", "mich", ",", "o", "Ruh", ",", "mit", "lin\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "FM", "NN", "$,", "APPR", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Stillen Fl\u00fcgeln! Geister meiner S\u00fcnden,", "tokens": ["Stil\u00b7len", "Fl\u00fc\u00b7geln", "!", "Geis\u00b7ter", "mei\u00b7ner", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Nahet euch dem Sterbelager nicht,", "tokens": ["Na\u00b7het", "euch", "dem", "Ster\u00b7be\u00b7la\u00b7ger", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wo mein schwimmend Auge bricht!", "tokens": ["Wo", "mein", "schwim\u00b7mend", "Au\u00b7ge", "bricht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJD", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Du mein Engel, komm von Gottes Throne,", "tokens": ["Du", "mein", "En\u00b7gel", ",", "komm", "von", "Got\u00b7tes", "Thro\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,", "VVFIN", "APPR", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Bringe mit die helle Siegerkrone,", "tokens": ["Brin\u00b7ge", "mit", "die", "hel\u00b7le", "Sie\u00b7ger\u00b7kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wehe Himmelsluft und Engelsruh", "tokens": ["We\u00b7he", "Him\u00b7mel\u00b7sluft", "und", "En\u00b7gels\u00b7ruh"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "KON", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Mir mit deiner Palme zu!", "tokens": ["Mir", "mit", "dei\u00b7ner", "Pal\u00b7me", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Leite mich auf tausend Sonnenwegen", "tokens": ["Lei\u00b7te", "mich", "auf", "tau\u00b7send", "Son\u00b7nen\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "CARD", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Jenem Engelparadies' entgegen,", "tokens": ["Je\u00b7nem", "En\u00b7gel\u00b7pa\u00b7ra\u00b7dies'", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wo die Gute, welche mich gebar,", "tokens": ["Wo", "die", "Gu\u00b7te", ",", "wel\u00b7che", "mich", "ge\u00b7bar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "PRF", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Schon so lange gl\u00fccklich war;", "tokens": ["Schon", "so", "lan\u00b7ge", "gl\u00fcck\u00b7lich", "war", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wo die jungen Geister meiner Br\u00fcder", "tokens": ["Wo", "die", "jun\u00b7gen", "Geis\u00b7ter", "mei\u00b7ner", "Br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Unter Blumen spielen, s\u00fcsse Lieder", "tokens": ["Un\u00b7ter", "Blu\u00b7men", "spie\u00b7len", ",", "s\u00fcs\u00b7se", "Lie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "VVINF", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "In die Lauten singen, jung und sch\u00f6n", "tokens": ["In", "die", "Lau\u00b7ten", "sin\u00b7gen", ",", "jung", "und", "sch\u00f6n"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF", "$,", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Zwischen Engeln um mich stehn!", "tokens": ["Zwi\u00b7schen", "En\u00b7geln", "um", "mich", "stehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wohnt' ich doch, von diesem Erdgewimmel", "tokens": ["Wohnt'", "ich", "doch", ",", "von", "die\u00b7sem", "Erd\u00b7ge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "$,", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schon entfernt, in eurem Freudenhimmel,", "tokens": ["Schon", "ent\u00b7fernt", ",", "in", "eu\u00b7rem", "Freu\u00b7den\u00b7him\u00b7mel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Theure Seelen! Kniet' ich, kniet' ich schon", "tokens": ["Theu\u00b7re", "See\u00b7len", "!", "Kniet'", "ich", ",", "kniet'", "ich", "schon"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "An des Gottvers\u00f6hners Thron!", "tokens": ["An", "des", "Gott\u00b7ver\u00b7s\u00f6h\u00b7ners", "Thron", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}