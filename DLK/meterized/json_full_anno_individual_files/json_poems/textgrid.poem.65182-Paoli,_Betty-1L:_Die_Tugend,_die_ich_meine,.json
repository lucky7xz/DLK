{"textgrid.poem.65182": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Tugend, die ich meine,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Tugend, die ich meine,", "tokens": ["Die", "Tu\u00b7gend", ",", "die", "ich", "mei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "F\u00fcr die mein Herz in Brand,", "tokens": ["F\u00fcr", "die", "mein", "Herz", "in", "Brand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Abhold dem eitlen Scheine,", "tokens": ["Ab\u00b7hold", "dem", "eit\u00b7len", "Schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Tr\u00e4gt sie ein schlicht Gewand.", "tokens": ["Tr\u00e4gt", "sie", "ein", "schlicht", "Ge\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJD", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Sie r\u00fchrt den Sinn der Menge", "tokens": ["Sie", "r\u00fchrt", "den", "Sinn", "der", "Men\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit holdem Reize nicht;", "tokens": ["Mit", "hol\u00b7dem", "Rei\u00b7ze", "nicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Aus ihrem Aug' blickt Strenge,", "tokens": ["Aus", "ih\u00b7rem", "Aug'", "blickt", "Stren\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ernst ist ihr Angesicht.", "tokens": ["Ernst", "ist", "ihr", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sp\u00e4t reifen ihre Saaten,", "tokens": ["Sp\u00e4t", "rei\u00b7fen", "ih\u00b7re", "Saa\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und karg scheint ihr Gewinn;", "tokens": ["Und", "karg", "scheint", "ihr", "Ge\u00b7winn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es rei\u00dfen ihre Thaten", "tokens": ["Es", "rei\u00b7\u00dfen", "ih\u00b7re", "Tha\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nicht zur Bewund'rung hin.", "tokens": ["Nicht", "zur", "Be\u00b7wun\u00b7d'\u00b7rung", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Nach ewig heil'gen Zielen", "tokens": ["Nach", "e\u00b7wig", "heil'\u00b7gen", "Zie\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "F\u00e4hrt sie auf rauher Spur,", "tokens": ["F\u00e4hrt", "sie", "auf", "rau\u00b7her", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Geha\u00dft, verfolgt von vielen,", "tokens": ["Ge\u00b7ha\u00dft", ",", "ver\u00b7folgt", "von", "vie\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "APPR", "PIAT", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Geliebt von wen'gen nur.", "tokens": ["Ge\u00b7liebt", "von", "wen'\u00b7gen", "nur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wer k\u00fchn sich ihr will weihen,", "tokens": ["Wer", "k\u00fchn", "sich", "ihr", "will", "wei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "PRF", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der nehme wohl in acht:", "tokens": ["Der", "neh\u00b7me", "wohl", "in", "acht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "CARD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihm Lorbeer'n zu verleihen,", "tokens": ["Ihm", "Lor\u00b7beer'n", "zu", "ver\u00b7lei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Steht nicht in ihrer Macht!", "tokens": ["Steht", "nicht", "in", "ih\u00b7rer", "Macht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mit schmetternden Fanfaren", "tokens": ["Mit", "schmet\u00b7tern\u00b7den", "Fan\u00b7fa\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Begr\u00fc\u00dft ihn nicht der Ruhm", "tokens": ["Be\u00b7gr\u00fc\u00dft", "ihn", "nicht", "der", "Ruhm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "In seinem unscheinbaren,", "tokens": ["In", "sei\u00b7nem", "un\u00b7schein\u00b7ba\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Selbstlosen Heldentum.", "tokens": ["Selbst\u00b7lo\u00b7sen", "Hel\u00b7den\u00b7tum", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Sie aber, die er sch\u00fctzet,", "tokens": ["Sie", "a\u00b7ber", ",", "die", "er", "sch\u00fct\u00b7zet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der er sich zugesellt,", "tokens": ["Der", "er", "sich", "zu\u00b7ge\u00b7sellt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nur sie erh\u00e4lt und st\u00fctzet", "tokens": ["Nur", "sie", "er\u00b7h\u00e4lt", "und", "st\u00fct\u00b7zet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und tr\u00e4gt den Bau der Welt.", "tokens": ["Und", "tr\u00e4gt", "den", "Bau", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es ist die Hehre, Reine", "tokens": ["Es", "ist", "die", "Heh\u00b7re", ",", "Rei\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zu h\u00f6chstem Dienst geweiht!", "tokens": ["Zu", "h\u00f6chs\u00b7tem", "Dienst", "ge\u00b7weiht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Tugend, die ich meine,", "tokens": ["Die", "Tu\u00b7gend", ",", "die", "ich", "mei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ist die Gerechtigkeit.", "tokens": ["Ist", "die", "Ge\u00b7rech\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Die Tugend, die ich meine,", "tokens": ["Die", "Tu\u00b7gend", ",", "die", "ich", "mei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "F\u00fcr die mein Herz in Brand,", "tokens": ["F\u00fcr", "die", "mein", "Herz", "in", "Brand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Abhold dem eitlen Scheine,", "tokens": ["Ab\u00b7hold", "dem", "eit\u00b7len", "Schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "p