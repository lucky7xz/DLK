{"dta.poem.609": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Bey dem  \n Ottisch- und Ulrichschen  \n Hochzeit-Feste  \n  Gratulirte  zu dem aus dem Ulrichs.  Cabinet  erlangt\u1ebd Schau-st\u00fcck.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Bild/ das auf der Stirn des Vaters Nahmen f\u00fchret/", "tokens": ["Ein", "Bild", "/", "das", "auf", "der", "Stirn", "des", "Va\u00b7ters", "Nah\u00b7men", "f\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PDS", "APPR", "ART", "NN", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dem der Mutter Geist aus beyden Augen lacht/", "tokens": ["Und", "dem", "der", "Mut\u00b7ter", "Geist", "aus", "bey\u00b7den", "Au\u00b7gen", "lacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "NN", "NN", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das reiner Tugend-Gold am meisten kostbahr macht/", "tokens": ["Das", "rei\u00b7ner", "Tu\u00b7gen\u00b7dGold", "am", "meis\u00b7ten", "kost\u00b7bahr", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und da\u00df des Fleisses-Bild in seinem Wapen zieret *", "tokens": ["Und", "da\u00df", "des", "Fleis\u00b7ses\u00b7Bild", "in", "sei\u00b7nem", "Wa\u00b7pen", "zie\u00b7ret", "*"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "XY"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Desselben Kostbarkeit kein Silber kauffen kan/", "tokens": ["Des\u00b7sel\u00b7ben", "Kost\u00b7bar\u00b7keit", "kein", "Sil\u00b7ber", "kauf\u00b7fen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PIAT", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das trifft/ Herr Br\u00e4utigam/ er jetzund gl\u00fccklich an.", "tokens": ["Das", "trifft", "/", "Herr", "Br\u00e4u\u00b7ti\u00b7gam", "/", "er", "je\u00b7tzund", "gl\u00fcck\u00b7lich", "an", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$(", "NN", "NE", "$(", "PPER", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Herr Ulrichs ", "tokens": ["Herr", "Ul\u00b7richs"], "token_info": ["word", "word"], "pos": ["NN", "NE"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Die wohlgepreget sind von guten Korn und Schroot/", "tokens": ["Die", "wohl\u00b7ge\u00b7pre\u00b7get", "sind", "von", "gu\u00b7ten", "Korn", "und", "Schroot", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VAFIN", "APPR", "ADJA", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und die vor keinem Strich/ als nur allein dem Tod/", "tokens": ["Und", "die", "vor", "kei\u00b7nem", "Strich", "/", "als", "nur", "al\u00b7lein", "dem", "Tod", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "PIAT", "NN", "$(", "KOKOM", "ADV", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Den unverf\u00e4lschten Glantz der holden Wangen neigen/", "tokens": ["Den", "un\u00b7ver\u00b7f\u00e4lschten", "Glantz", "der", "hol\u00b7den", "Wan\u00b7gen", "nei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Derselben keine nur allein von aussen prahlt/", "tokens": ["Der\u00b7sel\u00b7ben", "kei\u00b7ne", "nur", "al\u00b7lein", "von", "aus\u00b7sen", "prahlt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADV", "ADV", "APPR", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Weil sie nicht triegend Schmuck noch F\u00fcrni\u00df angemahlt.", "tokens": ["Weil", "sie", "nicht", "trie\u00b7gend", "Schmuck", "noch", "F\u00fcr\u00b7ni\u00df", "an\u00b7ge\u00b7mahlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "NN", "ADV", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Dis eine Schau-st\u00fcck war vor ihn noch aufgehoben/", "tokens": ["Dis", "ei\u00b7ne", "Schau\u00b7st\u00fcck", "war", "vor", "ihn", "noch", "auf\u00b7ge\u00b7ho\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VAFIN", "APPR", "PPER", "ADV", "VVPP", "$("], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.14": {"text": "So diese Uberschrifft: ", "tokens": ["So", "die\u00b7se", "U\u00b7ber\u00b7schrifft", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Die Tugend selber hat auf den ", "tokens": ["Die", "Tu\u00b7gend", "sel\u00b7ber", "hat", "auf", "den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VAFIN", "APPR", "ART"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Um ihre muntre Stirn den Spruch: ", "tokens": ["Um", "ih\u00b7re", "mun\u00b7tre", "Stirn", "den", "Spruch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Die Rand-Schrifft ist hiebey: ", "tokens": ["Die", "Ran\u00b7dSchrifft", "ist", "hie\u00b7bey", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "Die Deutung zeiget jetzt/ da\u00df es Herr ", "tokens": ["Die", "Deu\u00b7tung", "zei\u00b7get", "jetzt", "/", "da\u00df", "es", "Herr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "$(", "KOUS", "PPER", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Ich w\u00fcnsche Gl\u00fcck dazu! GOtt segne Kauff und Waare/", "tokens": ["Ich", "w\u00fcn\u00b7sche", "Gl\u00fcck", "da\u00b7zu", "!", "Gott", "seg\u00b7ne", "Kauff", "und", "Waa\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PAV", "$.", "NN", "ADJA", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und lasse Zucker-Brod stets eure Speise seyn/", "tokens": ["Und", "las\u00b7se", "Zu\u00b7cke\u00b7rBrod", "stets", "eu\u00b7re", "Spei\u00b7se", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "ADV", "PPOSAT", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Er schenck euch ", "tokens": ["Er", "schenck", "euch"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.22": {"text": "Und gebe tausend Lust dem neu-verm\u00e4hlten Paare/", "tokens": ["Und", "ge\u00b7be", "tau\u00b7send", "Lust", "dem", "neu\u00b7ver\u00b7m\u00e4hl\u00b7ten", "Paa\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "CARD", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Er m\u00fcsse selbst den Oel in eure Flammen thaun/", "tokens": ["Er", "m\u00fcs\u00b7se", "selbst", "den", "O\u00b7el", "in", "eu\u00b7re", "Flam\u00b7men", "thaun", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Da\u00df man dis Kleinod nicht mag ohne Wucher schaun.", "tokens": ["Da\u00df", "man", "dis", "Klei\u00b7nod", "nicht", "mag", "oh\u00b7ne", "Wu\u00b7cher", "schaun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PDS", "NN", "PTKNEG", "VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}