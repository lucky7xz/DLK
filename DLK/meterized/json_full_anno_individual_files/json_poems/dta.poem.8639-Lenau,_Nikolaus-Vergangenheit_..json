{"dta.poem.8639": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Vergangenheit .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.57", "da:0.42"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Hesperus, der blasse Funken,", "tokens": ["Hes\u00b7pe\u00b7rus", ",", "der", "blas\u00b7se", "Fun\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Winkt uns melancholisch zu.", "tokens": ["Winkt", "uns", "me\u00b7lan\u00b7cho\u00b7lisch", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wieder ist ein Tag gesunken", "tokens": ["Wie\u00b7der", "ist", "ein", "Tag", "ge\u00b7sun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In die stille Todesruh;", "tokens": ["In", "die", "stil\u00b7le", "To\u00b7des\u00b7ruh", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Leichte Abendw\u00f6lkchen schweben", "tokens": ["Leich\u00b7te", "Ab\u00b7end\u00b7w\u00f6lk\u00b7chen", "schwe\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hin im sanften Mondenglanz,", "tokens": ["Hin", "im", "sanf\u00b7ten", "Mon\u00b7den\u00b7glanz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und aus bleichen Rosen weben", "tokens": ["Und", "aus", "blei\u00b7chen", "Ro\u00b7sen", "we\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie dem Todten einen Kranz.", "tokens": ["Sie", "dem", "Tod\u00b7ten", "ei\u00b7nen", "Kranz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Friedhof der entschlafnen Tage,", "tokens": ["Fried\u00b7hof", "der", "ent\u00b7schlaf\u00b7nen", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Schweigende Vergangenheit!", "tokens": ["Schwei\u00b7gen\u00b7de", "Ver\u00b7gan\u00b7gen\u00b7heit", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Du begr\u00e4bst des Herzens Klage,", "tokens": ["Du", "be\u00b7gr\u00e4bst", "des", "Her\u00b7zens", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, und seine Seligkeit.", "tokens": ["Ach", ",", "und", "sei\u00b7ne", "Se\u00b7lig\u00b7keit", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KON", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}