{"textgrid.poem.56157": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "16.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Immer wieder von uns aufgerissen,", "tokens": ["Im\u00b7mer", "wie\u00b7der", "von", "uns", "auf\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "ist der Gott die Stelle, welche heilt.", "tokens": ["ist", "der", "Gott", "die", "Stel\u00b7le", ",", "wel\u00b7che", "heilt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wir sind Scharfe, denn wir wollen wissen,", "tokens": ["Wir", "sind", "Schar\u00b7fe", ",", "denn", "wir", "wol\u00b7len", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$,", "KON", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "aber er ist heiter und verteilt.", "tokens": ["a\u00b7ber", "er", "ist", "hei\u00b7ter", "und", "ver\u00b7teilt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "KON", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Selbst die reine, die geweihte Spende", "tokens": ["Selbst", "die", "rei\u00b7ne", ",", "die", "ge\u00b7weih\u00b7te", "Spen\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "$,", "ART", "ADJA", "NN"], "meter": "+-+