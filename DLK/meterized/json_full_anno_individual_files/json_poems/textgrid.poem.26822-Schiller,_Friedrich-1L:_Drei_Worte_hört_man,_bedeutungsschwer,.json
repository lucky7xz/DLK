{"textgrid.poem.26822": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Drei Worte h\u00f6rt man, bedeutungsschwer,", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Drei Worte h\u00f6rt man, bedeutungsschwer,", "tokens": ["Drei", "Wor\u00b7te", "h\u00f6rt", "man", ",", "be\u00b7deu\u00b7tungs\u00b7schwer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PIS", "$,", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Im Munde der Guten und Besten;", "tokens": ["Im", "Mun\u00b7de", "der", "Gu\u00b7ten", "und", "Bes\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Sie schallen vergeblich, ihr Klang ist leer,", "tokens": ["Sie", "schal\u00b7len", "ver\u00b7geb\u00b7lich", ",", "ihr", "Klang", "ist", "leer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Sie k\u00f6nnen nicht helfen und tr\u00f6sten.", "tokens": ["Sie", "k\u00f6n\u00b7nen", "nicht", "hel\u00b7fen", "und", "tr\u00f6s\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "KON", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Verscherzt ist dem Menschen des Lebens Frucht,", "tokens": ["Ver\u00b7scherzt", "ist", "dem", "Men\u00b7schen", "des", "Le\u00b7bens", "Frucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "ART", "NN", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Solang er die Schatten zu haschen sucht.", "tokens": ["So\u00b7lang", "er", "die", "Schat\u00b7ten", "zu", "ha\u00b7schen", "sucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Solang er glaubt an die Goldene Zeit,", "tokens": ["So\u00b7lang", "er", "glaubt", "an", "die", "Gol\u00b7de\u00b7ne", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wo das Rechte, das Gute wird siegen, \u2013", "tokens": ["Wo", "das", "Rech\u00b7te", ",", "das", "Gu\u00b7te", "wird", "sie\u00b7gen", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ART", "NN", "VAFIN", "VVINF", "$,", "$("], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Das Rechte, das Gute f\u00fchrt ewig Streit,", "tokens": ["Das", "Rech\u00b7te", ",", "das", "Gu\u00b7te", "f\u00fchrt", "e\u00b7wig", "Streit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "ADJD", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nie wird der Feind ihm erliegen,", "tokens": ["Nie", "wird", "der", "Feind", "ihm", "er\u00b7lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und erstickst du ihn nicht in den L\u00fcften frei,", "tokens": ["Und", "er\u00b7stickst", "du", "ihn", "nicht", "in", "den", "L\u00fcf\u00b7ten", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "PTKNEG", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "Stets w\u00e4chst ihm die Kraft auf der Erde neu.", "tokens": ["Stets", "w\u00e4chst", "ihm", "die", "Kraft", "auf", "der", "Er\u00b7de", "neu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Solang er glaubt, da\u00df das buhlende Gl\u00fcck", "tokens": ["So\u00b7lang", "er", "glaubt", ",", "da\u00df", "das", "buh\u00b7len\u00b7de", "Gl\u00fcck"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sich dem Edeln vereinigen werde \u2013", "tokens": ["Sich", "dem", "E\u00b7deln", "ver\u00b7ei\u00b7ni\u00b7gen", "wer\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "VVINF", "VAFIN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Dem Schlechten folgt es mit Liebesblick,", "tokens": ["Dem", "Schlech\u00b7ten", "folgt", "es", "mit", "Lie\u00b7bes\u00b7blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nicht dem Guten geh\u00f6ret die Erde.", "tokens": ["Nicht", "dem", "Gu\u00b7ten", "ge\u00b7h\u00f6\u00b7ret", "die", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Er ist ein Fremdling, er wandert aus", "tokens": ["Er", "ist", "ein", "Fremd\u00b7ling", ",", "er", "wan\u00b7dert", "aus"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPER", "VVFIN", "PTKVZ"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und suchet ein unverg\u00e4nglich Haus.", "tokens": ["Und", "su\u00b7chet", "ein", "un\u00b7ver\u00b7g\u00e4ng\u00b7lich", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJD", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Solang er glaubt, da\u00df dem irdschen Verstand", "tokens": ["So\u00b7lang", "er", "glaubt", ",", "da\u00df", "dem", "ird\u00b7schen", "Ver\u00b7stand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Wahrheit je wird erscheinen,", "tokens": ["Die", "Wahr\u00b7heit", "je", "wird", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ihren Schleier hebt keine sterbliche Hand,", "tokens": ["Ih\u00b7ren", "Schlei\u00b7er", "hebt", "kei\u00b7ne", "sterb\u00b7li\u00b7che", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Wir k\u00f6nnen nur raten und meinen.", "tokens": ["Wir", "k\u00f6n\u00b7nen", "nur", "ra\u00b7ten", "und", "mei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Du kerkerst den Geist in ein t\u00f6nend Wort,", "tokens": ["Du", "ker\u00b7kerst", "den", "Geist", "in", "ein", "t\u00f6\u00b7nend", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Doch der freie wandelt im Sturme fort.", "tokens": ["Doch", "der", "frei\u00b7e", "wan\u00b7delt", "im", "Stur\u00b7me", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Drum, edle Seele, entrei\u00df dich dem Wahn", "tokens": ["Drum", ",", "ed\u00b7le", "See\u00b7le", ",", "ent\u00b7rei\u00df", "dich", "dem", "Wahn"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "$,", "ADJA", "NN", "$,", "VVFIN", "PRF", "ART", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Und den himmlischen Glauben bewahre!", "tokens": ["Und", "den", "himm\u00b7li\u00b7schen", "Glau\u00b7ben", "be\u00b7wah\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Was kein Ohr vernahm, was die Augen nicht sahn,", "tokens": ["Was", "kein", "Ohr", "ver\u00b7nahm", ",", "was", "die", "Au\u00b7gen", "nicht", "sahn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "VVFIN", "$,", "PRELS", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Es ist dennoch, das Sch\u00f6ne, das Wahre!", "tokens": ["Es", "ist", "den\u00b7noch", ",", "das", "Sch\u00f6\u00b7ne", ",", "das", "Wah\u00b7re", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Es ist nicht drau\u00dfen, da sucht es der Tor,", "tokens": ["Es", "ist", "nicht", "drau\u00b7\u00dfen", ",", "da", "sucht", "es", "der", "Tor", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Es ist in dir, du bringst es ewig hervor.", "tokens": ["Es", "ist", "in", "dir", ",", "du", "bringst", "es", "e\u00b7wig", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "$,", "PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.6": {"line.1": {"text": "Drei Worte h\u00f6rt man, bedeutungsschwer,", "tokens": ["Drei", "Wor\u00b7te", "h\u00f6rt", "man", ",", "be\u00b7deu\u00b7tungs\u00b7schwer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PIS", "$,", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Im Munde der Guten und Besten;", "tokens": ["Im", "Mun\u00b7de", "d