fo": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJD", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Solang er glaubt, da\u00df dem irdschen Verstand", "tokens": ["So\u00b7lang", "er", "glaubt", ",", "da\u00df", "dem", "ird\u00b7schen", "Ver\u00b7stand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Wahrheit je wird erscheinen,", "tokens": ["Die", "Wahr\u00b7heit", "je", "wird", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ihren Schleier hebt keine sterbliche Hand,", "tokens": ["Ih\u00b7ren", "Schlei\u00b7er", "hebt", "kei\u00b7ne", "sterb\u00b7li\u00b7che", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Wir k\u00f6nnen nur raten und meinen.", "tokens": ["Wir", "k\u00f6n\u00b7nen", "nur", "ra\u00b7ten", "und", "mei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Du kerkerst den Geist in ein t\u00f6nend Wort,", "tokens": ["Du", "ker\u00b7kerst", "den", "Geist", "in", "ein", "t\u00f6\u00b7nend", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Doch der freie wandelt im Sturme fort.", "tokens": ["Doch", "der", "frei\u00b7e", "wan\u00b7delt", "im", "Stur\u00b7me", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "Drum, edle Seele, entrei\u00df dich dem Wahn", "tokens": ["Drum", ",", "ed\u00b7le", "See\u00b7le", ",", "ent\u00b7rei\u00df", "dich", "dem", "Wahn"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "$,", "ADJA", "NN", "$,", "VVFIN", "PRF", "ART", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Und den himmlischen Glauben bewahre!", "tokens": ["Und", "den", "himm\u00b7li\u00b7schen", "Glau\u00b7ben", "be\u00b7wah\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Was kein Ohr vernahm, was die Augen nicht sahn,", "tokens": ["Was", "kein", "Ohr", "ver\u00b7nahm", ",", "was", "die", "Au\u00b7gen", "nicht", "sahn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "VVFIN", "$,", "PRELS", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Es ist dennoch, das Sch\u00f6ne, das Wahre!", "tokens": ["Es", "ist", "den\u00b7noch", ",", "das", "Sch\u00f6\u00b7ne", ",", "das", "Wah\u00b7re", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Es ist nicht drau\u00dfen, da sucht es der Tor,", "tokens": ["Es", "ist", "nicht", "drau\u00b7\u00dfen", ",", "da", "sucht", "es", "der", "Tor", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Es ist in dir, du bringst es ewig hervor.", "tokens": ["Es", "ist", "in", "dir", ",", "du", "bringst", "es", "e\u00b7wig", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "$,", "PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}}}}