{"textgrid.poem.40436": {"metadata": {"author": {"name": "Scheffel, Joseph Viktor von", "birth": "N.A.", "death": "N.A."}, "title": "Der F\u00fcnfundsechziger", "genre": "verse", "period": "N.A.", "pub_year": 1856, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In luftiger Trinkkemenaten", "tokens": ["In", "luf\u00b7ti\u00b7ger", "Trink\u00b7ke\u00b7me\u00b7na\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "\u2013 Den Ort gesteht man nicht ein \u2013", "tokens": ["\u2013", "Den", "Ort", "ge\u00b7steht", "man", "nicht", "ein", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PIS", "PTKNEG", "ART", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Da pr\u00fcften drei sp\u00e4te Nomaden", "tokens": ["Da", "pr\u00fcf\u00b7ten", "drei", "sp\u00e4\u00b7te", "No\u00b7ma\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "CARD", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Den edelsten pf\u00e4lzischen Wein.", "tokens": ["Den", "e\u00b7dels\u00b7ten", "pf\u00e4l\u00b7zi\u00b7schen", "Wein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Aus r\u00f6tlichen R\u00f6mern erblinkte", "tokens": ["Aus", "r\u00f6t\u00b7li\u00b7chen", "R\u00f6\u00b7mern", "er\u00b7blink\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Des Rieslings feinperlendes Gold,", "tokens": ["Des", "Ries\u00b7lings", "fein\u00b7per\u00b7len\u00b7des", "Gold", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Des H\u00f6hensaums Rebgel\u00e4nd' winkte", "tokens": ["Des", "H\u00f6\u00b7hen\u00b7saums", "Reb\u00b7ge\u00b7l\u00e4nd'", "wink\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Im Mondschein den Trinkenden hold.", "tokens": ["Im", "Mond\u00b7schein", "den", "Trin\u00b7ken\u00b7den", "hold", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Der erste, ein weitum gereister", "tokens": ["Der", "ers\u00b7te", ",", "ein", "wei\u00b7tum", "ge\u00b7reis\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Philologus, spitzte den Mund:", "tokens": ["Phi\u00b7lo\u00b7lo\u00b7gus", ",", "spitz\u00b7te", "den", "Mund", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "\u00bb", "tokens": ["\u00bb"], "token_info": ["punct"], "pos": ["$("]}, "line.4": {"text": "Mit \u00c4ther und Sonne im Bund.", "tokens": ["Mit", "\u00c4\u00b7ther", "und", "Son\u00b7ne", "im", "Bund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Drum flutet's und glutet im Becher", "tokens": ["Drum", "flu\u00b7tet's", "und", "glu\u00b7tet", "im", "Be\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "KON", "VVFIN", "APPRART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Geistfunkelnd, sanftrhythmisch und voll,", "tokens": ["Ge\u00b7ist\u00b7fun\u00b7kelnd", ",", "sanftr\u00b7hyth\u00b7misch", "und", "voll", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.7": {"text": "Als s\u00e4ngen homerische Zecher", "tokens": ["Als", "s\u00e4n\u00b7gen", "ho\u00b7me\u00b7ri\u00b7sche", "Ze\u00b7cher"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein jonisches Kneiplied in Moll.\u00ab", "tokens": ["Ein", "jo\u00b7ni\u00b7sches", "Knei\u00b7plied", "in", "Moll", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Der zweite, ein trockener Kenner", "tokens": ["Der", "zwei\u00b7te", ",", "ein", "tro\u00b7cke\u00b7ner", "Ken\u00b7ner"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und Deuter des r\u00f6mischen Rechts:", "tokens": ["Und", "Deu\u00b7ter", "des", "r\u00f6\u00b7mi\u00b7schen", "Rechts", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "\u00bbproficiat\u00ab, sprach er, \u00bbihr M\u00e4nner,", "tokens": ["\u00bb", "pro\u00b7fi\u00b7ci\u00b7at", "\u00ab", ",", "sprach", "er", ",", "\u00bb", "ihr", "M\u00e4n\u00b7ner", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "FM.la", "$(", "$,", "VVFIN", "PPER", "$,", "$(", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Wir l\u00e4ppern allhiero nichts Schlechts.", "tokens": ["Wir", "l\u00e4p\u00b7pern", "all\u00b7hie\u00b7ro", "nichts", "Schlechts", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wer schaut nicht, wenn bacchisches Donum", "tokens": ["Wer", "schaut", "nicht", ",", "wenn", "bac\u00b7chi\u00b7sches", "Do\u00b7num"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PTKNEG", "$,", "KOUS", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "So goldklar im Kelchglase scheint,", "tokens": ["So", "gold\u00b7klar", "im", "Kelch\u00b7gla\u00b7se", "scheint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Das Justum, Aequum et Bonum", "tokens": ["Das", "Jus\u00b7tum", ",", "A\u00b7e\u00b7quum", "et", "Bo\u00b7num"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In diesem R\u00f6mer vereint?\u00ab", "tokens": ["In", "die\u00b7sem", "R\u00f6\u00b7mer", "ver\u00b7eint", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PDAT", "NN", "VVPP", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Der dritte, der putzte die Lichter,", "tokens": ["Der", "drit\u00b7te", ",", "der", "putz\u00b7te", "die", "Lich\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die m\u00e4chtig heruntergebrannt,", "tokens": ["Die", "m\u00e4ch\u00b7tig", "her\u00b7un\u00b7ter\u00b7ge\u00b7brannt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und sprach: \u00bbZwar bin ich kein Dichter", "tokens": ["Und", "sprach", ":", "\u00bb", "Zwar", "bin", "ich", "kein", "Dich\u00b7ter"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "$(", "ADV", "VAFIN", "PPER", "PIAT", "NN"], "meter": "+--++-+-", "measure": "dactylic.init"}, "line.4": {"text": "Und kunstlos und schlicht von Verstand;", "tokens": ["Und", "kunst\u00b7los", "und", "schlicht", "von", "Ver\u00b7stand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Doch n\u00e4hert sich solch einem Schoppen", "tokens": ["Doch", "n\u00e4\u00b7hert", "sich", "solch", "ei\u00b7nem", "Schop\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "PIAT", "ART", "NN"], "meter": "-++--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Mein Herz ... dann \u00fcberwallt's ...", "tokens": ["Mein", "Herz", "...", "dann", "\u00fc\u00b7ber\u00b7wallt's", "..."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "ADV", "APPRART", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "'s is halt e verflucht feiner Troppen,", "tokens": ["'s", "is", "halt", "e", "ver\u00b7flucht", "fei\u00b7ner", "Trop\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "FM", "VVFIN", "NE", "VVPP", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Ich segne die H\u00fcgel der Pfalz!\u00ab", "tokens": ["Ich", "seg\u00b7ne", "die", "H\u00fc\u00b7gel", "der", "Pfalz", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NE", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Derweilen ging draus auf dem Damme", "tokens": ["Der\u00b7wei\u00b7len", "ging", "draus", "auf", "dem", "Dam\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PAV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Spie\u00dftragend ein vierter vorbei,", "tokens": ["Spie\u00df\u00b7tra\u00b7gend", "ein", "vier\u00b7ter", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Der blies eine wundersame", "tokens": ["Der", "blies", "ei\u00b7ne", "wun\u00b7der\u00b7sa\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gewaltige Melodei:", "tokens": ["Ge\u00b7wal\u00b7ti\u00b7ge", "Me\u00b7lo\u00b7dei", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "\u00bbihr Herren, und lasset Euch sagen,", "tokens": ["\u00bb", "ihr", "Her\u00b7ren", ",", "und", "las\u00b7set", "Euch", "sa\u00b7gen", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Die Stadtgemeinde braucht Schlaf,", "tokens": ["Die", "Stadt\u00b7ge\u00b7mein\u00b7de", "braucht", "Schlaf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Die Glocke hat eilf Uhr geschlagen,", "tokens": ["Die", "Glo\u00b7cke", "hat", "eilf", "Uhr", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "CARD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wer jetzt nicht zu Bett geht, zahlt Straf'.\u00ab", "tokens": ["Wer", "jetzt", "nicht", "zu", "Bett", "geht", ",", "zahlt", "Straf'", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PWS", "ADV", "PTKNEG", "APPR", "NN", "VVFIN", "$,", "VVFIN", "NE", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "In luftiger Trinkkemenaten", "tokens": ["In", "luf\u00b7ti\u00b7ger", "Trink\u00b7ke\u00b7me\u00b7na\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "\u2013 Den Ort gesteht man nicht ein \u2013", "tokens": ["\u2013", "Den", "Ort", "ge\u00b7steht", "man", "nicht", "ein", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PIS", "PTKNEG", "ART", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Da pr\u00fcften drei sp\u00e4te Nomaden", "tokens": ["Da", "pr\u00fcf\u00b7ten", "drei", "sp\u00e4\u00b7te", "No\u00b7ma\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "CARD", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Den edelsten pf\u00e4lzischen Wein.", "tokens": ["Den", "e\u00b7dels\u00b7ten", "pf\u00e4l\u00