{"dta.poem.7288": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Das \u00f6de Haus.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Tiefab im Tobel liegt ein Haus,", "tokens": ["Tief\u00b7ab", "im", "To\u00b7bel", "liegt", "ein", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zerfallen nach des F\u00f6rsters Tode,", "tokens": ["Zer\u00b7fal\u00b7len", "nach", "des", "F\u00f6rs\u00b7ters", "To\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dort ruh' ich manche Stunde aus,", "tokens": ["Dort", "ruh'", "ich", "man\u00b7che", "Stun\u00b7de", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vergraben unter Rank' und Lode;", "tokens": ["Ver\u00b7gra\u00b7ben", "un\u00b7ter", "Rank'", "und", "Lo\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "'s ist eine Wildni\u00df, wo der Tag", "tokens": ["'s", "ist", "ei\u00b7ne", "Wild\u00b7ni\u00df", ",", "wo", "der", "Tag"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PWAV", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Nur halb die schweren Wimpern lichtet;", "tokens": ["Nur", "halb", "die", "schwe\u00b7ren", "Wim\u00b7pern", "lich\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der Felsen tiefe Kluft verdichtet", "tokens": ["Der", "Fel\u00b7sen", "tie\u00b7fe", "Kluft", "ver\u00b7dich\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ergrauter Aeste Schattenhaag.", "tokens": ["Er\u00b7grau\u00b7ter", "A\u00b7es\u00b7te", "Schat\u00b7ten\u00b7haag", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich horche tr\u00e4umend, wie im Spalt", "tokens": ["Ich", "hor\u00b7che", "tr\u00e4u\u00b7mend", ",", "wie", "im", "Spalt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "VVPP", "$,", "PWAV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die schwarzen Fliegen taumelnd summen,", "tokens": ["Die", "schwar\u00b7zen", "Flie\u00b7gen", "tau\u00b7melnd", "sum\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie Seufzer streifen durch den Wald,", "tokens": ["Wie", "Seuf\u00b7zer", "strei\u00b7fen", "durch", "den", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Am Strauche irre K\u00e4fer brummen;", "tokens": ["Am", "Strau\u00b7che", "ir\u00b7re", "K\u00e4\u00b7fer", "brum\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn sich die Abendr\u00f6the dr\u00e4ngt", "tokens": ["Wenn", "sich", "die", "A\u00b7bend\u00b7r\u00f6\u00b7the", "dr\u00e4ngt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "An sickernden Geschiefers Lauge,", "tokens": ["An", "si\u00b7ckern\u00b7den", "Ge\u00b7schie\u00b7fers", "Lau\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Dann ist's als ob ein tr\u00fcbes Auge,", "tokens": ["Dann", "ist's", "als", "ob", "ein", "tr\u00fc\u00b7bes", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "KOKOM", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein rothgeweintes dr\u00fcber h\u00e4ngt.", "tokens": ["Ein", "roth\u00b7ge\u00b7wein\u00b7tes", "dr\u00fc\u00b7ber", "h\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wo an zerri\u00dfner Laube Joch", "tokens": ["Wo", "an", "zer\u00b7ri\u00df\u00b7ner", "Lau\u00b7be", "Joch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die langen magern Scho\u00dfen streichen,", "tokens": ["Die", "lan\u00b7gen", "ma\u00b7gern", "Scho\u00b7\u00dfen", "strei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "An wildverwachs'ner Hecke noch", "tokens": ["An", "wild\u00b7ver\u00b7wachs'\u00b7ner", "He\u00b7cke", "noch"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADV"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Im Moose Nelkensprossen schleichen,", "tokens": ["Im", "Moo\u00b7se", "Nel\u00b7ken\u00b7spros\u00b7sen", "schlei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dort hat vom tr\u00f6pfelnden Gestein", "tokens": ["Dort", "hat", "vom", "tr\u00f6p\u00b7feln\u00b7den", "Ge\u00b7stein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das dunkle Na\u00df sich durchgesogen,", "tokens": ["Das", "dunk\u00b7le", "Na\u00df", "sich", "durch\u00b7ge\u00b7so\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Kreucht um den Buchs in tr\u00e4gen Bogen,", "tokens": ["Kreucht", "um", "den", "Buchs", "in", "tr\u00e4\u00b7gen", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.8": {"text": "Und sinkt am Fenchelstrauche ein.", "tokens": ["Und", "sinkt", "am", "Fen\u00b7chel\u00b7strau\u00b7che", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Das Dach, von Moose \u00fcberschwellt,", "tokens": ["Das", "Dach", ",", "von", "Moo\u00b7se", "\u00fc\u00b7bersc\u00b7hwellt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "L\u00e4\u00dft wirre Schober niederragen,", "tokens": ["L\u00e4\u00dft", "wir\u00b7re", "Scho\u00b7ber", "nie\u00b7der\u00b7ra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und eine Spinne hat ihr Zelt", "tokens": ["Und", "ei\u00b7ne", "Spin\u00b7ne", "hat", "ihr", "Zelt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Fensterloche aufgeschlagen;", "tokens": ["Im", "Fens\u00b7ter\u00b7lo\u00b7che", "auf\u00b7ge\u00b7schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da h\u00e4ngt, ein Blatt von zartem Flor,", "tokens": ["Da", "h\u00e4ngt", ",", "ein", "Blatt", "von", "zar\u00b7tem", "Flor", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der schillernden Libelle Fl\u00fcgel,", "tokens": ["Der", "schil\u00b7lern\u00b7den", "Li\u00b7bel\u00b7le", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Und ihres Panzers goldner Spiegel", "tokens": ["Und", "ih\u00b7res", "Pan\u00b7zers", "gold\u00b7ner", "Spie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ragt kopflos am Gesims hervor.", "tokens": ["Ragt", "kopf\u00b7los", "am", "Ge\u00b7sims", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Zuweilen hat ein Schmetterling", "tokens": ["Zu\u00b7wei\u00b7len", "hat", "ein", "Schmet\u00b7ter\u00b7ling"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich gaukelnd in der Schlucht gefangen,", "tokens": ["Sich", "gau\u00b7kelnd", "in", "der", "Schlucht", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und bleibt sekundenlang am Ring", "tokens": ["Und", "bleibt", "se\u00b7kun\u00b7den\u00b7lang", "am", "Ring"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der kr\u00e4nkelnden Narzisse hangen;", "tokens": ["Der", "kr\u00e4n\u00b7keln\u00b7den", "Nar\u00b7zis\u00b7se", "han\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Streicht eine Taube durch den Hain,", "tokens": ["Streicht", "ei\u00b7ne", "Tau\u00b7be", "durch", "den", "Hain", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So schweigt am Tobelrand ihr Girren,", "tokens": ["So", "schweigt", "am", "To\u00b7bel\u00b7rand", "ihr", "Gir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Man h\u00f6ret nur die Fl\u00fcgel schwirren", "tokens": ["Man", "h\u00f6\u00b7ret", "nur", "die", "Fl\u00fc\u00b7gel", "schwir\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und sieht den Schatten am Gestein.", "tokens": ["Und", "sieht", "den", "Schat\u00b7ten", "am", "Ge\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und auf dem Heerde, wo der Schnee", "tokens": ["Und", "auf", "dem", "Heer\u00b7de", ",", "wo", "der", "Schnee"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Seit Jahren durch den Schlot geflogen,", "tokens": ["Seit", "Jah\u00b7ren", "durch", "den", "Schlot", "ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Liegt Aschenmoder feucht und z\u00e4h,", "tokens": ["Liegt", "A\u00b7schen\u00b7mo\u00b7der", "feucht", "und", "z\u00e4h", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Pilzes Glocken \u00fcberzogen;", "tokens": ["Von", "Pil\u00b7zes", "Glo\u00b7cken", "\u00fc\u00b7berz\u00b7o\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Noch h\u00e4ngt am Mauerpflock ein Rest", "tokens": ["Noch", "h\u00e4ngt", "am", "Mau\u00b7er\u00b7pflock", "ein", "Rest"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Verwirrten Wergs, das Seil zu spinnen,", "tokens": ["Ver\u00b7wirr\u00b7ten", "Wergs", ",", "das", "Seil", "zu", "spin\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wie halbvermorschtes Haar und drinnen", "tokens": ["Wie", "halb\u00b7ver\u00b7morschtes", "Haar", "und", "drin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "KON", "ADV"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.8": {"text": "Der Schwalbe \u00fcberj\u00e4hrig Nest.", "tokens": ["Der", "Schwal\u00b7be", "\u00fc\u00b7ber\u00b7j\u00e4h\u00b7rig", "Nest", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und von des Balkens Haken nikt", "tokens": ["Und", "von", "des", "Bal\u00b7kens", "Ha\u00b7ken", "nikt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Schellenband an Schnall' und Riemen,", "tokens": ["Ein", "Schel\u00b7len\u00b7band", "an", "Schnall'", "und", "Rie\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit grober Wolle ist gestickt", "tokens": ["Mit", "gro\u00b7ber", "Wol\u00b7le", "ist", "ge\u00b7stickt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201ediana\u201c auf dem Lederstriemen;", "tokens": ["\u201e", "di\u00b7a\u00b7na", "\u201c", "auf", "dem", "Le\u00b7der\u00b7strie\u00b7men", ";"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "$(", "APPR", "ART", "NN", "$."], "meter": "-----+-+-", "measure": "unknown.measure.di"}, "line.5": {"text": "Ein Pfeifchen auch verga\u00df man hier,", "tokens": ["Ein", "Pfei\u00b7fchen", "auch", "ver\u00b7ga\u00df", "man", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PIS", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als man den Tannensarg geschlossen;", "tokens": ["Als", "man", "den", "Tan\u00b7nen\u00b7sarg", "ge\u00b7schlos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Den Mann begrub man, todt geschossen", "tokens": ["Den", "Mann", "be\u00b7grub", "man", ",", "todt", "ge\u00b7schos\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PIS", "$,", "ADJD", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Hat man das alte treue Thier.", "tokens": ["Hat", "man", "das", "al\u00b7te", "treu\u00b7e", "Thier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], 