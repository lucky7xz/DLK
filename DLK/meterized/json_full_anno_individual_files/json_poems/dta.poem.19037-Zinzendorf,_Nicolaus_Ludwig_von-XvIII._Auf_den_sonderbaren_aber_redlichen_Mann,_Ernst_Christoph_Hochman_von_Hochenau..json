{"dta.poem.19037": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "XvIII.  Auf den sonderbaren aber redlichen  \n Mann, Ernst Christoph Hochman von  \n Hochenau.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Ogeist in JEsu Geist versuncken!", "tokens": ["O\u00b7geist", "in", "Je\u00b7su", "Geist", "ver\u00b7sun\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O Leib voll reiner Gnaden-Funcken!", "tokens": ["O", "Leib", "voll", "rei\u00b7ner", "Gna\u00b7den\u00b7Fun\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "O du vom Lamm erkauffte Braut!", "tokens": ["O", "du", "vom", "Lamm", "er\u00b7kauff\u00b7te", "Braut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An der man nichts als Segen schaut.", "tokens": ["An", "der", "man", "nichts", "als", "Se\u00b7gen", "schaut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIS", "PIS", "KOKOM", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Geh ein in deines JEsu Ruhe,", "tokens": ["Geh", "ein", "in", "dei\u00b7nes", "Je\u00b7su", "Ru\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "APPR", "PPOSAT", "NE", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wirff weg die ausgetretnen Schue", "tokens": ["Wirff", "weg", "die", "aus\u00b7ge\u00b7tret\u00b7nen", "Schue"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Moderischen Lebens-Zeit,", "tokens": ["Der", "Mo\u00b7de\u00b7ri\u00b7schen", "Le\u00b7bens\u00b7Zeit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und dringe in die Ewigkeit.", "tokens": ["Und", "drin\u00b7ge", "in", "die", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Geniesse da der Himmels-S\u00e4ffte,", "tokens": ["Ge\u00b7nies\u00b7se", "da", "der", "Him\u00b7mels\u00b7S\u00e4ff\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die hier in allerley Gesch\u00e4ffte", "tokens": ["Die", "hier", "in", "al\u00b7ler\u00b7ley", "Ge\u00b7sch\u00e4ff\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sich in und an dir kund gethan,", "tokens": ["Sich", "in", "und", "an", "dir", "kund", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "KON", "APPR", "PPER", "PTKVZ", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf! stimme Lammes-Lieder an.", "tokens": ["Auf", "!", "stim\u00b7me", "Lam\u00b7mes\u00b7Lie\u00b7der", "an", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Geh hin in JEsu s\u00fcssen Frieden;", "tokens": ["Geh", "hin", "in", "Je\u00b7su", "s\u00fcs\u00b7sen", "Frie\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du warest hier schon abgeschieden,", "tokens": ["Du", "wa\u00b7rest", "hier", "schon", "ab\u00b7ge\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hie schon von Creaturen los,", "tokens": ["Hie", "schon", "von", "Crea\u00b7tu\u00b7ren", "los", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und nur in deiner Armuth gro\u00df.", "tokens": ["Und", "nur", "in", "dei\u00b7ner", "Ar\u00b7muth", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Geh hin! wir sind in Mesechs H\u00fctten,", "tokens": ["Geh", "hin", "!", "wir", "sind", "in", "Me\u00b7sechs", "H\u00fct\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "PPER", "VAFIN", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wir folgen dir mit matten Schritten,", "tokens": ["Wir", "fol\u00b7gen", "dir", "mit", "mat\u00b7ten", "Schrit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und giessen auf dein k\u00fchles Gras", "tokens": ["Und", "gies\u00b7sen", "auf", "dein", "k\u00fch\u00b7les", "Gras"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein GOtt-geweyhtes Thr\u00e4nen-Maa\u00df.", "tokens": ["Ein", "Got\u00b7tge\u00b7wey\u00b7htes", "Thr\u00e4\u00b7nen\u00b7Maa\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Du glaubtest, aber ohne Zancken;", "tokens": ["Du", "glaub\u00b7test", ",", "a\u00b7ber", "oh\u00b7ne", "Zan\u00b7cken", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du liebtest, aber ohne Wancken;", "tokens": ["Du", "lieb\u00b7test", ",", "a\u00b7ber", "oh\u00b7ne", "Wan\u00b7cken", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Friedens-H\u00fcttlein prangte hier", "tokens": ["Dein", "Frie\u00b7dens\u00b7H\u00fctt\u00b7lein", "prang\u00b7te", "hier"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Friede in dem Streit-Revier.", "tokens": ["Mit", "Frie\u00b7de", "in", "dem", "Streit\u00b7Re\u00b7vier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Der Secten-Geist, das Rott-Gesinde,", "tokens": ["Der", "Sec\u00b7ten\u00b7Geist", ",", "das", "Rot\u00b7tGe\u00b7sin\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vermochte nichts an diesem Kinde,", "tokens": ["Ver\u00b7moch\u00b7te", "nichts", "an", "die\u00b7sem", "Kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und sein in Unschuld zarter Sinn", "tokens": ["Und", "sein", "in", "Un\u00b7schuld", "zar\u00b7ter", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zog alle in die Liebe hin.", "tokens": ["Zog", "al\u00b7le", "in", "die", "Lie\u00b7be", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Herr JEsu, diesen treuen Zeugen,", "tokens": ["Herr", "Je\u00b7su", ",", "die\u00b7sen", "treu\u00b7en", "Zeu\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sein seligs Reden, weises Schweigen,", "tokens": ["Sein", "se\u00b7ligs", "Re\u00b7den", ",", "wei\u00b7ses", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und seiner Liebe Gnaden-Schein,", "tokens": ["Und", "sei\u00b7ner", "Lie\u00b7be", "Gna\u00b7den\u00b7Schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Pr\u00e4g unserm Angedencken ein.", "tokens": ["Pr\u00e4g", "un\u00b7serm", "An\u00b7ge\u00b7den\u00b7cken", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Und wecke unsre tr\u00e4ge Sinnen,", "tokens": ["Und", "we\u00b7cke", "uns\u00b7re", "tr\u00e4\u00b7ge", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es eben also zu beginnen", "tokens": ["Es", "e\u00b7ben", "al\u00b7so", "zu", "be\u00b7gin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ach! mache uns so eingekehrt,", "tokens": ["Ach", "!", "ma\u00b7che", "uns", "so", "ein\u00b7ge\u00b7kehrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So klein, so arm, so liebens-werth.", "tokens": ["So", "klein", ",", "so", "arm", ",", "so", "lie\u00b7bens\u00b7werth", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}