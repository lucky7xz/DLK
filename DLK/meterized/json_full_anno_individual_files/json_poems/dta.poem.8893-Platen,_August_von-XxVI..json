{"dta.poem.8893": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XxVI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Und kann mich nicht von dir, Venedig, trennen:", "tokens": ["Und", "kann", "mich", "nicht", "von", "dir", ",", "Ve\u00b7ne\u00b7dig", ",", "tren\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "APPR", "PPER", "$,", "NE", "$,", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "H\u00f6r' ich Fusina, h\u00f6r' ich Mestre nennen,", "tokens": ["H\u00f6r'", "ich", "Fu\u00b7si\u00b7na", ",", "h\u00f6r'", "ich", "Me\u00b7stre", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NE", "$,", "VVFIN", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.3": {"text": "So scheint ein Frost mir durch die Brust zu schleichen.", "tokens": ["So", "scheint", "ein", "Frost", "mir", "durch", "die", "Brust", "zu", "schlei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Stets mehr empfind' ich dich als ohne Gleichen,", "tokens": ["Stets", "mehr", "emp\u00b7find'", "ich", "dich", "als", "oh\u00b7ne", "Glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PRF", "KOUS", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Seit mir's gelingt, dich mehr und mehr zu kennen:", "tokens": ["Seit", "mir's", "ge\u00b7lingt", ",", "dich", "mehr", "und", "mehr", "zu", "ken\u00b7nen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "$,", "PPER", "ADV", "KON", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im Tiefsten f\u00fchl' ich meine Seele brennen,", "tokens": ["Im", "Tiefs\u00b7ten", "f\u00fchl'", "ich", "mei\u00b7ne", "See\u00b7le", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Gro\u00dfes sieht und Gro\u00dfes will erreichen.", "tokens": ["Die", "Gro\u00b7\u00dfes", "sieht", "und", "Gro\u00b7\u00dfes", "will", "er\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Welch eine F\u00fclle wohnt von Kraft und Milde", "tokens": ["Welch", "ei\u00b7ne", "F\u00fcl\u00b7le", "wohnt", "von", "Kraft", "und", "Mil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sogar im Marmor hier, im spr\u00f6den, kalten,", "tokens": ["So\u00b7gar", "im", "Mar\u00b7mor", "hier", ",", "im", "spr\u00f6\u00b7den", ",", "kal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ADV", "$,", "APPRART", "VVINF", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und in so manchem tiefgef\u00fchlten Bilde!", "tokens": ["Und", "in", "so", "man\u00b7chem", "tief\u00b7ge\u00b7f\u00fchl\u00b7ten", "Bil\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch um noch mehr zu fesseln mich, zu halten,", "tokens": ["Doch", "um", "noch", "mehr", "zu", "fes\u00b7seln", "mich", ",", "zu", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUI", "ADV", "ADV", "PTKZU", "VVINF", "PPER", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So mischt sich unter jene Kunstgebilde", "tokens": ["So", "mischt", "sich", "un\u00b7ter", "je\u00b7ne", "Kunst\u00b7ge\u00b7bil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die sch\u00f6nste Bl\u00fcthe lebender Gestalten.", "tokens": ["Die", "sch\u00f6ns\u00b7te", "Bl\u00fc\u00b7the", "le\u00b7ben\u00b7der", "Ge\u00b7stal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}