{"dta.poem.14500": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Herbst .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist nun der Herbst gekommen,", "tokens": ["Es", "ist", "nun", "der", "Herbst", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Hat das sch\u00f6ne Sommerkleid", "tokens": ["Hat", "das", "sch\u00f6\u00b7ne", "Som\u00b7mer\u00b7kleid"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Von den Feldern weggenommen", "tokens": ["Von", "den", "Fel\u00b7dern", "weg\u00b7ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Bl\u00e4tter ausgestreut,", "tokens": ["Und", "die", "Bl\u00e4t\u00b7ter", "aus\u00b7ge\u00b7streut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Vor dem b\u00f6sen Winterwinde", "tokens": ["Vor", "dem", "b\u00f6\u00b7sen", "Win\u00b7ter\u00b7win\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Deckt er warm und sachte zu", "tokens": ["Deckt", "er", "warm", "und", "sach\u00b7te", "zu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "VVFIN", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Mit dem bunten Laub die Gr\u00fcnde,", "tokens": ["Mit", "dem", "bun\u00b7ten", "Laub", "die", "Gr\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die schon m\u00fcde geh'n zur Ruh.", "tokens": ["Die", "schon", "m\u00fc\u00b7de", "geh'n", "zur", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}