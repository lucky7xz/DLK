 ["Und", "ein", "Ge\u00b7spenst", "nicht", "schau\u00b7rig", "schre\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PTKNEG", "ADJD", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Den Wanderer durchs Sonnenland.", "tokens": ["Den", "Wan\u00b7de\u00b7rer", "durchs", "Son\u00b7nen\u00b7land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Mit Eisenfingern h\u00e4lt umspannt", "tokens": ["Mit", "Ei\u00b7sen\u00b7fin\u00b7gern", "h\u00e4lt", "um\u00b7spannt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u2013 Und trostlos sieht ihr Auge drein \u2013", "tokens": ["\u2013", "Und", "trost\u00b7los", "sieht", "ihr", "Au\u00b7ge", "drein", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "ADJD", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie fest ein nacktes M\u00e4gdelein,", "tokens": ["Sie", "fest", "ein", "nack\u00b7tes", "M\u00e4g\u00b7del\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Dorn und Distel hergebannt.", "tokens": ["In", "Dorn", "und", "Dis\u00b7tel", "her\u00b7ge\u00b7bannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Das M\u00e4gdlein ist so fein und zart,", "tokens": ["Das", "M\u00e4gd\u00b7lein", "ist", "so", "fein", "und", "zart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als sei's von Engelsform und -Art.", "tokens": ["Als", "sei's", "von", "En\u00b7gels\u00b7form", "und", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPR", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es sieht mit ahnungslosem Blick", "tokens": ["Es", "sieht", "mit", "ah\u00b7nungs\u00b7lo\u00b7sem", "Blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das weltentstellende Geschick", "tokens": ["Das", "wel\u00b7tent\u00b7stel\u00b7len\u00b7de", "Ge\u00b7schick"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und fragt mit unschuldvollem Walten:", "tokens": ["Und", "fragt", "mit", "un\u00b7schuld\u00b7vol\u00b7lem", "Wal\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "\u00bbwer bist du, so mich festzuhalten?\u00ab", "tokens": ["\u00bb", "wer", "bist", "du", ",", "so", "mich", "fest\u00b7zu\u00b7hal\u00b7ten", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VAFIN", "PPER", "$,", "ADV", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und grausig gellt's durchs goldne Tal", "tokens": ["Und", "grau\u00b7sig", "gellt's", "durchs", "gold\u00b7ne", "Tal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJA", "APPRART",