{"dta.poem.20602": {"metadata": {"author": {"name": "Hebel, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Die Verg\u00e4nglichkeit .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1803", "urn": "urn:nbn:de:kobv:b4-200905192133", "language": ["de:0.85", "en:0.14"], "booktitle": "[Hebel, Johann Peter]: Allemannische Gedichte. Karlsruhe, 1803."}, "poem": {"stanza.1": {"line.1": {"text": "Du gute Burst, \u2019s cha frili sy, was meinsch?", "tokens": ["Du", "gu\u00b7te", "Burst", ",", "'s", "cha", "fri\u00b7li", "sy", ",", "was", "meinsch", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PPER", "FM", "FM", "FM", "$,", "PRELS", "ADJD", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "\u2019schunnt alles iung und neu, und alles schlicht", "tokens": ["'schunnt", "al\u00b7les", "i\u00b7ung", "und", "neu", ",", "und", "al\u00b7les", "schlicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN", "KON", "ADJD", "$,", "KON", "PIS", "VVFIN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "im Alter zu, und alles nimmt en End,", "tokens": ["im", "Al\u00b7ter", "zu", ",", "und", "al\u00b7les", "nimmt", "en", "End", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,", "KON", "PIS", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "und n\u00fct stoht still. H\u00f6rsch nit, wie \u2019s", "tokens": ["und", "n\u00fct", "stoht", "still", ".", "H\u00f6rsch", "nit", ",", "wie", "'s"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PTKVZ", "$.", "NN", "PTKNEG", "$,", "PWAV", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wasser ruuscht,", "tokens": ["Was\u00b7ser", "ruuscht", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "und siehsch am Himmel obe Stern an Stern?", "tokens": ["und", "sieh\u00b7sch", "am", "Him\u00b7mel", "o\u00b7be", "Stern", "an", "Stern", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "ADJA", "NN", "APPR", "NN", "$."], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Me meint, vo alle r\u00fchr si kein, und doch", "tokens": ["Me", "meint", ",", "vo", "al\u00b7le", "r\u00fchr", "si", "kein", ",", "und", "doch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,", "KON", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "ruckt alles witers, alles chunnt und goht.", "tokens": ["ruckt", "al\u00b7les", "wi\u00b7ters", ",", "al\u00b7les", "chunnt", "und", "goht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "$,", "PIS", "ADJD", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Je, \u2019s isch nit anderst, lueg mi a, wie", "tokens": ["Je", ",", "'s", "isch", "nit", "an\u00b7derst", ",", "lu\u00b7eg", "mi", "a", ",", "wie"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADV", "$,", "PPER", "ADJD", "PTKNEG", "ADV", "$,", "FM", "FM", "FM", "$,", "PWAV"], "meter": "+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.10": {"text": "d\u2019 witt.", "tokens": ["d'", "witt", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.11": {"text": "De bisch no iung; u\u00e4rsch, i bi au so gsi,", "tokens": ["De", "bisch", "no", "i\u00b7ung", ";", "u\u00b7\u00e4rsch", ",", "i", "bi", "au", "so", "gsi", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "$.", "ADJD", "$,", "FM", "FM", "FM", "FM", "FM", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "iezt w\u00fcrds mer anderst, \u2019s Alter, \u2019s Alter", "tokens": ["iezt", "w\u00fcrds", "mer", "an\u00b7derst", ",", "'s", "Al\u00b7ter", ",", "'s", "Al\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "$,", "PPER", "NN", "$,", "PPER", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "chunnt,", "tokens": ["chunnt", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+", "measure": "single.up"}, "line.14": {"text": "und woni gang, go Gresgen oder Wies,", "tokens": ["und", "wo\u00b7ni", "gang", ",", "go", "Gres\u00b7gen", "o\u00b7der", "Wies", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADJD", "$,", "FM", "NN", "KON", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "in Feld und Wald, go Basel oder heim,", "tokens": ["in", "Feld", "und", "Wald", ",", "go", "Ba\u00b7sel", "o\u00b7der", "heim", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "NE", "NE", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "\u2019s isch einerley, i gang im Chilchhof zu, \u2014", "tokens": ["'s", "isch", "ei\u00b7ner\u00b7ley", ",", "i", "gang", "im", "Chilch\u00b7hof", "zu", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADJD", "PTKVZ", "$,", "NE", "ADV", "APPRART", "NN", "PTKVZ", "$,", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.17": {"text": "briegg, alder nit! \u2014 und bi\u00df de bisch wien", "tokens": ["briegg", ",", "al\u00b7der", "nit", "!", "und", "bi\u00df", "de", "bisch", "wi\u00b7en"], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "ADV", "PTKNEG", "$.", "$(", "KON", "APPR", "NE", "ADV", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.18": {"text": "ich,", "tokens": ["ich", ","], "token_info": ["word", "punct"], "pos": ["PPER", "$,"], "meter": "-", "measure": "single.down"}, "line.19": {"text": "e gstandene Ma, se bini n\u00fcmme do,", "tokens": ["e", "gstan\u00b7de\u00b7ne", "Ma", ",", "se", "bi\u00b7ni", "n\u00fcm\u00b7me", "do", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "und d\u2019 Schof und Gei\u00dfe weide uf mi\u2019m Grab.", "tokens": ["und", "d'", "Schof", "und", "Gei\u00b7\u00dfe", "wei\u00b7de", "uf", "mi'm", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "KON", "NN", "VVFIN", "APPR", "NE", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.21": {"text": "Jo wegerli, und \u2019s Hus wird alt und w\u00fcst;", "tokens": ["Jo", "we\u00b7ger\u00b7li", ",", "und", "'s", "Hus", "wird", "alt", "und", "w\u00fcst", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "KON", "PPER", "NE", "VAFIN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "der Rege w\u00e4scht der\u2019s w\u00fcster alli Nacht,", "tokens": ["der", "Re\u00b7ge", "w\u00e4scht", "der's", "w\u00fcs\u00b7ter", "al\u00b7li", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "ADJD", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "und d\u2019 Sunne bleicht der\u2019s schw\u00e4rzer alli Tag,", "tokens": ["und", "d'", "Sun\u00b7ne", "bleicht", "der's", "schw\u00e4r\u00b7zer", "al\u00b7li", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "VVFIN", "PIS", "ADJD", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.24": {"text": "und im Vert\u00e4fer popperet der Wurm.", "tokens": ["und", "im", "Ver\u00b7t\u00e4\u00b7fer", "pop\u00b7pe\u00b7ret", "der", "Wurm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Es regnet no dur d\u2019 B\u00fchne ab, es pfist", "tokens": ["Es", "reg\u00b7net", "no", "dur", "d'", "B\u00fch\u00b7ne", "ab", ",", "es", "pfist"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "FM", "FM", "FM", "NN", "PTKVZ", "$,", "PPER", "VVFIN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "der Wind dur d\u2019 Chlimse. Dr\u00fcber thuesch", "tokens": ["der", "Wind", "dur", "d'", "Chlim\u00b7se", ".", "Dr\u00fc\u00b7ber", "thu\u00b7esch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "APPR", "NE", "NE", "$.", "ADV", "ADJD"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.27": {"text": "du au", "tokens": ["du", "au"], "token_info": ["word", "word"], "pos": ["PPER", "NE"], "meter": "-+", "measure": "iambic.single"}, "line.28": {"text": "no d\u2019 Auge zu; es ch\u00f6mme Chindes-Chind,", "tokens": ["no", "d'", "Au\u00b7ge", "zu", ";", "es", "ch\u00f6m\u00b7me", "Chin\u00b7des\u00b7Chind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "PTKVZ", "$.", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.29": {"text": "und pletze dra. Z\u2019lezt fuults im Fundement,", "tokens": ["und", "plet\u00b7ze", "dra", ".", "Z'\u00b7lezt", "fu\u00b7ults", "im", "Fun\u00b7de\u00b7ment", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "$.", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "und\u2019s hilft n\u00fct me. Und wemme nootno gar", "tokens": ["un\u00b7d's", "hilft", "n\u00fct", "me", ".", "Und", "wem\u00b7me", "noot\u00b7no", "gar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$.", "KON", "VVFIN", "ADV", "ADV"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.31": {"text": "zweytusig zehlt, isch alles zsemme g\u2019keit.", "tokens": ["zwey\u00b7tu\u00b7sig", "zehlt", ",", "isch", "al\u00b7les", "zsem\u00b7me", "g'\u00b7keit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "ADJD", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.32": {"text": "Und endli sinkt \u2019s ganz D\u00f6rfli in si Grab.", "tokens": ["Und", "end\u00b7li", "sinkt", "'s", "ganz", "D\u00f6rf\u00b7li", "in", "si", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PPER", "ADV", "NE", "APPR", "NE", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.33": {"text": "Wo d\u2019 Chilche stoht, wo \u2019s Vogts und \u2019s", "tokens": ["Wo", "d'", "Chil\u00b7che", "stoht", ",", "wo", "'s", "Vogts", "und", "'s"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "NN", "VVFIN", "$,", "PWAV", "PPER", "NE", "KON", "PPER"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.34": {"text": "Here Hus,", "tokens": ["He\u00b7re", "Hus", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.35": {"text": "goht mit der Zit der Pflug \u2014", "tokens": ["goht", "mit", "der", "Zit", "der", "Pflug"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}