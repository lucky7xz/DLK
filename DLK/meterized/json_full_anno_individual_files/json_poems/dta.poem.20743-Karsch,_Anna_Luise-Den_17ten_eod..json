{"dta.poem.20743": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Den 17ten   eod.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mich darf nicht ein Patent des Kaisers hei\u00dfen kr\u00f6nen,                 ", "tokens": ["Mich", "darf", "nicht", "ein", "Pa\u00b7tent", "des", "Kai\u00b7sers", "hei\u00b7\u00dfen", "kr\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ART", "NN", "ART", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mich darf nur Freundes Lob umt\u00f6nen,", "tokens": ["Mich", "darf", "nur", "Freun\u00b7des", "Lob", "um\u00b7t\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Lorber und mein Gl\u00fcck sind sie.", "tokens": ["Mein", "Lor\u00b7ber", "und", "mein", "Gl\u00fcck", "sind", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "VAFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit ihren Herzen f\u00fchlt mein Herz viel Harmonie.", "tokens": ["Mit", "ih\u00b7ren", "Her\u00b7zen", "f\u00fchlt", "mein", "Herz", "viel", "Har\u00b7mo\u00b7nie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie \u00fcberblicken meine M\u00e4ngel,", "tokens": ["Sie", "\u00fc\u00b7berb\u00b7li\u00b7cken", "mei\u00b7ne", "M\u00e4n\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Denn wenn ich Sapho bin, so bin ich noch kein Engel,", "tokens": ["Denn", "wenn", "ich", "Sa\u00b7pho", "bin", ",", "so", "bin", "ich", "noch", "kein", "En\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NE", "VAFIN", "$,", "ADV", "VAFIN", "PPER", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Auch hat oft Fehler noch mein Lied.", "tokens": ["Auch", "hat", "oft", "Feh\u00b7ler", "noch", "mein", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "NN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Manch kleines Fleckchen hat die Rose, wenn sie bl\u00fcht,", "tokens": ["Manch", "klei\u00b7nes", "Fleck\u00b7chen", "hat", "die", "Ro\u00b7se", ",", "wenn", "sie", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mein Herz h\u00f6rt niemals auf zu wallen,", "tokens": ["Mein", "Herz", "h\u00f6rt", "nie\u00b7mals", "auf", "zu", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "APPR", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Auch h\u00f6ret mein Gesang nicht auf euch vorzuschallen,", "tokens": ["Auch", "h\u00f6\u00b7ret", "mein", "Ge\u00b7sang", "nicht", "auf", "euch", "vor\u00b7zu\u00b7schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PTKNEG", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So lange bis die Uhr des Lebens stille steht,", "tokens": ["So", "lan\u00b7ge", "bis", "die", "Uhr", "des", "Le\u00b7bens", "stil\u00b7le", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "ART", "NN", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Dann seufz ich noch dreimal: Mein Sch\u00e4fer! mein", "tokens": ["Dann", "seufz", "ich", "noch", "drei\u00b7mal", ":", "Mein", "Sch\u00e4\u00b7fer", "!", "mein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "$.", "PPOSAT", "NN", "$.", "PPOSAT"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.13": {"text": "Poet!", "tokens": ["Po\u00b7et", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+", "measure": "iambic.single"}}}}}