{"textgrid.poem.37503": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Schmied und Teufel", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein kleiner Teufel, b\u00f6s und frech,", "tokens": ["Ein", "klei\u00b7ner", "Teu\u00b7fel", ",", "b\u00f6s", "und", "frech", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kommt aus der H\u00f6lle, schwarz wie Pech.", "tokens": ["Kommt", "aus", "der", "H\u00f6l\u00b7le", ",", "schwarz", "wie", "Pech", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "ADJD", "KOKOM", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Schmied tut sich entsatzen,", "tokens": ["Der", "Schmied", "tut", "sich", "ent\u00b7sat\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Teufel will ihn kratzen.", "tokens": ["Der", "Teu\u00b7fel", "will", "ihn", "krat\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Durch eine hohle Tonnen", "tokens": ["Durch", "ei\u00b7ne", "hoh\u00b7le", "Ton\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist ihm der Schmied entronnen.", "tokens": ["Ist", "ihm", "der", "Schmied", "ent\u00b7ron\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Schmied sitzt bei der Schraube,", "tokens": ["Der", "Schmied", "sitzt", "bei", "der", "Schrau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Teufel zupft die Haube.", "tokens": ["Der", "Teu\u00b7fel", "zupft", "die", "Hau\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der Teufel n\u00e4hert der Klammer sich:", "tokens": ["Der", "Teu\u00b7fel", "n\u00e4\u00b7hert", "der", "Klam\u00b7mer", "sich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PRF", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ja, siehst du wohl! Da hat er dich!", "tokens": ["Ja", ",", "siehst", "du", "wohl", "!", "Da", "hat", "er", "dich", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "PPER", "ADV", "$.", "ADV", "VAFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Er fa\u00dft ihn mit der Zange,", "tokens": ["Er", "fa\u00dft", "ihn", "mit", "der", "Zan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dem Teufel wird es bange.", "tokens": ["Dem", "Teu\u00b7fel", "wird", "es", "ban\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word"