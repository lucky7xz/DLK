{"dta.poem.7277": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Das Hirtenfeuer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.85", "af:0.14"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Dunkel, dunkel im Moor,", "tokens": ["Dun\u00b7kel", ",", "dun\u00b7kel", "im", "Moor", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Ueber der Haide Nacht,", "tokens": ["Ue\u00b7ber", "der", "Hai\u00b7de", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Nur das rieselnde Rohr", "tokens": ["Nur", "das", "rie\u00b7seln\u00b7de", "Rohr"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Neben der M\u00fchle wacht,", "tokens": ["Ne\u00b7ben", "der", "M\u00fch\u00b7le", "wacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Und an des Rades Speichen", "tokens": ["Und", "an", "des", "Ra\u00b7des", "Spei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Schwellende Tropfen schleichen.", "tokens": ["Schwel\u00b7len\u00b7de", "Trop\u00b7fen", "schlei\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Unke kauert im Sumpf,", "tokens": ["Un\u00b7ke", "kau\u00b7ert", "im", "Sumpf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Igel im Grase duckt,", "tokens": ["I\u00b7gel", "im", "Gra\u00b7se", "duckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "In dem modernden Stumpf", "tokens": ["In", "dem", "mo\u00b7dern\u00b7den", "Stumpf"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Schlafend die Kr\u00f6te zuckt,", "tokens": ["Schla\u00b7fend", "die", "Kr\u00f6\u00b7te", "zuckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Und am sandigen Hange", "tokens": ["Und", "am", "san\u00b7di\u00b7gen", "Han\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Rollt sich fester die Schlange.", "tokens": ["Rollt", "sich", "fes\u00b7ter", "die", "Schlan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.3": {"line.1": {"text": "Was glimmt dort hinterm Ginster,", "tokens": ["Was", "glimmt", "dort", "hin\u00b7term", "Gins\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und bildet lichte Scheiben?", "tokens": ["Und", "bil\u00b7det", "lich\u00b7te", "Schei\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nun wirft es Funkenflinster,", "tokens": ["Nun", "wirft", "es", "Fun\u00b7ken\u00b7flins\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die l\u00f6schend niederst\u00e4uben;", "tokens": ["Die", "l\u00f6\u00b7schend", "nie\u00b7der\u00b7st\u00e4u\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nun wieder alles dunkel \u2014", "tokens": ["Nun", "wie\u00b7der", "al\u00b7les", "dun\u00b7kel"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "ADJD", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ich h\u00f6r des Stahles Picken,", "tokens": ["Ich", "h\u00f6r", "des", "Stah\u00b7les", "Pi\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ein Knistern, ein Gefunkel \u2014", "tokens": ["Ein", "Knis\u00b7tern", ",", "ein", "Ge\u00b7fun\u00b7kel"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und auf die Flammen z\u00fccken.", "tokens": ["Und", "auf", "die", "Flam\u00b7men", "z\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und Hirtenbuben hocken", "tokens": ["Und", "Hir\u00b7ten\u00b7bu\u00b7ben", "ho\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Kreis' umher, sie strecken", "tokens": ["Im", "Kreis'", "um\u00b7her", ",", "sie", "stre\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "PTKVZ", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die H\u00e4nde, Torfes Brocken", "tokens": ["Die", "H\u00e4n\u00b7de", ",", "Tor\u00b7fes", "Bro\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Seh ich die Lohe lecken;", "tokens": ["Seh", "ich", "die", "Lo\u00b7he", "le\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Da bricht ein starker Knabe", "tokens": ["Da", "bricht", "ein", "star\u00b7ker", "Kna\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Aus des Gestrippes Windel,", "tokens": ["Aus", "des", "Ge\u00b7strip\u00b7pes", "Win\u00b7del", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und schleifet nach im Trabe", "tokens": ["Und", "schlei\u00b7fet", "nach", "im", "Tra\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein w\u00fcst Wacholderb\u00fcndel.", "tokens": ["Ein", "w\u00fcst", "Wa\u00b7chol\u00b7der\u00b7b\u00fcn\u00b7del", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Er l\u00e4\u00dft's am Feuer kippen \u2014", "tokens": ["Er", "l\u00e4\u00dft's", "am", "Feu\u00b7er", "kip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hei, wie die Buben johlen,", "tokens": ["Hei", ",", "wie", "die", "Bu\u00b7ben", "joh\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und mit den Fingern schnippen", "tokens": ["Und", "mit", "den", "Fin\u00b7gern", "schnip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Die Funken-Girandolen!", "tokens": ["Die", "Fun\u00b7ken\u00b7Gi\u00b7ran\u00b7do\u00b7len", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie ihre Zipfelm\u00fctzen", "tokens": ["Wie", "ih\u00b7re", "Zip\u00b7fel\u00b7m\u00fct\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Am Ohre lustig flattern,", "tokens": ["Am", "Oh\u00b7re", "lus\u00b7tig", "flat\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und wie die Nadeln spritzen,", "tokens": ["Und", "wie", "die", "Na\u00b7deln", "sprit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und wie die Aeste knattern!", "tokens": ["Und", "wie", "die", "A\u00b7es\u00b7te", "knat\u00b7tern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Die Flamme sinkt, sie hocken", "tokens": ["Die", "Flam\u00b7me", "sinkt", ",", "sie", "ho\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf's Neu' umher im Kreise,", "tokens": ["Auf's", "Neu'", "um\u00b7her", "im", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wieder fliegen Brocken,", "tokens": ["Und", "wie\u00b7der", "flie\u00b7gen", "Bro\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und wieder schwehlt es leise;", "tokens": ["Und", "wie\u00b7der", "schwehlt", "es", "lei\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gl\u00fchrothe Lichter streichen", "tokens": ["Gl\u00fch\u00b7ro\u00b7the", "Lich\u00b7ter", "strei\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "An Haarbusch und Gesichte,", "tokens": ["An", "Haar\u00b7busch", "und", "Ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und schier D\u00e4monen gleichen", "tokens": ["Und", "schier", "D\u00e4\u00b7mo\u00b7nen", "glei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die kleinen Haidewichte.", "tokens": ["Die", "klei\u00b7nen", "Hai\u00b7de\u00b7wich\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Der da, der Unbeschuh'te,", "tokens": ["Der", "da", ",", "der", "Un\u00b7be\u00b7schuh'\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was streckt er in das Dunkel", "tokens": ["Was", "streckt", "er", "in", "das", "Dun\u00b7kel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den Arm wie eine Ruthe,", "tokens": ["Den", "Arm", "wie", "ei\u00b7ne", "Ru\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Kreise welch' Gemunkel?", "tokens": ["Im", "Krei\u00b7se", "welch'", "Ge\u00b7mun\u00b7kel", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PWAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie sp\u00e4hn wie junge Geier", "tokens": ["Sie", "sp\u00e4hn", "wie", "jun\u00b7ge", "Gei\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von ihrer Ginstersch\u00fctte:", "tokens": ["Von", "ih\u00b7rer", "Gins\u00b7ter\u00b7sch\u00fct\u00b7te", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Hah, noch ein Hirtenfeuer,", "tokens": ["Hah", ",", "noch", "ein", "Hir\u00b7ten\u00b7feu\u00b7er", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Recht an des Dammes Mitte!", "tokens": ["Recht", "an", "des", "Dam\u00b7mes", "Mit\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Man sieht es eben steigen", "tokens": ["Man", "sieht", "es", "e\u00b7ben", "stei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und seine Schimmer breiten,", "tokens": ["Und", "sei\u00b7ne", "Schim\u00b7mer", "brei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den wirren Funkenreigen", "tokens": ["Den", "wir\u00b7ren", "Fun\u00b7ken\u00b7rei\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ueber'n Wacholder gleiten;", "tokens": ["Ue\u00b7ber'n", "Wa\u00b7chol\u00b7der", "glei\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Buben fl\u00fcstern leise,", "tokens": ["Die", "Bu\u00b7ben", "fl\u00fcs\u00b7tern", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sie r\u00e4uspern ihre Kehlen,", "tokens": ["Sie", "r\u00e4us\u00b7pern", "ih\u00b7re", "Keh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und alte Haideweisen", "tokens": ["Und", "al\u00b7te", "Hai\u00b7de\u00b7wei\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Verzittern durch die Schmehlen.", "tokens": ["Ver\u00b7zit\u00b7tern", "durch", "die", "Schmeh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "\u201ehelo, heloe!", "tokens": ["\u201e", "he\u00b7lo", ",", "he\u00b7loe", "!"], "token_info": ["punct", "word", "punct", "word", "punct"], "pos": ["$(", "ITJ", "$,", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "\u201eheloe, loe!", "tokens": ["\u201e", "he\u00b7loe", ",", "loe", "!"], "token_info": ["punct", "word", "punct", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "\u201ekomm du auf uns're Haide,", "tokens": ["\u201e", "komm", "du", "auf", "un\u00b7s'\u00b7re", "Hai\u00b7de", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "\u201ewo ich meine Sch\u00e4flein weide,", "tokens": ["\u201e", "wo", "ich", "mei\u00b7ne", "Sch\u00e4f\u00b7lein", "wei\u00b7de", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "\u201ekomm, o komm in unser Bruch,", "tokens": ["\u201e", "komm", ",", "o", "komm", "in", "un\u00b7ser", "Bruch", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "FM", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "\u201eda gibt's der Bl\u00fcmelein genug, \u2014", "tokens": ["\u201e", "da", "gibt's", "der", "Bl\u00fc\u00b7me\u00b7lein", "ge\u00b7nug", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VVFIN", "ART", "NN", "ADV", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "\u201ehelo, heloe!\u201c", "tokens": ["\u201e", "he\u00b7lo", ",", "he\u00b7loe", "!", "\u201c"], "token_info": ["punct", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "ITJ", "$,", "VVFIN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.10": {"line.1": {"text": "Die Knaben schweigen, lauschen nach dem Tann,", "tokens": ["Die", "Kna\u00b7ben", "schwei\u00b7gen", ",", "lau\u00b7schen", "nach", "dem", "Tann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und leise durch den Ginster zieht's heran:", "tokens": ["Und", "lei\u00b7se", "durch", "den", "Gins\u00b7ter", "zieht's", "he\u00b7ran", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}