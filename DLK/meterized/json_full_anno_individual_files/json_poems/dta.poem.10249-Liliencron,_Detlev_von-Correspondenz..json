{"dta.poem.10249": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Correspondenz.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Im Garten, heute Morgen,             ", "tokens": ["Im", "Gar\u00b7ten", ",", "heu\u00b7te", "Mor\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als ich deinen Brief erbrach,", "tokens": ["Als", "ich", "dei\u00b7nen", "Brief", "er\u00b7brach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fand ich darin verborgen", "tokens": ["Fand", "ich", "da\u00b7rin", "ver\u00b7bor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PAV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Rosenblatt.", "tokens": ["Ein", "Ro\u00b7sen\u00b7blatt", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Ein Rosenblatt, deinen Locken entsunken.", "tokens": ["Ein", "Ro\u00b7sen\u00b7blatt", ",", "dei\u00b7nen", "Lo\u00b7cken", "ent\u00b7sun\u00b7ken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Als ich es trunken mit den Lippen ber\u00fchrte,", "tokens": ["Als", "ich", "es", "trun\u00b7ken", "mit", "den", "Lip\u00b7pen", "be\u00b7r\u00fchr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Kam ein Windhauch und entf\u00fchrte", "tokens": ["Kam", "ein", "Wind\u00b7hauch", "und", "ent\u00b7f\u00fchr\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Den holden Gast.", "tokens": ["Den", "hol\u00b7den", "Gast", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Nun segelt es lustig zu dir zur\u00fcck.", "tokens": ["Nun", "se\u00b7gelt", "es", "lus\u00b7tig", "zu", "dir", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Gleich einer Krone tr\u00e4gt es mein Gl\u00fcck", "tokens": ["Gleich", "ei\u00b7ner", "Kro\u00b7ne", "tr\u00e4gt", "es", "mein", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.11": {"text": "Auf tiefrothem Sammt \u2014 und verbla\u00dft.", "tokens": ["Auf", "tief\u00b7ro\u00b7them", "Sammt", "und", "ver\u00b7bla\u00dft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$(", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}