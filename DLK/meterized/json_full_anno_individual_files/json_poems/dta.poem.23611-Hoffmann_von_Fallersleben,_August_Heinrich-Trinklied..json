{"dta.poem.23611": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Trinklied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Unsre V\u00e4ter sind gesessen", "tokens": ["Uns\u00b7re", "V\u00e4\u00b7ter", "sind", "ge\u00b7ses\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch vor vollen Gl\u00e4sern hier,", "tokens": ["Auch", "vor", "vol\u00b7len", "Gl\u00e4\u00b7sern", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unsre V\u00e4ter sind vergessen,", "tokens": ["Uns\u00b7re", "V\u00e4\u00b7ter", "sind", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und vergessen werden wir.", "tokens": ["Und", "ver\u00b7ges\u00b7sen", "wer\u00b7den", "wir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wer kann alles auch behalten,", "tokens": ["Wer", "kann", "al\u00b7les", "auch", "be\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PIS", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was geschieht und nicht geschieht?", "tokens": ["Was", "ge\u00b7schieht", "und", "nicht", "ge\u00b7schieht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KON", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ob sich hier die Stirn' in Falten,", "tokens": ["Ob", "sich", "hier", "die", "Stirn'", "in", "Fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADV", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dort der Mund zum L\u00e4cheln zieht?", "tokens": ["Dort", "der", "Mund", "zum", "L\u00e4\u00b7cheln", "zieht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Leer' und volle K\u00f6pf' und Taschen", "tokens": ["Leer'", "und", "vol\u00b7le", "K\u00f6pf'", "und", "Ta\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Werden nach uns auch noch sein,", "tokens": ["Wer\u00b7den", "nach", "uns", "auch", "noch", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "ADV", "ADV", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nach uns giebt's noch Kr\u00fcg' und Flaschen,", "tokens": ["Nach", "uns", "giebt's", "noch", "Kr\u00fcg'", "und", "Fla\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ADV", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gl\u00e4ser mit und ohne Wein.", "tokens": ["Gl\u00e4\u00b7ser", "mit", "und", "oh\u00b7ne", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "KON", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}