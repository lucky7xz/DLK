{"dta.poem.14711": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Waldm\u00e4dchen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wo die wilden B\u00e4che rauschen", "tokens": ["Wo", "die", "wil\u00b7den", "B\u00e4\u00b7che", "rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die hohen Palmen steh'n,", "tokens": ["Und", "die", "ho\u00b7hen", "Pal\u00b7men", "steh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn die J\u00e4ger heimlich lauschen\u201a", "tokens": ["Wenn", "die", "J\u00e4\u00b7ger", "heim\u00b7lich", "lau\u00b7schen", "\u201a"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Viele Rehe einsam geh'n.", "tokens": ["Vie\u00b7le", "Re\u00b7he", "ein\u00b7sam", "geh'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["PIAT", "NN", "ADJD", "VVFIN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bin ein Reh, flieg' durch die Tr\u00fcmmer,", "tokens": ["Bin", "ein", "Reh", ",", "flieg'", "durch", "die", "Tr\u00fcm\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ueber die H\u00f6h',", "tokens": ["Ue\u00b7ber", "die", "H\u00f6h'", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Wo im Schnee", "tokens": ["Wo", "im", "Schnee"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "APPRART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Still die letzten Gipfel schimmern\u201a", "tokens": ["Still", "die", "letz\u00b7ten", "Gip\u00b7fel", "schim\u00b7mern", "\u201a"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Folg' mir nicht, erjagst mich nimmer!", "tokens": ["Fol\u00b7g'", "mir", "nicht", ",", "er\u00b7jagst", "mich", "nim\u00b7mer", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}