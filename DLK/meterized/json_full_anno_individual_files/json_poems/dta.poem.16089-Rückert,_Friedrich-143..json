{"dta.poem.16089": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "143.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4r' es mit einem dir mislungen oder zweien,", "tokens": ["W\u00e4r'", "es", "mit", "ei\u00b7nem", "dir", "mis\u00b7lun\u00b7gen", "o\u00b7der", "zwei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "PPER", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du k\u00f6nntest sagen, da\u00df sie schuld am Zwiespalt seien.", "tokens": ["Du", "k\u00f6nn\u00b7test", "sa\u00b7gen", ",", "da\u00df", "sie", "schuld", "am", "Zwies\u00b7palt", "sei\u00b7en", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "KOUS", "PPER", "ADJD", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Da es mit mehreren, mit allen dir mislingt,", "tokens": ["Da", "es", "mit", "meh\u00b7re\u00b7ren", ",", "mit", "al\u00b7len", "dir", "mis\u00b7lingt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "VVINF", "$,", "APPR", "PIAT", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie kannst du zweifeln, da\u00df die Schuld aus dir entspringt?", "tokens": ["Wie", "kannst", "du", "zwei\u00b7feln", ",", "da\u00df", "die", "Schuld", "aus", "dir", "ent\u00b7springt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFI