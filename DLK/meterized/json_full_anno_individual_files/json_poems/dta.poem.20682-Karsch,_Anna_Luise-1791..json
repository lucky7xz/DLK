{"dta.poem.20682": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "1791.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Verhelen kann ich Dir's, o Ritter, nun nicht l\u00e4nger,                   ", "tokens": ["Ver\u00b7he\u00b7len", "kann", "ich", "Dir's", ",", "o", "Rit\u00b7ter", ",", "nun", "nicht", "l\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "NE", "$,", "FM", "NN", "$,", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du stammst in grader Linie", "tokens": ["Du", "stammst", "in", "gra\u00b7der", "Li\u00b7nie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Von einem edlen Minnes\u00e4nger.", "tokens": ["Von", "ei\u00b7nem", "ed\u00b7len", "Min\u00b7ne\u00b7s\u00e4n\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem thats im tapfern Herzen weh,", "tokens": ["Dem", "thats", "im", "tap\u00b7fern", "Her\u00b7zen", "weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn irgend sich ein Ritter fand zum Tadel", "tokens": ["Wenn", "ir\u00b7gend", "sich", "ein", "Rit\u00b7ter", "fand", "zum", "Ta\u00b7del"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PRF", "ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Dame Seiner Huldigung \u2014", "tokens": ["Der", "Da\u00b7me", "Sei\u00b7ner", "Hul\u00b7di\u00b7gung"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Ich lobe Deinen Seelenadel", "tokens": ["Ich", "lo\u00b7be", "Dei\u00b7nen", "See\u00b7le\u00b7na\u00b7del"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und Deinen Geisteswaffenschwung,", "tokens": ["Und", "Dei\u00b7nen", "Geis\u00b7tes\u00b7waf\u00b7fen\u00b7schwung", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Womit Du den hast \u00fcberstritten,", "tokens": ["Wo\u00b7mit", "Du", "den", "hast", "\u00fc\u00b7bers\u00b7trit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "VAFIN", "VVPP", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Der mit dahergeschwatztem Ton", "tokens": ["Der", "mit", "da\u00b7her\u00b7ge\u00b7schwatz\u00b7tem", "Ton"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Den Angriff that auf Weiber-Witz und Sitten;", "tokens": ["Den", "An\u00b7griff", "that", "auf", "Wei\u00b7ber\u00b7Witz", "und", "Sit\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Es war ja nur ein Alltagshohn,", "tokens": ["Es", "war", "ja", "nur", "ein", "All\u00b7tags\u00b7hohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ein hundertmal schon abgenutzt Gesp\u00f6tte,", "tokens": ["Ein", "hun\u00b7dert\u00b7mal", "schon", "ab\u00b7ge\u00b7nutzt", "Ge\u00b7sp\u00f6t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVPP", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Das Deinen starken Widerstand", "tokens": ["Das", "Dei\u00b7nen", "star\u00b7ken", "Wi\u00b7der\u00b7stand"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Zur H\u00e4lfte kaum verdienet h\u00e4tte.", "tokens": ["Zur", "H\u00e4lf\u00b7te", "kaum", "ver\u00b7die\u00b7net", "h\u00e4t\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Der Mann, den Du mit einer Hand", "tokens": ["Der", "Mann", ",", "den", "Du", "mit", "ei\u00b7ner", "Hand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Gar leicht zu Falle konntest bringen,", "tokens": ["Gar", "leicht", "zu", "Fal\u00b7le", "konn\u00b7test", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Ist ein armseelig Ritterlein.", "tokens": ["Ist", "ein", "arm\u00b7see\u00b7lig", "Rit\u00b7ter\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Der andre, welchen Dir die Muse half bezwingen,", "tokens": ["Der", "and\u00b7re", ",", "wel\u00b7chen", "Dir", "die", "Mu\u00b7se", "half", "be\u00b7zwin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$,", "PWAT", "PPER", "ART", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Von der ich r\u00fchme, sie sey mein,", "tokens": ["Von", "der", "ich", "r\u00fch\u00b7me", ",", "sie", "sey", "mein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Ist aller Ehren werth. Gr\u00fc\u00df Ihn von meinetwegen,", "tokens": ["Ist", "al\u00b7ler", "Eh\u00b7ren", "werth", ".", "Gr\u00fc\u00df", "Ihn", "von", "mei\u00b7net\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADJD", "$.", "NN", "PPER", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und wenn Du nicht die ganze Welt", "tokens": ["Und", "wenn", "Du", "nicht", "die", "gan\u00b7ze", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Nach Deinem Wunsch mir kannst zu F\u00fc\u00dfen legen,", "tokens": ["Nach", "Dei\u00b7nem", "Wunsch", "mir", "kannst", "zu", "F\u00fc\u00b7\u00dfen", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "So bleibt es Dir doch freigestellt,", "tokens": ["So", "bleibt", "es", "Dir", "doch", "frei\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Vom Sitz des Herzogs und des Helden", "tokens": ["Vom", "Sitz", "des", "Her\u00b7zogs", "und", "des", "Hel\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Zu reisen in die K\u00f6nigliche Stadt,", "tokens": ["Zu", "rei\u00b7sen", "in", "die", "K\u00f6\u00b7nig\u00b7li\u00b7che", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Und bei der Dame Dich zu melden,", "tokens": ["Und", "bei", "der", "Da\u00b7me", "Dich", "zu", "mel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Die Dich so ganz bezaubert hat", "tokens": ["Die", "Dich", "so", "ganz", "be\u00b7zau\u00b7bert", "hat"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "ADV", "VVPP", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Mit einem Reiz, der nie veraltet.", "tokens": ["Mit", "ei\u00b7nem", "Reiz", ",", "der", "nie", "ver\u00b7al\u00b7tet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Im \u00fcbrigen hat die Natur", "tokens": ["Im", "\u00fcb\u00b7ri\u00b7gen", "hat", "die", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "VAFIN", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.31": {"text": "Sie zum Bezaubern nicht gestaltet,", "tokens": ["Sie", "zum", "Be\u00b7zau\u00b7bern", "nicht", "ge\u00b7stal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Befrag die Freundinn Campe nur,", "tokens": ["Be\u00b7frag", "die", "Freun\u00b7dinn", "Cam\u00b7pe", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NE", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Sie wird Dir Red und Antwort geben.", "tokens": ["Sie", "wird", "Dir", "Red", "und", "Ant\u00b7wort", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie spricht: das Aug\u2019 ist dunkelblau und klar,", "tokens": ["Sie", "spricht", ":", "das", "Aug'", "ist", "dun\u00b7kel\u00b7blau", "und", "klar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ist ziemlich noch voll Geist und Leben.", "tokens": ["Ist", "ziem\u00b7lich", "noch", "voll", "Geist", "und", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gefallen w\u00fcrd\u2019 es, wenn das Haar", "tokens": ["Ge\u00b7fal\u00b7len", "w\u00fcrd'", "es", ",", "wenn", "das", "Haar"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Augenwimpern l\u00e4nger w\u00e4r gezogen,", "tokens": ["Der", "Au\u00b7gen\u00b7wim\u00b7pern", "l\u00e4n\u00b7ger", "w\u00e4r", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Auch fehlt dem Augenbram ein feingew\u00f6lbter Bogen.", "tokens": ["Auch", "fehlt", "dem", "Au\u00b7gen\u00b7bram", "ein", "fein\u00b7ge\u00b7w\u00f6lb\u00b7ter", "Bo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Stirn ist gro\u00df, die Schl\u00e4fe tief gedr\u00fcckt,", "tokens": ["Die", "Stirn", "ist", "gro\u00df", ",", "die", "Schl\u00e4\u00b7fe", "tief", "ge\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der Mund ist viel zu platt geschnitten,", "tokens": ["Der", "Mund", "ist", "viel", "zu", "platt", "ge\u00b7schnit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ob ihn gleich noch die R\u00f6the schm\u00fcckt,", "tokens": ["Ob", "ihn", "gleich", "noch", "die", "R\u00f6\u00b7the", "schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die von den Jahren nichts gelitten,", "tokens": ["Die", "von", "den", "Jah\u00b7ren", "nichts", "ge\u00b7lit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Von acht und sechzig Jahren nichts \u2014", "tokens": ["Von", "acht", "und", "sech\u00b7zig", "Jah\u00b7ren", "nichts"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "KON", "CARD", "NN", "PIS", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Das ist besonders, wirst Du sagen;", "tokens": ["Das", "ist", "be\u00b7son\u00b7ders", ",", "wirst", "Du", "sa\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "$,", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Doch nach der Bildung des Gesichts", "tokens": ["Doch", "nach", "der", "Bil\u00b7dung", "des", "Ge\u00b7sichts"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.13": {"text": "Scheinst Du, Herr Ritter, nicht zu fragen.", "tokens": ["Scheinst", "Du", ",", "Herr", "Rit\u00b7ter", ",", "nicht", "zu", "fra\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "NN", "$,", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Du liebst die Seele, die nicht stirbt,", "tokens": ["Du", "liebst", "die", "See\u00b7le", ",", "die", "nicht", "stirbt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Wenn sich in der begrabnen H\u00fclle", "tokens": ["Wenn", "sich", "in", "der", "be\u00b7grab\u00b7nen", "H\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Ein Mottenschwarm um Unterhalt bewirbt,", "tokens": ["Ein", "Mot\u00b7ten\u00b7schwarm", "um", "Un\u00b7ter\u00b7halt", "be\u00b7wirbt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Wenn eine kalte Schlummerstille", "tokens": ["Wenn", "ei\u00b7ne", "kal\u00b7te", "Schlum\u00b7mer\u00b7stil\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Den Kopf bedeckt, der ehedem", "tokens": ["Den", "Kopf", "be\u00b7deckt", ",", "der", "e\u00b7he\u00b7dem"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "PRELS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Gedanken ohne Zahl geboren,", "tokens": ["Ge\u00b7dan\u00b7ken", "oh\u00b7ne", "Zahl", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Und nun den Geist, der sie gedacht, verloren.", "tokens": ["Und", "nun", "den", "Geist", ",", "der", "sie", "ge\u00b7dacht", ",",