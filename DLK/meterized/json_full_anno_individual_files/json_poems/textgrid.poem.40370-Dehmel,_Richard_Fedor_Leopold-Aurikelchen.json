{"textgrid.poem.40370": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Aurikelchen", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aurikelchen, Aurikelchen", "tokens": ["Au\u00b7ri\u00b7kel\u00b7chen", ",", "Au\u00b7ri\u00b7kel\u00b7chen"], "token_info": ["word", "punct", "word"], "pos": ["NE", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "stehn auf meinem Beet,", "tokens": ["stehn", "auf", "mei\u00b7nem", "Beet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "und sehn den blauen Himmel an,", "tokens": ["und", "sehn", "den", "blau\u00b7en", "Him\u00b7mel", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "wo schon den ganzen Morgen", "tokens": ["wo", "schon", "den", "gan\u00b7zen", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "die goldne Sonne steht.", "tokens": ["die", "gold\u00b7ne", "Son\u00b7ne", "steht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Aurikelchen, Aurikelchen,", "tokens": ["Au\u00b7ri\u00b7kel\u00b7chen", ",", "Au\u00b7ri\u00b7kel\u00b7chen", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "was kuckt ihr denn so sehr?", "tokens": ["was", "kuckt", "ihr", "denn", "so", "sehr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr seid ja selbst so gelb wie Gold,", "tokens": ["Ihr", "seid", "ja", "selbst", "so", "gelb", "wie", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und habt ein hellrot Herzchen,", "tokens": ["und", "habt", "ein", "hell\u00b7rot", "Herz\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "was wollt ihr denn noch mehr!", "tokens": ["was", "wollt", "ihr", "denn", "noch", "mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Aurikelchen, Aurikelchen", "tokens": ["Au\u00b7ri\u00b7kel\u00b7chen", ",", "Au\u00b7ri\u00b7kel\u00b7chen"], "token_info": ["word", "punct", "word"], "pos": ["NE", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "stehn auf meinem Beet,", "tokens": ["stehn", "auf", "mei\u00b7nem", "Beet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "und sehn den blauen Himmel an,", "tokens": ["und", "sehn", "den", "blau\u00b7en", "Him\u00b7mel", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "wo schon