{"textgrid.poem.46535": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[die kein Weh gethan auf Erden]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die kein Weh gethan auf Erden,", "tokens": ["Die", "kein", "Weh", "ge\u00b7than", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVPP", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mu\u00df nun leiden diese Wehn;", "tokens": ["Mu\u00df", "nun", "lei\u00b7den", "die\u00b7se", "Wehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "VVFIN", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df sie dir ertr\u00e4glich werden,", "tokens": ["Da\u00df", "sie", "dir", "er\u00b7tr\u00e4g\u00b7lich", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denke, da\u00df sie auch vergehn.", "tokens": ["Den\u00b7ke", ",", "da\u00df", "sie", "auch", "ver\u00b7gehn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn das Leben \u00fcberwindet,", "tokens": ["Wenn", "das", "Le\u00b7ben", "\u00fc\u00b7berw\u00b7in\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und sie bl\u00fchet frisch und jung;", "tokens": ["Und", "sie", "bl\u00fc\u00b7het", "frisch", "und",