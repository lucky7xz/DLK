: "Zu Zehnten, Dom und Br\u00fccken,", "tokens": ["Zu", "Zehn\u00b7ten", ",", "Dom", "und", "Br\u00fc\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Woll'n nicht das Haupt im Sendgericht", "tokens": ["Woll'n", "nicht", "das", "Haupt", "im", "Send\u00b7ge\u00b7richt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "PTKNEG", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor deinen Grafen b\u00fccken!", "tokens": ["Vor", "dei\u00b7nen", "Gra\u00b7fen", "b\u00fc\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Auf, schlaget alle Pfaffen tot,", "tokens": ["Auf", ",", "schla\u00b7get", "al\u00b7le", "Pfaf\u00b7fen", "tot", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "VVFIN", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Burgen brennet nieder,", "tokens": ["Die", "Bur\u00b7gen", "bren\u00b7net", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Dem Donar und dem Sassen\u00f4t", "tokens": ["Dem", "Do\u00b7nar", "und", "dem", "Sas\u00b7sen\u00f4t"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "T\u00fcrmt Stein und Altar wieder!", "tokens": ["T\u00fcrmt", "Stein", "und", "Al\u00b7tar", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Herr Karl kann uns verderben, \u2013", "tokens": ["Herr", "Karl", "kann", "uns", "ver\u00b7der\u00b7ben", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "NE", "VMFIN", "PPER", "VVFIN", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Nicht zwingen, da\u00df wir Knechte sind:", "tokens": ["Nicht", "zwin\u00b7gen", ",", "da\u00df", "wir", "Knech\u00b7te", "sind", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$,", "KOUS", "PPER", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Auf, f\u00fchr' uns, Herzog Wittekind,", "tokens": ["Auf", ",", "f\u00fchr'", "uns", ",", "Her\u00b7zog", "Wit\u00b7te\u00b7kind", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "$,", "VVFIN", "PPER", "$,", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Wir wollen lieber sterben!", "tokens": ["Wir", "wol\u00b7len", "lie\u00b7ber", "ster\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Herr Kaiser Karl, du meinst es gut", "tokens": ["Herr", "Kai\u00b7ser", "Karl", ",", "du", "meinst", "es", "gut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NN", "NE", "$,", "PPER", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit uns verstockten Heiden:", "tokens": ["Mit", "uns", "ver\u00b7stock\u00b7ten", "Hei\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In deines gro\u