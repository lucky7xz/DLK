{"dta.poem.23788": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Der S\u00fcnden-Schlaff.  \n Sonnet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1700", "urn": "urn:nbn:de:kobv:b4-200905197532", "language": ["de:0.99"], "booktitle": "[Canitz, Friedrich Rudolph Ludwig von]: Neben-Stunden Unterschiedener Gedichte. [Hrsg. v. Joachim Lange]. Berlin, 1700."}, "poem": {"stanza.1": {"line.1": {"text": "O GOtt! ich bin nicht werth/ da\u00df du mir so viel G\u00fcte/", "tokens": ["O", "Gott", "!", "ich", "bin", "nicht", "werth", "/", "da\u00df", "du", "mir", "so", "viel", "G\u00fc\u00b7te", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PPER", "VAFIN", "PTKNEG", "ADJD", "$(", "KOUS", "PPER", "PPER", "ADV", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von Kindes-Beinen an bi\u00df diesen Tag gezeigt.", "tokens": ["Von", "Kin\u00b7des\u00b7Bei\u00b7nen", "an", "bi\u00df", "die\u00b7sen", "Tag", "ge\u00b7zeigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie k\u00f6mmts denn da\u00df mein Mund von deinem", "tokens": ["Wie", "k\u00f6mmts", "denn", "da\u00df", "mein", "Mund", "von", "dei\u00b7nem"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "KON", "KOUS", "PPOSAT", "NN", "APPR", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da ich doch ohne dich in tausend Noth geriethe?", "tokens": ["Da", "ich", "doch", "oh\u00b7ne", "dich", "in", "tau\u00b7send", "Noth", "ge\u00b7rie\u00b7the", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PRF", "APPR", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie k\u00f6mmts da\u00df \u00f6fter nicht aus feurigem Gem\u00fcthe", "tokens": ["Wie", "k\u00f6mmts", "da\u00df", "\u00f6f\u00b7ter", "nicht", "aus", "feu\u00b7ri\u00b7gem", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "KOUS", "ADV", "PTKNEG", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein Weyrauch voller Danck zu deinem Throne steigt?", "tokens": ["Mein", "Wey\u00b7rauch", "vol\u00b7ler", "Danck", "zu", "dei\u00b7nem", "Thro\u00b7ne", "steigt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich habe leyder mich zum S\u00fcnden-Schlaaf geneigt!", "tokens": ["Ich", "ha\u00b7be", "ley\u00b7der", "mich", "zum", "S\u00fcn\u00b7den\u00b7Schlaaf", "ge\u00b7neigt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Wollust s\u00fcsser Traum entgeistert mein Gebl\u00fcte/", "tokens": ["Der", "Wol\u00b7lust", "s\u00fcs\u00b7ser", "Traum", "ent\u00b7geis\u00b7tert", "mein", "Ge\u00b7bl\u00fc\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Herr wecke du mich auf/ der du mein Retter bist;", "tokens": ["Herr", "we\u00b7cke", "du", "mich", "auf", "/", "der", "du", "mein", "Ret\u00b7ter", "bist", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PRF", "APPR", "$(", "PRELS", "PPER", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich wei\u00df da\u00df in dem Schlaaf mein Tod verborgen ist;", "tokens": ["Ich", "wei\u00df", "da\u00df", "in", "dem", "Schlaaf", "mein", "Tod", "ver\u00b7bor\u00b7gen", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "APPR", "ART", "NN", "PPOSAT", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df Tr\u00e4ume dieser Welt wie leichte Schatten triegen;", "tokens": ["Da\u00df", "Tr\u00e4u\u00b7me", "die\u00b7ser", "Welt", "wie", "leich\u00b7te", "Schat\u00b7ten", "trie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PDAT", "NN", "KOKOM", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Komm bald und mache mich doch deiner Liebe werth/", "tokens": ["Komm", "bald", "und", "ma\u00b7che", "mich", "doch", "dei\u00b7ner", "Lie\u00b7be", "werth", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KON", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wenn mein m\u00fcdes Hertz ja eine Ruh begehrt/", "tokens": ["Und", "wenn", "mein", "m\u00fc\u00b7des", "Hertz", "ja", "ei\u00b7ne", "Ruh", "be\u00b7gehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "ADJA", "NN", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So la\u00df es nur allein in deinen Wunden liegen.", "tokens": ["So", "la\u00df", "es", "nur", "al\u00b7lein", "in", "dei\u00b7nen", "Wun\u00b7den", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}