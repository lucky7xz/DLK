"text": "Rei\u00df ihm die pfeil vnd kocher ab,", "tokens": ["Rei\u00df", "ihm", "die", "pfeil", "vnd", "ko\u00b7cher", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "KON", "ADJD", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Vnd leg ihn ewig schlaffen.", "tokens": ["Vnd", "leg", "ihn", "e\u00b7wig", "schlaf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nur du bitt ich: du zihl auff mich;", "tokens": ["Nur", "du", "bitt", "ich", ":", "du", "zihl", "auff", "mich", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "$.", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von dir will sein getroffen:", "tokens": ["Von", "dir", "will", "sein", "ge\u00b7trof\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "O reines gifft: wan Jesus trifft!", "tokens": ["O", "rei\u00b7nes", "gifft", ":", "wan", "Je\u00b7sus", "trifft", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "PWAV", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Al\u00dfdan ist heil zu hoffen.", "tokens": ["Al\u00df\u00b7dan", "ist", "heil", "zu", "hof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wen Jesu lieb wird machen wund,", "tokens": ["Wen",