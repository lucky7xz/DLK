{"dta.poem.8705": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Am Bette eines Kindes.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Wiege sie sanft, o Schlaf, die holde Kleine!", "tokens": ["Wie\u00b7ge", "sie", "sanft", ",", "o", "Schlaf", ",", "die", "hol\u00b7de", "Klei\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "FM", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch die zarte Verh\u00fcllung deines Schleiers", "tokens": ["Durch", "die", "zar\u00b7te", "Ver\u00b7h\u00fcl\u00b7lung", "dei\u00b7nes", "Schlei\u00b7ers"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "PPOSAT", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "L\u00e4chelt sie; so l\u00e4chelt die Rose still durch", "tokens": ["L\u00e4\u00b7chelt", "sie", ";", "so", "l\u00e4\u00b7chelt", "die", "Ro\u00b7se", "still", "durch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$.", "ADV", "VVFIN", "ART", "NN", "ADJD", "APPR"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Abendged\u00fcfte.", "tokens": ["A\u00b7bend\u00b7ge\u00b7d\u00fcf\u00b7te", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Wiege sie sanft, und lege deinem Bruder", "tokens": ["Wie\u00b7ge", "sie", "sanft", ",", "und", "le\u00b7ge", "dei\u00b7nem", "Bru\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie, dem ernsteren, leise in die Arme,", "tokens": ["Sie", ",", "dem", "erns\u00b7te\u00b7ren", ",", "lei\u00b7se", "in", "die", "Ar\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "VVINF", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Ihm, durch dessen dichteren Schleier uns kein", "tokens": ["Ihm", ",", "durch", "des\u00b7sen", "dich\u00b7te\u00b7ren", "Schlei\u00b7er", "uns", "kein"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "APPR", "PRELAT", "ADJA", "NN", "PPER", "PIAT"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "L\u00e4cheln mehr schimmert!", "tokens": ["L\u00e4\u00b7cheln", "mehr", "schim\u00b7mert", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Denn mit gez\u00fccktem Dolche harrt der Kummer", "tokens": ["Denn", "mit", "ge\u00b7z\u00fcck\u00b7tem", "Dol\u00b7che", "harrt", "der", "Kum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An der seligen Kindheit Pforte meines", "tokens": ["An", "der", "se\u00b7li\u00b7gen", "Kind\u00b7heit", "Pfor\u00b7te", "mei\u00b7nes"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "PPOSAT"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Lieblings, wo der Friede sie scheidend k\u00fc\u00dft, und", "tokens": ["Lieb\u00b7lings", ",", "wo", "der", "Frie\u00b7de", "sie", "schei\u00b7dend", "k\u00fc\u00dft", ",", "und"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "PPER", "ADJD", "VVFIN", "$,", "KON"], "meter": "+---+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Schwindet auf immer.", "tokens": ["Schwin\u00b7det", "auf", "im\u00b7mer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADV", "$."], "meter": "+-+--", "measure": "unknown.measure.di"}}}}}