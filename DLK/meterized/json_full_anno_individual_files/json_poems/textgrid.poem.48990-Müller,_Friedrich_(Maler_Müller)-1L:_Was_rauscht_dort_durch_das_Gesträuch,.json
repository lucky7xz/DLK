{"textgrid.poem.48990": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was rauscht dort durch das Gestr\u00e4uch,", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was rauscht dort durch das Gestr\u00e4uch,", "tokens": ["Was", "rauscht", "dort", "durch", "das", "Ge\u00b7str\u00e4uch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Durch Nesseln, durch Brombeer'n und Schleen?", "tokens": ["Durch", "Nes\u00b7seln", ",", "durch", "Brom\u00b7beer'n", "und", "Schleen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Wie ist mir mein Herzel so weich,", "tokens": ["Wie", "ist", "mir", "mein", "Her\u00b7zel", "so", "weich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Mein L\u00e4mmchen, kann ich es nicht sehen?", "tokens": ["Mein", "L\u00e4mm\u00b7chen", ",", "kann", "ich", "es", "nicht", "se\u00b7hen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mir trauert mein Mut und mein Sinn \u2013", "tokens": ["Mir", "trau\u00b7ert", "mein", "Mut", "und", "mein", "Sinn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Ach, ach! Mein L\u00e4mmchen ist hin.", "tokens": ["Ach", ",", "ach", "!", "Mein", "L\u00e4mm\u00b7chen", "ist", "hin", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ITJ", "$.", "PPOSAT", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Was pl\u00e4tschert dort unten am Teich?", "tokens": ["Was", "pl\u00e4t\u00b7schert", "dort", "un\u00b7ten", "am", "Teich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Was bringt da die Distel und Kr\u00e4uter?", "tokens": ["Was", "bringt", "da", "die", "Dis\u00b7tel", "und", "Kr\u00e4u\u00b7ter", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ART", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Was raschelt im Eichengezweig?", "tokens": ["Was", "ra\u00b7schelt", "im", "Ei\u00b7chen\u00b7ge\u00b7zweig", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPRART", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Ich kann, ich kann nicht mehr weiter ...", "tokens": ["Ich", "kann", ",", "ich", "kann", "nicht", "mehr", "wei\u00b7ter", "..."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VMFIN", "PTKNEG", "ADV", "ADV", "$("], "meter": "++----+-", "measure": "unknown.measure.tri"}, "line.5": {"text": "Ihr Wiesen, wie seid ihr so gr\u00fcn!", "tokens": ["Ihr", "Wie\u00b7sen", ",", "wie", "seid", "ihr", "so", "gr\u00fcn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Doch ach! mein L\u00e4mmchen ist hin.", "tokens": ["Doch", "ach", "!", "mein", "L\u00e4mm\u00b7chen", "ist", "hin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PPOSAT", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "O L\u00e4mmchen, wie bin ich so m\u00fcd,", "tokens": ["O", "L\u00e4mm\u00b7chen", ",", "wie", "bin", "ich", "so", "m\u00fcd", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Soll ich dich denn nimmrer finden?", "tokens": ["Soll", "ich", "dich", "denn", "nimm\u00b7rer", "fin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch Efeu, durch Bremmen, durch Ried,", "tokens": ["Durch", "E\u00b7feu", ",", "durch", "Brem\u00b7men", ",", "durch", "Ried", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "APPR", "NN", "$,", "APPR", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Im Tal, auf Heiden und Gr\u00fcnden", "tokens": ["Im", "Tal", ",", "auf", "Hei\u00b7den", "und", "Gr\u00fcn\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Hab' ich gesucht und geschrien:", "tokens": ["Hab'", "ich", "ge\u00b7sucht", "und", "ge\u00b7schri\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "KON", "VVPP", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Wo bist du, mein L\u00e4mmchen, doch hin?", "tokens": ["Wo", "bist", "du", ",", "mein", "L\u00e4mm\u00b7chen", ",", "doch", "hin", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "ADV", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Der Rosenstrauch trauert mit mir,", "tokens": ["Der", "Ro\u00b7sen\u00b7strauch", "trau\u00b7ert", "mit", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "In dem ich aus s\u00fc\u00dfem Lavendel", "tokens": ["In", "dem", "ich", "aus", "s\u00fc\u00b7\u00dfem", "La\u00b7ven\u00b7del"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Gebauet ein Nestelein dir", "tokens": ["Ge\u00b7bau\u00b7et", "ein", "Nes\u00b7tel\u00b7ein", "dir"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PPER"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Mit Majoran, Thymian, Quendel.", "tokens": ["Mit", "Ma\u00b7jo\u00b7ran", ",", "Thy\u00b7mi\u00b7an", ",", "Quen\u00b7del", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NE", "$,", "NE", "$,", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Wenn hei\u00df die Sonne dir schien,", "tokens": ["Wenn", "hei\u00df", "die", "Son\u00b7ne", "dir", "schien", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Dann lagst du, mein L\u00e4mmchen, darin.", "tokens": ["Dann", "lagst", "du", ",", "mein", "L\u00e4mm\u00b7chen", ",", "da\u00b7rin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "PAV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Auch trauert das Farrenkraut,", "tokens": ["Auch", "trau\u00b7ert", "das", "Far\u00b7ren\u00b7kraut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "In dem ich mich \u00f6fters verstecket;", "tokens": ["In", "dem", "ich", "mich", "\u00f6f\u00b7ters", "ver\u00b7ste\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "ADV", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Dann liefst du, dann schrieest du laut,", "tokens": ["Dann", "liefst", "du", ",", "dann", "schrie\u00b7est", "du", "laut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bis da\u00df du mich endlich entdecket.", "tokens": ["Bis", "da\u00df", "du", "mich", "end\u00b7lich", "ent\u00b7de\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "PRF", "ADV", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Dann h\u00fcpfte ich fr\u00f6hlich um dich,", "tokens": ["Dann", "h\u00fcpf\u00b7te", "ich", "fr\u00f6h\u00b7lich", "um", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "PPER", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Und du sprangst bl\u00f6ckend um mich.", "tokens": ["Und", "du", "sprangst", "bl\u00f6\u00b7ckend", "um", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Ach, ach! Die Nacht bricht herein,", "tokens": ["Ach", ",", "ach", "!", "Die", "Nacht", "bricht", "her\u00b7ein", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ITJ", "$.", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "O L\u00e4mmchen, da\u00df ich dich nicht habe!", "tokens": ["O", "L\u00e4mm\u00b7chen", ",", "da\u00df", "ich", "dich", "nicht", "ha\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KOUS", "PPER", "PRF", "PTKNEG", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Heut mu\u00df ich schlafen allein,", "tokens": ["Heut", "mu\u00df", "ich", "schla\u00b7fen", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "ADV", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Allein, ich verlassener Knabe.", "tokens": ["Al\u00b7lein", ",", "ich", "ver\u00b7las\u00b7se\u00b7ner", "Kna\u00b7be", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "ADJA", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Weg Balsam, weg, Rosemarin!", "tokens": ["Weg", "Bal\u00b7sam", ",", "weg", ",", "Ro\u00b7se\u00b7ma\u00b7rin", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "NE", "$,", "PTKVZ", "$,", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Mein L\u00e4mmchen, mein L\u00e4mmchen ist hin.", "tokens": ["Mein", "L\u00e4mm\u00b7chen", ",", "mein", "L\u00e4mm\u00b7chen", "ist", "hin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "VAFIN", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.7": {"line.1": {"text": "Was rauscht dort durch das Gestr\u00e4uch,", "tokens": ["Was", "rauscht", "dort", "durch", "das", "Ge\u00b7str\u00e4uch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Durch Nesseln, durch Brombeer'n und Schleen?", "tokens": ["Durch", "Nes\u00b7seln", ",", "durch", "Brom\u00b7beer'n", "und", "Schleen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Wie ist mir mein Herzel so weich,", "tokens": ["Wie", "ist", "mir", "mein", "Her\u00b7zel", "so", "weich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Mein L\u00e4mmchen, kann ich es nicht sehen?", "tokens": ["Mein", "L\u00e4mm\u00b7chen", ",", "kann", "ich", "es", "nicht", "se\u00b7hen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", 