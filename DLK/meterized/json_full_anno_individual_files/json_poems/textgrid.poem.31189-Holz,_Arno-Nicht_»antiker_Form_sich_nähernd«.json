{"textgrid.poem.31189": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Nicht \u00bbantiker Form sich n\u00e4hernd\u00ab", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In M\u00fcnchen schneit's, und das Volk schreit nach Brod.", "tokens": ["In", "M\u00fcn\u00b7chen", "schneit's", ",", "und", "das", "Volk", "schreit", "nach", "Brod", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "$,", "KON", "ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Gaslichtverbreitung.", "tokens": ["Gas\u00b7licht\u00b7ver\u00b7brei\u00b7tung", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Der Aetna raucht und F\u00fcrst Bismarck ist todt.", "tokens": ["Der", "A\u00b7et\u00b7na", "raucht", "und", "F\u00fcrst", "Bis\u00b7marck", "ist", "todt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "KON", "NN", "NE", "VAFIN", "ADJD", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Nein, diese Zeitung!", "tokens": ["Nein", ",", "die\u00b7se", "Zei\u00b7tung", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDAT", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Wozu durch alle diese Ritzen", "tokens": ["Wo\u00b7zu", "durch", "al\u00b7le", "die\u00b7se", "Rit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PIS", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sein Blut ins Nichts vertropfen?", "tokens": ["Sein", "Blut", "ins", "Nichts", "ver\u00b7trop\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Gem\u00fcthlich hinterm Ofen sitzen", "tokens": ["Ge\u00b7m\u00fcth\u00b7lich", "hin\u00b7term", "O\u00b7fen", "sit\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und seine Pfeife stopfen!", "tokens": ["Und", "sei\u00b7ne", "Pfei\u00b7fe", "stop\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Die Sonne scheint, und die Welt ist rund.", "tokens": ["Die", "Son\u00b7ne", "scheint", ",", "und", "die", "Welt", "ist", "rund", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Gr\u00fcn wehn die Cypressen.", "tokens": ["Gr\u00fcn", "wehn", "die", "Cyp\u00b7res\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.11": {"text": "Ein Schnabus l\u00e4sst sich trinken und", "tokens": ["Ein", "Schna\u00b7bus", "l\u00e4sst", "sich", "trin\u00b7ken", "und"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "VVINF", "KON"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Ein Rollmops essen.", "tokens": ["Ein", "Roll\u00b7mops", "es\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "