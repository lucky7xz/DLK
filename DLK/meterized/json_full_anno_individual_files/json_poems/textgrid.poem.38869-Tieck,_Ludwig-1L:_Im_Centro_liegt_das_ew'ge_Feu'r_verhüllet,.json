{"textgrid.poem.38869": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im Centro liegt das ew'ge Feu'r verh\u00fcllet,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.71", "nl:0.28"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Centro liegt das ew'ge Feu'r verh\u00fcllet,", "tokens": ["Im", "Cent\u00b7ro", "liegt", "das", "ew'\u00b7ge", "Feu'r", "ver\u00b7h\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem gro\u00dfen Vater ringt es stets entgegen", "tokens": ["Dem", "gro\u00b7\u00dfen", "Va\u00b7ter", "ringt", "es", "stets", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit s\u00fc\u00dfen sehnsuchtsvollen Pulsesschl\u00e4gen,", "tokens": ["Mit", "s\u00fc\u00b7\u00dfen", "sehn\u00b7suchts\u00b7vol\u00b7len", "Pul\u00b7ses\u00b7schl\u00e4\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df Baum und Blum' zum blauen Aether quillet.", "tokens": ["Da\u00df", "Baum", "und", "Blum'", "zum", "blau\u00b7en", "A\u00b7e\u00b7ther", "quil\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "KON", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Doch wird ihm oft nicht so die Brunst gestillet,", "tokens": ["Doch", "wird", "ihm", "oft", "nicht", "so", "die", "Brunst", "ge\u00b7stil\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PTKNEG", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dann mu\u00df dem wild zerst\u00f6r'nden Flammen-Seegen", "tokens": ["Dann", "mu\u00df", "dem", "wild", "zer\u00b7st\u00f6r'n\u00b7den", "Flam\u00b7men\u00b7See\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sich Blume, Flur und Waldberg seitw\u00e4rts legen,", "tokens": ["Sich", "Blu\u00b7me", ",", "Flur", "und", "Wald\u00b7berg", "seit\u00b7w\u00e4rts", "le\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "$,", "NN", "KON", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dann klopft der Erde Herz hoch lusterf\u00fcllet.", "tokens": ["Dann", "klopft", "der", "Er\u00b7de", "Herz", "hoch", "lus\u00b7ter\u00b7f\u00fcl\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "In's alte Chaos will die Welt zerrinnen,", "tokens": ["In's", "al\u00b7te", "Chaos", "will", "die", "Welt", "zer\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die heil'ge Furcht kann sie zur\u00fcck nur halten,", "tokens": ["Die", "heil'\u00b7ge", "Furcht", "kann", "sie", "zu\u00b7r\u00fcck", "nur", "hal\u00b7ten", ","], "token_in