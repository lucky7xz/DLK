{"textgrid.poem.43220": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was haben sie aus ihr gemacht,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was haben sie aus ihr gemacht,", "tokens": ["Was", "ha\u00b7ben", "sie", "aus", "ihr", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was aus mir?", "tokens": ["Was", "aus", "mir", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Sie hat so heiter einst gelacht,", "tokens": ["Sie", "hat", "so", "hei\u00b7ter", "einst", "ge\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich mit ihr!", "tokens": ["Ich", "mit", "ihr", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Ich schrieb so manches sch\u00f6ne Gedicht", "tokens": ["Ich", "schrieb", "so", "man\u00b7ches", "sch\u00f6\u00b7ne", "Ge\u00b7dicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "An das Kind,", "tokens": ["An", "das", "Kind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Und sah nichts, als ihr sch\u00f6nes Gesicht, \u2013", "tokens": ["Und", "sah", "nichts", ",", "als