{"textgrid.poem.49276": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "18. Auff den ersten Januarii (1625)", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Jahre pflegen zwar ihr rechtes Ziel zu finden", "tokens": ["Die", "Jah\u00b7re", "pfle\u00b7gen", "zwar", "ihr", "rech\u00b7tes", "Ziel", "zu", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und werden fortgef\u00fchrt als eine schnelle Fluth,", "tokens": ["Und", "wer\u00b7den", "fort\u00b7ge\u00b7f\u00fchrt", "als", "ei\u00b7ne", "schnel\u00b7le", "Fluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVPP", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die ehe fleucht als k\u00f6mpt; der Menschen rauer Muth", "tokens": ["Die", "e\u00b7he", "fleucht", "als", "k\u00f6mpt", ";", "der", "Men\u00b7schen", "rau\u00b7er", "Muth"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "VVFIN", "$.", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wird, ist und bleibt verstockt in mehr als tausent S\u00fcnden.", "tokens": ["Wird", ",", "ist", "und", "bleibt", "ver\u00b7stockt", "in", "mehr", "als", "tau\u00b7sent", "S\u00fcn\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "VAFIN", "KON", "VVFIN", "VVPP", "APPR", "PIAT", "KOKOM", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Geist wil offte zwar sich etwas unterwinden,", "tokens": ["Der", "Geist", "wil", "off\u00b7te", "zwar", "sich", "et\u00b7was", "un\u00b7ter\u00b7win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ADV", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dein Himmel zu zugehen; doch was er macht und thut", "tokens": ["Dein", "Him\u00b7mel", "zu", "zu\u00b7ge\u00b7hen", ";", "doch", "was", "er", "macht", "und", "thut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$.", "KON", "PWS", "PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Ist schwach und wird gehemmt durch unser Fleisch und Blut.", "tokens": ["Ist", "schwach", "und", "wird", "ge\u00b7hemmt", "durch", "un\u00b7ser", "Fleisch", "und", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "VAFIN", "VVPP", "APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der Geist von oben her mu\u00df einig uns entz\u00fcnden", "tokens": ["Der", "Geist", "von", "o\u00b7ben", "her", "mu\u00df", "ei\u00b7nig", "uns", "ent\u00b7z\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADV", "APZR", "VMFIN", "ADJD", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mit seiner starcken Brunst, mu\u00df d\u00e4mpffen unsern Wahn,", "tokens": ["Mit", "sei\u00b7ner", "star\u00b7cken", "Brunst", ",", "mu\u00df", "d\u00e4mpf\u00b7fen", "un\u00b7sern", "Wahn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "VMFIN", "VVINF", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der keine Fr\u00f6mmigkeit und Tugend fassen kan.", "tokens": ["Der", "kei\u00b7ne", "Fr\u00f6m\u00b7mig\u00b7keit", "und", "Tu\u00b7gend", "fas\u00b7sen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "KON", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "O Gott, nimb mit der Zeit des alten Jahres hin", "tokens": ["O", "Gott", ",", "nimb", "mit", "der", "Zeit", "des", "al\u00b7ten", "Jah\u00b7res", "hin"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "ADV", "APPR", "ART", "NN", "ART", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Mein alte grosse Schult; gib, da\u00df ich Neu und Schmertzen", "tokens": ["Mein", "al\u00b7te", "gros\u00b7se", "Schult", ";", "gib", ",", "da\u00df", "ich", "Neu", "und", "Schmert\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$.", "VVIMP", "$,", "KOUS", "PPER", "ADJD", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Hier \u00fcber tragen mag, und schicke meinem Hertzen", "tokens": ["Hier", "\u00fc\u00b7ber", "tra\u00b7gen", "mag", ",", "und", "schi\u00b7cke", "mei\u00b7nem", "Hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "VVINF", "VMFIN", "$,", "KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Mit diesem neuen Jahr auch einen neuen Sinn.", "tokens": ["Mit", "die\u00b7sem", "neu\u00b7en", "Jahr", "auch", "ei\u00b7nen", "neu\u00b7en", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Jahre pflegen zwar ihr rechtes Ziel zu finden", "tokens": ["Die", "Jah\u00b7re", "pfle\u00b7gen", "zwar", "ihr", "rech\u00b7tes", "Ziel", "zu", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und werden fortgef\u00fchrt als eine schnelle Fluth,", "tokens": ["Und", "wer\u00b7den", "fort\u00b7ge\u00b7f\u00fchrt", "als", "ei\u00b7ne", "schnel\u00b7le", "Fluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVPP", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die ehe fleucht als k\u00f6mpt; der Menschen rauer Muth", "tokens": ["Die", "e\u00b7he", "fleucht", "als", "k\u00f6mpt", ";", "der", "Men\u00b7schen", "rau\u00b7er", "Muth"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "VVFIN", "$.", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wird, ist und bleibt verstockt in mehr als tausent S\u00fcnden.", "tokens": ["Wird", ",", "ist", "und", "bleibt", "ver\u00b7stockt", "in", "mehr", "als", "tau\u00b7sent", "S\u00fcn\u00b7den", "."], "token_info": ["word", "punct", "word", "word