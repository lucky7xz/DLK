{"dta.poem.2594": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf Hn. B. H. d. R. in H. u. J. A. C S.  \n Hochzeit/ den 22. Aprill 1681.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr Nymfen an dem Zack/ ihr Berg-Einwohnerinnen/", "tokens": ["Ihr", "Nym\u00b7fen", "an", "dem", "Zack", "/", "ihr", "Ber\u00b7gEin\u00b7woh\u00b7ne\u00b7rin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$(", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit denen Echo oft bey stillen N\u00e4chten w\u00e4scht/", "tokens": ["Mit", "de\u00b7nen", "E\u00b7cho", "oft", "bey", "stil\u00b7len", "N\u00e4ch\u00b7ten", "w\u00e4scht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "NN", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und die ihr euren Durst am klaren Bober lescht/", "tokens": ["Und", "die", "ihr", "eu\u00b7ren", "Durst", "am", "kla\u00b7ren", "Bo\u00b7ber", "lescht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ermuntert Hertz und Geist was hohes zubeginnen.", "tokens": ["Er\u00b7mun\u00b7tert", "Hertz", "und", "Geist", "was", "ho\u00b7hes", "zu\u00b7be\u00b7gin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "PWS", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Werfft nur den Fichten/ Krantz von dem beschneyten Haar/", "tokens": ["Werfft", "nur", "den", "Fich\u00b7ten", "/", "Krantz", "von", "dem", "be\u00b7schney\u00b7ten", "Haar", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$(", "NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.6": {"text": "Der Winter ist hinweg/ der Fr\u00fchling mahlt das Jahr.", "tokens": ["Der", "Win\u00b7ter", "ist", "hin\u00b7weg", "/", "der", "Fr\u00fch\u00b7ling", "mahlt", "das", "Jahr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$(", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wie seyd ihr denn so wild als eures Zackes-Fluthen?", "tokens": ["Wie", "seyd", "ihr", "denn", "so", "wild", "als", "eu\u00b7res", "Za\u00b7ckes\u00b7Flu\u00b7then", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "KOKOM", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Was traget ihr f\u00fcr Scheu au\u00df Wald und Pusch zugehn?", "tokens": ["Was", "tra\u00b7get", "ihr", "f\u00fcr", "Scheu", "au\u00df", "Wald", "und", "Pusch", "zu\u00b7gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Verlast/ Armseligste/ der Felsen g\u00e4he H\u00f6h\u2019n/", "tokens": ["Ver\u00b7last", "/", "Arm\u00b7se\u00b7ligs\u00b7te", "/", "der", "Fel\u00b7sen", "g\u00e4\u00b7he", "H\u00f6h'n", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "ART", "NN", "ADJA", "NN", "$("], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Der Tag ist euch geweyht und gehet euch zum guten.", "tokens": ["Der", "Tag", "ist", "euch", "ge\u00b7weyht", "und", "ge\u00b7het", "euch", "zum", "gu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "KON", "VVFIN", "PPER", "APPRART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Last s\u00fcsses Seitenspiel erklingen durch die Luft/", "tokens": ["Last", "s\u00fcs\u00b7ses", "Sei\u00b7ten\u00b7spiel", "er\u00b7klin\u00b7gen", "durch", "die", "Luft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.12": {"text": "Nun euch die Nachtigall mit ihrer Stimme rufft.", "tokens": ["Nun", "euch", "die", "Nach\u00b7ti\u00b7gall", "mit", "ih\u00b7rer", "Stim\u00b7me", "rufft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Was aber spiel ich viel mit dunckler W\u00f6rter Decke?", "tokens": ["Was", "a\u00b7ber", "spiel", "ich", "viel", "mit", "dunck\u00b7ler", "W\u00f6r\u00b7ter", "De\u00b7cke", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Auf Nymfen kleidet euch in Seiden-Zarten Schleyr/", "tokens": ["Auf", "Nym\u00b7fen", "klei\u00b7det", "euch", "in", "Sei\u00b7den\u00b7Zar\u00b7ten", "Schleyr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "APPR", "NE", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Herr H\u00fcbner/ den ihr kennt/ begeht die Hochzeit-Feyr/", "tokens": ["Herr", "H\u00fcb\u00b7ner", "/", "den", "ihr", "kennt", "/", "be\u00b7geht", "die", "Hoch\u00b7zeit\u00b7Feyr", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$(", "ART", "PPER", "VVFIN", "$(", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Da\u00df eurem Vorwitz ich ja weiter nichts verstecke/", "tokens": ["Da\u00df", "eu\u00b7rem", "Vor\u00b7witz", "ich", "ja", "wei\u00b7ter", "nichts", "ver\u00b7ste\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ADV", "ADV", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "De\u00df Sommers Auszug k\u00f6mmt und stellt sich bey euch ein/", "tokens": ["De\u00df", "Som\u00b7mers", "Aus\u00b7zug", "k\u00f6mmt", "und", "stellt", "sich", "bey", "euch", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "KON", "VVFIN", "PRF", "APPR", "PPER", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die sch\u00f6ne ", "tokens": ["Die", "sch\u00f6\u00b7ne"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.19": {"text": "Heist diese G\u00e4stin doch viel tausendmahl willkommen/", "tokens": ["Heist", "die\u00b7se", "G\u00e4s\u00b7tin", "doch", "viel", "tau\u00b7send\u00b7mahl", "will\u00b7kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "ADV", "ADV", "ADV", "ADJD", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.20": {"text": "Denn eure Gegend wird durch ihre Blicke klar/", "tokens": ["Denn", "eu\u00b7re", "Ge\u00b7gend", "wird", "durch", "ih\u00b7re", "Bli\u00b7cke", "klar", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Eilt/ sucht Violen auf/ zu zieren Haupt und Haar/", "tokens": ["Eilt", "/", "sucht", "Vi\u00b7o\u00b7len", "auf", "/", "zu", "zie\u00b7ren", "Haupt", "und", "Haar", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "VVFIN", "NN", "APPR", "$(", "PTKZU", "VVFIN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Denn Blumen haben selbst von ihr den Glantz genommen.", "tokens": ["Denn", "Blu\u00b7men", "ha\u00b7ben", "selbst", "von", "ihr", "den", "Glantz", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ADV", "APPR", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Seht wie der rothe Mund besch\u00e4mt der Nelcken-Pracht/", "tokens": ["Seht", "wie", "der", "ro\u00b7the", "Mund", "be\u00b7sch\u00e4mt", "der", "Nel\u00b7cken\u00b7Pracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und wie sich jedes Glied zu Lust-Narcissen macht.", "tokens": ["Und", "wie", "sich", "je\u00b7des", "Glied", "zu", "Lust\u00b7\u00b7N\u00b7a\u00b7rcis\u00b7sen", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PRF", "PIAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.25": {"text": "Sucht Nymfen/ sucht hervor was von Ersinnligkeiten/", "tokens": ["Sucht", "Nym\u00b7fen", "/", "sucht", "her\u00b7vor", "was", "von", "Er\u00b7sinn\u00b7lig\u00b7kei\u00b7ten", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$(", "VVFIN", "PTKVZ", "PWS", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Die Liebe finden mag/ wenn sie sich dienstbar weist/", "tokens": ["Die", "Lie\u00b7be", "fin\u00b7den", "mag", "/", "wenn", "sie", "sich", "dienst\u00b7bar", "weist", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$(", "KOUS", "PPER", "PRF", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und ob schon eure Brust vor diesem stund beeist/", "tokens": ["Und", "ob", "schon", "eu\u00b7re", "Brust", "vor", "die\u00b7sem", "stund", "be\u00b7eist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "PPOSAT", "NN", "APPR", "PDAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "So wird der Lentz bey euch itzt seine Sch\u00e4tz au\u00dfbreiten.", "tokens": ["So", "wird", "der", "Lentz", "bey", "euch", "itzt", "sei\u00b7ne", "Sch\u00e4tz", "au\u00df\u00b7brei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "PPER", "ADV", "PPOSAT", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Seht wie das nahe Thal mit Blumen ist besternt/", "tokens": ["Seht", "wie", "das", "na\u00b7he", "Thal", "mit", "Blu\u00b7men", "ist", "bes\u00b7ternt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "ADJA", "NN", "APPR", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und wie des Zackes Mund schwatzhafftig buhlen lernt.", "tokens": ["Und", "wie", "des", "Za\u00b7ckes", "Mund", "schwatz\u00b7haff\u00b7tig", "buh\u00b7len", "lernt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "NN", "ADJD", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ehrt diese Freundin doch als eurer Freuden Sonne/", "tokens": ["Ehrt", "die\u00b7se", "Freun\u00b7din", "doch", "als", "eu\u00b7rer", "Freu\u00b7den", "Son\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "ADV", "KOUS", "PPOSAT", "NN", "NN", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.32": {"text": "Der reichen Berge Scho\u00df tr\u00e4gt allen Uberflu\u00df/", "tokens": ["Der", "rei\u00b7chen", "Ber\u00b7ge", "Scho\u00df", "tr\u00e4gt", "al\u00b7len", "U\u00b7ber\u00b7flu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Und weil der Liebes-GOtt zur Tafel dienen mu\u00df/", "tokens": ["Und", "weil", "der", "Lie\u00b7bes\u00b7GOtt", "zur", "Ta\u00b7fel", "die\u00b7nen", "mu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPRART", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Dem Schmertzen eine Lust/ und Schmausen eine Wonne/", "tokens": ["Dem", "Schmert\u00b7zen", "ei\u00b7ne", "Lust", "/", "und", "Schmau\u00b7sen", "ei\u00b7ne", "Won\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$(", "KON", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "So wird er/ wie er pflegt/ seyn listig abgericht/", "tokens": ["So", "wird", "er", "/", "wie", "er", "pflegt", "/", "seyn", "lis\u00b7tig", "ab\u00b7ge\u00b7richt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$(", "PWAV", "PPER", "VVFIN", "$(", "PPOSAT", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Da\u00df jedem treflich wol und auch genug geschicht.", "tokens": ["Da\u00df", "je\u00b7dem", "tref\u00b7lich", "wol", "und", "auch", "ge\u00b7nug", "ge\u00b7schicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "ADV", "KON", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Zu dem so sind ber\u00fchmt ", "tokens": ["Zu", "dem", "so", "sind", "be\u00b7r\u00fchmt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "VAFIN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.38": {"text": "Wie Gastfrey jederzeit er Freunden sich geweist/", "tokens": ["Wie", "Gast\u00b7frey", "je\u00b7der\u00b7zeit", "er", "Freun\u00b7den", "sich", "ge\u00b7weist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "PPER", "NN", "PRF", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Und wenn ein Frembder nur in sein Haus eingereist/", "tokens": ["Und", "wenn", "ein", "Fremb\u00b7der", "nur", "in", "sein", "Haus", "ein\u00b7ge\u00b7reist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-++--+", "measure": "iambic.hexa.chol"}, "line.40": {"text": "Mit was vor H\u00f6ffligkeit pflag er ihn zubegleiten/", "tokens": ["Mit", "was", "vor", "H\u00f6ff\u00b7lig\u00b7keit", "pflag", "er", "ihn", "zu\u00b7be\u00b7glei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "APPR", "NN", "VVFIN", "PPER", "PPER", "VVINF", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.41": {"text": "So da\u00df der G\u00f6tter Mahl f\u00fcr dem man nichtig sch\u00e4tzt/", "tokens": ["So", "da\u00df", "der", "G\u00f6t\u00b7ter", "Mahl", "f\u00fcr", "dem", "man", "nich\u00b7tig", "sch\u00e4tzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "NN", "APPR", "PRELS", "PIS", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Wo sich die Redligkeit und Treu zu Tische setzt.", "tokens": ["Wo", "sich", "die", "Red\u00b7lig\u00b7keit", "und", "Treu", "zu", "Ti\u00b7sche", "setzt", "."], "token_info": ["word", "word", "