{"dta.poem.16328": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "64.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was du nie m\u00fcde wirst zu f\u00fchlen, wirst du nie", "tokens": ["Was", "du", "nie", "m\u00fc\u00b7de", "wirst", "zu", "f\u00fch\u00b7len", ",", "wirst", "du", "nie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "PPER", "ADV", "ADJD", "VAFIN", "PTKZU", "VVINF", "$,", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu sagen m\u00fcde, doch zu h\u00f6ren werden sie,", "tokens": ["Zu", "sa\u00b7gen", "m\u00fc\u00b7de", ",", "doch", "zu", "h\u00f6\u00b7ren", "wer\u00b7den", "sie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VMFIN", "$,", "ADV", "PTKZU", "VVINF", "VAFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die ausgenommen die wie du desgleichen f\u00fchlen,", "tokens": ["Die", "aus\u00b7ge\u00b7nom\u00b7men", "die", "wie", "du", "des\u00b7glei\u00b7chen", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "PWAV", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht die aus Langerweil' in Neuigkeiten w\u00fchlen.", "tokens": ["Nicht", "die", "aus", "Lan\u00b7ger\u00b7weil'", "in", "Neu\u00b7ig\u00b7kei\u00b7ten", "w\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "APPR", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was dir am Herzen liegt, das sagst du nie genug,", "tokens": ["Was", "dir", "am", "Her\u00b7zen", "liegt", ",", "das", "sagst", "du", "nie", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "VVFIN", "$,", "PDS", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und unerm\u00fcdlich ist der Bauer hinterm Pflug.", "tokens": ["Und", "un\u00b7er\u00b7m\u00fcd\u00b7lich", "ist", "der", "Bau\u00b7er", "hin\u00b7term", "Pflug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "So unerm\u00fcdlich bin ich meinen Pflug zu treiben,", "tokens": ["So", "un\u00b7er\u00b7m\u00fcd\u00b7lich", "bin", "ich", "mei\u00b7nen", "Pflug", "zu", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und euch mein bestes Korn ins Herz zu s\u00e4n durch Schreiben.", "tokens": ["Und", "euch", "mein", "bes\u00b7tes", "Korn", "ins", "Herz", "zu", "s\u00e4n", "durch", "Schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "APPR", "NE", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}