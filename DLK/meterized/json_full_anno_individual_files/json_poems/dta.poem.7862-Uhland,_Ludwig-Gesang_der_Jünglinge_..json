{"dta.poem.7862": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Gesang der J\u00fcnglinge .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Heilig ist die Jugendzeit!", "tokens": ["Hei\u00b7lig", "ist", "die", "Ju\u00b7gend\u00b7zeit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Treten wir in Tempelhallen,", "tokens": ["Tre\u00b7ten", "wir", "in", "Tem\u00b7pel\u00b7hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo in d\u00fcstrer Einsamkeit", "tokens": ["Wo", "in", "d\u00fcst\u00b7rer", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dumpf die Tritte widerschallen!", "tokens": ["Dumpf", "die", "Trit\u00b7te", "wi\u00b7der\u00b7schal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Edler Geist des Ernstes soll", "tokens": ["Ed\u00b7ler", "Geist", "des", "Erns\u00b7tes", "soll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sich in J\u00fcnglingsseelen senken,", "tokens": ["Sich", "in", "J\u00fcng\u00b7lings\u00b7see\u00b7len", "sen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Jede still und andachtsvoll", "tokens": ["Je\u00b7de", "still", "und", "an\u00b7dachts\u00b7voll"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ihrer heil\u2019gen Kraft gedenken.", "tokens": ["Ih\u00b7rer", "heil'\u00b7gen", "Kraft", "ge\u00b7den\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Gehn wir in\u2019s Gefild hervor,", "tokens": ["Gehn", "wir", "in's", "Ge\u00b7fild", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Das sich stolz dem Himmel zeiget,", "tokens": ["Das", "sich", "stolz", "dem", "Him\u00b7mel", "zei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der so feierlich empor", "tokens": ["Der", "so", "fei\u00b7er\u00b7lich", "em\u00b7por"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJD", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ueber\u2019m Erdenfr\u00fchling steiget!", "tokens": ["Ue\u00b7ber'm", "Er\u00b7den\u00b7fr\u00fch\u00b7ling", "stei\u00b7get", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Eine Welt von Fruchtbarkeit", "tokens": ["Ei\u00b7ne", "Welt", "von", "Frucht\u00b7bar\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird aus dieser Bl\u00fcthe brechen.", "tokens": ["Wird", "aus", "die\u00b7ser", "Bl\u00fc\u00b7the", "bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Heilig ist die Fr\u00fchlingszeit,", "tokens": ["Hei\u00b7lig", "ist", "die", "Fr\u00fch\u00b7lings\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Soll an J\u00fcnglingsseelen sprechen!", "tokens": ["Soll", "an", "J\u00fcng\u00b7lings\u00b7see\u00b7len", "spre\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Fasset die Pokale nur!", "tokens": ["Fas\u00b7set", "die", "Po\u00b7ka\u00b7le", "nur", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Seht ihr nicht so purpurn blinken", "tokens": ["Seht", "ihr", "nicht", "so", "pur\u00b7purn", "blin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Blut der \u00fcppigen Natur?", "tokens": ["Blut", "der", "\u00fcp\u00b7pi\u00b7gen", "Na\u00b7tur", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00dft uns hohen Muthes trinken!", "tokens": ["La\u00dft", "uns", "ho\u00b7hen", "Mu\u00b7thes", "trin\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df sich eine Feuerkraft", "tokens": ["Da\u00df", "sich", "ei\u00b7ne", "Feu\u00b7er\u00b7kraft"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Selig in der andern f\u00fchle.", "tokens": ["Se\u00b7lig", "in", "der", "an\u00b7dern", "f\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Heilig ist der Rebensaft,", "tokens": ["Hei\u00b7lig", "ist", "der", "Re\u00b7ben\u00b7saft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ist des Jugendschwungs Gespiele.", "tokens": ["Ist", "des", "Ju\u00b7gend\u00b7schwungs", "Ge\u00b7spie\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Seht das holde M\u00e4dchen hier!", "tokens": ["Seht", "das", "hol\u00b7de", "M\u00e4d\u00b7chen", "hier", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie entfaltet sich im Spiele;", "tokens": ["Sie", "ent\u00b7fal\u00b7tet", "sich", "im", "Spie\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eine Welt erbl\u00fcht in ihr", "tokens": ["Ei\u00b7ne", "Welt", "er\u00b7bl\u00fcht", "in", "ihr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zarter, himmlischer Gef\u00fchle.", "tokens": ["Zar\u00b7ter", ",", "himm\u00b7li\u00b7scher", "Ge\u00b7f\u00fch\u00b7le", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sie gedeiht im Sonnenschein,", "tokens": ["Sie", "ge\u00b7deiht", "im", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Unsre Kraft in Sturm und Regen.", "tokens": ["Uns\u00b7re", "Kraft", "in", "Sturm", "und", "Re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Heilig soll das M\u00e4dchen seyn,", "tokens": ["Hei\u00b7lig", "soll", "das", "M\u00e4d\u00b7chen", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "VMFIN", "ART", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Denn wir reifen uns entgegen!", "tokens": ["Denn", "wir", "rei\u00b7fen", "uns", "ent\u00b7ge\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Darum geht in Tempel ein,", "tokens": ["Da\u00b7rum", "geht", "in", "Tem\u00b7pel", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "APPR", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Edeln Ernst in euch zu saugen!", "tokens": ["E\u00b7deln", "Ernst", "in", "euch", "zu", "sau\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "St\u00e4rkt an Fr\u00fchling euch und Wein,", "tokens": ["St\u00e4rkt", "an", "Fr\u00fch\u00b7ling", "euch", "und", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PPER", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sonnet euch an sch\u00f6nen Augen!", "tokens": ["Son\u00b7net", "euch", "an", "sch\u00f6\u00b7nen", "Au\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Jugend, Fr\u00fchling, Festpokal,", "tokens": ["Ju\u00b7gend", ",", "Fr\u00fch\u00b7ling", ",", "Fest\u00b7po\u00b7kal", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "M\u00e4dchen in der holden Bl\u00fcthe,", "tokens": ["M\u00e4d\u00b7chen", "in", "der", "hol\u00b7den", "Bl\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Heilig seyn sie allzumal", "tokens": ["Hei\u00b7lig", "seyn", "sie", "all\u00b7zu\u00b7mal"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VAINF", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Unsrem ernsteren Gem\u00fcthe!", "tokens": ["Uns\u00b7rem", "erns\u00b7te\u00b7ren", "Ge\u00b7m\u00fc\u00b7the", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}