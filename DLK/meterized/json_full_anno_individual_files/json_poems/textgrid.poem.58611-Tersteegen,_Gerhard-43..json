{"textgrid.poem.58611": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "43.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O selig, wer sich Gott in keuscher Lieb' verm\u00e4hlet,", "tokens": ["O", "se\u00b7lig", ",", "wer", "sich", "Gott", "in", "keu\u00b7scher", "Lieb'", "ver\u00b7m\u00e4h\u00b7let", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "PWS", "PRF", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der ganz des Herren ist, der ihn nach Wunsch regiert!", "tokens": ["Der", "ganz", "des", "Her\u00b7ren", "ist", ",", "der", "ihn", "nach", "Wunsch", "re\u00b7giert", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VAFIN", "$,", "PRELS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dann hadre, wer da will, kein Feind ihn st\u00f6rt, noch qu\u00e4let;", "tokens": ["Dann", "had\u00b7re", ",", "wer", "da", "will", ",", "kein", "Feind", "ihn", "st\u00f6rt", ",", "noch", "qu\u00e4\u00b7let", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWS", "ADV", "VMFIN", "$,", "PIAT", "NN", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der f\u00fcr ihn spricht, ist nah, der Herr ihn defendiert.", "tokens": ["Der", "f\u00fcr", "ihn", "spricht", ",", "ist", "nah", ",", "der", "Herr", "ihn", "de\u00b7fen\u00b7diert", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVFIN", "$,", "VAFIN", "ADJD", "$,", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Wer hat denn Recht zu mir? Dem Herren bin ich eigen,", "tokens": ["Wer", "hat", "denn", "Recht", "zu", "mir", "?", "Dem", "Her\u00b7ren", "bin", "ich", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "NN", "APPR", "PPER", "$.", "ART", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mein Herz geh\u00f6rt nur ihm und keinem andern zu.", "tokens": ["Mein", "Herz", "ge\u00b7h\u00f6rt", "nur", "ihm", "und", "kei\u00b7nem", "an\u00b7dern", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "PPER", "KON", "PIAT", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Gott hilft; versuchet nur, ihr werdet mich nicht beugen!", "tokens": ["Gott", "hilft", ";", "ver\u00b7su\u00b7chet", "nur", ",", "ihr", "wer\u00b7det", "mich", "nicht", "beu\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "VVFIN", "ADV", "$,", "PPER", "VAFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Gott hilft; verdammet nur, ich bleibe doch in Ruh!", "tokens": ["Gott", "hilft", ";", "ver\u00b7dam\u00b7met", "nur", ",", "ich", "blei\u00b7be", "doch", "in", "Ruh", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["N