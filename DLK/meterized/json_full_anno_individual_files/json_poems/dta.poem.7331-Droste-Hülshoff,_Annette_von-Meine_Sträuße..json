{"dta.poem.7331": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Meine Str\u00e4u\u00dfe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "So oft mir ward eine liebe Stund'", "tokens": ["So", "oft", "mir", "ward", "ei\u00b7ne", "lie\u00b7be", "Stund'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Unterm blauen Himmel im Freien,", "tokens": ["Un\u00b7term", "blau\u00b7en", "Him\u00b7mel", "im", "Frei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Da habe ich, zu des Gedenkens Bund,", "tokens": ["Da", "ha\u00b7be", "ich", ",", "zu", "des", "Ge\u00b7den\u00b7kens", "Bund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mir Zeichen geflochten mit Treuen,", "tokens": ["Mir", "Zei\u00b7chen", "ge\u00b7floch\u00b7ten", "mit", "Treu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVPP", "APPR", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Einen schlichten Kranz, einen wilden Strau\u00df,", "tokens": ["Ei\u00b7nen", "schlich\u00b7ten", "Kranz", ",", "ei\u00b7nen", "wil\u00b7den", "Strau\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Lie\u00df dr\u00fcber die Seele wallen;", "tokens": ["Lie\u00df", "dr\u00fc\u00b7ber", "die", "See\u00b7le", "wal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Nun stehe ich einsam im stillen Haus,", "tokens": ["Nun", "ste\u00b7he", "ich", "ein\u00b7sam", "im", "stil\u00b7len", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Und sehe die Bl\u00e4tter zerfallen.", "tokens": ["Und", "se\u00b7he", "die", "Bl\u00e4t\u00b7ter", "zer\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Vergi\u00dfmeinnicht mit dem Rosaband \u2014", "tokens": ["Ver\u00b7gi\u00df\u00b7mein\u00b7nicht", "mit", "dem", "Ros\u00b7a\u00b7band"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das waren d\u00e4mmrige Tage,", "tokens": ["Das", "wa\u00b7ren", "d\u00e4mm\u00b7ri\u00b7ge", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Als euch entwandte der Freundin Hand", "tokens": ["Als", "euch", "ent\u00b7wand\u00b7te", "der", "Freun\u00b7din", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Dem Weiher dr\u00fcben am Haage;", "tokens": ["Dem", "Wei\u00b7her", "dr\u00fc\u00b7ben", "am", "Haa\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wir schw\u00e4rmten in wirrer Gef\u00fchle Flut,", "tokens": ["Wir", "schw\u00e4rm\u00b7ten", "in", "wir\u00b7rer", "Ge\u00b7f\u00fch\u00b7le", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In sechzehnj\u00e4hrigen Schmerzen;", "tokens": ["In", "sech\u00b7zehn\u00b7j\u00e4h\u00b7ri\u00b7gen", "Schmer\u00b7zen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Nun schl\u00e4ft sie lange. \u2014 Sie war doch gut,", "tokens": ["Nun", "schl\u00e4ft", "sie", "lan\u00b7ge", ".", "Sie", "war", "doch", "gut", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$.", "$(", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Ich liebte sie recht von Herzen!", "tokens": ["Ich", "lieb\u00b7te", "sie", "recht", "von", "Her\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Gar weite Wege hast du gemacht,", "tokens": ["Gar", "wei\u00b7te", "We\u00b7ge", "hast", "du", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Camelia, staubige Sch\u00f6ne,", "tokens": ["Ca\u00b7me\u00b7lia", ",", "stau\u00b7bi\u00b7ge", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "In deinem Kelche die Fl\u00f6te wacht,", "tokens": ["In", "dei\u00b7nem", "Kel\u00b7che", "die", "Fl\u00f6\u00b7te", "wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Trompeten und Cymbelget\u00f6ne;", "tokens": ["Trom\u00b7pe\u00b7ten", "und", "Cym\u00b7bel\u00b7ge\u00b7t\u00f6\u00b7ne", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Wie zitterten durch das gr\u00fcne Revier", "tokens": ["Wie", "zit\u00b7ter\u00b7ten", "durch", "das", "gr\u00fc\u00b7ne", "Re\u00b7vier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+---", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Buntfarbige Lampen und Schleyer!", "tokens": ["Bunt\u00b7far\u00b7bi\u00b7ge", "Lam\u00b7pen", "und", "Schle\u00b7yer", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Da brach der zierliche G\u00e4rtner mir", "tokens": ["Da", "brach", "der", "zier\u00b7li\u00b7che", "G\u00e4rt\u00b7ner", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PPER"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Den Strau\u00df beim bengalischen Feuer.", "tokens": ["Den", "Strau\u00df", "beim", "ben\u00b7ga\u00b7li\u00b7schen", "Feu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Dies Alpenr\u00f6schen n\u00e4hrte mit Schnee", "tokens": ["Dies", "Al\u00b7pen\u00b7r\u00f6\u00b7schen", "n\u00e4hr\u00b7te", "mit", "Schnee"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ein eisgrau starrender Riese;", "tokens": ["Ein", "eis\u00b7grau", "star\u00b7ren\u00b7der", "Rie\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und diese Tange entfischt' ich der See", "tokens": ["Und", "die\u00b7se", "Tan\u00b7ge", "ent\u00b7fischt'", "ich", "der", "See"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Aus Muschelgescherbe und Kiese;", "tokens": ["Aus", "Mu\u00b7schel\u00b7ge\u00b7scher\u00b7be", "und", "Kie\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Es war ein volles, gesegnetes Jahr,", "tokens": ["Es", "war", "ein", "vol\u00b7les", ",", "ge\u00b7se\u00b7gne\u00b7tes", "Jahr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die Trauben hiengen gleich Pfunden,", "tokens": ["Die", "Trau\u00b7ben", "hien\u00b7gen", "gleich", "Pfun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Als aus der Rebe flatterndem Haar", "tokens": ["Als", "aus", "der", "Re\u00b7be", "flat\u00b7tern\u00b7dem", "Haar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Ich diesen Kranz mir gewunden.", "tokens": ["Ich", "die\u00b7sen", "Kranz", "mir", "ge\u00b7wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PDAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Und ihr, meine Str\u00e4u\u00dfe von wildem Haid',", "tokens": ["Und", "ihr", ",", "mei\u00b7ne", "Str\u00e4u\u00b7\u00dfe", "von", "wil\u00b7dem", "Haid'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit lockerm Halme geschlungen,", "tokens": ["Mit", "lo\u00b7ckerm", "Hal\u00b7me", "ge\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "O s\u00fc\u00dfe Sonne, o Einsamkeit,", "tokens": ["O", "s\u00fc\u00b7\u00dfe", "Son\u00b7ne", ",", "o", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die uns redet mit heimischen Zungen!", "tokens": ["Die", "uns", "re\u00b7det", "mit", "hei\u00b7mi\u00b7schen", "Zun\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Ich hab' sie gepfl\u00fcckt an Tagen so lind,", "tokens": ["Ich", "hab'", "sie", "ge\u00b7pfl\u00fcckt", "an", "Ta\u00b7gen", "so", "lind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "APPR", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wenn die goldenen K\u00e4ferchen spielen,", "tokens": ["Wenn", "die", "gol\u00b7de\u00b7nen", "K\u00e4\u00b7fer\u00b7chen", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.7": {"text": "Dann f\u00fchlte ich mich meines Landes Kind,", "tokens": ["Dann", "f\u00fchl\u00b7te", "ich", "mich", "mei\u00b7nes", "Lan\u00b7des", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und die fremden Schlacken zerfielen.", "tokens": ["Und", "die", "frem\u00b7den", "Schla\u00b7cken", "zer\u00b7fie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Und wenn ich gr\u00fcble an meinem Teich,", "tokens": ["Und", "wenn", "ich", "gr\u00fcb\u00b7le", "an", "mei\u00b7nem", "Teich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Im duftigen Moose gestrecket,", "tokens": ["Im", "duf\u00b7ti\u00b7gen", "Moo\u00b7se", "ge\u00b7stre\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Wenn aus dem Spiegel mein Antlitz bleich", "tokens": ["Wenn", "aus", "dem", "Spie\u00b7gel", "mein", "Ant\u00b7litz", "bleich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "PPOSAT", "NN", "ADJD"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Mit rieselndem Schauer mich necket,", "tokens": ["Mit", "rie\u00b7seln\u00b7dem", "Schau\u00b7er", "mich", "ne\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Dann lang' ich sachte, sachte hinab,", "tokens": ["Dann", "lang'", "ich", "sach\u00b7te", ",", "sach\u00b7te", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Und fische die tr\u00e4ufelnden Schmehlen;", "tokens": ["Und", "fi\u00b7sche", "die", "tr\u00e4u\u00b7feln\u00b7den", "Schmeh\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Dort h\u00e4ngen sie, dr\u00fcben am Fensterstab,", "tokens": ["Dort", "h\u00e4n\u00b7gen", "sie", ",", "dr\u00fc\u00b7ben", "am", "Fens\u00b7ter\u00b7stab", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Wie arme vertrocknete Seelen.", "tokens": ["Wie", "ar\u00b7me", "ver\u00b7trock\u00b7ne\u00b7te", "See\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.7": {"line.1": {"text": "So mochte ich still und heimlich mir", "tokens": ["So", "moch\u00b7te", "ich", "still", "und", "heim\u00b7lich", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "PPER"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Eine Zauberhalle bereiten,", "tokens": ["Ei\u00b7ne", "Zau\u00b7ber\u00b7hal\u00b7le", "be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Wenn es d\u00e4mmert dort, und dr\u00fcben, und hier,", "tokens": ["Wenn", "es", "d\u00e4m\u00b7mert", "dort", ",", "und", "dr\u00fc\u00b7ben", ",", "und", "hier", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADV", "$,", "KON", "ADV", "$,", "KON", "ADV", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Von den W\u00e4nden seh ich es gleiten;", "tokens": ["Von", "den", "W\u00e4n\u00b7den", "seh", "ich", "es", "glei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PPER", "VVINF", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Eine Fey entschleicht der Camelia sich,", "tokens": ["Ei\u00b7ne", "Fey", "ent\u00b7schleicht", "der", "Ca\u00b7me\u00b7lia", "sich", ","], "token_info": [