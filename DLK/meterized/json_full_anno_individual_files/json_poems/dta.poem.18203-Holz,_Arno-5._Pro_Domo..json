{"dta.poem.18203": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "5.  \n  Pro Domo.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eo wirf ihn fort den Hungerknochen!", "tokens": ["\u201e", "o", "wirf", "ihn", "fort", "den", "Hun\u00b7ger\u00b7kno\u00b7chen", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "VVFIN", "PPER", "PTKVZ", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es hat die leidge Politik", "tokens": ["Es", "hat", "die", "leid\u00b7ge", "Po\u00b7li\u00b7tik"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schon Manchem hier den Hals gebrochen.", "tokens": ["Schon", "Man\u00b7chem", "hier", "den", "Hals", "ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Auch ", "tokens": ["Auch"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Doch wozu ihn versificiren?", "tokens": ["Doch", "wo\u00b7zu", "ihn", "ver\u00b7si\u00b7fi\u00b7ci\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Welt ist heute prosatoll", "tokens": ["Die", "Welt", "ist", "heu\u00b7te", "pro\u00b7sa\u00b7toll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wird ihn schwerlich honoriren.", "tokens": ["Und", "wird", "ihn", "schwer\u00b7lich", "ho\u00b7no\u00b7ri\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Such lieber hohe Protegees,", "tokens": ["Such", "lie\u00b7ber", "ho\u00b7he", "Pro\u00b7te\u00b7gees", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Sozialismus ist uns schnuppe,", "tokens": ["Dein", "So\u00b7zi\u00b7a\u00b7lis\u00b7mus", "ist", "uns", "schnup\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Denn schlie\u00dflich w\u00e4rmst du nur, gesteh's,", "tokens": ["Denn", "schlie\u00df\u00b7lich", "w\u00e4rmst", "du", "nur", ",", "ge\u00b7steh's", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "$,", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die achtundvierzger Bettelsupe.", "tokens": ["Die", "ach\u00b7tund\u00b7vierz\u00b7ger", "Bet\u00b7tel\u00b7su\u00b7pe", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Drum still, du Sturm im Wasserglas,", "tokens": ["Drum", "still", ",", "du", "Sturm", "im", "Was\u00b7ser\u00b7glas", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PTKVZ", "$,", "PPER", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und reime fortab nur auf \u201eTriebe\u201c \u2014", "tokens": ["Und", "rei\u00b7me", "for\u00b7tab", "nur", "auf", "\u201e", "Trie\u00b7be", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "APPR", "$(", "NN", "$(", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du s\u00e4st wie Lucifer nur Ha\u00df,", "tokens": ["Du", "s\u00e4st", "wie", "Lu\u00b7ci\u00b7fer", "nur", "Ha\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "NE", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Herz der Kunst hei\u00dft aber Liebe!\u201c", "tokens": ["Das", "Herz", "der", "Kunst", "hei\u00dft", "a\u00b7ber", "Lie\u00b7be", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ADV", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich h\u00f6r's und fluche: Sapprement!", "tokens": ["Ich", "h\u00f6r's", "und", "flu\u00b7che", ":", "Sap\u00b7pre\u00b7ment", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$.", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zwar lieblich locken die Moneten,", "tokens": ["Zwar", "lieb\u00b7lich", "lo\u00b7cken", "die", "Mo\u00b7ne\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Doch fehlt mir leider das Talent", "tokens": ["Doch", "fehlt", "mir", "lei\u00b7der", "das", "Ta\u00b7lent"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Zum schwarzwei\u00dfrothen Hofpoeten.", "tokens": ["Zum", "schwarz\u00b7wei\u00df\u00b7ro\u00b7then", "Hof\u00b7po\u00b7e\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Drum bitte, mir drei Schritt vom Leib", "tokens": ["Drum", "bit\u00b7te", ",", "mir", "drei", "Schritt", "vom", "Leib"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "PTKANT", "$,", "PPER", "CARD", "NN", "APPRART", "NN"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit euern Tombackpoesieen", "tokens": ["Mit", "eu\u00b7ern", "Tom\u00b7back\u00b7po\u00b7e\u00b7si\u00b7e\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und zischt nicht wie ein feiles Weib:", "tokens": ["Und", "zischt", "nicht", "wie", "ein", "fei\u00b7les", "Weib", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Tritt ein in unsre Koterieen!", "tokens": ["Tritt", "ein", "in", "uns\u00b7re", "Ko\u00b7te\u00b7ri\u00b7e\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Th\u00e4t ich's, ich w\u00e4r ein Halb-Poet,", "tokens": ["Th\u00e4t", "ich's", ",", "ich", "w\u00e4r", "ein", "Halb\u00b7Po\u00b7et", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So aber ruf ich durch die Gassen:", "tokens": ["So", "a\u00b7ber", "ruf", "ich", "durch", "die", "Gas\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}