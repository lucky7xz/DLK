{"dta.poem.4220": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Sonne.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Alles, was sich regt und lebet, wird von einem Geist", "tokens": ["Al\u00b7les", ",", "was", "sich", "regt", "und", "le\u00b7bet", ",", "wird", "von", "ei\u00b7nem", "Geist"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "$,", "PRELS", "PRF", "VVFIN", "KON", "VVFIN", "$,", "VAFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "regiert,", "tokens": ["re\u00b7giert", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Welcher der Bewegung Ursprung. Wann nun der Pla-", "tokens": ["Wel\u00b7cher", "der", "Be\u00b7we\u00b7gung", "Ur\u00b7sprung", ".", "Wann", "nun", "der", "Pla"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAT", "ART", "NN", "NN", "$.", "PWAV", "ADV", "ART", "TRUNC"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.4": {"text": "neten Heer,", "tokens": ["ne\u00b7ten", "Heer", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "--+", "measure": "anapaest.init"}, "line.5": {"text": "Und, nebst ihnen, unsre Erde von der Sonnen umgef\u00fchrt,", "tokens": ["Und", ",", "nebst", "ih\u00b7nen", ",", "uns\u00b7re", "Er\u00b7de", "von", "der", "Son\u00b7nen", "um\u00b7ge\u00b7f\u00fchrt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "PPER", "$,", "PPOSAT", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Auch von ihr erw\u00e4rmet wird, so da\u00df, wenn nur sie nicht", "tokens": ["Auch", "von", "ihr", "er\u00b7w\u00e4r\u00b7met", "wird", ",", "so", "da\u00df", ",", "wenn", "nur", "sie", "nicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPER", "VVFIN", "VAFIN", "$,", "ADV", "KOUS", "$,", "KOUS", "ADV", "PPER", "PTKNEG"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.7": {"text": "w\u00e4r,", "tokens": ["w\u00e4r", ","], "token_info": ["word", "punct"], "pos": ["VAFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Alles Leben und Bewegen ihrer aller stille stehn,", "tokens": ["Al\u00b7les", "Le\u00b7ben", "und", "Be\u00b7we\u00b7gen", "ih\u00b7rer", "al\u00b7ler", "stil\u00b7le", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "VVFIN", "PPOSAT", "PIAT", "ADJA", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Ja den Augenblick vergehn,", "tokens": ["Ja", "den", "Au\u00b7gen\u00b7blick", "ver\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Stocken und verkommen m\u00fc\u00dfte; sollte man, mit wahren", "tokens": ["Sto\u00b7cken", "und", "ver\u00b7kom\u00b7men", "m\u00fc\u00df\u00b7te", ";", "soll\u00b7te", "man", ",", "mit", "wah\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "KON", "VVINF", "VMFIN", "$.", "VMFIN", "PIS", "$,", "APPR", "VVINF"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.11": {"text": "Schl\u00fcssen,", "tokens": ["Schl\u00fcs\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.12": {"text": "Denn die Sonne nicht die Seele der Planeten nennen", "tokens": ["Denn", "die", "Son\u00b7ne", "nicht", "die", "See\u00b7le", "der", "Pla\u00b7ne\u00b7ten", "nen\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PTKNEG", "ART", "NN", "ART", "NN", "VVINF"], "meter": "--+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "m\u00fcssen?", "tokens": ["m\u00fcs\u00b7sen", "?"], "token_info": ["word", "punct"], "pos": ["VMFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.14": {"text": "Ja sie wird wohl nimmer anders, auch von dieser unsrer", "tokens": ["Ja", "sie", "wird", "wohl", "nim\u00b7mer", "an\u00b7ders", ",", "auch", "von", "die\u00b7ser", "uns\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "PPER", "VAFIN", "ADV", "ADV", "ADV", "$,", "ADV", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.15": {"text": "Erden,", "tokens": ["Er\u00b7den", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.16": {"text": "Als derselben wahre Seele k\u00f6nnen angesehen werden.", "tokens": ["Als", "der\u00b7sel\u00b7ben", "wah\u00b7re", "See\u00b7le", "k\u00f6n\u00b7nen", "an\u00b7ge\u00b7se\u00b7hen", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ADJA", "NN", "VMFIN", "VVPP", "VAINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Ach! so schaue k\u00fcnftighin unsre Sonne jedermann,", "tokens": ["Ach", "!", "so", "schau\u00b7e", "k\u00fcnf\u00b7tig\u00b7hin", "uns\u00b7re", "Son\u00b7ne", "je\u00b7der\u00b7mann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "ADV", "PPOSAT", "NN", "PIS", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Anders als bisher geschehen, mit vermehrter Andacht, an!", "tokens": ["An\u00b7ders", "als", "bis\u00b7her", "ge\u00b7sche\u00b7hen", ",", "mit", "ver\u00b7mehr\u00b7ter", "An\u00b7dacht", ",", "an", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "KOKOM", "ADV", "VVPP", "$,", "APPR", "ADJA", "NN", "$,", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Da sie aus der Gottheit stammet,", "tokens": ["Da", "sie", "aus", "der", "Got\u00b7theit", "stam\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Zu der Creaturen Besten leuchtet, sie belebet, flammet,", "tokens": ["Zu", "der", "Crea\u00b7tu\u00b7ren", "Bes\u00b7ten", "leuch\u00b7tet", ",", "sie", "be\u00b7le\u00b7bet", ",", "flam\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$,", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.5": {"text": "Und weit mehr, als alle Dinge, GOtt, als wie im Spiegel,", "tokens": ["Und", "weit", "mehr", ",", "als", "al\u00b7le", "Din\u00b7ge", ",", "Gott", ",", "als", "wie", "im", "Spie\u00b7gel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "$,", "KOUS", "PIAT", "NN", "$,", "NN", "$,", "KOUS", "KOKOM", "APPRART", "NN", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "weiset;", "tokens": ["wei\u00b7set", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Wird in ihr die wahre Gottheit auch am w\u00fcrdigsten gepreiset.", "tokens": ["Wird", "in", "ihr", "die", "wah\u00b7re", "Got\u00b7theit", "auch", "am", "w\u00fcr\u00b7digs\u00b7ten", "ge\u00b7prei\u00b7set", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "ART", "ADJA", "NN", "ADV", "APPRART", "VAFIN", "VVPP", "$."], "meter": "-+--+------+--+-", "measure": "iambic.tetra.relaxed"}}}}}