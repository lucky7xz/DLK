{"textgrid.poem.54564": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "53.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Holdseligstes Geschlecht an Treffligkeit und Sinnen/", "tokens": ["Hold\u00b7se\u00b7ligs\u00b7tes", "Ge\u00b7schlecht", "an", "Tref\u00b7flig\u00b7keit", "und", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "KON", "NN", "$("], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Wen hast du nicht bi\u00dfher zu deinem Dienst bewegt?", "tokens": ["Wen", "hast", "du", "nicht", "bi\u00df\u00b7her", "zu", "dei\u00b7nem", "Dienst", "be\u00b7wegt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es hat sich West und Ost und Nord und Sud erregt/", "tokens": ["Es", "hat", "sich", "West", "und", "Ost", "und", "Nord", "und", "Sud", "er\u00b7regt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "NE", "KON", "NN", "KON", "NE", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und deine Gunst gesucht durch Liebe zu gewinnen.", "tokens": ["Und", "dei\u00b7ne", "Gunst", "ge\u00b7sucht", "durch", "Lie\u00b7be", "zu", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"