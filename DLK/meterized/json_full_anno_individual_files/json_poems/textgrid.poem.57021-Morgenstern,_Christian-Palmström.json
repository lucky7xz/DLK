{"textgrid.poem.57021": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Palmstr\u00f6m", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Palmstr\u00f6m steht an einem Teiche", "tokens": ["Palm\u00b7str\u00f6m", "steht", "an", "ei\u00b7nem", "Tei\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "und entfaltet gro\u00df ein rotes Taschentuch:", "tokens": ["und", "ent\u00b7fal\u00b7tet", "gro\u00df", "ein", "ro\u00b7tes", "Ta\u00b7schen\u00b7tuch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Auf dem Tuch ist eine Eiche", "tokens": ["Auf", "dem", "Tuch", "ist", "ei\u00b7ne", "Ei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "dargestellt, sowie ein Mensch mit einem Buch.", "tokens": ["dar\u00b7ge\u00b7stellt", ",", "so\u00b7wie", "ein", "Mensch", "mit", "ei\u00b7nem", "Buch", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KON", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Palmstr\u00f6m wagt nicht sich hineinzuschneuzen \u2013", "tokens": ["Palm\u00b7str\u00f6m", "wagt", "nicht", "sich", "hin\u00b7ein\u00b7zu\u00b7schneu\u00b7zen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "PRF", "VVINF", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "er geh\u00f6rt zu jenen K\u00e4uzen,", "tokens": ["er", "ge\u00b7h\u00f6rt", "zu", "je\u00b7nen", "K\u00e4u\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "die oft unvermittelt-nackt", "tokens": ["die", "oft", "un\u00b7ver\u00b7mit\u00b7tel\u00b7tnackt"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ehrfurcht vor dem Sch\u00f6nen packt.", "tokens": ["Ehr\u00b7furcht", "vor", "dem", "Sch\u00f6\u00b7nen", "packt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Z\u00e4rtlich faltet er zusammen,", "tokens": ["Z\u00e4rt\u00b7lich", "fal\u00b7tet", "er", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "was er eben erst entbreitet.", "tokens": ["was", "er", "e\u00b7ben", "erst", "ent\u00b7brei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und kein F\u00fchlender wird ihn verdammen,", "tokens": ["Und", "kein", "F\u00fch\u00b7len\u00b7der", "wird", "ihn", "ver\u00b7dam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "weil er ungeschneuzt entschreitet.", "tokens": ["weil", "er", "un\u00b7ge\u00b7schneuzt", "ent\u00b7schrei\u00b7tet", "."], "token_inf