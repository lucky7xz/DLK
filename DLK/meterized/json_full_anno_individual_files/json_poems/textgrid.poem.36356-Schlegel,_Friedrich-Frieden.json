-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Getreuer wieder hin!", "tokens": ["Ge\u00b7treu\u00b7er", "wie\u00b7der", "hin", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "In stiller Brust gen\u00e4hrt,", "tokens": ["In", "stil\u00b7ler", "Brust", "ge\u00b7n\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Mu\u00df Fried' und Demut wohnen,", "tokens": ["Mu\u00df", "Fried'", "und", "De\u00b7mut", "woh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Der alte Glaube thronen,", "tokens": ["Der", "al\u00b7te", "Glau\u00b7be", "thro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Eh' Heil uns wiederkehrt.", "tokens": ["Eh'", "Heil", "uns", "wie\u00b7der\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}