 "pos": ["ADV", "VMFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verrat die Fl\u00fcgel breiten,", "tokens": ["Ver\u00b7rat", "die", "Fl\u00fc\u00b7gel", "brei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Edle untergehn;", "tokens": ["Das", "Ed\u00b7le", "un\u00b7ter\u00b7gehn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Nichtig war all ihr Streben,", "tokens": ["Nich\u00b7tig", "war", "all", "ihr", "Stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PIAT", "PPOSAT", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Und ohne Herz ihr Leben,", "tokens": ["Und", "oh\u00b7ne", "Herz", "ihr", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie mocht' es wohl bestehn?", "tokens": ["Wie", "mocht'", "es", "wohl", "be\u00b7stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wie gr\u00fcnte Friede wohl,", "tokens": ["Wie", "gr\u00fcn\u00b7te", "Frie\u00b7de", "wohl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Wo vor des Neides Bissen,", "tokens": ["Wo", "vor", "des", "Nei\u00b7des", "Bis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Von Gier und Angst zerrissen,", "tokens": ["Von", "Gier", "und", "Angst", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Nichts Hohes atmen soll?", "tokens": ["Nichts", "Ho\u00b7hes", "at\u00b7men", "soll", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJA", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Soll aber schon hienieden,", "tokens": ["Soll", "a\u00b7ber", "schon", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von Gott herab, der Frieden", "tokens": ["Von", "Gott", "her\u00b7ab", ",", "der", "Frie\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "ADV", "$,", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "G\u00fcrten der Erde Brust,", "tokens": ["G\u00fcr\u00b7ten", "der", "Er\u00b7de", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Da\u00df fern der Mensch von Reue,", "tokens": ["Da\u00df", "fern", "der", "Mensch", "von", "Reu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sich einst in sch\u00f6ner Treue,", "tokens": ["Sich", "einst", "in", "sch\u00f6\u00b7ner", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Des Himmels sei bewu\u00dft;", "tokens": ["Des", "Him\u00b7mels", "sei", "be\u00b7wu\u00dft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "So kehrt zu Gott zur\u00fcck!", "tokens": ["So", "kehrt", "zu", "Gott", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Des Glaubens hohe Palme,", "tokens": ["Des", "Glau\u00b7bens", "ho\u00b7he", "Pal\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Der Hoffnung Segenshalme", "tokens": ["Der", "Hoff\u00b7nung", "Se\u00b7gens\u00b7hal\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Bringt euch der Liebe Gl\u00fcck.", "tokens": ["Bringt", "euch", "der", "Lie\u00b7be", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "O la\u00dft das wilde Streiten,", "tokens": ["O", "la\u00dft", "das", "wil\u00b7de", "Strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wollt kindlich nur bereiten", "tokens": ["Wollt", "kind\u00b7lich", "nur", "be\u00b7rei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ADJD", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Euch auf den gro\u00dfen Tag,", "tokens": ["Euch", "auf", "den", "gro\u00b7\u00dfen", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wo wieder hier zu kommen,", "tokens": ["Wo", "wie\u00b7der", "hier", "zu", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zur Rettung aller Frommen,", "tokens": ["Zur", "Ret\u00b7tung", "al\u00b7ler", "From\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Herr uns einst versprach.", "tokens": ["Der", "Herr", "uns", "einst", "ver\u00b7sprach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Vom Herzen rei\u00dft den Wurm,", "tokens": ["Vom", "Her\u00b7zen", "rei\u00dft", "den", "Wurm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Seid wieder Gottes Kinder,", "tokens": ["Seid", "wie\u00b7der", "Got\u00b7tes", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADV", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "So wird die Zeit gelinder,", "tokens": ["So", "wird", "die", "Zeit", "ge\u00b7lin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "So schweigt der wilde Sturm.", "tokens": ["So", "schweigt", "der", "wil\u00b7de", "Sturm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Als Gott ihr widersprochen,", "tokens": ["Als", "Gott", "ihr", "wi\u00b7der\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Treu' ihm hat gebrochen,", "tokens": ["Die", "Treu'", "ihm", "hat", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da war es, wo's begann;", "tokens": ["Da", "war", "es", ",", "wo's", "be\u00b7gann", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "PWAV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ihr wolltet alles fassen,", "tokens": ["Ihr", "woll\u00b7tet", "al\u00b7les", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Als wild ein w\u00fctend Hassen", "tokens": ["Als", "wild", "ein", "w\u00fc\u00b7tend", "Has\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "ART", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit Blindheit euch umspann.", "tokens": ["Mit", "Blind\u00b7heit", "euch", "um\u00b7spann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Tod entstieg dem Grab,", "tokens": ["Der", "Tod", "ent\u00b7stieg", "dem", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Die Liebe war entflohen,", "tokens": ["Die", "Lie\u00b7be", "war", "ent\u00b7flo\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Voll Mitleid sahn die hohen", "tokens": ["Voll", "Mit\u00b7leid", "sahn", "die", "ho\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "M\u00e4chte auf uns herab.", "tokens": ["M\u00e4ch\u00b7te", "auf", "uns", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.12": {"line.1": {"text": "O Torheit zu beweinen,", "tokens": ["O", "Tor\u00b7heit", "zu", "be\u00b7wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df, blinden Wahns, will meinen", "tokens": ["Da\u00df", ",", "blin\u00b7den", "Wahns", ",", "will", "mei\u00b7nen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "$,", "ADJA", "NN", "$,", "VMFIN", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Mensch, er sei nun frei;", "tokens": ["Der", "Mensch", ",", "er", "sei", "nun", "frei", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und doch in tiefem Herzen", "tokens": ["Und", "doch", "in", "tie\u00b7fem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die rettungslosen Schmerzen,", "tokens": ["Die", "ret\u00b7tungs\u00b7lo\u00b7sen", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der sch\u00f6ne Bund entzwei.", "tokens": ["Der", "sch\u00f6\u00b7ne", "Bund", "ent\u00b7zwei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Es ri\u00df des Lebens Band,", "tokens": ["Es", "ri\u00df", "des", "Le\u00b7bens", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Da\u00df alles los nun schwebet,", "tokens": ["Da\u00df", "al\u00b7les", "los", "nun", "schwe\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Im Sturme zagt und bebet,", "tokens": ["Im", "Stur\u00b7me", "zagt", "und", "be\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Findt keiner Hoffnung Land.", "tokens": ["Findt", "kei\u00b7ner", "Hoff\u00b7nung", "Land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Was schn\u00f6de ihr zerst\u00f6rtet,", "tokens": ["Was", "schn\u00f6\u00b7de", "ihr", "zer\u00b7st\u00f6r\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da ihr euch selbst bet\u00f6rtet,", "tokens": ["Da", "ihr", "euch", "selbst", "be\u00b7t\u00f6r\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kann Hochmut nicht erbaun.", "tokens": ["Kann", "Hoch\u00b7mut", "nicht", "er\u00b7baun", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Was m\u00f6chtet ihr ersinnen?", "tokens": ["Was", "m\u00f6ch\u00b7tet", "ihr", "er\u00b7sin\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ihr seht es all' zerrinnen,", "tokens": ["Ihr", "seht", "es", "all'", "zer\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Habt selber kein Vertraun.", "tokens": ["Habt", "sel\u00b7ber", "kein", "Ver\u00b7traun", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Was Gott uns liebend gab,", "tokens": ["Was", "Gott", "uns", "lie\u00b7bend", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Wie m\u00f6gt ihr's neu erschaffen?", "tokens": ["Wie", "m\u00f6gt", "ih\u00b7r's", "neu", "er\u00b7schaf\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PIS", "ADJD", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "li