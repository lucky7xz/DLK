{"dta.poem.21275": {"metadata": {"author": {"name": "Ramler, Karl Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Abschied von den Helden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20476-3", "language": ["de:0.99"], "booktitle": "Ramler, Karl Wilhelm: Oden. Berlin, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht Friedrichs Helden, welche der\nBrenne liebt,", "tokens": ["Nicht", "Fried\u00b7richs", "Hel\u00b7den", ",", "wel\u00b7che", "der", "Bren\u00b7ne", "liebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NE", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Schwerin und Heinrich, Bevern und", "tokens": ["Schwe\u00b7rin", "und", "Hein\u00b7rich", ",", "Be\u00b7vern", "und"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "KON", "NE", "$,", "NN", "KON"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Nicht jeder Gwelfe nur und Seidlitz", "tokens": ["Nicht", "je\u00b7der", "Gwel\u00b7fe", "nur", "und", "Seid\u00b7litz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "PIAT", "NN", "ADV", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sind der gewaltigen Hymne w\u00fcrdig.", "tokens": ["Sind", "der", "ge\u00b7wal\u00b7ti\u00b7gen", "Hym\u00b7ne", "w\u00fcr\u00b7dig", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "---+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Auch ihr, der Staaten friedliche W\u00e4chter,", "tokens": ["Auch", "ihr", ",", "der", "Staa\u00b7ten", "fried\u00b7li\u00b7che", "W\u00e4ch\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein hohes Recht an unsern gefl\u00fcgelten", "tokens": ["Ein", "ho\u00b7hes", "Recht", "an", "un\u00b7sern", "ge\u00b7fl\u00fc\u00b7gel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ges\u00e4ngen; auch der tapfre Richter", "tokens": ["Ge\u00b7s\u00e4n\u00b7gen", ";", "auch", "der", "tapf\u00b7re", "Rich\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "M\u00e4chtiger Frevel und armer Unschuld;", "tokens": ["M\u00e4ch\u00b7ti\u00b7ger", "Fre\u00b7vel", "und", "ar\u00b7mer", "Un\u00b7schuld", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Auch deren Geist dem immer erneuerten", "tokens": ["Auch", "de\u00b7ren", "Geist", "dem", "im\u00b7mer", "er\u00b7neu\u00b7er\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PDS", "NN", "ART", "ADV", "ADJA"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Geschlecht der Menschen G\u00fcter und K\u00fcnste", "tokens": ["Ge\u00b7schlecht", "der", "Men\u00b7schen", "G\u00fc\u00b7ter", "und", "K\u00fcns\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Auch wer allwachsam seinen B\u00fcrgern", "tokens": ["Auch", "wer", "all\u00b7wach\u00b7sam", "sei\u00b7nen", "B\u00fcr\u00b7gern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PWS", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ueberfluss, Sitte, Gesundheit aus-", "tokens": ["Ue\u00b7ber\u00b7fluss", ",", "Sit\u00b7te", ",", "Ge\u00b7sund\u00b7heit", "aus"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "NN", "$,", "NN", "TRUNC"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Noch viele goldne Pfeile ruhn unversucht", "tokens": ["Noch", "vie\u00b7le", "gold\u00b7ne", "Pfei\u00b7le", "ruhn", "un\u00b7ver\u00b7sucht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "ADJA", "NN", "VVINF", "ADJD"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Im K\u00f6cher eines Dichters, der fr\u00fche schon", "tokens": ["Im", "K\u00f6\u00b7cher", "ei\u00b7nes", "Dich\u00b7ters", ",", "der", "fr\u00fc\u00b7he", "schon"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "ART", "ADJA", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sein Leben ganz den liederreichen", "tokens": ["Sein", "Le\u00b7ben", "ganz", "den", "lie\u00b7der\u00b7rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schwestern Uraniens angelobt hat;", "tokens": ["Schwes\u00b7tern", "U\u00b7ra\u00b7ni\u00b7ens", "an\u00b7ge\u00b7lobt", "hat", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Der, hoffend auf die Krone der After-", "tokens": ["Der", ",", "hof\u00b7fend", "auf", "die", "Kro\u00b7ne", "der", "Af\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "$,", "VVPP", "APPR", "ART", "NN", "ART", "TRUNC"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den b\u00fcrgerlichen Ehren entsagete;", "tokens": ["Den", "b\u00fcr\u00b7ger\u00b7li\u00b7chen", "Eh\u00b7ren", "ent\u00b7sa\u00b7ge\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+---", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Der alle Wege, die zum Reichthum,", "tokens": ["Der", "al\u00b7le", "We\u00b7ge", ",", "die", "zum", "Reicht\u00b7hum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,", "PRELS", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fchren, verliess: ein zufriedner J\u00fcng-", "tokens": ["F\u00fch\u00b7ren", ",", "ver\u00b7liess", ":", "ein", "zu\u00b7fried\u00b7ner", "J\u00fcng"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "$.", "ART", "ADJA", "TRUNC"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Verleiht, bevor diess Haupthaar der Reif", "tokens": ["Ver\u00b7leiht", ",", "be\u00b7vor", "diess", "Haupt\u00b7haar", "der", "Reif"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "PDAT", "NN", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ein guter Gott mir Einen Aonischen", "tokens": ["Ein", "gu\u00b7ter", "Gott", "mir", "Ei\u00b7nen", "A\u00b7o\u00b7nisc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit B\u00e4chen und Geb\u00fcsch durchflochtnen", "tokens": ["Mit", "B\u00e4\u00b7chen", "und", "Ge\u00b7b\u00fcsch", "durch\u00b7flocht\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Winkel der Erde: so sollen alle", "tokens": ["Win\u00b7kel", "der", "Er\u00b7de", ":", "so", "sol\u00b7len", "al\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$.", "ADV", "VMFIN", "PIS"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Durch alle Winde fliegen, den Weisesten", "tokens": ["Durch", "al\u00b7le", "Win\u00b7de", "flie\u00b7gen", ",", "den", "Wei\u00b7ses\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$,", "ART", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein s\u00fcsser Klang, dem Ohre des bl\u00f6den", "tokens": ["Ein", "s\u00fcs\u00b7ser", "Klang", ",", "dem", "Oh\u00b7re", "des", "bl\u00f6\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "ART", "ADJA"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Unmerklich. \u2014 Ungeschw\u00e4cht soll ihre", "tokens": ["Un\u00b7mer\u00b7klich", ".", "Un\u00b7ge\u00b7schw\u00e4cht", "soll", "ih\u00b7re"], "token_info": ["word", "punct", "punct", "word", "word", "word"], "pos": ["ADJD", "$.", "$(", "NN", "VMFIN", "PPOSAT"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "T\u00f6ne der Brittische Barde trinken;", "tokens": ["T\u00f6\u00b7ne", "der", "Brit\u00b7ti\u00b7sche", "Bar\u00b7de", "trin\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Sie sollen hell den Himmel Ausoniens", "tokens": ["Sie", "sol\u00b7len", "hell", "den", "Him\u00b7mel", "Au\u00b7so\u00b7ni\u00b7ens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADJD", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durchwirbeln; (dort war ehmals ihr Vater-", "tokens": ["Durch\u00b7wir\u00b7beln", ";(", "dort", "war", "eh\u00b7mals", "ihr", "Va\u00b7ter"], "token_info": ["word", "emoticon", "word", "word", "word", "word", "word"], "pos": ["NN", "$(", "ADV", "VAFIN", "ADV", "PPOSAT", "NN"], "meter": "+-+-++--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Auch Galliens vergn\u00fcgter S\u00e4nger", "tokens": ["Auch", "Gal\u00b7li\u00b7ens", "ver\u00b7gn\u00fcg\u00b7ter", "S\u00e4n\u00b7ger"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00f6re den Nachhall, nicht ohne Scheel-", "tokens": ["H\u00f6\u00b7re", "den", "Nach\u00b7hall", ",", "nicht", "oh\u00b7ne", "Scheel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ART", "NN", "$,", "PTKNEG", "APPR", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}}}}