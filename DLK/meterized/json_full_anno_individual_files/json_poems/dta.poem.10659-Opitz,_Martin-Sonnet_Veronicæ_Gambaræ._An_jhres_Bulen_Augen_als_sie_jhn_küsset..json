{"dta.poem.10659": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet Veronic\u00e6 Gambar\u00e6.  \n  An jhres Bulen Augen/ als sie jhn k\u00fcsset.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr Wohnhau\u00df vnd Losier der Liebe/ last empfinden", "tokens": ["Ihr", "Wohn\u00b7hau\u00df", "vnd", "Lo\u00b7sier", "der", "Lie\u00b7be", "/", "last", "emp\u00b7fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN", "ART", "NN", "$(", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mich ewren sch\u00f6nen Glantz/ zu euch/ mein Firmament/", "tokens": ["Mich", "ew\u00b7ren", "sch\u00f6\u00b7nen", "Glantz", "/", "zu", "euch", "/", "mein", "Fir\u00b7ma\u00b7ment", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "$(", "APPR", "PPER", "$(", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zu euch/ jhr Augen/ ich mein Hertz vnd Sinnen wendt/", "tokens": ["Zu", "euch", "/", "jhr", "Au\u00b7gen", "/", "ich", "mein", "Hertz", "vnd", "Sin\u00b7nen", "wendt", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$(", "PPOSAT", "NN", "$(", "PPER", "PPOSAT", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df meine Finsterni\u00df durch ewer Liecht verschwinden/", "tokens": ["Da\u00df", "mei\u00b7ne", "Fins\u00b7ter\u00b7ni\u00df", "durch", "e\u00b7wer", "Liecht", "ver\u00b7schwin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wann ewer Glantz vffgeht/ wann sich die Stralen finden/", "tokens": ["Wann", "e\u00b7wer", "Glantz", "vff\u00b7geht", "/", "wann", "sich", "die", "Stra\u00b7len", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$(", "PWAV", "PRF", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Hat alle meine Klag/ hat alles Wehe ein End/", "tokens": ["Hat", "al\u00b7le", "mei\u00b7ne", "Klag", "/", "hat", "al\u00b7les", "We\u00b7he", "ein", "End", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PPOSAT", "NN", "$(", "VAFIN", "PIAT", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.7": {"text": "Mein Hertze wird so froh/ da\u00df es sich selbst nicht kennt/", "tokens": ["Mein", "Hert\u00b7ze", "wird", "so", "froh", "/", "da\u00df", "es", "sich", "selbst", "nicht", "kennt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "VAFIN", "ADV", "ADJD", "$(", "KOUS", "PPER", "PRF", "ADV", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Kein Trawren darff bey mir zu sein sich vnderwinden.", "tokens": ["Kein", "Traw\u00b7ren", "darff", "bey", "mir", "zu", "sein", "sich", "vn\u00b7der\u00b7win\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "APPR", "PPER", "PTKZU", "VAINF", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Von euch/ jhr Brunnenquell der rechten Liebes flammen/", "tokens": ["Von", "euch", "/", "jhr", "Brun\u00b7nen\u00b7quell", "der", "rech\u00b7ten", "Lie\u00b7bes", "flam\u00b7men", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$(", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Kompt alle Lebens Lust/ kompt alles Gut zusammen/", "tokens": ["Kompt", "al\u00b7le", "Le\u00b7bens", "Lust", "/", "kompt", "al\u00b7les", "Gut", "zu\u00b7sam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "NN", "$(", "VVFIN", "PIS", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Was mir in dieser Welt verehren kan das Gl\u00fcck:", "tokens": ["Was", "mir", "in", "die\u00b7ser", "Welt", "ver\u00b7eh\u00b7ren", "kan", "das", "Gl\u00fcck", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PDAT", "NN", "VVFIN", "VMFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Seid mir hierumb hinfort gewogen vnd geneiget/", "tokens": ["Seid", "mir", "hie\u00b7rumb", "hin\u00b7fort", "ge\u00b7wo\u00b7gen", "vnd", "ge\u00b7nei\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "PAV", "ADV", "VVPP", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Vnd durch die Trefflichkeit/ die sich bey euch ereuget/", "tokens": ["Vnd", "durch", "die", "Treff\u00b7lich\u00b7keit", "/", "die", "sich", "bey", "euch", "e\u00b7reu\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$(", "PRELS", "PRF", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Zieht mein betr\u00fcbtes Hertz von Todes noth zu r\u00fcck.", "tokens": ["Zieht", "mein", "be\u00b7tr\u00fcb\u00b7tes", "Hertz", "von", "To\u00b7des", "noth", "zu", "r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "NN", "NN", "PTKZU", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}