{"textgrid.poem.52832": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Spektakel, welch' ein Schrecken!", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Spektakel, welch' ein Schrecken!", "tokens": ["O", "Spek\u00b7ta\u00b7kel", ",", "welch'", "ein", "Schre\u00b7cken", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAT", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das ist Trauersiegellak.", "tokens": ["Das", "ist", "Trau\u00b7er\u00b7sie\u00b7gel\u00b7lak", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jeremias Birkenstecken,", "tokens": ["Je\u00b7re\u00b7mi\u00b7as", "Bir\u00b7ken\u00b7ste\u00b7cken", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "B\u00fcrste deinen schwarzen Frack!", "tokens": ["B\u00fcrs\u00b7te", "dei\u00b7nen", "schwar\u00b7zen", "Frack", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Welche Botschaft! Biedermaier,", "tokens": ["Wel\u00b7che", "Bot\u00b7schaft", "!", "Bie\u00b7der\u00b7mai\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PWAT", "NN", "$.", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieser Edle, lebt nicht mehr!", "tokens": ["Die\u00b7ser", "Ed\u00b7le", ",", "lebt", "nicht", "mehr", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "$,", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bindet Flor an meine Leier,", "tokens": ["Bin\u00b7det", "Flor", "an", "mei\u00b7ne", "Lei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn der Vorgang schmerzt mich sehr.", "tokens": ["Denn", "der", "Vor\u00b7gang", "schmerzt", "mich", "sehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Bindet Flor an Hut und Hauben,", "tokens": ["Bin\u00b7det", "Flor", "an", "Hut", "und", "Hau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df die Thr\u00e4ne besser flie\u00dft,", "tokens": ["Da\u00df", "die", "Thr\u00e4\u00b7ne", "bes\u00b7ser", "flie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Niemand wird die Nachricht glauben,", "tokens": ["Nie\u00b7mand", "wird", "die", "Nach\u00b7richt", "glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis er's in dem Bl\u00e4ttle liest.", "tokens": ["Bis", "er's", "in", "dem", "Bl\u00e4tt\u00b7le", "liest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Gott! h\u00e4tt'