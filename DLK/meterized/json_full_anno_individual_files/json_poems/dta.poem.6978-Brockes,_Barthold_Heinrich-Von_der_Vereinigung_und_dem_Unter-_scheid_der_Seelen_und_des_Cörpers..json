{"dta.poem.6978": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Vereinigung und dem Unter-  \n scheid der Seelen und des C\u00f6rpers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Durch die zwo n\u00f6thige Verbindungen, erwecken", "tokens": ["Durch", "die", "zwo", "n\u00f6\u00b7thi\u00b7ge", "Ver\u00b7bin\u00b7dun\u00b7gen", ",", "er\u00b7we\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "CARD", "ADJA", "NN", "$,", "VVINF"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Die Sinnen beyderseits in uns Begierd\u2019 und Schrecken.", "tokens": ["Die", "Sin\u00b7nen", "bey\u00b7der\u00b7seits", "in", "uns", "Be\u00b7gierd'", "und", "Schre\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Dinge, so der Geist dem C\u00f6rper sch\u00e4dlich hielte,", "tokens": ["Die", "Din\u00b7ge", ",", "so", "der", "Geist", "dem", "C\u00f6r\u00b7per", "sch\u00e4d\u00b7lich", "hiel\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ART", "NN", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und die er diensam fand, sind Ursach, da\u00df man Lust,", "tokens": ["Und", "die", "er", "dien\u00b7sam", "fand", ",", "sind", "Ur\u00b7sach", ",", "da\u00df", "man", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PRELS", "PPER", "PDS", "VVFIN", "$,", "VAFIN", "NN", "$,", "KOUS", "PIS", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Auch Schmertzen allbereit an unsrer Mutter Brust,", "tokens": ["Auch", "Schmert\u00b7zen", "all\u00b7be\u00b7reit", "an", "uns\u00b7rer", "Mut\u00b7ter", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wiewol fast unvermercket, f\u00fchlte.", "tokens": ["Wie\u00b7wol", "fast", "un\u00b7ver\u00b7mer\u00b7cket", ",", "f\u00fchl\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Denselben Druck, den man zu Anfangs kaum vermerckt,", "tokens": ["Den\u00b7sel\u00b7ben", "Druck", ",", "den", "man", "zu", "An\u00b7fangs", "kaum", "ver\u00b7merckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "PIS", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sind durch der Geister Lauf verneuret und verst\u00e4rckt.", "tokens": ["Sind", "durch", "der", "Geis\u00b7ter", "Lauf", "ver\u00b7neu\u00b7ret", "und", "ver\u00b7st\u00e4rckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "NN", "VVFIN", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Das durch dieselbigen erf\u00fcllte Hertz", "tokens": ["Das", "durch", "die\u00b7sel\u00b7bi\u00b7gen", "er\u00b7f\u00fcll\u00b7te", "Hertz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Dehnt durch die Luft sich aus, und ziehet sich durch Schmertz", "tokens": ["Dehnt", "durch", "die", "Luft", "sich", "aus", ",", "und", "zie\u00b7het", "sich", "durch", "Schmertz"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "PRF", "PTKVZ", "$,", "KON", "VVFIN", "PRF", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Zusammen in sich selbst. Die Seel\u2019, als welche sich", "tokens": ["Zu\u00b7sam\u00b7men", "in", "sich", "selbst", ".", "Die", "Seel'", ",", "als", "wel\u00b7che", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "PRF", "ADV", "$.", "ART", "NN", "$,", "KOUS", "PRELS", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Mit ihrem C\u00f6rper festiglich", "tokens": ["Mit", "ih\u00b7rem", "C\u00f6r\u00b7per", "fes\u00b7tig\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Vereinet und verbindet,", "tokens": ["Ver\u00b7ei\u00b7net", "und", "ver\u00b7bin\u00b7det", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Und durch dies Instrument, das, was ihn r\u00fchrt, empfindet,", "tokens": ["Und", "durch", "dies", "Inst\u00b7ru\u00b7ment", ",", "das", ",", "was", "ihn", "r\u00fchrt", ",", "emp\u00b7fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "PDS", "NN", "$,", "PDS", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Hat Recht, in dem, was ihn betrifft, zu wehlen.", "tokens": ["Hat", "Recht", ",", "in", "dem", ",", "was", "ihn", "be\u00b7tr\u00b7ifft", ",", "zu", "weh\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "APPR", "ART", "$,", "PRELS", "PPER", "VVFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Sie hat das Regiment, er wird durch sie gef\u00fchret.", "tokens": ["Sie", "hat", "das", "Re\u00b7gi\u00b7ment", ",", "er", "wird", "durch", "sie", "ge\u00b7f\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPER", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Verlangen wird durch sie, sowol als Furcht, formiret,", "tokens": ["Ver\u00b7lan\u00b7gen", "wird", "durch", "sie", ",", "so\u00b7wol", "als", "Furcht", ",", "for\u00b7mi\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PPER", "$,", "KON", "KOUS", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Als die zur Folge so, als auch zur Flucht geneigt.", "tokens": ["Als", "die", "zur", "Fol\u00b7ge", "so", ",", "als", "auch", "zur", "Flucht", "ge\u00b7neigt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "APPRART", "NN", "ADV", "$,", "KOUS", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Es haben uns die ersten Sinnlichkeiten", "tokens": ["Es", "ha\u00b7ben", "uns", "die", "ers\u00b7ten", "Sinn\u00b7lich\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Die Leidenschafften all\u2019erzeugt,", "tokens": ["Die", "Lei\u00b7den\u00b7schaff\u00b7ten", "all'\u00b7er\u00b7zeugt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Die durch Gewohnheit uns bestricken und verleiten.", "tokens": ["Die", "durch", "Ge\u00b7wohn\u00b7heit", "uns", "be\u00b7stri\u00b7cken", "und", "ver\u00b7lei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PPER", "VVINF", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die Neigung f\u00fcr die Lust, der Abscheu f\u00fcr die Pein,", "tokens": ["Die", "Nei\u00b7gung", "f\u00fcr", "die", "Lust", ",", "der", "Ab\u00b7scheu", "f\u00fcr", "die", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Der Eckel und die Neigungs-Triebe,", "tokens": ["Der", "E\u00b7ckel", "und", "die", "Nei\u00b7gungs\u00b7Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Indem dieselbigen vermehret seyn,", "tokens": ["In\u00b7dem", "die\u00b7sel\u00b7bi\u00b7gen", "ver\u00b7meh\u00b7ret", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "VVFIN", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Und immer uns, mit st\u00e4rckerm Druck, erf\u00fcllen;", "tokens": ["Und", "im\u00b7mer", "uns", ",", "mit", "st\u00e4r\u00b7ckerm", "Druck", ",", "er\u00b7f\u00fcl\u00b7len", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "PPER", "$,", "APPR", "ADJA", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "So werden sie im Geist zu Ha\u00df und Liebe.", "tokens": ["So", "wer\u00b7den", "sie", "im", "Geist", "zu", "Ha\u00df", "und", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPRART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Dies ist die menschliche Beschaffenheit.", "tokens": ["Dies", "ist", "die", "menschli\u00b7che", "Be\u00b7schaf\u00b7fen\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}