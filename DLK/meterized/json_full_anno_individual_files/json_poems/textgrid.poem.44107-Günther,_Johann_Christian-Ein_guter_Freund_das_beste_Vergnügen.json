{"textgrid.poem.44107": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "Ein guter Freund das beste Vergn\u00fcgen", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Vergn\u00fcgen heist auf Erden", "tokens": ["Mein", "Ver\u00b7gn\u00fc\u00b7gen", "heist", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ein vertrauter Freund allein;", "tokens": ["Ein", "ver\u00b7trau\u00b7ter", "Freund", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn ich den kan habhaft werden,", "tokens": ["Wenn", "ich", "den", "kan", "hab\u00b7haft", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So stimmt Herz und Lippen ein,", "tokens": ["So", "stimmt", "Herz", "und", "Lip\u00b7pen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Und die Losung ist das Pfand:", "tokens": ["Und", "die", "Lo\u00b7sung", "ist", "das", "Pfand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Freundschaft ist das sch\u00f6nste Band.", "tokens": ["Freund\u00b7schaft", "ist", "das", "sch\u00f6ns\u00b7te", "Band", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hier giebt sich ein holdes Gosen", "tokens": ["Hier", "giebt", "sich", "ein", "hol\u00b7des", "Go\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Tausendfacher Anmuth an,", "tokens": ["Tau\u00b7send\u00b7fa\u00b7cher", "An\u00b7muth", "an", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo man stets die Zuckerrosen", "tokens": ["Wo", "man", "stets", "die", "Zu\u00b7cker\u00b7ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der Vergn\u00fcgung brechen kan,", "tokens": ["Der", "Ver\u00b7gn\u00fc\u00b7gung", "bre\u00b7chen", "kan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und ein recht gelobtes Land:", "tokens": ["Und", "ein", "recht", "ge\u00b7lob\u00b7tes", "Land", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Freundschaft ist das sch\u00f6nste Band.", "tokens": ["Freund\u00b7schaft", "ist", "das", "sch\u00f6ns\u00b7te", "Band", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Strebt vor mir nach eitlem Gute,", "tokens": ["Strebt", "vor", "mir", "nach", "eit\u00b7lem", "Gu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blinde Thoren, spat und fr\u00fch!", "tokens": ["Blin\u00b7de", "Tho\u00b7ren", ",", "spat", "und", "fr\u00fch", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mir ist gar nicht so zu Muthe,", "tokens": ["Mir", "ist", "gar", "nicht", "so", "zu", "Mu\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "ADV", "APPR", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dies verlohnt sich wohl der M\u00fch.", "tokens": ["Dies", "ver\u00b7lohnt", "sich", "wohl", "der", "M\u00fch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Was ist Geld? Ein glatter Sand.", "tokens": ["Was", "ist", "Geld", "?", "Ein", "glat\u00b7ter", "Sand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "NN", "$.", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Freundschaft ist das sch\u00f6nste Band.", "tokens": ["Freund\u00b7schaft", "ist", "das", "sch\u00f6ns\u00b7te", "Band", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Andre m\u00f6gen sich mit Sorgen", "tokens": ["And\u00b7re", "m\u00f6\u00b7gen", "sich", "mit", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "PRF", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um des andern Gunst bem\u00fchn", "tokens": ["Um", "des", "an\u00b7dern", "Gunst", "be\u00b7m\u00fchn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und vom Abend bis an Morgen", "tokens": ["Und", "vom", "A\u00b7bend", "bis", "an", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An dem Liebesjoche ziehn.", "tokens": ["An", "dem", "Lie\u00b7bes\u00b7jo\u00b7che", "ziehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mir beliebt kein solcher Tand:", "tokens": ["Mir", "be\u00b7liebt", "kein", "sol\u00b7cher", "Tand", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Freundschaft ist das sch\u00f6nste Band.", "tokens": ["Freund\u00b7schaft", "ist", "das", "sch\u00f6ns\u00b7te", "Band", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Lieben ist ein stets Leiden,", "tokens": ["Lie\u00b7ben", "ist", "ein", "stets", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Das manch heimlich Weh gebiehrt", "tokens": ["Das", "manch", "heim\u00b7lich", "Weh", "ge\u00b7biehrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "ADJD", "NN", "VVPP"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Und bey seinen seltnen Freuden", "tokens": ["Und", "bey", "sei\u00b7nen", "selt\u00b7nen", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tausend Kummer mit sich f\u00fchrt,", "tokens": ["Tau\u00b7send", "Kum\u00b7mer", "mit", "sich", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ein vermyrther Zuckerkand:", "tokens": ["Ein", "ver\u00b7myrt\u00b7her", "Zu\u00b7cker\u00b7kand", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Freundschaft ist das sch\u00f6nste Band.", "tokens": ["Freund\u00b7schaft", "ist", "das", "sch\u00f6ns\u00b7te", "Band", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Freundschaft kan aus allen Sachen,", "tokens": ["Freund\u00b7schaft", "kan", "aus", "al\u00b7len", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn der Liebe Garn zerrei\u00dft,", "tokens": ["Wenn", "der", "Lie\u00b7be", "Garn", "zer\u00b7rei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Honigseim aus Wermuth machen,", "tokens": ["Ho\u00b7ni\u00b7gseim", "aus", "Wer\u00b7muth", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der mit lauter Anmuth speist;", "tokens": ["Der", "mit", "lau\u00b7ter", "An\u00b7muth", "speist", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sie hast allen Unbestand:", "tokens": ["Sie", "hast", "al\u00b7len", "Un\u00b7be\u00b7stand", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Freundschaft ist das sch\u00f6nste Band.", "tokens": ["Freund\u00b7schaft", "ist", "das", "sch\u00f6ns\u00b7te", "Band", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "An ihr tref ich aller Orten", "tokens": ["An", "ihr", "tref", "ich", "al\u00b7ler", "Or\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ein so gro\u00df Vergn\u00fcgen an,", "tokens": ["Ein", "so", "gro\u00df", "Ver\u00b7gn\u00fc\u00b7gen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das ich gar mit keinen Worten", "tokens": ["Das", "ich", "gar", "mit", "kei\u00b7nen", "Wor\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht genung beschreiben kan.", "tokens": ["Nicht", "ge\u00b7nung", "be\u00b7schrei\u00b7ben", "kan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dieses Kleinod stiehlt niemand:", "tokens": ["Die\u00b7ses", "Klei\u00b7nod", "stiehlt", "nie\u00b7mand", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "PIS", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Freundschaft ist das sch\u00f6nste Band.", "tokens": ["Freund\u00b7schaft", "ist", "das", "sch\u00f6ns\u00b7te", "Band", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Nichts soll meinen Sinn besiegen,", "tokens": ["Nichts", "soll", "mei\u00b7nen", "Sinn", "be\u00b7sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wahre Freundschaft soll allein", "tokens": ["Wah\u00b7re", "Freund\u00b7schaft", "soll", "al\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VMFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf der Welt hier mein Vergn\u00fcgen", "tokens": ["Auf", "der", "Welt", "hier", "mein", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der stete Wahlspruch seyn,", "tokens": ["Und", "der", "ste\u00b7te", "Wahl\u00b7spruch", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der mir allen Harm entwand:", "tokens": ["Der", "mir", "al\u00b7len", "Harm", "ent\u00b7wand", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Freundschaft ist das sch\u00f6nste Band.", "tokens": ["Freund\u00b7schaft", "ist", "das", "sch\u00f6ns\u00b7te", "Band", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Mein Vergn\u00fcgen heist auf Erden", "tokens": ["Mein", "Ver\u00b7gn\u00fc\u00b7gen", "heist", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ein vertrauter Freund allein;", "tokens": ["Ein", "ver\u00b7trau\u00b7ter", "Freund", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn ich den kan habhaft werden,", "tokens": ["Wenn", "ich", "den", "kan", "hab\u00b7haft", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So stimmt Herz und Lippen ein,", "tokens": ["So", "stimmt", "Herz", "und", "Lip\u00b7pen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Und die Losung ist das Pfand:", "tokens": ["Und", "die", "Lo\u00b7sung", "i