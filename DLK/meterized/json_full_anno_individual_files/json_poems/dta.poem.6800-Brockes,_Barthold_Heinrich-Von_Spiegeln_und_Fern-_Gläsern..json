{"dta.poem.6800": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von Spiegeln und Fern-  \n Gl\u00e4sern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Durch eben diese Kunst verm\u00f6gen Gl\u00e4ser auch,", "tokens": ["Durch", "e\u00b7ben", "die\u00b7se", "Kunst", "ver\u00b7m\u00f6\u00b7gen", "Gl\u00e4\u00b7ser", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PDAT", "NN", "VVFIN", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch unterschiedentlich geschliffne Fl\u00e4ch-und Kr\u00fcmmen,", "tokens": ["Durch", "un\u00b7ter\u00b7schie\u00b7dent\u00b7lich", "ge\u00b7schliff\u00b7ne", "Fl\u00e4ch\u00b7\u00b7und", "Kr\u00fcm\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den C\u00f6rpern andere Gestalten zu bestimmen:", "tokens": ["Den", "C\u00f6r\u00b7pern", "an\u00b7de\u00b7re", "Ge\u00b7stal\u00b7ten", "zu", "be\u00b7stim\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nachdem das Licht,", "tokens": ["Nach\u00b7dem", "das", "Licht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Das durch die Gl\u00e4ser bricht,", "tokens": ["Das", "durch", "die", "Gl\u00e4\u00b7ser", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Sich dehnt, verbreitet und sich f\u00fcget,", "tokens": ["Sich", "dehnt", ",", "ver\u00b7brei\u00b7tet", "und", "sich", "f\u00fc\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "$,", "VVPP", "KON", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da jeder Gegenwurff uns dann betr\u00fcget.", "tokens": ["Da", "je\u00b7der", "Ge\u00b7gen\u00b7wurff", "uns", "dann", "be\u00b7tr\u00fc\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Sie \u00e4ndern, n\u00e4hern sich, sie werden gro\u00df und klein,", "tokens": ["Sie", "\u00e4n\u00b7dern", ",", "n\u00e4\u00b7hern", "sich", ",", "sie", "wer\u00b7den", "gro\u00df", "und", "klein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "VVFIN", "PRF", "$,", "PPER", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Entfernen sich, sie k\u00f6nnen gar", "tokens": ["Ent\u00b7fer\u00b7nen", "sich", ",", "sie", "k\u00f6n\u00b7nen", "gar"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PRF", "$,", "PPER", "VMFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Vermehret und vervielfacht seyn.", "tokens": ["Ver\u00b7meh\u00b7ret", "und", "ver\u00b7viel\u00b7facht", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Jedwede Seite stellt den gantzen Vorwurff dar,", "tokens": ["Jed\u00b7we\u00b7de", "Sei\u00b7te", "stellt", "den", "gant\u00b7zen", "Vor\u00b7wurff", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So, da\u00df durchs Auge mein Gem\u00fcth,", "tokens": ["So", ",", "da\u00df", "durchs", "Au\u00b7ge", "mein", "Ge\u00b7m\u00fcth", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Statt eines Vorwurffs, zwantzig sieht.", "tokens": ["Statt", "ei\u00b7nes", "Vor\u00b7wurffs", ",", "zwant\u00b7zig", "sieht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "CARD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}