{"textgrid.poem.46931": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Mann ist wacker, der, sein Pfund benutzend,", "tokens": ["Der", "Mann", "ist", "wa\u00b7cker", ",", "der", ",", "sein", "Pfund", "be\u00b7nut\u00b7zend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "PRELS", "$,", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum Dienst des Vaterlands kehrt seine Kr\u00e4fte:", "tokens": ["Zum", "Dienst", "des", "Va\u00b7ter\u00b7lands", "kehrt", "sei\u00b7ne", "Kr\u00e4f\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+--++-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nun denn, mein Geist, geh auch an dein Gesch\u00e4fte,", "tokens": ["Nun", "denn", ",", "mein", "Geist", ",", "geh", "auch", "an", "dein", "Ge\u00b7sch\u00e4f\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PPOSAT", "NN", "$,", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Den Arm mit den dir eignen Waffen putzend.", "tokens": ["Den", "Arm", "mit", "den", "dir", "eig\u00b7nen", "Waf\u00b7fen", "put\u00b7zend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "PPER", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wie k\u00fchne Krieger jetzt, mit Glutblick trutzend,", "tokens": ["Wie", "k\u00fch\u00b7ne", "Krie\u00b7ger", "jetzt", ",", "mit", "Glut\u00b7blick", "trut\u00b7zend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "ADV", "$,", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In Reih'n sich stellend, heben ihre Sch\u00e4fte;", "tokens": ["In", "Reih'n", "sich", "stel\u00b7lend", ",", "he\u00b7ben", "ih\u00b7re", "Sch\u00e4f\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "ADJD", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "So stell' auch Krieger, zwar nur nachge\u00e4ffte,", "tokens": ["So", "stell'", "auch", "Krie\u00b7ger", ",", "zwar", "nur", "nach\u00b7ge\u00b7\u00e4ff\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NN", "$,", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Geharnischter Sonette ein paar Dutzend.", "tokens": ["Ge\u00b7har\u00b7nischter", "So\u00b7net\u00b7te", "ein", "paar", "Dut\u00b7zend", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "PIAT", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Auf denn, die ihr aus meines Busens Ader", "tokens": ["Auf", "denn", ",", "die", "ihr", "aus", "mei\u00b7nes", "Bu\u00b7sens", "A\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "$,", "PRELS", "PPER", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Aufquellt, wie Riesen aus des Stromes Bette,", "tokens": ["Auf\u00b7quellt", ",", "wie", "Rie\u00b7sen", "aus", "des", "Stro\u00b7mes", "Bet\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Stellt euch in eure rauschenden Geschwader!", "tokens": ["Stellt", "euch", "in", "eu\u00b7re", "rau\u00b7schen\u00b7den", "Ge\u00b7schwa\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Schlie\u00dft eure Glieder zu vereinter Kette", "tokens": ["Schlie\u00dft", "eu\u00b7re", "Glie\u00b7der", "zu", "ver\u00b7ein\u00b7ter", "Ket\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und ruft, mithadernd in den gro\u00dfen Hader,", "tokens": ["Und", "ruft", ",", "mit\u00b7ha\u00b7dernd", "in", "den", "gro\u00b7\u00dfen", "Ha\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Erst: Waffen! Waffen! und dann: Rette! Rette!", "tokens": ["Erst", ":", "Waf\u00b7fen", "!", "Waf\u00b7fen", "!", "und", "dann", ":", "Ret\u00b7te", "!", "Ret\u00b7te", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "$.", "NN", "$.", "NN", "$.", "KON", "ADV", "$.", "NN", "$.", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Mann ist wacker, der, sein Pfund benutzend,", "tokens": ["Der", "Mann", "ist", "wa\u00b7cker", ",", "d