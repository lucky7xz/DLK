{"textgrid.poem.42469": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was soll die dunkelrote Rose,", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was soll die dunkelrote Rose,", "tokens": ["Was", "soll", "die", "dun\u00b7kel\u00b7ro\u00b7te", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mir heute just ins Haus gebracht?", "tokens": ["Mir", "heu\u00b7te", "just", "ins", "Haus", "ge\u00b7bracht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da f\u00e4llts mir ein, und vor mir seh ich", "tokens": ["Da", "f\u00e4llts", "mir", "ein", ",", "und", "vor", "mir", "seh", "ich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "APPR", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Freund, an den ich oft gedacht.", "tokens": ["Den", "Freund", ",", "an", "den", "ich", "oft", "ge\u00b7dacht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Begleitet war die sch\u00f6ne Blume", "tokens": ["Be\u00b7glei\u00b7tet", "war", "die", "sch\u00f6\u00b7ne", "Blu\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von einem Schreiben, einem Wort:", "tokens": ["Von", "ei\u00b7nem", "Schrei\u00b7ben", ",", "ei\u00b7nem", "Wort", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbgedenken eines hei\u00dfen Tages.\u00ab", "tokens": ["\u00bb", "ge\u00b7den\u00b7ken", "ei\u00b7nes", "hei\u00b7\u00dfen", "Ta\u00b7ges", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVINF", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ich errate Zeit und Ort.", "tokens": ["Und", "ich", "er\u00b7ra\u00b7te", "Zeit", "und", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wir lagen beide schwer verwundet", "tokens": ["Wir", "la\u00b7gen", "bei\u00b7de", "schwer", "ver\u00b7wun\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "ADJD", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In eines Gartens Sommerlust.", "tokens": ["In", "ei\u00b7nes", "Gar\u00b7tens", "Som\u00b7mer\u00b7lust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mir war das linke Bein zerschmettert,", "tokens": ["Mir", "war", "das", "lin\u00b7ke", "Bein", "zer\u00b7schmet\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dir sa\u00df die Kugel in der Brust.", "tokens": ["Dir", "sa\u00df", "die", "K