{"dta.poem.12550": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Lx.  \n  Pr\u00e4rogative.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Seid ihr wirklich ", "tokens": ["Seid", "ihr", "wirk\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["VAIMP", "PPER", "ADJD"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "als das b\u00fcrgerliche Pack:", "tokens": ["als", "das", "b\u00fcr\u00b7ger\u00b7li\u00b7che", "Pack", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00fctet euch doch vor den Fl\u00f6hen,", "tokens": ["H\u00fc\u00b7tet", "euch", "doch", "vor", "den", "Fl\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "denn die haben drin Geschmack!", "tokens": ["denn", "die", "ha\u00b7ben", "drin", "Ge\u00b7schmack", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sollten's meine Fl\u00f6he merken,", "tokens": ["Soll\u00b7ten's", "mei\u00b7ne", "Fl\u00f6\u00b7he", "mer\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.6": {"text": "meine ", "tokens": ["mei\u00b7ne"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Diese kleinen Epigramme:", "tokens": ["Die\u00b7se", "klei\u00b7nen", "E\u00b7pig\u00b7ram\u00b7me", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "weh' dem deutschen Adel, wehe!", "tokens": ["weh'", "dem", "deut\u00b7schen", "A\u00b7del", ",", "we\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}