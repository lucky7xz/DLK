{"textgrid.poem.49893": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Zu Er\u00f6ffnung eines Albums", "genre": "verse", "period": "N.A.", "pub_year": 1846, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Album! Schneewei\u00df Pergamentpapier,", "tokens": ["Ein", "Al\u00b7bum", "!", "Schnee\u00b7wei\u00df", "Per\u00b7ga\u00b7ment\u00b7pa\u00b7pier", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und Schnitt und Decke sch\u00f6n verziert mit Golde!", "tokens": ["Und", "Schnitt", "und", "De\u00b7cke", "sch\u00f6n", "ver\u00b7ziert", "mit", "Gol\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "ADJD", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht wahr, wenn sich's nur nicht so langsam f\u00fcllen sollte,", "tokens": ["Nicht", "wahr", ",", "wenn", "sich's", "nur", "nicht", "so", "lang\u00b7sam", "f\u00fcl\u00b7len", "soll\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "KOUS", "PIS", "ADV", "PTKNEG", "ADV", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit Spr\u00fcchen, Bildern, hundertfacher Zier?", "tokens": ["Mit", "Spr\u00fc\u00b7chen", ",", "Bil\u00b7dern", ",", "hun\u00b7dert\u00b7fa\u00b7cher", "Zier", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Zur H\u00e4lfte wenigstens s\u00e4h man es gar