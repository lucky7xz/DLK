{"textgrid.poem.24941": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Die erste Schwalbe", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun der Himmel wieder lichter", "tokens": ["Nun", "der", "Him\u00b7mel", "wie\u00b7der", "lich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die letzte Flocke schwand,", "tokens": ["Und", "die", "letz\u00b7te", "Flo\u00b7cke", "schwand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kehrst du, wie dem Griechendichter,", "tokens": ["Kehrst", "du", ",", "wie", "dem", "Grie\u00b7chen\u00b7dich\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kehrst du mir vom Morgenland?", "tokens": ["Kehrst", "du", "mir", "vom", "Mor\u00b7gen\u00b7land", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Unter Palmen und Eypressen,", "tokens": ["Un\u00b7ter", "Pal\u00b7men", "und", "Ey\u00b7pres\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sch\u00f6ne S\u00e4ngerfreundin, ward", "tokens": ["Sch\u00f6\u00b7ne", "S\u00e4n\u00b7ger\u00b7freun\u00b7din", ",", "ward"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADJA", "NN", "$,", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nicht der Freund von dir vergessen,", "tokens": ["Nicht", "der", "Freund", "von", "dir", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Der im Norden dein geharrt?", "tokens": ["Der", "im", "Nor\u00b7den", "dein", "ge\u00b7harrt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "PPOSAT", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Gr\u00fc\u00dfend unsre Nacht, die kalte,", "tokens": ["Gr\u00fc\u00b7\u00dfend", "uns\u00b7re", "Nacht", ",", "die", "kal\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat dich jener Strand geschickt,", "tokens": ["Hat", "dich", "je\u00b7ner", "Strand", "ge\u00b7schickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PDAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo noch ungetr\u00fcbt das alte", "tokens": ["Wo", "noch", "un\u00b7ge\u00b7tr\u00fcbt", "das", "al\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADJD", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Flammenauge niederblickt,", "tokens": ["Flam\u00b7men\u00b7au\u00b7ge", "nie\u00b7der\u00b7blickt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und du singst uns von den K\u00fcsten,", "tokens": ["Und", "du", "singst", "uns", "von", "den", "K\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die das heil'ge Meer umschmiegt,", "tokens": ["Die", "das", "heil'\u00b7ge", "Meer", "um\u00b7schmiegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Das an seinen Mutterbr\u00fcsten", "tokens": ["Das", "an", "sei\u00b7nen", "Mut\u00b7ter\u00b7br\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Unsern ersten Schlaf gewiegt;", "tokens": ["Un\u00b7sern", "ers\u00b7ten", "Schlaf", "ge\u00b7wiegt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Von dem Land, das, eh die Blindheit", "tokens": ["Von", "dem", "Land", ",", "das", ",", "eh", "die", "Blind\u00b7heit"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PDS", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unsern Geist mit Nacht umgraut,", "tokens": ["Un\u00b7sern", "Geist", "mit", "Nacht", "um\u00b7graut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit dem Seherblick der Kindheit", "tokens": ["Mit", "dem", "Se\u00b7her\u00b7blick", "der", "Kind\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wir in Tr\u00e4umen oft geschaut;", "tokens": ["Wir", "in", "Tr\u00e4u\u00b7men", "oft", "ge\u00b7schaut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo wir, wenn die frische Quelle", "tokens": ["Wo", "wir", ",", "wenn", "die", "fri\u00b7sche", "Quel\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Uns zu ihren Borden lud,", "tokens": ["Uns", "zu", "ih\u00b7ren", "Bor\u00b7den", "lud", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "In des Erdenmorgens Helle", "tokens": ["In", "des", "Er\u00b7den\u00b7mor\u00b7gens", "Hel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mit den Hirten oft geruht.", "tokens": ["Mit", "den", "Hir\u00b7ten", "oft", "ge\u00b7ruht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sing denn mit dem Ruf des Werde", "tokens": ["Sing", "denn", "mit", "dem", "Ruf", "des", "Wer\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das erstorbne Leben wach;", "tokens": ["Das", "ers\u00b7torb\u00b7ne", "Le\u00b7ben", "wach", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Durch das gro\u00dfe Herz der Erde", "tokens": ["Durch", "das", "gro\u00b7\u00dfe", "Herz", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00df es pulsen hundertfach,", "tokens": ["La\u00df", "es", "pul\u00b7sen", "hun\u00b7dert\u00b7fach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df in Fr\u00fchlingswonne klopfend", "tokens": ["Da\u00df", "in", "Fr\u00fch\u00b7lings\u00b7won\u00b7ne", "klop\u00b7fend"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Es die Winterbande sprengt,", "tokens": ["Es", "die", "Win\u00b7ter\u00b7ban\u00b7de", "sprengt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und der erste Tau sich tropfend", "tokens": ["Und", "der", "ers\u00b7te", "Tau", "sich", "trop\u00b7fend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "PRF", "VVPP"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "An die erste Bl\u00fcte h\u00e4ngt!", "tokens": ["An", "die", "ers\u00b7te", "Bl\u00fc\u00b7te", "h\u00e4ngt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ach! in seinen Schmerz versponnen", "tokens": ["Ach", "!", "in", "sei\u00b7nen", "Schmerz", "ver\u00b7spon\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlief mein Herz den Winterschlaf,", "tokens": ["Schlief", "mein", "Herz", "den", "Win\u00b7ter\u00b7schlaf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo kein warmer Blick der Sonnen", "tokens": ["Wo", "kein", "war\u00b7mer", "Blick", "der", "Son\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den verpuppten Tr\u00e4umer traf;", "tokens": ["Den", "ver\u00b7pupp\u00b7ten", "Tr\u00e4u\u00b7mer", "traf", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alle meine muntern Geister,", "tokens": ["Al\u00b7le", "mei\u00b7ne", "mun\u00b7tern", "Geis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die sonst Lebenslust gespr\u00fcht,", "tokens": ["Die", "sonst", "Le\u00b7bens\u00b7lust", "ge\u00b7spr\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Neigten starrend in beeister", "tokens": ["Neig\u00b7ten", "star\u00b7rend", "in", "be\u00b7eis\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nacht die H\u00e4upter schlummerm\u00fcd.", "tokens": ["Nacht", "die", "H\u00e4up\u00b7ter", "schlum\u00b7mer\u00b7m\u00fcd", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Doch bei deiner Stimme ersten", "tokens": ["Doch", "bei", "dei\u00b7ner", "Stim\u00b7me", "ers\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kl\u00e4ngen klopfte hoch mein Herz;", "tokens": ["Kl\u00e4n\u00b7gen", "klopf\u00b7te", "hoch", "mein", "Herz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie aus Gr\u00e4bern, wenn sie bersten,", "tokens": ["Wie", "aus", "Gr\u00e4\u00b7bern", ",", "wenn", "sie", "bers\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die Erstandnen himmelw\u00e4rts,", "tokens": ["Die", "Er\u00b7stand\u00b7nen", "him\u00b7mel\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.5": {"text": "Schwangen aus der Seele Tiefen", "tokens": ["Schwan\u00b7gen", "aus", "der", "See\u00b7le", "Tie\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "W\u00fcnsche, tief verh\u00fcllt vom Tod,", "tokens": ["W\u00fcn\u00b7sche", ",", "tief", "ver\u00b7h\u00fcllt", "vom", "Tod", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VVPP", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Hoffnungen, die lange schliefen,", "tokens": ["Hoff\u00b7nun\u00b7gen", ",", "die", "lan\u00b7ge", "schlie\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Jubelnd sich ins Morgenrot.", "tokens": ["Ju\u00b7belnd", "sich", "ins", "Mor\u00b7gen\u00b7rot", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Aus des Kummers Grabgespinste,", "tokens": ["Aus", "des", "Kum\u00b7mers", "Grab\u00b7ge\u00b7spins\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ein befreiter Falter, brach", "tokens": ["Ein", "be\u00b7frei\u00b7ter", "Fal\u00b7ter", ",", "brach"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine Liebeslust und blinzte", "tokens": ["Mei\u00b7ne", "Lie\u00b7bes\u00b7lust", "und", "blinz\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In den goldnen Fr\u00fchlingstag;", "tokens": ["In", "den", "gold\u00b7nen", "Fr\u00fch\u00b7lings\u00b7tag", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Um sie flatterten und summten", "tokens": ["Um", "sie", "flat\u00b7ter\u00b7ten", "und", "summ\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "PPER", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Freuden aus der Gruft empor,", "tokens": ["Freu\u00b7den", "aus", "der", "Gruft", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wirbelte der lang verstummten", "tokens": ["Wir\u00b7bel\u00b7te", "der", "lang", "ver\u00b7stumm\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJD", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Lieder muntrer Lerchenchor.", "tokens": ["Lie\u00b7der", "mun\u00b7trer", "Ler\u00b7chen\u00b7chor", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Sei denn, da der alte H\u00e4rmer,", "tokens": ["Sei", "denn", ",", "da", "der", "al\u00b7te", "H\u00e4r\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da der Gram des Winters wich,", "tokens": ["Da", "der", "Gram", "des", "Win\u00b7ters", "wich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sei der erste Sang der Schw\u00e4rmer", "tokens": ["Sei", "der", "ers\u00b7te", "Sang", "der", "Schw\u00e4r\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dir geweiht \u2013 wie nenn' ich dich?", "tokens": ["Dir", "ge\u00b7weiht", "\u2013", "wie", "nenn'", "ich", "dich", "?"], "token_info": ["word", "word", "punct", "