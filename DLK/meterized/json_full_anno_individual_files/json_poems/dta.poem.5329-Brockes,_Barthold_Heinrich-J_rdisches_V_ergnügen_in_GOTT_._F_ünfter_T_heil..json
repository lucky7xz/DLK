{"dta.poem.5329": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Doch flie\u00dft aus der Erk\u00e4nntni\u00df nicht,", "tokens": ["Doch", "flie\u00dft", "aus", "der", "Er\u00b7k\u00e4nnt\u00b7ni\u00df", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df, da ich alles nicht kan fassen,", "tokens": ["Da\u00df", ",", "da", "ich", "al\u00b7les", "nicht", "kan", "fas\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPER", "PIS", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich alles auch mu\u00df unterlassen;", "tokens": ["Ich", "al\u00b7les", "auch", "mu\u00df", "un\u00b7ter\u00b7las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ach nein!", "tokens": ["Ach", "nein", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKANT", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Vernunft und Hofnung spricht:", "tokens": ["Ver\u00b7nunft", "und", "Hof\u00b7nung", "spricht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Auch durch Bewunderung allein,", "tokens": ["Auch", "durch", "Be\u00b7wun\u00b7de\u00b7rung", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn auch ", "tokens": ["Wenn", "auch"], "token_info": ["word", "word"], "pos": ["KOUS", "ADV"], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Wird unserm GOTT ein Lob bereitet.", "tokens": ["Wird", "un\u00b7serm", "GoTT", "ein", "Lob", "be\u00b7rei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}