{"textgrid.poem.40647": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oed' ist die\u00df Eiland, baumlos, windversengt,", "tokens": ["O\u00b7ed'", "ist", "die\u00df", "Ei\u00b7land", ",", "baum\u00b7los", ",", "wind\u00b7ver\u00b7sengt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "VAFIN", "PDS", "NN", "$,", "ADJD", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Die starre Burg und Warte der Orkane;", "tokens": ["Die", "star\u00b7re", "Burg", "und", "War\u00b7te", "der", "Or\u00b7ka\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bleifarbig um die morschen Zinnen h\u00e4ngt", "tokens": ["Blei\u00b7far\u00b7big", "um", "die", "mor\u00b7schen", "Zin\u00b7nen", "h\u00e4ngt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das Nordgew\u00f6lk, wie eine graue Fahne.", "tokens": ["Das", "Nord\u00b7ge\u00b7w\u00f6lk", ",", "wie", "ei\u00b7ne", "grau\u00b7e", "Fah\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Lenzschwalbe flieht, aus ihrem Nest verdr\u00e4ngt,", "tokens": ["Lenz\u00b7schwal\u00b7be", "flieht", ",", "aus", "ih\u00b7rem", "Nest", "ver\u00b7dr\u00e4ngt", ","], "token_info": ["word", "word", "punct", "