{"textgrid.poem.38908": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Warum die Blume das K\u00f6pfchen senkt,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Warum die Blume das K\u00f6pfchen senkt,", "tokens": ["Wa\u00b7rum", "die", "Blu\u00b7me", "das", "K\u00f6pf\u00b7chen", "senkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Warum die Rosen so bla\u00df?", "tokens": ["Wa\u00b7rum", "die", "Ro\u00b7sen", "so", "bla\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ach! die Thr\u00e4ne am Blatt der Lilie h\u00e4ngt,", "tokens": ["Ach", "!", "die", "Thr\u00e4\u00b7ne", "am", "Blatt", "der", "Li\u00b7lie", "h\u00e4ngt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ART", "NN", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Vergangen das sch\u00f6n frische Gras.", "tokens": ["Ver\u00b7gan\u00b7gen", "das", "sch\u00f6n", "fri\u00b7sche", "Gras", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.5": {"text": "Die Blumen erbleichen,", "tokens": ["Die", "Blu\u00b7men", "er\u00b7blei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Die Farben entweichen,", "tokens": ["Die", "Far\u00b7ben", "ent\u00b7wei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Denn sie, denn sie ist weit", "tokens": ["Denn", "sie", ",", "denn", "sie", "ist", "weit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "KON", "PPER", "VAFIN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Die allerholdseeligste Maid.", "tokens": ["Die", "al\u00b7ler\u00b7hold\u00b7see\u00b7ligs\u00b7te", "Maid", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Keine Anmuth auf dem Feld,", "tokens": ["Kei\u00b7ne", "An\u00b7muth", "auf", "dem", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Keine s\u00fc\u00dfe Bl\u00fcthe am Baume mehr,", "tokens": ["Kei\u00b7ne", "s\u00fc\u00b7\u00dfe", "Bl\u00fc\u00b7the", "am", "Bau\u00b7me", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "APPRART", "NN", "ADV", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Die Farben, die T\u00f6ne durchstreifen die Welt", "tokens": ["Die", "Far\u00b7ben", ",", "die", "T\u00f6\u00b7ne", "durch\u00b7strei\u00b7fen", "die", "Welt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und suchen die Sch\u00f6nste weit umher.", "tokens": ["Und", "su\u00b7chen", "die", "Sch\u00f6ns\u00b7te", "weit", "um\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Unser Thal ist leer", "tokens": ["Un\u00b7ser", "Thal", "ist", "leer"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Bis zur Wiederkehr,", "tokens": ["Bis", "zur", "Wie\u00b7der\u00b7kehr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Ach! bringt sie gefesselt in Sch\u00f6ne", "tokens": ["Ach", "!", "bringt", "sie", "ge\u00b7fes\u00b7selt", "in", "Sch\u00f6\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "VVPP", "APPR", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Zur\u00fccke ihr Farben, ihr T\u00f6ne.", "tokens": ["Zu\u00b7r\u00fc\u00b7cke", "ihr", "Far\u00b7ben", ",", "ihr", "T\u00f6\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Regenbogen leuchtet voran", "tokens": ["Re\u00b7gen\u00b7bo\u00b7gen", "leuch\u00b7tet", "vo\u00b7ran"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "PTKVZ"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und Blumen folgen ihm nach,", "tokens": ["Und", "Blu\u00b7men", "fol\u00b7gen", "ihm", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Nacht'gall singt auf der Bahn,", "tokens": ["Nacht'\u00b7gall", "singt", "auf", "der", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Rieselt der silberne Bach:", "tokens": ["Rie\u00b7selt", "der", "sil\u00b7ber\u00b7ne", "Bach", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Thun als w\u00e4re der Fr\u00fchling vergangen,", "tokens": ["Thun", "als", "w\u00e4\u00b7re", "der", "Fr\u00fch\u00b7ling", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Doch bringen sie sie nur gefangen,", "tokens": ["Doch", "brin\u00b7gen", "sie", "sie", "nur", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wird Fr\u00fchling aus dem Herbst alsbald,", "tokens": ["Wird", "Fr\u00fch\u00b7ling", "aus", "dem", "Herbst", "als\u00b7bald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Herrscht \u00fcber uns kein Winter kalt.", "tokens": ["Herrscht", "\u00fc\u00b7ber", "uns", "kein", "Win\u00b7ter", "kalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach! ihr findet sie nicht, ihr findet sie nicht,", "tokens": ["Ach", "!", "ihr", "fin\u00b7det", "sie", "nicht", ",", "ihr", "fin\u00b7det", "sie", "nicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Habt kein Auge, die Sch\u00f6nste zu suchen,", "tokens": ["Habt", "kein", "Au\u00b7ge", ",", "die", "Sch\u00f6ns\u00b7te", "zu", "su\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Euch mangelt der Liebe Augenlicht,", "tokens": ["Euch", "man\u00b7gelt", "der", "Lie\u00b7be", "Au\u00b7gen\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ihr erm\u00fcdet \u00fcber dem Suchen.", "tokens": ["Ihr", "er\u00b7m\u00fc\u00b7det", "\u00fc\u00b7ber", "dem", "Su\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Treibt wie Blumen die Sache als fr\u00f6hlichen Scherz,", "tokens": ["Treibt", "wie", "Blu\u00b7men", "die", "Sa\u00b7che", "als", "fr\u00f6h\u00b7li\u00b7chen", "Scherz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "NN", "ART", "NN", "KOUS", "ADJA", "NN", "$,"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Ach! nehmet mein Herz,", "tokens": ["Ach", "!", "neh\u00b7met", "mein", "Herz", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Damit nach dem holden Engelskinde", "tokens": ["Da\u00b7mit", "nach", "dem", "hol\u00b7den", "En\u00b7gels\u00b7kin\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Der Fr\u00fchling den Weg gewi\u00dflich finde.", "tokens": ["Der", "Fr\u00fch\u00b7ling", "den", "Weg", "ge\u00b7wi\u00df\u00b7lich", "fin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Und habt ihr Kinder entdeckt die Spur,", "tokens": ["Und", "habt", "ihr", "Kin\u00b7der", "ent\u00b7deckt", "die", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "VVPP", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "O, so h\u00f6rt, o, so h\u00f6rt mein \u00e4ngstlich Flehn,", "tokens": ["O", ",", "so", "h\u00f6rt", ",", "o", ",", "so", "h\u00f6rt", "mein", "\u00e4ngst\u00b7lich", "Flehn", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "$,", "FM", "$,", "ADV", "VVFIN", "PPOSAT", "ADJD", "VVINF", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "M\u00fc\u00dft nicht zu tief in die Augen ihr sehn,", "tokens": ["M\u00fc\u00dft", "nicht", "zu", "tief", "in", "die", "Au\u00b7gen", "ihr", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PTKA", "ADJD", "APPR", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Ihre Blicke bezaubern, verblenden euch nur.", "tokens": ["Ih\u00b7re", "Bli\u00b7cke", "be\u00b7zau\u00b7bern", ",", "ver\u00b7blen\u00b7den", "euch", "nur", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Kein Wesen vor ihr besteht,", "tokens": ["Kein", "We\u00b7sen", "vor", "ihr", "be\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "All's in Liebe vergeht,", "tokens": ["All's", "in", "Lie\u00b7be", "ver\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Mag nichts anders mehr sein", "tokens": ["Mag", "nichts", "an\u00b7ders", "mehr", "sein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "ADV", "ADV", "VAINF"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Als ihre Lieb' allein.", "tokens": ["Als", "ih\u00b7re", "Lieb'", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Bedenkt, da\u00df Fr\u00fchling und Blumenglanz", "tokens": ["Be\u00b7denkt", ",", "da\u00df", "Fr\u00fch\u00b7ling", "und", "Blu\u00b7men\u00b7glanz"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "NN", "KON", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wo ihr Fu\u00df wandelt, immer schon ist,", "tokens": ["Wo", "ihr", "Fu\u00df", "wan\u00b7delt", ",", "im\u00b7mer", "schon", "ist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$,", "ADV", "ADV", "VAFIN", "$,"], "meter": "--++-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Kommt zu mir zur\u00fcck mit leichtem Tanz,", "tokens": ["Kommt", "zu", "mir", "zu\u00b7r\u00fcck", "mit", "leich\u00b7tem", "Tanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Da\u00df Fr\u00fchling und Nacht'gall doch um mich ist;", "tokens": ["Da\u00df", "Fr\u00fch\u00b7ling", "und", "Nacht'\u00b7gall", "doch", "um", "mich", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "ADV", "APPR", "PPER", "VAFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Mu\u00df dann sp\u00e4t und fr\u00fch", "tokens": ["Mu\u00df", "dann", "sp\u00e4t", "und", "fr\u00fch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ADJD", "KON", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Mich behelfen ohne sie,", "tokens": ["Mich", "be\u00b7hel\u00b7fen", "oh\u00b7ne", "sie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Mit bitters\u00fc\u00dfen Liebesthr\u00e4nen", "tokens": ["Mit", "bit\u00b7ter\u00b7s\u00fc\u00b7\u00dfen", "Lie\u00b7best\u00b7hr\u00e4\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mich einsam nach der Sch\u00f6nsten sehnen.", "tokens": ["Mich", "ein\u00b7sam", "nach", "der", "Sch\u00f6ns\u00b7ten", "seh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Aber bleibt, aber bleibt nur wo ihr seid,", "tokens": ["A\u00b7ber", "bleibt", ",", "a\u00b7ber", "bleibt", "nur", "wo", "ihr", "seid", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADV", "VVFIN", "ADV", "PWAV", "PPER", "VAFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Mag euch auch ohne sie nicht wiedersehn,", "tokens": ["Mag", "euch", "auch", "oh\u00b7ne", "sie", "nicht", "wie\u00b7der\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Blumen und Fr\u00fchlingston wird Herzeleid,", "tokens": ["Blu\u00b7men", "und", "Fr\u00fch\u00b7lings\u00b7ton", "wird", "Her\u00b7ze\u00b7leid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Will inde\u00df hier im bittersten Tode vergehn.", "tokens": ["Will", "in\u00b7de\u00df", "hier", "im", "bit\u00b7ters\u00b7ten", "To\u00b7de", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-----+--+--+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Mich selber zu strafen,", "tokens": ["Mich", "sel\u00b7ber", "zu", "stra\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Im Grabe tief schlafen,", "tokens": ["Im", "Gra\u00b7be", "tie