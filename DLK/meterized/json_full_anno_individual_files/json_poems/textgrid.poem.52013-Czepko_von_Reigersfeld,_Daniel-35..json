{"textgrid.poem.52013": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "35.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann uns der k\u00fchle Wein die d\u00fcrren Lippen netzt,", "tokens": ["Wann", "uns", "der", "k\u00fch\u00b7le", "Wein", "die", "d\u00fcr\u00b7ren", "Lip\u00b7pen", "netzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und man die Zunge sieht im goldnen Bade schwimmen,", "tokens": ["Und", "man", "die", "Zun\u00b7ge", "sieht", "im", "gold\u00b7nen", "Ba\u00b7de", "schwim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und jeder seine Wort aus freyem Hertzen setzt,", "tokens": ["Und", "je\u00b7der", "sei\u00b7ne", "Wort", "aus", "frey\u00b7em", "Hert\u00b7zen", "setzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So l\u00e4st auch deine Treu der Freundschafft Lunten glimmen.", "tokens": ["So", "l\u00e4st", "auch", "dei\u00b7ne", "Treu", "der", "Freund\u00b7schafft", "Lun\u00b7ten", "glim\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jedoch wann Schlaff und Rausch aus Haubt und Augen gehn,", "tokens": ["Je\u00b7doch", "wann", "Schlaff", "und", "Rausch", "aus", "Haubt", "und", "Au\u00b7gen", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "NN", "KON", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So ist es bey dem Trunck ohn Hertz und Hand geschehen:", "tokens": ["So", "ist", "es", "bey", "dem", "Trunck", "ohn", "Hertz", "und", "Hand", "ge\u00b7sche\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "La\u00df deine Freundschafft auch bey n\u00fcchterm Munde sehn,", "tokens": ["La\u00df", "dei\u00b7ne", "Freund\u00b7schafft", "auch", "bey", "n\u00fcch\u00b7term", "Mun\u00b7de", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "ADV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sonst steht es \u00fcbel an: Viel sagen, nichts gestehen.", "tokens": ["Sonst", "steht", "es", "\u00fc\u00b7bel", "an", ":", "Viel", "sa\u00b7gen", ",", "nichts", "ge\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$.", "ADV", "VVINF", "$,", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wann uns der k\u00fchle Wein die d\u00fcrren Lippen netzt,", "tokens": 