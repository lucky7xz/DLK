{"dta.poem.23449": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Pfaffen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr seid nicht Christen, seid nur Pfaffen,", "tokens": ["Ihr", "seid", "nicht", "Chris\u00b7ten", ",", "seid", "nur", "Pfaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "NN", "$,", "VAFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Seid nicht des Heilands Ebenbild;", "tokens": ["Seid", "nicht", "des", "Hei\u00b7lands", "E\u00b7ben\u00b7bild", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr f\u00fchret nicht der Liebe Waffen,", "tokens": ["Ihr", "f\u00fch\u00b7ret", "nicht", "der", "Lie\u00b7be", "Waf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und traget nicht der Demuth Schild.", "tokens": ["Und", "tra\u00b7get", "nicht", "der", "De\u00b7muth", "Schild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Heiland hat der Welt den Frieden,", "tokens": ["Der", "Hei\u00b7land", "hat", "der", "Welt", "den", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und nur der S\u00fcnde Krieg gebracht:", "tokens": ["Und", "nur", "der", "S\u00fcn\u00b7de", "Krieg", "ge\u00b7bracht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr aber habt zum Krieg hienieden", "tokens": ["Ihr", "a\u00b7ber", "habt", "zum", "Krieg", "hien\u00b7ie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die ganze Menschheit angefacht.", "tokens": ["Die", "gan\u00b7ze", "Menschheit", "an\u00b7ge\u00b7facht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Ihr kreuzigt t\u00e4glich noch den Heiland,", "tokens": ["Ihr", "kreu\u00b7zigt", "t\u00e4g\u00b7lich", "noch", "den", "Hei\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erschien' er wie er einst erschien,", "tokens": ["Er\u00b7schien'", "er", "wie", "er", "einst", "er\u00b7schien", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr riefet wie die Juden weiland", "tokens": ["Ihr", "rie\u00b7fet", "wie", "die", "Ju\u00b7den", "wei\u00b7land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und lauter nur: ha, kreuzigt ihn!", "tokens": ["Und", "lau\u00b7ter", "nur", ":", "ha", ",", "kreu\u00b7zigt", "ihn", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "$.", "ITJ", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}