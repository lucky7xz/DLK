{"textgrid.poem.41072": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Das Schnellste", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was kommt gar bald daher?", "tokens": ["Was", "kommt", "gar", "bald", "da\u00b7her", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ADV", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ein Wagen, dran sechs Pferde,", "tokens": ["Ein", "Wa\u00b7gen", ",", "dran", "sechs", "Pfer\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PAV", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Fliegt donnernd \u00fcber die Erde,", "tokens": ["Fliegt", "don\u00b7nernd", "\u00fc\u00b7ber", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Staubwolken hinter ihm her.", "tokens": ["Staub\u00b7wol\u00b7ken", "hin\u00b7ter", "ihm", "her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Was kommt noch b\u00e4lder daher?", "tokens": ["Was", "kommt", "noch", "b\u00e4l\u00b7der", "da\u00b7her", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ADV", "PAV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Der sonderbare Wagen,", "tokens": ["Der", "son\u00b7der\u00b7ba\u00b7re", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measu