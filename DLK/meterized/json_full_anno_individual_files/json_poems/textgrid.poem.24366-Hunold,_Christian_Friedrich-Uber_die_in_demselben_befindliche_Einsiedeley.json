{"textgrid.poem.24366": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Uber die in demselben befindliche Einsiedeley", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwar recht nat\u00fcrlich wohl; doch strenger hier zu leben/", "tokens": ["Zwar", "recht", "na\u00b7t\u00fcr\u00b7lich", "wohl", ";", "doch", "stren\u00b7ger", "hier", "zu", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "ADV", "$.", "ADV", "ADJD", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als in der W\u00fcsteney. Dort darf man um sich sehn/", "tokens": ["Als", "in", "der", "W\u00fcs\u00b7te\u00b7ney", ".", "Dort", "darf", "man", "um", "sich", "sehn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "$.", "ADV", "VMFIN", "PIS", "APPR", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hier aber keinen Schritt von seiner H\u00f6hle gehn/", "tokens": ["Hier", "a\u00b7ber", "kei\u00b7nen", "Schritt", "von", "sei\u00b7ner", "H\u00f6h\u00b7le", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So ist man schon mit dem/ wovor man flieht umgeben.", "tokens": ["So", "ist", "man", "schon", "mit", "dem", "/", "wo\u00b7vor", "man", "flieht", "um\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "APPR", "ART", "$(", "PWAV", "PIS", "VVFIN", "VVPP", "$."], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.5": {"text": "Wer hier zu leben denckt/ und nie aus Reitzung f\u00e4llt/", "tokens": ["Wer", "hier", "zu", "le\u00b7ben", "denckt", "/", "und", "nie", "aus", "Reit\u00b7zung", "f\u00e4llt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PTKZU", "VVINF", "VVFIN", "$(", "KON", "ADV", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der kan sich \u00fcberall als einem Weisen trauen/", "tokens": ["Der", "kan", "sich", "\u00fc\u00b7be\u00b7rall", "als", "ei\u00b7nem", "Wei\u00b7sen", "trau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PRF", "ADV", "KOUS", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und wie in Einsamkeit hier eine sch\u00f6ne Welt/", "tokens": ["Und", "wie", "in", "Ein\u00b7sam\u00b7keit", "hier", "ei\u00b7ne", "sch\u00f6\u00b7ne", "Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPR", "NN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So in der sch\u00f6nsten Welt die Einsamkeit beschauen.", "tokens": ["So", "in", "der", "sch\u00f6ns\u00b7ten", "Welt", "die", "Ein\u00b7sam\u00b7keit", "be\u00b7schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Zwar recht nat\u00fcrlich wohl; doch strenger hier zu le