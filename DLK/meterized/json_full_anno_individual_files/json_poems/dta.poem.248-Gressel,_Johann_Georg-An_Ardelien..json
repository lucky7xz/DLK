{"dta.poem.248": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An  Ardelien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Sinnen/ die sich selbst nicht kennen/", "tokens": ["Sin\u00b7nen", "/", "die", "sich", "selbst", "nicht", "ken\u00b7nen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PRELS", "PRF", "ADV", "PTKNEG", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind ", "tokens": ["Sind"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Dir zu zeigen Feur und Brennen/", "tokens": ["Dir", "zu", "zei\u00b7gen", "Feur", "und", "Bren\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "NN", "KON", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das in meinem Hertzen gl\u00fcht/", "tokens": ["Das", "in", "mei\u00b7nem", "Hert\u00b7zen", "gl\u00fcht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "So dein feurig Augen-Strahl", "tokens": ["So", "dein", "feu\u00b7rig", "Au\u00b7gen\u00b7Strahl"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Halb zur Freude und zur Quaal", "tokens": ["Halb", "zur", "Freu\u00b7de", "und", "zur", "Qua\u00b7al"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "KON", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Hat darinnen angez\u00fcndet/", "tokens": ["Hat", "da\u00b7rin\u00b7nen", "an\u00b7ge\u00b7z\u00fcn\u00b7det", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df es nichts als Flammen findet.", "tokens": ["Da\u00df", "es", "nichts", "als", "Flam\u00b7men", "fin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "KOKOM", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aber! ach wo ist mein Hertze?", "tokens": ["A\u00b7ber", "!", "ach", "wo", "ist", "mein", "Hert\u00b7ze", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$.", "XY", "PWAV", "VAFIN", "PPOSAT", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist es noch in meiner Brust?", "tokens": ["Ist", "es", "noch", "in", "mei\u00b7ner", "Brust", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nein! ich f\u00fchle/ da\u00df mein Schmertze", "tokens": ["Nein", "!", "ich", "f\u00fch\u00b7le", "/", "da\u00df", "mein", "Schmert\u00b7ze"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$.", "PPER", "VVFIN", "$(", "KOUS", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deutlich zeiget den Verlust.", "tokens": ["Deut\u00b7lich", "zei\u00b7get", "den", "Ver\u00b7lust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Du hast es/ mein Licht/ bey dir/", "tokens": ["Du", "hast", "es", "/", "mein", "Licht", "/", "bey", "dir", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$(", "PPOSAT", "NN", "$(", "APPR", "PPER", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und die Schaalen sind nur mir", "tokens": ["Und", "die", "Schaa\u00b7len", "sind", "nur", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Blo\u00df zum Leben \u00fcberblieben/", "tokens": ["Blo\u00df", "zum", "Le\u00b7ben", "\u00fc\u00b7berb\u00b7lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da der Kern dir ist verschrieben.", "tokens": ["Da", "der", "Kern", "dir", "ist", "ver\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wer nun so/ wie ich/ mu\u00df leben/", "tokens": ["Wer", "nun", "so", "/", "wie", "ich", "/", "mu\u00df", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "$(", "PWAV", "PPER", "$(", "VMFIN", "VVINF", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der brennt/ stirbt/ und lebt zugleich/", "tokens": ["Der", "brennt", "/", "stirbt", "/", "und", "lebt", "zu\u00b7gleich", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$(", "VVFIN", "$(", "KON", "VVFIN", "ADV", "$("], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "In Verwirrung mu\u00df er schweben/", "tokens": ["In", "Ver\u00b7wir\u00b7rung", "mu\u00df", "er", "schwe\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An Vergn\u00fcgen arm und reich", "tokens": ["An", "Ver\u00b7gn\u00fc\u00b7gen", "arm", "und", "reich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "An dem Schmertzen der ihn plagt/", "tokens": ["An", "dem", "Schmert\u00b7zen", "der", "ihn", "plagt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "PPER", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und die heissen Sinnen nagt.", "tokens": ["Und", "die", "heis\u00b7sen", "Sin\u00b7nen", "nagt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Er kan keine Stunde z\u00e4hlen/", "tokens": ["Er", "kan", "kei\u00b7ne", "Stun\u00b7de", "z\u00e4h\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "VVINF", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Die nicht dreut ein neues Qu\u00e4hlen.", "tokens": ["Die", "nicht", "dreut", "ein", "neu\u00b7es", "Qu\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Deiner Freundlichkeiten-Sterne/", "tokens": ["Dei\u00b7ner", "Freund\u00b7lich\u00b7kei\u00b7ten\u00b7S\u00b7ter\u00b7ne", "/"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der beliebten Augen-Licht/", "tokens": ["Der", "be\u00b7lieb\u00b7ten", "Au\u00b7gen\u00b7Licht", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Treiben meine Freyheit ferne/", "tokens": ["Trei\u00b7ben", "mei\u00b7ne", "Frey\u00b7heit", "fer\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADJA", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die gar nicht darwider ficht;", "tokens": ["Die", "gar", "nicht", "dar\u00b7wi\u00b7der", "ficht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PTKNEG", "PAV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sie wil gern in Banden stehn/", "tokens": ["Sie", "wil", "gern", "in", "Ban\u00b7den", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "NN", "VVINF", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und mich einen Sclaven sehn.", "tokens": ["Und", "mich", "ei\u00b7nen", "Scla\u00b7ven", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Solcher angenehmen Augen/", "tokens": ["Sol\u00b7cher", "an\u00b7ge\u00b7neh\u00b7men", "Au\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die zum Brand und L\u00f6schen taugen.", "tokens": ["Die", "zum", "Brand", "und", "L\u00f6\u00b7schen", "tau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sch\u00f6ne da mein Hertz nun deine/", "tokens": ["Sch\u00f6\u00b7ne", "da", "mein", "Hertz", "nun", "dei\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "ADV", "PPOSAT", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und in deinen Br\u00fcsten ist/", "tokens": ["Und", "in", "dei\u00b7nen", "Br\u00fcs\u00b7ten", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VAFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Werd ich fast zum kalten Steine/", "tokens": ["Werd", "ich", "fast", "zum", "kal\u00b7ten", "Stei\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo mich nicht dein Auge gr\u00fc\u00dft/", "tokens": ["Wo", "mich", "nicht", "dein", "Au\u00b7ge", "gr\u00fc\u00dft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und durch einem sanfften Blick", "tokens": ["Und", "durch", "ei\u00b7nem", "sanff\u00b7ten", "Blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Bringet meinen Geist zur\u00fcck/", "tokens": ["Brin\u00b7get", "mei\u00b7nen", "Geist", "zu\u00b7r\u00fcck", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Der in deinem Hertzen wohnet/", "tokens": ["Der", "in", "dei\u00b7nem", "Hert\u00b7zen", "woh\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und der Br\u00fcste Pracht bethronet.", "tokens": ["Und", "der", "Br\u00fcs\u00b7te", "Pracht", "be\u00b7th\u00b7ro\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Schencke mir mein Hertze wieder/", "tokens": ["Schen\u00b7cke", "mir", "mein", "Hert\u00b7ze", "wie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "VVFIN", "ADV", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber deines mit dabey/", "tokens": ["A\u00b7ber", "dei\u00b7nes", "mit", "da\u00b7bey", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "APPR", "PAV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "G\u00f6nn/ da\u00df meinen Augen-Lieder", "tokens": ["G\u00f6nn", "/", "da\u00df", "mei\u00b7nen", "Au\u00b7gen\u00b7Lie\u00b7der"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$(", "KOUS", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine Brust ein Polster sey:", "tokens": ["Dei\u00b7ne", "Brust", "ein", "Pols\u00b7ter", "sey", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lege Feur zu Holtz und Stroh/", "tokens": ["Le\u00b7ge", "Feur", "zu", "Holtz", "und", "Stroh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "NN", "KON", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mach mein traurges Hertze froh;", "tokens": ["Mach", "mein", "traur\u00b7ges", "Hert\u00b7ze", "froh", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Liebe/ da ich dich verehre/", "tokens": ["Lie\u00b7be", "/", "da", "ich", "dich", "ver\u00b7eh\u00b7re", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOUS", "PPER", "PRF", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und mein Feur mit Flammen mehre.", "tokens": ["Und", "mein", "Feur", "mit", "Flam\u00b7men", "meh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Deiner Augen Heyl ", "tokens": ["Dei\u00b7ner", "Au\u00b7gen", "Heyl"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Mir ein neues Hertz zuf\u00fcgt/", "tokens": ["Mir", "ein", "neu\u00b7es", "Hertz", "zu\u00b7f\u00fcgt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich halte Lust-", "tokens": ["Und", "ich", "hal\u00b7te", "Lust"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "TRUNC"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Wenn auf deiner Brust es liegt.", "tokens": ["Wenn", "auf", "dei\u00b7ner", "Brust", "es", "liegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn ", "tokens": ["Wenn"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Scheinet mit beliebtem Licht", "tokens": ["Schei\u00b7net", "mit", "be\u00b7lieb\u00b7tem", "Licht"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "So vergeht den Hertzens-Mauren", "tokens": ["So", "ver\u00b7geht", "den", "Hert\u00b7zens\u00b7Mau\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Jhr verd\u00fcstert Liebes-Trauren.", "tokens": ["Ihr", "ver\u00b7d\u00fcs\u00b7tert", "Lie\u00b7bes\u00b7Trau\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Drum mein Engel kenn die Flam\u0303en/", "tokens": ["Drum", "mein", "En\u00b7gel", "kenn", "die", "Flam\u0303en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die dein Auge mir erregt;", "tokens": ["Die", "dein", "Au\u00b7ge", "mir", "er\u00b7regt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und weil sie von dir herstammen", "tokens": ["Und", "weil", "sie", "von", "dir", "her\u00b7stam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "PPER", "VVINF"], "meter": "--+-+-+-", "measur