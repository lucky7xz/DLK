{"textgrid.poem.33058": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dies war mir oft ein Trost im Leben,", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dies war mir oft ein Trost im Leben,", "tokens": ["Dies", "war", "mir", "oft", "ein", "Trost", "im", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Gl\u00fcck, das Keiner rauben wird \u2013", "tokens": ["Ein", "Gl\u00fcck", ",", "das", "Kei\u00b7ner", "rau\u00b7ben", "wird", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "VVINF", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df sich noch niemals eine Seele", "tokens": ["Da\u00df", "sich", "noch", "nie\u00b7mals", "ei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In meinem Wesen hat geirrt.", "tokens": ["In", "mei\u00b7nem", "We\u00b7sen", "hat", "ge\u00b7irrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Verrathen manchmal, oft betrogen,", "tokens": ["Ver\u00b7ra\u00b7then", "manch\u00b7mal", ",", "oft", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kenn' ich es tief, dies bitt're Leid,", "tokens": ["Kenn'", "ich", "es", "tief", ",", "dies", "bitt'\u00b7re", "Leid", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADJD", "$,", "PDS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch da\u00df ich niemals selbst gelogen,", "tokens": ["Doch", "da\u00df", "ich", "nie\u00b7mals", "selbst", "ge\u00b7lo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bleibt Balsam mir f\u00fcr alle Zeit! \u2013", "tokens": ["Bleibt", "Bal\u00b7sam", "mir", "f\u00fcr", "al\u00b7le", "Zeit", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "NE", "PPER", "APPR", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich gleiche nicht der Sonne Schimmer,", "tokens": ["Ich", "glei\u00b7che", "nicht", "der", "Son\u00b7ne", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bin nicht so reich an Glanz und Licht,", "tokens": ["Bin", "nicht", "so", "reich", "an", "Glanz", "und", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Sternlein nur, de\u00df schwacher Flimmer,", "tokens": ["Ein", "Stern\u00b7lein", "nur", ",", "de\u00df", "schwa\u00b7cher", "Flim\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein kleines, enges Rund umflicht.", "tokens": ["Ein", "klei\u00b7nes", ",", "en\u00b7ges", "Rund", "um\u00b7flicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch, wie die Stolze mitteninnen", "tokens": ["Doch", ",", "wie", "die", "Stol\u00b7ze", "mit\u00b7te\u00b7nin\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gl\u00e4nzt wandellos am Himmelszelt,", "tokens": ["Gl\u00e4nzt", "wan\u00b7del\u00b7los", "am", "Him\u00b7mels\u00b7zelt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So steh' ich fest in meinem Sinnen,", "tokens": ["So", "steh'", "ich", "fest", "in", "mei\u00b7nem", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ohne Trug kennt mich die Welt.", "tokens": ["Und", "oh\u00b7ne", "Trug", "kennt", "mich", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und wer mit liebendem Vertrauen", "tokens": ["Und", "wer", "mit", "lie\u00b7ben\u00b7dem", "Ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "APPR", "ADJA", "NN"], "meter": "-+-+-