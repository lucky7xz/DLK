{"textgrid.poem.36515": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "14. Der Dichter und ein Fuchs", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr Dichter, sprach ein Fuchs, der an der Kette lag,", "tokens": ["Herr", "Dich\u00b7ter", ",", "sprach", "ein", "Fuchs", ",", "der", "an", "der", "Ket\u00b7te", "lag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VVFIN", "ART", "NE", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich bitte, la\u00dft mich los! ich will ein St\u00fcckchen machen,", "tokens": ["Ich", "bit\u00b7te", ",", "la\u00dft", "mich", "los", "!", "ich", "will", "ein", "St\u00fcck\u00b7chen", "ma\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVIMP", "PPER", "PTKVZ", "$.", "PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ihr sollt dar\u00fcber lachen;", "tokens": ["Ihr", "sollt", "da\u00b7r\u00fc\u00b7ber", "la\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PAV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nur heut' auf einen Tag!", "tokens": ["Nur", "heut'", "auf", "ei\u00b7nen", "Tag", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Auf einen Augenblick", "tokens": ["Auf", "ei\u00b7nen", "Au\u00b7gen\u00b7blick"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "D\u00fcrft es nur sein, du Schalk! so lachtest du der Kette.", "tokens": ["D\u00fcrft", "es", "nur", "sein", ",", "du", "Schalk", "!", "so", "lach\u00b7test", "du", "der", "Ket\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "VAINF", "$,", "PPER", "NN", "$.", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ja! wer von dir nicht schon so manches Schelmenst\u00fcck", "tokens": ["Ja", "!", "wer", "von", "dir", "nicht", "schon", "so", "man\u00b7ches", "Schel\u00b7men\u00b7st\u00fcck"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "PWS", "APPR", "PPER", "PTKNEG", "ADV", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Geh\u00f6ret und gelesen h\u00e4tte!", "tokens": ["Ge\u00b7h\u00f6\u00b7ret", "und", "ge\u00b7le\u00b7sen", "h\u00e4t\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du bist so schlau, so schlau! so listig, da\u00df man dich", "tokens": ["Du", "bist", "so", "schlau", ",", "so", "schlau", "!", "so", "lis\u00b7tig", ",", "da\u00df", "man", "dich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$.", "ADV", "ADJD", "$,", "KOUS", "PIS", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Fest h\u00e4lt, wenn man dich hat; die Kunst ist, dich zu kriegen!", "tokens": ["Fest", "h\u00e4lt", ",", "wenn", "man", "dich", "hat", ";", "die", "Kunst", "ist", ",", "dich", "zu", "krie\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "KOUS", "PIS", "PPER", "VAFIN", "$.", "ART", "NN", "VAFIN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Darum, du Vogel, du! wer klug ist, tr\u00f6stet sich", "tokens": ["Da\u00b7rum", ",", "du", "Vo\u00b7gel", ",", "du", "!", "wer", "klug", "ist", ",", "tr\u00f6s\u00b7tet", "sich"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PAV", "$,", "PPER", "NE", "$,", "PPER", "$.", "PWS", "ADJD", "VAFIN", "$,", "VVFIN", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "An seiner Kette selbst, und bleibt geduldig liegen.", "tokens": ["An", "sei\u00b7ner", "Ket\u00b7te", "selbst", ",", "und", "bleibt", "ge\u00b7dul\u00b7dig", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$,", "KON", "VVFIN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Herr Dichter, sprach ein Fuchs, der an der Kette lag,", "tokens": ["Herr", "Dich\u00b7ter", ",", "sprach", "ein", "Fuchs", ",", "der", "an", "der", "Ket\u00b7te", "lag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VVFIN", "ART", "NE", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich bitte, la\u00dft mich los! ich will ein St\u00fcckchen machen,", "tokens": ["Ich"