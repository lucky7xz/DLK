{"dta.poem.22749": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "52. Umb einen Ku\u00dfists GOtt zuthun.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Was wil doch GOttes Sohn da\u00df Er ins Elend", "tokens": ["Was", "wil", "doch", "Got\u00b7tes", "Sohn", "da\u00df", "Er", "ins", "E\u00b7lend"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "ADV", "NN", "NN", "KOUS", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "t\u00f6mbt/", "tokens": ["t\u00f6mbt", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Und ein solch schweres Kreutz auf seine Schultern", "tokens": ["Und", "ein", "solch", "schwe\u00b7res", "Kreutz", "auf", "sei\u00b7ne", "Schul\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "PIAT", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "nimbt?", "tokens": ["nimbt", "?"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Ja da\u00df Er bi\u00df in Tod sich \u00e4ngstet f\u00fcr und f\u00fcr?", "tokens": ["Ja", "da\u00df", "Er", "bi\u00df", "in", "Tod", "sich", "\u00e4ngs\u00b7tet", "f\u00fcr", "und", "f\u00fcr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "KOUS", "PPER", "ADV", "APPR", "NN", "PRF", "VVFIN", "APPR", "KON", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Er suchet anders nichts als einen Ku\u00df von dir.", "tokens": ["Er", "su\u00b7chet", "an\u00b7ders", "nichts", "als", "ei\u00b7nen", "Ku\u00df", "von", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIS", "KOKOM", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}