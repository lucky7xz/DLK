{"dta.poem.5289": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Anderweitige Betrachtung der  \n Kirsch-Bl\u00fcthe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein GOtt, da ich hier stille stehe,", "tokens": ["Mein", "Gott", ",", "da", "ich", "hier", "stil\u00b7le", "ste\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "PPER", "ADV", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und, mit f\u00fcr Lust erstarrten Blicken,", "tokens": ["Und", ",", "mit", "f\u00fcr", "Lust", "er\u00b7starr\u00b7ten", "Bli\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Bl\u00fcthe, womit sich die Kirschen-B\u00e4ume schm\u00fccken,", "tokens": ["Die", "Bl\u00fc\u00b7the", ",", "wo\u00b7mit", "sich", "die", "Kir\u00b7schen\u00b7B\u00e4u\u00b7me", "schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit billiger Aufmercksamheit, besehe;", "tokens": ["Mit", "bil\u00b7li\u00b7ger", "Auf\u00b7merck\u00b7sam\u00b7heit", ",", "be\u00b7se\u00b7he", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Entdeck\u2019 ich auf das neu und mercke", "tokens": ["Ent\u00b7deck'", "ich", "auf", "das", "neu", "und", "mer\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "ART", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Noch nie bemerckte Wunder-Wercke,", "tokens": ["Noch", "nie", "be\u00b7merck\u00b7te", "Wun\u00b7der\u00b7\u00b7Wer\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die, da\u00df man deine Macht und Lieb\u2019 in ihnen ehrt,", "tokens": ["Die", ",", "da\u00df", "man", "dei\u00b7ne", "Macht", "und", "Lieb'", "in", "ih\u00b7nen", "ehrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PIS", "PPOSAT", "NN", "KON", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Auf gantz besondre Weise, wehrt.", "tokens": ["Auf", "gantz", "be\u00b7sond\u00b7re", "Wei\u00b7se", ",", "wehrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Es \u00f6fnen sich die braunen Knospen kaum", "tokens": ["Es", "\u00f6f\u00b7nen", "sich", "die", "brau\u00b7nen", "Knos\u00b7pen", "kaum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Die, wie wir einsten schon besehen,", "tokens": ["Die", ",", "wie", "wir", "eins\u00b7ten", "schon", "be\u00b7se\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "PPER", "VVFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Aus manchem k\u00fcnstlichem Gewebe selbst bestehen;", "tokens": ["Aus", "man\u00b7chem", "k\u00fcnst\u00b7li\u00b7chem", "Ge\u00b7we\u00b7be", "selbst", "be\u00b7ste\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So wird man in derselben innerm Raum", "tokens": ["So", "wird", "man", "in", "der\u00b7sel\u00b7ben", "in\u00b7nerm", "Raum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIS", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Drey gr\u00fcne Bl\u00e4tterchen gewahr,", "tokens": ["Drey", "gr\u00fc\u00b7ne", "Bl\u00e4t\u00b7ter\u00b7chen", "ge\u00b7wahr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "So hohl und gantz erf\u00fcllt mit zartem Haar:", "tokens": ["So", "hohl", "und", "gantz", "er\u00b7f\u00fcllt", "mit", "zar\u00b7tem", "Haar", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Die, wenn sie von einander gehn,", "tokens": ["Die", ",", "wenn", "sie", "von", "ein\u00b7an\u00b7der", "gehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Wie gr\u00fcne Blumen anzusehn.", "tokens": ["Wie", "gr\u00fc\u00b7ne", "Blu\u00b7men", "an\u00b7zu\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVIZU", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Aus diesen siehet man drey andre steigen,", "tokens": ["Aus", "die\u00b7sen", "sie\u00b7het", "man", "drey", "and\u00b7re", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVFIN", "PIS", "CARD", "PIS", "VVINF", "$,"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.18": {"text": "Die l\u00e4nglichter, und die sich in der Mitten", "tokens": ["Die", "l\u00e4ng\u00b7lich\u00b7ter", ",", "und", "die", "sich", "in", "der", "Mit\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "KON", "PRELS", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Als wie ein Hertz durchschnitten,", "tokens": ["Als", "wie", "ein", "Hertz", "durch\u00b7schnit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Und, aus dem Schnitt, ein nettes Bl\u00e4tchen zeigen.", "tokens": ["Und", ",", "aus", "dem", "Schnitt", ",", "ein", "net\u00b7tes", "Bl\u00e4t\u00b7chen", "zei\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ART", "NN", "$,", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Nachhero werden noch drey andre, welche gr\u00f6sser,", "tokens": ["Nach\u00b7he\u00b7ro", "wer\u00b7den", "noch", "drey", "and\u00b7re", ",", "wel\u00b7che", "gr\u00f6s\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "CARD", "PIS", "$,", "PRELS", "ADJD", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.22": {"text": "Fast von derselben Art erblickt,", "tokens": ["Fast", "von", "der\u00b7sel\u00b7ben", "Art", "er\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Nur da\u00df darin die Form von Bl\u00e4ttern besser", "tokens": ["Nur", "da\u00df", "da\u00b7rin", "die", "Form", "von", "Bl\u00e4t\u00b7tern", "bes\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PAV", "ART", "NN", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Und deutlicher schon ausgedr\u00fcckt.", "tokens": ["Und", "deut\u00b7li\u00b7cher", "schon", "aus\u00b7ge\u00b7dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "An eines jeden Fu\u00df, die Frucht noch mehr zu sch\u00fctzen,", "tokens": ["An", "ei\u00b7nes", "je\u00b7den", "Fu\u00df", ",", "die", "Frucht", "noch", "mehr", "zu", "sch\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "$,", "ART", "NN", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Sieht man aufs neu zwey gr\u00fcne Spitzgen sitzen;", "tokens": ["Sieht", "man", "aufs", "neu", "zwey", "gr\u00fc\u00b7ne", "Spitz\u00b7gen", "sit\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPRART", "ADJD", "CARD", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Noch \u00fcber diesen steht ein Blatt,", "tokens": ["Noch", "\u00fc\u00b7ber", "die\u00b7sen", "steht", "ein", "Blatt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Das rings um seinen Fu\u00df vier kleine Spitzen", "tokens": ["Das", "rings", "um", "sei\u00b7nen", "Fu\u00df", "vier", "klei\u00b7ne", "Spit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "APPR", "PPOSAT", "NN", "CARD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Als ein absonderlich Gew\u00e4chse hat;", "tokens": ["Als", "ein", "ab\u00b7son\u00b7der\u00b7lich", "Ge\u00b7w\u00e4ch\u00b7se", "hat", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJD", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}