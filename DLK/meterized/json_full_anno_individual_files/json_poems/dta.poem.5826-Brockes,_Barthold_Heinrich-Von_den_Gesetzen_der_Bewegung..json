{"dta.poem.5826": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Gesetzen der Bewegung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Krafft, so C\u00f6rper treibt, die Schwere, so sie dr\u00fcckt,", "tokens": ["Die", "Krafft", ",", "so", "C\u00f6r\u00b7per", "treibt", ",", "die", "Schwe\u00b7re", ",", "so", "sie", "dr\u00fcckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "NE", "VVFIN", "$,", "ART", "NN", "$,", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Macht sie zu mancherley Bewegungen geschickt:", "tokens": ["Macht", "sie", "zu", "man\u00b7cher\u00b7ley", "Be\u00b7we\u00b7gun\u00b7gen", "ge\u00b7schickt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es zeigt sich manch vermischt, und manch gef\u00fcgt Bewegen,", "tokens": ["Es", "zeigt", "sich", "manch", "ver\u00b7mischt", ",", "und", "manch", "ge\u00b7f\u00fcgt", "Be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "VVPP", "$,", "KON", "ADJD", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durch W\u00fcrckungen, die sich einander gantz entgegen.", "tokens": ["Durch", "W\u00fcr\u00b7ckun\u00b7gen", ",", "die", "sich", "ein\u00b7an\u00b7der", "gantz", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "wor