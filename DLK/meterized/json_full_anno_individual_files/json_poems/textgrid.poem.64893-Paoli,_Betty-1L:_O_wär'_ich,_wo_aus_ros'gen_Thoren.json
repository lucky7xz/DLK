 ["Doch", "we\u00b7he", "!", "weh", "!", "hier", "mu\u00df", "ich", "le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "$.", "PTKVZ", "$.", "ADV", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In dieser st\u00fcrmevollen Ruh'", "tokens": ["In", "die\u00b7ser", "st\u00fcr\u00b7me\u00b7vol\u00b7len", "Ruh'"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und nur die bangen W\u00fcnsche schweben", "tokens": ["Und", "nur", "die", "ban\u00b7gen", "W\u00fcn\u00b7sche", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem lichten Morgenlande zu!", "tokens": ["Dem", "lich\u00b7ten", "Mor\u00b7gen\u00b7lan\u00b7de", "zu", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}