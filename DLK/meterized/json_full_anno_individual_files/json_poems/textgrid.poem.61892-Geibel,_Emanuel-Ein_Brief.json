{"textgrid.poem.61892": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Ein Brief", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das waren goldbeschwingte Tage,", "tokens": ["Das", "wa\u00b7ren", "gold\u00b7be\u00b7schwing\u00b7te", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die ich im sonnigen Waldrevier,", "tokens": ["Die", "ich", "im", "son\u00b7ni\u00b7gen", "Wald\u00b7re\u00b7vier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der Welt entr\u00fcckt und ihrer Plage,", "tokens": ["Der", "Welt", "ent\u00b7r\u00fcckt", "und", "ih\u00b7rer", "Pla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch einmal jung, verschw\u00e4rmt mit dir.", "tokens": ["Noch", "ein\u00b7mal", "jung", ",", "ver\u00b7schw\u00e4rmt", "mit", "dir", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$,", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nun kehrt in seine stillen Gleise", "tokens": ["Nun", "kehrt", "in", "sei\u00b7ne", "stil\u00b7len", "Glei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zur\u00fcck mein Leben allgemach,", "tokens": ["Zu\u00b7r\u00fcck", "mein", "Le\u00b7ben", "all\u00b7ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch klingt in tiefster Brust mir leise", "tokens": ["Doch", "klingt", "in", "tiefs\u00b7ter", "Brust", "mir", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "PPER", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Echo meines Gl\u00fcckes nach.", "tokens": ["Das", "E\u00b7cho", "mei\u00b7nes", "Gl\u00fc\u00b7ckes", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Zwar bannt die Pflicht mich streng in Schranken,", "tokens": ["Zwar", "bannt", "die", "Pflicht", "mich", "streng", "in", "Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und manchmal nur im Tageslauf", "tokens": ["Und", "manch\u00b7mal", "nur", "im", "Ta\u00b7ges\u00b7lauf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Taucht \u00fcberm Strome der Gedanken", "tokens": ["Taucht", "\u00fc\u00b7berm", "Stro\u00b7me", "der", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mir wie ein Stern dein Bildnis auf.", "tokens": ["Mir", "wie", "ein", "Stern", "dein", "Bild\u00b7nis", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ART", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch wenn getreu beim Abendneigen", "tokens": ["Doch", "wenn", "ge\u00b7treu", "beim", "A\u00b7ben\u00b7dnei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Werk, das mich erf\u00fcllt, vollbracht,", "tokens": ["Das", "Werk", ",", "das", "mich", "er\u00b7f\u00fcllt", ",", "voll\u00b7bracht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann steuert, wieder ganz dein eigen,", "tokens": ["Dann", "steu\u00b7ert", ",", "wie\u00b7der", "ganz", "dein", "ei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "ADV", "PPOSAT", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Seele durch das Meer der Nacht.", "tokens": ["Die", "See\u00b7le", "durch", "das", "Meer", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Dann red' ich wach zu dir und walle", "tokens": ["Dann", "red'", "ich", "wach", "zu", "dir", "und", "wal\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "PPER", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vereint mit dir des Traumes Bahn,", "tokens": ["Ver\u00b7eint", "mit", "dir", "des", "Trau\u00b7mes", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die trauten St\u00e4tten gr\u00fc\u00df' ich alle,", "tokens": ["Die", "trau\u00b7ten", "St\u00e4t\u00b7ten", "gr\u00fc\u00df'", "ich", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die unsrer Liebe Werden sahn:", "tokens": ["Die", "uns\u00b7rer", "Lie\u00b7be", "Wer\u00b7den", "sahn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Den Buchengang, den uns der Morgen", "tokens": ["Den", "Bu\u00b7chen\u00b7gang", ",", "den", "uns", "der", "Mor\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In herbstlich goldnen Duft getaucht,", "tokens": ["In", "herbst\u00b7lich", "gold\u00b7nen", "Duft", "ge\u00b7taucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als du von meiner Stirn die Sorgen", "tokens": ["Als", "du", "von", "mei\u00b7ner", "Stirn", "die", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit liebem Wort hinweggehaucht;", "tokens": ["Mit", "lie\u00b7bem", "Wort", "hin\u00b7weg\u00b7ge\u00b7haucht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Das H\u00fcttlein in des Parkes Schatten", "tokens": ["Das", "H\u00fctt\u00b7lein", "in", "des", "Par\u00b7kes", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von Ros' und wildem Wein umkr\u00e4nzt,", "tokens": ["Von", "Ros'", "und", "wil\u00b7dem", "Wein", "um\u00b7kr\u00e4nzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf dessen Schwelle du dem Matten", "tokens": ["Auf", "des\u00b7sen", "Schwel\u00b7le", "du", "dem", "Mat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den frischen Trunk so oft kredenzt;", "tokens": ["Den", "fri\u00b7schen", "Trunk", "so", "oft", "kre\u00b7denzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Das graue Jagdschlo\u00df \u00fcberm Weiher,", "tokens": ["Das", "grau\u00b7e", "Jagd\u00b7schlo\u00df", "\u00fc\u00b7berm", "Wei\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo wir entz\u00fcckt ins Laubgewog", "tokens": ["Wo", "wir", "ent\u00b7z\u00fcckt", "ins", "Laub\u00b7ge\u00b7wog"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hinabgelauscht, indes der Reiher", "tokens": ["Hin\u00b7ab\u00b7ge\u00b7lauscht", ",", "in\u00b7des", "der", "Rei\u00b7her"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durchs Sp\u00e4trot seine Kreise zog.", "tokens": ["Durchs", "Sp\u00e4t\u00b7rot", "sei\u00b7ne", "Krei\u00b7se", "zog", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Und wieder h\u00f6r' ich froh erschrocken", "tokens": ["Und", "wie\u00b7der", "h\u00f6r'", "ich", "froh", "er\u00b7schro\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Laut, der meine Seele bannt,", "tokens": ["Den", "Laut", ",", "der", "mei\u00b7ne", "See\u00b7le", "bannt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mich streift das Wehen deiner Locken,", "tokens": ["Mich", "streift", "das", "We\u00b7hen", "dei\u00b7ner", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Druck empfind' ich deiner Hand.", "tokens": ["Den", "Druck", "emp\u00b7find'", "ich", "dei\u00b7ner", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Ach, alles, alles kommt aufs neue,", "tokens": ["Ach", ",", "al\u00b7les", ",", "al\u00b7les", "kommt", "aufs", "neu\u00b7e", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PIS", "$,", "PIS", "VVFIN", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was mich so reich und froh gemacht;", "tokens": ["Was", "mich", "so", "reich", "und", "froh", "ge\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das sanfte Mondlicht deiner Treue", "tokens": ["Das", "sanf\u00b7te", "Mond\u00b7licht", "dei\u00b7ner", "Treu\u00b7e"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schwebt \u00fcber mir die ganze Nacht.", "tokens": ["Schwebt", "\u00fc\u00b7ber", "mir", "die", "gan\u00b7ze", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Und morgens dann in goldner Fr\u00fche,", "tokens": ["Und", "mor\u00b7gens", "dann", "in", "gold\u00b7ner", "Fr\u00fc\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn kaum der letzte Stern erblich,", "tokens": ["Wenn", "kaum", "der", "letz\u00b7te", "Stern", "er\u00b7blich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gest\u00e4rkt zu jeder Lebensm\u00fche", "tokens": ["Ge\u00b7st\u00e4rkt", "zu", "je\u00b7der", "Le\u00b7bens\u00b7m\u00fc\u00b7he"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erwacht mein Herz und segnet dich.", "tokens": ["Er\u00b7wacht", "mein", "Herz", "und", "seg\u00b7net", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Das waren goldbeschwingte Tage,", "tokens": ["Das", "wa\u00b7ren", "gold\u00b7be\u00b7schwing\u00b7te", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die ich im sonnigen Waldrevier,", "tokens": ["Die", "ich", "im", "son\u00b7ni\u00b7gen", "Wald\u00b7re\u00b7vier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der Welt entr\u00fcckt und ihrer Plage,", "tokens": ["Der", "Welt", "ent\u00b7r\u00fcckt", "und", "ih\u00b7rer", "Pla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch einmal jung, verschw\u00e4rmt mit dir.", "tokens": ["Noch", "ein\u00b7mal", "jung", ",", "ver\u00b7schw\u00e4rmt", "mit", "dir", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$,", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Nun kehrt in seine