bic.penta"}, "line.3": {"text": "Und nichts bleibt \u00fcbrig dir vor diesem Viehchen", "tokens": ["Und", "nichts", "bleibt", "\u00fcb\u00b7rig", "dir", "vor", "die\u00b7sem", "Vieh\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "ADJD", "PPER", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als sacht dich in dich selber zu verkriechen!", "tokens": ["Als", "sacht", "dich", "in", "dich", "sel\u00b7ber", "zu", "ver\u00b7krie\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PRF", "APPR", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Drei Dinge haben hier im Leben Macht:", "tokens": ["Drei", "Din\u00b7ge", "ha\u00b7ben", "hier", "im", "Le\u00b7ben", "Macht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "ADV", "APPRART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Neid, die Hoffahrt und die Niedertracht;", "tokens": ["Der", "Neid", ",", "die", "Hof\u00b7fahrt", "und", "die", "Nie\u00b7der\u00b7tracht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch, wenn sie dich auch noch so sch\u00f6n bespucken,", "tokens": ["Doch", ",", "wenn", "sie", "dich", "auch", "noch", "so", "sch\u00f6n", "be\u00b7spu\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "PRF", "ADV", "ADV", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Am End