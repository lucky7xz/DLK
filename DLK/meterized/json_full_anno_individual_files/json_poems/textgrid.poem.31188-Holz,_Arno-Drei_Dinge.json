{"textgrid.poem.31188": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Drei Dinge", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Drei Dinge haben hier im Leben Macht:", "tokens": ["Drei", "Din\u00b7ge", "ha\u00b7ben", "hier", "im", "Le\u00b7ben", "Macht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "ADV", "APPRART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Neid, die Hoffahrt und die Niedertracht;", "tokens": ["Der", "Neid", ",", "die", "Hof\u00b7fahrt", "und", "die", "Nie\u00b7der\u00b7tracht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch, wenn sie dich auch noch so sch\u00f6n bespucken,", "tokens": ["Doch", ",", "wenn", "sie", "dich", "auch", "noch", "so", "sch\u00f6n", "be\u00b7spu\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "PRF", "ADV", "ADV", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Am Ende wirst du sie zu Boden ducken!", "tokens": ["Am", "En\u00b7de", "wirst", "du", "sie", "zu", "Bo\u00b7den", "du\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Verloren aber bist du auf der Welt,", "tokens": ["Ver\u00b7lo\u00b7ren", "a\u00b7ber", "bist", "du", "auf", "der", "Welt", ","], "token_in