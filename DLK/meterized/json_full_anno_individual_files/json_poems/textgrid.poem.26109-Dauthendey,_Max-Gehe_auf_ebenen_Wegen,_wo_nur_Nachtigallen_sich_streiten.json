{"textgrid.poem.26109": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Gehe auf ebenen Wegen, wo nur Nachtigallen sich streiten", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Parke leuchten die gelben und roten Tulpenbeete,", "tokens": ["Im", "Par\u00b7ke", "leuch\u00b7ten", "die", "gel\u00b7ben", "und", "ro\u00b7ten", "Tul\u00b7pen\u00b7bee\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "KON", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und der Springbrunnen springt wie eine helle Rakete.", "tokens": ["Und", "der", "Spring\u00b7brun\u00b7nen", "springt", "wie", "ei\u00b7ne", "hel\u00b7le", "Ra\u00b7ke\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Der rosige Pfirsichbaum bl\u00fcht an den alten Terrassen,", "tokens": ["Der", "ro\u00b7si\u00b7ge", "Pfir\u00b7sich\u00b7baum", "bl\u00fcht", "an", "den", "al\u00b7ten", "Ter\u00b7ras\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Steinfiguren stehen dort, die sich an den H\u00e4nden fassen.", "tokens": ["Stein\u00b7fi\u00b7gu\u00b7ren", "ste\u00b7hen", "dort", ",", "die", "sich", "an", "den", "H\u00e4n\u00b7den", "fas\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "$,", "PRELS", "PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Ich gehe den Lauben nach und besuche den Finken,", "tokens": ["Ich", "ge\u00b7he", "den", "Lau\u00b7ben", "nach", "und", "be\u00b7su\u00b7che", "den", "Fin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-+--+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Sehe die Bl\u00fctenb\u00e4ume gleich silbernen Leuchtern blinken,", "tokens": ["Se\u00b7he", "die", "Bl\u00fc\u00b7ten\u00b7b\u00e4u\u00b7me", "gleich", "sil\u00b7ber\u00b7nen", "Leuch\u00b7tern", "blin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "+--+-+--+--+-+-", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Atme die L\u00e4ssigkeit, die alle Bl\u00fcten verbreiten;", "tokens": ["At\u00b7me", "die", "L\u00e4s\u00b7sig\u00b7keit", ",", "die", "al\u00b7le", "Bl\u00fc\u00b7ten", "ver\u00b7brei\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "PIAT", "NN", "VVINF", "$."], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Gehe auf ebenen Wegen, wo nur Nachtigallen sich streiten,", "tokens": ["Ge\u00b7he", "auf", "e\u00b7be\u00b7nen", "We\u00b7gen", ",", "wo", "nur", "Nach\u00b7ti\u00b7gal\u00b7len", "sich", "strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "$,", "PWAV", "ADV", "NN", "PRF", "VVFIN", "$,"], "meter": "+--+--+-+-+-+--+-", "measure": "dactylic.di.plus"}, "line.9": {"text": "Und atme die Liebesluft der Fr\u00fchlinge