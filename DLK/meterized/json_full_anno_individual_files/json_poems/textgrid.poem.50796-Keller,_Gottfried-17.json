{"textgrid.poem.50796": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "17", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich mu\u00df ein Weilchen wohl geschlafen haben,", "tokens": ["Ich", "mu\u00df", "ein", "Weil\u00b7chen", "wohl", "ge\u00b7schla\u00b7fen", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn wie aus Tr\u00e4umen schein ich mir erwacht;", "tokens": ["Denn", "wie", "aus", "Tr\u00e4u\u00b7men", "schein", "ich", "mir", "er\u00b7wacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPR", "NN", "VVFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bin ich leibhaftig, wirklich denn begraben?", "tokens": ["Bin", "ich", "leib\u00b7haf\u00b7tig", ",", "wirk\u00b7lich", "denn", "be\u00b7gra\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "ADJD", "ADV", "VVPP", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Noch immer diese enge, schwarze Nacht?", "tokens": ["Noch", "im\u00b7mer", "die\u00b7se", "en\u00b7ge", ",", "schwar\u00b7ze", "Nacht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "PDAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mein Atem ist wohl heftig, rasch gegangen,", "tokens": ["Mein", "A\u00b7tem", "ist", "wohl", "hef\u00b7tig", ",", "rasch", "ge\u00b7gan\u00b7gen", ","], "token_info": 