{"dta.poem.13814": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Aufbruch .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Horch! was h\u00f6r' ich drau\u00dfen klingen", "tokens": ["Horch", "!", "was", "h\u00f6r'", "ich", "drau\u00b7\u00dfen", "klin\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "PWS", "VVFIN", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wild verlockend wie zur Jagd?", "tokens": ["Wild", "ver\u00b7lo\u00b7ckend", "wie", "zur", "Jagd", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "KOKOM", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, das Herz m\u00f6cht' mir zerspringen,", "tokens": ["Ach", ",", "das", "Herz", "m\u00f6cht'", "mir", "zer\u00b7sprin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "VMFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie es jauchzt und weint und klagt.", "tokens": ["Wie", "es", "jauchzt", "und", "weint", "und", "klagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}