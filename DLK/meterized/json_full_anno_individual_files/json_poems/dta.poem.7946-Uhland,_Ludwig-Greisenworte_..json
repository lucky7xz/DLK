{"dta.poem.7946": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Greisenworte .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Sagt nicht mehr: guten Morgen! guten Tag!", "tokens": ["Sagt", "nicht", "mehr", ":", "gu\u00b7ten", "Mor\u00b7gen", "!", "gu\u00b7ten", "Tag", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "$.", "ADJA", "NN", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sagt immer: guten Abend! gute Nacht!", "tokens": ["Sagt", "im\u00b7mer", ":", "gu\u00b7ten", "A\u00b7bend", "!", "gu\u00b7te", "Nacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$.", "ADJA", "NN", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denn Abend ist es um mich und die Nacht", "tokens": ["Denn", "A\u00b7bend", "ist", "es", "um", "mich", "und", "die", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VAFIN", "PPER", "APPR", "PPER", "KON", "ART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ist nahe mir; o w\u00e4re sie schon da!", "tokens": ["Ist", "na\u00b7he", "mir", ";", "o", "w\u00e4\u00b7re", "sie", "schon", "da", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "$.", "FM", "VAFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Komm her, mein Kind! o du mein s\u00fc\u00dfes Leben!", "tokens": ["Komm", "her", ",", "mein", "Kind", "!", "o", "du", "mein", "s\u00fc\u00b7\u00dfes", "Le\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPOSAT", "NN", "$.", "FM", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nein! komm, mein Kind, o du mein s\u00fc\u00dfer Tod!", "tokens": ["Nein", "!", "komm", ",", "mein", "Kind", ",", "o", "du", "mein", "s\u00fc\u00b7\u00dfer", "Tod", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "VVFIN", "$,", "PPOSAT", "NN", "$,", "FM", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denn Alles, was mir bitter, nenn\u2019 ich Leben,", "tokens": ["Denn", "Al\u00b7les", ",", "was", "mir", "bit\u00b7ter", ",", "nenn'", "ich", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PWS", "PPER", "ADJD", "$,", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und was mir s\u00fc\u00df ist, nenn\u2019 ich alles Tod.", "tokens": ["Und", "was", "mir", "s\u00fc\u00df", "ist", ",", "nenn'", "ich", "al\u00b7les", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADJD", "VAFIN", "$,", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}