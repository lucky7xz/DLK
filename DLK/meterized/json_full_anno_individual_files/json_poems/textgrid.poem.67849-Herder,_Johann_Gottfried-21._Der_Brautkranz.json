{"textgrid.poem.67849": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "21. Der Brautkranz", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Voll von Ruhm und Siegeszeichen,", "tokens": ["Voll", "von", "Ruhm", "und", "Sie\u00b7ges\u00b7zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mehr als Mars es je gewesen,", "tokens": ["Mehr", "als", "Mars", "es", "je", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "NN", "PPER", "ADV", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "War der edle tapfre Gazul", "tokens": ["War", "der", "ed\u00b7le", "tapf\u00b7re", "Ga\u00b7zul"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Nun aus Gelves heimgekehret.", "tokens": ["Nun", "aus", "Gel\u00b7ves", "heim\u00b7ge\u00b7keh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wohl empfing ihn in Sankt Lucar", "tokens": ["Wohl", "emp\u00b7fing", "ihn", "in", "Sankt", "Lu\u00b7car"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "VVFIN", "NE"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Lindaraja, seine Dame,", "tokens": ["Lind\u00b7a\u00b7ra\u00b7ja", ",", "sei\u00b7ne", "Da\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$,"], "meter": "++--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Die ihn o wie z\u00e4rtlich liebet,", "tokens": ["Die", "ihn", "o", "wie", "z\u00e4rt\u00b7lich", "lie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "FM", "KOKOM", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und nicht minder liebt er sie.", "tokens": ["Und", "nicht", "min\u00b7der", "liebt", "er", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Beide nun allein zusammen", "tokens": ["Bei\u00b7de", "nun", "al\u00b7lein", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "ADV", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In des Blumengartens Bl\u00fcthe,", "tokens": ["In", "des", "Blu\u00b7men\u00b7gar\u00b7tens", "Bl\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wechseln sie der Liebe Pf\u00e4nder,", "tokens": ["Wech\u00b7seln", "sie", "der", "Lie\u00b7be", "Pf\u00e4n\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jedes f\u00fchlet, wen es liebt.", "tokens": ["Je\u00b7des", "f\u00fch\u00b7let", ",", "wen", "es", "liebt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Lindaraja hat aus zarter", "tokens": ["Lind\u00b7a\u00b7ra\u00b7ja", "hat", "aus", "zar\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VAFIN", "APPR", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Neigung einen Kranz geflochten,", "tokens": ["Nei\u00b7gung", "ei\u00b7nen", "Kranz", "ge\u00b7floch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00f6n von Nelken und von Rosen,", "tokens": ["Sch\u00f6n", "von", "Nel\u00b7ken", "und", "von", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "KON", "APPR", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Un(d) von auserw\u00e4hlten W\u00fcrzen.", "tokens": ["Un", "(", "d", ")", "von", "au\u00b7ser\u00b7w\u00e4hl\u00b7ten", "W\u00fcr\u00b7zen", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "XY", "$(", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.5": {"line.1": {"text": "Hat ihn rings umsteckt mit Veilchen,", "tokens": ["Hat", "ihn", "rings", "um\u00b7steckt", "mit", "Veil\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die die Bl\u00fcmlein sind der Liebe,", "tokens": ["Die", "die", "Bl\u00fcm\u00b7lein", "sind", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und so sezt sie ihrem Gazul", "tokens": ["Und", "so", "sezt", "sie", "ih\u00b7rem", "Ga\u00b7zul"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf das Haupt den Kranz und r\u00fchmet:", "tokens": ["Auf", "das", "Haupt", "den", "Kranz", "und", "r\u00fch\u00b7met", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "\u00bbnimmer war doch Ganymedes", "tokens": ["\u00bb", "nim\u00b7mer", "war", "doch", "Ga\u00b7ny\u00b7me\u00b7des"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VAFIN", "ADV", "NN"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Sch\u00f6n wie du von Angesichte,", "tokens": ["Sch\u00f6n", "wie", "du", "von", "An\u00b7ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn dich Jupiter jezt s\u00e4he,", "tokens": ["Wenn", "dich", "Ju\u00b7pi\u00b7ter", "jezt", "s\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fchret' er dich mit sich fort.\u00ab", "tokens": ["F\u00fch\u00b7ret'", "er", "dich", "mit", "sich", "fort", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "PRF", "PTKVZ", "$.", "$("], "meter": "+---+-+", "measure": "dactylic.init"}}, "stanza.7": {"line.1": {"text": "Gazul freudig sie umarmend", "tokens": ["Ga\u00b7zul", "freu\u00b7dig", "sie", "um\u00b7ar\u00b7mend"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADJD", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spricht mit Lachen: \u00bbmeine Liebe,", "tokens": ["Spricht", "mit", "La\u00b7chen", ":", "\u00bb", "mei\u00b7ne", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$.", "$(", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00f6n wie du war wahrlich jene", "tokens": ["Sch\u00f6n", "wie", "du", "war", "wahr\u00b7lich", "je\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "PWAV", "PPER", "VAFIN", "ADV", "PDAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Griechin nicht, die Paris raubte,", "tokens": ["Grie\u00b7ch\u00b7in", "nicht", ",", "die", "Pa\u00b7ris", "raub\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "$,", "PRELS", "NE", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.8": {"line.1": {"text": "Um die Troja ging verlohren,", "tokens": ["Um", "die", "Tro\u00b7ja", "ging", "ver\u00b7loh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NE", "VVFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um die Alles stand in Flammen:", "tokens": ["Um", "die", "Al\u00b7les", "stand", "in", "Flam\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "PIS", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00f6n, wie du, war jene nimmer,", "tokens": ["Sch\u00f6n", ",", "wie", "du", ",", "war", "je\u00b7ne", "nim\u00b7mer", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPER", "$,", "VAFIN", "PDS", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Du die Siegerin des Amors.\u00ab", "tokens": ["Du", "die", "Sie\u00b7ge\u00b7rin", "des", "A\u00b7mors", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ART", "NN", "ART", "NE", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "\u00bbwenn ich denn so sch\u00f6n dir scheine,", "tokens": ["\u00bb", "wenn", "ich", "denn", "so", "sch\u00f6n", "dir", "schei\u00b7ne", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ADV", "ADV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gazul, la\u00df uns uns verm\u00e4hlen!", "tokens": ["Ga\u00b7zul", ",", "la\u00df", "uns", "uns", "ver\u00b7m\u00e4h\u00b7len", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVIMP", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hast mir ja dein Wort gegeben,", "tokens": ["Hast", "mir", "ja", "dein", "Wort", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mein Gemahl zu werden, Gazul.\u00ab", "tokens": ["Mein", "Ge\u00b7mahl", "zu", "wer\u00b7den", ",", "Ga\u00b7zul", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VAINF", "$,", "NE", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Wohl, o wohl, spricht Gazul, la\u00df uns!", "tokens": ["Wohl", ",", "o", "wohl", ",", "spricht", "Ga\u00b7zul", ",", "la\u00df", "uns", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "FM", "ADV", "$,", "VVFIN", "NE", "$,", "VVIMP", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Denn dabei bin ich Gewinner.", "tokens": ["Denn", "da\u00b7bei", "bin", "ich", "Ge\u00b7win\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und so feiren sie mit Freude", "tokens": ["Und", "so", "fei\u00b7ren", "sie", "mit", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hochzeitfest und werden Christen.", "tokens": ["Hoch\u00b7zeit\u00b7fest", "und", "wer\u00b7den", "Chris\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VAFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Voll von Ruhm und Siegeszeichen,", "tokens": ["Voll", "von", "Ruhm", "und", "Sie\u00b7ges\u00b7zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mehr als Mars es je gewesen,", "tokens": ["Mehr", "als", "Mars", "es", "je", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "NN", "PPER", "ADV", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "War der edle tapfre Gazul", "tokens": ["War", "der", "ed\u00b7le", "tapf\u00b7re", "Ga\u00b7zul"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Nun aus Gelves heimgekehret.", "t