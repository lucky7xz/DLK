13", "\u2013", "Doch", "bald", ",", "mit", "Lug", "und", "Trug", ","], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "$(", "$(", "KON", "ADV", "$,", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit neuen Banden, fester noch gekettet,", "tokens": ["Mit", "neu\u00b7en", "Ban\u00b7den", ",", "fes\u00b7ter", "noch", "ge\u00b7ket\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein neues Rom den Geist in Fesseln schlug:", "tokens": ["Ein", "neu\u00b7es", "Rom", "den", "Geist", "in", "Fes\u00b7seln", "schlug", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Da z\u00fcndete der Mann aus Sachsenstamme", "tokens": ["Da", "z\u00fcn\u00b7de\u00b7te", "der", "Mann", "aus", "Sach\u00b7sen\u00b7stam\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Feuer vor dem Wittenberger Dom", "tokens": ["Das", "Feu\u00b7er", "vor", "dem", "Wit\u00b7ten\u00b7ber\u00b7ger", "Dom"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und warf des Papstes Bannbrief in die Flamme", "tokens": ["Und", "warf", "des", "Paps\u00b7tes", "Bann\u00b7brief", "in", "die", "Flam\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "wor