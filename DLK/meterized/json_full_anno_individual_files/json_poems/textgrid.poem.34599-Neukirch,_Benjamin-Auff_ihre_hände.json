{"textgrid.poem.34599": {"metadata": {"author": {"name": "Neukirch, Benjamin", "birth": "N.A.", "death": "N.A."}, "title": "Auff ihre h\u00e4nde", "genre": "verse", "period": "N.A.", "pub_year": 1697, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So offt ich euch beschau/ ihr angenehmen h\u00e4nde/", "tokens": ["So", "offt", "ich", "euch", "be\u00b7schau", "/", "ihr", "an\u00b7ge\u00b7neh\u00b7men", "h\u00e4n\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PPER", "PTKVZ", "$(", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So offtmahls f\u00fchl ich auch im hertzen neue br\u00e4nde/", "tokens": ["So", "offt\u00b7mahls", "f\u00fchl", "ich", "auch", "im", "hert\u00b7zen", "neu\u00b7e", "br\u00e4n\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und saug/ ich wei\u00df nicht was f\u00fcr s\u00fcsse funcken ein.", "tokens": ["Und", "saug", "/", "ich", "wei\u00df", "nicht", "was", "f\u00fcr", "s\u00fcs\u00b7se", "fun\u00b7cken", "ein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "PPER", "VVFIN", "PTKNEG", "PRELS", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie geht es aber zu? Ihr seyd von elffen-beine/", "tokens": ["Wie", "geht", "es", "a\u00b7ber", "zu", "?", "Ihr", "seyd", "von", "elf\u00b7fen\u00b7bei\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "PTKVZ", "$.", "PPER", "VAFIN", "APPR", "NE", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die finger gleichen schnee und reinem marmel-steine/", "tokens": ["Die", "fin\u00b7ger", "glei\u00b7chen", "schnee", "und", "rei\u00b7nem", "mar\u00b7mel\u00b7stei\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df euer schnee zugleich kan glut und flammen speyn.", "tokens": ["Da\u00df", "eu\u00b7er", "schnee", "zu\u00b7gleich", "kan", "glut", "und", "flam\u00b7men", "speyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "VMFIN", "NN", "KON", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Jedoch was frag ich erst? Ich hab es ja empfunden/", "tokens": ["Je\u00b7doch", "was", "frag", "ich", "erst", "?", "Ich", "hab", "es", "ja", "emp\u00b7fun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VVFIN", "PPER", "ADV", "$.", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Das feuer brennet noch in meinen tieffen wunden;", "tokens": ["Das", "feu\u00b7er", "bren\u00b7net", "noch", "in", "mei\u00b7nen", "tief\u00b7fen", "wun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wiewohl ich klag allhier euch meine wunden nicht.", "tokens": ["Wie\u00b7wohl", "ich", "klag", "all\u00b7hier", "euch", "mei\u00b7ne", "wun\u00b7den", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADV", "PPER", "PPOSAT", "ADJA", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ihr habet recht gethan/ ich will es also haben/", "tokens": ["Ihr", "ha\u00b7bet", "recht", "ge\u00b7than", "/", "ich", "will", "es", "al\u00b7so", "ha\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$(", "PPER", "VMFIN", "PPER", "ADV", "VAFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Eur schnee/ der mich entz\u00fcndt/ kan mich auch wieder laben/", "tokens": ["Eur", "schnee", "/", "der", "mich", "ent\u00b7z\u00fcndt", "/", "kan", "mich", "auch", "wie\u00b7der", "la\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PRELS", "PPER", "VVPP", "$(", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und giebt mir, sterb ich schon/ auch noch im tode licht.", "tokens": ["Und", "giebt", "mir", ",", "sterb", "ich", "schon", "/", "auch", "noch", "im", "to\u00b7de", "licht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "ADV", "$(", "ADV", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Durch euch allein kan ich die gantze welt besiegen;", "tokens": ["Durch", "euch", "al\u00b7lein", "kan", "ich", "die", "gant\u00b7ze", "welt", "be\u00b7sie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VMFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Dann kan sich manche gleich an ihrer brust vergn\u00fcgen/", "tokens": ["Dann", "kan", "sich", "man\u00b7che", "gleich", "an", "ih\u00b7rer", "brust", "ver\u00b7gn\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PRF", "PIS", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Hat Doris ihr gesicht mit kreid und kalck gebleicht/", "tokens": ["Hat", "Do\u00b7ris", "ihr", "ge\u00b7sicht", "mit", "kreid", "und", "kalck", "ge\u00b7bleicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "PPER", "VVPP", "APPR", "NN", "KON", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und Phillis einen mund/ den Venus selbst gepriesen/", "tokens": ["Und", "Phil\u00b7lis", "ei\u00b7nen", "mund", "/", "den", "Ve\u00b7nus", "selbst", "ge\u00b7prie\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ART", "NN", "$(", "ART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "So bin und bleib ich doch im hertzen \u00fcberwiesen/", "tokens": ["So", "bin", "und", "bleib", "ich", "doch", "im", "hert\u00b7zen", "\u00fc\u00b7berw\u00b7ie\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "KON", "VVFIN", "PPER", "ADV", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "al