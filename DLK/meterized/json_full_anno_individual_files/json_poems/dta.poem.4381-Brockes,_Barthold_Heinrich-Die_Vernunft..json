{"dta.poem.4381": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Vernunft.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was ich von unserer Vernunft vern\u00fcnftig einzusehn", "tokens": ["Was", "ich", "von", "un\u00b7se\u00b7rer", "Ver\u00b7nunft", "ver\u00b7n\u00fcnf\u00b7tig", "ein\u00b7zu\u00b7sehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "PPOSAT", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "vermag,", "tokens": ["ver\u00b7mag", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Ist dieses: da\u00df sie anders nichts, als eine F\u00e4higkeit der", "tokens": ["Ist", "die\u00b7ses", ":", "da\u00df", "sie", "an\u00b7ders", "nichts", ",", "als", "ei\u00b7ne", "F\u00e4\u00b7hig\u00b7keit", "der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PDAT", "$.", "KOUS", "PPER", "ADV", "PIS", "$,", "KOUS", "ART", "NN", "ART"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Seelen,", "tokens": ["See\u00b7len", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Sich viel\u2019 Jdeen vorzustellen, und, nach gemachtem Ueber-", "tokens": ["Sich", "viel'", "Jdeen", "vor\u00b7zu\u00b7stel\u00b7len", ",", "und", ",", "nach", "ge\u00b7mach\u00b7tem", "Ue\u00b7ber"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PRF", "PIAT", "NN", "VVIZU", "$,", "KON", "$,", "APPR", "ADJA", "TRUNC"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "schlag,", "tokens": ["schlag", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Und richtigem Zusammenhalten, das Best\u2019 aus ihnen", "tokens": ["Und", "rich\u00b7ti\u00b7gem", "Zu\u00b7sam\u00b7men\u00b7hal\u00b7ten", ",", "das", "Best'", "aus", "ih\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "$,", "ART", "NN", "APPR", "PPER"], "meter": "-+---+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "zu erw\u00e4hlen.", "tokens": ["zu", "er\u00b7w\u00e4h\u00b7len", "."], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}