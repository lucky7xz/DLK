us", "fer\u00b7ner", "Hir\u00b7ten\u00b7schal\u00b7me"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "dringen sanft hervor.", "tokens": ["drin\u00b7gen", "sanft", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.11": {"line.1": {"text": "In dem Spiegel aber ihrer Runde", "tokens": ["In", "dem", "Spie\u00b7gel", "a\u00b7ber", "ih\u00b7rer", "Run\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "schaut v. Korf begl\u00fcckt,", "tokens": ["schaut", "v.", "Korf", "be\u00b7gl\u00fcckt", ","], "token_info": ["word", "abbreviation", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "was ihm je in jeder guten Stunde", "tokens": ["was", "ihm", "je", "in", "je\u00b7der", "gu\u00b7ten", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADV", "APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "durch den Sinn ger\u00fcckt:", "tokens": ["durch", "den", "Sinn", "ge\u00b7r\u00fcckt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Seine Welt erblickt mit offnem Munde", "tokens": ["Sei\u00b7ne", "Welt", "er\u00b7blickt", "mit", "off\u00b7nem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVPP", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Korf entz\u00fc