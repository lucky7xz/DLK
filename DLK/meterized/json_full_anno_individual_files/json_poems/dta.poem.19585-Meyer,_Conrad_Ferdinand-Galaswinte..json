{"dta.poem.19585": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Galaswinte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.71", "nl:0.28"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Im Saale jubelt Hochzeit \u2014", "tokens": ["Im", "Saa\u00b7le", "ju\u00b7belt", "Hoch\u00b7zeit"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Arme vor dem Busen", "tokens": ["Die", "Ar\u00b7me", "vor", "dem", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kreuzt Fredegund in Demut,", "tokens": ["Kreuzt", "Fre\u00b7de\u00b7gund", "in", "De\u00b7mut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des K\u00f6nigs list'ge Buhlin:", "tokens": ["Des", "K\u00f6\u00b7nigs", "list'\u00b7ge", "Buh\u00b7lin", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "\u201eich bin die Magd und leuchte", "tokens": ["\u201e", "ich", "bin", "die", "Magd", "und", "leuch\u00b7te"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dem Br\u00e4utchen auf die Kammer!\u201c", "tokens": ["Dem", "Br\u00e4ut\u00b7chen", "auf", "die", "Kam\u00b7mer", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die Alabasterampel", "tokens": ["Die", "A\u00b7la\u00b7bas\u00b7ter\u00b7am\u00b7pel"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Mit r\u00f6mischen Sculpturen,", "tokens": ["Mit", "r\u00f6\u00b7mi\u00b7schen", "Sculp\u00b7tu\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Die schwebend einst geschimmert", "tokens": ["Die", "schwe\u00b7bend", "einst", "ge\u00b7schim\u00b7mert"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "In stillem Grabesdunkel,", "tokens": ["In", "stil\u00b7lem", "Gra\u00b7bes\u00b7dun\u00b7kel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Tr\u00e4gt Fredegund in Demut", "tokens": ["Tr\u00e4gt", "Fre\u00b7de\u00b7gund", "in", "De\u00b7mut"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Und hellt die Hochzeitskammer,", "tokens": ["Und", "hellt", "die", "Hoch\u00b7zeits\u00b7kam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Sie setzt die Ampel nieder", "tokens": ["Sie", "setzt", "die", "Am\u00b7pel", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Und geht und l\u00e4chelt t\u00fcckisch.", "tokens": ["Und", "geht", "und", "l\u00e4\u00b7chelt", "t\u00fc\u00b7ckisch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.15": {"text": "Die zarte Galaswinte", "tokens": ["Die", "zar\u00b7te", "Ga\u00b7las\u00b7win\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Blickt in die weh'nde Flamme,", "tokens": ["Blickt", "in", "die", "weh'n\u00b7de", "Flam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Die Flamme loht und flackert,", "tokens": ["Die", "Flam\u00b7me", "loht", "und", "fla\u00b7ckert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Die Ampel springt in Scherben,", "tokens": ["Die", "Am\u00b7pel", "springt", "in", "Scher\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Die F\u00fcrstin weint im Dunkel:", "tokens": ["Die", "F\u00fcrs\u00b7tin", "weint", "im", "Dun\u00b7kel", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "\u201edie mich gebracht aus Spanien,", "tokens": ["\u201e", "die", "mich", "ge\u00b7bracht", "aus", "Spa\u00b7ni\u00b7en", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PRELS", "PPER", "VVPP", "APPR", "NE", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.21": {"text": "Dein Kind dem Frankenk\u00f6nig,", "tokens": ["Dein", "Kind", "dem", "Fran\u00b7ken\u00b7k\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Jetzt drehst du auf dem Rosse", "tokens": ["Jetzt", "drehst", "du", "auf", "dem", "Ros\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Im Schein der Wanderfackel", "tokens": ["Im", "Schein", "der", "Wan\u00b7der\u00b7fa\u00b7ckel"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Noch einmal dich und breitest", "tokens": ["Noch", "ein\u00b7mal", "dich", "und", "brei\u00b7test"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Nach mir die Arme, Mutter!\u201c", "tokens": ["Nach", "mir", "die", "Ar\u00b7me", ",", "Mut\u00b7ter", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "$,", "NN", "$.", "$("], "meter": "---+-+-", "measure": "unknown.measure.di"}}}}}