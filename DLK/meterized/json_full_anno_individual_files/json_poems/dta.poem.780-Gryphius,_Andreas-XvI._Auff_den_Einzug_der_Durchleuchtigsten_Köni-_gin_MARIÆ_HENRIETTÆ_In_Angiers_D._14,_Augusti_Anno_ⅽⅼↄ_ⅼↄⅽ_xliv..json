{"dta.poem.780": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XvI.  \n Auff den Einzug der Durchleuchtigsten K\u00f6ni-  \n gin  MARI\u00c6 HENRIETT\u00c6  In Angiers  \n  D. 14, Augusti Anno \u217d\u217c\u2184 \u217c\u2184\u217d  xliv.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Die K\u00f6nige gezeugt/ die K\u00f6niglich gebohren/ ", "tokens": ["Die", "K\u00f6\u00b7ni\u00b7ge", "ge\u00b7zeugt", "/", "die", "K\u00f6\u00b7nig\u00b7lich", "ge\u00b7boh\u00b7ren", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$(", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die K\u00f6nige geliebt/ die bey noch zartem Jahr", "tokens": ["Die", "K\u00f6\u00b7ni\u00b7ge", "ge\u00b7liebt", "/", "die", "bey", "noch", "zar\u00b7tem", "Jahr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$(", "ART", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein K\u00f6nig jhr verm\u00e4hlt/ die K\u00f6nige gebahr/", "tokens": ["Ein", "K\u00f6\u00b7nig", "jhr", "ver\u00b7m\u00e4hlt", "/", "die", "K\u00f6\u00b7ni\u00b7ge", "ge\u00b7bahr", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "$(", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nach dem drey Kronen sie/ zur K\u00f6nigin erkohren:", "tokens": ["Nach", "dem", "drey", "Kro\u00b7nen", "sie", "/", "zur", "K\u00f6\u00b7ni\u00b7gin", "er\u00b7koh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "CARD", "NN", "PPER", "$(", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Fraw\u2019 auff welche sich viel tausend Mann verschwor\u1ebd/", "tokens": ["Die", "Fra\u00b7w'", "auff", "wel\u00b7che", "sich", "viel", "tau\u00b7send", "Mann", "ver\u00b7schwor\u1ebd", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PRELS", "PRF", "ADV", "CARD", "NN", "NE", "$("], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Verha\u00dft bey jhrem Volck/ geacht bey frembder Schaar", "tokens": ["Ver\u00b7ha\u00dft", "bey", "jhrem", "Volck", "/", "ge\u00b7acht", "bey", "fremb\u00b7der", "Schaar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$(", "VVPP", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Bey Nachbarn sonder lust/ bey Freunden in gefahr/", "tokens": ["Bey", "Nach\u00b7barn", "son\u00b7der", "lust", "/", "bey", "Freun\u00b7den", "in", "ge\u00b7fahr", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "VVFIN", "$(", "APPR", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verjagt ins Vaterland/ vermi\u00dft doch nicht verloren:", "tokens": ["Ver\u00b7jagt", "ins", "Va\u00b7ter\u00b7land", "/", "ver\u00b7mi\u00dft", "doch", "nicht", "ver\u00b7lo\u00b7ren", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$(", "VVFIN", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die gegenwertig schreckt: abwesend hefftig krigt:", "tokens": ["Die", "ge\u00b7gen\u00b7wer\u00b7tig", "schreckt", ":", "ab\u00b7we\u00b7send", "heff\u00b7tig", "krigt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "$.", "ADJD", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Helden niederwirfft/ vnd in der Senfften ligt", "tokens": ["Die", "Hel\u00b7den", "nie\u00b7der\u00b7wirfft", "/", "vnd", "in", "der", "Senff\u00b7ten", "ligt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$(", "KON", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wirst du erfrew\u2019t ", "tokens": ["Wirst", "du", "er\u00b7fre\u00b7w't"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PPER", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Schaw\u2019 an die Majest\u00e4t die in den Augen spielt", "tokens": ["Scha\u00b7w'", "an", "die", "Ma\u00b7jes\u00b7t\u00e4t", "die", "in", "den", "Au\u00b7gen", "spielt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "ART", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Das Antlitz das endeckt/ die sorgen die es f\u00fchlt/", "tokens": ["Das", "Ant\u00b7litz", "das", "en\u00b7deckt", "/", "die", "sor\u00b7gen", "die", "es", "f\u00fchlt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDS", "VVFIN", "$(", "ART", "NN", "ART", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnd lerne/ das was hoch/ auch schmacht\u2019 in h\u00f6her grawen.", "tokens": ["Vnd", "ler\u00b7ne", "/", "das", "was", "hoch", "/", "auch", "schmacht'", "in", "h\u00f6\u00b7her", "gra\u00b7wen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "PDS", "PRELS", "ADJD", "$(", "ADV", "VVFIN", "APPR", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}}}}