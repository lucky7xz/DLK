{"dta.poem.9468": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es bricht herf\u00fcr der N\u00e4chte Licht/", "tokens": ["Es", "bricht", "her\u00b7f\u00fcr", "der", "N\u00e4ch\u00b7te", "Licht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ach Liebster/ kom\u0303t/ sein Angesicht", "tokens": ["Ach", "Liebs\u00b7ter", "/", "kom\u0303t", "/", "sein", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ITJ", "NN", "$(", "VVFIN", "$(", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Soll meine Sonne werden:", "tokens": ["Soll", "mei\u00b7ne", "Son\u00b7ne", "wer\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Nacht das sehr verschwigne Kind/", "tokens": ["Die", "Nacht", "das", "sehr", "ver\u00b7schwig\u00b7ne", "Kind", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Erweckt den k\u00fchlen Suden-Wind", "tokens": ["Er\u00b7weckt", "den", "k\u00fch\u00b7len", "Su\u00b7den\u00b7Wind"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der fasterhitzten Erden:", "tokens": ["Der", "fas\u00b7ter\u00b7hitz\u00b7ten", "Er\u00b7den", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Es wehn und gehn", "tokens": ["Es", "wehn", "und", "gehn"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVINF", "KON", "VVINF"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Alle Winde saufft und linde/", "tokens": ["Al\u00b7le", "Win\u00b7de", "saufft", "und", "lin\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "KON", "ADJA", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Mond und Sterne", "tokens": ["Mond", "und", "Ster\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Wincken durch die Lufft von ferne.", "tokens": ["Win\u00b7cken", "durch", "die", "Lufft", "von", "fer\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ja Venus ziht zum ersten auff/", "tokens": ["Ja", "Ve\u00b7nus", "ziht", "zum", "ers\u00b7ten", "auff", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "NN", "VVFIN", "APPRART", "ADJA", "APPR", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und will durch seinen sanfften lauff", "tokens": ["und", "will", "durch", "sei\u00b7nen", "sanff\u00b7ten", "lauff"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "uns beyde selbst begleiten/", "tokens": ["uns", "bey\u00b7de", "selbst", "be\u00b7glei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "ADV", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dem folgt das andre Sternen-heer", "tokens": ["Dem", "folgt", "das", "and\u00b7re", "Ster\u00b7nen\u00b7heer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "und hengt das Gold je mehr und mehr", "tokens": ["und", "hengt", "das", "Gold", "je", "mehr", "und", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "ADV", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Am Himmel auf von weiten;", "tokens": ["Am", "Him\u00b7mel", "auf", "von", "wei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "APPR", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ja ich wil mich/", "tokens": ["Ja", "ich", "wil", "mich", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VMFIN", "PPER", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "O mein Leben/ Jhm nun eben", "tokens": ["O", "mein", "Le\u00b7ben", "/", "Jhm", "nun", "e\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "$(", "PPER", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Gantz ergeben/", "tokens": ["Gantz", "er\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Nun wilich nach Freuden streben!", "tokens": ["Nun", "wi\u00b7lich", "nach", "Freu\u00b7den", "stre\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}