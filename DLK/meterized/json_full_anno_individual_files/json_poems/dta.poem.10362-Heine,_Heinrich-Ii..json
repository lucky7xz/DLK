{"dta.poem.10362": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ii.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Im tollen Wahn hatt' ich dich einst verlassen,", "tokens": ["Im", "tol\u00b7len", "Wahn", "hatt'", "ich", "dich", "einst", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich wollte gehn die ganze Welt zu Ende,", "tokens": ["Ich", "woll\u00b7te", "gehn", "die", "gan\u00b7ze", "Welt", "zu", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wollte sehn ob ich die Liebe f\u00e4nde,", "tokens": ["Und", "woll\u00b7te", "sehn", "ob", "ich", "die", "Lie\u00b7be", "f\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "VVINF", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um liebevoll die Liebe zu umfassen.", "tokens": ["Um", "lie\u00b7be\u00b7voll", "die", "Lie\u00b7be", "zu", "um\u00b7fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Liebe suchte ich auf allen Gassen,", "tokens": ["Die", "Lie\u00b7be", "such\u00b7te", "ich", "auf", "al\u00b7len", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Vor jeder Th\u00fcre streckt' ich aus die H\u00e4nde,", "tokens": ["Vor", "je\u00b7der", "Th\u00fc\u00b7re", "streckt'", "ich", "aus", "die", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und bettelte um gringe Liebesspende, \u2014", "tokens": ["Und", "bet\u00b7tel\u00b7te", "um", "grin\u00b7ge", "Lie\u00b7bes\u00b7spen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Doch lachend gab man mir nur kaltes Hassen.", "tokens": ["Doch", "la\u00b7chend", "gab", "man", "mir", "nur", "kal\u00b7tes", "Has\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PIS", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und immer irrte ich nach Liebe, immer", "tokens": ["Und", "im\u00b7mer", "irr\u00b7te", "ich", "nach", "Lie\u00b7be", ",", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "NN", "$,", "ADV"], "meter": "-+-+-+-+-++", "measure": "unknown.measure.hexa"}, "line.10": {"text": "Nach Liebe, doch die Liebe fand ich nimmer,", "tokens": ["Nach", "Lie\u00b7be", ",", "doch", "die", "Lie\u00b7be", "fand", "ich", "nim\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADV", "ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und kehrte um nach Hause, krank und tr\u00fcbe.", "tokens": ["Und", "kehr\u00b7te", "um", "nach", "Hau\u00b7se", ",", "krank", "und", "tr\u00fc\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "APPR", "NN", "$,", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Doch da bist du entgegen mir gekommen,", "tokens": ["Doch", "da", "bist", "du", "ent\u00b7ge\u00b7gen", "mir", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und ach! was da in deinem Aug' geschwommen,", "tokens": ["Und", "ach", "!", "was", "da", "in", "dei\u00b7nem", "Aug'", "ge\u00b7schwom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PWS", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Das war die s\u00fc\u00dfe, langgesuchte Liebe.", "tokens": ["Das", "war", "die", "s\u00fc\u00b7\u00dfe", ",", "lang\u00b7ge\u00b7such\u00b7te", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}