{"dta.poem.7638": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Im gro\u00dfen Rechnungsbuch der Welt ist eingeschrieben,", "tokens": ["Im", "gro\u00b7\u00dfen", "Rech\u00b7nungs\u00b7buch", "der", "Welt", "ist", "ein\u00b7ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was wir genie\u00dfen, was wir haben, was wir lieben.", "tokens": ["Was", "wir", "ge\u00b7nie\u00b7\u00dfen", ",", "was", "wir", "ha\u00b7ben", ",", "was", "wir", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVINF", "$,", "PRELS", "PPER", "VAFIN", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie lang es zum Genu\u00df auf dieser Welt uns bleibt,", "tokens": ["Wie", "lang", "es", "zum", "Ge\u00b7nu\u00df", "auf", "die\u00b7ser", "Welt", "uns", "bleibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "APPRART", "NN", "APPR", "PDAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er wei\u00df es, der das Buch in seinem Sinne schreibt.", "tokens": ["Er", "wei\u00df", "es", ",", "der", "das", "Buch", "in", "sei\u00b7nem", "Sin\u00b7ne", "schreibt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PRELS", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}