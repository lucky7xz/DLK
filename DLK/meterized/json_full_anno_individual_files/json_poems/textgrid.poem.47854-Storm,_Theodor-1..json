{"textgrid.poem.47854": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1871, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lang und breit war ich gesessen", "tokens": ["Lang", "und", "breit", "war", "ich", "ge\u00b7ses\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "VAFIN", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00dcberm schwarzen Kontrapunkt;", "tokens": ["\u00dc\u00b7berm", "schwar\u00b7zen", "Kon\u00b7tra\u00b7punkt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf ein Haar dem Stadttrompeter", "tokens": ["Auf", "ein", "Haar", "dem", "Stadt\u00b7trom\u00b7pe\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gaben sie mich zum Adjunkt.", "tokens": ["Ga\u00b7ben", "sie", "mich", "zum", "Ad\u00b7junkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPRART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Hei, da bin ich ausgerissen;", "tokens": ["Hei", ",", "da", "bin", "ich", "aus\u00b7ge\u00b7ris\u00b7sen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6ne Welt, so nimm mich nun!", "tokens": ["Sch\u00f6\u00b7ne", "Welt", ",", "so", "nimm", "mich", "nun", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "VVIMP", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch die St\u00e4dte will ich schweifen,", "tokens": ["Durch", "die", "St\u00e4d\u00b7te", "will", "ich", "schwei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An den Quellen will ich ruhn.", "tokens": ["An", "den", "Quel\u00b7len", "will", "ich", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nur die Fiedel auf dem R\u00fccken;", "tokens": ["Nur", "die", "Fie\u00b7del", "auf", "dem", "R\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vorw\u00e4rts \u00fcber Berg und Strom!", "tokens": ["Vor\u00b7w\u00e4rts", "\u00fc\u00b7ber", "Berg", "und", "Strom", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schon durchschreit ich deine Hallen,", "tokens": ["Schon", "durch\u00b7schreit", "ich", "dei\u00b7ne", "Hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hoher k\u00fchler Waldesdom.", "tokens": ["Ho\u00b7her", "k\u00fch\u00b7ler", "Wal\u00b7des\u00b7dom", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "