{"dta.poem.119": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Liebes Brief an  Clelien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Verg\u00f6nne Clelia, da\u00df mein verirrter Geist", "tokens": ["Ver\u00b7g\u00f6n\u00b7ne", "Cle\u00b7lia", ",", "da\u00df", "mein", "ver\u00b7irr\u00b7ter", "Geist"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "NE", "$,", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In deinen Haven treibt/ wo ", "tokens": ["In", "dei\u00b7nen", "Ha\u00b7ven", "treibt", "/", "wo"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$(", "PWAV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den Port/ wo lauter Lust sich ums Gestade weist/", "tokens": ["Den", "Port", "/", "wo", "lau\u00b7ter", "Lust", "sich", "ums", "Ge\u00b7sta\u00b7de", "weist", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$(", "PWAV", "PIAT", "NN", "PRF", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wo Myrthen/ Tausend-Sch\u00f6n und ", "tokens": ["Wo", "Myr\u00b7then", "/", "Tau\u00b7sen\u00b7dSch\u00f6n", "und"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PWAV", "NN", "$(", "NE", "KON"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wo ", "tokens": ["Wo"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Mit sammt der Kitzelung zur Wohnung auffgeschlagen/", "tokens": ["Mit", "sammt", "der", "Kit\u00b7ze\u00b7lung", "zur", "Woh\u00b7nung", "auff\u00b7ge\u00b7schla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "NN", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wo sich die Anmuth hat den dreyen zugesellt/", "tokens": ["Wo", "sich", "die", "An\u00b7muth", "hat", "den", "drey\u00b7en", "zu\u00b7ge\u00b7sellt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "VAFIN", "ART", "CARD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wohin uns die Natur den besten Schatz getragen.", "tokens": ["Wo\u00b7hin", "uns", "die", "Na\u00b7tur", "den", "bes\u00b7ten", "Schatz", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wo Milch und Honig fleust/ wo ein Gelobtes-Land/", "tokens": ["Wo", "Milch", "und", "Ho\u00b7nig", "fleust", "/", "wo", "ein", "Ge\u00b7lob\u00b7tes\u00b7Land", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVFIN", "$(", "PWAV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wo Zucker und ", "tokens": ["Wo", "Zu\u00b7cker", "und"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "NN", "KON"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Wo Liebes-Rosen zeugt der Alabaster Strand/", "tokens": ["Wo", "Lie\u00b7bes\u00b7Ro\u00b7sen", "zeugt", "der", "A\u00b7la\u00b7bas\u00b7ter", "Strand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Daselbst verg\u00f6nne mir die L\u00fcste zu geniessen.", "tokens": ["Da\u00b7selbst", "ver\u00b7g\u00f6n\u00b7ne", "mir", "die", "L\u00fcs\u00b7te", "zu", "ge\u00b7nies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So dis ", "tokens": ["So", "dis"], "token_info": ["word", "word"], "pos": ["ADV", "PDS"], "meter": "++", "measure": "spondeus"}, "line.14": {"text": "Die mich mit ", "tokens": ["Die", "mich", "mit"], "token_info": ["word", "word", "word"], "pos": ["ART", "PPER", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.15": {"text": "In welchem sich mehr Lust als Sand am Meere findt/", "tokens": ["In", "wel\u00b7chem", "sich", "mehr", "Lust", "als", "Sand", "am", "Mee\u00b7re", "findt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "PIAT", "NN", "KOUS", "NN", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die ich mit ", "tokens": ["Die", "ich", "mit"], "token_info": ["word", "word", "word"], "pos": ["ART", "PPER", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.17": {"text": "Weil sein erdichtes Reich nicht solche Engel hegt/", "tokens": ["Weil", "sein", "er\u00b7dich\u00b7tes", "Reich", "nicht", "sol\u00b7che", "En\u00b7gel", "hegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PTKNEG", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die uns mit einer Kost von ", "tokens": ["Die", "uns", "mit", "ei\u00b7ner", "Kost", "von"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Man da nur blossen Schein zu seiner Tafel tr\u00e4gt/", "tokens": ["Man", "da", "nur", "blos\u00b7sen", "Schein", "zu", "sei\u00b7ner", "Ta\u00b7fel", "tr\u00e4gt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ADV", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wenn man schon Lust verlangt kan man nur Unmuth haben.", "tokens": ["Wenn", "man", "schon", "Lust", "ver\u00b7langt", "kan", "man", "nur", "Un\u00b7muth", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "NN", "VVFIN", "VMFIN", "PIS", "ADV", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wie sich nur ", "tokens": ["Wie", "sich", "nur"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PRF", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.22": {"text": "Weil die beliebte Frucht dem Munde wird entzogen/", "tokens": ["Weil", "die", "be\u00b7lieb\u00b7te", "Frucht", "dem", "Mun\u00b7de", "wird", "ent\u00b7zo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wie er im Wasser steht und \u00fcber Durst doch klagt/", "tokens": ["Wie", "er", "im", "Was\u00b7ser", "steht", "und", "\u00fc\u00b7ber", "Durst", "doch", "klagt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "VVFIN", "KON", "APPR", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "So gehts in ", "tokens": ["So", "gehts", "in"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.25": {"text": "Du aber schenckest mir/ ich weis nicht was vor Frucht", "tokens": ["Du", "a\u00b7ber", "schen\u00b7ckest", "mir", "/", "ich", "weis", "nicht", "was", "vor", "Frucht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "$(", "PPER", "PTKVZ", "PTKNEG", "PWS", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Wenn sich dein hohler Schoo\u00df mit meinen Gliedern paaret/", "tokens": ["Wenn", "sich", "dein", "hoh\u00b7ler", "Schoo\u00df", "mit", "mei\u00b7nen", "Glie\u00b7dern", "paa\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Den mein verirter Geist zu seinen Haven sucht/", "tokens": ["Den", "mein", "ver\u00b7ir\u00b7ter", "Geist", "zu", "sei\u00b7nen", "Ha\u00b7ven", "sucht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Wo er vor allen Sturm am besten ist verwahret.", "tokens": ["Wo", "er", "vor", "al\u00b7len", "Sturm", "am", "bes\u00b7ten", "ist", "ver\u00b7wah\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PIAT", "NN", "PTKA", "ADJD", "VAFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}