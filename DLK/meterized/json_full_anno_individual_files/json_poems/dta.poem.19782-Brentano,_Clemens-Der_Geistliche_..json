{"dta.poem.19782": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Der Geistliche .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Winter ist hin, der Pilgrim zieht ins Feld,               ", "tokens": ["Win\u00b7ter", "ist", "hin", ",", "der", "Pil\u00b7grim", "zieht", "ins", "Feld", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "$,", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Im Fr\u00fchling er sich umschaut in der Welt,", "tokens": ["Im", "Fr\u00fch\u00b7ling", "er", "sich", "um\u00b7schaut", "in", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "PRF", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wo er hinkommt, find er kein bleibend St\u00e4dt,", "tokens": ["Wo", "er", "hin\u00b7kommt", ",", "find", "er", "kein", "blei\u00b7bend", "St\u00e4dt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PIAT", "ADJD", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "F\u00fchlet ers jezt, was ihn da f\u00fchren wohl th\u00e4t,", "tokens": ["F\u00fch\u00b7let", "ers", "jezt", ",", "was", "ihn", "da", "f\u00fch\u00b7ren", "wohl", "th\u00e4t", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "$,", "PRELS", "PPER", "ADV", "VVFIN", "ADV", "VVFIN", "$,"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Im Sinn ihm liegen nur heilige Oerter,", "tokens": ["Im", "Sinn", "ihm", "lie\u00b7gen", "nur", "hei\u00b7li\u00b7ge", "O\u00b7er\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+--+--+--", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wohin er auch zieht, dahin nur begehrt er,", "tokens": ["Wo\u00b7hin", "er", "auch", "zieht", ",", "da\u00b7hin", "nur", "be\u00b7gehrt", "er", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "$,", "PAV", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-++-+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Von seinem Vorhaben zur\u00fccke nicht weichet,", "tokens": ["Von", "sei\u00b7nem", "Vor\u00b7ha\u00b7ben", "zu\u00b7r\u00fc\u00b7cke", "nicht", "wei\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+---+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Bis er das Veterland endlich erreichet.", "tokens": ["Bis", "er", "das", "Ve\u00b7ter\u00b7land", "end\u00b7lich", "er\u00b7rei\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Geistlicher Pilgrim, halt dich nicht auf,", "tokens": ["Geist\u00b7li\u00b7cher", "Pil\u00b7grim", ",", "halt", "dich", "nicht", "auf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$,"], "meter": "+--++-+-+", "measure": "dactylic.init"}, "line.2": {"text": "La\u00df dich nicht hindern, weit ist dein Lauf,", "tokens": ["La\u00df", "dich", "nicht", "hin\u00b7dern", ",", "weit", "ist", "dein", "Lauf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "VVINF", "$,", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Hie in kein Ding verliebe dich sehr,", "tokens": ["Hie", "in", "kein", "Ding", "ver\u00b7lie\u00b7be", "dich", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Sonst machen sie dir die Reise nur schwer,", "tokens": ["Sonst", "ma\u00b7chen", "sie", "dir", "die", "Rei\u00b7se", "nur", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "All falschen Betrug im Gesang der Sirenen,", "tokens": ["All", "fal\u00b7schen", "Be\u00b7trug", "im", "Ge\u00b7sang", "der", "Si\u00b7re\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Liebkosen der Welt du wei\u00dft zu verh\u00f6hnen,", "tokens": ["Lieb\u00b7ko\u00b7sen", "der", "Welt", "du", "wei\u00dft", "zu", "ver\u00b7h\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Ach bist du erm\u00fcdet, wie rauh sind die Wege,", "tokens": ["Ach", "bist", "du", "er\u00b7m\u00fc\u00b7det", ",", "wie", "rauh", "sind", "die", "We\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VAFIN", "PPER", "VVFIN", "$,", "PWAV", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.8": {"text": "Wie wird es so dunkel, wie schmal sind sie Stege.", "tokens": ["Wie", "wird", "es", "so", "dun\u00b7kel", ",", "wie", "schmal", "sind", "sie", "Ste\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADJD", "$,", "PWAV", "ADJD", "VAFIN", "PPER", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Ich bin ein Pilgrim, reis' ins heilige Land,", "tokens": ["Ich", "bin", "ein", "Pil\u00b7grim", ",", "reis'", "ins", "hei\u00b7li\u00b7ge", "Land", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Ob ich komm wieder, das ist Gott bekannt,", "tokens": ["Ob", "ich", "komm", "wie\u00b7der", ",", "das", "ist", "Gott", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADV", "$,", "PDS", "VAFIN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nach Rom, Lorett in Italia,", "tokens": ["Nach", "Rom", ",", "Lo\u00b7rett", "in", "I\u00b7ta\u00b7lia", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch nach St. Jakob in Galitia.", "tokens": ["Auch", "nach", "St.", "Ja\u00b7kob", "in", "Ga\u00b7li\u00b7tia", "."], "token_info": ["word", "word", "abbreviation", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "NE", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gott mich begleite, da\u00df ichs gl\u00fccklich ende,", "tokens": ["Gott", "mich", "be\u00b7glei\u00b7te", ",", "da\u00df", "ichs", "gl\u00fcck\u00b7lich", "en\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "$,", "KOUS", "PIS", "ADJD", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Mein M\u00fch und Zeit zu seinem Dienst anwende,", "tokens": ["Mein", "M\u00fch", "und", "Zeit", "zu", "sei\u00b7nem", "Dienst", "an\u00b7wen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "All Tritt und Schritt geschehen ihm zu Ehren,", "tokens": ["All", "Tritt", "und", "Schritt", "ge\u00b7sche\u00b7hen", "ihm", "zu", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "VVPP", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Er geb mir Gnad, da\u00df ich m\u00f6g wiederkehren.", "tokens": ["Er", "geb", "mir", "Gnad", ",", "da\u00df", "ich", "m\u00f6g", "wie\u00b7der\u00b7keh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$,", "KOUS", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+++-+-", "measure": "unknown.measure.hexa"}}, "stanza.4": {"line.1": {"text": "Viel mu\u00df ich leiden auf der Wanderschaft,", "tokens": ["Viel", "mu\u00df", "ich", "lei\u00b7den", "auf", "der", "Wan\u00b7der\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ach lieber Herr verleih mir St\u00e4rk und Kraft,", "tokens": ["Ach", "lie\u00b7ber", "Herr", "ver\u00b7leih", "mir", "St\u00e4rk", "und", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "NN", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denn der Gefahr ich unterworfen bin,", "tokens": ["Denn", "der", "Ge\u00b7fahr", "ich", "un\u00b7ter\u00b7wor\u00b7fen", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hilft nichts daf\u00fcr, ich schlag mirs aus dem Sinn.", "tokens": ["Hilft", "nichts", "da\u00b7f\u00fcr", ",", "ich", "schlag", "mirs", "aus", "dem", "Sinn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PAV", "$,", "PPER", "VVFIN", "NE", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mein schweres B\u00fcndel mu\u00df ich selber tragen,", "tokens": ["Mein", "schwe\u00b7res", "B\u00fcn\u00b7del", "mu\u00df", "ich", "sel\u00b7ber", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wei\u00df keinen Weg, darum mu\u00df ich oft fragen,", "tokens": ["Wei\u00df", "kei\u00b7nen", "Weg", ",", "da\u00b7rum", "mu\u00df", "ich", "oft", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$,", "PAV", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Gro\u00df Ungewitter, Ungelegenheiten,", "tokens": ["Gro\u00df", "Un\u00b7ge\u00b7wit\u00b7ter", ",", "Un\u00b7ge\u00b7le\u00b7gen\u00b7hei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "NE", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mich werden plagen, ich sehs schon von weiten.", "tokens": ["Mich", "wer\u00b7den", "pla\u00b7gen", ",", "ich", "sehs", "schon", "von", "wei\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAINF", "VVFIN", "$,", "PPER", "ADV", "ADV", "APPR", "ADJA", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Der bittre Hunger mir die Kr\u00e4fte fri\u00dft,", "tokens": ["Der", "bitt\u00b7re", "Hun\u00b7ger", "mir", "die", "Kr\u00e4f\u00b7te", "fri\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der t\u00e4glich Durst mein steter Gleitsmann ist,", "tokens": ["Der", "t\u00e4g\u00b7lich", "Durst", "mein", "ste\u00b7ter", "Gleits\u00b7mann", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "PPOSAT", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bey langem Tag, wohl in dem Sommer hei\u00df", "tokens": ["Bey", "lan\u00b7gem", "Tag", ",", "wohl", "in", "dem", "Som\u00b7mer", "hei\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "ADV", "APPR", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Thu ich vergiessen manchen Tropfen Schweis.", "tokens": ["Thu", "ich", "ver\u00b7gies\u00b7sen", "man\u00b7chen", "Trop\u00b7fen", "Schweis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Geld hab ich nicht, davon ich m\u00f6chte zehren,", "tokens": ["Geld", "hab", "ich", "nicht", ",", "da\u00b7von", "ich", "m\u00f6ch\u00b7te", "zeh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PTKNEG", "$,", "PAV", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Doch trau ich Gott, der wird mir Speis bescheren.", "tokens": ["Doch", "trau", "ich", "Gott", ",", "der", "wird", "mir", "Speis", "be\u00b7sche\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "$,", "PRELS", "VAFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die m\u00fcden F\u00fc\u00df mich machen schier verzagen,", "tokens": ["Die", "m\u00fc\u00b7den", "F\u00fc\u00df", "mich", "ma\u00b7chen", "schier", "ver\u00b7za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Gern h\u00e4ttens, da\u00df ich sie am Hals th\u00e4t tragen.", "tokens": ["Gern", "h\u00e4t\u00b7tens", ",", "da\u00df", "ich", "sie", "am", "Hals", "th\u00e4t", "tra\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "KOUS", "PPER", "PPER", "APPRART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Komm ich zu einem klaren Wasserbach,", "tokens": ["Komm", "ich", "zu", "ei\u00b7nem", "kla\u00b7ren", "Was\u00b7ser\u00b7bach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bald um ein gutes besser wird mein Sach,", "tokens": ["Bald", "um", "ein", "gu\u00b7tes", "bes\u00b7ser", "wird", "mein", "Sach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich halt mich auf dabey, leg die B\u00fcrd,", "tokens": ["Ich", "halt", "mich", "auf", "da\u00b7bey", ",", "leg", "die", "B\u00fcrd", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PAV", "$,", "NE", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Mir ist, als wenn ich neu geboren w\u00fcrd,", "tokens": ["Mir", "ist", ",", "als", "wenn", "ich", "neu", "ge\u00b7bo\u00b7ren", "w\u00fcrd", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "KOUS", "PPER", "ADJD", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich tret hinein und thu mich recht abk\u00fchlen,", "tokens": ["Ich", "tret", "hin\u00b7ein", "und", "thu", "mich", "recht", "ab\u00b7k\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "VVFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Fast alle Glieder mein das K\u00fchl bald f\u00fchlen,", "tokens": ["Fast", "al\u00b7le", "Glie\u00b7der", "mein", "das", "K\u00fchl", "bald", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "PPOSAT", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ich spr\u00fctz mirs ins Gesicht und thu mich waschen,", "tokens": ["Ich", "spr\u00fctz", "mirs", "ins", "Ge\u00b7sicht", "und", "thu", "mich", "wa\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "APPRART", "NN", "KON", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und f\u00fcll wohl auch damit mein Pilgertaschen.", "tokens": ["Und", "f\u00fcll", "wohl", "auch", "da\u00b7mit", "mein", "Pil\u00b7ger\u00b7ta\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "PAV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Ein gr\u00fcnen Baum ich seh gar schattenreich,", "tokens": ["Ein", "gr\u00fc\u00b7nen", "Baum", "ich", "seh", "gar", "schat\u00b7ten\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Darunter ich mich niederlasse gleich,", "tokens": ["Da\u00b7run\u00b7ter", "ich", "mich", "nie\u00b7der\u00b7las\u00b7se", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PRF", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich schau hinauf, ob er von Obst hat was,", "tokens": ["Ich", "schau", "hin\u00b7auf", ",", "ob", "er", "von", "Obst", "hat", "was", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "KOUS", "PPER", "APPR", "NN", "VAFIN", "PIS", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit Stein und Pr\u00fcgeln ich ihm abnehm das.", "tokens": ["Mit", "Stein", "und", "Pr\u00fc\u00b7geln", "ich", "ihm", "ab\u00b7nehm", "das", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PPER", "PPER", "VVFIN", "PDS", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Den matten K\u00f6rper thu ich wacker laben,", "tokens": ["Den", "mat\u00b7ten", "K\u00f6r\u00b7per", "thu", "ich", "wa\u00b7cker", "la\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die S\u00e4ck ich voll anschieb, wenn ichs kann haben,", "tokens": ["Die", "S\u00e4ck", "ich", "voll", "an\u00b7schieb", ",", "wenn", "ichs", "kann", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADJD", "VVFIN", "$,", "KOUS", "PIS", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Damit den Durst und Hunger ich vertreibe,", "tokens": ["Da\u00b7mit", "den", "Durst", "und", "Hun\u00b7ger", "ich", "ver\u00b7trei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "KON", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und derg