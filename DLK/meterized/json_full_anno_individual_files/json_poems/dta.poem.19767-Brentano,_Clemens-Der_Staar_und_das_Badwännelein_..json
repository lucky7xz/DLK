, "was", "weinst", "du", "dann", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPOSAT", "NN", "PWS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bin ich dir nicht gut f\u00fcr einen Mann,", "tokens": ["Bin", "ich", "dir", "nicht", "gut", "f\u00fcr", "ei\u00b7nen", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "PTKNEG", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.18": {"line.1": {"text": "Du bist mir gut f\u00fcr einen Mann,", "tokens": ["Du", "bist", "mir", "gut", "f\u00fcr", "ei\u00b7nen", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich wein \u00fcber, was der Staar mir sang.", "tokens": ["Ich", "wein", "\u00fc\u00b7ber", ",", "was", "der", "Staar", "mir", "sang", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.19": {"line.1": {"text": "Ich war im Garten und brach das Kraut,", "tokens": ["Ich", "war", "im", "Gar\u00b7ten", "und", "brach", "das", "Kraut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da sang der Staar: o weh du Braut!", "tokens": ["Da", "sang", "der", "Staar", ":", "o", "weh", "du", "Braut", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "