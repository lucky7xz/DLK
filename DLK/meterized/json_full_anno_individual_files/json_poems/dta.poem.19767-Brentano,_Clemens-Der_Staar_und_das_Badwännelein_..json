d", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "$(", "FM", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "\u201ein dem Badw\u00e4nnelein ist sie hergetragen,", "tokens": ["\u201e", "in", "dem", "Bad\u00b7w\u00e4n\u00b7nel\u00b7ein", "ist", "sie", "her\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "\u201edarin mu\u00df sie ihm die F\u00fc\u00dfe zwagen,", "tokens": ["\u201e", "da\u00b7rin", "mu\u00df", "sie", "ihm", "die", "F\u00fc\u00b7\u00dfe", "zwa\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PAV", "VMFIN", "PPER", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.13": {"line.1": {"text": "\u201eder Vater starb in Leid und Noth,", "tokens": ["\u201e", "der", "Va\u00b7ter", "starb", "in", "Leid", "und", "Noth", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201edie Mutter gr\u00e4mt sich schier zu todt.", "tokens": ["\u201e", "die", "Mut\u00b7ter", "gr\u00e4mt", "sich", "schier", "zu", "todt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PRF", "ADJD", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "\u201eo weh du Braut! du Findelkind,", "tokens": ["\u201e", "o", "weh", "du", "Braut", "!", "du", "Fin\u00b7del\u00b7kind", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "w