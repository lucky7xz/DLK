{"dta.poem.19767": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Der Staar und das Badw\u00e4nnelein .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Herr Konrad war ein m\u00fcder Mann,               ", "tokens": ["Herr", "Kon\u00b7rad", "war", "ein", "m\u00fc\u00b7der", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er band sein Ro\u00df am Wirthshaus an.", "tokens": ["Er", "band", "sein", "Ro\u00df", "am", "Wirths\u00b7haus", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das M\u00e4gdlein sprach, steig ab, steig ab,", "tokens": ["Das", "M\u00e4gd\u00b7lein", "sprach", ",", "steig", "ab", ",", "steig", "ab", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VVFIN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihre Aeuglein schwankten auf und ab.", "tokens": ["Ih\u00b7re", "A\u00b7e\u00b7u\u00b7glein", "schwank\u00b7ten", "auf", "und", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.3": {"line.1": {"text": "Ach Jungfer liebste Jungfrau mein,", "tokens": ["Ach", "Jung\u00b7fer", "liebs\u00b7te", "Jung\u00b7frau", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "ADJA", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schenk mir ein Becher k\u00fchlen Wein ein,", "tokens": ["Schenk", "mir", "ein", "Be\u00b7cher", "k\u00fch\u00b7len", "Wein", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach Herre, lieber Herre mein!", "tokens": ["Ach", "Her\u00b7re", ",", "lie\u00b7ber", "Her\u00b7re", "mein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "ADV", "NN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich bring ein Becher k\u00fchlen Wein.", "tokens": ["Ich", "bring", "ein", "Be\u00b7cher", "k\u00fch\u00b7len", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Trink ab, trink ab du rother Mund,", "tokens": ["Trink", "ab", ",", "trink", "ab", "du", "ro\u00b7ther", "Mund", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "VVFIN", "PTKVZ", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Trink aus den Becher auf den Grund.", "tokens": ["Trink", "aus", "den", "Be\u00b7cher", "auf", "den", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.6": {"line.1": {"text": "Frau Wirthin, liebe Frau Wirthin mein,", "tokens": ["Frau", "Wirt\u00b7hin", ",", "lie\u00b7be", "Frau", "Wirt\u00b7hin", "mein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADJA", "NN", "NN", "PPOSAT", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ist dies f\u00fcrwahr euer T\u00f6chterlein?", "tokens": ["Ist", "dies", "f\u00fcr\u00b7wahr", "eu\u00b7er", "T\u00f6ch\u00b7ter\u00b7lein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Mein T\u00f6chterlein ist sie nicht f\u00fcrwahr,", "tokens": ["Mein", "T\u00f6ch\u00b7ter\u00b7lein", "ist", "sie", "nicht", "f\u00fcr\u00b7wahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "PTKNEG", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie ist mein Magd f\u00fcr immerdar.", "tokens": ["Sie", "ist", "mein", "Magd", "f\u00fcr", "im\u00b7mer\u00b7dar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "APPR", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Wollt ihr mir sie leihen auf eine Nacht?", "tokens": ["Wollt", "ihr", "mir", "sie", "lei\u00b7hen", "auf", "ei\u00b7ne", "Nacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PPER", "VVINF", "APPR", "ART", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "So will ich euch geben des Goldes Macht.", "tokens": ["So", "will", "ich", "euch", "ge\u00b7ben", "des", "Gol\u00b7des", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tr