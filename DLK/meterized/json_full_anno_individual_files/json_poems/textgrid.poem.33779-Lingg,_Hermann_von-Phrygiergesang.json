{"textgrid.poem.33779": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Phrygiergesang", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Urg\u00f6ttin des Alls, o vernimm in den H\u00f6h'n", "tokens": ["Ur\u00b7g\u00f6t\u00b7tin", "des", "Alls", ",", "o", "ver\u00b7nimm", "in", "den", "H\u00f6h'n"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "FM", "ADJD", "APPR", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Korybantengesang zu der Zimbeln Get\u00f6n,", "tokens": ["Ko\u00b7ry\u00b7ban\u00b7ten\u00b7ge\u00b7sang", "zu", "der", "Zim\u00b7beln", "Ge\u00b7t\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "In der waldigen Nacht zu der Str\u00f6me Gebraus!", "tokens": ["In", "der", "wal\u00b7di\u00b7gen", "Nacht", "zu", "der", "Str\u00f6\u00b7me", "Ge\u00b7braus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "O wie sehnt uns nach dir in die St\u00fcrme hinaus,", "tokens": ["O", "wie", "sehnt", "uns", "nach", "dir", "in", "die", "St\u00fcr\u00b7me", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VVFIN", "PPER", "APPR", "PPER", "APPR", "ART", "NN", "APZR", "$,"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Durch die Schlucht des Gebirgs mit entfesselter Brust,", "tokens": ["Durch", "di