{"textgrid.poem.59821": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Lehrer ist Hafis, mein Bethaus ist die Schenke,", "tokens": ["Mein", "Leh\u00b7rer", "ist", "Ha\u00b7fis", ",", "mein", "Be\u00b7thaus", "ist", "die", "Schen\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NE", "$,", "PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ich liebe gute Menschen und st\u00e4rkende Getr\u00e4nke;", "tokens": ["Ich", "lie\u00b7be", "gu\u00b7te", "Men\u00b7schen", "und", "st\u00e4r\u00b7ken\u00b7de", "Ge\u00b7tr\u00e4n\u00b7ke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+--+---+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Drum bin ich wohlgelitten in den Kreisen", "tokens": ["Drum", "bin", "ich", "wohl\u00b7ge\u00b7lit\u00b7ten", "in", "den", "Krei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Zecher, und sie nennen mich den Weisen,", "tokens": ["Der", "Ze\u00b7cher", ",", "und", "sie", "nen\u00b7nen", "mich", "den", "Wei\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Komm ich \u2013 da kommt der Weise! sagen sie;", "tokens": ["Komm", "ich", "\u2013", "da", "kommt", "der", "Wei\u00b7se", "!", "sa\u00b7gen", "sie", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "ADV", "VVFIN", "ART", "NN", "$.", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Geh ich \u2013 schon geht der Weise! klagen sie;", "tokens": ["Geh", "ich", "\u2013", "schon", "geht", "der", "Wei\u00b7se", "!", "kla\u00b7gen", "sie", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPER", "$(", "ADV", "VVFIN", "ART", "NN", "$.", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Fehl ich \u2013 wo steckt der Weise? fragen sie!", "tokens": ["Fehl", "ich", "\u2013", "wo", "steckt", "der", "Wei\u00b7se", "?", "fra\u00b7gen", "sie", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$(", "PWAV", "VVFIN", "ART", "NN", "$.", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Bleib ich \u2013 in lust'ger Weise schlagen sie", "tokens": ["Bleib", "ich", "\u2013", "in", "lust'\u00b7ger", "Wei\u00b7se", "schla\u00b7gen", "sie"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$(", "APPR", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Laut Glas an Glas. Drum bitt' ich Gott den Herrn,", "tokens": ["Laut", "Glas", "an", "Glas", ".", "Drum", "bitt'", "ich", "Gott", "den", "Herrn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$.", "PAV", "VVFIN", "PPER", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Da\u00df er stets Herz und Fu\u00df die rechten Pfade lenke,", "tokens": ["Da\u00df", "er", "stets", "Herz", "und", "Fu\u00df", "die", "rech\u00b7ten", "Pfa\u00b7de", "len\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "KON", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Weit ab von der Moschee und allen Bonzen fern", "tokens": ["Weit", "ab", "von", "der", "Mo\u00b7schee", "und", "al\u00b7len", "Bon\u00b7zen", "fern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "PTKVZ", "APPR", "ART", "NN", "KON", "PIAT", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Mein Herz zur Liebe f\u00fchre und meinen Fu\u00df zur Schenke;", "tokens": ["Mein", "Herz", "zur", "Lie\u00b7be", "f\u00fch\u00b7re", "und", "mei\u00b7nen", "Fu\u00df", "zur", "Schen\u00b7ke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVFIN", "KON", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Da\u00df ich dem Wahn der Menschen und ihrer Dummheit ferne", "tokens": ["Da\u00df", "ich", "dem", "Wahn", "der", "Men\u00b7schen", "und", "ih\u00b7rer", "Dumm\u00b7heit", "fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "KON", "PPOSAT", "NN", "ADJA"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Das R\u00e4tsel meines Daseins im Becher Weins ergr\u00fcnde,", "tokens": ["Das", "R\u00e4t\u00b7sel", "mei\u00b7nes", "Da\u00b7seins", "im", "Be\u00b7cher", "Weins", "er\u00b7gr\u00fc