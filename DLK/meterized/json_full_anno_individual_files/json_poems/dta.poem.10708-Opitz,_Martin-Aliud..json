{"dta.poem.10708": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Aliud.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.85", "nl:0.14"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Ob vns der Liebe last schon viel pein leget an/", "tokens": ["Ob", "vns", "der", "Lie\u00b7be", "last", "schon", "viel", "pein", "le\u00b7get", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "ADV", "PIAT", "NN", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So frewet man sich doch/ wann man jhr Huld erwirbet/", "tokens": ["So", "fre\u00b7wet", "man", "sich", "doch", "/", "wann", "man", "jhr", "Huld", "er\u00b7wir\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PRF", "ADV", "$(", "PWAV", "PIS", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ohn jhre Werck die Welt gar nicht bestehen kan/", "tokens": ["Ohn", "jhre", "Werck", "die", "Welt", "gar", "nicht", "be\u00b7ste\u00b7hen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "ADV", "PTKNEG", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Wer nicht verderbet wird durch Liebe/ der verdirbet.", "tokens": ["Wer", "nicht", "ver\u00b7der\u00b7bet", "wird", "durch", "Lie\u00b7be", "/", "der", "ver\u00b7dir\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct