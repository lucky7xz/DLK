{"textgrid.poem.59887": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gott hie\u00df die Sonne gl\u00fchen", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott hie\u00df die Sonne gl\u00fchen", "tokens": ["Gott", "hie\u00df", "die", "Son\u00b7ne", "gl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und leuchten durch alle Welt;", "tokens": ["Und", "leuch\u00b7ten", "durch", "al\u00b7le", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Er hie\u00df die Rose bl\u00fchen", "tokens": ["Er", "hie\u00df", "die", "Ro\u00b7se", "bl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf duftigem Blumenfeld.", "tokens": ["Auf", "duf\u00b7ti\u00b7gem", "Blu\u00b7men\u00b7feld", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Er hie\u00df die Berge sich t\u00fcrmen", "tokens": ["Er", "hie\u00df", "die", "Ber\u00b7ge", "sich", "t\u00fcr\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PRF", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und \u00fcber die Lande erheben \u2013", "tokens": ["Und", "\u00fc\u00b7ber", "die", "Lan\u00b7de", "er\u00b7he\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Lie\u00df Winde wehen und st\u00fcrmen,", "tokens": ["Lie\u00df", "Win\u00b7de", "we\u00b7hen", "und", "st\u00fcr\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Schuf vielgestaltiges Leben.", "tokens": ["Schuf", "viel\u00b7ge\u00b7stal\u00b7ti\u00b7ges", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Er gab den V\u00f6geln Gefieder,", "tokens": ["Er", "gab", "den", "V\u00f6\u00b7geln", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Dem Meere sein ewiges Rauschen,", "tokens": ["Dem", "Mee\u00b7re", "sein", "e\u00b7wi\u00b7ges", "Rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Mir gab er sinnige Lieder,", "tokens": ["Mir", "gab", "er", "sin\u00b7ni\u00b7ge", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Euch Ohren, ihnen zu lauschen!", "tokens": ["Euch", "Oh\u00b7ren", ",", "ih\u00b7nen", "zu", "lau\u00b7schen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Gott hie\u00d