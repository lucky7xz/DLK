{"textgrid.poem.66300": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Alles ist Euer", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alle ist Euer, o Worte des ewigen Lebens!", "tokens": ["Al\u00b7le", "ist", "Eu\u00b7er", ",", "o", "Wor\u00b7te", "des", "e\u00b7wi\u00b7gen", "Le\u00b7bens", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPOSAT", "$,", "FM", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.2": {"text": "F\u00fchl' sie, Vertrauter des Mittlers, voll heiliges Bebens!", "tokens": ["F\u00fchl'", "sie", ",", "Ver\u00b7trau\u00b7ter", "des", "Mitt\u00b7lers", ",", "voll", "hei\u00b7li\u00b7ges", "Be\u00b7bens", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "NN", "ART", "NN", "$,", "ADJD", "ADJA", "NN", "$."], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.3": {"text": "Alles ist dein!", "tokens": ["Al\u00b7les", "ist", "dein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPOSAT", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Irdischen Menschen allein", "tokens": ["Ir\u00b7di\u00b7schen", "Men\u00b7schen", "al\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "ADV"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "T\u00f6nen die Worte vergebens.", "tokens": ["T\u00f6\u00b7nen", "die", "Wor\u00b7te", "ver\u00b7ge\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "G\u00f6ttliche W\u00fcrde! Entz\u00fcckende Hoheit des Christen!", "tokens": ["G\u00f6tt\u00b7li\u00b7che", "W\u00fcr\u00b7de", "!", "Ent\u00b7z\u00fc\u00b7cken\u00b7de", "Ho\u00b7heit", "des", "Chris\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.2": {"text": "Ist er gleich d\u00fcrftig, ein Waller in traurigen W\u00fcsten,", "tokens": ["Ist", "er", "gleich", "d\u00fcrf\u00b7tig", ",", "ein", "Wal\u00b7ler", "in", "trau\u00b7ri\u00b7gen", "W\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$,", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.3": {"text": "Findet er gleich", "tokens": ["Fin\u00b7det", "er", "gleich"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Thoren geachtet und reich", "tokens": ["Tho\u00b7ren", "ge\u00b7ach\u00b7tet", "und", "reich"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVPP", "KON", "ADJD"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Sklaven von thierischen L\u00fcsten:", "tokens": ["Skla\u00b7ven", "von", "thie\u00b7ri\u00b7schen", "L\u00fcs\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Bleibt doch sein Auge gerichtet nach heiligen H\u00f6hen;", "tokens": ["Bleibt", "doch", "sein", "Au\u00b7ge", "ge\u00b7rich\u00b7tet", "nach", "hei\u00b7li\u00b7gen", "H\u00f6\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.2": {"text": "G\u00fcter der Thoren, die sieht er im Sturme verwehen!", "tokens": ["G\u00fc\u00b7ter", "der", "Tho\u00b7ren", ",", "die", "sieht", "er", "im", "Stur\u00b7me", "ver\u00b7we\u00b7hen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "VVFIN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.3": {"text": "Aber er fa\u00dft,", "tokens": ["A\u00b7ber", "er", "fa\u00dft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Statt der verg\u00e4nglichen Last,", "tokens": ["Statt", "der", "ver\u00b7g\u00e4ng\u00b7li\u00b7chen", "Last", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "G\u00fcter, die nimmer vergehen.", "tokens": ["G\u00fc\u00b7ter", ",", "die", "nim\u00b7mer", "ver\u00b7ge\u00b7hen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Jede geheiligte Gabe des gr\u00f6\u00dferen Mannes,", "tokens": ["Je\u00b7de", "ge\u00b7hei\u00b7lig\u00b7te", "Ga\u00b7be", "des", "gr\u00f6\u00b7\u00dfe\u00b7ren", "Man\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.2": {"text": "Paulus und Kephas, Apollo, Jacobus, Johannes,", "tokens": ["Pau\u00b7lus", "und", "Ke\u00b7phas", ",", "A\u00b7pol\u00b7lo", ",", "Ja\u00b7co\u00b7bus", ",", "Jo\u00b7han\u00b7nes", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "KON", "NE", "$,", "NE", "$,", "NE", "$,", "NE", "$,"], "meter": "+--+--+-++--+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Luther der Held,", "tokens": ["Lu\u00b7ther", "der", "Held", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Der sich entgegenstellt", "tokens": ["Der", "sich", "ent\u00b7ge\u00b7gen\u00b7stellt"], "token_info": ["word", "word", "word"], "pos": ["ART", "PRF", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Blitzen des p\u00e4pstlichen Bannes:", "tokens": ["Blit\u00b7zen", "des", "p\u00e4pst\u00b7li\u00b7chen", "Ban\u00b7nes", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "Alle Geschenke der Erde, die Menschen umgeben;", "tokens": ["Al\u00b7le", "Ge\u00b7schen\u00b7ke", "der", "Er\u00b7de", ",", "die", "Men\u00b7schen", "um\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$,", "ART", "NN", "VVPP", "$."], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.2": {"text": "Dinge, die k\u00fcnftig die Allmacht des Sch\u00f6pfers erheben,", "tokens": ["Din\u00b7ge", ",", "die", "k\u00fcnf\u00b7tig", "die", "All\u00b7macht", "des", "Sch\u00f6p\u00b7fers", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADJD", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.3": {"text": "Leben und Tod", "tokens": ["Le\u00b7ben", "und", "Tod"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Ist euch auf Gottes Gebot", "tokens": ["Ist", "euch", "auf", "Got\u00b7tes", "Ge\u00b7bot"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "NN", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Unter die F\u00fc\u00dfe gegeben.", "tokens": ["Un\u00b7ter", "die", "F\u00fc\u00b7\u00dfe", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.6": {"line.1": {"text": "Alles ist euer, ihr Christen! Vom Heiligthum nieder", "tokens": ["Al\u00b7les", "ist", "eu\u00b7er", ",", "ihr", "Chris\u00b7ten", "!", "Vom", "Hei\u00b7lig\u00b7thum", "nie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VAFIN", "PPOSAT", "$,", "PPOSAT", "NN", "$.", "APPRART", "NN", "PTKVZ"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.2": {"text": "Schaut selbst der Mittler auf seine ihm \u00e4hnlichen Glieder;", "tokens": ["Schaut", "selbst", "der", "Mitt\u00b7ler", "auf", "sei\u00b7ne", "ihm", "\u00e4hn\u00b7li\u00b7chen", "Glie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "PPER", "ADJA", "NN", "$."], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "L\u00e4chelt und spricht,", "tokens": ["L\u00e4\u00b7chelt", "und", "spricht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Frieden und Gnad' im Gesicht:", "tokens": ["Frie\u00b7den", "und", "Gnad'", "im", "Ge\u00b7sicht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPRART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Alles ist euer, ihr Br\u00fcder.", "tokens": ["Al\u00b7les", "ist", "eu\u00b7er", ",", "ihr", "Br\u00fc\u00b7der", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPOSAT", "$,", "PPOSAT", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.7": {"line.1": {"text": "Himmel und Erde und Welten und Sonnen und Meere,", "tokens": ["Him\u00b7mel", "und", "Er\u00b7de", "und", "Wel\u00b7ten", "und", "Son\u00b7nen", "und", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.2": {"text": "Geistergestalten, der Engel unz\u00e4hlige Heere,", "tokens": ["Geis\u00b7ter\u00b7ge\u00b7stal\u00b7ten", ",", "der", "En\u00b7gel", "un\u00b7z\u00e4h\u00b7li\u00b7ge", "Hee\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+--+-+---+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Alles ist dein,", "tokens": ["Al\u00b7les", "ist", "dein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPOSAT", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Bruder! o jauchze mit drein;", "tokens": ["Bru\u00b7der", "!", "o", "jauch\u00b7ze", "mit", "drein", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "FM", "VVFIN", "APPR", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Singe des Ewigen Ehre.", "tokens": ["Sin\u00b7ge", "des", "E\u00b7wi\u00b7gen", "Eh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.8": {"line.1": {"text": "Bist du oft elend, verlassen und krank und gefangen,", "tokens": ["Bist", "du", "oft", "e\u00b7lend", ",", "ver\u00b7las\u00b7sen", "und", "krank", "und", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$,", "VVPP", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.2": {"text": "Triefen dir Z\u00e4hren des Kummers von bl\u00e4sseren Wangen;", "tokens": ["Trie\u00b7fen", "dir", "Z\u00e4h\u00b7ren", "des", "Kum\u00b7mers", "von", "bl\u00e4s\u00b7se\u00b7ren", "Wan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.3": {"text": "Droben im Licht,", "tokens": ["Dro\u00b7ben", "im", "Licht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Freu' dich, da triefen sie nicht!", "tokens": ["Freu'", "dich", ",", "da", "trie\u00b7fen", "sie", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Dort ist das Alte vergangen.", "tokens": ["Dort", "ist", "das", "Al\u00b7te", "ver\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Singt denn, ihr k\u00fcnftigen Herrscher, in heiliger Feier,", "tokens": ["Singt", "denn", ",", "ihr", "k\u00fcnf\u00b7ti\u00b7gen", "Herr\u00b7scher", ",", "in", "hei\u00b7li\u00b7ger", "Fei\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Eure erstaunliche Herrlichkeit, die euch so theuer", "tokens": ["Eu\u00b7re", "er\u00b7staun\u00b7li\u00b7che", "Herr\u00b7lich\u00b7keit", ",", "die", "euch", "so", "theu\u00b7er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "ADJD"], "meter": "+--+--+-+-+-+-", "measure": "elegiambus"}, "line.3": {"text": "Jesus erwarb,", "tokens": ["Je\u00b7sus", "er\u00b7warb", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Als er auf Golgatha starb.", "tokens": ["Als", "er", "auf", "Gol\u00b7ga\u00b7tha", "starb", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NE", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Amen! ja Alles ist euer!", "tokens": ["A\u00b7men", "!", "ja", "Al\u00b7les", "ist", "eu\u00b7er", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "PIS", "VAFIN", "PPOSAT", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.10": {"line.1": {"text": "Alle ist Euer, o Worte des ewigen Lebens!", "tokens": ["Al\u00b7le", "ist", "Eu\u00b7er", ",", "o", "Wor\u00b7te", "des", "e\u00b7wi\u00b7gen", "Le\u00b7bens", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPOSAT", "$,", "FM", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.2": {"text": "F\u00fchl' sie, Vertrauter des Mittlers, voll