{"textgrid.poem.47334": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Thr\u00e4nenb\u00e4che", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich zanke mit Thr\u00e4nenb\u00e4chen", "tokens": ["Ich", "zan\u00b7ke", "mit", "Thr\u00e4\u00b7nen\u00b7b\u00e4\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Des Auges Tag und Nacht,", "tokens": ["Des", "Au\u00b7ges", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die aus dem Hause brechen", "tokens": ["Die", "aus", "dem", "Hau\u00b7se", "bre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit ungest\u00fcmer Macht.", "tokens": ["Mit", "un\u00b7ge\u00b7st\u00fc\u00b7mer", "Macht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich frage sie immer und immer:", "tokens": ["Ich", "fra\u00b7ge", "sie", "im\u00b7mer", "und", "im\u00b7mer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "KON", "ADV", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Wohin denn gehet ihr?", "tokens": ["Wo\u00b7hin", "denn", "ge\u00b7het", "ihr", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und andres er