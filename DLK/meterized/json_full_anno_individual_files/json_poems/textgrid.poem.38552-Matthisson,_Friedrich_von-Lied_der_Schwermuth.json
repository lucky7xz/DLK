{"textgrid.poem.38552": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Lied der Schwermuth", "genre": "verse", "period": "N.A.", "pub_year": 1780, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des sch\u00f6nsten Tages Abend sinkt bekr\u00f6nt,", "tokens": ["Des", "sch\u00f6ns\u00b7ten", "Ta\u00b7ges", "A\u00b7bend", "sinkt", "be\u00b7kr\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit glanzbes\u00e4umten Purpurwolken nieder,", "tokens": ["Mit", "glanz\u00b7be\u00b7s\u00e4um\u00b7ten", "Pur\u00b7pur\u00b7wol\u00b7ken", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des Haines Bild, vom goldnen Stral versch\u00f6nt,", "tokens": ["Des", "Hai\u00b7nes", "Bild", ",", "vom", "gold\u00b7nen", "Stral", "ver\u00b7sch\u00f6nt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Blinkt aus des See's krystallnem Spiegel wieder,", "tokens": ["Blinkt", "aus", "des", "See's", "krys\u00b7tall\u00b7nem", "Spie\u00b7gel", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "ADV", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.5": {"text": "Durch Feld und Wald und Saatgefilde t\u00f6nt", "tokens": ["Durch", "Feld", "und", "Wald", "und", "Saat\u00b7ge\u00b7fil\u00b7de", "t\u00f6nt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die S\u00e4ngerin der N\u00e4chte Zauberlieder,", "tokens": ["Die", "S\u00e4n\u00b7ge\u00b7rin", "der", "N\u00e4ch\u00b7te", "Zau\u00b7berl\u00b7ie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Jezt schwebst du, Herzerfreuerin, o Ruh'!", "tokens": ["Jezt", "schwebst", "du", ",", "Her\u00b7zer\u00b7freu\u00b7e\u00b7rin", ",", "o", "Ruh'", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der m\u00fcden Sch\u00f6pfung schlummertr\u00e4ufelnd zu!", "tokens": ["Der", "m\u00fc\u00b7den", "Sch\u00f6p\u00b7fung", "schlum\u00b7mer\u00b7tr\u00e4u\u00b7felnd", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O G\u00f6ttin, mit dem Engelangesicht,", "tokens": ["O", "G\u00f6t\u00b7tin", ",", "mit", "dem", "En\u00b7ge\u00b7lan\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von Gott zum Trost dem Sterblichen beschieden,", "tokens": ["Von", "Gott", "zum", "Trost", "dem", "Sterb\u00b7li\u00b7chen", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn jeder Stab in seinen H\u00e4nden bricht,", "tokens": ["Wenn", "je\u00b7der", "Stab", "in", "sei\u00b7nen", "H\u00e4n\u00b7den", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Du thaust jezt Labung auf das Haupt des M\u00fcden,", "tokens": ["Du", "thaust", "jezt", "La\u00b7bung", "auf", "das", "Haupt", "des", "M\u00fc\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Nur mir ins Herz str\u00f6mst du wie vormals nicht,", "tokens": ["Nur", "mir", "ins", "Herz", "str\u00f6mst", "du", "wie", "vor\u00b7mals", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPRART", "NN", "VVFIN", "PPER", "KOKOM", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Allg\u00fctige! des Himmels s\u00fcssen Frieden!", "tokens": ["All\u00b7g\u00fc\u00b7ti\u00b7ge", "!", "des", "Him\u00b7mels", "s\u00fcs\u00b7sen", "Frie\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$.", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Hast du, die einst mir stets zur Seite stand,", "tokens": ["Hast", "du", ",", "die", "einst", "mir", "stets", "zur", "Sei\u00b7te", "stand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "PRELS", "ADV", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Den Blik auf ewig von mir weggewandt?", "tokens": ["Den", "Blik", "auf", "e\u00b7wig", "von", "mir", "weg\u00b7ge\u00b7wandt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJD", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dich sucht, in banger, sternenloser Nacht", "tokens": ["Dich", "sucht", ",", "in", "ban\u00b7ger", ",", "ster\u00b7nen\u00b7lo\u00b7ser", "Nacht"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ADJD", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mein Geist, auf na\u00dfgeweinter Schlummerst\u00e4te,", "tokens": ["Mein", "Geist", ",", "auf", "na\u00df\u00b7ge\u00b7wein\u00b7ter", "Schlum\u00b7mer\u00b7st\u00e4\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo nur der Schwermuth tr\u00fcbes Auge wacht,", "tokens": ["Wo", "nur", "der", "Schwer\u00b7muth", "tr\u00fc\u00b7bes", "Au\u00b7ge", "wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dich, die mich einst zum Erdengott erh\u00f6hte,", "tokens": ["Dich", ",", "die", "mich", "einst", "zum", "Er\u00b7den\u00b7gott", "er\u00b7h\u00f6h\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Als noch, voll Herrlichkeit und Himmelspracht,", "tokens": ["Als", "noch", ",", "voll", "Herr\u00b7lich\u00b7keit", "und", "Him\u00b7mel\u00b7spracht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "$,", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Des Jugendlenzes erste Morgenr\u00f6the,", "tokens": ["Des", "Ju\u00b7gend\u00b7len\u00b7zes", "ers\u00b7te", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "O wie so rein, so mild, so wolkenlos", "tokens": ["O", "wie", "so", "rein", ",", "so", "mild", ",", "so", "wol\u00b7ken\u00b7los"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "KOKOM", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ihr sch\u00f6nes Licht auf meine Pfade go\u00df!", "tokens": ["Ihr", "sch\u00f6\u00b7nes", "Licht", "auf", "mei\u00b7ne", "Pfa\u00b7de", "go\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Als mein stillheitres, unbefangnes Herz", "tokens": ["Als", "mein", "still\u00b7heit\u00b7res", ",", "un\u00b7be\u00b7fang\u00b7nes", "Herz"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Das grosse Lebensschauspiel noch nicht kannte,", "tokens": ["Das", "gros\u00b7se", "Le\u00b7bens\u00b7schau\u00b7spiel", "noch", "nicht", "kann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bei fremder Freude, wie bei fremdem Schmerz", "tokens": ["Bei", "frem\u00b7der", "Freu\u00b7de", ",", "wie", "bei", "frem\u00b7dem", "Schmerz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Von engelreiner Mitempfindung brannte,", "tokens": ["Von", "en\u00b7gel\u00b7rei\u00b7ner", "Mi\u00b7temp\u00b7fin\u00b7dung", "brann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und ach! wie oft, gefl\u00fcgelt himmelw\u00e4rts,", "tokens": ["Und", "ach", "!", "wie", "oft", ",", "ge\u00b7fl\u00fc\u00b7gelt", "him\u00b7mel\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PWAV", "ADV", "$,", "VVPP", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In Andacht schmolz! \u2013 o Ruh! o Gottgesandte!", "tokens": ["In", "An\u00b7dacht", "schmolz", "!", "\u2013", "o", "Ruh", "!", "o", "Gott\u00b7ge\u00b7sand\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$.", "$(", "FM", "NN", "$.", "FM", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Da kr\u00e4nztest du mit Scherz und Freude mich,", "tokens": ["Da", "kr\u00e4nz\u00b7test", "du", "mit", "Scherz", "und", "Freu\u00b7de", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Da nannt' ich Schwester, Busenfreundin dich!", "tokens": ["Da", "nannt'", "ich", "Schwes\u00b7ter", ",", "Bu\u00b7sen\u00b7freun\u00b7din", "dich", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "NN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Im Nachtigallenhain, am Wasserfall,", "tokens": ["Im", "Nach\u00b7ti\u00b7gal\u00b7len\u00b7hain", ",", "am", "Was\u00b7ser\u00b7fall", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Am blumenvollen Hang beb\u00fcschter H\u00fcgel,", "tokens": ["Am", "blu\u00b7men\u00b7vol\u00b7len", "Hang", "be\u00b7b\u00fcschter", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Im Erlenwald, im bunten Fr\u00fchlingsthal,", "tokens": ["Im", "Er\u00b7len\u00b7wald", ",", "im", "bun\u00b7ten", "Fr\u00fch\u00b7lings\u00b7thal", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "An mondbegl\u00e4nzter B\u00e4che klarem Spiegel,", "tokens": ["An", "mond\u00b7be\u00b7gl\u00e4nz\u00b7ter", "B\u00e4\u00b7che", "kla\u00b7rem", "Spie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Im Morgenlicht, im Abendd\u00e4mmrungsstral,", "tokens": ["Im", "Mor\u00b7gen\u00b7licht", ",", "im", "A\u00b7bend\u00b7d\u00e4mm\u00b7rungs\u00b7stral", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Umschwebte wonnes\u00e4uselnd mich dein Fl\u00fcgel,", "tokens": ["Um\u00b7schweb\u00b7te", "won\u00b7ne\u00b7s\u00e4u\u00b7selnd", "mich", "dein", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Auf Rosen hingegossen, wehtest du", "tokens": ["Auf", "Ro\u00b7sen", "hin\u00b7ge\u00b7gos\u00b7sen", ",", "weh\u00b7test", "du"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "VVPP", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Mir Schlaf und Paradiesestr\u00e4ume zu!", "tokens": ["Mir", "Schlaf", "und", "Pa\u00b7ra\u00b7die\u00b7ses\u00b7tr\u00e4u\u00b7me", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Oft wenn in schlummerloser Nacht, eu'r Bild", "tokens": ["Oft", "wenn", "in", "schlum\u00b7mer\u00b7lo\u00b7ser", "Nacht", ",", "eu'r", "Bild"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "KOUS", "APPR", "ADJA", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit allen seinen tausend Seligkeiten", "tokens": ["Mit", "al\u00b7len", "sei\u00b7nen", "tau\u00b7send", "Se\u00b7lig\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "PPOSAT", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und goldnen Szenen mir die Seele f\u00fcllt,", "tokens": ["Und", "gold\u00b7nen", "Sze\u00b7nen", "mir", "die", "See\u00b7le", "f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "O holde, ruhgeweihte Knabenzeiten!", "tokens": ["O", "hol\u00b7de", ",", "ruh\u00b7ge\u00b7weih\u00b7te", "Kna\u00b7ben\u00b7zei\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dann wird die Dunkelheit, die mich umh\u00fcllt,", "tokens": ["Dann", "wird", "die", "Dun\u00b7kel\u00b7heit", ",", "die", "mich", "um\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Noch n\u00e4chtlicher, und heisse Thr\u00e4nen gleiten;", "tokens": ["Noch", "n\u00e4cht\u00b7li\u00b7cher", ",", "und", "heis\u00b7se", "Thr\u00e4\u00b7nen", "glei\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "$,", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Vergebens fleh' ich weinend vom Geschik", "tokens": ["Ver\u00b7ge\u00b7bens", "fleh'", "ich", "wei\u00b7nend", "vom", "Ge\u00b7schik"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Nur einen Tropfen eurer Lust zur\u00fck.", "tokens": ["Nur", "ei\u00b7nen", "Trop\u00b7fen", "eu\u00b7rer", "Lust", "zu\u00b7r\u00fck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.