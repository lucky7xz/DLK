{"textgrid.poem.38557": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An den Lebensnachen", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn schleierlos Aurora der Fluth entsteigt,", "tokens": ["Wenn", "schlei\u00b7er\u00b7los", "Au\u00b7ro\u00b7ra", "der", "Fluth", "ent\u00b7steigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "NE", "ART", "NN", "VVPP", "$,"], "meter": "-+--+---+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Im Bl\u00fcthenschmuck des Lenzes die Sch\u00f6pfung lacht,", "tokens": ["Im", "Bl\u00fc\u00b7then\u00b7schmuck", "des", "Len\u00b7zes", "die", "Sch\u00f6p\u00b7fung", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wenn k\u00fchle Morgenl\u00fcfte s\u00e4useln,", "tokens": ["Wenn", "k\u00fch\u00b7le", "Mor\u00b7gen\u00b7l\u00fcf\u00b7te", "s\u00e4u\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und mit den Locken der Haine spielen:", "tokens": ["Und", "mit", "den", "Lo\u00b7cken", "der", "Hai\u00b7ne", "spie\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Dann, Lebensnachen, gleite gehaltnern Laufs,", "tokens": ["Dann", ",", "Le\u00b7bens\u00b7na\u00b7chen", ",", "glei\u00b7te", "ge\u00b7halt\u00b7nern", "Laufs", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wie Schw\u00e4ne sanft auf spiegelnder Woge fort,", "tokens": ["Wie", "Schw\u00e4\u00b7ne", "sanft", "auf", "spie\u00b7geln\u00b7der", "Wo\u00b7ge", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da\u00df ich der Uferblumen viele,", "tokens": ["Da\u00df", "ich", "der", "Uf\u00b7er\u00b7blu\u00b7men", "vie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mir um die Schl\u00e4fe zu duften, breche!", "tokens": ["Mir", "um", "die", "Schl\u00e4\u00b7fe", "zu", "duf\u00b7ten", ",", "bre\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,", "ADJA", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Wenn aber Zeus im Donnergew\u00f6lke z\u00fcrnt,", "tokens": ["Wenn", "a\u00b7ber", "Zeus", "im", "Don\u00b7ner\u00b7ge\u00b7w\u00f6l\u00b7ke", "z\u00fcrnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NE", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Poseidon stolze Flotten wie d\u00fcrres Laub", "tokens": ["Po\u00b7sei\u00b7don", "stol\u00b7ze", "Flot\u00b7ten", "wie", "d\u00fcr\u00b7res", "Laub"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.rel