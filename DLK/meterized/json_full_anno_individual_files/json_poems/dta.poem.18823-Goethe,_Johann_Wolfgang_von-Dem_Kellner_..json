{"dta.poem.18823": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Dem Kellner .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Setze mir nicht, du Grobian,", "tokens": ["Set\u00b7ze", "mir", "nicht", ",", "du", "Gro\u00b7bi\u00b7an", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "PPER", "NE", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Mir den Krug so derb vor die Nase!", "tokens": ["Mir", "den", "Krug", "so", "derb", "vor", "die", "Na\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wer mir Wein bringt sehe mich freundlich an,", "tokens": ["Wer", "mir", "Wein", "bringt", "se\u00b7he", "mich", "freund\u00b7lich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "NN", "VVFIN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sonst tr\u00fcbt sich der Eilfer im Glase.", "tokens": ["Sonst", "tr\u00fcbt", "sich", "der", "Eil\u00b7fer", "im", "Gla\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}