{"dta.poem.19324": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "57.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du w\u00e4hnst, o Weiser, dich vom alten Wahn entkettet,", "tokens": ["Du", "w\u00e4hnst", ",", "o", "Wei\u00b7ser", ",", "dich", "vom", "al\u00b7ten", "Wahn", "ent\u00b7ket\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "FM", "NN", "$,", "PPER", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wirklich zur Wirklichkeit des Denkens hingerettet.", "tokens": ["Wirk\u00b7lich", "zur", "Wirk\u00b7lich\u00b7keit", "des", "Den\u00b7kens", "hin\u00b7ge\u00b7ret\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "Du sprichst, \u201eIch setze nichts voraus mehr gegenw\u00e4rtig,", "tokens": ["Du", "sprichst", ",", "\u201e", "Ich", "set\u00b7ze", "nichts", "vo\u00b7raus", "mehr", "ge\u00b7gen\u00b7w\u00e4r\u00b7tig", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "$(", "PPER", "VVFIN", "PIS", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201eeben so wenig nehm' ich etwas an als fertig.", "tokens": ["\u201e", "e\u00b7ben", "so", "we\u00b7nig", "nehm'", "ich", "et\u00b7was", "an", "als", "fer\u00b7tig", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "PIS", "VVFIN", "PPER", "ADV", "PTKVZ", "KOKOM", "ADJD", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.3": {"line.1": {"text": "\u201eich sehe zu, was ist unmittelbar gegeben,", "tokens": ["\u201e", "ich", "se\u00b7he", "zu", ",", "was", "ist", "un\u00b7mit\u00b7tel\u00b7bar", "ge\u00b7ge\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PTKVZ", "$,", "PWS", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201ewie es entwickelnd sich vermittelt.\u201c \u2014 Das ists eben!", "tokens": ["\u201e", "wie", "es", "ent\u00b7wi\u00b7ckelnd", "sich", "ver\u00b7mit\u00b7telt", ".", "\u201c", "Das", "ists", "e\u00b7ben", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PPER", "VVPP", "PRF", "VVPP", "$.", "$(", "$(", "PDS", "VAFIN", "ADV", "$."], "meter": "++-+---+--+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Wo ist unmittelbar gegebnes denn zu Haus?", "tokens": ["Wo", "ist", "un\u00b7mit\u00b7tel\u00b7bar", "ge\u00b7geb\u00b7nes", "denn", "zu", "Haus", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADJD", "ADJA", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was du vermitteln willst, das setzest du voraus.", "tokens": ["Was", "du", "ver\u00b7mit\u00b7teln", "willst", ",", "das", "set\u00b7zest", "du", "vo\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVINF", "VMFIN", "$,", "PDS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}