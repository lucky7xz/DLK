{"textgrid.poem.67936": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "2. An die Regeng\u00f6ttin", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6ne G\u00f6ttin, Himmelstochter,", "tokens": ["Sch\u00f6\u00b7ne", "G\u00f6t\u00b7tin", ",", "Him\u00b7mel\u00b7stoch\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit dem vollen Wasserkruge,", "tokens": ["Mit", "dem", "vol\u00b7len", "Was\u00b7ser\u00b7kru\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Den dein Bruder dann zerschmettert,", "tokens": ["Den", "dein", "Bru\u00b7der", "dann", "zer\u00b7schmet\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df es wettert Ungewitter,", "tokens": ["Da\u00df", "es", "wet\u00b7tert", "Un\u00b7ge\u00b7wit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Blitz und Donner! \u2013", "tokens": ["Blitz", "und", "Don\u00b7ner", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "KON", "NN", "$.", "$("], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Sch\u00f6ne G\u00f6ttin, K\u00f6nigstochter!", "tokens": ["Sch\u00f6\u00b7ne", "G\u00f6t\u00b7tin", ",", "K\u00f6\u00b7nig\u00b7stoch\u00b7ter", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und dann giebest du uns Regen,", "tokens": ["Und", "dann", "gie\u00b7best", "du", "uns", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Milden Regen. Doch du streuest", "tokens": ["Mil\u00b7den", "Re\u00b7gen", ".", "Doch", "du", "streu\u00b7est"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "KON", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oft auch Flocken, oft auch Schlossen:", "tokens": ["Oft", "auch", "Flo\u00b7cken", ",", "oft", "auch", "Schlos\u00b7sen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "$,", "ADV", "ADV", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn so hat dirs Er der Weltgeist,", "tokens": ["Denn", "so", "hat", "dirs", "Er", "der", "Welt\u00b7geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PIS", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Er der Weltgott, Virakocha,", "tokens": ["Er", "der", "Welt\u00b7gott", ",", "Vi\u00b7ra\u00b7ko\u00b7cha", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,", "NE", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Anvertrauet, anempfohlen.", "tokens": ["An\u00b7ver\u00b7trau\u00b7et", ",", "an\u00b7em\u00b7pfoh\u00b7len", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sch\u00