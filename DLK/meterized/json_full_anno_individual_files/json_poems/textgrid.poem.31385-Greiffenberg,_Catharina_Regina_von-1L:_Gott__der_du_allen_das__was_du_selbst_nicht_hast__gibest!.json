{"textgrid.poem.31385": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gott / der du allen das / was du selbst nicht hast / gibest!", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott / der du allen das / was du selbst nicht hast / gibest!", "tokens": ["Gott", "/", "der", "du", "al\u00b7len", "das", "/", "was", "du", "selbst", "nicht", "hast", "/", "gi\u00b7best", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$(", "PRELS", "PPER", "PIAT", "PDS", "$(", "PWS", "PPER", "ADV", "PTKNEG", "VAFIN", "$(", "VVFIN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Du bist des gantz befreyt / was du den andern bist.", "tokens": ["Du", "bist", "des", "gantz", "be\u00b7freyt", "/", "was", "du", "den", "an\u00b7dern", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADV", "VVPP", "$(", "PWS", "PPER", "ART", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "mein und der ganzen Welt Vranfang von dir ist /", "tokens": ["mein", "und", "der", "gan\u00b7zen", "Welt", "Vran\u00b7fang", "von", "dir", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "KON", "ART", "ADJA", "NN", "NN", "APPR", "PPER", "VAFIN", "$("], "meter": "---+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "weil die mittheilend Krafft du uns erschaffend' \u00fcbest.", "tokens": ["weil", "die", "mit\u00b7thei\u00b7lend", "Krafft", "du", "uns", "er\u00b7schaf\u00b7fend'", "\u00fc\u00b7best", "."], "token_info": ["w