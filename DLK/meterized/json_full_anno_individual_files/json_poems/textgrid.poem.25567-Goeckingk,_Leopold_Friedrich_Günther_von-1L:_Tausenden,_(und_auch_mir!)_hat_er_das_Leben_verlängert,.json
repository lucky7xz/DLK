{"textgrid.poem.25567": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Tausenden, (und auch mir!) hat er das Leben verl\u00e4ngert,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tausenden, (und auch mir!) hat er das Leben verl\u00e4ngert,", "tokens": ["Tau\u00b7sen\u00b7den", ",", "(", "und", "auch", "mir", "!", ")", "hat", "er", "das", "Le\u00b7ben", "ver\u00b7l\u00e4n\u00b7gert", ","], "token_info": ["word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "$(", "KON", "ADV", "PPER", "$.", "$(", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+---+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Nur das seinige hat leider! sein Eifer verk\u00fcrzt.", "tokens": ["Nur", "das", "sei\u00b7ni\u00b7ge", "hat", "lei\u00b7der", "!", "sein", "Ei\u00b7fer", "ver\u00b7k\u00fcrzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "PPOSS", "VAFIN", "ADV", "$.", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Und doch h\u00e4tt' er so gern sich l\u00e4nger des Lebens gefreuet;", "tokens": ["Und", "doch", "h\u00e4tt'", "er", "so", "gern", "sich", "l\u00e4n\u00b7ger", "des", "Le\u00b7bens", "ge\u00b7freu\u00b7et", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "ADV", "PRF", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Aber federleicht wog es ihm gegen die Pflicht.", "tokens": ["A\u00b7ber", "fe\u00b7der\u00b7leicht", "wog", "es", "ihm", "ge\u00b7gen", "die", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-++--+--+", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Lauschend mit sp\u00e4hendem Blick', erforscht' an der dunkelen Werkstatt", "tokens": ["Lau\u00b7schend", "mit", "sp\u00e4\u00b7hen\u00b7dem", "Blick'", ",", "er\u00b7forscht'", "an", "der", "dun\u00b7ke\u00b7len", "Werk\u00b7statt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$,", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Der Natur, sein Geist ihre verheimlichte Kraft.", "tokens": ["Der", "Na\u00b7tur", ",", "sein", "Geist", "ih\u00b7re", "ver\u00b7heim\u00b7lich\u00b7te", "Kraft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "--+-+---+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Hofft' er, irgend den Kreis des menschlichen Wissens und Wohlseyns", "tokens": ["Hofft'", "er", ",", "ir\u00b7gend", "den", "Kreis", "des", "menschli\u00b7chen", "Wis\u00b7sens", "und", "Wohl\u00b7seyns"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "ADV", "ART", "NN", "ART", "ADJA", "NN", "KON", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Noch erweitert zu sehn, um eine Linie nur:", "tokens": ["Noch", "er\u00b7wei\u00b7tert", "zu", "sehn", ",", "um", "ei\u00b7ne", "Li\u00b7nie", "nur", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "PTKZU", "VVINF", "$,", "KOUI", "ART", "NN", "ADV", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Wie erheiterte sich sein Auge! Wie freut' er der Nachwelt", "tokens": ["Wie", "er\u00b7hei\u00b7ter\u00b7te", "sich", "sein", "Au\u00b7ge", "!", "Wie", "freut'", "er", "der", "Nach\u00b7welt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PRF", "PPOSAT", "NN", "$.", "PWAV", "VVFIN", "PPER", "ART", "NN"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Gl\u00fcckes, sich im voraus, gleich als gen\u00f6\u00df' er es selbst.", "tokens": ["Gl\u00fc\u00b7ckes", ",", "sich", "im", "vo\u00b7raus", ",", "gleich", "als", "gen\u00f6\u00df'", "er", "es", "selbst", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRF", "APPRART", "PTKVZ", "$,", "ADV", "KOKOM", "VVFIN", "PPER", "PPER", "ADV", "$."], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Eine Gattin war sein, mit immer noch bl\u00fchenden Reitzen,", "tokens": ["Ei\u00b7ne", "Gat\u00b7tin", "war", "sein", ",", "mit", "im\u00b7mer", "noch", "bl\u00fc\u00b7hen\u00b7den", "Reit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VAINF", "$,", "APPR", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Hatte der Lenze sie gleich zwanzig schon mit ihm verlebt.", "tokens": ["Hat\u00b7te", "der", "Len\u00b7ze", "sie", "gleich", "zwan\u00b7zig", "schon", "mit", "ihm", "ver\u00b7lebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "ADV", "CARD", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.3": {"text": "Doch es konnt' ihr Reitz im ersten Fr\u00fchlinge schwinden,", "tokens": ["Doch", "es", "konnt'", "ihr", "Reitz", "im", "ers\u00b7ten", "Fr\u00fch\u00b7lin\u00b7ge", "schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Klein war dieser Verlust, blieb ihr der sch\u00f6nere Geist,", "tokens": ["Klein", "war", "die\u00b7ser", "Ver\u00b7lust", ",", "blieb", "ihr", "der", "sch\u00f6\u00b7ne\u00b7re", "Geist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PDAT", "NN", "$,", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Blieb der zarte Scherz nur immer in ihrem Gefolge,", "tokens": ["Blieb", "der", "zar\u00b7te", "Scherz", "nur", "im\u00b7mer", "in", "ih\u00b7rem", "Ge\u00b7fol\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Und lebendig der Wunsch, heiter den Gatten zu sehn,", "tokens": ["Und", "le\u00b7ben\u00b7dig", "der", "Wunsch", ",", "hei\u00b7ter", "den", "Gat\u00b7ten", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "$,", "ADJD", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Und der bescheidene Sinn, der alle Tugenden hebet,", "tokens": ["Und", "der", "be\u00b7schei\u00b7de\u00b7ne", "Sinn", ",", "der", "al\u00b7le", "Tu\u00b7gen\u00b7den", "he\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Wie der Puder den Grund einer Aurikel versch\u00f6nt.", "tokens": ["Wie", "der", "Pu\u00b7der", "den", "Grund", "ei\u00b7ner", "Au\u00b7ri\u00b7kel", "ver\u00b7sch\u00f6nt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}}, "stanza.4": {"line.1": {"text": "Welch ein liebender Kreis von weisen Freunden umgab ihn!", "tokens": ["Welch", "ein", "lie\u00b7ben\u00b7der", "Kreis", "von", "wei\u00b7sen", "Freun\u00b7den", "um\u00b7gab", "ihn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Jeder sch\u00e4tzte den Arzt, Denker und Sp\u00f6tter in ihm;", "tokens": ["Je\u00b7der", "sch\u00e4tz\u00b7te", "den", "Arzt", ",", "Den\u00b7ker", "und", "Sp\u00f6t\u00b7ter", "in", "ihm", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "$,", "NN", "KON", "NN", "APPR", "PPER", "$."], "meter": "+-+---+--+-++", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Aber alle noch mehr den Mann, de\u00df Leben ein Einklang", "tokens": ["A\u00b7ber", "al\u00b7le", "noch", "mehr", "den", "Mann", ",", "de\u00df", "Le\u00b7ben", "ein", "Ein\u00b7klang"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PIS", "ADV", "ADV", "ART", "NN", "$,", "ART", "NN", "ART", "NN"], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "S\u00fc\u00dferer T\u00f6ne war, als sie die Stoa noch gab.", "tokens": ["S\u00fc\u00b7\u00dfe\u00b7rer", "T\u00f6\u00b7ne", "war", ",", "als", "sie", "die", "Stoa", "noch", "gab", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "$,", "KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Gleich den Weisen Athens liebt' er die fr\u00f6hlichen Zirkel;", "tokens": ["Gleich", "den", "Wei\u00b7sen", "A\u00b7thens", "liebt'", "er", "die", "fr\u00f6h\u00b7li\u00b7chen", "Zir\u00b7kel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NE", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Seine Sorgen allein blieben im Herzen versteckt;", "tokens": ["Sei\u00b7ne", "Sor\u00b7gen", "al\u00b7lein", "blie\u00b7ben", "im", "Her\u00b7zen", "ver\u00b7steckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Alles opfert' er sonst auf dem Altare der Freundschaft,", "tokens": ["Al\u00b7les", "op\u00b7fert'", "er", "sonst", "auf", "dem", "Al\u00b7ta\u00b7re", "der", "Freund\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Seinen Witz und Wein, seine Erfahrungen gern.", "tokens": ["Sei\u00b7nen", "Witz", "und", "Wein", ",", "sei\u00b7ne", "Er\u00b7fah\u00b7run\u00b7gen", "gern", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$,", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "Von den Pfeilen, geschnellt von fremden Bogen, ging keiner", "tokens": ["Von", "den", "Pfei\u00b7len", ",", "ge\u00b7schnellt", "von", "frem\u00b7den", "Bo\u00b7gen", ",", "ging", "kei\u00b7ner"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "VVPP", "APPR", "ADJA", "NN", "$,", "VVFIN", "PIS"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Je verloren f\u00fcr ihn; wie er behende sie fing!", "tokens": ["Je", "ver\u00b7lo\u00b7ren", "f\u00fcr", "ihn", ";", "wie", "er", "be\u00b7hen\u00b7de", "sie", "fing", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPR", "PPER", "$.", "PWAV", "PPER", "VVFIN", "PPER", "VVFIN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Und wie schickt' er sie oft, bei l\u00e4chelndem Munde, mit Rosen", "tokens": ["Und", "wie", "schickt'", "er", "sie", "oft", ",", "bei", "l\u00e4\u00b7cheln\u00b7dem", "Mun\u00b7de", ",", "mit", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PWAV", "VVFIN", "PPER", "PPER", "ADV", "$,", "APPR", "ADJA", "NN", "$,", "APPR", "NN"], "meter": "-+---+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Ihre Spitzen besteckt, hurtig dem Sch\u00fctzen zur\u00fcck!", "tokens": ["Ih\u00b7re", "Spit\u00b7zen", "be\u00b7steckt", ",", "hur\u00b7tig", "dem", "Sch\u00fct\u00b7zen", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Und so glich sein Lebensgenu\u00df den schl\u00e4ngelnden G\u00e4ngen", "tokens": ["Und", "so", "glich", "sein", "Le\u00b7bens\u00b7ge\u00b7nu\u00df", "den", "schl\u00e4n\u00b7geln\u00b7den", "G\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Eines englischen Parks, ja! noch verdoppelt sogar!", "tokens": ["Ei\u00b7nes", "eng\u00b7li\u00b7schen", "Parks", ",", "ja", "!", "noch", "ver\u00b7dop\u00b7pelt", "so\u00b7gar", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "$.", "ADV", "VVFIN", "ADV", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Denn die Armuth hatt' am kalten eisernen Arme", "tokens": ["Denn", "die", "Ar\u00b7muth", "hatt'", "am", "kal\u00b7ten", "ei\u00b7ser\u00b7nen", "Ar\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "APPRART", "ADJA", "ADJA", "NN"], "meter": "--++--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ihn in fr\u00fcherer Zeit rauhere Pfade gef\u00fchrt.", "tokens": ["Ihn", "in", "fr\u00fc\u00b7he\u00b7rer", "Zeit", "rau\u00b7he\u00b7re", "Pfa\u00b7de", "ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Ach! drum h\u00e4tt' er so gern sich l\u00e4nger des Lebens gefreuet,", "tokens": ["Ach", "!", "drum", "h\u00e4tt'", "er", "so", "gern", "sich", "l\u00e4n\u00b7ger", "des", "Le\u00b7bens", "ge\u00b7freu\u00b7et", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PAV", "VAFIN", "PPER", "ADV", "ADV", "PRF", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Aber federleicht wog es ihm gegen die Pflicht,", "tokens": ["A\u00b7ber", "fe\u00b7der\u00b7leicht", "wog", "es", "ihm", "ge\u00b7gen", "die", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Und so verlie\u00df er uns fr\u00fch! Ihn tadlen m\u00f6chte die Freundschaft,", "tokens": ["Und", "so", "ver\u00b7lie\u00df", "er", "uns", "fr\u00fch", "!", "Ihn", "tad\u00b7len", "m\u00f6ch\u00b7te", "die", "Freund\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "ADJD", "$.", "PPER", "VVINF", "VMFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Nur die Bewunderung h\u00e4lt jeglichen Tadel zur\u00fcck.", "tokens": ["Nur", "die", "Be\u00b7wun\u00b7de\u00b7rung", "h\u00e4lt", "jeg\u00b7li\u00b7chen", "Ta\u00b7del", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Tausenden, (und auch mir!) hat er das Leben verl\u00e4ngert,", "tokens": ["Tau\u00b7sen\u00b7den", ",", "(", "und", "auch", "mir", "!", ")", "hat", "er", "das", "Le\u00b7ben", "ver\u00b7l\u00e4n\u00b7gert", ","], 