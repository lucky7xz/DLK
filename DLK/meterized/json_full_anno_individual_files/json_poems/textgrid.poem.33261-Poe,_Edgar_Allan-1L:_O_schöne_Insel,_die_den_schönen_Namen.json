{"textgrid.poem.33261": {"metadata": {"author": {"name": "Poe, Edgar Allan", "birth": "N.A.", "death": "N.A."}, "title": "1L: O sch\u00f6ne Insel, die den sch\u00f6nen Namen", "genre": "verse", "period": "N.A.", "pub_year": 1829, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O sch\u00f6ne Insel, die den sch\u00f6nen Namen", "tokens": ["O", "sch\u00f6\u00b7ne", "In\u00b7sel", ",", "die", "den", "sch\u00f6\u00b7nen", "Na\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich von der s\u00fc\u00dfesten der Blumen nimmt,", "tokens": ["Sich", "von", "der", "s\u00fc\u00b7\u00dfes\u00b7ten", "der", "Blu\u00b7men", "nimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ach, da\u00df bei deinem Schaun mich \u00fcberkamen", "tokens": ["Ach", ",", "da\u00df", "bei", "dei\u00b7nem", "Schaun", "mich", "\u00fc\u00b7ber\u00b7ka\u00b7men"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KOUS", "APPR", "PPOSAT", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "All jene Stunden, die einst froh gestimmt!", "tokens": ["All", "je\u00b7ne", "Stun\u00b7den", ",", "die", "einst", "froh", "ge\u00b7stimmt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PDAT", "NN", "$,", "PRELS", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie viele Szenen lang versunkner Wonne!", "tokens": ["Wie", "vie\u00b7le", "Sze\u00b7nen", "lang", "ver\u00b7su