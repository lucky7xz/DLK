{"textgrid.poem.60303": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Zweifel und Ruhe", "genre": "verse", "period": "N.A.", "pub_year": 1841, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Mensch auf halbem Weg entschlief", "tokens": ["Der", "Mensch", "auf", "hal\u00b7bem", "Weg", "ent\u00b7schlief"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Schatten eines alten Baumes,", "tokens": ["Im", "Schat\u00b7ten", "ei\u00b7nes", "al\u00b7ten", "Bau\u00b7mes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In Banden eines s\u00fc\u00dfen Traumes,", "tokens": ["In", "Ban\u00b7den", "ei\u00b7nes", "s\u00fc\u00b7\u00dfen", "Trau\u00b7mes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schlief manche Wanderstunde, tief.", "tokens": ["Schlief", "man\u00b7che", "Wan\u00b7der\u00b7stun\u00b7de", ",", "tief", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$,", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Das Laub des Baumes rauschte mild", "tokens": ["Das", "Laub", "des", "Bau\u00b7mes", "rauschte", "mild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "NE"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Und bat den Schlaf: o bleibe lang!", "tokens": ["Und", "bat", "den", "Schlaf", ":", "o", "blei\u00b7be", "lang", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$.", "FM", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zum Traume sprach der V\u00f6gel Sang:", "tokens": ["Zum", "Trau\u00b7me", "sprach", "der", "V\u00f6\u00b7gel", "Sang", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "O male fort dein buntes Bild;", "tokens": ["O", "ma\u00b7le", "fort", "dein", "bun\u00b7tes", "Bild", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PTKVZ", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Da\u00df uns der Schl\u00e4fer nicht erwache,", "tokens": ["Da\u00df", "uns", "der", "Schl\u00e4\u00b7fer", "nicht", "er\u00b7wa\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Er weile unter diesem Dache!", "tokens": ["Er", "wei\u00b7le", "un\u00b7ter", "die\u00b7sem", "Da\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da kam der Zweifel, ihn zu wecken;", "tokens": ["Da", "kam", "der", "Zwei\u00b7fel", ",", "ihn", "zu", "we\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er klopft ihm auf die Schulter sacht", "tokens": ["Er", "klopft", "ihm", "auf", "die", "Schul\u00b7ter", "sacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und spricht: steh auf, bevor es Nacht,", "tokens": ["Und", "spricht", ":", "steh", "auf", ",", "be\u00b7vor", "es", "Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "VVFIN", "PTKVZ", "$,", "KOUS", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Ziele sind noch weite Strecken.", "tokens": ["Zum", "Zie\u00b7le", "sind", "noch", "wei\u00b7te", "Stre\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich bin dein Freund, ein rauher zwar,", "tokens": ["Ich", "bin", "dein", "Freund", ",", "ein", "rau\u00b7her", "zwar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "ART", "ADJA", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch treu, und warne vor Gefahr.", "tokens": ["Doch", "treu", ",", "und", "war\u00b7ne", "vor", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er f\u00fchrt ihn fort durch stille Heiden,", "tokens": ["Er", "f\u00fchrt", "ihn", "fort", "durch", "stil\u00b7le", "Hei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo Lust und Zier des Lebens scheiden,", "tokens": ["Wo", "Lust", "und", "Zier", "des", "Le\u00b7bens", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Natur bl\u00fcht abseit seinem Herzen,", "tokens": ["Na\u00b7tur", "bl\u00fcht", "ab\u00b7seit", "sei\u00b7nem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihn fassen unvers\u00f6hnte Schmerzen.", "tokens": ["Ihn", "fas\u00b7sen", "un\u00b7ver\u00b7s\u00f6hn\u00b7te", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie sonst vom stillen Heideland", "tokens": ["Wie", "sonst", "vom", "stil\u00b7len", "Hei\u00b7de\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Wandrer V\u00f6gel scheucht empor,", "tokens": ["Der", "Wand\u00b7rer", "V\u00f6\u00b7gel", "scheucht", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So rauscht ihm an des Zweifels Hand", "tokens": ["So", "rauscht", "ihm", "an", "des", "Zwei\u00b7fels", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Von Fragen auf ein wilder Chor,", "tokens": ["Von", "Fra\u00b7gen", "auf", "ein", "wil\u00b7der", "Chor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die schreiend fort zur Ferne dringen,", "tokens": ["Die", "schrei\u00b7end", "fort", "zur", "Fer\u00b7ne", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PTKVZ", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Doch Antwort nicht zur\u00fcck ihm bringen.", "tokens": ["Doch", "Ant\u00b7wort", "nicht", "zu\u00b7r\u00fcck", "ihm", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKNEG", "PTKVZ", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Dann wird es \u00f6der, stiller immer,", "tokens": ["Dann", "wird", "es", "\u00f6\u00b7der", ",", "stil\u00b7ler", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "ADJA", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "D\u00e4mmrung versagt den letzten Schimmer;", "tokens": ["D\u00e4mm\u00b7rung", "ver\u00b7sagt", "den", "letz\u00b7ten", "Schim\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.13": {"text": "Der Wandrer schreitet tr\u00fcb und sacht", "tokens": ["Der", "Wand\u00b7rer", "schrei\u00b7tet", "tr\u00fcb", "und", "sacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Mit seinem F\u00fchrer durch die Nacht.", "tokens": ["Mit", "sei\u00b7nem", "F\u00fch\u00b7rer", "durch", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch wenn ihm auf dem Gang nicht graut,", "tokens": ["Doch", "wenn", "ihm", "auf", "dem", "Gang", "nicht", "graut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wenn er kr\u00e4ftig horcht und schaut", "tokens": ["Und", "wenn", "er", "kr\u00e4f\u00b7tig", "horcht", "und", "schaut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In seines Herzens tiefsten Grund,", "tokens": ["In", "sei\u00b7nes", "Her\u00b7zens", "tiefs\u00b7ten", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So wird ihm hier der Himmel kund.", "tokens": ["So", "wird", "ihm", "hier", "der", "Him\u00b7mel", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da unten str\u00f6mt der ewge Quell,", "tokens": ["Da", "un\u00b7ten", "str\u00f6mt", "der", "ew\u00b7ge", "Quell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da klingt es hold, da strahlt es hell,", "tokens": ["Da", "klingt", "es", "hold", ",", "da", "strahlt", "es", "hell", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "ADV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er schaut den Brunnen und das Meer", "tokens": ["Er", "schaut", "den", "Brun\u00b7nen", "und", "das", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und fragt nicht mehr: wohin? woher?", "tokens": ["Und", "fragt", "nicht", "mehr", ":", "wo\u00b7hin", "?", "wo\u00b7her", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "$.", "PWAV", "$.", "PWAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der Mensch auf halbem Weg entschlief", "tokens": ["Der", "Mensch", "auf", "hal\u00b7bem", "Weg", "ent\u00b7schlief"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Schatten eines alten Baumes,", "tokens": ["Im", "Schat\u00b7ten", "ei\u00b7nes", "al\u00b7ten", "Bau\u00b7mes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In Banden eines s\u00fc\u00dfen Traumes,", "tokens": ["In", "Ban\u00b7den", "ei\u00b7nes", "s\u00fc\u00b7\u00dfen", "Trau\u00b7mes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schlief manche Wanderstunde, tief.", "tokens": ["Schlief", "man\u00b7che", "Wan\u00b7der\u00b7stun\u00b7de", ",