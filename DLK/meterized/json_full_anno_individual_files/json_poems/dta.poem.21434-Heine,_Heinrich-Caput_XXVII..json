{"dta.poem.21434": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XXVII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dem wirklichen Aristophanes,", "tokens": ["Dem", "wirk\u00b7li\u00b7chen", "A\u00b7ris\u00b7to\u00b7pha\u00b7nes", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem ginge es schlecht, dem Armen;", "tokens": ["Dem", "gin\u00b7ge", "es", "schlecht", ",", "dem", "Ar\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "$,", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wir w\u00fcrden ihn bald begleitet sehn", "tokens": ["Wir", "w\u00fcr\u00b7den", "ihn", "bald", "be\u00b7glei\u00b7tet", "sehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "VVINF"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit Ch\u00f6ren von Gensd\u2019armen.", "tokens": ["Mit", "Ch\u00f6\u00b7ren", "von", "Gens\u00b7d'\u00b7ar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Der P\u00f6bel bek\u00e4m\u2019 die Erlaubni\u00df bald", "tokens": ["Der", "P\u00f6\u00b7bel", "be\u00b7k\u00e4m'", "die", "Er\u00b7laub\u00b7ni\u00df", "bald"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADV"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Zu schimpfen statt zu wedeln;", "tokens": ["Zu", "schimp\u00b7fen", "statt", "zu", "we\u00b7deln", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Polizei erhielte Befehl", "tokens": ["Die", "Po\u00b7li\u00b7zei", "er\u00b7hiel\u00b7te", "Be\u00b7fehl"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Zu fahnden auf den Edeln.", "tokens": ["Zu", "fahn\u00b7den", "auf", "den", "E\u00b7deln", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O K\u00f6nig! Ich meine es gut mit dir,", "tokens": ["O", "K\u00f6\u00b7nig", "!", "Ich", "mei\u00b7ne", "es", "gut", "mit", "dir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PPER", "VVFIN", "PPER", "ADJD", "APPR", "PPER", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und will einen Rath dir geben:", "tokens": ["Und", "will", "ei\u00b7nen", "Rath", "dir", "ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die todten Dichter, verehre sie nur,", "tokens": ["Die", "tod\u00b7ten", "Dich\u00b7ter", ",", "ver\u00b7eh\u00b7re", "sie", "nur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Doch schone die da leben.", "tokens": ["Doch", "scho\u00b7ne", "die", "da", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Beleid\u2019ge lebendige Dichter nicht,", "tokens": ["Be\u00b7lei\u00b7d'\u00b7ge", "le\u00b7ben\u00b7di\u00b7ge", "Dich\u00b7ter", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "PTKNEG", "$,"], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie haben Flammen und Waffen,", "tokens": ["Sie", "ha\u00b7ben", "Flam\u00b7men", "und", "Waf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die furchtbarer sind als Jovis Blitz,", "tokens": ["Die", "furcht\u00b7ba\u00b7rer", "sind", "als", "Jo\u00b7vis", "Blitz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "KOKOM", "NE", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Den ja der Poet erschaffen.", "tokens": ["Den", "ja", "der", "Po\u00b7et", "er\u00b7schaf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}}}}}