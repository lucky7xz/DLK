{"textgrid.poem.25092": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Noch, die Zweige \u00fcberdeckend,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch, die Zweige \u00fcberdeckend,", "tokens": ["Noch", ",", "die", "Zwei\u00b7ge", "\u00fc\u00b7berd\u00b7e\u00b7ckend", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Herbstlaub, das nicht weichen will!", "tokens": ["Herbst\u00b7laub", ",", "das", "nicht", "wei\u00b7chen", "will", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und schon neue Knospen weckend", "tokens": ["Und", "schon", "neu\u00b7e", "Knos\u00b7pen", "we\u00b7ckend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJA", "NN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Naht der fr\u00f6hliche April.", "tokens": ["Naht", "der", "fr\u00f6h\u00b7li\u00b7che", "Ap\u00b7ril", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Seine Wipfel ihm entgegen", "tokens": ["Sei\u00b7ne", "Wip\u00b7fel", "ihm", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PPER", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Freudeschauernd wirft der Wald;", "tokens": ["Freu\u00b7de\u00b7schau\u00b7ernd", "wirft", "der", "Wald", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos"