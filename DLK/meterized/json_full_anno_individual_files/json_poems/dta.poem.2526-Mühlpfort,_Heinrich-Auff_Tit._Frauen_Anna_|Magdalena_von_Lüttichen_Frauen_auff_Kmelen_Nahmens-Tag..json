{"dta.poem.2526": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Auff Tit. Frauen Anna |Magdalena von  \n L\u00fcttichen/  \n Frauen auff Kmelen/ Nahmens-Tag.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich wolte wohl was hohes schreiben/", "tokens": ["Ich", "wol\u00b7te", "wohl", "was", "ho\u00b7hes", "schrei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PWS", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu ehren diesem edlen Tag/", "tokens": ["Zu", "eh\u00b7ren", "die\u00b7sem", "ed\u00b7len", "Tag", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und meine Sinnen sch\u00e4rffer treiben/", "tokens": ["Und", "mei\u00b7ne", "Sin\u00b7nen", "sch\u00e4rf\u00b7fer", "trei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als wie ich sonst zu tichten pflag.", "tokens": ["Als", "wie", "ich", "sonst", "zu", "tich\u00b7ten", "pflag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch wil mir fast die Krafft gebrechen/", "tokens": ["Doch", "wil", "mir", "fast", "die", "Krafft", "ge\u00b7bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und Ph\u00f6bus Huld und Gunst absprechen.", "tokens": ["Und", "Ph\u00f6\u00b7bus", "Huld", "und", "Gunst", "ab\u00b7spre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Hoch-Edle Frau/ von Trefflichkeiten", "tokens": ["Hoch\u00b7Ed\u00b7le", "Frau", "/", "von", "Treff\u00b7lich\u00b7kei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$(", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der h\u00f6chsten Tugend au\u00dfgeziert/", "tokens": ["Der", "h\u00f6chs\u00b7ten", "Tu\u00b7gend", "au\u00df\u00b7ge\u00b7ziert", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wie mag ich Opffer zubereiten/", "tokens": ["Wie", "mag", "ich", "Opf\u00b7fer", "zu\u00b7be\u00b7rei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Das meiner Schuldigkeit geb\u00fchrt?", "tokens": ["Das", "mei\u00b7ner", "Schul\u00b7dig\u00b7keit", "ge\u00b7b\u00fchrt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ich wei\u00df wohl/ da\u00df man Heroinen", "tokens": ["Ich", "wei\u00df", "wohl", "/", "da\u00df", "man", "He\u00b7roi\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$(", "KOUS", "PIS", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Nichts schlechtes gab/ sie zu bedienen.", "tokens": ["Nichts", "schlech\u00b7tes", "gab", "/", "sie", "zu", "be\u00b7die\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "ADJA", "VVFIN", "$(", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Zu dem so m\u00fcssen kluge Geister", "tokens": ["Zu", "dem", "so", "m\u00fcs\u00b7sen", "klu\u00b7ge", "Geis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "VMFIN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Die k\u00f6stlichsten Lobsprecher seyn.", "tokens": ["Die", "k\u00f6st\u00b7lichs\u00b7ten", "Lob\u00b7spre\u00b7cher", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Apollo selbst der K\u00fcnste Meister", "tokens": ["A\u00b7pol\u00b7lo", "selbst", "der", "K\u00fcns\u00b7te", "Meis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Schleust die Vernunfft in Schrancken ein/", "tokens": ["Schleust", "die", "Ver\u00b7nunfft", "in", "Schran\u00b7cken", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NN", "ART", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.17": {"text": "Und saget/ da\u00df dergleichen Gaben", "tokens": ["Und", "sa\u00b7get", "/", "da\u00df", "derg\u00b7lei\u00b7chen", "Ga\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$(", "KOUS", "PIS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Auch nicht die M\u00e4nner-Sinnen haben.", "tokens": ["Auch", "nicht", "die", "M\u00e4n\u00b7ner\u00b7Sin\u00b7nen", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Gewi\u00df/ es wird der Neid gestehen/", "tokens": ["Ge\u00b7wi\u00df", "/", "es", "wird", "der", "Neid", "ge\u00b7ste\u00b7hen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "PPER", "VAFIN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Da\u00df ihr Verstand dem Himmel gleicht/", "tokens": ["Da\u00df", "ihr", "Ver\u00b7stand", "dem", "Him\u00b7mel", "gleicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Da\u00df ihre Tugend in den H\u00f6hen", "tokens": ["Da\u00df", "ih\u00b7re", "Tu\u00b7gend", "in", "den", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Viel heller als die Sternen leucht.", "tokens": ["Viel", "hel\u00b7ler", "als", "die", "Ster\u00b7nen", "leucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Der Rede Liebligkeit und Zierde", "tokens": ["Der", "Re\u00b7de", "Lieb\u00b7lig\u00b7keit", "und", "Zier\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Macht Stock und Steinen auch Begierde.", "tokens": ["Macht", "Stock", "und", "Stei\u00b7nen", "auch", "Be\u00b7gier\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Glantz aller sch\u00f6nen H\u00f6fligkeiten/", "tokens": ["Glantz", "al\u00b7ler", "sch\u00f6\u00b7nen", "H\u00f6f\u00b7lig\u00b7kei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "So je die Gratien erdacht/", "tokens": ["So", "je", "die", "Gra\u00b7ti\u00b7en", "er\u00b7dacht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Wie stlmm ich doch nur meine Seiten/", "tokens": ["Wie", "stlmm", "ich", "doch", "nur", "mei\u00b7ne", "Sei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Da\u00df sie die au\u00dferle\u00dfne Pracht/", "tokens": ["Da\u00df", "sie", "die", "au\u00b7\u00dfer\u00b7le\u00df\u00b7ne", "Pracht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Nach ihrer W\u00fcrde recht besingeu/", "tokens": ["Nach", "ih\u00b7rer", "W\u00fcr\u00b7de", "recht", "be\u00b7sin\u00b7geu", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Und angenehme Lieder bringen?", "tokens": ["Und", "an\u00b7ge\u00b7neh\u00b7me", "Lie\u00b7der", "brin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Begl\u00fcckte Frau/ nicht nur vom Stande/", "tokens": ["Be\u00b7gl\u00fcck\u00b7te", "Frau", "/", "nicht", "nur", "vom", "Stan\u00b7de", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "PTKNEG", "ADV", "APPRART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Der die ber\u00fchmsten Ahnen wei\u00df\u2019t.", "tokens": ["Der", "die", "be\u00b7r\u00fchms\u00b7ten", "Ah\u00b7nen", "wei\u00df'", "t."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Zier in dem gantzen Mei\u00dfner-Lande/", "tokens": ["Zier", "in", "dem", "gant\u00b7zen", "Mei\u00df\u00b7ner\u00b7Lan\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.34": {"text": "So weit die Elbe di\u00df durchfleust;", "tokens": ["So", "weit", "die", "El\u00b7be", "di\u00df", "durch\u00b7fleust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NE", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Hoch und Wohledel von Gebl\u00fcte/", "tokens": ["Hoch", "und", "Woh\u00b7le\u00b7del", "von", "Ge\u00b7bl\u00fc\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "NN", "APPR", "NN", "$("], "meter": "+-+-++-+-", "measure": "unknown.measure.penta"}, "line.36": {"text": "Doch noch viel edler von Gem\u00fchte.", "tokens": ["Doch", "noch", "viel", "ed\u00b7ler", "von", "Ge\u00b7m\u00fch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "ADJA", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Was sonst den Weibern karg gegeben/", "tokens": ["Was", "sonst", "den", "Wei\u00b7bern", "karg", "ge\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Da\u00df Sch\u00f6nheit mit Verstand sich paart/", "tokens": ["Da\u00df", "Sch\u00f6n\u00b7heit", "mit", "Ver\u00b7stand", "sich", "paart", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "NN", "PRF", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Das siht man hier vollkommen/ eben", "tokens": ["Das", "siht", "man", "hier", "voll\u00b7kom\u00b7men", "/", "e\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PDS", "VVFIN", "PIS", "ADV", "ADJD", "$(", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Auch in der sch\u00f6nsten T\u00f6chter Art;", "tokens": ["Auch", "in", "der", "sch\u00f6ns\u00b7ten", "T\u00f6ch\u00b7ter", "Art", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Bey denen ", "tokens": ["Bey", "de\u00b7nen"], "token_info": ["word", "word"], "pos": ["APPR", "PRELS"], "meter": "+--", "measure": "dactylic.init"}, "line.42": {"text": "Stets um die beste Tugend streiten.", "tokens": ["Stets", "um", "die", "bes\u00b7te", "Tu\u00b7gend", "strei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Es satzte Rom viel Sieges-Zeichen/", "tokens": ["Es", "satz\u00b7te", "Rom", "viel", "Sie\u00b7ges\u00b7Zei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "PIAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "Den hochverdienten F\u00fcrsten auff.", "tokens": ["Den", "hoch\u00b7ver\u00b7dien\u00b7ten", "F\u00fcrs\u00b7ten", "auff", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "K\u00f6nt\u2019 ich die Krafft und Kunst erreichen/", "tokens": ["K\u00f6nt'", "ich", "die", "Krafft", "und", "Kunst", "er\u00b7rei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.46": {"text": "So wolt ich f\u00fcr der Zeiten Lauff/", "tokens": ["So", "wolt", "ich", "f\u00fcr", "der", "Zei\u00b7ten", "Lauff", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Ein solches Angedencken stifften/", "tokens": ["Ein", "sol\u00b7ches", "An\u00b7ge\u00b7den\u00b7cken", "stiff\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.48": {"text": "Ins k\u00fcnfftige durch meine Schrifften.", "tokens": ["Ins", "k\u00fcnff\u00b7ti\u00b7ge", "durch", "mei\u00b7ne", "Schriff\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.49": {"text": "Denn Marmors\u00e4ulen auffzuf\u00fchren/", "tokens": ["Denn", "Mar\u00b7mor\u00b7s\u00e4u\u00b7len", "auff\u00b7zu\