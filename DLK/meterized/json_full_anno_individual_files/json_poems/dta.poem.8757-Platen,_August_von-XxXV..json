{"dta.poem.8757": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XxXV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.85", "fr:0.14"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Der sommerlichen Sprudelquelle,", "tokens": ["Der", "som\u00b7mer\u00b7li\u00b7chen", "Spru\u00b7del\u00b7quel\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und siehst entstehn und siehst vergehn den Schaum?", "tokens": ["Und", "siehst", "ent\u00b7stehn", "und", "siehst", "ver\u00b7gehn", "den", "Schaum", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVINF", "KON", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So ruhn wir Menschen auf des Lebens Schwelle,", "tokens": ["So", "ruhn", "wir", "Men\u00b7schen", "auf", "des", "Le\u00b7bens", "Schwel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und was wir hoffen, was wir suchen stets,", "tokens": ["Und", "was", "wir", "hof\u00b7fen", ",", "was", "wir", "su\u00b7chen", "stets", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein leichter Hauch gebiert's, ein leichter Hauch verweht's.", "tokens": ["Ein", "leich\u00b7ter", "Hauch", "ge\u00b7biert's", ",", "ein", "leich\u00b7ter", "Hauch", "ver\u00b7weht'", "s."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "abbreviation"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Es \u00fcbt sich mehr und mehr das Herz,", "tokens": ["Es", "\u00fcbt", "sich", "mehr", "und", "mehr", "das", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "KON", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und st\u00e4hlt sich, da\u00df von Tag zu Tage", "tokens": ["Und", "st\u00e4hlt", "sich", ",", "da\u00df", "von", "Tag", "zu", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "$,", "KOUS", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit gr\u00f6\u00dferm Muth es immer neuen Schmerz,", "tokens": ["Mit", "gr\u00f6\u00b7\u00dferm", "Muth", "es", "im\u00b7mer", "neu\u00b7en", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und immer neuen Kummer trage:", "tokens": ["Und", "im\u00b7mer", "neu\u00b7en", "Kum\u00b7mer", "tra\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Erringen qu\u00e4lt, Errung'nem droht Verlust,", "tokens": ["Er\u00b7rin\u00b7gen", "qu\u00e4lt", ",", "Er\u00b7rung'\u00b7nem", "droht", "Ver\u00b7lust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und ew'ge Sehnsucht hebt die bange J\u00fcnglingsbrust.", "tokens": ["Und", "ew'\u00b7ge", "Sehn\u00b7sucht", "hebt", "die", "ban\u00b7ge", "J\u00fcng\u00b7lings\u00b7brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Drum preis' ich den, der nicht begehrt!", "tokens": ["Drum", "preis'", "ich", "den", ",", "der", "nicht", "be\u00b7gehrt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "$,", "PRELS", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was w\u00e4re hier im leichten Staube", "tokens": ["Was", "w\u00e4\u00b7re", "hier", "im", "leich\u00b7ten", "Stau\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Des Suchens oder Findens werth?", "tokens": ["Des", "Su\u00b7chens", "o\u00b7der", "Fin\u00b7dens", "werth", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach h\u00f6h'rem Ziel verweist der h\u00f6h're Glaube;", "tokens": ["Nach", "h\u00f6h'\u00b7rem", "Ziel", "ver\u00b7weist", "der", "h\u00f6h'\u00b7re", "Glau\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Hier ist es nicht, wo jedes Ding verlezt,", "tokens": ["Hier", "ist", "es", "nicht", ",", "wo", "je\u00b7des", "Ding", "ver\u00b7lezt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "$,", "PWAV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Jenseits des Lebens ward dein Ziel hinausgesezt!", "tokens": ["Jen\u00b7seits", "des", "Le\u00b7bens", "ward", "dein", "Ziel", "hin\u00b7aus\u00b7ge\u00b7sezt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.4": {"line.1": {"text": "Im Geiste strebe zu entfliehn", "tokens": ["Im", "Geis\u00b7te", "stre\u00b7be", "zu", "ent\u00b7fliehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PTKZU", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Schranken dieser Menscheninnung,", "tokens": ["Den", "Schran\u00b7ken", "die\u00b7ser", "Men\u00b7sche\u00b7nin\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und la\u00df am Busen dir vor\u00fcberziehn", "tokens": ["Und", "la\u00df", "am", "Bu\u00b7sen", "dir", "vor\u00b7\u00fc\u00b7ber\u00b7ziehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "APPRART", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Stimmungen der wechselnden Gesinnung;", "tokens": ["Die", "Stim\u00b7mun\u00b7gen", "der", "wech\u00b7seln\u00b7den", "Ge\u00b7sin\u00b7nung", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.5": {"text": "Dann tr\u00fcbt der Klarheit innern Spiegel nie,", "tokens": ["Dann", "tr\u00fcbt", "der", "Klar\u00b7heit", "in\u00b7nern", "Spie\u00b7gel", "nie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Durch Lieb' und Sorg' und Ha\u00df, die rege Phantasie.", "tokens": ["Durch", "Lieb'", "und", "Sor\u00b7g'", "und", "Ha\u00df", ",", "die", "re\u00b7ge", "Phan\u00b7ta\u00b7sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "La\u00df Andre denn mit ird'schem Blick", "tokens": ["La\u00df", "And\u00b7re", "denn", "mit", "ird'\u00b7schem", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PIS", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach ihren bunten Zwecken haschen,", "tokens": ["Nach", "ih\u00b7ren", "bun\u00b7ten", "Zwe\u00b7cken", "ha\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sobald Geschick sie oder Mi\u00dfgeschick", "tokens": ["So\u00b7bald", "Ge\u00b7schick", "sie", "o\u00b7der", "Mi\u00df\u00b7ge\u00b7schick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "PPER", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Im steten Wandel spielend \u00fcberraschen:", "tokens": ["Im", "ste\u00b7ten", "Wan\u00b7del", "spie\u00b7lend", "\u00fc\u00b7berr\u00b7a\u00b7schen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Gesch\u00e4ftig sind sie, doch ihr Thun ist leer,", "tokens": ["Ge\u00b7sch\u00e4f\u00b7tig", "sind", "sie", ",", "doch", "ihr", "Thun", "ist", "leer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und schnellzerst\u00f6rend folgt das Schicksal hinterher.", "tokens": ["Und", "schnell\u00b7zer\u00b7st\u00f6\u00b7rend", "folgt", "das", "Schick\u00b7sal", "hin\u00b7ter\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}