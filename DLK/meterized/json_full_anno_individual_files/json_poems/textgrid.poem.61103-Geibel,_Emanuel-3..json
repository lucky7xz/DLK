{"textgrid.poem.61103": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jubeln am Morgen die Lerchen und dehnt in heiterer Bl\u00e4ue", "tokens": ["Ju\u00b7beln", "am", "Mor\u00b7gen", "die", "Ler\u00b7chen", "und", "dehnt", "in", "hei\u00b7te\u00b7rer", "Bl\u00e4u\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "\u00dcber des \u00fcppigen Tals Wipfeln der Himmel sich aus:", "tokens": ["\u00dc\u00b7ber", "des", "\u00fcp\u00b7pi\u00b7gen", "Tals", "Wip\u00b7feln", "der", "Him\u00b7mel", "sich", "aus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "ART", "NN", "PRF", "PTKVZ", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "O wie erfreut mich alsdann Homers anmutige Klarheit,", "tokens": ["O", "wie", "er\u00b7freut", "mich", "als\u00b7dann", "Ho\u00b7mers", "an\u00b7mu\u00b7ti\u00b7ge", "Klar\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VVFIN", "PPER", "ADV", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wie bewegt mir alsdann Sophokles' W\u00fcrde das Herz!", "tokens": ["Wie", "be\u00b7wegt", "mir", "als\u00b7dann", "So\u00b7pho\u00b7kles'", "W\u00fcr\u00b7de", "das", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "NE", "VAFIN", "ART", "NN", "$."], "meter": "-