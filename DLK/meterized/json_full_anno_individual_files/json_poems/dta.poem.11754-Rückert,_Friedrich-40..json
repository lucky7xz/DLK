{"dta.poem.11754": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "40.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der alte Hauswirth, in der Wirthschaft wohl erfahren,", "tokens": ["Der", "al\u00b7te", "Haus\u00b7wirth", ",", "in", "der", "Wirth\u00b7schaft", "wohl", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat dich gelehrt, wo du, wo nicht du sollest sparen.", "tokens": ["Hat", "dich", "ge\u00b7lehrt", ",", "wo", "du", ",", "wo", "nicht", "du", "sol\u00b7lest", "spa\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "PWAV", "PPER", "$,", "PWAV", "PTKNEG", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Voll sch\u00f6pf' aus vollem Fa\u00df, das leere leere schnell,", "tokens": ["Voll", "sch\u00f6pf'", "aus", "vol\u00b7lem", "Fa\u00df", ",", "das", "lee\u00b7re", "lee\u00b7re", "schnell", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ADJA", "NN", "$,", "ART", "ADJA", "ADJA", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch zwischen voll und leer, da halte Haus, Gesell!", "tokens": ["Doch", "zwi\u00b7schen", "voll", "und", "leer", ",", "da", "hal\u00b7te", "Haus", ",", "Ge\u00b7sell", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "ADJD", "KON", "ADJD", "$,", "KOUS", "ADJA", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Voll sch\u00f6pf aus vollem Fa\u00df, und in der Mitte spar;", "tokens": ["Voll", "sch\u00f6pf", "aus", "vol\u00b7lem", "Fa\u00df", ",", "und", "in", "der", "Mit\u00b7te", "spar", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ADJA", "NN", "$,", "KON", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Neige sparen ist unn\u00fctz und undankbar.", "tokens": ["Die", "Nei\u00b7ge", "spa\u00b7ren", "ist", "un\u00b7n\u00fctz", "und", "un\u00b7dank\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Warum? kein Sparen frommt, da\u00df neu Ersch\u00f6pftes steige,", "tokens": ["Wa\u00b7rum", "?", "kein", "Spa\u00b7ren", "frommt", ",", "da\u00df", "neu", "Er\u00b7sch\u00f6pf\u00b7tes", "stei\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PIAT", "NN", "VVFIN", "$,", "KOUS", "ADJD", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und schal am Ende wird dir nur die schmale Neige.", "tokens": ["Und", "schal", "am", "En\u00b7de", "wird", "dir", "nur", "die", "schma\u00b7le", "Nei\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Des Fasses Anbruch sei ein Fest, ein Fest sein Ende;", "tokens": ["Des", "Fas\u00b7ses", "An\u00b7bruch", "sei", "ein", "Fest", ",", "ein", "Fest", "sein", "En\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ART", "NN", "$,", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Haustrunk ist Mittleres, das Aeu\u00dfre G\u00f6tterspende.", "tokens": ["Haus\u00b7trunk", "ist", "Mitt\u00b7le\u00b7res", ",", "das", "A\u00b7e\u00b7u\u00df\u00b7re", "G\u00f6t\u00b7ter\u00b7spen\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "++-+-+-+-+-+-+-", "measure": "unknown.measure.octa.plus"}}, "stanza.6": {"line.1": {"text": "Der Anfang und das End' ist unklar, oben Schaum,", "tokens": ["Der", "An\u00b7fang", "und", "das", "End'", "ist", "un\u00b7klar", ",", "o\u00b7ben", "Schaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VAFIN", "ADJD", "$,", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hef' unten, klarer Wein ist in dem Mittelraum.", "tokens": ["He\u00b7f'", "un\u00b7ten", ",", "kla\u00b7rer", "Wein", "ist", "in", "dem", "Mit\u00b7tel\u00b7raum", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "ADJA", "NN", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}}}}