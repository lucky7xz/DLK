{"textgrid.poem.38298": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Jahreszeiten", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schwarzbraun ist meine dunkle Farbe,", "tokens": ["Schwarz\u00b7braun", "ist", "mei\u00b7ne", "dunk\u00b7le", "Far\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Darin will ich mich kleiden;", "tokens": ["Da\u00b7rin", "will", "ich", "mich", "klei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den besten Schatz und den ich hab,", "tokens": ["Den", "bes\u00b7ten", "Schatz", "und", "den", "ich", "hab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der will jezt von mir scheiden.", "tokens": ["Der", "will", "jezt", "von", "mir", "schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ei scheidet sich dann der Winter von mir,", "tokens": ["Ei", "schei\u00b7det", "sich", "dann", "der", "Win\u00b7ter", "von", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ADV", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So kommt ein frischer Sommer;", "tokens": ["So", "kommt", "ein", "fri\u00b7scher", "Som\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hat er dann Lust und Liebe zu mir,", "tokens": ["Hat", "er", "dann", "Lust", "und", "Lie\u00b7be", "zu", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "NN", "KON", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "So wird er wiederum kommen.", "tokens": ["So", "wird", "er", "wie\u00b7de\u00b7rum", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Dort droben vor meines Vaters Haus,", "tokens": ["Dort", "dro\u00b7ben", "vor", "mei\u00b7nes", "Va\u00b7ters", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da steht eine gr\u00fcne Linde;", "tokens": ["Da", "steht", "ei\u00b7ne", "gr\u00fc\u00b7ne", "Lin\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Darauf sa\u00df die Frau Nachtigall", "tokens": ["Da\u00b7rauf", "sa\u00df", "die", "Frau", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "NN", "NN"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.4": {"text": "Und sang von heller Stimme.", "tokens": ["Und", "sang", "von", "hel\u00b7ler", "Stim\u00b7me", "."], "token_info": ["word", "word", "word", "word", 