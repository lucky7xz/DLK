{"textgrid.poem.33349": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Die Kunst zu lieben", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "M\u00e4dchen, will man recht sich freu'n", "tokens": ["M\u00e4d\u00b7chen", ",", "will", "man", "recht", "sich", "freu'n"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VMFIN", "PIS", "ADJD", "PRF", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie sich's ziemt, so mu\u00df man fein", "tokens": ["Wie", "sich's", "ziemt", ",", "so", "mu\u00df", "man", "fein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "VVFIN", "$,", "ADV", "VMFIN", "PIS", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Amors Spiele kennen;", "tokens": ["A\u00b7mors", "Spie\u00b7le", "ken\u00b7nen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Also, M\u00e4dchen, h\u00f6re mich,", "tokens": ["Al\u00b7so", ",", "M\u00e4d\u00b7chen", ",", "h\u00f6\u00b7re", "mich", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Im vertrauen Ton will ich", "tokens": ["Im", "ver\u00b7trau\u00b7en", "Ton", "will", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VMFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sie dir alle nennen.", "tokens": ["Sie", "dir", "al\u00b7le", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "PIS", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Erstlich soll ein liebend Herz", "tokens": ["Erst\u00b7lich", "soll", "ein", "lie\u00b7bend", "Herz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VMFIN", "ART", "ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Jede Handlung, jeden Schmerz", "tokens": ["Je\u00b7de", "Hand\u00b7lung", ",", "je\u00b7den", "Schmerz"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Adeln und beleben;", "tokens": ["A\u00b7deln", "und", "be\u00b7le\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Nur die Liebe lehrt die Kunst,", "tokens": ["Nur", "die", "Lie\u00b7be", "lehrt", "die", "Kunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jedem Spiele, jeder Gunst", "tokens": ["Je\u00b7dem", "Spie\u00b7le", ",", "je\u00b7der", "Gunst"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Grazie zu geben.", "tokens": ["Gra\u00b7zie", "zu", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "K\u00fcsse sind der Liebe Bund:", "tokens": ["K\u00fcs\u00b7se", "sind", "der", "Lie\u00b7be", "Bund", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Es ist s\u00fc\u00df, wenn Mund an Mund", "tokens": ["Es", "ist", "s\u00fc\u00df", ",", "wenn", "Mund", "an", "Mund"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "KOUS", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sich mein Blick umnebelt;", "tokens": ["Sich", "mein", "Blick", "um\u00b7ne\u00b7belt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "VVPP", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Aber noch weit s\u00fcsser, wenn", "tokens": ["A\u00b7ber", "noch", "weit", "s\u00fcs\u00b7ser", ",", "wenn"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADV", "ADJD", "ADJD", "$,", "KOUS"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dein gespitztes Z\u00fcngelchen", "tokens": ["Dein", "ge\u00b7spitz\u00b7tes", "Z\u00fcn\u00b7gel\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.6": {"text": "Mit dem meinen schn\u00e4belt.", "tokens": ["Mit", "dem", "mei\u00b7nen", "schn\u00e4\u00b7belt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Auch schmeckt trefflich jeder Ku\u00df,", "tokens": ["Auch", "schmeckt", "treff\u00b7lich", "je\u00b7der", "Ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PIAT", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Den ich nicht erbetteln mu\u00df;", "tokens": ["Den", "ich", "nicht", "er\u00b7bet\u00b7teln", "mu\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber, M\u00e4dchen, glaube,", "tokens": ["A\u00b7ber", ",", "M\u00e4d\u00b7chen", ",", "glau\u00b7be", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "$,", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Noch viel besser schmeckt er mir,", "tokens": ["Noch", "viel", "bes\u00b7ser", "schmeckt", "er", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VVFIN", "PPER", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn du schmollst, und ich ihn dir,", "tokens": ["Wenn", "du", "schmollst", ",", "und", "ich", "ihn", "dir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KON", "PPER", "PPER", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dann versto\u00dfen raube.", "tokens": ["Dann", "ver\u00b7sto\u00b7\u00dfen", "rau\u00b7be", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Doch wenn der Gesellschaft Zwang", "tokens": ["Doch", "wenn", "der", "Ge\u00b7sell\u00b7schaft", "Zwang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Uns oft manche Stunde lang", "tokens": ["Uns", "oft", "man\u00b7che", "Stun\u00b7de", "lang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "PIAT", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf die Folter spannet,", "tokens": ["Auf", "die", "Fol\u00b7ter", "span\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Und verw\u00fcnschter Lauscher Blick", "tokens": ["Und", "ver\u00b7w\u00fcnschter", "Lau\u00b7scher", "Blick"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Uns dann in uns selbst zur\u00fcck", "tokens": ["Uns", "dann", "in", "uns", "selbst", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "APPR", "PPER", "ADV", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Menschenfeindlich bannet;", "tokens": ["Men\u00b7schen\u00b7feind\u00b7lich", "ban\u00b7net", ";"], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Dann soll, Jedem unsichtbar,", "tokens": ["Dann", "soll", ",", "Je\u00b7dem", "un\u00b7sicht\u00b7bar", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "$,", "PIAT", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dir im feuchten Augenpaar", "tokens": ["Dir", "im", "feuch\u00b7ten", "Au\u00b7gen\u00b7paar"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Stille Liebe blinken,", "tokens": ["Stil\u00b7le", "Lie\u00b7be", "blin\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Und in jedem L\u00e4cheln soll", "tokens": ["Und", "in", "je\u00b7dem", "L\u00e4\u00b7cheln", "soll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Naher, naher Liebeszoll", "tokens": ["Na\u00b7her", ",", "na\u00b7her", "Lie\u00b7bes\u00b7zoll"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Mir entgegen winken.", "tokens": ["Mir", "ent\u00b7ge\u00b7gen", "win\u00b7ken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Schlaue Liebest\u00e4ndeley,", "tokens": ["Schlau\u00b7e", "Lie\u00b7be\u00b7st\u00e4n\u00b7de\u00b7ley", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00e4ndedruck, Lieb\u00e4ugeley,", "tokens": ["H\u00e4n\u00b7de\u00b7druck", ",", "Lie\u00b7b\u00e4u\u00b7ge\u00b7ley", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Unter'm Tisch ein F\u00fc\u00dfchen,", "tokens": ["Un\u00b7ter'm", "Tisch", "ein", "F\u00fc\u00df\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Fest an meines angedr\u00fcckt,", "tokens": ["Fest", "an", "mei\u00b7nes", "an\u00b7ge\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Auch, wenn Niemand auf uns blickt,", "tokens": ["Auch", ",", "wenn", "Nie\u00b7mand", "auf", "uns", "blickt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PIS", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ein verstohl'nes K\u00fc\u00dfchen.", "tokens": ["Ein", "ver\u00b7stohl'\u00b7nes", "K\u00fc\u00df\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Und die tausend K\u00fcnstchen all'", "tokens": ["Und", "die", "tau\u00b7send", "K\u00fcnst\u00b7chen", "all'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "CARD", "NN", "PIAT"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Werden, Liebchen, \u00fcberall", "tokens": ["Wer\u00b7den", ",", "Lieb\u00b7chen", ",", "\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["VAFIN", "$,", "NN", "$,", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lebensfroh uns machen,", "tokens": ["Le\u00b7bens\u00b7froh", "uns", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Und in jedem Cirkel wird,", "tokens": ["Und", "in", "je\u00b7dem", "Cir\u00b7kel", "wird", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Von dem Neid unausgesp\u00fcrt,", "tokens": ["Von", "dem", "Neid", "un\u00b7aus\u00b7ge\u00b7sp\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Uns die Liebe lachen.", "tokens": ["Uns", "die", "Lie\u00b7be", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Aber, wenn wir ganz allein", "tokens": ["A\u00b7ber", ",", "wenn", "wir", "ganz", "al\u00b7lein"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "PPER", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Blo\u00df der Liebe Gl\u00fcck uns weih'n", "tokens": ["Blo\u00df", "der", "Lie\u00b7be", "Gl\u00fcck", "uns", "weih'n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "NN", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ungeseh'n uns k\u00fcssen:", "tokens": ["Un\u00b7ge\u00b7seh'n", "uns", "k\u00fcs\u00b7sen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Dann la\u00df Phantasie und Herz,", "tokens": ["Dann", "la\u00df", "Phan\u00b7ta\u00b7sie", "und", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "NN", "KON", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Jeder Laune, jedem Schmerz", "tokens": ["Je\u00b7der", "Lau\u00b7ne", ",", "je\u00b7dem", "Schmerz"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Alle Z\u00fcgel schiessen!", "tokens": ["Al\u00b7le", "Z\u00fc\u00b7gel", "schies\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "Dann la\u00df uns beim ersten Ku\u00df,", "tokens": ["Dann", "la\u00df", "uns", "beim", "ers\u00b7ten", "Ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Aufgel\u00f6st in Lieb'sgenu\u00df", "tokens": ["Auf\u00b7ge\u00b7l\u00f6st", "in", "Lie\u00b7b's\u00b7ge\u00b7nu\u00df"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "In einander sinken,", "tokens": ["In", "ein\u00b7an\u00b7der", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PRF", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Und mit trunknem Geist und Sinn", "tokens": ["Und", "mit", "trunk\u00b7nem", "Geist", "und", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aus dem Wollustbecher in", "tokens": ["Aus", "dem", "Wol\u00b7lust\u00b7be\u00b7cher", "in"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Langen Z\u00fcgen trinken.", "tokens": ["Lan\u00b7gen", "Z\u00fc\u00b7gen", "trin\u00b7ken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.11": {"line.1": {"text": "Sieh doch, wie durch Zauberei", "tokens": ["Sieh", "doch", ",", "wie", "durch", "Zau\u00b7be\u00b7rei"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADV", "$,", "PWAV", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist mir all' die K\u00fcnstelei", "tokens": ["Ist", "mir", "all'", "die", "K\u00fcns\u00b7te\u00b7lei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PIS", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Angesichts verschwunden:", "tokens": ["An\u00b7ge\u00b7sichts", "ver\u00b7schwun\u00b7den", ":"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Nichts sag' ich dir weiter an,", "tokens": ["Nichts", "sag'", "ich", "dir", "wei\u00b7ter", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PPER", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer die Lust beregeln kann,", "tokens": ["Wer", "die", "Lust", "be\u00b7re\u00b7geln", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Hat sie nie empfunden.", "tokens": ["Hat", "sie", "nie", "emp\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.12": {"line.1": {"text": "M\u00e4dchen, will man recht sich freu'n", "tokens": ["M\u00e4d\u00b7chen", ",", "will", "man", "recht", "sich", "freu'n"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VMFIN", "PIS", "ADJD", "PRF", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie sich's ziemt, so mu\u00df man fein", "tokens": ["Wie", "sich's", "ziemt", ",", "so", "mu\u00df", "man", "fein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "VVFIN", "$,", "ADV", "VMFIN", "PIS", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Amors Spiele kennen;", "tokens": ["A\u00b7mors", "Spie\u00b7le", "ken\u00b7nen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Also, M\u00e4dchen, h\u00f6re mich,", "tokens": ["Al\u00b7so", ",", "M\u00e4d\u00b7chen", ",", "h\u00f6\u00b7re", "mich", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Im vertrauen Ton will ich", "tokens": ["Im", "ver\u00b7trau\u00b7en", "Ton", "will", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VMFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sie dir alle nennen.", "tokens": ["Sie", "dir", "al\u00b7le", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "PIS", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.13": {"line.1": {"text": "Erstlich soll ein liebend Herz", "tokens": ["Erst\u00b7lich", "soll", "ein", "lie\u00b7bend", "Herz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VMFIN", "ART", "ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Jede Handlung, jeden Schmerz", "tokens": ["Je\u00b7de", "Hand\u00b7lung", ",", "je\u00b7den", "Schmerz"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Adeln und beleben;", "tokens": ["A\u00b7deln", "und", "be\u00b7le\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Nur die Liebe lehrt die Kunst,", "tokens": ["Nur", "die", "Lie\u00b7be", "lehrt", "die", "Kunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jedem Spiele, jeder Gunst", "tokens": ["Je\u00b7dem", "Spie\u00b7le", ",", "je\u00b7der", "Gunst"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Grazie zu geben.", "tokens": ["Gra\u00b7zie", "zu", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.14": {"line.1": {"text": "K\u00fcsse sind der Liebe Bund:", "tokens": ["K\u00fcs\u00b7se", "sind", "der", "Lie\u00b7be", "Bund", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Es ist s\u00fc\u00df, wenn Mund an Mund", "tokens": ["Es", "ist", "s\u00fc\u00df", ",", "wenn", "Mund", "an", "Mund"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "KOUS", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sich mein Blick umnebelt;", "tokens": ["Sich", "mein", "Blick", "um\u00b7ne\u00b7belt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "VVPP", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Aber noch weit s\u00fcsser, wenn", "tokens": ["A\u00b7ber", "noch", "weit", "s\u00fcs\u00b7ser", ",", "wenn"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADV", "ADJD", "ADJD", "$,", "KOUS"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dein gespitztes Z\u00fcngelchen", "tokens": ["Dein", "ge\u00b7spitz\u00b7tes", "Z\u00fcn\u00b7gel\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.6": {"text": "Mit dem meinen schn\u00e4belt.", "tokens": ["Mit", "dem", "mei\u00b7nen", "schn\u00e4\u00b7belt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.15": {"line.1": {"text": "Auch schmeckt trefflich jeder Ku\u00df,", "tokens": ["Auch", "schmeckt", "treff\u00b7lich", "je\u00b7der", "Ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PIAT", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Den ich nicht erbetteln mu\u00df;", "tokens": ["Den", "ich", "nicht", "er\u00b7bet\u00b7teln", "mu\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber, M\u00e4dchen, glaube,", "tokens": ["A\u00b7ber", ",", "M\u00e4d\u00b7chen", ",", "glau\u00b7be", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "$,", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Noch viel besser schmeckt er mir,", "tokens": ["Noch", "viel", "bes\u00b7ser", "schmeckt", "er", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VVFIN", "PPER", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn du schmollst, und ich ihn dir,", "tokens": ["Wenn", "du", "schmollst", ",", "und", "ich", "ihn", "dir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KON", "PPER", "PPER", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dann versto\u00dfen raube.", "tokens": ["Dann", "ver\u00b7sto\u00b7\u00dfen", "rau\u00b7be", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.16": {"line.1": {"text": "Doch wenn der Gesellschaft Zwang", "tokens": ["Doch", "wenn", "der", "Ge\u00b7sell\u00b7schaft", "Zwang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Uns oft manche Stunde lang", "tokens": ["Uns", "oft", "man\u00b7che", "Stun\u00b7de", "lang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "PIAT", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf die Folter spannet,", "tokens": ["Auf", "die", "Fol\u00b7ter", "span\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Und verw\u00fcnschter Lauscher Blick", "tokens": ["Und", "ver\u00b7w\u00fcnschter", "Lau\u00b7scher", "Blick"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Uns dann in uns selbst zur\u00fcck", "tokens": ["Uns", "dann", "in", "uns", "selbst", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "APPR", "PPER", "ADV", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Menschenfeindlich bannet;", "tokens": ["Men\u00b7schen\u00b7feind\u00b7lich", "ban\u00b7net", ";"], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.17": {"line.1": {"text": "Dann soll, Jedem unsichtbar,", "tokens": ["Dann", "soll", ",", "Je\u00b7dem", "un\u00b7sicht\u00b7bar", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "$,", "PIAT", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dir im feuchten Augenpaar", "tokens": ["Dir", "im", "feuch\u00b7ten", "Au\u00b7gen\u00b7paar"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPRART", "ADJA", "NN"], "meter": "+-+