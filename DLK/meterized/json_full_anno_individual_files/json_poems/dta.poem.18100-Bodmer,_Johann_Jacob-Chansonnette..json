{"dta.poem.18100": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Chansonnette.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198432", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mit Liebes-Brunst behafftet seyn,", "tokens": ["Mit", "Lie\u00b7bes\u00b7Brunst", "be\u00b7haff\u00b7tet", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist wahrlich eine schwere Pein,", "tokens": ["Ist", "wahr\u00b7lich", "ei\u00b7ne", "schwe\u00b7re", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es ist kein Schmertz auf dieser Erd,", "tokens": ["Es", "ist", "kein", "Schmertz", "auf", "die\u00b7ser", "Erd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der recht mit ihm verglichen werd:", "tokens": ["Der", "recht", "mit", "ihm", "ver\u00b7gli\u00b7chen", "werd", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Drum will ich mich Gantz emsiglich", "tokens": ["Drum", "will", "ich", "mich", "Gantz", "em\u00b7sig\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "PRF", "ADV", "ADJD"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Von dem Ley\u2019en Allzeit scheiden,", "tokens": ["Von", "dem", "Ley'\u00b7en", "All\u00b7zeit", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die s\u00fcsse Gifft vermeiden.", "tokens": ["Und", "die", "s\u00fcs\u00b7se", "Gifft", "ver\u00b7mei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Auf da\u00df nun nicht die schn\u00f6de Brunst", "tokens": ["Auf", "da\u00df", "nun", "nicht", "die", "schn\u00f6\u00b7de", "Brunst"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "ADV", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Mich lasse zu ihr tragen Gunst,", "tokens": ["Mich", "las\u00b7se", "zu", "ihr", "tra\u00b7gen", "Gunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Soll Venus mich nicht treffen an", "tokens": ["Soll", "Ve\u00b7nus", "mich", "nicht", "tref\u00b7fen", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "NN", "PPER", "PTKNEG", "VVINF", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Auf irgend einer Liebes-Bahn.", "tokens": ["Auf", "ir\u00b7gend", "ei\u00b7ner", "Lie\u00b7bes\u00b7Bahn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Der Tugend Weg Ist ein sch\u00f6n Steg,", "tokens": ["Der", "Tu\u00b7gend", "Weg", "Ist", "ein", "sch\u00f6n", "Steg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Darauf eben Ich will schweben,", "tokens": ["Da\u00b7rauf", "e\u00b7ben", "Ich", "will", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Und ihr gantz verpflichtet leben.", "tokens": ["Und", "ihr", "gantz", "ver\u00b7pflich\u00b7tet", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVPP", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Recht und gar wol auch Pallas blieb", "tokens": ["Recht", "und", "gar", "wol", "auch", "Pal\u00b7las", "blieb"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADV", "ADV", "ADV", "NE", "VVFIN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.16": {"text": "Allzeit befreyet von der Lieb,", "tokens": ["All\u00b7zeit", "be\u00b7fre\u00b7yet", "von", "der", "Lieb", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Sie gab dem Feuer niemals Raum,", "tokens": ["Sie", "gab", "dem", "Feu\u00b7er", "nie\u00b7mals", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Und hielte sich in stetem Zaum.", "tokens": ["Und", "hiel\u00b7te", "sich", "in", "ste\u00b7tem", "Zaum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Auf gr\u00fcner Heyd Sie allezeit", "tokens": ["Auf", "gr\u00fc\u00b7ner", "Heyd", "Sie", "al\u00b7le\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Mit dem Hetzen Sich th\u00e4t letzen,", "tokens": ["Mit", "dem", "Het\u00b7zen", "Sich", "th\u00e4t", "let\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Und frey aller Sorg ergetzen.", "tokens": ["Und", "frey", "al\u00b7ler", "Sorg", "er\u00b7get\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PIAT", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.22": {"text": "Ich will ins k\u00fcnfftig fleissig auch", "tokens": ["Ich", "will", "ins", "k\u00fcnff\u00b7tig", "fleis\u00b7sig", "auch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPRART", "ADJD", "ADJD", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Nachfolgen dieser G\u00f6ttin Brauch,", "tokens": ["Nach\u00b7fol\u00b7gen", "die\u00b7ser", "G\u00f6t\u00b7tin", "Brauch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Denn Venus ist die gr\u00f6ste Last,", "tokens": ["Denn", "Ve\u00b7nus", "ist", "die", "gr\u00f6s\u00b7te", "Last", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Cupido ist ein sch\u00e4dlich Gast.", "tokens": ["Cu\u00b7pi\u00b7do", "ist", "ein", "sch\u00e4d\u00b7lich", "Gast", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "ADJD", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.26": {"text": "Wen e", "tokens": ["Wen", "e"], "token_info": ["word", "word"], "pos": ["PWS", "NE"], "meter": "+-", "measure": "trochaic.single"}, "line.27": {"text": "Mu\u00df verderben, Oft auch sterben,", "tokens": ["Mu\u00df", "ver\u00b7der\u00b7ben", ",", "Oft", "auch", "ster\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVFIN", "$,", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Und f\u00fcr Freuden Schmertz ererben,", "tokens": ["Und", "f\u00fcr", "Freu\u00b7den", "Schmertz", "er\u00b7er\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Also belohnt er alle doch,", "tokens": ["Al\u00b7so", "be\u00b7lohnt", "er", "al\u00b7le", "doch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "ADV", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.30": {"text": "Die sich ergeben seinem Joch,", "tokens": ["Die", "sich", "er\u00b7ge\u00b7ben", "sei\u00b7nem", "Joch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Und di\u00df bedenck ich offt und viel,", "tokens": ["Und", "di\u00df", "be\u00b7denck", "ich", "offt", "und", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPER", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Es mag lieb haben wer da will,", "tokens": ["Es", "mag", "lieb", "ha\u00b7ben", "wer", "da", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "VAINF", "PWS", "ADV", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Ich bleibe mein. Allzeit allein,", "tokens": ["Ich", "blei\u00b7be", "mein", ".", "All\u00b7zeit", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "$.", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Offt nach schertzen. Kommen Schmertzen,", "tokens": ["Offt", "nach", "schert\u00b7zen", ".", "Kom\u00b7men", "Schmert\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "VVINF", "$.", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Wohl dem der das thut behertzen.", "tokens": ["Wohl", "dem", "der", "das", "thut", "be\u00b7hert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ART", "PDS", "VVFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}