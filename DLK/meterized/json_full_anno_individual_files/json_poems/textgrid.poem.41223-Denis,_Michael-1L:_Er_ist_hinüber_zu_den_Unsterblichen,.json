{"textgrid.poem.41223": {"metadata": {"author": {"name": "Denis, Michael", "birth": "N.A.", "death": "N.A."}, "title": "1L: Er ist hin\u00fcber zu den Unsterblichen,", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er ist hin\u00fcber zu den Unsterblichen,", "tokens": ["Er", "ist", "hin\u00b7\u00fc\u00b7ber", "zu", "den", "U\u00b7nsterb\u00b7li\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+---", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Unwiderbringlich menschlicher Z\u00e4rtlichkeit!", "tokens": ["Un\u00b7wi\u00b7der\u00b7bring\u00b7lich", "menschli\u00b7cher", "Z\u00e4rt\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er ist hin\u00fcber! Keine Thr\u00e4nen", "tokens": ["Er", "ist", "hin\u00b7\u00fc\u00b7ber", "!", "Kei\u00b7ne", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "$.", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weinen, o**! dich uns zur\u00fccke!", "tokens": ["Wei\u00b7nen", ",", "o", "*", "*", "!", "dich", "uns", "zu\u00b7r\u00fc\u00b7cke", "!"], "token_info": ["word", "punct", "word", "punct", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "XY", "XY", "$.", "PPER", "PPER", "PTKVZ", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Sanft, wie die Quelle, die durch die Wiese schleicht,", "tokens": ["Sanft", ",", "wie", "die", "Quel\u00b7le", ",", "die", "durch", "die", "Wie\u00b7se", "schleicht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Nie trug sie brausend schwimmender Schl\u00f6sser Last,", "tokens": ["Nie", "trug", "sie", "brau\u00b7send", "schwim\u00b7men\u00b7der", "Schl\u00f6s\u00b7ser", "Last", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nie schwoll sie sch\u00e4umend \u00fcber Blumen,", "tokens": ["Nie", "schwoll", "sie", "sch\u00e4u\u00b7mend", "\u00fc\u00b7ber", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die sich am n\u00fcchternen Rande spiegeln:", "tokens": ["Die", "sich", "am", "n\u00fcch\u00b7ter\u00b7nen", "Ran\u00b7de", "spie\u00b7geln", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Sie w\u00fcrzt die Fluren heimlich und unbelobt,", "tokens": ["Sie", "w\u00fcrzt", "die", "Flu\u00b7ren", "heim\u00b7lich", "und", "un\u00b7be\u00b7lobt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nur Wand'rern findbar, welche der Durst gereizt,", "tokens": ["Nur", "Wan\u00b7d'\u00b7rern", "find\u00b7bar", ",", "wel\u00b7che", "der", "Durst", "ge\u00b7reizt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADJD", "$,", "PRELS", "ART", "NN", "ADJD", "$,"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dann selber sanft noch, wann ihr Lauf sich", "tokens": ["Dann", "sel\u00b7ber", "sanft", "noch", ",", "wann", "ihr", "Lauf", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "ADV", "$,", "PWAV", "PPOSAT", "NN", "PRF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weit in den Ocean hin verlieret:", "tokens": ["Weit", "in", "den", "O\u00b7cean", "hin", "ver\u00b7lie\u00b7ret", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So flo\u00df, o Freund! dein Leben. Ganz unbemerkt", "tokens": ["So", "flo\u00df", ",", "o", "Freund", "!", "dein", "Le\u00b7ben", ".", "Ganz", "un\u00b7be\u00b7merkt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "FM", "NN", "$.", "PPOSAT", "NN", "$.", "ADV", "ADJD"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dem lauten Haufen feiler Verewiger,", "tokens": ["Dem", "lau\u00b7ten", "Hau\u00b7fen", "fei\u00b7ler", "Ve\u00b7re\u00b7wi\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dem schielen Auge kleiner Richter,", "tokens": ["Dem", "schie\u00b7len", "Au\u00b7ge", "klei\u00b7ner", "Rich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "That'st du der Ewigkeit werth're Thaten,", "tokens": ["That'st", "du", "der", "E\u00b7wig\u00b7keit", "wert\u00b7h'\u00b7re", "Tha\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Als Mancher, welchen rauschender Beifall bl\u00e4ht.", "tokens": ["Als", "Man\u00b7cher", ",", "wel\u00b7chen", "rau\u00b7schen\u00b7der", "Bei\u00b7fall", "bl\u00e4ht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "PWAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dich wies kein Finger, wenn du vor\u00fcbergingst,", "tokens": ["Dich", "wies", "kein", "Fin\u00b7ger", ",", "wenn", "du", "vor\u00b7\u00fc\u00b7ber\u00b7gingst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Doch kannten dich gew\u00e4hlte Freunde,", "tokens": ["Doch", "kann\u00b7ten", "dich", "ge\u00b7w\u00e4hl\u00b7te", "Freun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Freunde, von dir, ach! zu fr\u00fch verlassen.", "tokens": ["Freun\u00b7de", ",", "von", "dir", ",", "ach", "!", "zu", "fr\u00fch", "ver\u00b7las\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPER", "$,", "ITJ", "$.", "PTKA", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Schon lange drangst du, von der Natur gef\u00fchrt,", "tokens": ["Schon", "lan\u00b7ge", "drangst", "du", ",", "von", "der", "Na\u00b7tur", "ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Tief in der Wesen inn're Geheimnisse.", "tokens": ["Tief", "in", "der", "We\u00b7sen", "inn'\u00b7re", "Ge\u00b7heim\u00b7nis\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Was ist, und w\u00e4chst, und f\u00fchlt, und denket,", "tokens": ["Was", "ist", ",", "und", "w\u00e4chst", ",", "und", "f\u00fchlt", ",", "und", "den\u00b7ket", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "KON", "VVFIN", "$,", "KON", "VVFIN", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Lehrte sie dich in geweihten Stunden.", "tokens": ["Lehr\u00b7te", "sie", "dich", "in", "ge\u00b7weih\u00b7ten", "Stun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Von ihr gelehret, trat'st du zur Jugend hin.", "tokens": ["Von", "ihr", "ge\u00b7leh\u00b7ret", ",", "trat'st", "du", "zur", "Ju\u00b7gend", "hin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$,", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ich sah, wie Bienen, bl\u00fchende J\u00fcnglinge", "tokens": ["Ich", "sah", ",", "wie", "Bie\u00b7nen", ",", "bl\u00fc\u00b7hen\u00b7de", "J\u00fcng\u00b7lin\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+--++-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Von deinen Lippen Honig saugen,", "tokens": ["Von", "dei\u00b7nen", "Lip\u00b7pen", "Ho\u00b7nig", "sau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und f\u00fcr die Lehre dir Herzen schenken.", "tokens": ["Und", "f\u00fcr", "die", "Leh\u00b7re", "dir", "Her\u00b7zen", "schen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Freund! r\u00fchrt ein Wunsch dich deiner Verlassenen!", "tokens": ["Freund", "!", "r\u00fchrt", "ein", "Wunsch", "dich", "dei\u00b7ner", "Ver\u00b7las\u00b7se\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "ART", "NN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Von jenen H\u00f6hen, wo dich die Tugend kr\u00f6n't,", "tokens": ["Von", "je\u00b7nen", "H\u00f6\u00b7hen", ",", "wo", "dich", "die", "Tu\u00b7gend", "kr\u00f6n't", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "O senke deinen Blick zur St\u00e4tte,", "tokens": ["O", "sen\u00b7ke", "dei\u00b7nen", "Blick", "zur", "St\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Welche verkl\u00e4ret dich einst zur\u00fcckgibt!", "tokens": ["Wel\u00b7che", "ver\u00b7kl\u00e4\u00b7ret", "dich", "einst", "zu\u00b7r\u00fcck\u00b7gibt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "+--+-++-+-", "measure": "iambic.penta.invert"}}, "stanza.9": {"line.1": {"text": "Hier steh'n sie! z\u00e4rtlich f\u00fchlende Lehrlinge!", "tokens": ["Hier", "steh'n", "sie", "!", "z\u00e4rt\u00b7lich", "f\u00fch\u00b7len\u00b7de", "Lehr\u00b7lin\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom Auge redet dankender Schmerz herab,", "tokens": ["Vom", "Au\u00b7ge", "re\u00b7det", "dan\u00b7ken\u00b7der", "Schmerz", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PAV", "NN", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Vom Auge, welches dich bald oben,", "tokens": ["Vom", "Au\u00b7ge", ",", "wel\u00b7ches", "dich", "bald", "o\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bald in dem Schoose der Erde suchet.", "tokens": ["Bald", "in", "dem", "Schoo\u00b7se", "der", "Er\u00b7de", "su\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.10": {"line.1": {"text": "Verlangst du Thr\u00e4nen auch von der Freunde Schaar? \u2013", "tokens": ["Ver\u00b7langst", "du", "Thr\u00e4\u00b7nen", "auch", "von", "der", "Freun\u00b7de", "Schaar", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "NN", "ADV", "APPR", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "O nein! wir Freunde liebten dich m\u00e4nnlicher.", "tokens": ["O", "nein", "!", "wir", "Freun\u00b7de", "lieb\u00b7ten", "dich", "m\u00e4nn\u00b7li\u00b7cher", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKANT", "$.", "PPER", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Gewi\u00df dich wieder einst zu finden,", "tokens": ["Ge\u00b7wi\u00df", "dich", "wie\u00b7der", "einst", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wollen wir m\u00e4nnlicher dich verlieren.", "tokens": ["Wol\u00b7len", "wir", "m\u00e4nn\u00b7li\u00b7cher", "dich", "ver\u00b7lie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PPER", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.11": {"line.1": {"text": "Er ist hin\u00fcber zu den Unsterblichen,", "tokens": ["Er", "ist", "hin\u00b7\u00fc\u00b7ber", "zu", "den", "U\u00b7nsterb\u00b7li\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+---", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Unwiderbringlich menschlicher Z\u