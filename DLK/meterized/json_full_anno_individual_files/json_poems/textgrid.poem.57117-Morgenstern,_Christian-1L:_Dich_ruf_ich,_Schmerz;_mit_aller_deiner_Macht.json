{"textgrid.poem.57117": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dich ruf ich, Schmerz; mit aller deiner Macht", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dich ruf ich, Schmerz; mit aller deiner Macht", "tokens": ["Dich", "ruf", "ich", ",", "Schmerz", ";", "mit", "al\u00b7ler", "dei\u00b7ner", "Macht"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "NN", "$.", "APPR", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "triff dieses Herz, da\u00df es gemartert werde", "tokens": ["triff", "die\u00b7ses", "Herz", ",", "da\u00df", "es", "ge\u00b7mar\u00b7tert", "wer\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PDAT", "NN", "$,", "KOUS", "PPER", "VVPP", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und, das ich bin, dies H\u00e4uflein arme Erde,", "tokens": ["und", ",", "das", "ich", "bin", ",", "dies", "H\u00e4uf\u00b7lein", "ar\u00b7me", "Er\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PRELS", "PPER", "VAFIN", "$,", "PDS", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "emporh\u00e4lt aus der allgemeinen Nacht.", "tokens": ["em\u00b7por\u00b7h\u00e4lt", "aus", "der", "all\u00b7ge\u00b7mei\u00b7nen", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dich ruf ich, Menschenfreund der besten Art;", "tokens": ["Dich", "ruf", "ich", ",", "Men\u00b7schen\u00b7freund", "der", "bes\u00b7ten", "Art", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "mi\u00dftraue nicht, da\u00df ich dich je verkennte;", "tokens": ["mi\u00df\u00b7trau\u00b7e", "nicht", ",", "da\u00df", "ich", "dich", "je", "ver\u00b7kenn\u00b7te", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "du Schmerz, durch den uns wohl das Gr\u00f6\u00dfte ward,", "tokens": ["du", "Schmerz", ",", "durch", "den", "uns", "wohl", "das", "Gr\u00f6\u00df\u00b7te", "ward", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "APPR", "ART", "PPER", "ADV", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "was Menschenwert von Gott und Tiere trennte.", "tokens": ["was", "Men\u00b7schen\u00b7wert", "von", "Gott", "und", "Tie\u00b7re", "trenn\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dich ruf ich; gib mir deinen bittern Krug;", "tokens": ["Dich", "ruf", "ich", ";", "gib", "mir", "dei\u00b7nen", "bit\u00b7tern", "Krug", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "VVIMP", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und siehst du mich auch bang mich von ihm wenden; \u2013", "tokens