{"textgrid.poem.64909": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Huld, die dir im Auge schwimmt,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Huld, die dir im Auge schwimmt,", "tokens": ["Huld", ",", "die", "dir", "im", "Au\u00b7ge", "schwimmt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sagt mir, da\u00df dein Groll geschieden;", "tokens": ["Sagt", "mir", ",", "da\u00df", "dein", "Groll", "ge\u00b7schie\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber meine Seele stimmt", "tokens": ["A\u00b7ber", "mei\u00b7ne", "See\u00b7le", "stimmt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht in dieses Wort voll Frieden.", "tokens": ["Nicht", "in", "die\u00b7ses", "Wort", "voll", "Frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PDAT", "NN", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Da\u00df vor meinem wilden Schmerz", "tokens": ["Da\u00df", "vor", "mei\u00b7nem", "wil\u00b7den", "Schmerz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dein gerechter Zorn gebrochen", "tokens": ["Dein", "ge\u00b7rech\u00b7ter", "Zorn", "ge\u00b7bro\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Frommt mir nicht, so lang mein Herz", "tokens": ["Frommt", "mir", "nicht", ",", "so", "lang", "mein", "Herz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "ADV", "ADJD", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich nicht gleichfalls losgesprochen.", "tokens": ["Mich", "nicht", "gleich\u00b7falls", "los\u00b7ge\u00b7spro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Tiefre Qual ist's, wenn, vers\u00f6hnt,", "tokens": ["Tief\u00b7re", "Qual", "ist's", ",", "wenn", ",", "ver\u00b7s\u00f6hnt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "$,", "KOUS", "$,", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Du dich liebend zu mir neigest,", "tokens": ["Du", "dich", "lie\u00b7bend", "zu", "mir", "nei\u00b7gest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Vorwurf lauter t\u00f6nt,", "tokens": ["Und", "der", "Vor\u00b7wurf", "lau\u00b7ter", "t\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn du schonend ihn verschweigest.", "tokens": ["Wenn", "du", "scho\u00b7nend", "ihn", "ver\u00b7schwei\u00b7gest", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER"