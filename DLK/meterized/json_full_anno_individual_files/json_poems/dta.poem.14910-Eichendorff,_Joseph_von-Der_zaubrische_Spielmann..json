{"dta.poem.14910": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der zaubrische Spielmann.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Alle Blumen trunken lauschen,", "tokens": ["Al\u00b7le", "Blu\u00b7men", "trun\u00b7ken", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von den Kl\u00e4ngen hold durchirrt,", "tokens": ["Von", "den", "Kl\u00e4n\u00b7gen", "hold", "durch\u00b7irrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lieblicher die Brunnen rauschen,", "tokens": ["Lieb\u00b7li\u00b7cher", "die", "Brun\u00b7nen", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sie eilet s\u00fc\u00df verwirrt. \u2014", "tokens": ["Und", "sie", "ei\u00b7let", "s\u00fc\u00df", "ver\u00b7wirrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "ADJD", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}