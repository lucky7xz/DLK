{"textgrid.poem.63425": {"metadata": {"author": {"name": "Brinckman, John", "birth": "N.A.", "death": "N.A."}, "title": "Ick meen man so", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Up Maidag w\u00fcrr ick achteihn Johr \u2013", "tokens": ["Up", "Mai\u00b7dag", "w\u00fcrr", "ick", "ach\u00b7teihn", "Johr", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "nee, wu de Tit henl\u00f6ppt!", "tokens": ["nee", ",", "wu", "de", "Tit", "hen\u00b7l\u00f6ppt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Un f\u00f6ftig s\u00fcnd v\u00f6r mi noch dor,", "tokens": ["Un", "f\u00f6f\u00b7tig", "s\u00fcnd", "v\u00f6r", "mi", "noch", "dor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "wenn recht de Kuckuck r\u00f6ppt.", "tokens": ["wenn", "recht", "de", "Ku\u00b7ckuck", "r\u00f6ppt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Un Hans, je de hett lo\u00dft sick fri", "tokens": ["Un", "Hans", ",", "je", "de", "hett", "lo\u00dft", "sick", "fri"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "ADV", "FM", "FM", "FM", "FM", "FM"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "un deent nu bi d