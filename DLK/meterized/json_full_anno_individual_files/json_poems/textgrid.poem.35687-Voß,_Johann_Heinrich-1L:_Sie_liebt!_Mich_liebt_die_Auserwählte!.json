{"textgrid.poem.35687": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sie liebt! Mich liebt die Auserw\u00e4hlte!", "genre": "verse", "period": "N.A.", "pub_year": 1774, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie liebt! Mich liebt die Auserw\u00e4hlte!", "tokens": ["Sie", "liebt", "!", "Mich", "liebt", "die", "Au\u00b7ser\u00b7w\u00e4hl\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Engel kam von ihr", "tokens": ["Ein", "En\u00b7gel", "kam", "von", "ihr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Abendlispel, und erz\u00e4hlte", "tokens": ["Im", "A\u00b7bend\u00b7lis\u00b7pel", ",", "und", "er\u00b7z\u00e4hl\u00b7te"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die leisen Seufzer mir!", "tokens": ["Die", "lei\u00b7sen", "Seuf\u00b7zer", "mir", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "F\u00fcr mich, o Selma! bebt im Stillen", "tokens": ["F\u00fcr", "mich", ",", "o", "Sel\u00b7ma", "!", "bebt", "im", "Stil\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "FM", "NE", "$.", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dein Herz voll s\u00fc\u00dfer Qual;", "tokens": ["Dein", "Herz", "voll", "s\u00fc\u00b7\u00dfer", "Qua