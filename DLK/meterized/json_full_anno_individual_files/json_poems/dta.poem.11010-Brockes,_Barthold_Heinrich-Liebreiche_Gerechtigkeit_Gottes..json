{"dta.poem.11010": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Liebreiche  \n Gerechtigkeit Gottes.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ob ihr nun von der Gottheit billig so niedrig nicht geden-\nken sollet;", "tokens": ["Ob", "ihr", "nun", "von", "der", "Got\u00b7theit", "bil\u00b7lig", "so", "nied\u00b7rig", "nicht", "ge\u00b7den", "ken", "sol\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "NN", "ADJD", "ADV", "ADJD", "PTKNEG", "TRUNC", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Wenn ihr dennoch von Gottes Wesen euch ja Begriffe machen", "tokens": ["Wenn", "ihr", "den\u00b7noch", "von", "Got\u00b7tes", "We\u00b7sen", "euch", "ja", "Be\u00b7grif\u00b7fe", "ma\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "NN", "PPER", "ADV", "NN", "VVINF"], "meter": "-+-+-+-+-++-+-+-", "measure": "unknown.measure.octa.plus"}, "line.3": {"text": "Die etwas menschlichs an sich haben: M\u00fc\u00dft ihr euch wenig-", "tokens": ["Die", "et\u00b7was", "menschlichs", "an", "sich", "ha\u00b7ben", ":", "M\u00fc\u00dft", "ihr", "euch", "we\u00b7nig"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PIS", "PIS", "APPR", "PRF", "VAFIN", "$.", "VVFIN", "PPER", "PRF", "TRUNC"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Nach einem liebreich-billigen und Gro\u00dfmuth-vollen Muster, ihn", "tokens": ["Nach", "ei\u00b7nem", "lieb\u00b7reich\u00b7bil\u00b7li\u00b7gen", "und", "Gro\u00df\u00b7muth\u00b7vol\u00b7len", "Mus\u00b7ter", ",", "ihn"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "ADJA", "KON", "ADJA", "NN", "$,", "PPER"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.5": {"text": "Jm h\u00f6chsten Grad euch vorzustellen, nicht aber unter einem Bilde,", "tokens": ["Jm", "h\u00f6chs\u00b7ten", "Grad", "euch", "vor\u00b7zu\u00b7stel\u00b7len", ",", "nicht", "a\u00b7ber", "un\u00b7ter", "ei\u00b7nem", "Bil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPER", "VVIZU", "$,", "PTKNEG", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.6": {"text": "Von einem, der von Ernst und Streng, und von Gerecht den", "tokens": ["Von", "ei\u00b7nem", ",", "der", "von", "Ernst", "und", "Streng", ",", "und", "von", "Ge\u00b7recht", "den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "$,", "PRELS", "APPR", "NE", "KON", "NE", "$,", "KON", "APPR", "NN", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der aber bitter, unvers\u00f6hnlich und unbarmherzig in der That.", "tokens": ["Der", "a\u00b7ber", "bit\u00b7ter", ",", "un\u00b7ver\u00b7s\u00f6hn\u00b7lich", "und", "un\u00b7barm\u00b7her\u00b7zig", "in", "der", "That", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "$,", "ADJD", "KON", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.8": {"text": "Sprich nicht: Es stellt ja Moses Gott sehr ernsthaft und", "tokens": ["Sprich", "nicht", ":", "Es", "stellt", "ja", "Mo\u00b7ses", "Gott", "sehr", "ernst\u00b7haft", "und"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PTKNEG", "$.", "PPER", "VVFIN", "ADV", "NE", "NN", "ADV", "ADJD", "KON"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wie, neben so viel andren Stellen, sein Fluch absonderlich uns", "tokens": ["Wie", ",", "ne\u00b7ben", "so", "viel", "an\u00b7dren", "Stel\u00b7len", ",", "sein", "Fluch", "ab\u00b7son\u00b7der\u00b7lich", "uns"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "$,", "APPR", "ADV", "PIAT", "ADJA", "NN", "$,", "PPOSAT", "NN", "ADJD", "PPER"], "meter": "----+--+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Denn erstlich zeigt sich von sich selbst, da\u00df mehr, als anderwer-", "tokens": ["Denn", "erst\u00b7lich", "zeigt", "sich", "von", "sich", "selbst", ",", "da\u00df", "mehr", ",", "als", "an\u00b7der\u00b7wer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PRF", "APPR", "PRF", "ADV", "$,", "KOUS", "PIS", "$,", "KOUS", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.11": {"text": "Die Herzensh\u00e4rtigkeit der Juden zu einer Ursach hergeh\u00f6ret.", "tokens": ["Die", "Her\u00b7zens\u00b7h\u00e4r\u00b7tig\u00b7keit", "der", "Ju\u00b7den", "zu", "ei\u00b7ner", "Ur\u00b7sach", "her\u00b7ge\u00b7h\u00f6\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.12": {"text": "Zum andern zeigt uns oftermal", "tokens": ["Zum", "an\u00b7dern", "zeigt", "uns", "of\u00b7ter\u00b7mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Von Gottes gro\u00dfer Lieb und G\u00fcte uns einen hellen Gnadenstral,", "tokens": ["Von", "Got\u00b7tes", "gro\u00b7\u00dfer", "Lieb", "und", "G\u00fc\u00b7te", "uns", "ei\u00b7nen", "hel\u00b7len", "Gna\u00b7den\u00b7stral", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "KON", "NN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.14": {"text": "Und da\u00df sein Lieben, seine Langmuth, so Straf als Zorn weit", "tokens": ["Und", "da\u00df", "sein", "Lie\u00b7ben", ",", "sei\u00b7ne", "Lang\u00b7muth", ",", "so", "Straf", "als", "Zorn", "weit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPOSAT", "ADJA", "$,", "PPOSAT", "NN", "$,", "ADV", "NN", "KOUS", "NN", "ADJD"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Wenn er diejenigen Verbrecher, die irgend wider sein Geboth", "tokens": ["Wenn", "er", "die\u00b7je\u00b7ni\u00b7gen", "Ver\u00b7bre\u00b7cher", ",", "die", "ir\u00b7gend", "wi\u00b7der", "sein", "Ge\u00b7both"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PDAT", "NN", "$,", "PRELS", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.16": {"text": "Sich je vergangen u. vers\u00fcndigt, ins vierte Glied zu strafen droht,", "tokens": ["Sich", "je", "ver\u00b7gan\u00b7gen", "u.", "ver\u00b7s\u00fcn\u00b7digt", ",", "ins", "vier\u00b7te", "Glied", "zu", "stra\u00b7fen", "droht", ","], "token_info": ["word", "word", "word", "abbreviation", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVPP", "KON", "VVPP", "$,", "APPRART", "ADJA", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+--+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.17": {"text": "Dem aber, der sie h\u00e4lt, verspricht, zu segnen bis ins tausende.", "tokens": ["Dem", "a\u00b7ber", ",", "der", "sie", "h\u00e4lt", ",", "ver\u00b7spricht", ",", "zu", "seg\u00b7nen", "bis", "ins", "tau\u00b7sen\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "$,", "PTKZU", "VVINF", "KON", "APPRART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-+--", "measure": "unknown.measure.septa"}, "line.18": {"text": "Zum dritten mu\u00df ich dir von Mose die klare Wahrheit hier", "tokens": ["Zum", "drit\u00b7ten", "mu\u00df", "ich", "dir", "von", "Mo\u00b7se", "die", "kla\u00b7re", "Wahr\u00b7heit", "hier"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "VMFIN", "PPER", "PPER", "APPR", "NE", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.19": {"text": "Da\u00df alle Fl\u00fcch aufs Jrdische, aufs Ewige sich nicht erstrecken.", "tokens": ["Da\u00df", "al\u00b7le", "Fl\u00fcch", "aufs", "Jr\u00b7di\u00b7sche", ",", "aufs", "E\u00b7wi\u00b7ge", "sich", "nicht", "er\u00b7stre\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "APPRART", "NN", "$,", "APPRART", "ADJA", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}}}}}