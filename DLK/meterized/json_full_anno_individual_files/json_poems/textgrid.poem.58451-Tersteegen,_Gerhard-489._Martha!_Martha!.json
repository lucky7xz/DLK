{"textgrid.poem.58451": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "489. Martha! Martha!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach Martha, la\u00df dein Sorgen doch", "tokens": ["Ach", "Mar\u00b7tha", ",", "la\u00df", "dein", "Sor\u00b7gen", "doch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NE", "$,", "VVIMP", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sei nicht so gesch\u00e4ftig!", "tokens": ["Und", "sei", "nicht", "so", "ge\u00b7sch\u00e4f\u00b7tig", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dein Vormund, Jesus, lebet noch", "tokens": ["Dein", "Vor\u00b7mund", ",", "Je\u00b7sus", ",", "le\u00b7bet", "noch"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "NE", "$,", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So gut, so weis' und kr\u00e4ftig;", "tokens": ["So", "gut", ",", "so", "weis'", "und", "kr\u00e4f\u00b7tig", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "La\u00df ihn mit deinen Sachen walten,", "tokens": ["La\u00df", "ihn", "mit", "dei\u00b7nen", "Sa\u00b7chen", "wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dein Sorgen sei, ihm stillehalten!", "tokens": ["Dein", "Sor\u00