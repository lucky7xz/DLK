{"dta.poem.19351": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "84.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wennauch von Rache nicht das Recht ist so benannt,", "tokens": ["Wen\u00b7nauch", "von", "Ra\u00b7che", "nicht", "das", "Recht", "ist", "so", "be\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "PTKNEG", "ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch von der Seite sind die Wurzeln anverwandt.", "tokens": ["Doch", "von", "der", "Sei\u00b7te", "sind", "die", "Wur\u00b7zeln", "an\u00b7ver\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Tritst du aus deines Rechts in meines Rechtes Kreis,", "tokens": ["Tritst", "du", "aus", "dei\u00b7nes", "Rechts", "in", "mei\u00b7nes", "Rech\u00b7tes", "Kreis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So ist mein Widerstand des Uebertretens Preis.", "tokens": ["So", "ist", "mein", "Wi\u00b7der\u00b7stand", "des", "Ue\u00b7bert\u00b7re\u00b7tens", "Preis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch, thatst du Unrecht mir, darf ich dir's wieder thun?", "tokens": ["Doch", ",", "thatst", "du", "Un\u00b7recht", "mir", ",", "darf", "ich", "dir's", "wie\u00b7der", "thun", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPER", "NN", "PPER", "$,", "VMFIN", "PPER", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dann thust du's wieder mir, und wo wird's endlich ruhn?", "tokens": ["Dann", "thust", "du's", "wie\u00b7der", "mir", ",", "und", "wo", "wird's", "end\u00b7lich", "ruhn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "PPER", "$,", "KON", "PWAV", "VAFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die Rach' ist schrankenlos, das Recht ist nur in Schranken;", "tokens": ["Die", "Rach'", "ist", "schran\u00b7ken\u00b7los", ",", "das", "Recht", "ist", "nur", "in", "Schran\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Darum beschr\u00e4nke dich in Wort, Werk und Gedanken.", "tokens": ["Da\u00b7rum", "be\u00b7schr\u00e4n\u00b7ke", "dich", "in", "Wort", ",", "Werk", "und", "Ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PRF", "APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Beschr\u00e4nke dich, damit du seiest unbeschr\u00e4nkt,", "tokens": ["Be\u00b7schr\u00e4n\u00b7ke", "dich", ",", "da\u00b7mit", "du", "sei\u00b7est", "un\u00b7be\u00b7schr\u00e4nkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und kr\u00e4nk nicht innen dich, wenn man dich au\u00dfen kr\u00e4nkt.", "tokens": ["Und", "kr\u00e4nk", "nicht", "in\u00b7nen", "dich", ",", "wenn", "man", "dich", "au\u00b7\u00dfen", "kr\u00e4nkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "PPER", "$,", "KOUS", "PIS", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Dein ist dein Recht, doch dein ist nicht Gericht und Rache;", "tokens": ["Dein", "ist", "dein", "Recht", ",", "doch", "dein", "ist", "nicht", "Ge\u00b7richt", "und", "Ra\u00b7che", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "PPOSAT", "NN", "$,", "ADV", "PPOSAT", "VAFIN", "PTKNEG", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein allgemeines Recht vertritt die Einzelsache.", "tokens": ["Ein", "all\u00b7ge\u00b7mei\u00b7nes", "Recht", "ver\u00b7tritt", "die", "Ein\u00b7zel\u00b7sa\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Wei\u00dft du dein Thun gerecht, und andres ungerecht,", "tokens": ["Wei\u00dft", "du", "dein", "Thun", "ge\u00b7recht", ",", "und", "and\u00b7res", "un\u00b7ge\u00b7recht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "ADJD", "$,", "KON", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So la\u00df die Rache dem, der nichts l\u00e4\u00dft unger\u00e4cht.", "tokens": ["So", "la\u00df", "die", "Ra\u00b7che", "dem", ",", "der", "nichts", "l\u00e4\u00dft", "un\u00b7ge\u00b7r\u00e4cht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ART", "NN", "ART", "$,", "PRELS", "PIS", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}