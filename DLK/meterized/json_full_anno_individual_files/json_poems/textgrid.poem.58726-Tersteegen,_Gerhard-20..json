{"textgrid.poem.58726": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "20.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fanget uns die kleinen F\u00fcchse, die die Weinberge verderben! Hoheslied 2, 15", "tokens": ["Fan\u00b7get", "uns", "die", "klei\u00b7nen", "F\u00fcch\u00b7se", ",", "die", "die", "Wein\u00b7ber\u00b7ge", "ver\u00b7der\u00b7ben", "!", "Ho\u00b7hes\u00b7lied", "2", ",", "15"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "number", "punct", "number"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$.", "NN", "CARD", "$,", "CARD"], "meter": "+-+-+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Komm, Gottesliebe, ganz aus meiner Seel' zu jagen", "tokens": ["Komm", ",", "Got\u00b7tes\u00b7lie\u00b7be", ",", "ganz", "aus", "mei\u00b7ner", "Seel'", "zu", "ja\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "NN", "$,", "ADV", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Eigenheiten all', die mich verst\u00f6r'n und plagen,", "tokens": ["Die", "Ei\u00b7gen\u00b7hei\u00b7ten", "all'", ",", "die", "mich", "ver\u00b7st\u00f6r'n", "und", "pla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "$,", "PRELS", "PPER", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mich deine Wahrheit unterweis',", "tokens": ["Mich", "dei\u00b7ne", "Wahr\u00b7heit", "un\u00b7ter\u00b7weis'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein s\u00fc\u00dfes Feu'r in mich eingeu\u00df!", "tokens": ["Dein", "s\u00fc\u00b7\u00dfes", "Feu'r", "in", "mich", "ein\u00b7geu\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich war der Weinberg selbst, so bl\u00fchend anzuschauen,", "tokens": ["Ich", "war", "der", "Wein\u00b7berg", "selbst", ",", "so", "bl\u00fc\u00b7hend", "an\u00b7zu\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "$,", "ADV", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du wolltest Tag und Nacht ihn pflegen und ihn bauen;", "tokens": ["Du", "woll\u00b7test", "Tag", "und", "Nacht", "ihn", "pfle\u00b7gen", "und", "ihn", "bau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "KON", "NN", "PPER", "VVINF", "KON", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Feind lie\u00df leider F\u00fcchse drein,", "tokens": ["Der", "Feind", "lie\u00df", "lei\u00b7der", "F\u00fcch\u00b7se", "drein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die dir, o Lieb', zum Ansto\u00df sein.", "tokens": ["Die", "dir", ",", "o", "Lieb'", ",", "zum", "An\u00b7sto\u00df", "sein", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "FM", "NN", "$,", "APPRART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wenn F\u00fcchse hin und her gehn, w\u00fchlen in der Erden,", "tokens": ["Wenn", "F\u00fcch\u00b7se", "hin", "und", "her", "gehn", ",", "w\u00fch\u00b7len", "in", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PTKVZ", "KON", "ADV", "VVINF", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die sch\u00f6nsten Pflanzen gar verst\u00f6rt und schmutzig werden;", "tokens": ["Die", "sch\u00f6ns\u00b7ten", "Pflan\u00b7zen", "gar", "ver\u00b7st\u00f6rt", "und", "schmut\u00b7zig", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVPP", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Eigenheit bekriegt die Lieb'", "tokens": ["Die", "Ei\u00b7gen\u00b7heit", "be\u00b7kriegt", "die", "Lieb'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ist der Liebe Kinderdieb.", "tokens": ["Und", "ist", "der", "Lie\u00b7be", "Kin\u00b7der\u00b7dieb", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Rott aus dies b\u00f6s' Geschlecht, mein Herr, das so entgegen", "tokens": ["Rott", "aus", "dies", "b\u00f6s'", "Ge\u00b7schlecht", ",", "mein", "Herr", ",", "das", "so", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos"