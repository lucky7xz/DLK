{"dta.poem.23563": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Herrnhuter in beiderlei Gestalt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nie wollt ihr des Herrn vergessen,", "tokens": ["Nie", "wollt", "ihr", "des", "Herrn", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Nicht beim Trinken noch beim Essen,", "tokens": ["Nicht", "beim", "Trin\u00b7ken", "noch", "beim", "Es\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ihr tunkt in rothen Wein", "tokens": ["Und", "ihr", "tunkt", "in", "ro\u00b7then", "Wein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ein biscuiten L\u00e4mmlein ein.", "tokens": ["Ein", "bi\u00b7scui\u00b7ten", "L\u00e4mm\u00b7lein", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "So erf\u00fcllt ihr Gottes Willen", "tokens": ["So", "er\u00b7f\u00fcllt", "ihr", "Got\u00b7tes", "Wil\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Im Geheimen und im Stillen,", "tokens": ["Im", "Ge\u00b7hei\u00b7men", "und", "im", "Stil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es isst auf Christi Tod", "tokens": ["Und", "es", "isst", "auf", "Chris\u00b7ti", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "APPR", "NE", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Euer Nachbar trocken Brot.", "tokens": ["Eu\u00b7er", "Nach\u00b7bar", "tro\u00b7cken", "Brot", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}