{"dta.poem.13560": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Sonnette .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Das Leben hat zum Ritter ihn geschlagen;", "tokens": ["Das", "Le\u00b7ben", "hat", "zum", "Rit\u00b7ter", "ihn", "ge\u00b7schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er soll der Sch\u00f6nheit neid'sche Kerker lichten,", "tokens": ["Er", "soll", "der", "Sch\u00f6n\u00b7heit", "nei\u00b7d'\u00b7sche", "Ker\u00b7ker", "lich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADJA", "NN", "ADJA", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da\u00df nicht sich alle g\u00f6tterlos vernichten,", "tokens": ["Da\u00df", "nicht", "sich", "al\u00b7le", "g\u00f6t\u00b7ter\u00b7los", "ver\u00b7nich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "PRF", "PIS", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Soll er die G\u00f6tter zu beschw\u00f6ren wagen.", "tokens": ["Soll", "er", "die", "G\u00f6t\u00b7ter", "zu", "be\u00b7schw\u00f6\u00b7ren", "wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}