{"dta.poem.10100": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Geranium.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die verschiedlich rothe Gluth, das Rubinen-gleiche\nfunckeln", "tokens": ["Die", "ver\u00b7schied\u00b7lich", "ro\u00b7the", "Gluth", ",", "das", "Ru\u00b7bi\u00b7nen\u00b7glei\u00b7che", "fun\u00b7ckeln"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,", "ART", "NN", "VVINF"], "meter": "+-+-+-+----+-+-", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Von den Tulpen, von den Rosen, Malva, Nelcken, Mah,", "tokens": ["Von", "den", "Tul\u00b7pen", ",", "von", "den", "Ro\u00b7sen", ",", "Mal\u00b7va", ",", "Nel\u00b7cken", ",", "Mah", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,", "NE", "$,", "NN", "$,", "NE", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "Hab\u2019 ich mit vergn\u00fcgter Seelen, offt erstaunet angeblickt,", "tokens": ["Hab'", "ich", "mit", "ver\u00b7gn\u00fcg\u00b7ter", "See\u00b7len", ",", "offt", "er\u00b7stau\u00b7net", "an\u00b7ge\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN", "$,", "ADV", "VVFIN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Offt zu GOTTES Ruhm betrachtet, offt Den, Der sie so", "tokens": ["Offt", "zu", "GoT\u00b7TES", "Ruhm", "be\u00b7trach\u00b7tet", ",", "offt", "Den", ",", "Der", "sie", "so"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "NE", "NN", "VVPP", "$,", "ADV", "NE", "$,", "ART", "PPER", "ADV"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.5": {"text": "Inniglich ger\u00fchrt besungen. Jetzt erblick ich abermahl", "tokens": ["In\u00b7nig\u00b7lich", "ge\u00b7r\u00fchrt", "be\u00b7sun\u00b7gen", ".", "Jetzt", "er\u00b7blick", "ich", "a\u00b7ber\u00b7mahl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "VVINF", "$.", "ADV", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Eine neue rothe Sch\u00f6nheit, einen Flammen-reichen Strahl,", "tokens": ["Ei\u00b7ne", "neu\u00b7e", "ro\u00b7the", "Sch\u00f6n\u00b7heit", ",", "ei\u00b7nen", "Flam\u00b7men\u00b7rei\u00b7chen", "Strahl", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Der mir, wie ein schneller Blitz, durch das Aug\u2019 ins Her-", "tokens": ["Der", "mir", ",", "wie", "ein", "schnel\u00b7ler", "Blitz", ",", "durch", "das", "Aug'", "ins", "Her"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "$,", "PWAV", "ART", "ADJA", "NN", "$,", "APPR", "ART", "NN", "APPRART", "TRUNC"], "meter": "----+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Meiner Andacht Gluth entz\u00fcndet, mich recht inniglich ver-", "tokens": ["Mei\u00b7ner", "An\u00b7dacht", "Gluth", "ent\u00b7z\u00fcn\u00b7det", ",", "mich", "recht", "in\u00b7nig\u00b7lich", "ver"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "VVPP", "$,", "PPER", "ADV", "ADJD", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.9": {"text": "Ja f\u00fcr Lust mich fast verwirret. F\u00fcr Verwundrung ward", "tokens": ["Ja", "f\u00fcr", "Lust", "mich", "fast", "ver\u00b7wir\u00b7ret", ".", "F\u00fcr", "Ver\u00b7wund\u00b7rung", "ward"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "APPR", "NN", "PPER", "ADV", "VVFIN", "$.", "APPR", "NN", "VAFIN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.10": {"text": "Als ich j\u00fcngst dein Feur erblickte, funckelndes ", "tokens": ["Als", "ich", "j\u00fcngst", "dein", "Feur", "er\u00b7blick\u00b7te", ",", "fun\u00b7ckeln\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$,", "ADJA"], "meter": "+-+-+-+-+--", "measure": "unknown.measure.penta"}}, "stanza.2": {"line.1": {"text": "Deine lieblich helle R\u00f6the, ob sie gleich ein wenig bla\u00df,", "tokens": ["Dei\u00b7ne", "lieb\u00b7lich", "hel\u00b7le", "R\u00f6\u00b7the", ",", "ob", "sie", "gleich", "ein", "we\u00b7nig", "bla\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "ADJA", "NN", "$,", "KOUS", "PPER", "ADV", "ART", "PIS", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Ist dennoch von solcher Krafft, da\u00df sie dem Rubin-Bala\u00df", "tokens": ["Ist", "den\u00b7noch", "von", "sol\u00b7cher", "Krafft", ",", "da\u00df", "sie", "dem", "Ru\u00b7bin\u00b7Ba\u00b7la\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "PIAT", "NN", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "+---+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.3": {"text": "Nicht nur gleicht; ihn \u00fcbertrifft. Wie es l\u00e4sst, so strahlt,", "tokens": ["Nicht", "nur", "gleicht", ";", "ihn", "\u00fc\u00b7bert\u00b7rifft", ".", "Wie", "es", "l\u00e4sst", ",", "so", "strahlt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVFIN", "$.", "PPER", "VVFIN", "$.", "PWAV", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "$,"], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Aus desselben glatten Bl\u00e4ttern keine Farb\u2019, ein r\u00f6thlich", "tokens": ["Aus", "des\u00b7sel\u00b7ben", "glat\u00b7ten", "Bl\u00e4t\u00b7tern", "kei\u00b7ne", "Fa\u00b7rb'", ",", "ein", "r\u00f6th\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "PIAT", "NN", "$,", "ART", "ADJD"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Und dringt durch das Aug\u2019 ins Hertze, weckt den einge-", "tokens": ["Und", "dringt", "durch", "das", "Aug'", "ins", "Hert\u00b7ze", ",", "weckt", "den", "ein\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "APPRART", "VVFIN", "$,", "VVFIN", "ART", "TRUNC"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Vom Gewohnheits-Schlummer auf, facht der Andacht Flam-", "tokens": ["Vom", "Ge\u00b7wohn\u00b7heits\u00b7Schlum\u00b7mer", "auf", ",", "facht", "der", "An\u00b7dacht", "Flam"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PTKVZ", "$,", "VVFIN", "ART", "NN", "TRUNC"], "meter": "--+-+--+-+--", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Lenckt der Seelen rege Krafft, durch die Lust, zum Sch\u00f6pfer", "tokens": ["Lenckt", "der", "See\u00b7len", "re\u00b7ge", "Krafft", ",", "durch", "die", "Lust", ",", "zum", "Sch\u00f6p\u00b7fer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$,", "APPR", "ART", "NN", "$,", "APPRART", "NN"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "Dessen Allmacht, Weisheit, Liebe man am besten ehren kann,", "tokens": ["Des\u00b7sen", "All\u00b7macht", ",", "Weis\u00b7heit", ",", "Lie\u00b7be", "man", "am", "bes\u00b7ten", "eh\u00b7ren", "kann", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "NN", "$,", "NN", "PIS", "PTKA", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Durch Empfindlichkeit der Wunder, die Sein\u2019 Allmacht,", "tokens": ["Durch", "Emp\u00b7find\u00b7lich\u00b7keit", "der", "Wun\u00b7der", ",", "die", "Sein'", "All\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "Aus der tieffen Nacht des Nichts, voller Sch\u00f6nheit, Glantz", "tokens": ["Aus", "der", "tief\u00b7fen", "Nacht", "des", "Nichts", ",", "vol\u00b7ler", "Sch\u00f6n\u00b7heit", ",", "Glantz"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$,", "ADJA", "NN", "$,", "NN"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Voller Nutzen, voller Krafft, durch ein Wort hervor ge-", "tokens": ["Vol\u00b7ler", "Nut\u00b7zen", ",", "vol\u00b7ler", "Krafft", ",", "durch", "ein", "Wort", "her\u00b7vor", "ge"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,", "APPR", "ART", "NN", "PTKVZ", "TRUNC"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Ein, durch eine solche Lust, Jhm zum Ruhm, ger\u00fchrt", "tokens": ["Ein", ",", "durch", "ei\u00b7ne", "sol\u00b7che", "Lust", ",", "Jhm", "zum", "Ruhm", ",", "ge\u00b7r\u00fchrt"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "$,", "APPR", "ART", "PIAT", "NN", "$,", "PPER", "APPRART", "NN", "$,", "VVPP"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Ist dem Sch\u00f6pfer angenehm, dar\u00fcm lasset uns allhier", "tokens": ["Ist", "dem", "Sch\u00f6p\u00b7fer", "an\u00b7ge\u00b7nehm", ",", "da\u00b7r\u00fcm", "las\u00b7set", "uns", "all\u00b7hier"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ADJD", "$,", "PAV", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+---+--+", "measure": "iambic.hexa.chol"}, "line.3": {"text": "Etwas n\u00e4her noch betrachten dieser Bluhmen Schmuck und", "tokens": ["Et\u00b7was", "n\u00e4\u00b7her", "noch", "be\u00b7trach\u00b7ten", "die\u00b7ser", "Bluh\u00b7men", "Schmuck", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADV", "VVFIN", "PDAT", "NN", "NN", "KON"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}, "stanza.4": {"line.1": {"text": "Wenn wir dieser Bluhmen Stengel, woran hundert", "tokens": ["Wenn", "wir", "die\u00b7ser", "Bluh\u00b7men", "Sten\u00b7gel", ",", "wo\u00b7ran", "hun\u00b7dert"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "PDAT", "NN", "NN", "$,", "PWAV", "CARD"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Mit Betrachtungs-vollen Blicken, wie wir billig sollten", "tokens": ["Mit", "Be\u00b7trach\u00b7tungs\u00b7vol\u00b7len", "Bli\u00b7cken", ",", "wie", "wir", "bil\u00b7lig", "soll\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "NN", "$,", "PWAV", "PPER", "ADJD", "VMFIN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.3": {"text": "Und von unten mit den Augen allgem\u00e4hlig aufw\u00e4rts gehn;", "tokens": ["Und", "von", "un\u00b7ten", "mit", "den", "Au\u00b7gen", "all\u00b7ge\u00b7m\u00e4h\u00b7lig", "auf\u00b7w\u00e4rts", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADV", "APPR", "ART", "NN", "ADJD", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Finden wir, da\u00df alle Stengel, wie die Stengel insgemein,", "tokens": ["Fin\u00b7den", "wir", ",", "da\u00df", "al\u00b7le", "Sten\u00b7gel", ",", "wie", "die", "Sten\u00b7gel", "ins\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KOUS", "PIAT", "NN", "$,", "PWAV", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Allgemach sich auch verd\u00fcnnen. Und hier\u00fcber fiel mir ein:", "tokens": ["All\u00b7ge\u00b7mach", "sich", "auch", "ver\u00b7d\u00fcn\u00b7nen", ".", "Und", "hier\u00b7\u00fc\u00b7ber", "fiel", "mir", "ein", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "ADV", "VVINF", "$.", "KON", "PAV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Ach! wie unbegreifflich sind, zu bewundernde Natur,", "tokens": ["Ach", "!", "wie", "un\u00b7be\u00b7greif\u00b7flich", "sind", ",", "zu", "be\u00b7wun\u00b7dern\u00b7de", "Na\u00b7tur", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "ADJD", "VAFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Deine Wercke! wer kann fassen, wer begreiffet, wie behende", "tokens": ["Dei\u00b7ne", "Wer\u00b7cke", "!", "wer", "kann", "fas\u00b7sen", ",", "wer", "be\u00b7greif\u00b7fet", ",", "wie", "be\u00b7hen\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "PWS", "VMFIN", "VVINF", "$,", "PWS", "VVFIN", "$,", "PWAV", "ADJA"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Dein Gewerbe, deine Wege? Wo des Stengels Gr\u00fcn zu", "tokens": ["Dein", "Ge\u00b7wer\u00b7be", ",", "dei\u00b7ne", "We\u00b7ge", "?", "Wo", "des", "Sten\u00b7gels", "Gr\u00fcn", "zu"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$.", "PWAV", "ART", "NN", "NN", "PTKZU"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.9": {"text": "Zeigt sich gleich ein feurig Roth. Wer facht diese rothe", "tokens": ["Zeigt", "sich", "gleich", "ein", "feu\u00b7rig", "Roth", ".", "Wer", "facht", "die\u00b7se", "ro\u00b7the"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADV", "ART", "ADJD", "NN", "$.", "PWS", "VVFIN", "PDAT", "ADJA"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Eben an dem Ort doch an? Welcher Finger, welche Hand,", "tokens": ["E\u00b7ben", "an", "dem", "Ort", "doch", "an", "?", "Wel\u00b7cher", "Fin\u00b7ger", ",", "wel\u00b7che", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "PTKVZ", "$.", "PWAT", "NN", "$,", "PWAT", "NN", "$,"], "meter": "+---+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Welcher Pinsel, welche Kunst, welch erfindender Verstand", "tokens": ["Wel\u00b7cher", "Pin\u00b7sel", ",", "wel\u00b7che", "Kunst", ",", "welch", "er\u00b7fin\u00b7den\u00b7der", "Ver\u00b7stand"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAT", "NN", "$,", "PWAT", "NN", "$,", "PWAT", "ADJA", "NN"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.12": {"text": "F\u00e4rbt die Stelle sonder Farben? Hier erstaunen unsre", "tokens": ["F\u00e4rbt", "die", "Stel\u00b7le", "son\u00b7der", "Far\u00b7ben", "?", "Hier", "er\u00b7stau\u00b7nen", "uns\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "KON", "NN", "$.", "ADV", "VVFIN", "PPOSAT"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.13": {"text": "Und, weil mans nicht fassen kann,", "tokens": ["Und", ",", "weil", "mans", "nicht", "fas\u00b7sen", "kann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PIS", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Beten sie mit Lust und Ehrfurcht, und mit Andacht billig an", "tokens": ["Be\u00b7ten", "sie", "mit", "Lust", "und", "Ehr\u00b7furcht", ",", "und", "mit", "An\u00b7dacht", "bil\u00b7lig", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$,", "KON", "APPR", "NN", "ADJD", "PTKVZ"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.15": {"text": "Den Allm\u00e4chtigen, in Wercken blo\u00df allein, erblickten Meister,", "tokens": ["Den", "All\u00b7m\u00e4ch\u00b7ti\u00b7gen", ",", "in", "Wer\u00b7cken", "blo\u