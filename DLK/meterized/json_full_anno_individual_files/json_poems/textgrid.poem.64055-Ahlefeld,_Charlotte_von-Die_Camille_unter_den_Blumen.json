{"textgrid.poem.64055": {"metadata": {"author": {"name": "Ahlefeld, Charlotte von", "birth": "N.A.", "death": "N.A."}, "title": "Die Camille unter den Blumen", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bleiche Camille, Du bl\u00fchst verachtet im einsamen Felde,", "tokens": ["Blei\u00b7che", "Ca\u00b7mil\u00b7le", ",", "Du", "bl\u00fchst", "ver\u00b7ach\u00b7tet", "im", "ein\u00b7sa\u00b7men", "Fel\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+-+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Denn Dein simples Gewand reizet die L\u00fcsternen nicht.", "tokens": ["Denn", "Dein", "sim\u00b7ples", "Ge\u00b7wand", "rei\u00b7zet", "die", "L\u00fcs\u00b7ter\u00b7nen", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Farblos stehst Du verlassen im s\u00e4uselnden Hauche des Windes \u2013", "tokens": ["Farb\u00b7los", "stehst", "Du", "ver\u00b7las\u00b7sen", "im", "s\u00e4u\u00b7seln\u00b7den", "Hau\u00b7che", "des", "Win\u00b7des", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "ART", "NN", "$("], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Nimmer w\u00e4hlt' Dich zum Kranz jugendlich fr\u00f6hliche Lust.", "tokens": ["Nim\u00b7mer", "w\u00e4hlt'", "Dich", "zum", "Kranz", "ju\u00b7gend\u00b7lich", "fr\u00f6h\u00b7li\u00b7che", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "+--+-+---+--+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Lass denn den schw\u00e4rmenden Tross die bunteren Blumen erjagen,", "tokens": ["Lass", "denn", "den", "schw\u00e4r\u00b7men\u00b7den", "Tross", "die", "bun\u00b7te\u00b7ren", "Blu\u00b7men", "er\u00b7ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Kr\u00e4nke, Bescheidne! Dich nicht, dass Dich die Freude verschm\u00e4ht.", "tokens": ["Kr\u00e4n\u00b7ke", ",", "Be\u00b7scheid\u00b7ne", "!", "Dich", "nicht", ",", "dass", "Dich", "die", "Freu\u00b7de", "ver\u00b7schm\u00e4ht", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$.", "PPER", "PTKNEG", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+--+-+--+-+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Bl\u00fche einsam nur fort am gr\u00fcnen Rande des Weges,", "tokens": ["Bl\u00fc\u00b7he", "ein\u00b7sam", "nur", "fort", "am", "gr\u00fc\u00b7nen", "Ran\u00b7de", "des", "We\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "PTKVZ", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Bricht auch die Liebe Dich nicht \u2013 findet das Leiden Dich doch.", "tokens": ["Bricht", "auch", "die", "Lie\u00b7be", "Dich", "nicht", "\u2013", "fin\u00b7det", "das", "Lei\u00b7den", "Dich", "doch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PPER", "PTKNEG", "$(", "VVFIN", "ART", "NN", "PPER", "ADV", "$."], "meter": "+--+-+-+--+-+-", "measure": "iambic.hexa.invert"}}, "stanza.3": {"line.1": {"text": "Heilsam wirkende Kr\u00e4fte hat die Natur Dir in Busen", "tokens": ["Heil\u00b7sam", "wir\u00b7ken\u00b7de", "Kr\u00e4f\u00b7te", "hat", "die", "Na\u00b7tur", "Dir", "in", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "VAFIN", "ART", "NN", "PPER", "APPR", "NN"], "meter": "+-+--+-+---+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "M\u00fctterlich sorgsam gelegt, und sie verl\u00e4ugnen sich nicht.", "tokens": ["M\u00fct\u00b7ter\u00b7lich", "sorg\u00b7sam", "ge\u00b7legt", ",", "und", "sie", "ver\u00b7l\u00e4ug\u00b7nen", "sich", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VVPP", "$,", "KON", "PPER", "VVFIN", "PRF", "PTKNEG", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Lindernd stillest Du Schmerzen, wenn andre das Auge erg\u00f6tzen,", "tokens": ["Lin\u00b7dernd", "stil\u00b7lest", "Du", "Schmer\u00b7zen", ",", "wenn", "and\u00b7re", "das", "Au\u00b7ge", "er\u00b7g\u00f6t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "NN", "$,", "KOUS", "PIS", "ART", "NN", "VVINF", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Und im einfachen Schmuck birgt sich Dein hohes Verdienst.", "tokens": ["Und", "im", "ein\u00b7fa\u00b7chen", "Schmuck", "birgt", "sich", "Dein", "ho\u00b7hes", "Ver\u00b7dienst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "VVFIN", "PRF", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}, "stanza.4": {"line.1": {"text": "Welkend wirft man sie weg, der Flora schimmernde Kinder,", "tokens": ["Wel\u00b7kend", "wirft", "man", "sie", "weg", ",", "der", "Flo\u00b7ra", "schim\u00b7mern\u00b7de", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "PPER", "PTKVZ", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Ihr Beruf ist erf\u00fcllt, haben sie l\u00e4chelnd gebl\u00fcht \u2013", "tokens": ["Ihr", "Be\u00b7ruf", "ist", "er\u00b7f\u00fcllt", ",", "ha\u00b7ben", "sie", "l\u00e4\u00b7chelnd", "ge\u00b7bl\u00fcht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,", "VAFIN", "PPER", "ADJD", "VVPP", "$("], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und es kehret ihr Staub vergessen zum Schoosse der Erde", "tokens": ["Und", "es", "keh\u00b7ret", "ihr", "Staub", "ver\u00b7ges\u00b7sen", "zum", "Schoos\u00b7se", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "VVPP", "APPRART", "NN", "ART", "NN"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Spurlos wieder zur\u00fcck, gleich einem Schatten der Nacht.", "tokens": ["Spur\u00b7los", "wie\u00b7der", "zu\u00b7r\u00fcck", ",", "gleich", "ei\u00b7nem", "Schat\u00b7ten", "der", "Nacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKVZ", "$,", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Schweifet mein Blick umher auf den bunten Kreisen des Lebens,", "tokens": ["Schwei\u00b7fet", "mein", "Blick", "um\u00b7her", "auf", "den", "bun\u00b7ten", "Krei\u00b7sen", "des", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "D\u00fcnkst Du ein Sinnbild zu seyn mir im Get\u00f6se der Welt.", "tokens": ["D\u00fcnkst", "Du", "ein", "Sinn\u00b7bild", "zu", "seyn", "mir", "im", "Ge\u00b7t\u00f6\u00b7se", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKZU", "VAINF", "PPER", "APPRART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Ach, geschieden ist oft ", "tokens": ["Ach", ",", "ge\u00b7schie\u00b7den", "ist", "oft"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "VVPP", "VAFIN", "ADV"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Und der Schimmer h\u00e4lt nicht, was er so schmeichelnd verspricht.", "tokens": ["Und", "der", "Schim\u00b7mer", "h\u00e4lt", "nicht", ",", "was", "er", "so", "schmei\u00b7chelnd", "ver\u00b7spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "M\u00f6ge der eitele Sinn am Farbenspiel sich vergn\u00fcgen,", "tokens": ["M\u00f6\u00b7ge", "der", "ei\u00b7te\u00b7le", "Sinn", "am", "Far\u00b7ben\u00b7spiel", "sich", "ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "APPRART", "NN", "PRF", "VVPP", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "W\u00e4hrend er Dich \u00fcbersieht \u2013 stolz vor\u00fcber Dir geht.", "tokens": ["W\u00e4h\u00b7rend", "er", "Dich", "\u00fc\u00b7ber\u00b7sieht", "\u2013", "stolz", "vor\u00b7\u00fc\u00b7ber", "Dir", "geht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$(", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.3": {"text": "Mit dem Gef\u00fchle der Kraft und der stillen Tugend im Innern,", "tokens": ["Mit", "dem", "Ge\u00b7f\u00fch\u00b7le", "der", "Kraft", "und", "der", "stil\u00b7len", "Tu\u00b7gend", "im", "In\u00b7nern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "KON", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Schmerzet es wenig, verkannt und verachtet zu seyn.", "tokens": ["Schmer\u00b7zet", "es", "we\u00b7nig", ",", "ver\u00b7kannt", "und", "ver\u00b7ach\u00b7tet", "zu", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "$,", "VVPP", "KON", "VVFIN", "PTKZU", "VAINF", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}}, "stanza.7": {"line.1": {"text": "Bleiche Camille, Du bl\u00fchst verachtet im einsamen Felde,", "tokens": ["Blei\u00b7che", "Ca\u00b7mil\u00b7le", ",", "Du", "bl\u00fchst", "ver\u00b7ach\u00b7tet", "im", "ein\u00b7sa\u00b7men", "Fel\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+-+-+-", "measure": "dactylic.di.plus"}