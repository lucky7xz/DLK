{"textgrid.poem.46958": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Zweites Grab", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu Ottensen an der Mauer", "tokens": ["Zu", "Ot\u00b7ten\u00b7sen", "an", "der", "Mau\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "APPR", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der Kirch' ist noch ein Grab,", "tokens": ["Der", "Kirch'", "ist", "noch", "ein", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Darin des Lebens Trauer", "tokens": ["Da\u00b7rin", "des", "Le\u00b7bens", "Trau\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Held gelegt hat ab.", "tokens": ["Ein", "Held", "ge\u00b7legt", "hat", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Geschrieben ist der Namen", "tokens": ["Ge\u00b7schrie\u00b7ben", "ist", "der", "Na\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nicht auf den Leichenstein;", "tokens": ["Nicht", "auf", "den", "Lei\u00b7chen\u00b7stein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch er samt seinem Samen", "tokens": ["Doch", "er", "samt", "sei\u00b7nem", "Sa\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Wird nie vergessen sein.", "tokens": ["Wird", "nie", "ver\u00b7ges\u00b7sen", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Von Braunschweig ist's der Alte,", "tokens": ["Von", "Braun\u00b7schweig", "ist's", "der", "Al\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Karl Wilhelm Ferdinand,", "tokens": ["Karl", "Wil\u00b7helm", "Fer\u00b7di\u00b7nand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der vor des Hirnes Spalte", "tokens": ["Der", "vor", "des", "Hir\u00b7nes", "Spal\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hier Ruh' im Grabe fand.", "tokens": ["Hier", "Ruh'", "im", "Gra\u00b7be", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Lorbeerkranz entbl\u00e4ttert,", "tokens": ["Der", "Lor\u00b7beer\u00b7kranz", "ent\u00b7bl\u00e4t\u00b7tert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den auf dem Haupt er trug,", "tokens": ["Den", "auf", "dem", "Haupt", "er", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Stirn vom Schlag zerschmettert,", "tokens": ["Die", "Stirn", "vom", "Schlag", "zer\u00b7schmet\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der ihn bei Jena schlug;", "tokens": ["Der", "ihn", "bei", "Je\u00b7na", "schlug", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nicht, wo er war geboren,", "tokens": ["Nicht", ",", "wo", "er", "war", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "PWAV", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hat d\u00fcrfen sterben er:", "tokens": ["Hat", "d\u00fcr\u00b7fen", "ster\u00b7ben", "er", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VMFIN", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von seines Braunschweigs Thoren", "tokens": ["Von", "sei\u00b7nes", "Braun\u00b7schweigs", "Tho\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kam irrend er hieher;", "tokens": ["Kam", "ir\u00b7rend", "er", "hie\u00b7her", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "PPER", "PAV", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Umirrend mit den Scherben", "tokens": ["Um\u00b7ir\u00b7rend", "mit", "den", "Scher\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Haupts von Land zu Land,", "tokens": ["Des", "Haupts", "von", "Land", "zu", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das, eh' es konnte sterben,", "tokens": ["Das", ",", "eh'", "es", "konn\u00b7te", "ster\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "KOUS", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erst allen Schmerz empfand;", "tokens": ["Erst", "al\u00b7len", "Schmerz", "emp\u00b7fand", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Das erst noch mu\u00dfte denken", "tokens": ["Das", "erst", "noch", "mu\u00df\u00b7te", "den\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Zukunft lange Not,", "tokens": ["Der", "Zu\u00b7kunft", "lan\u00b7ge", "Not", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Eh' es sich durfte senken", "tokens": ["Eh'", "es", "sich", "durf\u00b7te", "sen\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "VMFIN", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Beschwichtigt in den Tod.", "tokens": ["Be\u00b7schwich\u00b7tigt", "in", "den", "Tod", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Jetzt hat sich's hier gesenket,", "tokens": ["Jetzt", "hat", "sich's", "hier", "ge\u00b7sen\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Doch hebt sich's, wie man glaubt,", "tokens": ["Doch", "hebt", "sich's", ",", "wie", "man", "glaubt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Noch aus der Gruft und denket", "tokens": ["Noch", "aus", "der", "Gruft", "und", "den\u00b7ket"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das alte Feldherrnhaupt.", "tokens": ["Das", "al\u00b7te", "Feld\u00b7herrn\u00b7haupt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Da sieht es die Befreiung", "tokens": ["Da", "sieht", "es", "die", "Be\u00b7frei\u00b7ung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nun wohl auf deutscher Flur,", "tokens": ["Nun", "wohl", "auf", "deut\u00b7scher", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch auch von der Entweihung", "tokens": ["Doch", "auch", "von", "der", "Ent\u00b7wei\u00b7hung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die unvertilgte Spur.", "tokens": ["Die", "un\u00b7ver\u00b7tilg\u00b7te", "Spur", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Da sieht es der Zw\u00f6lfhundert", "tokens": ["Da", "sieht", "es", "der", "Zw\u00f6lf\u00b7hun\u00b7dert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Grabst\u00e4tte sich so nah',", "tokens": ["Grab\u00b7st\u00e4t\u00b7te", "sich", "so", "nah'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ruft wohl aus verwundert:", "tokens": ["Und", "ruft", "wohl", "aus", "ver\u00b7wun\u00b7dert", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Feldherr ward ich ja.", "tokens": ["Ein", "Feld\u00b7herr", "ward", "ich", "ja", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "O Feldherrnamt wie grausend!", "tokens": ["O", "Feld\u00b7herr\u00b7namt", "wie", "grau\u00b7send", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KOKOM", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Um mich, den Feldherrn, her", "tokens": ["Um", "mich", ",", "den", "Feld\u00b7herrn", ",", "her"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KOUI", "PPER", "$,", "ART", "NN", "$,", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gelagert sind die Tausend,", "tokens": ["Ge\u00b7la\u00b7gert", "sind", "die", "Tau\u00b7send", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein gro\u00dfes Schmerzenheer.", "tokens": ["Ein", "gro\u00b7\u00dfes", "Schmer\u00b7zen\u00b7heer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Euch hat auf andern Pfaden,", "tokens": ["Euch", "hat", "auf", "an\u00b7dern", "Pfa\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und doch aus gleichem Grund,", "tokens": ["Und", "doch", "aus", "glei\u00b7chem", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Tod hieher geladen,", "tokens": ["Der", "Tod", "hie\u00b7her", "ge\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihr seid mit mir im Bund.", "tokens": ["Ihr", "seid", "mit", "mir", "im", "Bund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Da\u00df ohne Totenhemde", "tokens": ["Da\u00df", "oh\u00b7ne", "To\u00b7ten\u00b7hem\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr auf den Gr\u00e4bern sitzt,", "tokens": ["Ihr", "auf", "den", "Gr\u00e4\u00b7bern", "sitzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das schmerzt mich, weil der Fremde", "tokens": ["Das", "schmerzt", "mich", ",", "weil", "der", "Frem\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Noch geht in Purpur itzt.", "tokens": ["Noch", "geht", "in", "Pur\u00b7pur", "itzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Ist keiner mehr am Leben,", "tokens": ["Ist", "kei\u00b7ner", "mehr", "am", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Purpur auszuziehn", "tokens": ["Den", "Pur\u00b7pur", "aus\u00b7zu\u00b7ziehn"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dem Fremden, und zu geben", "tokens": ["Dem", "Frem\u00b7den", ",", "und", "zu", "ge\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Euch nackten Toten ihn?", "tokens": ["Euch", "nack\u00b7ten", "To\u00b7ten", "ihn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Mit seinen dunklen Sch\u00fctzen", "tokens": ["Mit", "sei\u00b7nen", "dunk\u00b7len", "Sch\u00fct\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der \u00d6ls, mein wackrer Sohn,", "tokens": ["Der", "\u00d6ls", ",", "mein", "wack\u00b7rer", "Sohn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der k\u00f6nnte wohl euch n\u00fctzen;", "tokens": ["Der", "k\u00f6nn\u00b7te", "wohl", "euch", "n\u00fct\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch fiel auch der nun schon.", "tokens": ["Doch", "fiel", "auch", "der", "nun", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Jetzt kann ich keinen nennen,", "tokens": ["Jetzt", "kann", "ich", "kei\u00b7nen", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIAT", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da ihn der Tod geraubt;", "tokens": ["Da", "ihn", "der", "Tod", "ge\u00b7raubt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und schmerzlich f\u00fchl' ich brennen", "tokens": ["Und", "schmerz\u00b7lich", "f\u00fchl'", "ich", "bren\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Spalt' in meinem Haupt.", "tokens": ["Die", "Spalt'", "in", "mei\u00b7nem", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Zu Ottensen an der Mauer", "tokens": ["Zu", "Ot\u00b7ten\u00b7sen", "an", "der", "Mau\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "APPR", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der Kirch' ist noch ein Grab,", "tokens": ["Der", "Kirch'", "ist", "noch", "ein", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Darin des Lebens Trauer", "tokens": ["Da\u00b7rin", "des", "Le\u00b7bens", "Trau\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Held gelegt hat ab.", "tokens": ["Ein", "Held", "ge\u00b7legt", "hat", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Geschrieben ist der Namen", "tokens": ["Ge\u00b7schrie\u00b7ben", "ist", "der", "Na\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nicht auf den Leichenstein;", "tokens": ["Nicht", "auf", "den", "Lei\u00b7chen\u00b7stein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch er samt seinem Samen", "tokens": ["Doch", "er", "samt", "sei\u00b7nem", "Sa\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Wird nie vergessen sein.", "tokens": ["Wird", "nie", "ver\u00b7ges\u00b7sen", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.19": {"line.1": {"text": "Von Braunschweig ist's der Alte,", "tokens": ["Von", "Braun\u00b7schweig", "ist's", "der", "Al\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Karl Wilhelm Ferdinand,", "tokens": ["Karl", "Wil\u00b7helm", "Fer\u00b7di\u00b7nand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der vor des Hirnes Spalte", "tokens": ["Der", "vor", "des", "Hir\u00b7nes", "Spal\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hier Ruh' im Grabe fand.", "tokens": ["Hier", "Ruh'", "im", "Gra\u00b7be", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.20": {"line.1": {"text": "Der Lorbeerkranz entbl\u00e4ttert,", "tokens": ["Der", "Lor\u00b7beer\u00b7kranz", "ent\u00b7bl\u00e4t\u00b7tert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den auf dem Haupt er trug,", "tokens": ["Den", "auf", "dem", "Haupt", "er", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Stirn vom Schlag zerschmettert,", "tokens": ["Die", "Stirn", "vom", "Schlag", "zer\u00b7schmet\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der ihn bei Jena schlug;", "tokens": ["Der", "ihn", "bei", "Je\u00b7na", "schlug", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.21": {"line.1": {"text": "Nicht, wo er war geboren,", "tokens": ["Nicht", ",", "wo", "er", "war", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "PWAV", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hat d\u00fcrfen sterben er:", "tokens": ["Hat", "d\u00fcr\u00b7fen", "ster\u00b7ben", "er", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VMFIN", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von seines Braunschweigs Thoren", "tokens": ["Von", "sei\u00b7nes", "Braun\u00b7schweigs", "Tho\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kam irrend er hieher;", "tokens": ["Kam", "ir\u00b7rend", "er", "hie\u00b7her", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "PPER", "PAV", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.22": {"line.1": {"text": "Umirrend mit den Scherben", "tokens": ["Um\u00b7ir\u00b7rend", "mit", "den", "Scher\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Haupts von Land zu Land,", "tokens": ["Des", "Haupts", "von", "Land", "zu", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das, eh' es konnte sterben,", "tokens": ["Das", ",", "eh'", "es", "konn\u00b7te", "ster\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "KOUS", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erst allen Schmerz empfand;", "tokens": ["Erst", "al\u00b7len", "Schmerz", "emp\u00b7fand", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.23": {"line.1": {"text": "Das erst noch mu\u00dfte denken", "tokens": ["Das", "erst", "noch", "mu\u00df\u00b7te", "den\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Zukunft lange Not,", "tokens": ["Der", "Zu\u00b7kunft", "lan\u00b7ge", "Not", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Eh' es sich durfte senken", "tokens": ["Eh'", "es", "sich", "durf\u00b7te", "sen\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "VMFIN", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Beschwichtigt in den Tod.", "tokens": ["Be\u00b7schwich\u00b7tigt", "in", "den", "Tod", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.24": {"line.1": {"text": "Jetzt hat sich's hier gesenket,", "tokens": ["Jetzt", "hat", "sich's", "hier", "ge\u00b7sen\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Doch hebt sich's, wie man glaubt,", "tokens": ["Doch", "hebt", "sich's", ",", "wie", "man", "glaubt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Noch aus der Gruft und denket", "tokens": ["Noch", "aus", "der", "Gruft", "und", "den\u00b7ket"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das alte Feldherrnhaupt.", "tokens": ["Das", "al\u00b7te", "Feld\u00b7herrn\u00b7haupt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.25": {"line.1": {"text": "Da sieht es die Befreiung", "tokens": ["Da", "sieht", "es", "die", "Be\u00b7frei\u00b7ung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nun wohl auf deutscher Flur,", "tokens": ["Nun", "wohl", "auf", "deut\u00b7scher", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch auch von der Entweihung", "tokens": ["Doch", "auch", "von", "der", "Ent\u00b7wei\u00b7hung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die unvertilgte Spur.", "tokens": ["Die", "un\u00b7ver\u00b7tilg\u00b7te", "Spur", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.26": {"line.1": {"text": "Da sieht es der Zw\u00f6lfhundert", "tokens": ["Da", "sieht", "es", "der", "Zw\u00f6lf\u00b7hun\u00b7dert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Grabst\u00e4tte sich so nah',", "tokens": ["Grab\u00b7st\u00e4t\u00b7te", "sich", "so", "nah'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ruft wohl aus verwundert:", "tokens": ["Und", "ruft", "wohl", "aus", "ver\u00b7wun\u00b7dert", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Feldherr ward ich ja.", "tokens": ["Ein", "Feld\u00b7herr", "ward", "ich", "ja", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.27": {"line.1": {"text": "O Feldherrnamt wie grausend!", "tokens": ["O", "Feld\u00b7herr\u00b7namt", "wie", "grau\u00b7send", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KOKOM", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Um mich, den Feldherrn, her", "tokens": ["Um", "mich", ",", "den", "Feld\u00b7herrn", ",", "her"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KOUI", "PPER", "$,", "ART", "NN", "$,", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gelagert sind die Tausend,", "tokens": ["Ge\u00b7la\u00b7gert", "sind", "die", "Tau\u00b7send", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein gro\u00dfes Schmerzenheer.", "tokens": ["Ein", "gro\u00b7\u00dfes", "Schmer\u00b7zen\u00b7heer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.28": {"line.1": {"text": "Euch hat auf andern Pfaden,", "tokens": ["Euch", "hat", "auf", "an\u00b7dern", "Pfa\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und doch aus gleichem Grund,", "tokens": ["Und", "doch", "aus", "glei\u00b7chem", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Tod hieher geladen,", "tokens": ["Der", "Tod", "hie\u00b7her", "ge\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihr seid mit mir im Bund.", "tokens": ["Ihr", "seid", "mit", "mir", "im", "Bund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.29": {"line.1": {"text": "Da\u00df ohne Totenhemde", "tokens": ["Da\u00df", "oh\u00b7ne", "To\u00b7ten\u00b7hem\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr auf den Gr\u00e4bern sitzt,", "tokens": ["Ihr", "auf", "den", "Gr\u00e4\u00b7bern", "sitzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das schmerzt mich, weil der Fremde", "tokens": ["Das", "schmerzt", "mich", ",", "weil", "der", "Frem\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Noch geht in Purpur itzt.", "tokens": ["Noch", "geht", "in", "Pur\u00b7pur", "itzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.30": {"line.1": {"text": "Ist keiner mehr am Leben,", "tokens": ["Ist", "kei\u00b7ner", "mehr", "am", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Purpur auszuziehn", "tokens": ["Den", "Pur\u00b7pur", "aus\u00b7zu\u00b7ziehn"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dem Fremden, und zu geben", "tokens": ["Dem", "Frem\u00b7den", ",", "und", "zu", "ge\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Euch nackten Toten ihn?", "tokens": ["Euch", "nack\u00b7ten", "To\u00b7ten", "ihn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.31": {"line.1": {"text": "Mit seinen dunklen Sch\u00fctzen", "tokens": ["Mit", "sei\u00b7nen", "dunk\u00b7len", "Sch\u00fct\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der \u00d6ls, mein wackrer Sohn,", "tokens": ["Der", "\u00d6ls", ",", "mein", "wack\u00b7rer", "Sohn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der k\u00f6nnte wohl euch n\u00fctzen;", "tokens": ["Der", "k\u00f6nn\u00b7te", "wohl", "euch", "n\u00fct\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch fiel auch der nun schon.", "tokens": ["Doch", "fiel", "auch", "der", "nun", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.32": {"line.1": {"text": "Jetzt kann ich keinen nennen,", "tokens": ["Jetzt", "kann", "ich", "kei\u00b7nen", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIAT", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da ihn der Tod geraubt;", "tokens": ["Da", "ihn", "der", "Tod", "ge\u00b7raubt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und schmerzlich f\u00fchl' ich brennen", "tokens": ["Und", "schmerz\u00b7lich", "f\u00fchl'", "ich", "bren\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Spalt' in meinem Haupt.", "tokens": ["Die", "Spalt'", "in", "mei\u00b7nem", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}