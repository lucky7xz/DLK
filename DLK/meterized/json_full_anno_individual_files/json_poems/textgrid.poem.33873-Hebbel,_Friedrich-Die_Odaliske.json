{"textgrid.poem.33873": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Odaliske", "genre": "verse", "period": "N.A.", "pub_year": 1853, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es harrt auf weichem Purpursammt", "tokens": ["Es", "harrt", "auf", "wei\u00b7chem", "Pur\u00b7pur\u00b7sammt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die j\u00fcngste Sclavin ihres Herrn,", "tokens": ["Die", "j\u00fcngs\u00b7te", "Scla\u00b7vin", "ih\u00b7res", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und unter dunkler Braue flammt", "tokens": ["Und", "un\u00b7ter", "dunk\u00b7ler", "Brau\u00b7e", "flammt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Auge, wie ein irrer Stern.", "tokens": ["Ihr", "Au\u00b7ge", ",", "wie", "ein", "ir\u00b7rer", "Stern", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sei stammt aus jenem Lande nicht,", "tokens": ["Sei", "stammt", "aus", "je\u00b7nem", "Lan\u00b7de", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "VVFIN", "APPR", "PDAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo ehrbar-blond der Weizen reift,", "tokens": ["Wo", "ehr\u00b7ba\u00b7rblond", "der", "Wei\u00b7zen", "reift", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und stachligt-keuch die Gerste sticht,", "tokens": ["Und", "stach\u00b7lig\u00b7tkeuch", "die", "Gers\u00b7te", "sticht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn man sie noch so leise streift.", "tokens": ["Wenn", "man", "sie", "noch", "so", "lei\u00b7se", "streift", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie ist der Feuerzone Kind,", "tokens": ["Sie", "ist", "der", "Feu\u00b7er\u00b7zo\u00b7ne", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo jede Frucht von selber f\u00e4llt,", "tokens": ["Wo", "je\u00b7de", "Frucht", "von", "sel\u00b7ber", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Weil sie der Baum, der zu geschwind", "tokens": ["Weil", "sie", "der", "Baum", ",", "der", "zu", "ge\u00b7schwind"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die zweite zeitigt, gar nicht h\u00e4lt.", "tokens": ["Die", "zwei\u00b7te", "zei\u00b7tigt", ",", "gar", "nicht", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$,", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie hat von dem Johannisstrauch", "tokens": ["Sie", "hat", "von", "dem", "Jo\u00b7han\u00b7niss\u00b7trauch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die karge Beere nie gepfl\u00fcckt,", "tokens": ["Die", "kar\u00b7ge", "Bee\u00b7re", "nie", "ge\u00b7pfl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die, ohne Kraft und ohne Hauch,", "tokens": ["Die", ",", "oh\u00b7ne", "Kraft", "und", "oh\u00b7ne", "Hauch", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUI", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zur Abwehr gar den Dorn noch z\u00fcckt.", "tokens": ["Zur", "Ab\u00b7wehr", "gar", "den", "Dorn", "noch", "z\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch ward sie oft vom Wein bespritzt,", "tokens": ["Doch", "ward", "sie", "oft", "vom", "Wein", "be\u00b7spritzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weil himmelan die Rebe drang", "tokens": ["Weil", "him\u00b7me\u00b7lan", "die", "Re\u00b7be", "drang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und dann, vom Sonnenstral zerschlitzt,", "tokens": ["Und", "dann", ",", "vom", "Son\u00b7nen\u00b7stral", "zer\u00b7schlitzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Traube in der Luft zersprang.", "tokens": ["Die", "Trau\u00b7be", "in", "der", "Luft", "zer\u00b7sprang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Drum sitzt sie auch nicht seufzend da,", "tokens": ["Drum", "sitzt", "sie", "auch", "nicht", "seuf\u00b7zend", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "PTKNEG", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun ihre eig'ne Stunde naht,", "tokens": ["Nun", "ih\u00b7re", "eig'\u00b7ne", "Stun\u00b7de", "naht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie denkt der Rosen, fern und nah',", "tokens": ["Sie", "denkt", "der", "Ro\u00b7sen", ",", "fern", "und", "nah'", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die sie schon selbst gebrochen hat.", "tokens": ["Die", "sie", "schon", "selbst", "ge\u00b7bro\u00b7chen", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und sieh, der Pascha tritt herein,", "tokens": ["Und", "sieh", ",", "der", "Pa\u00b7scha", "tritt", "her\u00b7ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zwar ernst und d\u00fcster, doch nicht alt,", "tokens": ["Zwar", "ernst", "und", "d\u00fcs\u00b7ter", ",", "doch", "nicht", "alt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und vor ihm her den Becher Wein", "tokens": ["Und", "vor", "ihm", "her", "den", "Be\u00b7cher", "Wein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Tr\u00e4gt eines Mohren Nachtgestalt.", "tokens": ["Tr\u00e4gt", "ei\u00b7nes", "Moh\u00b7ren", "Nacht\u00b7ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Er sieht das M\u00e4gdlein lange an,", "tokens": ["Er", "sieht", "das", "M\u00e4gd\u00b7lein", "lan\u00b7ge", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mi\u00dft Zug f\u00fcr Zug, und nickt nur still,", "tokens": ["Mi\u00dft", "Zug", "f\u00fcr", "Zug", ",", "und", "nickt", "nur", "still", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "NN", "$,", "KON", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zum goldnen Becher greift er dann", "tokens": ["Zum", "gold\u00b7nen", "Be\u00b7cher", "greift", "er", "dann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und fragt, ob sie nicht trinken will.", "tokens": ["Und", "fragt", ",", "ob", "sie", "nicht", "trin\u00b7ken", "will", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Ihr aber schwillt schon jetzt das Blut", "tokens": ["Ihr", "a\u00b7ber", "schwillt", "schon", "jetzt", "das", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bis an der Adern letzten Rand,", "tokens": ["Bis", "an", "der", "A\u00b7dern", "letz\u00b7ten", "Rand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Drum f\u00fcrchtet sie des Weines Glut,", "tokens": ["Drum", "f\u00fcrch\u00b7tet", "sie", "des", "Wei\u00b7nes", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und st\u00f6\u00dft ihn weg mit ihrer Hand.", "tokens": ["Und", "st\u00f6\u00dft", "ihn", "weg", "mit", "ih\u00b7rer", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Nun weis't er stumm den Mohren fort,", "tokens": ["Nun", "weis't", "er", "stumm", "den", "Moh\u00b7ren", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem wild das Auge gl\u00fcht vor Lust,", "tokens": ["Dem", "wild", "das", "Au\u00b7ge", "gl\u00fcht", "vor", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und setzt sich an den weichsten Ort", "tokens": ["Und", "setzt", "sich", "an", "den", "weichs\u00b7ten", "Ort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und k\u00fc\u00dft ihr langsam Mund und Brust.", "tokens": ["Und", "k\u00fc\u00dft", "ihr", "lang\u00b7sam", "Mund", "und", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Doch pl\u00f6tzlich dringt ein j\u00e4her Schrei", "tokens": ["Doch", "pl\u00f6tz\u00b7lich", "dringt", "ein", "j\u00e4\u00b7her", "Schrei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von au\u00dfen ihr in's bange Ohr;", "tokens": ["Von", "au\u00b7\u00dfen", "ihr", "in's", "ban\u00b7ge", "Ohr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PPOSAT", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie ruft verst\u00f6rt, was das denn sei?", "tokens": ["Sie", "ruft", "ver\u00b7st\u00f6rt", ",", "was", "das", "denn", "sei", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVPP", "$,", "PWS", "PDS", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und er versetzt: es starb der Mohr!", "tokens": ["Und", "er", "ver\u00b7setzt", ":", "es", "starb", "der", "Mohr", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVPP", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Er trank den Wein, den ich dir bot,", "tokens": ["Er", "trank", "den", "Wein", ",", "den", "ich", "dir", "bot", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wird der S\u00fcnde nimmer froh,", "tokens": ["Und", "wird", "der", "S\u00fcn\u00b7de", "nim\u00b7mer", "froh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn beigemischt war ihm der Tod! \u2013", "tokens": ["Denn", "bei\u00b7ge\u00b7mischt", "war", "ihm", "der", "Tod", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVPP", "VAFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich pr\u00fcfe jede Sclavin so!", "tokens": ["Ich", "pr\u00fc\u00b7fe", "je\u00b7de", "Scla\u00b7vi