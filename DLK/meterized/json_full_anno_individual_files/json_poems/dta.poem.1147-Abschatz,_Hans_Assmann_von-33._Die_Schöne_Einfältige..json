{"dta.poem.1147": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "33. Die Sch\u00f6ne Einf\u00e4ltige.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Mein liebstes Rosen-Bild/ das nur der Unschuld Kreide/ ", "tokens": ["Mein", "liebs\u00b7tes", "Ro\u00b7sen\u00b7Bild", "/", "das", "nur", "der", "Un\u00b7schuld", "Krei\u00b7de", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "PDS", "ADV", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kein Firnis schlauer List noch falsche Schmincke ziert/", "tokens": ["Kein", "Fir\u00b7nis", "schlau\u00b7er", "List", "noch", "fal\u00b7sche", "Schmin\u00b7cke", "ziert", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJA", "NN", "ADV", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein T\u00e4ubchen/ das kein Hertz/ wie keine Galle f\u00fchrt/", "tokens": ["Mein", "T\u00e4ub\u00b7chen", "/", "das", "kein", "Hertz", "/", "wie", "kei\u00b7ne", "Gal\u00b7le", "f\u00fchrt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "ART", "PIAT", "NN", "$(", "KOKOM", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df du nichts weist/ und viel doch gl\u00e4ubst/ ist meine Freude.", "tokens": ["Da\u00df", "du", "nichts", "weist", "/", "und", "viel", "doch", "gl\u00e4ubst", "/", "ist", "mei\u00b7ne", "Freu\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VVFIN", "$(", "KON", "ADV", "ADV", "VVFIN", "$(", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dein Sinn bekleidet sich mit reiner Einfalt-Seide/", "tokens": ["Dein", "Sinn", "be\u00b7klei\u00b7det", "sich", "mit", "rei\u00b7ner", "Ein\u00b7fal\u00b7tSei\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Durch die man deine Lieb\u2019 ohn langes Suchen sp\u00fcrt:", "tokens": ["Durch", "die", "man", "dei\u00b7ne", "Lieb'", "ohn", "lan\u00b7ges", "Su\u00b7chen", "sp\u00fcrt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIS", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Was solte mir ein Weib/ die gerne disputirt/", "tokens": ["Was", "sol\u00b7te", "mir", "ein", "Weib", "/", "die", "ger\u00b7ne", "dis\u00b7pu\u00b7tirt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ART", "NN", "$(", "ART", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dein Teutsches Ja und Nein vergn\u00fcgt uns alle beyde.", "tokens": ["Dein", "Teut\u00b7sches", "Ja", "und", "Nein", "ver\u00b7gn\u00fcgt", "uns", "al\u00b7le", "bey\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "NN", "VVFIN", "PPER", "PIAT", "PIS", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mein Wunsch ist/ da\u00df ich stets im Friede leben kan/", "tokens": ["Mein", "Wunsch", "ist", "/", "da\u00df", "ich", "stets", "im", "Frie\u00b7de", "le\u00b7ben", "kan", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$(", "KOUS", "PPER", "ADV", "APPRART", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dich/ nicht die Eitelkeit/ hab\u2019 ich zur Frau erwehlet.", "tokens": ["Dich", "/", "nicht", "die", "Ei\u00b7tel\u00b7keit", "/", "hab'", "ich", "zur", "Frau", "er\u00b7weh\u00b7let", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "PTKNEG", "ART", "NN", "$(", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wenn dort Xantippen nur nichts mehr als dir gefehlet/", "tokens": ["Wenn", "dort", "Xan\u00b7tip\u00b7pen", "nur", "nichts", "mehr", "als", "dir", "ge\u00b7feh\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "ADV", "PIS", "PIS", "KOKOM", "PPER", "VVPP", "$("], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "So st\u00fcnde Socrates den M\u00e4nnern oben an.", "tokens": ["So", "st\u00fcn\u00b7de", "So\u00b7cra\u00b7tes", "den", "M\u00e4n\u00b7nern", "o\u00b7ben", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.13": {"text": "Bist du ein lindes Wachs/ so ist mirs ein Gel\u00fccke/", "tokens": ["Bist", "du", "ein", "lin\u00b7des", "Wachs", "/", "so", "ist", "mirs", "ein", "Ge\u00b7l\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$(", "ADV", "VAFIN", "NE", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da\u00df ich nur Ehr und Treu zum Siegel in dich dr\u00fccke.", "tokens": ["Da\u00df", "ich", "nur", "Ehr", "und", "Treu", "zum", "Sie\u00b7gel", "in", "dich", "dr\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "KON", "NN", "APPRART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}