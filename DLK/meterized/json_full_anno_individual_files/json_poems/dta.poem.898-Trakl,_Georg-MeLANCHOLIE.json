{"dta.poem.898": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "MeLANCHOLIE", "genre": "Lyrik", "period": "N.A.", "pub_year": "1913", "urn": "urn:nbn:de:kobv:b4-30357-9", "language": ["de:0.99"], "booktitle": "Trakl, Georg: Gedichte. Leipzig, 1913."}, "poem": {"stanza.1": {"line.1": {"text": "Bl\u00e4uliche Schatten. O ihr dunklen Augen,", "tokens": ["Bl\u00e4u\u00b7li\u00b7che", "Schat\u00b7ten", ".", "O", "ihr", "dunk\u00b7len", "Au\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "NE", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Die lang mich anschaun im Vor\u00fcbergleiten.", "tokens": ["Die", "lang", "mich", "an\u00b7schaun", "im", "Vor\u00b7\u00fc\u00b7berg\u00b7lei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Guitarrenkl\u00e4nge sanft den Herbst begleiten", "tokens": ["Gui\u00b7tar\u00b7ren\u00b7kl\u00e4n\u00b7ge", "sanft", "den", "Herbst", "be\u00b7glei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Garten, aufgel\u00f6st in braunen Laugen.", "tokens": ["Im", "Gar\u00b7ten", ",", "auf\u00b7ge\u00b7l\u00f6st", "in", "brau\u00b7nen", "Lau\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Des Todes ernste D\u00fcsternis bereiten", "tokens": ["Des", "To\u00b7des", "erns\u00b7te", "D\u00fcs\u00b7ter\u00b7nis", "be\u00b7rei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nymphische H\u00e4nde, an roten Br\u00fcsten saugen", "tokens": ["Nym\u00b7phi\u00b7sche", "H\u00e4n\u00b7de", ",", "an", "ro\u00b7ten", "Br\u00fcs\u00b7ten", "sau\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "APPR", "ADJA", "NN", "VVINF"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.7": {"text": "Verfallne Lippen und in schwarzen Laugen", "tokens": ["Ver\u00b7fall\u00b7ne", "Lip\u00b7pen", "und", "in", "schwar\u00b7zen", "Lau\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Des Sonnenj\u00fcnglings feuchte Locken gleiten.", "tokens": ["Des", "Son\u00b7nen\u00b7j\u00fcng\u00b7lings", "feuch\u00b7te", "Lo\u00b7cken", "glei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}