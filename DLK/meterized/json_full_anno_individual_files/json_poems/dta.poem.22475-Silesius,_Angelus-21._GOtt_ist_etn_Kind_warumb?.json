{"dta.poem.22475": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "21. GOtt ist etn Kind/ warumb?", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Der Ewge GOttes Sohn wird heut erst Kind ge-", "tokens": ["Der", "Ew\u00b7ge", "Got\u00b7tes", "Sohn", "wird", "heut", "erst", "Kind", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "VAFIN", "ADV", "ADV", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "nennt/", "tokens": ["nennt", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Da Er doch tausend Jahr den Vatter schon gekennt:", "tokens": ["Da", "Er", "doch", "tau\u00b7send", "Jahr", "den", "Vat\u00b7ter", "schon", "ge\u00b7kennt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "CARD", "NN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Warumb? Er war kein Kind. Die Mutter machts", "tokens": ["Wa\u00b7rumb", "?", "Er", "war", "kein", "Kind", ".", "Die", "Mut\u00b7ter", "machts"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "$.", "PPER", "VAFIN", "PIAT", "NN", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "allein", "tokens": ["al\u00b7lein"], "token_info": ["word"], "pos": ["ADV"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Da\u00df Er warhafftiglich kan Kind gegr\u00fcsset sein.", "tokens": ["Da\u00df", "Er", "war\u00b7haff\u00b7tig\