{"textgrid.poem.63098": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Ich hasse alle", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hasse alle und bin ihnen gram,", "tokens": ["Ich", "has\u00b7se", "al\u00b7le", "und", "bin", "ih\u00b7nen", "gram", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "KON", "VAFIN", "PPER", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die nicht von mir wissen,", "tokens": ["Die", "nicht", "von", "mir", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df ich diesen oder jenen Witz gerissen,", "tokens": ["Da\u00df", "ich", "die\u00b7sen", "o\u00b7der", "je\u00b7nen", "Witz", "ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDAT", "KON", "PDAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Da\u00df ich diesen oder jenen Ku\u00df von diesen oder jenen Lippen nahm.", "tokens": ["Da\u00df", "ich", "die\u00b7sen", "o\u00b7der", "je\u00b7nen", "Ku\u00df", "von", "die\u00b7sen", "o\u00b7der", "je\u00b7nen", "Lip\u00b7pen", "nahm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDAT", "KON", "PDAT", "NN", "APPR", "PDAT", "KON", "PDAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Ich werfe mein rundes Herz in die Welt,", "tokens": ["Ich", "wer\u00b7fe", "mein", "run\u00b7des", "Herz", "in", "die", "Welt",