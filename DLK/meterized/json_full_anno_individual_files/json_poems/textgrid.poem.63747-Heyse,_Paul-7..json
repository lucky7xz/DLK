{"textgrid.poem.63747": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Sonntag stets und Feiertags mitunter", "tokens": ["Am", "Sonn\u00b7tag", "stets", "und", "Fei\u00b7er\u00b7tags", "mi\u00b7tun\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "KON", "NN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist freier Eintritt hier. Das Volk in Scharen", "tokens": ["Ist", "frei\u00b7er", "Ein\u00b7tritt", "hier", ".", "Das", "Volk", "in", "Scha\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJA", "NN", "ADV", "$.", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Str\u00f6mt durch die S\u00e4le, froh, den Frank zu sparen,", "tokens": ["Str\u00f6mt", "durch", "die", "S\u00e4\u00b7le", ",", "froh", ",", "den", "Frank", "zu", "spa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "ADJD", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und gafft und staunt und lacht und plaudert munter.", "tokens": ["Und", "gafft", "und", "staunt", "und", "lacht", "und", "plau\u00b7dert", "mun\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "ADJD", "KON", "VVFIN", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein stattlich B\u00fcrgerweib sah ich darunter,", "tokens": ["Ein", "statt\u00b7lich", "B\u0