{"textgrid.poem.38546": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Theon an Lyda", "genre": "verse", "period": "N.A.", "pub_year": 1779, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nimmer, nimmer darf ich dir gestehen", "tokens": ["Nim\u00b7mer", ",", "nim\u00b7mer", "darf", "ich", "dir", "ge\u00b7ste\u00b7hen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "VMFIN", "PPER", "PPER", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Was, beim ersten Drucke deiner Hand,", "tokens": ["Was", ",", "beim", "ers\u00b7ten", "Dru\u00b7cke", "dei\u00b7ner", "Hand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "APPRART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "S\u00fcsse Zauberin, mein Herz empfand!", "tokens": ["S\u00fcs\u00b7se", "Zau\u00b7be\u00b7rin", ",", "mein", "Herz", "emp\u00b7fand", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Meiner Einsamkeit verborgnes Flehen,", "tokens": ["Mei\u00b7ner", "Ein\u00b7sam\u00b7keit", "ver\u00b7borg\u00b7nes", "Fle\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Meine Seufzer wird der Sturm verwehen,", "tokens": ["Mei\u00b7ne", "Seuf\u00b7zer", "wird", "der", "Sturm", "ver\u00b7we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"