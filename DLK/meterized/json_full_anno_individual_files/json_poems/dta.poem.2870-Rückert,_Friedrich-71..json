{"dta.poem.2870": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "71.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Hand, die dich begabt, sieh an, nicht nur die Gaben;", "tokens": ["Die", "Hand", ",", "die", "dich", "be\u00b7gabt", ",", "sieh", "an", ",", "nicht", "nur", "die", "Ga\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "ADJD", "$,", "VVIMP", "PTKVZ", "$,", "PTKNEG", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mehr als Erworbnes gilt wie wir's erworben haben.", "tokens": ["Mehr", "als", "Er\u00b7worb\u00b7nes", "gilt", "wie", "wir's", "er\u00b7wor\u00b7ben", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "NE", "VVFIN", "KOKOM", "VAFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wenn gute G\u00f6tter dir geschenkt und Geister hold", "tokens": ["Wenn", "gu\u00b7te", "G\u00f6t\u00b7ter", "dir", "ge\u00b7schenkt", "und", "Geis\u00b7ter", "hold"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "PPER", "VVPP", "KON", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Staub oder d\u00fcrres Laub, wird dir's im Busen Gold.", "tokens": ["Staub", "o\u00b7der", "d\u00fcr\u00b7res", "Laub", ",", "wird", "dir's", "im", "Bu\u00b7sen", "Gold", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$,", "VAFIN", "PIS", "APPRART", "NN", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.3": {"line.1": {"text": "Und von Unholden wenn mit Silber oder Golde", "tokens": ["Und", "von", "Un\u00b7hol\u00b7den", "wenn", "mit", "Sil\u00b7ber", "o\u00b7der", "Gol\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KOUS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du dich bereichert glaubst, wird's in der Hand zu Molde.", "tokens": ["Du", "dich", "be\u00b7rei\u00b7chert", "glaubst", ",", "wird's", "in", "der", "Hand", "zu", "Mol\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ADJD", "VVFIN", "$,", "VAFIN", "APPR", "ART", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}