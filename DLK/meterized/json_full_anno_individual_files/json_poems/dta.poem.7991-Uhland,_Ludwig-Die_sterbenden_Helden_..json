"tokens": ["Die", "gold\u00b7ge\u00b7lock\u00b7te", ",", "l\u00e4\u00b7chelnd", "den", "Po\u00b7kal", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}