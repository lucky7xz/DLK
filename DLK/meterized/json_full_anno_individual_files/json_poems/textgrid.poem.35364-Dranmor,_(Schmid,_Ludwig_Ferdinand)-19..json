{"textgrid.poem.35364": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "19.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein unverdientes Los, ein blindes, schn\u00f6des \u2013", "tokens": ["Ein", "un\u00b7ver\u00b7dien\u00b7tes", "Los", ",", "ein", "blin\u00b7des", ",", "schn\u00f6\u00b7des", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "$,", "ADJA", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und was wird meines sein? wie kann ich wissen", "tokens": ["Und", "was", "wird", "mei\u00b7nes", "sein", "?", "wie", "kann", "ich", "wis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PWS", "VAFIN", "PPOSAT", "VAINF", "$.", "PWAV", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ob mein Geschick ein edles oder \u00f6des?", "tokens": ["Ob", "mein", "Ge\u00b7schick", "ein", "ed\u00b7les", "o\u00b7der", "\u00f6\u00b7des", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "ADJA", "KON", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wir alle sind umringt von Finsternissen.", "tokens": ["Wir", "al\u00b7le", "sind", "um\u00b7ringt", "von", "Fins\u00b7ter\u00b7nis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VAFIN", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Nicht immer sichtbar ist des Abgrunds Rand,", "tokens": ["Nicht", "im\u00b7mer", "sicht\u00b7bar", "ist", "des", "Ab\u00b7grunds", "Rand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Nicht zu beschw\u00f6ren jegliche Bedr\u00e4ngnis.", "tokens": ["Nicht", "zu", "be\u00b7schw\u00f6\u00b7ren", "jeg\u00b7li\u00b7che", "Be\u00b7dr\u00e4ng\u00b7nis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKZU", "VVINF", "PIAT", "NN", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.7": {"text": "Der Zufall waltet mehr als der Verstand,", "tokens": ["Der", "Zu\u00b7fall", "wal\u00b7tet", "mehr", "als", "der", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und mehr als Lebensweisheit das Verh\u00e4ngnis.", "tokens": ["Und", "mehr", "als", "Le\u00b7bens\u00b7weis\u00b7heit", "das", "Ver\u00b7h\u00e4ng\u00b7nis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOUS", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Es sichtet keine Gottheit die Atome,", "tokens": ["Es", "sich\u00b7tet", "kei\u00b7ne", "Got\u00b7theit", "die", "A\u00b7to\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Um die zu retten, jene zu verderben;", "tokens": ["Um", "die", "zu", "ret\u00b7ten", ",", "je\u00b7ne", "zu", "ver\u00b7der\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "PTKZU", "VVINF", "$,", "PDS", "PTKZU", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Entscheidend zwischen Leben oder Sterben", "tokens": ["Ent\u00b7schei\u00b7dend", "zwi\u00b7schen", "Le\u00b7ben", "o\u00b7der", "Ster\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Liegt oft ein Strohhalm in der Zeiten Strome.", "tokens": ["Liegt", "oft", "ein", "Stroh\u00b7halm", "in", "der", "Zei\u00b7ten", "Stro\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Wo sind die hei\u00dfersehnten Paradiese", "tokens": ["Wo", "sind", "die", "hei\u00b7\u00dfer\u00b7sehn\u00b7ten", "Pa\u00b7ra\u00b7die\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Und wo die Dunkelheit und wo das Licht?", "tokens": ["Und", "wo", "die", "Dun\u00b7kel\u00b7heit", "und", "wo", "das", "Licht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "KON", "PWAV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Wen trifft die Schuld, wenn unser Gleichgewicht", "tokens": ["Wen", "trifft", "die", "Schuld", ",", "wenn", "un\u00b7ser", "Gleich\u00b7ge\u00b7wicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "NN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Ersch\u00fcttert wird durch jede kleine Brise?", "tokens": ["Er\u00b7sch\u00fct\u00b7tert", "wird", "durch", "je\u00b7de", "klei\u00b7ne", "Bri\u00b7se", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "All unsre W\u00fcnsche langen Wohlergehens", "tokens": ["All", "uns\u00b7re", "W\u00fcn\u00b7sche", "lan\u00b7gen", "Woh\u00b7ler\u00b7ge\u00b7hens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.18": {"text": "Sind Gaukelspiel, wenn wir als brave Nauten", "tokens": ["Sind", "Gau\u00b7kel\u00b7spiel", ",", "wenn", "wir", "als", "bra\u00b7ve", "Nau\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "$,", "KOUS", "PPER", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Der Windsbraut todesk\u00fchn ins Auge schauten,", "tokens": ["Der", "Winds\u00b7braut", "to\u00b7des\u00b7k\u00fchn", "ins", "Au\u00b7ge", "schau\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Um \u00fcber Bord zu fallen unversehens;", "tokens": ["Um", "\u00fc\u00b7ber", "Bord", "zu", "fal\u00b7len", "un\u00b7ver\u00b7se\u00b7hens", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "NN", "PTKZU", "VVINF", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Dann, wenn die Wasser uns emporgeschnellt", "tokens": ["Dann", ",", "wenn", "die", "Was\u00b7ser", "uns", "em\u00b7por\u00b7ge\u00b7schnellt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Zum letztenmale, sehn wir aus der Ferne", "tokens": ["Zum", "letz\u00b7ten\u00b7ma\u00b7le", ",", "sehn", "wir", "aus", "der", "Fer\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "$,", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Des Schiffes wei\u00dfe Segel, hochgeschwellt,", "tokens": ["Des", "Schif\u00b7fes", "wei\u00b7\u00dfe", "Se\u00b7gel", ",", "hoch\u00b7ge\u00b7schwellt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Und sehen droben noch die stummen Sterne", "tokens": ["Und", "se\u00b7hen", "dro\u00b7ben", "noch", "die", "stum\u00b7men", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Erblassend durch des Himmels Dunkel schimmern,", "tokens": ["Er\u00b7blas\u00b7send", "durch", "des", "Him\u00b7mels", "Dun\u00b7kel", "schim\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Trostlose Sterne, kalt herniederblickend,", "tokens": ["Trost\u00b7lo\u00b7se", "Ster\u00b7ne", ",", "kalt", "her\u00b7nie\u00b7der\u00b7bli\u00b7ckend", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.27": {"text": "Bis, in der Wasserdecke sich verstrickend,", "tokens": ["Bis", ",", "in", "der", "Was\u00b7ser\u00b7de\u00b7cke", "sich", "ver\u00b7stri\u00b7ckend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "APPR", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Zum Abschied sie um unsre Augen flimmern.", "tokens": ["Zum", "Ab\u00b7schied", "sie", "um", "uns\u00b7re", "Au\u00b7gen", "flim\u00b7mern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "O dann, bevor es durch den Tod besiegelt,", "tokens": ["O", "dann", ",", "be\u00b7vor", "es", "durch", "den", "Tod", "be\u00b7sie\u00b7gelt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Bevor es ausgel\u00f6scht ist und verschollen,", "tokens": ["Be\u00b7vor", "es", "aus\u00b7ge\u00b7l\u00f6scht", "ist", "und", "ver\u00b7schol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Mag wohl, im innern Auge abgespiegelt,", "tokens": ["Mag", "wohl", ",", "im", "in\u00b7nern", "Au\u00b7ge", "ab\u00b7ge\u00b7spie\u00b7gelt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "$,", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Ein ganzes Dasein uns vor\u00fcberrollen,", "tokens": ["Ein", "gan\u00b7zes", "Da\u00b7sein", "uns", "vor\u00b7\u00fc\u00b7berr\u00b7ol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Ein Menschenleben, aber ohne Plage.", "tokens": ["Ein", "Men\u00b7schen\u00b7le\u00b7ben", ",", "a\u00b7ber", "oh\u00b7ne", "Pla\u00b7ge", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "All die vergangnen Schmerzen sind verwischt;", "tokens": ["All", "die", "ver\u00b7gang\u00b7nen", "Schmer\u00b7zen", "sind", "ver\u00b7wischt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Nur Wonnen werden wieder aufgefrischt", "tokens": ["Nur", "Won\u00b7nen", "wer\u00b7den", "wie\u00b7der", "auf\u00b7ge\u00b7frischt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Dort in dem ungeheuren Sarkophage.", "tokens": ["Dort", "in", "dem", "un\u00b7ge\u00b7heu\u00b7ren", "Sar\u00b7ko\u00b7pha\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Und frommt es, wenn in H\u00fctten und Pal\u00e4sten", "tokens": ["Und", "frommt", "es", ",", "wenn", "in", "H\u00fct\u00b7ten", "und", "Pa\u00b7l\u00e4s\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Wir unsre Eigenliebe eingemauert?", "tokens": ["Wir", "uns\u00b7re", "Ei\u00b7gen\u00b7lie\u00b7be", "ein\u00b7ge\u00b7mau\u00b7ert", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Die Krankheit naht, das Ungl\u00fcck sp\u00e4ht und lauert,", "tokens": ["Die", "Krank\u00b7heit", "naht", ",", "das", "Un\u00b7gl\u00fcck", "sp\u00e4ht", "und", "lau\u00b7ert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Und keine Vorsicht sch\u00fctzt vor rohen G\u00e4sten.", "tokens": ["Und", "kei\u00b7ne", "Vor\u00b7sicht", "sch\u00fctzt", "vor", "ro\u00b7hen", "G\u00e4s\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Die Gl\u00fccklichsten, wenn sie zur\u00fcckgeblieben", "tokens": ["Die", "Gl\u00fcck\u00b7lichs\u00b7ten", ",", "wenn", "sie", "zu\u00b7r\u00fcck\u00b7ge\u00b7blie\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Zu sp\u00e4tem Scheiden, dr\u00fcckt des Alters Last,", "tokens": ["Zu", "sp\u00e4\u00b7tem", "Schei\u00b7den", ",", "dr\u00fcckt", "des", "Al\u00b7ters", "Last", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "Und selbst der Sieche sucht die letzte Rast,", "tokens": ["Und", "selbst", "der", "Sie\u00b7che", "sucht", "die", "letz\u00b7te", "Rast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "Die einzig ungetr\u00fcbte, zu verschieben!", "tokens": ["Die", "ein\u00b7zig", "un\u00b7ge\u00b7tr\u00fcb\u00b7te", ",", "zu", "ver\u00b7schie\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.45": {"text": "Und alle, trotz Entt\u00e4uschung und Beschwerden,", "tokens": ["Und", "al\u00b7le", ",", "trotz", "Ent\u00b7t\u00e4u\u00b7schung", "und", "Be\u00b7schwer\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.46": {"text": "Der in der H\u00f6he, jener in der Tiefe \u2013 \u2013", "tokens": ["Der", "in", "der", "H\u00f6\u00b7he", ",", "je\u00b7ner", "in", "der", "Tie\u00b7fe", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "APPR", "ART", "NN", "$,", "PDAT", "APPR", "ART", "NN", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.47": {"text": "Und keiner doch, der nicht zum Himmel riefe:", "tokens": ["Und", "kei\u00b7ner", "doch", ",", "der", "nicht", "zum", "Him\u00b7mel", "rie\u00b7fe", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "$,", "PRELS", "PTKNEG", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.48": {"text": "\u00bbdas Leben ist nicht wert gelebt zu werden!\u00ab", "tokens": ["\u00bb", "das", "Le\u00b7ben", "ist", "nicht", "wert", "ge\u00b7lebt", "zu", "wer\u00b7den", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "PT