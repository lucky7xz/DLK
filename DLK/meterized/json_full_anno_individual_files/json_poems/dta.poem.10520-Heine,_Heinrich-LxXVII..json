{"dta.poem.10520": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "LxXVII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Ach, die Augen sind es wieder,", "tokens": ["Ach", ",", "die", "Au\u00b7gen", "sind", "es", "wie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die mich einst so lieblich gr\u00fc\u00dften,", "tokens": ["Die", "mich", "einst", "so", "lieb\u00b7lich", "gr\u00fc\u00df\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es sind die Lippen wieder,", "tokens": ["Und", "es", "sind", "die", "Lip\u00b7pen", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die mir's Leben einst vers\u00fc\u00dften;", "tokens": ["Die", "mir's", "Le\u00b7ben", "einst", "ver\u00b7s\u00fc\u00df\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Auch die Stimme ist es wieder,", "tokens": ["Auch", "die", "Stim\u00b7me", "ist", "es", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die ich einst so gern geh\u00f6ret,", "tokens": ["Die", "ich", "einst", "so", "gern", "ge\u00b7h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur ich selber bin's nicht wieder,", "tokens": ["Nur", "ich", "sel\u00b7ber", "bin's", "nicht", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "VAFIN", "PTKNEG", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bin ver\u00e4ndert heimgekehret.", "tokens": ["Bin", "ver\u00b7\u00e4n\u00b7dert", "heim\u00b7ge\u00b7keh\u00b7ret", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Von den wei\u00dfen, sch\u00f6nen Armen", "tokens": ["Von", "den", "wei\u00b7\u00dfen", ",", "sch\u00f6\u00b7nen", "Ar\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fest und liebevoll umschlossen,", "tokens": ["Fest", "und", "lie\u00b7be\u00b7voll", "um\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lieg ich jetzt an ihrem Herzen,", "tokens": ["Lieg", "ich", "jetzt", "an", "ih\u00b7rem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dumpfen Sinnes und verdrossen.", "tokens": ["Dum\u00b7pfen", "Sin\u00b7nes", "und", "ver\u00b7dros\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "KON", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}