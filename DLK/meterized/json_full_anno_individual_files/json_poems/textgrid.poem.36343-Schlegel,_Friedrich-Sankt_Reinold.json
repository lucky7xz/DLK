{"textgrid.poem.36343": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Sankt Reinold", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sankt Reinold als Einsiedel war", "tokens": ["Sankt", "Rei\u00b7nold", "als", "Ein\u00b7sie\u00b7del", "war"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "NE", "KOUS", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Andacht wohl ergeben,", "tokens": ["Der", "An\u00b7dacht", "wohl", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vergessen hatt' er ganz und gar", "tokens": ["Ver\u00b7ges\u00b7sen", "hatt'", "er", "ganz", "und", "gar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPER", "ADV", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Ritters Lust und Leben.", "tokens": ["Des", "Rit\u00b7ters", "Lust", "und", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er sucht sich seine Wahlstatt aus", "tokens": ["Er", "sucht", "sich", "sei\u00b7ne", "Wahl\u00b7statt", "aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bei K\u00f6ln, der Stadt am Rheine,", "tokens": ["Bei", "K\u00f6ln", ",", "der", "Stadt", "am", "Rhei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "ART", "NN", "APPRART", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Daselbst zu baun ein Gotteshaus,", "tokens": ["Da\u00b7selbst", "zu", "baun", "ein", "Got\u00b7tes\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PTKZU", "VVINF", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das w\u00fcnscht er noch alleine.", "tokens": ["Das", "w\u00fcnscht", "er", "noch", "al\u00b7lei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Bau war all sein Augenmerk,", "tokens": ["Der", "Bau", "war", "all", "sein", "Au\u00b7gen\u00b7merk", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er treibt es unerm\u00fcdlich,", "tokens": ["Er", "treibt", "es", "un\u00b7er\u00b7m\u00fcd\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vollenden will er sehn das Werk,", "tokens": ["Voll\u00b7en\u00b7den", "will", "er", "sehn", "das", "Werk", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sodann nur sterben friedlich.", "tokens": ["So\u00b7dann", "nur", "ster\u00b7ben", "fried\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Schon sieht er, wie der Bogen springt,", "tokens": ["Schon", "sieht", "er", ",", "wie", "der", "Bo\u00b7gen", "springt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Chor an rechter Stelle;", "tokens": ["Der", "Chor", "an", "rech\u00b7ter", "Stel\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und wenn des Turmes Kunst gelingt,", "tokens": ["Und", "wenn", "des", "Tur\u00b7mes", "Kunst", "ge\u00b7lingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ist fertig die Kapelle.", "tokens": ["Ist", "fer\u00b7tig", "die", "Ka\u00b7pel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Vom Bauen ist Verdru\u00df nicht weit,", "tokens": ["Vom", "Bau\u00b7en", "ist", "Ver\u00b7dru\u00df", "nicht", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "NN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Herr Reinold mu\u00df es b\u00fc\u00dfen;", "tokens": ["Herr", "Rei\u00b7nold", "mu\u00df", "es", "b\u00fc\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VMFIN", "PPER", "VVINF", "$."], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Knechte waren arge Leut',", "tokens": ["Die", "Knech\u00b7te", "wa\u00b7ren", "ar\u00b7ge", "Leut'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die leben ihren L\u00fcsten.", "tokens": ["Die", "le\u00b7ben", "ih\u00b7ren", "L\u00fcs\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der alte Ritter sich ihm regt", "tokens": ["Der", "al\u00b7te", "Rit\u00b7ter", "sich", "ihm", "regt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PRF", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ob diesem faulen Wesen,", "tokens": ["Ob", "die\u00b7sem", "fau\u00b7len", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Treulich mit F\u00e4usten er sie schl\u00e4gt,", "tokens": ["Treu\u00b7lich", "mit", "F\u00e4us\u00b7ten", "er", "sie", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "PPER", "PPER", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Schilt sie mit frommen Reden.", "tokens": ["Schilt", "sie", "mit", "from\u00b7men", "Re\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbwenn ihr zum Bau verdrossen seid,", "tokens": ["\u00bb", "wenn", "ihr", "zum", "Bau", "ver\u00b7dros\u00b7sen", "seid", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "APPRART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Hand in Scho\u00df wollt legen,", "tokens": ["Die", "Hand", "in", "Scho\u00df", "wollt", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit Schwatzen bringen hin die Zeit,", "tokens": ["Mit", "Schwat\u00b7zen", "brin\u00b7gen", "hin", "die", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Leib in Wollust pflegen;", "tokens": ["Den", "Leib", "in", "Wol\u00b7lust", "pfle\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So seid ihr schlimme Knechte wohl", "tokens": ["So", "seid", "ihr", "schlim\u00b7me", "Knech\u00b7te", "wohl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vor Gott und aller Augen,", "tokens": ["Vor", "Gott", "und", "al\u00b7ler", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die man zur Arbeit zwingen soll,", "tokens": ["Die", "man", "zur", "Ar\u00b7beit", "zwin\u00b7gen", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPRART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df sie zu Frommen taugen.\u00ab", "tokens": ["Da\u00df", "sie", "zu", "From\u00b7men", "tau\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "So treibt er's f\u00fcrder Tag f\u00fcr Tag,", "tokens": ["So", "treibt", "er's", "f\u00fcr\u00b7der", "Tag", "f\u00fcr", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Streng haltend auf dem Rechte;", "tokens": ["Streng", "hal\u00b7tend", "auf", "dem", "Rech\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vor Sonnenaufgang ist er wach,", "tokens": ["Vor", "Son\u00b7nen\u00b7auf\u00b7gang", "ist", "er", "wach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Treibt an die faulen Knechte.", "tokens": ["Treibt", "an", "die", "fau\u00b7len", "Knech\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Kaum da\u00df er sich gedulden kann", "tokens": ["Kaum", "da\u00df", "er", "sich", "ge\u00b7dul\u00b7den", "kann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "PRF", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Gotteshaus zu schauen,", "tokens": ["Das", "Got\u00b7tes\u00b7haus", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da will er f\u00fcrder beten dann,", "tokens": ["Da", "will", "er", "f\u00fcr\u00b7der", "be\u00b7ten", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sein Grab sich selber bauen.", "tokens": ["Sein", "Grab", "sich", "sel\u00b7ber", "bau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Indes die Knechte halten Rat,", "tokens": ["In\u00b7des", "die", "Knech\u00b7te", "hal\u00b7ten", "Rat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie sie ihn m\u00f6chten fassen,", "tokens": ["Wie", "sie", "ihn", "m\u00f6ch\u00b7ten", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bereden sich zu schlimmer Tat,", "tokens": ["Be\u00b7re\u00b7den", "sich", "zu", "schlim\u00b7mer", "Tat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weil sie sein Strafen hassen.", "tokens": ["Weil", "sie", "sein", "Stra\u00b7fen", "has\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Faulheit vor allem in der Welt", "tokens": ["Faul\u00b7heit", "vor", "al\u00b7lem", "in", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PIS", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ist wohl die \u00e4rgste S\u00fcnde;", "tokens": ["Ist", "wohl", "die", "\u00e4rgs\u00b7te", "S\u00fcn\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der B\u00f6se fest den Faulen h\u00e4lt,", "tokens": ["Der", "B\u00f6\u00b7se", "fest", "den", "Fau\u00b7len", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die alte T\u00fcck' entz\u00fcndet.", "tokens": ["Die", "al\u00b7te", "T\u00fcck", "ent\u00b7z\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Reinold, der redlich ihnen traut,", "tokens": ["Rei\u00b7nold", ",", "der", "red\u00b7lich", "ih\u00b7nen", "traut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ADJD", "PPER", "VVFIN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Kam wieder da gegangen;", "tokens": ["Kam", "wie\u00b7der", "da", "ge\u00b7gan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Beginnen die zu murren laut,", "tokens": ["Be\u00b7gin\u00b7nen", "die", "zu", "mur\u00b7ren", "laut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "PTKZU", "VVINF", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So sollt' es nun anfangen.", "tokens": ["So", "sollt'", "es", "nun", "an\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie werfen nach ihm manches St\u00fcck,", "tokens": ["Sie", "wer\u00b7fen", "nach", "ihm", "man\u00b7ches", "St\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Furchtsam ihn zu umklammern,", "tokens": ["Furcht\u00b7sam", "ihn", "zu", "um\u00b7klam\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Bis endlich da er f\u00e4llt zur\u00fcck,", "tokens": ["Bis", "end\u00b7lich", "da", "er", "f\u00e4llt", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "KOUS", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Schlagen sie ihn mit Hammern.", "tokens": ["Schla\u00b7gen", "sie", "ihn", "mit", "Ham\u00b7mern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.8": {"line.1": {"text": "Als tot nun auf dem Boden lag", "tokens": ["Als", "tot", "nun", "auf", "dem", "Bo\u00b7den", "lag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "ADV", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der fromme Herr im Blute,", "tokens": ["Der", "from\u00b7me", "Herr", "im", "Blu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da fliehn sie wie vom Donnerschlag,", "tokens": ["Da", "fliehn", "sie", "wie", "vom", "Don\u00b7ner\u00b7schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOKOM", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verr\u00fcckt in wildem Mute.", "tokens": ["Ver\u00b7r\u00fcckt", "in", "wil\u00b7dem", "Mu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Bauern des Weges fanden ihn,", "tokens": ["Bau\u00b7ern", "des", "We\u00b7ges", "fan\u00b7den", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Die ihn sogleich erkannten;", "tokens": ["Die", "ihn", "sog\u00b7leich", "er\u00b7kann\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Erschrocken knien sie bei ihm hin,", "tokens": ["Er\u00b7schro\u00b7cken", "kni\u00b7en", "sie", "bei", "ihm", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "F\u00fcr ihn zu Gott sich wandten.", "tokens": ["F\u00fcr", "ihn", "zu", "Gott", "sich", "wand\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Prachtvoll ward er bestattet dann", "tokens": ["Pracht\u00b7voll", "ward", "er", "be\u00b7stat\u00b7tet", "dann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "VVFIN", "ADV"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Mit Singen und Gel\u00e4ute,", "tokens": ["Mit", "Sin\u00b7gen", "und", "Ge\u00b7l\u00e4u\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Fahne weht dem Zug voran", "tokens": ["Die", "Fah\u00b7ne", "weht", "dem", "Zug", "vo\u00b7ran"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der schwarzen Trauerleute,", "tokens": ["Der", "schwar\u00b7zen", "Trau\u00b7er\u00b7leu\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und in der sch\u00f6nen Fahne war,", "tokens": ["Und", "in", "der", "sch\u00f6\u00b7nen", "Fah\u00b7ne", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auf buntem Schmuckgefilde,", "tokens": ["Auf", "bun\u00b7tem", "Schmuck\u00b7ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "In schwarzer Farbe, brennend klar,", "tokens": ["In", "schwar\u00b7zer", "Far\u00b7be", ",", "bren\u00b7nend", "klar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ro\u00df Bayard abgebildet.", "tokens": ["Ro\u00df", "Bay\u00b7ard", "ab\u00b7ge\u00b7bil\u00b7det", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NE", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Panzer und Handschuh ziert den Sarg,", "tokens": ["Pan\u00b7zer", "und", "Hand\u00b7schuh", "ziert", "den", "Sarg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Den Helmbusch sieht man wehen", "tokens": ["Den", "Helm\u00b7busch", "sieht", "man", "we\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PIS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Am Steine, der den Helden barg,", "tokens": ["Am", "Stei\u00b7ne", ",", "der", "den", "Hel\u00b7den", "barg", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gl\u00f6cklein und Stab daneben.", "tokens": ["Gl\u00f6c\u00b7klein", "und", "Stab", "da\u00b7ne\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PAV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und nun, wo er erschlagen war,", "tokens": ["Und", "nun", ",", "wo", "er", "er\u00b7schla\u00b7gen", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auf dieser selben Stelle,", "tokens": ["Auf", "die\u00b7ser", "sel\u00b7ben", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ward neu errichtet ein Altar,", "tokens": ["Ward", "neu", "er\u00b7rich\u00b7tet", "ein", "Al\u00b7tar", ","], "token_info": ["word", "word", "word", 