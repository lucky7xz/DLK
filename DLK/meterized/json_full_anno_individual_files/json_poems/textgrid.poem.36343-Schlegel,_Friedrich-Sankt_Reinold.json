"text": "Schon sieht er, wie der Bogen springt,", "tokens": ["Schon", "sieht", "er", ",", "wie", "der", "Bo\u00b7gen", "springt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Chor an rechter Stelle;", "tokens": ["Der", "Chor", "an", "rech\u00b7ter", "Stel\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und wenn des Turmes Kunst gelingt,", "tokens": ["Und", "wenn", "des", "Tur\u00b7mes", "Kunst", "ge\u00b7lingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ist fertig die Kapelle.", "tokens": ["Ist", "fer\u00b7tig", "die", "Ka\u00b7pel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Vom Bauen ist Verdru\u00df nicht weit,", "tokens": ["Vom", "Bau\u00b7en", "ist", "Ver\u00b7dru\u00df", "nicht", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "NN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Herr Reinold mu\u00df es b\u00fc\u00dfen;", "tokens": ["Herr", "Rei\u00b7nold", "mu\u00df", "es", "b\u00fc\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VMFIN", "PPER", "VVINF", "$."], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Knechte waren arge Leut',", "tokens": ["Die", "Knech\u00b7te", "wa\u00b7ren", "ar\u00b7ge", "Leut'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die leben ihren L\u00fcsten.", "tokens": ["Die", "le\u00b7ben", "ih\u00b7ren", "L\u00fcs\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der alte Ritter sich ihm regt", "tokens": ["Der", "al\u00b7te", "Rit\u00b7ter", "sich", "ihm", "regt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PRF", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ob diesem faulen Wesen,", "tokens": ["Ob", "die\u00b7sem", "fau\u00b7len", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Treulich mit F\u00e4usten er sie schl\u00e4gt,", "tokens": ["Treu\u00b7lich", "mit", "F\u00e4us\u00b7ten", "er", "sie", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "PPER", "PPER", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Schilt sie mit frommen Reden.", "tokens": ["Schilt", "sie", "mit", "from\u00b7men", "Re\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "\u00bbwenn ihr zum Bau verdrossen seid,", "tokens": ["\u00bb", "wenn", "ihr", "zum", "Bau", "ver\u00b7dros\u00b7sen", "seid", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "APPRART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "