: "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da fliehn sie wie vom Donnerschlag,", "tokens": ["Da", "fliehn", "sie", "wie", "vom", "Don\u00b7ner\u00b7schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOKOM", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verr\u00fcckt in wildem Mute.", "tokens": ["Ver\u00b7r\u00fcckt", "in", "wil\u00b7dem", "Mu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Bauern des Weges fanden ihn,", "tokens": ["Bau\u00b7ern", "des", "We\u00b7ges", "fan\u00b7den", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Die ihn sogleich erkannten;", "tokens": ["Die", "ihn", "sog\u00b7leich", "er\u00b7kann\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Erschrocken knien sie bei ihm hin,", "tokens": ["Er\u00b7schro\u00b7cken", "kni\u00b7en", "sie", "bei", "ihm", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "F\u00fcr ihn zu Gott sich wandten.", "tokens": ["F\u00fcr", "ihn", "zu", "Gott", "sich", "wand\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.19": {"line.1": {"text": "Prachtvoll ward er bestattet dann", "tokens": ["Pracht\u00b7voll", "ward", "er", "be\u00b7stat\u00b7tet", "dann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "VVFIN", "ADV"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Mit Singen und Gel\u00e4ute,", "tokens": ["Mit", "Sin\u00b7gen", "und", "Ge\u00b7l\u00e4u\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Fahne weht dem Zug voran", "tokens": ["Die", "Fah\u00b7ne", "weht", "dem", "Zug", "vo\u00b7ran"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der schwarzen Trauerleute,", "tokens": ["Der", "schwar\u00b7zen", "Trau\u00b7er\u00b7leu\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und in der sch\u00f6nen Fahne war,", "tokens": ["Und", "in", "der", "sch\u00f6\u00b7nen", "Fah\u00b7ne", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auf buntem Schmuckgefilde,", "tokens": ["Auf", "bun\u00b7tem", "Schmuck\u00b7ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "In schwarzer Farbe, brennend klar,", "tokens": ["In", "schwar\u00b7zer", "Far\u00b7be", ",", "bren\u00b7nend", "klar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ro\u00df Bayard abgebildet.", "tokens": ["Ro\u00df", "Bay\u00b7ard", "ab\u00b7ge\u00b7bil\u00b7det", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NE", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.20": {"line.1": {"text": "Panzer und Handschuh ziert den Sarg,", "tokens": ["Pan\u00b7zer", "und", "Hand\u00b7schuh", "ziert", "den", "Sarg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Den Helmbusch sieht man wehen", "tokens": ["Den", "Helm\u00b7busch", "sieht", "man", "we\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PIS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Am Steine, der den Helden barg,", "tokens": ["Am", "Stei\u00b7ne", ",", "der", "den", "Hel\u00b7den", "barg", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gl\u00f6cklein und Stab daneben.", "tokens": ["Gl\u00f6c\u00b7klein", "und", "Stab", "da\u00b7ne\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PAV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und nun, wo er erschlagen war,", "tokens": ["Und", "nun", ",", "wo", "er", "er\u00b7schla\u00b7gen", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auf dieser selben Stelle,", "tokens": ["Auf", "die\u00b7ser", "sel\u00b7ben", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ward neu errichtet ein Altar,", "tokens": ["Ward", "neu", "er\u00b7rich\u00b7tet", "ein", "Al\u00b7tar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Man zeigt noch die Kapelle.", "tokens": ["Man", "zeigt", "noch", "die", "Ka\u00b7pel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}