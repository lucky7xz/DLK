{"textgrid.poem.57807": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Heimliche Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die sch\u00f6nste Freude, die ich kenne,", "tokens": ["Die", "sch\u00f6ns\u00b7te", "Freu\u00b7de", ",", "die", "ich", "ken\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Rot R\u00f6selein, Vergi\u00dfnichtmein,", "tokens": ["Rot", "R\u00f6\u00b7se\u00b7lein", ",", "Ver\u00b7gi\u00df\u00b7nicht\u00b7mein", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und die ich keinem Menschen nenne,", "tokens": ["Und", "die", "ich", "kei\u00b7nem", "Men\u00b7schen", "nen\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELS", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Rot R\u00f6selein, Vergi\u00dfnichtmein,", "tokens": ["Rot", "R\u00f6\u00b7se\u00b7lein", ",", "Ver\u00b7gi\u00df\u00b7nicht\u00b7mein", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir beide wissen's ganz allein,", "tokens": ["Wir", "bei\u00b7de", "wis\u00b7sen's", "ganz", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Verschwiegen soll es sein.", "tokens": ["Ver\u00b7schwie\u00b7gen", "soll", "