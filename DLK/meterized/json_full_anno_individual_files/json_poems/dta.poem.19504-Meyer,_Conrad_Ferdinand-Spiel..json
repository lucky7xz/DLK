{"dta.poem.19504": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Spiel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Denkst, Freund, des wilden Knabenspiels du noch,", "tokens": ["Denkst", ",", "Freund", ",", "des", "wil\u00b7den", "Kna\u00b7ben\u00b7spiels", "du", "noch", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "ART", "ADJA", "NN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das wir getrieben einst am Bergesjoch,", "tokens": ["Das", "wir", "ge\u00b7trie\u00b7ben", "einst", "am", "Ber\u00b7ges\u00b7joch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wann unser freud'ge Wandertag verglomm", "tokens": ["Wann", "un\u00b7ser", "freu\u00b7d'\u00b7ge", "Wan\u00b7der\u00b7tag", "ver\u00b7glomm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und h\u00f6her stets und immer h\u00f6her klomm?", "tokens": ["Und", "h\u00f6\u00b7her", "stets", "und", "im\u00b7mer", "h\u00f6\u00b7her", "klomm", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "KON", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wir sprangen jubelnd \u00fcber Stock und Stein", "tokens": ["Wir", "spran\u00b7gen", "ju\u00b7belnd", "\u00fc\u00b7ber", "Stock", "und", "Stein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Bergan und wieder in das Licht hinein", "tokens": ["Ber\u00b7gan", "und", "wie\u00b7der", "in", "das", "Licht", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "ADV", "APPR", "ART", "NN", "APZR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und noch einmal und noch einmal,", "tokens": ["Und", "noch", "ein\u00b7mal", "und", "noch", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "KON", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bis uns entschl\u00fcpft' der letzte Sonnenstrahl.", "tokens": ["Bis", "uns", "ent\u00b7schl\u00fcpft'", "der", "letz\u00b7te", "Son\u00b7nen\u00b7strahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das Spiel das wir im Alpenthale dort", "tokens": ["Das", "Spiel", "das", "wir", "im", "Al\u00b7pen\u00b7tha\u00b7le", "dort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "PPER", "APPRART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Getrieben, Freund, wir spielen's heut noch fort.", "tokens": ["Ge\u00b7trie\u00b7ben", ",", "Freund", ",", "wir", "spie\u00b7len's", "heut", "noch", "fort", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "NN", "$,", "PPER", "VVFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wann neben uns das s\u00fc\u00dfe Licht erbleicht,", "tokens": ["Wann", "ne\u00b7ben", "uns", "das", "s\u00fc\u00b7\u00dfe", "Licht", "er\u00b7bleicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wir steigen, bis von neuem wir's erreicht.", "tokens": ["Wir", "stei\u00b7gen", ",", "bis", "von", "neu\u00b7em", "wir's", "er\u00b7reicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "APPR", "ADJA", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wir springen muthig \u00fcber Stock und Stein", "tokens": ["Wir", "sprin\u00b7gen", "mut\u00b7hig", "\u00fc\u00b7ber", "Stock", "und", "Stein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und mitten wieder in den Tag hinein", "tokens": ["Und", "mit\u00b7ten", "wie\u00b7der", "in", "den", "Tag", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "APPR", "ART", "NN", "APZR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und noch einmal und noch einmal,", "tokens": ["Und", "noch", "ein\u00b7mal", "und", "noch", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "KON", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bis uns entschl\u00fcpft der letzte Lebensstrahl.", "tokens": ["Bis", "uns", "ent\u00b7schl\u00fcpft", "der", "letz\u00b7te", "Le\u00b7bens\u00b7strahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}