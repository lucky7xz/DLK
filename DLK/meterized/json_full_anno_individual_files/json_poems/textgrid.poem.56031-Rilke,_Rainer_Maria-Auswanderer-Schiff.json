{"textgrid.poem.56031": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Auswanderer-Schiff", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Denk: da\u00df einer hei\u00df und gl\u00fchend fl\u00fcchte,", "tokens": ["Denk", ":", "da\u00df", "ei\u00b7ner", "hei\u00df", "und", "gl\u00fc\u00b7hend", "fl\u00fcch\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "KOUS", "ART", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "und die Sieger w\u00e4ren hinterher,", "tokens": ["und", "die", "Sie\u00b7ger", "w\u00e4\u00b7ren", "hin\u00b7ter\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "und auf einmal machte der", "tokens": ["und", "auf", "ein\u00b7mal", "mach\u00b7te", "der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADV", "VVFIN", "ART"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Fl\u00fcchtende kurz, unerwartet, Kehr", "tokens": ["Fl\u00fcch\u00b7ten\u00b7de", "kurz", ",", "un\u00b7er\u00b7war\u00b7tet", ",", "Kehr"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["NN", "ADJD", "$,", "ADJD", "$,", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "gegen Hunderte \u2013: so sehr", "tokens": ["ge\u00b7gen", "Hun\u00b7der\u00b7te", "\u2013", ":", "so", "sehr"], "token_info": ["word", "word", "punct", "punct", "word", "word"], "pos": ["APPR", "NN", "$(", "$.", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "warf sich das Ergl\u00fchende der Fr\u00fcchte", "tokens": ["warf", "sich", "das", "Er\u00b7gl\u00fc\u00b7hen\u00b7de", "der", "Fr\u00fcch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "immer wieder an das blaue Meer:", "tokens": ["im\u00b7mer", "wie\u00b7der", "an", "das", "blau\u00b7e", "Meer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "als das langsame Orangen-Boot", "tokens": ["als", "das", "lang\u00b7sa\u00b7me", "O\u00b7ran\u00b7gen\u00b7Boot"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "sie vor\u00fcbertrug bis an das gro\u00dfe", "tokens": ["sie", "vor\u00b7\u00fc\u00b7bert\u00b7rug", "bis", "an", "das", "gro\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "graue Schiff, zu dem, von Sto\u00df zu Sto\u00dfe,", "tokens": ["grau\u00b7e", "Schiff", ",", "zu", "dem", ",", "von", "Sto\u00df", "zu", "Sto\u00b7\u00dfe", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "ART", "$,", "APPR", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "andre Boote Fische hoben, Brot, \u2013", "tokens": ["and\u00b7re", "Boo\u00b7te", "Fi\u00b7sche", "ho\u00b7ben", ",", "Brot", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["ADJA", "NN", "NN", "VVFIN", "$,", "NN", "$,", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "w\u00e4hrend es, voll Hohn, in seinem Schoo\u00dfe", "tokens": ["w\u00e4h\u00b7rend", "es", ",", "voll", "Hohn", ",", "in", "sei\u00b7nem", "Schoo\u00b7\u00dfe"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "ADJD", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Kohlen aufnahm, offen wie der Tod.", "tokens": ["Koh\u00b7len", "auf\u00b7nahm", ",", "of\u00b7fen", "wie", "der", "Tod", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Denk: da\u00df einer hei\u00df und gl\u00fchend fl\u00fcchte,", "tokens": ["Denk", ":", "da\u00df", "ei\u00b7ner", "hei\u00df", "und", "gl\u00fc\u00b7hend", "fl\u00fcch\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "KOUS", "ART", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "und die Sieger w\u00e4ren hinterher,", "tokens": ["und", "die", "Sie\u00b7ger", "w\u00e4\u00b7ren", "hin\u00b7ter\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "und auf einmal machte der", "tokens": ["und", "auf", "ein\u00b7mal", "mach\u00b7te", "der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADV", "VVFIN", "ART"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Fl\u00fcchtende kurz, unerwartet, Kehr", "tokens": ["Fl\u00fcch\u00b7ten\u00b7de", "kurz", ",", "un\u00b7er\u00b7war\u00b7tet", ",", "Kehr"], "token_info": ["word", "word", "punct", "word", "punct