{"dta.poem.1024": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "An mein Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1618", "urn": "urn:nbn:de:kobv:b4-20628-9", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Molan/ B\u00fcchlein/ du must es wagen/", "tokens": ["Mo\u00b7lan", "/", "B\u00fcch\u00b7lein", "/", "du", "must", "es", "wa\u00b7gen", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "NN", "$(", "PPER", "VMFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zeuch hinau\u00df mit getrostem muht:", "tokens": ["Zeuch", "hin\u00b7au\u00df", "mit", "ge\u00b7tros\u00b7tem", "muht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APZR", "APPR", "ADJA", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Weil vnser gewissen gantz gut/", "tokens": ["Weil", "vn\u00b7ser", "ge\u00b7wis\u00b7sen", "gantz", "gut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "ADV", "ADJD", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "So gilt es gleich was man wirt sagen.", "tokens": ["So", "gilt", "es", "gleich", "was", "man", "wirt", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PWS", "PIS", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Deem\u00fchtig-k\u00fcssend zu begr\u00fcessen", "tokens": ["De\u00b7e\u00b7m\u00fch\u00b7tig\u00b7k\u00fcs\u00b7send", "zu", "be\u00b7gr\u00fces\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "PTKZU", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Einer m\u00e4chtigen G\u00f6ttin hand/", "tokens": ["Ei\u00b7ner", "m\u00e4ch\u00b7ti\u00b7gen", "G\u00f6t\u00b7tin", "hand", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Soll Dich Mi\u00dfgunst vnd Vnverstand", "tokens": ["Soll", "Dich", "Mi\u00df\u00b7gunst", "vnd", "Vn\u00b7ver\u00b7stand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weder verhindern noch verdriessen.", "tokens": ["We\u00b7der", "ver\u00b7hin\u00b7dern", "noch", "ver\u00b7dries\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Gefallen solt du gar nicht allen/", "tokens": ["Ge\u00b7fal\u00b7len", "solt", "du", "gar", "nicht", "al\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "PTKNEG", "PIAT", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vihlen gefallen ist zuvihl:", "tokens": ["Vih\u00b7len", "ge\u00b7fal\u00b7len", "ist", "zu\u00b7vihl", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VAFIN", "ADV", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Hast also dein gewisses spihl/", "tokens": ["Hast", "al\u00b7so", "dein", "ge\u00b7wis\u00b7ses", "spihl", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das du wenigen wirst gefallen.", "tokens": ["Das", "du", "we\u00b7ni\u00b7gen", "wirst", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Diser (wie wenig jhrer seyen)", "tokens": ["Di\u00b7ser", "(", "wie", "we\u00b7nig", "jhrer", "se\u00b7yen", ")"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PWAV", "PIS", "PPOSAT", "VVINF", "$("], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Seind gar gnug/ weil Sie gut vnd fromb;", "tokens": ["Seind", "gar", "gnug", "/", "weil", "Sie", "gut", "vnd", "fromb", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "$(", "KOUS", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vnd Sie/ bey denen du willkomb/", "tokens": ["Vnd", "Sie", "/", "bey", "de\u00b7nen", "du", "will\u00b7komb", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$(", "APPR", "PRELS", "PPER", "VMFIN", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Werden dich schon vor vnfall freyhen.", "tokens": ["Wer\u00b7den", "dich", "schon", "vor", "vn\u00b7fall", "frey\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wan du nu/ so wol aufgenommen/", "tokens": ["Wan", "du", "nu", "/", "so", "wol", "auf\u00b7ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "$(", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auch vor andern k\u00e4mest herf\u00fcr/", "tokens": ["Auch", "vor", "an\u00b7dern", "k\u00e4\u00b7mest", "her\u00b7f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIS", "VVFIN", "ADV", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "So wollen fr\u00f6lich bald nach dir", "tokens": ["So", "wol\u00b7len", "fr\u00f6\u00b7lich", "bald", "nach", "dir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADJD", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mehr sch\u00f6nere geschwistrigt kommen.", "tokens": ["Mehr", "sch\u00f6\u00b7ne\u00b7re", "ge\u00b7schwist\u00b7rigt", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}