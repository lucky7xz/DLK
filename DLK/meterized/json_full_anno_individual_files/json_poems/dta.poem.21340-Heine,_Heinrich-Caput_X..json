{"dta.poem.21340": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput X.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dicht hinter Hagen ward es Nacht,", "tokens": ["Dicht", "hin\u00b7ter", "Ha\u00b7gen", "ward", "es", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NE", "VAFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ich f\u00fchlte in den Ged\u00e4rmen", "tokens": ["Und", "ich", "f\u00fchl\u00b7te", "in", "den", "Ge\u00b7d\u00e4r\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "--+-++-+-", "measure": "anapaest.init"}, "line.3": {"text": "Ein seltsames Fr\u00f6steln. Ich konnte mich erst", "tokens": ["Ein", "selt\u00b7sa\u00b7mes", "Fr\u00f6s\u00b7teln", ".", "Ich", "konn\u00b7te", "mich", "erst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "VMFIN", "PRF", "ADV"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Zu Unna, im Wirthshaus, erw\u00e4rmen.", "tokens": ["Zu", "Un\u00b7na", ",", "im", "Wirths\u00b7haus", ",", "er\u00b7w\u00e4r\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NE", "$,", "APPRART", "NN", "$,", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}