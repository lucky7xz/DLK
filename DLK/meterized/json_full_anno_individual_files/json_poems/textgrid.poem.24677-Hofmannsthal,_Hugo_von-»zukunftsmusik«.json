{"textgrid.poem.24677": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "\u00bbzukunftsmusik\u00ab", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heiligen Mitleids rauschende Wellen,", "tokens": ["Hei\u00b7li\u00b7gen", "Mit\u00b7leids", "rau\u00b7schen\u00b7de", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Klingend an jegliches Herze sie schlagen;", "tokens": ["Klin\u00b7gend", "an", "jeg\u00b7li\u00b7ches", "Her\u00b7ze", "sie", "schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PIS", "VVFIN", "PPER", "VVINF", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Worte sind Formeln, die k\u00f6nnens nicht sagen,", "tokens": ["Wor\u00b7te", "sind", "For\u00b7meln", ",", "die", "k\u00f6n\u00b7nens", "nicht", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$,", "PRELS", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "K\u00f6nnen nicht fassen die Geister, die hellen.", "tokens": ["K\u00f6n\u00b7nen", "nicht", "fas\u00b7sen", "die", "Geis\u00b7ter", ",", "die", "hel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "VVFIN", "ART", "NN", "$,", "PRELS", "VVINF", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Frei sind die Seelen, zu jubeln, zu klagen,", "tokens": ["Frei", "sind", "die", "See\u00b7len", ",", "zu", "ju\u00b7beln", ",", "zu", "kla\u00b7gen", ","], "tok