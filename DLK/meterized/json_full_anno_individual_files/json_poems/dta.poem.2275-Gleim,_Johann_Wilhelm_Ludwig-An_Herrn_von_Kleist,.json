{"dta.poem.2275": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "An Herrn von Kleist,", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905198996", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie lieblich sprudelt diese Quelle!", "tokens": ["Wie", "lieb\u00b7lich", "spru\u00b7delt", "die\u00b7se", "Quel\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie sanft k\u00fc\u00dft mich der West im Gaukeln!", "tokens": ["Wie", "sanft", "k\u00fc\u00dft", "mich", "der", "West", "im", "Gau\u00b7keln", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie reitzend schwebt das Laub im Schatten!", "tokens": ["Wie", "reit\u00b7zend", "schwebt", "das", "Laub", "im", "Schat\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie fruchtbar bl\u00fcht die Lind am Ufer!", "tokens": ["Wie", "frucht\u00b7bar", "bl\u00fcht", "die", "Lind", "am", "U\u00b7fer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie munter steht das Thal voll Blumen!", "tokens": ["Wie", "mun\u00b7ter", "steht", "das", "Thal", "voll", "Blu\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hier, Freund! Hier ist das Land des Friedens,", "tokens": ["Hier", ",", "Freund", "!", "Hier", "ist", "das", "Land", "des", "Frie\u00b7dens", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$.", "ADV", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Hier ist es gut, hier la\u00df uns wohnen,", "tokens": ["Hier", "ist", "es", "gut", ",", "hier", "la\u00df", "uns", "woh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "ADV", "VVIMP", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Hier la\u00df uns, fern von Stolz und Zeptern,", "tokens": ["Hier", "la\u00df", "uns", ",", "fern", "von", "Stolz", "und", "Zep\u00b7tern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "$,", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die kurze Lebenszeit verl\u00e4ngern;", "tokens": ["Die", "kur\u00b7ze", "Le\u00b7bens\u00b7zeit", "ver\u00b7l\u00e4n\u00b7gern", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Hier soll sie, frei von niedern Sorgen,", "tokens": ["Hier", "soll", "sie", ",", "frei", "von", "nie\u00b7dern", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "So sanft, wie dieser Bach, verfliessen.", "tokens": ["So", "sanft", ",", "wie", "die\u00b7ser", "Bach", ",", "ver\u00b7flies\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAV", "PDAT", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Hier darf kein Gold vor Narren gl\u00e4nzen,", "tokens": ["Hier", "darf", "kein", "Gold", "vor", "Nar\u00b7ren", "gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIAT", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Hier h\u00f6rt man keinen Muffel seufzen,", "tokens": ["Hier", "h\u00f6rt", "man", "kei\u00b7nen", "Muf\u00b7fel", "seuf\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Hier l\u00e4uft kein Kramer mit Gewichten,", "tokens": ["Hier", "l\u00e4uft", "kein", "Kra\u00b7mer", "mit", "Ge\u00b7wich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Hier rast kein Menzel mit Husaren,", "tokens": ["Hier", "rast", "kein", "Men\u00b7zel", "mit", "Hu\u00b7sa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Hier wafnet sich kein Held zum Morden,", "tokens": ["Hier", "waf\u00b7net", "sich", "kein", "Held", "zum", "Mor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Hier soll uns kein erz\u00fcrnter Priester", "tokens": ["Hier", "soll", "uns", "kein", "er\u00b7z\u00fcrn\u00b7ter", "Pries\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Kein Freigeist und kein Quaker \u00e4rgern.", "tokens": ["Kein", "Frei\u00b7geist", "und", "kein", "Qua\u00b7ker", "\u00e4r\u00b7gern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Hier sind wir einsam, fromm und stille,", "tokens": ["Hier", "sind", "wir", "ein\u00b7sam", ",", "fromm", "und", "stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Hier schw\u00e4rmen keine schwarze Sorgen,", "tokens": ["Hier", "schw\u00e4r\u00b7men", "kei\u00b7ne", "schwar\u00b7ze", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Hier h\u00f6rt man kein Geschrei der Laster,", "tokens": ["Hier", "h\u00f6rt", "man", "kein", "Ge\u00b7schrei", "der", "Las\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Hier brennt kein Schwefel in der H\u00f6lle,", "tokens": ["Hier", "brennt", "kein", "Schwe\u00b7fel", "in", "der", "H\u00f6l\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Hier br\u00fcllt kein Teufel, wie ein L\u00f6we.", "tokens": ["Hier", "br\u00fcllt", "kein", "Teu\u00b7fel", ",", "wie", "ein", "L\u00f6\u00b7we", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,", "PWAV", "ART", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Hier wollen wir uns H\u00fctten bauen,", "tokens": ["Hier", "wol\u00b7len", "wir", "uns", "H\u00fct\u00b7ten", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Damit die Tugend sicher wohne;", "tokens": ["Da\u00b7mit", "die", "Tu\u00b7gend", "si\u00b7cher", "woh\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Hier sei mein Herz ihr froher Tempel,", "tokens": ["Hier", "sei", "mein", "Herz", "ihr", "fro\u00b7her", "Tem\u00b7pel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Hier wi\u00df es nichts von Furcht und Sorgen,", "tokens": ["Hier", "wi\u00df", "es", "nichts", "von", "Furcht", "und", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Hier wollen wir der Freundschaft opfern,", "tokens": ["Hier", "wol\u00b7len", "wir", "der", "Freund\u00b7schaft", "op\u00b7fern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Hier wollen wir den Himmel loben,", "tokens": ["Hier", "wol\u00b7len", "wir", "den", "Him\u00b7mel", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Ihn loben, aber ihn nichts bitten.", "tokens": ["Ihn", "lo\u00b7ben", ",", "a\u00b7ber", "ihn", "nichts", "bit\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "KON", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Hier wollen wir uns kennen lernen,", "tokens": ["Hier", "wol\u00b7len", "wir", "uns", "ken\u00b7nen", "ler\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Hier wollen wir am Ufer trinken,", "tokens": ["Hier", "wol\u00b7len", "wir", "am", "U\u00b7fer", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Und trinkend scherzen, und uns k\u00fcssen.", "tokens": ["Und", "trin\u00b7kend", "scher\u00b7zen", ",", "und", "uns", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "$,", "KON", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Was fehlt der F\u00fclle solcher Wonne?", "tokens": ["Was", "fehlt", "der", "F\u00fcl\u00b7le", "sol\u00b7cher", "Won\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Ach Freund, es fehlt uns noch die Liebe.", "tokens": ["Ach", "Freund", ",", "es", "fehlt", "uns", "noch", "die", "Lie\u00b7be", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Geh! hole du dein blondes M\u00e4dchen,", "tokens": ["Geh", "!", "ho\u00b7le", "du", "dein", "blon\u00b7des", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Ich will die braune Doris holen.", "tokens": ["Ich", "will", "die", "brau\u00b7ne", "Do\u00b7ris", "ho\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NE", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}