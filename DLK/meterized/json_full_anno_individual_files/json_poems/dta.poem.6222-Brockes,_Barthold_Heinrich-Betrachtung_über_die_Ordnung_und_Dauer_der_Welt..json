{"dta.poem.6222": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung \u00fcber die Ordnung  \n und Dauer der Welt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ja, es erh\u00f6hen sich und fliegen", "tokens": ["Ja", ",", "es", "er\u00b7h\u00f6\u00b7hen", "sich", "und", "flie\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PRF", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Edlen Geister, fort in ungehemmtem Lauff.", "tokens": ["Die", "Ed\u00b7len", "Geis\u00b7ter", ",", "fort", "in", "un\u00b7ge\u00b7hemm\u00b7tem", "Lauff", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PTKVZ", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie steigen auf so hohe Stiegen", "tokens": ["Sie", "stei\u00b7gen", "auf", "so", "ho\u00b7he", "Stie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bis zu dem Thron des SCHOEPFFERS selbst hinauf;", "tokens": ["Bis", "zu", "dem", "Thron", "des", "ScHOE\u00b7PF\u00b7FERS", "selbst", "hin\u00b7auf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "---+-++-+-+", "measure": "unknown.measure.penta"}, "line.5": {"text": "Da, wo man Geister sieht in einem hellen Schein,", "tokens": ["Da", ",", "wo", "man", "Geis\u00b7ter", "sieht", "in", "ei\u00b7nem", "hel\u00b7len", "Schein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PIS", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die unmaterialisch seyn.", "tokens": ["Die", "un\u00b7ma\u00b7te\u00b7ri\u00b7a\u00b7lisch", "seyn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wo ein begreifflich Licht, in ewig\u2019heller Pracht,", "tokens": ["Wo", "ein", "be\u00b7greif\u00b7flich", "Licht", ",", "in", "e\u00b7wig'\u00b7hel\u00b7ler", "Pracht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJD", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Wesen dauerhafft, die Tage ewig macht.", "tokens": ["Die", "We\u00b7sen", "dau\u00b7er\u00b7hafft", ",", "die", "Ta\u00b7ge", "e\u00b7wig", "macht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}