_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sieh, da tr\u00f6pfelt's auf die Nase \u2013", "tokens": ["Sieh", ",", "da", "tr\u00f6p\u00b7felt's", "auf", "die", "Na\u00b7se", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "NE", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Spute sich, wer laufen mag!", "tokens": ["Spu\u00b7te", "sich", ",", "wer", "lau\u00b7fen", "mag", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "PWS", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und dem kurzen Morgenspa\u00dfe", "tokens": ["Und", "dem", "kur\u00b7zen", "Mor\u00b7gens\u00b7pa\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Folgt ein langer Regentag.", "tokens": ["Folgt", "ein", "lan\u00b7ger", "Re\u00b7gen\u00b7tag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}