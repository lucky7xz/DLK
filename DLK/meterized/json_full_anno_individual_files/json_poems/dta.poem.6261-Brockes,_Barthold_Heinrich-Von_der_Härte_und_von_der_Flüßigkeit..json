{"dta.poem.6261": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der H\u00e4rte und von der  \n Fl\u00fc\u00dfigkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein C\u00f6rper, welcher das, worauf er trifft, verletzt,", "tokens": ["Ein", "C\u00f6r\u00b7per", ",", "wel\u00b7cher", "das", ",", "wo\u00b7rauf", "er", "trifft", ",", "ver\u00b7letzt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PDS", "$,", "PWAV", "PPER", "VVFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und was er r\u00fchrt, durchdringt, das Eisen nemlich, m\u00fcsse", "tokens": ["Und", "was", "er", "r\u00fchrt", ",", "durch\u00b7dringt", ",", "das", "Ei\u00b7sen", "nem\u00b7lich", ",", "m\u00fcs\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "VVFIN", "$,", "ART", "NN", "ADV", "$,", "VMFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zusammen seyn gesetzt,", "tokens": ["Zu\u00b7sam\u00b7men", "seyn", "ge\u00b7setzt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}