{"dta.poem.9434": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "IiI .  \n Die dritte Abtheilung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Umb Salomons Bette stehn sechzig ger\u00fcstet/", "tokens": ["Umb", "Sa\u00b7lo\u00b7mons", "Bet\u00b7te", "stehn", "sech\u00b7zig", "ge\u00b7r\u00fcs\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NE", "NN", "VVFIN", "CARD", "VVPP", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Von allen den St\u00e4rcksten aus seinem geschlecht/", "tokens": ["Von", "al\u00b7len", "den", "St\u00e4rcks\u00b7ten", "aus", "sei\u00b7nem", "ge\u00b7schlecht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "NN", "APPR", "PPOSAT", "VVPP", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Mit R\u00fcstung versehen wie einem gel\u00fcstet/", "tokens": ["Mit", "R\u00fcs\u00b7tung", "ver\u00b7se\u00b7hen", "wie", "ei\u00b7nem", "ge\u00b7l\u00fcs\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "KOKOM", "ART", "VVPP", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ein jeder s\u00fchrt Lantzen und Schwerter mit recht.", "tokens": ["Ein", "je\u00b7der", "s\u00fchrt", "Lant\u00b7zen", "und", "Schwer\u00b7ter", "mit", "recht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "NN", "KON", "NN", "APPR", "ADJD", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Sie sch\u00fctzen den K\u00f6nig des nachtes und wachen/", "tokens": ["Sie", "sch\u00fct\u00b7zen", "den", "K\u00f6\u00b7nig", "des", "nach\u00b7tes", "und", "wa\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "KON", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Wenn er sich erquicket mit seinem Gemahl/", "tokens": ["Wenn", "er", "sich", "er\u00b7quic\u00b7ket", "mit", "sei\u00b7nem", "Ge\u00b7mahl", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Der K\u00f6nig lie\u00df Pfeiler von Cedern-holtz machen/", "tokens": ["Der", "K\u00f6\u00b7nig", "lie\u00df", "Pfei\u00b7ler", "von", "Ce\u00b7dern\u00b7holtz", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "APPR", "NN", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.8": {"text": "und zierte mit S\u00e4nfften den F\u00fcrstlichen Saal/", "tokens": ["und", "zier\u00b7te", "mit", "S\u00e4nff\u00b7ten", "den", "F\u00fcrst\u00b7li\u00b7chen", "Saal", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.9": {"text": "Das K\u00fcssen der S\u00e4nsre war Purpur und Seide/", "tokens": ["Das", "K\u00fcs\u00b7sen", "der", "S\u00e4ns\u00b7re", "war", "Pur\u00b7pur", "und", "Sei\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "NN", "KON", "NE", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.10": {"text": "Die Seulen aus Silber/ die Decke verg\u00fcldt/", "tokens": ["Die", "Seu\u00b7len", "aus", "Sil\u00b7ber", "/", "die", "De\u00b7cke", "ver\u00b7g\u00fcldt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$(", "ART", "NN", "VVPP", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.11": {"text": "Der Boden war lieblich an diese", "tokens": ["Der", "Bo\u00b7den", "war", "lieb\u00b7lich", "an", "die\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPR", "PDAT"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Mit Liebe den T\u00f6chtern von Zi\u00f6n erfullt", "tokens": ["Mit", "Lie\u00b7be", "den", "T\u00f6ch\u00b7tern", "von", "Zi\u00b7\u00f6n", "er\u00b7fullt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN", "APPR", "NE", "VVFIN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Jhr T\u00f6chter von Solyme sehet das prangen/", "tokens": ["Ihr", "T\u00f6ch\u00b7ter", "von", "So\u00b7ly\u00b7me", "se\u00b7het", "das", "pran\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VVFIN", "PDS", "VVFIN", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Kom\u0303t/ sehet des K\u00f6niges herrliche Zier/", "tokens": ["Kom\u0303t", "/", "se\u00b7het", "des", "K\u00f6\u00b7ni\u00b7ges", "herr\u00b7li\u00b7che", "Zier", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "VVFIN", "ART", "NN", "ADJA", "NN", "$("], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.15": {"text": "Die Zierde/ die Salomo selbsten empfangen", "tokens": ["Die", "Zier\u00b7de", "/", "die", "Sa\u00b7lo\u00b7mo", "selbs\u00b7ten", "emp\u00b7fan\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ART", "NE", "ADV", "VVPP"], "meter": "-+--++-+--+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Am tage der Wollust und freuden allhier;", "tokens": ["Am", "ta\u00b7ge", "der", "Wol\u00b7lust", "und", "freu\u00b7den", "all\u00b7hier", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "KON", "NN", "ADV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.17": {"text": "Ach! sehet auf Salomons Heupte die Krone/", "tokens": ["Ach", "!", "se\u00b7het", "auf", "Sa\u00b7lo\u00b7mons", "Heup\u00b7te", "die", "Kro\u00b7ne", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "APPR", "NE", "NN", "ART", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.18": {"text": "Die Krone von Silber und Golde gestickt/", "tokens": ["Die", "Kro\u00b7ne", "von", "Sil\u00b7ber", "und", "Gol\u00b7de", "ge\u00b7stickt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.19": {"text": "Die jtzo die Mutter dem Liebesten Sohne", "tokens": ["Die", "jt\u00b7zo", "die", "Mut\u00b7ter", "dem", "Lie\u00b7bes\u00b7ten", "Soh\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.20": {"text": "Zu seinen Hochzeitlichen Ehren geschickt.", "tokens": ["Zu", "sei\u00b7nen", "Hoch\u00b7zeit\u00b7li\u00b7chen", "Eh\u00b7ren", "ge\u00b7schickt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+---+--+", "measure": "iambic.tetra.chol"}}}}}