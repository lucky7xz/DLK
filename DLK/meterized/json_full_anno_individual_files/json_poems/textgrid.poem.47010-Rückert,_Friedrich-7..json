{"textgrid.poem.47010": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O k\u00f6nnt' ich doch mit einem Schlag zerbrechen", "tokens": ["O", "k\u00f6nnt'", "ich", "doch", "mit", "ei\u00b7nem", "Schlag", "zer\u00b7bre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PPER", "ADV", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "All das Ger\u00e4te, das zu meinem Schaden", "tokens": ["All", "das", "Ge\u00b7r\u00e4\u00b7te", ",", "das", "zu", "mei\u00b7nem", "Scha\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ersonnen ist, die Hacken, Hauen, Spaten,", "tokens": ["Er\u00b7son\u00b7nen", "ist", ",", "die", "Ha\u00b7cken", ",", "Hau\u00b7en", ",", "Spa\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "ART", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Schaufeln, Gabeln, Sensen, Sicheln, Rechen.", "tokens": ["Die", "Schau\u00b7feln", ",", "Ga\u00b7beln", ",", "Sen\u00b7sen", ",", "Si\u00b7cheln", ",", "Re\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die plumpen, die sich jetzt so oft erfrechen,", "tokens": ["Die", "plum\u00b7pen", ",", "die", "sich", "jetzt", "so", "oft", "er\u00b7fre\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "ADV", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Arme meines M\u00e4dchens zu beladen,", "tokens": ["Die", "Ar\u00b7me", "mei\u00b7nes", "M\u00e4d\u00b7chens", "zu", "be\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo drau\u00dfen Regenstr\u00f6m' ihr Haar bald baden,", "tokens": ["Wo", "drau\u00b7\u00dfen", "Re\u00b7gen\u00b7str\u00f6m'", "ihr", "Haar", "bald", "ba\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Mittags Gluten bald ihr Antlitz stechen;", "tokens": ["Des", "Mit\u00b7tags", "Glu\u00b7ten", "bald", "ihr", "Ant\u00b7litz", "ste\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Derweil ich traurig sitze wie im Bauer", "tokens": ["Der\u00b7weil", "ich", "trau\u00b7rig", "sit\u00b7ze", "wie", "im", "Bau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "KOKOM", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Gimpel, der entbehrt sein t\u00e4glich Futter,", "tokens": ["Der", "Gim\u00b7pel", ",", "der", "ent\u00b7behrt", "sein", "t\u00e4g\u00b7lich", "Fut\u00b7ter", ","], "token_info": ["word", "word", 