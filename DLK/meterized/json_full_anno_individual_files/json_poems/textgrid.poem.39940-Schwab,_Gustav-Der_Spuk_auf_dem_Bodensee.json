{"textgrid.poem.39940": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Der Spuk auf dem Bodensee", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einst sang ich von dem Reiter, der \u00fcber Eis und Schnee", "tokens": ["Einst", "sang", "ich", "von", "dem", "Rei\u00b7ter", ",", "der", "\u00fc\u00b7ber", "Eis", "und", "Schnee"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "PRELS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Hinflog in vollem Trabe wohl durch den Bodensee,", "tokens": ["Hin\u00b7flog", "in", "vol\u00b7lem", "Tra\u00b7be", "wohl", "durch", "den", "Bo\u00b7den\u00b7see", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und dr\u00fcben angekommen, erst von der Kunde krank,", "tokens": ["Und", "dr\u00fc\u00b7ben", "an\u00b7ge\u00b7kom\u00b7men", ",", "erst", "von", "der", "Kun\u00b7de", "krank", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "$,", "ADV", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Auf gutem, festen Boden vom Pferde sterbend sank.", "tokens": ["Auf", "gu\u00b7tem", ",", "fes\u00b7ten", "Bo\u00b7den", "vom", "Pfer\u00b7de", "ster\u00b7bend", "sank", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "APPRART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Nun h\u00f6ret neue Wunder: der See ist wieder zu,", "tokens": ["Nun", "h\u00f6\u00b7ret", "neu\u00b7e", "Wun\u00b7der", ":", "der", "See", "ist", "wie\u00b7der", "zu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "$.", "ART", "NN", "VAFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Auf uferloser Fl\u00e4che wohnt stumme Grabesruh',", "tokens": ["Auf", "u\u00b7fer\u00b7lo\u00b7ser", "Fl\u00e4\u00b7che", "wohnt", "stum\u00b7me", "Gra\u00b7bes\u00b7ruh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wie Schafe gehn zur Weide die Nebel wollicht, bleich,", "tokens": ["Wie", "Scha\u00b7fe", "gehn", "zur", "Wei\u00b7de", "die", "Ne\u00b7bel", "wol\u00b7licht", ",", "bleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "VMFIN", "$,", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Es liegt der Mond in Stralen, gem\u00e4htem Grase gleich.", "tokens": ["Es", "liegt", "der", "Mond", "in", "Stra\u00b7len", ",", "ge\u00b7m\u00e4h\u00b7tem", "Gra\u00b7se", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Sonst pfiff der Wind im Segel, der Vogel sang im Blau,", "tokens": ["Sonst", "pfiff", "der", "Wind", "im", "Se\u00b7gel", ",", "der", "Vo\u00b7gel", "sang", "im", "Blau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$,", "ART", "NE", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die Hechte sandten pl\u00e4tschernd empor der Woge Thau;", "tokens": ["Die", "Hech\u00b7te", "sand\u00b7ten", "pl\u00e4t\u00b7schernd", "em\u00b7por", "der", "Wo\u00b7ge", "Thau", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "ART", "NN", "NN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Jetzt hat die bange W\u00fcste, die starre, keinen Mund,", "tokens": ["Jetzt", "hat", "die", "ban\u00b7ge", "W\u00fcs\u00b7te", ",", "die", "star\u00b7re", ",", "kei\u00b7nen", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "VVFIN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der Vogel fiel erfroren, die Welle schl\u00e4ft im Grund. \u2013", "tokens": ["Der", "Vo\u00b7gel", "fiel", "er\u00b7fro\u00b7ren", ",", "die", "Wel\u00b7le", "schl\u00e4ft", "im", "Grund", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NE", "VVFIN", "VVINF", "$,", "ART", "NN", "VVFIN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Was jagt in schnellem Sturme die Nebelwolken auf?", "tokens": ["Was", "jagt", "in", "schnel\u00b7lem", "Stur\u00b7me", "die", "Ne\u00b7bel\u00b7wol\u00b7ken", "auf", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Was auf des Eises Estrich ert\u00f6nt wie Rosses Lauf?", "tokens": ["Was", "auf", "des", "Ei\u00b7ses", "Est\u00b7rich", "er\u00b7t\u00f6nt", "wie", "Ros\u00b7ses", "Lauf", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "PPER", "VVFIN", "KOKOM", "NE", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Was fliegt mit Peitschenknalle heran? der Duft zerrei\u00dft:", "tokens": ["Was", "fliegt", "mit", "Peit\u00b7schen\u00b7knal\u00b7le", "he\u00b7ran", "?", "der", "Duft", "zer\u00b7rei\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "NN", "PTKVZ", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ein Reiter eilt vor\u00fcber \u2013 ist es des Toten Geist?", "tokens": ["Ein", "Rei\u00b7ter", "eilt", "vor\u00b7\u00fc\u00b7ber", "\u2013", "ist", "es", "des", "To\u00b7ten", "Geist", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$(", "VAFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Und kaum ist er verschwunden, in Duft und Luft getaucht,", "tokens": ["Und", "kaum", "ist", "er", "ver\u00b7schwun\u00b7den", ",", "in", "Duft", "und", "Luft", "ge\u00b7taucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVPP", "$,", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Schon wieder blinkt's im Dunste, der mondbeschienen raucht;", "tokens": ["Schon", "wie\u00b7der", "blinkt's", "im", "Duns\u00b7te", ",", "der", "mond\u00b7be\u00b7schie\u00b7nen", "raucht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "APPRART", "ADJA", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Es trabt, es rollt, es wiehert \u2013 ein Schlitten kommt heran,", "tokens": ["Es", "trabt", ",", "es", "rollt", ",", "es", "wie\u00b7hert", "\u2013", "ein", "Schlit\u00b7ten", "kommt", "he\u00b7ran", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$(", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Vier schwarze Rosse rennen mit ihm auf glatter Bahn.", "tokens": ["Vier", "schwar\u00b7ze", "Ros\u00b7se", "ren\u00b7nen", "mit", "ihm", "auf", "glat\u00b7ter", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VVFIN", "APPR", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "In gr\u00fcnem Kleid ein Gro\u00dfer, sein Bart hat roten Schein,", "tokens": ["In", "gr\u00fc\u00b7nem", "Kleid", "ein", "Gro\u00b7\u00dfer", ",", "sein", "Bart", "hat", "ro\u00b7ten", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$,", "PPOSAT", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "In schwarzem Rock ein Kleiner, schwarzaugig, bleich und fein,", "tokens": ["In", "schwar\u00b7zem", "Rock", "ein", "Klei\u00b7ner", ",", "schwarz\u00b7au\u00b7gig", ",", "bleich", "und", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ein dritter, dicht verh\u00fcllet, und eine zarte Frau,", "tokens": ["Ein", "drit\u00b7ter", ",", "dicht", "ver\u00b7h\u00fcl\u00b7let", ",", "und", "ei\u00b7ne", "zar\u00b7te", "Frau", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJD", "VVFIN", "$,", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Doch Alles schnell verschwindet im Nebel breit und grau.", "tokens": ["Doch", "Al\u00b7les", "schnell", "ver\u00b7schwin\u00b7det", "im", "Ne\u00b7bel", "breit", "und", "grau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADJD", "VVFIN", "APPRART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Und auf dem Eismeer lagert sich Stille wie zuvor,", "tokens": ["Und", "auf", "dem", "Eis\u00b7meer", "la\u00b7gert", "sich", "Stil\u00b7le", "wie", "zu\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PRF", "NN", "KOKOM", "ADV", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "In Osten th\u00fcrmt sich riesig die Nebelwand empor;", "tokens": ["In", "Os\u00b7ten", "th\u00fcrmt", "sich", "rie\u00b7sig", "die", "Ne\u00b7bel\u00b7wand", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PRF", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Kein Klang und keine Farbe, bis bla\u00df der Morgen graut", "tokens": ["Kein", "Klang", "und", "kei\u00b7ne", "Far\u00b7be", ",", "bis", "bla\u00df", "der", "Mor\u00b7gen", "graut"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "$,", "KOUS", "ADJD", "ART", "NN", "VVFIN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und auf der toten Ebne nur Eis und Wolken schaut.", "tokens": ["Und", "auf", "der", "to\u00b7ten", "Eb\u00b7ne", "nur", "Eis", "und", "Wol\u00b7ken", "schaut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ADV", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "\u00bbso leg' uns doch, o S\u00e4nger, das w\u00fcste Traumbild aus.", "tokens": ["\u00bb", "so", "leg'", "uns", "doch", ",", "o", "S\u00e4n\u00b7ger", ",", "das", "w\u00fcs\u00b7te", "Traum\u00b7bild", "aus", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ADV", "$,", "FM", "NN", "$,", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Was f\u00fcr Gespenster bringst du in kalter N\u00e4chte Graus?", "tokens": ["Was", "f\u00fcr", "Ge\u00b7spens\u00b7ter", "bringst", "du", "in", "kal\u00b7ter", "N\u00e4ch\u00b7te", "Graus", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "F\u00fcr welche S\u00fcnde wallen sie hier durch Schreckensnot, \u2013", "tokens": ["F\u00fcr", "wel\u00b7che", "S\u00fcn\u00b7de", "wal\u00b7len", "sie", "hier", "durch", "Schre\u00b7ckens\u00b7not", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PWAT", "NN", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und wagen auf dem Eise, schon tot, den zweiten Tod?\u00ab", "tokens": ["Und", "wa\u00b7gen", "auf", "dem", "Ei\u00b7se", ",", "schon", "tot", ",", "den", "zwei\u00b7ten", "Tod", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,", "ADV", "ADJD", "$,", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Gespenster? ei, wer sagte, da\u00df es Gespenster sind?", "tokens": ["Ge\u00b7spens\u00b7ter", "?", "ei", ",", "wer", "sag\u00b7te", ",", "da\u00df", "es", "Ge\u00b7spens\u00b7ter", "sind", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NE", "$,", "PWS", "VVFIN", "$,", "KOUS", "PPER", "NN", "VAFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Meint ihr, mit alten M\u00e4ren erschreck' ich Weib und Kind?", "tokens": ["Meint", "ihr", ",", "mit", "al\u00b7ten", "M\u00e4\u00b7ren", "er\u00b7schreck'", "ich", "Weib", "und", "Kind", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Was euch mein Lied berichtet, geschah in diesem Jahr,", "tokens": ["Was", "euch", "mein", "Lied", "be\u00b7rich\u00b7tet", ",", "ge\u00b7schah", "in", "die\u00b7sem", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Am ersten, hellen Sonntag im strengen Februar.", "tokens": ["Am", "ers\u00b7ten", ",", "hel\u00b7len", "Sonn\u00b7tag", "im", "stren\u00b7gen", "Feb\u00b7ru\u00b7ar", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "Die vier geschwinden Rappen sind keine H\u00f6llenbrut,", "tokens": ["Die", "vier", "ge\u00b7schwin\u00b7den", "Rap\u00b7pen", "sind", "kei\u00b7ne", "H\u00f6l\u00b7len\u00b7brut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "ADJA", "NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Zu Immenstadt im Stalle dort stehn sie ausgeruht,", "tokens": ["Zu", "Im\u00b7men\u00b7stadt", "im", "Stal\u00b7le", "dort", "stehn", "sie", "aus\u00b7ge\u00b7ruht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPRART", "NN", "ADV", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Dort winkt der schmucke Schlitten, er liegt nicht in dem Grund,", "tokens": ["Dort", "winkt", "der", "schmu\u00b7cke", "S