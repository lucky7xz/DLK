pos": ["NN", "APPR", "APPR", "NN", "$.", "PWS", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Der hat die Fahrt bestellet, der sandte mir Bericht.", "tokens": ["Der", "hat", "die", "Fahrt", "be\u00b7stel\u00b7let", ",", "der", "sand\u00b7te", "mir", "Be\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "VVFIN", "$,", "PRELS", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.24": {"line.1": {"text": "Sie leben Alle fr\u00f6hlich, sie sind ein christlich Blut,", "tokens": ["Sie", "le\u00b7ben", "Al\u00b7le", "fr\u00f6h\u00b7lich", ",", "sie", "sind", "ein", "christ\u00b7lich", "Blut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADJD", "$,", "PPER", "VAFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Voran Herr Sepp, der gerne den Wandrern g\u00fctlich thut;", "tokens": ["Vo\u00b7ran", "Herr", "Sepp", ",", "der", "ger\u00b7ne", "den", "Wand\u00b7rern", "g\u00fct\u00b7lich", "thut", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "NE", "$,", "PRELS", "ADV", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Nur spricht man, da\u00df er heimlich nach manchem Schatze gr\u00e4bt,", "tokens": ["Nur", "spricht", "man", ",", "da\u00df", "er", "heim\u00b7lich", "nach", "man\u00b7chem", "Schat\u00b7ze", "gr\u00e4bt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "KOUS", "PPER", "ADJD", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+