{"textgrid.poem.55380": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "2", "genre": "verse", "period": "N.A.", "pub_year": 1816, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott sandte seinen rohen Kindern", "tokens": ["Gott", "sand\u00b7te", "sei\u00b7nen", "ro\u00b7hen", "Kin\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gesetz und Ordnung, Wissenschaft und Kunst,", "tokens": ["Ge\u00b7setz", "und", "Ord\u00b7nung", ",", "Wis\u00b7sen\u00b7schaft", "und", "Kunst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Begabte die mit aller Himmelsgunst,", "tokens": ["Be\u00b7gab\u00b7te", "die", "mit", "al\u00b7ler", "Him\u00b7mels\u00b7gunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Erde grasses Los zu mindern.", "tokens": ["Der", "Er\u00b7de", "gras\u00b7ses", "Los", "zu", "min\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie kamen nackt vom Himmel an", "tokens": ["Sie", "ka\u00b7men", "nackt", "vom", "Him\u00b7mel", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und wu\u00dften sich nicht zu benehmen;", "tokens": ["Und", "wu\u00df\u00b7ten", "sich", "nicht", "zu", "be\u00b7neh\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die Poesie zog ihnen Kleider an,", "tokens": ["Die", "Poe\u00b7sie", "zog", "ih\u00b7nen", "Klei\u00b7der", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und keine hatte sich zu sch\u00e4men.", "tokens": ["Und", "kei\u00b7ne", "hat\u00b7te", "sich", "zu", "sch\u00e4\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "VAFIN", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Gott sandte seinen rohen Kindern", "tokens": ["Gott", "sand\u00b7te", "sei\u00b7nen", "ro\u00b7hen", "Kin\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gesetz und Ordnung, Wissenschaft und Kunst,", "tokens": ["Ge\u00b7setz", "und", "Ord\u00b7nung", ",", "Wis\u00b7sen\u00b7schaft", "und", "Kunst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Begabte die mit aller Himmelsgunst,", "tokens": ["Be\u00b7gab\u00b7te", "die", "mit", "al\u00b7ler", "Him\u00b7mels\u00b7gunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Erde grasses Los zu mindern.", "tokens": ["Der", "Er\u00b7de", "gras\u00b7ses", "Los