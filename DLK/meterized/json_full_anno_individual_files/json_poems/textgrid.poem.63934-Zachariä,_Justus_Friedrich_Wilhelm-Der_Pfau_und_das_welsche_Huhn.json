: "iambic.tetra"}, "line.3": {"text": "Dem jungen Herrn ward viel erlaubt,", "tokens": ["Dem", "jun\u00b7gen", "Herrn", "ward", "viel", "er\u00b7laubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der manche Gunstbezeugung raubt,", "tokens": ["Der", "man\u00b7che", "Gunst\u00b7be\u00b7zeu\u00b7gung", "raubt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So da\u00df fast Jeder denkt, der Pfau", "tokens": ["So", "da\u00df", "fast", "Je\u00b7der", "denkt", ",", "der", "Pfau"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "KOUS", "ADV", "PIS", "VVFIN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und dieses Huhn sey Mann und Frau.", "tokens": ["Und", "die\u00b7ses", "Huhn", "sey", "Mann", "und", "Frau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In diesem angenehmen Wahn", "tokens": ["In", "die\u00b7sem", "an\u00b7ge\u00b7neh\u00b7men", "Wahn"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Kam pl\u00f6tzlich eine Pfauin an.", "tokens": ["Kam", "pl\u00f6tz\u00b7lich", "ei\u00b7ne", "Pfau\u00b7in", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Sie setzet stolz sich auf das Dach,", "tokens": ["Sie", "set\u00b7zet", "stolz", "sich", "auf", "das", "Dach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Schreit, und macht Alles um sich wach.", "tokens": ["Schreit", ",", "und", "macht", "Al\u00b7les", "um", "sich", "wach", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "VVFIN", "PIS", "APPR", "PRF", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.11": {"text": "Der Pfau vernimmt kaum, da\u00df sie ruft,", "tokens": ["Der", "Pfau", "ver\u00b7nimmt", "kaum", ",", "da\u00df", "sie", "ruft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "So schwinget er sich in die Luft,", "tokens": ["So", "schwin\u00b7get", "er", "sich", "in", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Eilt undankbar mit ihr davon,", "tokens": ["Eilt", "un\u00b7dank\u00b7bar", "mit", "ihr", "da\u00b7von", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPER", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Und Schande blieb des H\u00fchnchens Lohn.", "tokens": ["Und", "Schan\u00b7de", "blieb", "des", "H\u00fchn\u00b7chens", "Lohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "So machen's noch in unsern Tagen", "tokens": ["So", "ma\u00b7chen's", "noch", "in", "un\u00b7sern", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Herr'n, die Federh\u00fcte tragen.", "tokens": ["Die", "Herr'n", ",", "die", "Fe\u00b7der\u00b7h\u00fc\u00b7te", "tra\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie wissen's noch gar wohl, Madam,", "tokens": ["Sie", "wis\u00b7sen's", "noch", "gar", "wohl", ",", "Ma\u00b7dam", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "$,", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Wie Herr Baron von Hochblut kam,", "tokens": ["Wie", "Herr", "Ba\u00b7ron", "von", "Hoch\u00b7blut", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vorlieb gern nahm mit Karolinchen;", "tokens": ["Vor\u00b7lieb", "gern", "nahm", "mit", "Ka\u00b7ro\u00b7lin\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bis pl\u00f6tzlich Fr\u00e4ulein Edelinchen", "tokens": ["Bis", "pl\u00f6tz\u00b7lich", "Fr\u00e4u\u00b7lein", "E\u00b7del\u00b7in\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Erschien und ihr den Br\u00e4ut'gam stahl,", "tokens": ["Er\u00b7schien", "und", "ihr", "den", "Br\u00e4ut'\u00b7gam", "stahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "PPER", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und sich der Herr Baron empfahl.", "tokens": ["Und", "sich", "der", "Herr", "Ba\u00b7ron", "emp\u00b7fahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}