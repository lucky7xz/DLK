{"textgrid.poem.63934": {"metadata": {"author": {"name": "Zachari\u00e4, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Pfau und das welsche Huhn", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vom Edelhof, der ihn erzogen,", "tokens": ["Vom", "E\u00b7del\u00b7hof", ",", "der", "ihn", "er\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "War einst ein Pfau hinweggeflogen;", "tokens": ["War", "einst", "ein", "Pfau", "hin\u00b7weg\u00b7ge\u00b7flo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er wu\u00dfte nicht mehr, wo er war;", "tokens": ["Er", "wu\u00df\u00b7te", "nicht", "mehr", ",", "wo", "er", "war", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "$,", "PWAV", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zuletzt kam er nach viel Gefahr", "tokens": ["Zu\u00b7letzt", "kam", "er", "nach", "viel", "Ge\u00b7fahr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PIAT", "NN"], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Zu einer kleinen Meierei.", "tokens": ["Zu", "ei\u00b7ner", "klei\u00b7nen", "Mei\u00b7e\u00b7rei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hier l\u00e4uft gleich Jung und Alt herbei,", "tokens": ["Hier", "l\u00e4uft", "gleich", "Jung", "und", "Alt", "her\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NN", "KON", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und preist mit \u00fcberm\u00e4\u00df'ger Freude", "tokens": ["Und", "preist", "mit", "\u00fc\u00b7ber\u00b7m\u00e4\u00df'\u00b7ger", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Den fremden Herrn im sch\u00f6nen Kleide.", "tokens": ["Den", "frem\u00b7den", "Herrn", "im", "sch\u00f6\u00b7nen", "Klei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Man streut ihm reichlich Futter hin.", "tokens": ["Man", "streut", "ihm", "reich\u00b7lich", "Fut\u00b7ter", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADJD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die andern H\u00fchner sehen ihn", "tokens": ["Die", "an\u00b7dern", "H\u00fch\u00b7ner", "se\u00b7hen", "ihn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Mit heimlicher Bewund'rung an,", "tokens": ["Mit", "heim\u00b7li\u00b7cher", "Be\u00b7wun\u00b7d'\u00b7rung", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.12": {"text": "Und mit gar gro\u00dfem Neid der Hahn.", "tokens": ["Und", "mit", "gar", "gro\u00b7\u00dfem", "Neid", "der", "Hahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADV", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Dem Pfau gefiel es hier so ziemlich,", "tokens": ["Dem", "Pfau", "ge\u00b7fiel", "es", "hier", "so", "ziem\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Nur schien es seinem Stolz nicht r\u00fchmlich,", "tokens": ["Nur", "schien", "es", "sei\u00b7nem", "Stolz", "nicht", "r\u00fchm\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Da\u00df er, so artig, so galant,", "tokens": ["Da\u00df", "er", ",", "so", "ar\u00b7tig", ",", "so", "ga\u00b7lant", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Hier nichts f\u00fcr sich zu lieben fand.", "tokens": ["Hier", "nichts", "f\u00fcr", "sich", "zu", "lie\u00b7ben", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "APPR", "PRF", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Was kann nicht Langeweile thun?", "tokens": ["Was", "kann", "nicht", "Lan\u00b7ge\u00b7wei\u00b7le", "thun", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PTKNEG", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Ein niedlich junges, welsches Huhn", "tokens": ["Ein", "nied\u00b7lich", "jun\u00b7ges", ",", "wel\u00b7sches", "Huhn"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "$,", "PWAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Schien unserm Stutzer noch allein", "tokens": ["Schien", "un\u00b7serm", "Stut\u00b7zer", "noch", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Der M\u00fche werth, verehrt zu seyn.", "tokens": ["Der", "M\u00fc\u00b7he", "werth", ",", "ver\u00b7ehrt", "zu", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Zwar eine Mutter war noch da,", "tokens": ["Zwar", "ei\u00b7ne", "Mut\u00b7ter", "war", "noch", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Die scharf auf ihre Tochter sah;", "tokens": ["Die", "scharf", "auf", "ih\u00b7re", "Toch\u00b7ter", "sah", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Allein der Pfau verstand sehr sch\u00f6n", "tokens": ["Al\u00b7lein", "der", "Pfau", "ver\u00b7stand", "sehr", "sch\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Die Mutter selbst zu hintergehn;", "tokens": ["Die", "Mut\u00b7ter", "selbst", "zu", "hin\u00b7ter\u00b7gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Und sah noch \u00fcberdies gar bald,", "tokens": ["Und", "sah", "noch", "\u00fc\u00b7ber\u00b7dies", "gar", "bald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Da\u00df in des T\u00f6chterchens Gestalt", "tokens": ["Da\u00df", "in", "des", "T\u00f6ch\u00b7ter\u00b7chens", "Ge\u00b7stalt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Der Mutter Blick vergaffet war.", "tokens": ["Der", "Mut\u00b7ter", "Blick", "ver\u00b7gaf\u00b7fet", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Er nimmt daher des Vortheils wahr,", "tokens": ["Er", "nimmt", "da\u00b7her", "des", "Vor\u00b7theils", "wahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Macht an die Tochter sich beherzt,", "tokens": ["Macht", "an", "die", "Toch\u00b7ter", "sich", "be\u00b7herzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Lieb\u00e4ugelt, lobet, lacht und scherzt.", "tokens": ["Lie\u00b7b\u00e4u\u00b7gelt", ",", "lo\u00b7bet", ",", "lacht", "und", "scherzt", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Sie war verliebten Temp'ramentes;", "tokens": ["Sie", "war", "ver\u00b7lieb\u00b7ten", "Temp'\u00b7ra\u00b7men\u00b7tes", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Der listige Herr Pfau erkennt es", "tokens": ["Der", "lis\u00b7ti\u00b7ge", "Herr", "Pfau", "er\u00b7kennt", "es"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NE", "VVFIN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Nur allzu sicher aus der Art,", "tokens": ["Nur", "all\u00b7zu", "si\u00b7cher", "aus", "der", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKA", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Mit welcher ihm begegnet ward.", "tokens": ["Mit", "wel\u00b7cher", "ihm", "be\u00b7geg\u00b7net", "ward", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Die Mutter merkte jetzt den Handel,", "tokens": ["Die", "Mut\u00b7ter", "merk\u00b7te", "jetzt", "den", "Han\u00b7del", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Und sprach: Mein Herr, der Tugendwandel", "tokens": ["Und", "sprach", ":", "Mein", "Herr", ",", "der", "Tu\u00b7gend\u00b7wan\u00b7del"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "$.", "PPOSAT", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Von meiner Tochter ist bekannt;", "tokens": ["Von", "mei\u00b7ner", "Toch\u00b7ter", "ist", "be\u00b7kannt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Sie schickt sich nicht f\u00fcr Ihren Stand,", "tokens": ["Sie", "schickt", "sich", "nicht", "f\u00fcr", "Ih\u00b7ren", "Stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Und ist nicht aus dem Pfaugeschlecht!", "tokens": ["Und", "ist", "nicht", "aus", "dem", "Pfau\u00b7ge\u00b7schlecht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Wir sind nur H\u00fchner schlecht und recht.", "tokens": ["Wir", "sind", "nur", "H\u00fch\u00b7ner", "schlecht", "und", "recht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Madam, (sprach hier der Pfau verstellt)", "tokens": ["Ma\u00b7dam", ",", "(", "sprach", "hier", "der", "Pfau", "ver\u00b7stellt", ")"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "$(", "VVFIN", "ADV", "ART", "NN", "VVPP", "$("], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.42": {"text": "Ich bitte Sie, was in der Welt", "tokens": ["Ich", "bit\u00b7te", "Sie", ",", "was", "in", "der", "Welt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Verdient es mehr, als wie sie Beide,", "tokens": ["Ver\u00b7di\u00b7ent", "es", "mehr", ",", "als", "wie", "sie", "Bei\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPER", "ADV", "$,", "KOUS", "PWAV", "PPER", "PIS", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.44": {"text": "Vom Pfaugeschlecht zu seyn? O Freude!", "tokens": ["Vom", "Pfau\u00b7ge\u00b7schlecht", "zu", "seyn", "?", "O", "Freu\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VAINF", "$.", "NE", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "Ich kann ein w\u00fcrdig Kind erh\u00f6hn,", "tokens": ["Ich", "kann", "ein", "w\u00fcr\u00b7dig", "Kind", "er\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Und es mir gleich und gl\u00fccklich sehn!", "tokens": ["Und", "es", "mir", "gleich", "und", "gl\u00fcck\u00b7lich", "sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPER", "ADV", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Madam betrachten selber nur", "tokens": ["Ma\u00b7dam", "be\u00b7trach\u00b7ten", "sel\u00b7ber", "nur"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Die kleine s\u00fc\u00dfe Kreatur.", "tokens": ["Die", "klei\u00b7ne", "s\u00fc\u00b7\u00dfe", "Kre\u00b7a\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Gleicht sie nicht v\u00f6llig einer Pfau?", "tokens": ["Gleicht", "sie", "nicht", "v\u00f6l\u00b7lig", "ei\u00b7ner", "Pfau", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Und geht und tr\u00e4gt sie nicht genau", "tokens": ["Und", "geht", "und", "tr\u00e4gt", "sie", "nicht", "ge\u00b7nau"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PPER", "PTKNEG", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Sich so wie unsre Sch\u00f6nen tragen?", "tokens": ["Sich", "so", "wie", "uns\u00b7re", "Sch\u00f6\u00b7nen", "tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "KOKOM", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.52": {"text": "Der Augenschein wird's Ihnen sagen!", "tokens": ["Der", "Au\u00b7gen\u00b7schein", "wird's", "Ih\u00b7nen", "sa\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Tochter h\u00f6ret ihn entz\u00fcckt;", "tokens": ["Die", "Toch\u00b7ter", "h\u00f6\u00b7ret", "ihn", "ent\u00b7z\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Mutter preiset sich begl\u00fcckt.", "tokens": ["Die", "Mut\u00b7ter", "prei\u00b7set", "sich", "be\u00b7gl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem jungen Herrn ward viel erlaubt