{"textgrid.poem.33267": {"metadata": {"author": {"name": "Poe, Edgar Allan", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Ring an meiner Hand,", "genre": "verse", "period": "N.A.", "pub_year": 1829, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Ring an meiner Hand,", "tokens": ["Der", "Ring", "an", "mei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der Kranz aufs Haar gesetzt \u2013", "tokens": ["Der", "Kranz", "aufs", "Haar", "ge\u00b7setzt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein ist nun Prunk und Tand", "tokens": ["Mein", "ist", "nun", "Prunk", "und", "Tand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "VAFIN", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und wunderbar Gewand,", "tokens": ["Und", "wun\u00b7der\u00b7bar", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und ich bin gl\u00fccklich jetzt.", "tokens": ["Und", "ich", "bin", "gl\u00fcck\u00b7lich", "jetzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und mein Herr, er liebt mich sehr;", "tokens": ["Und", "mein", "Herr", ",", "er", "liebt", "mich", "sehr", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch sein Schwur hat mich entsetzt \u2013", "tokens": ["Doch", "sein", "Schwur", "hat", "mich", "ent\u00b7setzt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sein Wort klang dumpf und schwer", "tokens": ["Sein", "Wort", "klang", "dumpf", "und", "schwer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wie Grabgel\u00e4ute her", "tokens": ["Wie", "Grab\u00b7ge\u00b7l\u00e4u\u00b7te", "her"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "NN", "APZR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und klang, als spr\u00e4che ", "tokens": ["Und", "klang", ",", "als", "spr\u00e4\u00b7che"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Der k\u00e4mpfend fiel im Heer \u2013", "tokens": ["Der", "k\u00e4mp\u00b7fend", "fiel", "im", "Heer", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "APPRART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und der wohl gl\u00fccklich jetzt.", "tokens": ["Und", "der", "wohl", "gl\u00fcck\u00b7lich", "jetzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "ADJD", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch er beruhigte mich", "tokens": ["Doch", "er", "be\u00b7ru\u00b7hig\u00b7te", "mich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit sanftem Ku\u00df zuletzt,", "tokens": ["Mit", "sanf\u00b7tem", "Ku\u00df", "zu\u00b7letzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Indes ein Tr\u00e4umen mich", "tokens": ["In\u00b7des", "ein", "Tr\u00e4u\u00b7men", "mich"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zum Kirchhof trug und ich", "tokens": ["Zum", "Kirch\u00b7hof", "trug", "und", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "KON", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "D'Elormie, dem Toten, mich", "tokens": ["D'E\u00b7lor\u00b7mie", ",", "dem", "To\u00b7ten", ",", "mich"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "$,", "ART", "NN", "$,", "PPER"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Verm\u00e4hlte innerlich.", "tokens": ["Ver\u00b7m\u00e4hl\u00b7te", "in\u00b7ner\u00b7lich", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "\u00bbo ich bin gl\u00fccklich jetzt!\u00ab", "tokens": ["\u00bb", "o", "ich", "bin", "gl\u00fcck\u00b7lich", "jetzt", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "PPER", "VAFIN", "ADJD", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und so war das Wort gesprochen", "tokens": ["Und", "so", "war", "das", "Wort", "ge\u00b7spro\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der Schwur, der Pflichten setzt;"