{"textgrid.poem.56752": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Hirt Ahasver", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich tr\u00e4umte j\u00fcngst, mir tr\u00e4umte, da\u00df ich tr\u00e4umte,", "tokens": ["Ich", "tr\u00e4um\u00b7te", "j\u00fcngst", ",", "mir", "tr\u00e4um\u00b7te", ",", "da\u00df", "ich", "tr\u00e4um\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "da\u00df ich getr\u00e4umt, getr\u00e4umt zu haben h\u00e4tt',", "tokens": ["da\u00df", "ich", "ge\u00b7tr\u00e4umt", ",", "ge\u00b7tr\u00e4umt", "zu", "ha\u00b7ben", "h\u00e4tt'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$,", "VVPP", "PTKZU", "VAINF", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "wie Ahasver mit zweimal sieben K\u00fchen,", "tokens": ["wie", "A\u00b7has\u00b7ver", "mit", "zwei\u00b7mal", "sie\u00b7ben", "K\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ADV", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "den sieben magern und den sieben fetten,", "tokens": ["den", "sie\u00b7ben", "ma\u00b7gern", "und", "den", "sie\u00b7ben", "fet\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "KON", "ART", "CARD", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "im Mondschein \u00fcbers Moor gewandert w\u00e4r',", "tokens": ["im", "Mond\u00b7schein", "\u00fc\u00b7bers", "Moor", "ge\u00b7wan\u00b7dert", "w\u00e4r'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "wor\u00fcber selbst ein sp\u00e4ter Weg mich wies.", "tokens": ["wo\u00b7r\u00fc\u00b7ber", "selbst", "ein", "sp\u00e4\u00b7ter", "Weg", "mich", "wies", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "\u00bbei guten Abend, Meister Ahasver,\u00ab \u2013", "tokens": ["\u00bb", "ei", "gu\u00b7ten", "A\u00b7bend", ",", "Meis\u00b7ter", "A\u00b7has\u00b7ver", ",", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "NE", "ADJA", "NN", "$,", "PPOSAT", "NN", "$,", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "begr\u00fc\u00dft ich keck ihn, da\u00df ein magres Tier", "tokens": ["be\u00b7gr\u00fc\u00dft", "ich", "keck", "ihn", ",", "da\u00df", "ein", "mag\u00b7res", "Tier"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "PPER", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "erschreckt zur Seite setzte, \u2013 \u00bbWas ist das?", "tokens": ["er\u00b7schreckt", "zur", "Sei\u00b7te", "setz\u00b7te", ",", "\u2013", "\u00bb", "Was", "ist", "das", "?"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "VVFIN", "$,", "$(", "$(", "PWS", "VAFIN", "PDS", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.10": {"text": "Ihr t