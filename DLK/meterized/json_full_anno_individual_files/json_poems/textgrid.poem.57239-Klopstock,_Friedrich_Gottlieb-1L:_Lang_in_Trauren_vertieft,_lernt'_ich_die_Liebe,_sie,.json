{"textgrid.poem.57239": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Lang in Trauren vertieft, lernt' ich die Liebe, sie,", "genre": "verse", "period": "N.A.", "pub_year": 1750, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lang in Trauren vertieft, lernt' ich die Liebe, sie,", "tokens": ["Lang", "in", "Trau\u00b7ren", "ver\u00b7tieft", ",", "lernt'", "ich", "die", "Lie\u00b7be", ",", "sie", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVPP", "$,", "VVFIN", "PPER", "ART", "NN", "$,", "PPER", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Die der Erde entfloh, aber auch wiederkehrt", "tokens": ["Die", "der", "Er\u00b7de", "ent\u00b7floh", ",", "a\u00b7ber", "auch", "wie\u00b7der\u00b7kehrt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ART", "NN", "VVFIN", "$,", "ADV", "ADV", "VVFIN"], "meter": "--+--+---+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Zu geheimerer Tugend,", "tokens": ["Zu", "ge\u00b7hei\u00b7me\u00b7rer", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Wie die erste der Liebenden", "tokens": ["Wie", "die", "ers\u00b7te", "der", "Lie\u00b7ben\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "ART", "NN"], "meter": "--+--+--", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Voller Unschuld im Hauch duftender L\u00fcfte kam,", "tokens": ["Vol\u00b7ler", "Un\u00b7schuld", "im", "Hauch", "duf\u00b7ten\u00b7der", "L\u00fcf\u00b7te", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Und mit jungem Gef\u00fchl an das Gestade trat,", "tokens": ["Und", "mit", "jun\u00b7gem", "Ge\u00b7f\u00fchl", "an", "das", "Ge\u00b7sta\u00b7de", "trat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Bald sich selbst mit den Rosen", "tokens": ["Bald", "sich", "selbst", "mit", "den", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PRF", "ADV", "APPR", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Von dem Hang des Gestades sah.", "tokens": ["Von", "dem", "Hang", "des", "Ge\u00b7sta\u00b7des", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Die erschien mir! O Schmerz, da sie erschienen war,", "tokens": ["Die", "er\u00b7schien", "mir", "!", "O", "Schmerz", ",", "da", "sie", "er\u00b7schie\u00b7nen", "war", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$.", "NE", "NN", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Warum trafest du mich mit dem gewaltigsten", "tokens": ["Wa\u00b7rum", "tra\u00b7fest", "du", "mich", "mit", "dem", "ge\u00b7wal\u00b7tigs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "APPR", "ART", "ADJA"], "meter": "--+---+--+--", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Deiner zitternden Kummer,", "tokens": ["Dei\u00b7ner", "zit\u00b7tern\u00b7den", "Kum\u00b7mer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Schwermuthsvoller, wie N\u00e4chte sind?", "tokens": ["Schwer\u00b7muths\u00b7vol\u00b7ler", ",", "wie", "N\u00e4ch\u00b7te", "sind", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "NN", "VAFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Jahre trafst du mich schon! Endlich (das hoft' ich nicht)", "tokens": ["Jah\u00b7re", "trafst", "du", "mich", "schon", "!", "End\u00b7lich", "(", "das", "hoft'", "ich", "nicht", ")"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PRF", "ADV", "$.", "ADV", "$(", "PDS", "VVFIN", "PPER", "PTKNEG", "$("], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Sinkt die traurige Nacht, ist nun nicht ewig mehr,", "tokens": ["Sinkt", "die", "trau\u00b7ri\u00b7ge", "Nacht", ",", "ist", "nun", "nicht", "e\u00b7wig", "mehr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "VAFIN", "ADV", "PTKNEG", "ADJD", "ADV", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Und mir wachen mit L\u00e4cheln", "tokens": ["Und", "mir", "wa\u00b7chen", "mit", "L\u00e4\u00b7cheln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Alle schlummernden Freuden auf!", "tokens": ["Al\u00b7le", "schlum\u00b7mern\u00b7den", "Freu\u00b7den", "auf", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Seyd ihrs selber? und t\u00e4uscht, t\u00e4uschet mein Herz mich nicht?", "tokens": ["Seyd", "ihrs", "sel\u00b7ber", "?", "und", "t\u00e4uscht", ",", "t\u00e4u\u00b7schet", "mein", "Herz", "mich", "nicht", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "NN", "ADV", "$.", "KON", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "PPER", "PTKNEG", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Ach ihr seyd es! die Ruh, dieses Gef\u00fchl, so sanft", "tokens": ["Ach", "ihr", "seyd", "es", "!", "die", "Ruh", ",", "die\u00b7ses", "Ge\u00b7f\u00fchl", ",", "so", "sanft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "PPER", "VAFIN", "PPER", "$.", "ART", "NN", "$,", "PDAT", "NN", "$,", "ADV", "ADJD"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Durch das Leben gegossen,", "tokens": ["Durch", "das", "Le\u00b7ben", "ge\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "F\u00fchlt' ich, als ich noch gl\u00fccklich war!", "tokens": ["F\u00fchlt'", "ich", ",", "als", "ich", "noch", "gl\u00fcck\u00b7lich", "war", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "O wie staun' ich mich an, dass ich itzt wieder bin,", "tokens": ["O", "wie", "staun'", "ich", "mich", "an", ",", "dass", "ich", "itzt", "wie\u00b7der", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VVFIN", "PPER", "PRF", "PTKVZ", "$,", "KOUS", "PPER", "ADV", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der ich war! wie entz\u00fcckt \u00fcber die Wandlunge", "tokens": ["Der", "ich", "war", "!", "wie", "ent\u00b7z\u00fcckt", "\u00fc\u00b7ber", "die", "Wand\u00b7lun\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VAFIN", "$.", "PWAV", "VVPP", "APPR", "ART", "NN"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Meines Schicksals, wie dankbar", "tokens": ["Mei\u00b7nes", "Schick\u00b7sals", ",", "wie", "dank\u00b7bar"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wallt mein freudiges Herz in mir!", "tokens": ["Wallt", "mein", "freu\u00b7di\u00b7ges", "Herz", "in", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.7": {"line.1": {"text": "Nichts Unedles, kein Stolz (ihm ist mein Herz zu gross!)", "tokens": ["Nichts", "U\u00b7ned\u00b7les", ",", "kein", "Stolz", "(", "ihm", "ist", "mein", "Herz", "zu", "gross", "!", ")"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIS", "NN", "$,", "PIAT", "NN", "$(", "PPER", "VAFIN", "PPOSAT", "NN", "APPR", "ADJA", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht bet\u00e4ubtes Gef\u00fchl; aber was ist es denn,", "tokens": ["Nicht", "be\u00b7t\u00e4ub\u00b7tes", "Ge\u00b7f\u00fchl", ";", "a\u00b7ber", "was", "ist", "es", "denn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "$.", "KON", "PWS", "VAFIN", "PPER", "ADV", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Das mich heitert? O Tugend,", "tokens": ["Das", "mich", "hei\u00b7tert", "?", "O", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "$.", "NE", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Sanfte Tugend, belohnest du?", "tokens": ["Sanf\u00b7te", "Tu\u00b7gend", ",", "be\u00b7loh\u00b7nest", "du", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.8": {"line.1": {"text": "Doch bist du es allein? oder (o darf ich auch", "tokens": ["Doch", "bist", "du", "es", "al\u00b7lein", "?", "o\u00b7der", "(", "o", "darf", "ich", "auch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PPER", "ADV", "$.", "KON", "$(", "FM", "VMFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mir vertrauen?) entschl\u00fcpft, Tugend, an deiner Hand", "tokens": ["Mir", "ver\u00b7trau\u00b7en", "?", ")", "ent\u00b7schl\u00fcpft", ",", "Tu\u00b7gend", ",", "an", "dei\u00b7ner", "Hand"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVINF", "$.", "$(", "VVPP", "$,", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Nicht ein M\u00e4dchen der Unschuld", "tokens": ["Nicht", "ein", "M\u00e4d\u00b7chen", "der", "Un\u00b7schuld"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ART", "NN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Deinen H\u00f6hn, und erscheinet mir?", "tokens": ["Dei\u00b7nen", "H\u00f6hn", ",", "und", "er\u00b7schei\u00b7net", "mir", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "VVFIN", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.9": {"line.1": {"text": "Sanft im Traume des Schlafs, sanfter im wachenden,", "tokens": ["Sanft", "im", "Trau\u00b7me", "des", "Schlafs", ",", "sanf\u00b7ter", "im", "wa\u00b7chen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "ART", "NN", "$,", "ADJD", "APPRART", "ADJA", "$,"], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Dass ich, wenn sie vor mir eilend vor\u00fcber schl\u00fcpft,", "tokens": ["Dass", "ich", ",", "wenn", "sie", "vor", "mir", "ei\u00b7lend", "vor\u00b7\u00fc\u00b7ber", "schl\u00fcpft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUS", "PPER", "APPR", "PPER", "ADJD", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Staml', und schweig', und beginne:", "tokens": ["Staml'", ",", "und", "schweig'", ",", "und", "be\u00b7gin\u00b7ne", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "VVFIN", "$,", "KON", "VVFIN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Warum eilst du? ich liebe dich!", "tokens": ["Wa\u00b7rum", "eilst", "du", "?", "ich", "lie\u00b7be", "dich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Ach, du kennst ja mein Herz, wie es geliebet hat!", "tokens": ["Ach", ",", "du", "kennst", "ja", "mein", "Herz", ",", "wie", "es", "ge\u00b7lie\u00b7bet", "hat", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$,", "PWAV", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gleicht ein Herz ihm? Vielleicht gleichet dein Herz ihm nur!", "tokens": ["Gleicht", "ein", "Herz", "ihm", "?", "Viel\u00b7leicht", "glei\u00b7chet", "dein", "Herz", "ihm", "nur", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "$.", "ADV", "VVFIN", "PPOSAT", "NN", "PPER", "ADV", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Darum liebe mich, Cidli,", "tokens": ["Da\u00b7rum", "lie\u00b7be", "mich", ",", "Cid\u00b7li", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "XY", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Denn ich lernte die Liebe dir!", "tokens": ["Denn", "ich", "lern\u00b7te", "die", "Lie\u00b7be", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "PPER", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.11": {"line.1": {"text": "Dich zu finden, ach dich, lernt' ich die Liebe, sie,", "tokens": ["Dich", "zu", "fin\u00b7den", ",", "ach", "dich", ",", "lernt'", "ich", "die", "Lie\u00b7be", ",", "sie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "XY", "PPER", "$,", "VVFIN", "PPER", "ART", "NN", "$,", "PPER", "$,"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Die mein steigendes Herz himlisch erweiterte,", "tokens": ["Die", "mein", "stei\u00b7gen\u00b7des", "Herz", "him\u00b7lisch", "er\u00b7wei\u00b7ter\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Nun in s\u00fcsseren Tr\u00e4umen", "tokens": ["Nun", "in", "s\u00fcs\u00b7se\u00b7ren", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Mich in Edens Gefilde tr\u00e4gt!", "tokens": ["Mich", "in", "E\u00b7dens", "Ge\u00b7fil\u00b7de", "tr\u00e4gt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NE", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.12": {"line.1": {"text": "Lang in Trauren vertieft, lernt' ich die Liebe, sie,", "tokens": ["Lang", "in", "Trau\u00b7ren", "ver\u00b7tieft", ",", "lernt'", "ich", "die", "Lie\u00b7be", ",", "sie", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVPP", "$,", "VVFIN", "PPER", "ART", "NN", "$,", "PPER", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Die der Erde entfloh, aber auch wiederkehrt", "tokens": ["Die", "der", "Er\u00b7de", "ent\u00b7floh", ",", "a\u00b7ber", "auch", "wie\u00b7der\u00b7kehrt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ART", "NN", "VVFIN", "$,", "ADV", "ADV", "VVFIN"], "meter": "--+--+---+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Zu geheimerer Tugend,", "tokens": ["Zu", "ge\u00b7hei\u00b7