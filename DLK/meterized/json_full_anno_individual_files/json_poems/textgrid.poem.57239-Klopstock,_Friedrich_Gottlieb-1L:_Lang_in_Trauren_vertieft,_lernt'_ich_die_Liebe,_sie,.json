"}, "line.4": {"text": "Denn ich lernte die Liebe dir!", "tokens": ["Denn", "ich", "lern\u00b7te", "die", "Lie\u00b7be", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "PPER", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.22": {"line.1": {"text": "Dich zu finden, ach dich, lernt' ich die Liebe, sie,", "tokens": ["Dich", "zu", "fin\u00b7den", ",", "ach", "dich", ",", "lernt'", "ich", "die", "Lie\u00b7be", ",", "sie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "XY", "PPER", "$,", "VVFIN", "PPER", "ART", "NN", "$,", "PPER", "$,"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Die mein steigendes Herz himlisch erweiterte,", "tokens": ["Die", "mein", "stei\u00b7gen\u00b7des", "Herz", "him\u00b7lisch", "er\u00b7wei\u00b7ter\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Nun in s\u00fcsseren Tr\u00e4umen", "tokens": ["Nun", "in", "s\u00fcs\u00b7se\u00b7ren", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Mich in Edens Gefilde tr\u00e4gt!", "tokens": ["Mich", "in", "E\u00b7dens", "Ge\u00b7fil\u00b7de", "tr\u00e4gt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NE", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}