{"textgrid.poem.55188": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Einer hohen Reisenden", "genre": "verse", "period": "N.A.", "pub_year": 1808, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohin du trittst, wird uns verkl\u00e4rte Stunde,", "tokens": ["Wo\u00b7hin", "du", "trittst", ",", "wird", "uns", "ver\u00b7kl\u00e4r\u00b7te", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dir leuchtet Klarheit frisch vom Angesicht,", "tokens": ["Dir", "leuch\u00b7tet", "Klar\u00b7heit", "frisch", "vom", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vom Auge Gutheit, Lieblichkeit vom Munde,", "tokens": ["Vom", "Au\u00b7ge", "Gut\u00b7heit", ",", "Lieb\u00b7lich\u00b7keit", "vom", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "$,", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus Wolken dringt ein reines Himmelslicht.", "tokens": ["Aus", "Wol\u00b7ken", "dringt", "ein", "rei\u00b7nes", "Him\u00b7mels\u00b7licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Ungeheuer Schwarm im Hintergrunde,", "tokens": ["Der", "Un\u00b7ge\u00b7heu\u00b7er", "Schwarm", "im", "Hin\u00b7ter\u00b7grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Er dr\u00e4ngt, er droht, jedoch er schreckt dich nicht,", "tokens": ["Er", "dr\u00e4ngt", ",", "er", "droht", ",", "je\u00b7doch", "er", "schreckt", "dich", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "ADV", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wie du mit Freiheit unbefangen schreitest,", "tokens": ["Wie", "du", "mit", "Frei\u00b7heit", "un\u00b7be\u00b7fan\u00b7gen", "schrei\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Das Herz erhebst und jeden Geist erweitest.", "tokens": ["Das", "Herz", "er\u00b7hebst", "und", "je\u00b7den", "Geist", "er\u00b7wei\u00b7test", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So wandelst du, dein Ebenbild zu schauen,", "tokens": ["So", "wan\u00b7delst", "du", ",", "dein", "E\u00b7ben\u00b7bild", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das majest\u00e4tisch uns von oben blickt,", "tokens": ["Das", "ma\u00b7jes\u00b7t\u00e4\u00b7tisch", "uns", "von", "o\u00b7ben", "blickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "PPER", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der M\u00fctter Urbild, K\u00f6nigin der Frauen,", "tokens": ["Der", "M\u00fct\u00b7ter", "Ur\u00b7bild", ",", "K\u00f6\u00b7ni\u00b7gin", "der", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Wunderpinsel hat sie ausgedr\u00fcckt.", "tokens": ["Ein", "Wun\u00b7der\u00b7pin\u00b7sel", "hat", "sie", "aus\u00b7ge\u00b7dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ihr beugt ein Mann, mit liebevollem Grauen,", "tokens": ["Ihr", "beugt", "ein", "Mann", ",", "mit", "lie\u00b7be\u00b7vol\u00b7lem", "Grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein Weib die Knie, in Demut still entz\u00fcckt;", "tokens": ["Ein", "Weib", "die", "Knie", ",", "in", "De\u00b7mut", "still", "ent\u00b7z\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "APPR", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Du aber kommst, ihr deine Hand zu reichen,", "tokens": ["Du", "a\u00b7ber", "kommst", ",", "ihr", "dei\u00b7ne", "Hand", "zu", "rei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$,", "PPER", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Als w\u00e4rest du zu Haus bei deinesgleichen.", "tokens": ["Als", "w\u00e4\u00b7rest", "du", "zu", "Haus", "bei", "dei\u00b7nes\u00b7glei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "APPR", "NN", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch schreite weiter, was auch hier sich finde,", "tokens": ["Doch", "schrei\u00b7te", "wei\u00b7ter", ",", "was", "auch", "hier", "sich", "fin\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$,", "PRELS", "ADV", "ADV", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum Lande hin, dem doch kein andres gleicht,", "tokens": ["Zum", "Lan\u00b7de", "hin", ",", "dem", "doch", "kein", "and\u00b7res", "gleicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,", "PRELS", "ADV", "PIAT", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo uns Natur befreit, wie Kunst auch binde,", "tokens": ["Wo", "uns", "Na\u00b7tur", "be\u00b7freit", ",", "wie", "Kunst", "auch", "bin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "VVPP", "$,", "PWAV", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Geist sich st\u00e4hlt, wenn sich das Herz erweicht,", "tokens": ["Der", "Geist", "sich", "st\u00e4hlt", ",", "wenn", "sich", "das", "Herz", "er\u00b7weicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVFIN", "$,", "KOUS", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Vor stillem Schaun so Zeit- als Volksgewinde", "tokens": ["Vor", "stil\u00b7lem", "Schaun", "so", "Zeit", "als", "Volks\u00b7ge\u00b7win\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADV", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zum Abgrund wallt, zur Himmelsh\u00f6he steigt:", "tokens": ["Zum", "Ab\u00b7grund", "wallt", ",", "zur", "Him\u00b7mels\u00b7h\u00f6\u00b7he", "steigt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dorthin geh\u00f6rst du, die du schaffend strebest,", "tokens": ["Dor\u00b7thin", "ge\u00b7h\u00f6rst", "du", ",", "die", "du", "schaf\u00b7fend", "stre\u00b7best", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die Tr\u00fcmmer herstellst, Totes neu belebest.", "tokens": ["Die", "Tr\u00fcm\u00b7mer", "her\u00b7stellst", ",", "To\u00b7tes", "neu", "be\u00b7le\u00b7best", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "F\u00fchr uns indes durch blumenreiche Matten,", "tokens": ["F\u00fchr", "uns", "in\u00b7des", "durch", "blu\u00b7men\u00b7rei\u00b7che", "Mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Am breiten Flu\u00df durchs wohlbekannte Tal,", "tokens": ["Am", "brei\u00b7ten", "Flu\u00df", "durchs", "wohl\u00b7be\u00b7kann\u00b7te", "Tal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo Reben sich um Sonnenh\u00fcgel gatten,", "tokens": ["Wo", "Re\u00b7ben", "sich", "um", "Son\u00b7nen\u00b7h\u00fc\u00b7gel", "gat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Fels dich sch\u00fctzt vor m\u00e4cht'gem Sonnenstrahl;", "tokens": ["Der", "Fels", "dich", "sch\u00fctzt", "vor", "m\u00e4cht'\u00b7gem", "Son\u00b7nen\u00b7strahl", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Genie\u00dfe froh der engen Laube Schatten,", "tokens": ["Ge\u00b7nie\u00b7\u00dfe", "froh", "der", "en\u00b7gen", "Lau\u00b7be", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der reinen Milch unschuldig w\u00fcrd'ges Mahl,", "tokens": ["Der", "rei\u00b7nen", "Milch", "un\u00b7schul\u00b7dig", "w\u00fcrd'\u00b7ges", "Mahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und hier und dort verg\u00f6nn, an deinen Blicken,", "tokens": ["Und", "hier", "und", "dort", "ver\u00b7g\u00f6nn", ",", "an", "dei\u00b7nen", "Bli\u00b7cken", ","], "token_info": ["word", "word", "