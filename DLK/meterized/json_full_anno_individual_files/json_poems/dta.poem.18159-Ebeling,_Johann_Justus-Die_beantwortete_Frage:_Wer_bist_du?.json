{"dta.poem.18159": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die beantwortete Frage:  \n Wer bist du?", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198805", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer bist du? Ich! ein Mensch: Wilt du", "tokens": ["Wer", "bist", "du", "?", "Ich", "!", "ein", "Mensch", ":", "Wilt", "du"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "$.", "PPER", "$.", "ART", "NN", "$.", "VMFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "den viehisch leben,", "tokens": ["den", "vie\u00b7hisch", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Nein, Nein ich sehe stets den grossen", "tokens": ["Nein", ",", "Nein", "ich", "se\u00b7he", "stets", "den", "gros\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PTKANT", "PPER", "VVFIN", "ADV", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sch\u00f6pfer an:", "tokens": ["Sch\u00f6p\u00b7fer", "an", ":"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Drum mu\u00df ich mich vielmehr durch seine Kraft be-", "tokens": ["Drum", "mu\u00df", "ich", "mich", "viel\u00b7mehr", "durch", "sei\u00b7ne", "Kraft", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "PRF", "ADV", "APPR", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "streben", "tokens": ["stre\u00b7ben"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Wie ich sein Ebenbild in mir abspiegeln kann.", "tokens": ["Wie", "ich", "sein", "E\u00b7ben\u00b7bild", "in", "mir", "ab\u00b7spie\u00b7geln", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "APPR", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+++-+", "measure": "unknown.measure.septa"}, "line.8": {"text": "Ich bin dabei ein Christ, ein Erbe von dem Himmel,", "tokens": ["Ich", "bin", "da\u00b7bei", "ein", "Christ", ",", "ein", "Er\u00b7be", "von", "dem", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "ART", "NN", "$,", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Doch wer nicht christlich lebt, komt nimmermehr", "tokens": ["Doch", "wer", "nicht", "christ\u00b7lich", "lebt", ",", "komt", "nim\u00b7mer\u00b7mehr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PWS", "PTKNEG", "ADJD", "VVFIN", "$,", "VVFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "hinein:", "tokens": ["hin\u00b7ein", ":"], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.11": {"text": "Mein Paradies mu\u00df dort, und nicht im Welt Ge-", "tokens": ["Mein", "Pa\u00b7ra\u00b7dies", "mu\u00df", "dort", ",", "und", "nicht", "im", "Welt", "Ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "$,", "KON", "PTKNEG", "APPRART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "t\u00fcmmel,", "tokens": ["t\u00fcm\u00b7mel", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Die Welt mu\u00df mir verha\u00dft mit ihren S\u00fcnden sein.", "tokens": ["Die", "Welt", "mu\u00df", "mir", "ver\u00b7ha\u00dft", "mit", "ih\u00b7ren", "S\u00fcn\u00b7den", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ich bin ein Sterblicher ein Bau von Staub und Erden", "tokens": ["Ich", "bin", "ein", "Sterb\u00b7li\u00b7cher", "ein", "Bau", "von", "Staub", "und", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wie bald dr\u00fckt mir der Todt gebrochne Augen zu:", "tokens": ["Wie", "bald", "dr\u00fckt", "mir", "der", "Todt", "ge\u00b7broch\u00b7ne", "Au\u00b7gen", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Drum soll mein Losungs Wort, so lang ich lebe", "tokens": ["Drum", "soll", "mein", "Lo\u00b7sungs", "Wort", ",", "so", "lang", "ich", "le\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPOSAT", "NN", "NN", "$,", "ADV", "ADJD", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "werden,", "tokens": ["wer\u00b7den", ","], "token_info": ["word", "punct"], "pos": ["VAINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.18": {"text": "Bedenke was du thust; Bedenke wer bist du.", "tokens": ["Be\u00b7den\u00b7ke", "was", "du", "thust", ";", "Be\u00b7den\u00b7ke", "wer", "bist", "du", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PWS", "PPER", "VVFIN", "$.", "VVFIN", "PWS", "VAFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}