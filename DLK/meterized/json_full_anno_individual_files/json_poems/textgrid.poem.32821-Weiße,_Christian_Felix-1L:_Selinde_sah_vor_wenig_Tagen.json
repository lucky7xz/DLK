": "Und ach! was hatt ich ihr gethan?", "tokens": ["Und", "ach", "!", "was", "hatt", "ich", "ihr", "ge\u00b7than", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PWS", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch Seufzen war nichts zu gewinnen;", "tokens": ["Durch", "Seuf\u00b7zen", "war", "nichts", "zu", "ge\u00b7win\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Drum fing ich laut zu weinen an.", "tokens": ["Drum", "fing", "ich", "laut", "zu", "wei\u00b7nen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADJD", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich warf mich nieder: auf den Knieen", "tokens": ["Ich", "warf", "mich", "nie\u00b7der", ":", "auf", "den", "Kni\u00b7e\u00b7en"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$.", "APPR", "ART", "NN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Stellt ich ihr meine Reue dar:", "tokens": ["Stellt", "ich", "ihr", "mei\u00b7ne", "Reu\u00b7e", "dar", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df ich \u2013 \u2013 da\u00df ich ihr schon verziehen,", "tokens": ["Da\u00df", "ich", "\u2013", "\u2013", "da\u00df", "ich", "ihr", "schon", "ver\u00b7zie\u00b7hen", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "P