{"textgrid.poem.32821": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Selinde sah vor wenig Tagen", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Selinde sah vor wenig Tagen", "tokens": ["Se\u00b7lin\u00b7de", "sah", "vor", "we\u00b7nig", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den jungen Damon freundlich an:", "tokens": ["Den", "jun\u00b7gen", "Da\u00b7mon", "freund\u00b7lich", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie konnt ich einen Blick vertragen,", "tokens": ["Wie", "konnt", "ich", "ei\u00b7nen", "Blick", "ver\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der viel, sehr viel bedeuten kann!", "tokens": ["Der", "viel", ",", "sehr", "viel", "be\u00b7deu\u00b7ten", "kann", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$,", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich wollte sie alsbald verlassen,", "tokens": ["Ich", "woll\u00b7te", "sie", "als\u00b7bald", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und schwur: ich wollte nimmermehr", "tokens": ["Und", "schwur", ":", "ich", "woll\u00b7te