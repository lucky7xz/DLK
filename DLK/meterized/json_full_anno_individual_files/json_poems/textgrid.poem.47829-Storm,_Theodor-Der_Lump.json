{"textgrid.poem.47829": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Der Lump", "genre": "verse", "period": "N.A.", "pub_year": 1852, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und bin ich auch ein rechter Lump,", "tokens": ["Und", "bin", "ich", "auch", "ein", "rech\u00b7ter", "Lump", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So bin ich dessen unverlegen;", "tokens": ["So", "bin", "ich", "des\u00b7sen", "un\u00b7ver\u00b7le\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PDS", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein frech Gem\u00fct, ein fromm Gesicht,", "tokens": ["Ein", "frech", "Ge\u00b7m\u00fct", ",", "ein", "fromm", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Herzbruder, sind ein wahrer Segen!", "tokens": ["Herz\u00b7bru\u00b7der", ",", "sind", "ein", "wah\u00b7rer", "Se\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Links nehm von Christi Mantel ich", "tokens": ["Links", "nehm", "von", "Chris\u00b7ti", "Man\u00b7tel", "ich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NE", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Zipfelchen, da\u00df es mir diene,", "tokens": ["Ein", "Zip\u00