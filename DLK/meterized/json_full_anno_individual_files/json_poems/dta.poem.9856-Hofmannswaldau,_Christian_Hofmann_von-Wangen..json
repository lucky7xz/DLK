{"dta.poem.9856": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Wangen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.71", "nl:0.14", "en:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Uns hat Cupido glut/ die rose blut geschencket/", "tokens": ["Uns", "hat", "Cu\u00b7pi\u00b7do", "glut", "/", "die", "ro\u00b7se", "blut", "ge\u00b7schen\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "NN", "$(", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die lilje schnee/ der sich um beyde zirckel schwencket/", "tokens": ["Die", "lil\u00b7je", "schnee", "/", "der", "sich", "um", "bey\u00b7de", "zir\u00b7ckel", "schwen\u00b7cket", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "PRELS", "PRF", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hier stehet helffenbein mit purpur rings umschr\u00e4ncket/", "tokens": ["Hier", "ste\u00b7het", "helf\u00b7fen\u00b7bein", "mit", "pur\u00b7pur", "rings", "um\u00b7schr\u00e4n\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "ADJD", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und manch verliebter mund steht blo\u00df auf uns gelencket/", "tokens": ["Und", "manch", "ver\u00b7lieb\u00b7ter", "mund", "steht", "blo\u00df", "auf", "uns", "ge\u00b7len\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "VVFIN", "ADV", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Wen nicht die liljen-milch und rosen-\u00f6le tr\u00e4ncket/", "tokens": ["Wen", "nicht", "die", "lil\u00b7jen\u00b7milch", "und", "ro\u00b7sen\u00b7\u00f6le", "tr\u00e4n\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "ART", "NN", "KON", "NE", "VVFIN", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Der ist ein marmolstein/ der nie an lust gedencket.", "tokens": ["Der", "ist", "ein", "mar\u00b7mol\u00b7stein", "/", "der", "nie", "an", "lust", "ge\u00b7den\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$(", "ART", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}