schheit", "auch", "ver\u00b7spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Nichts ist, was er nicht schuldig bliebe.", "tokens": ["Nichts", "ist", ",", "was", "er", "nicht", "schul\u00b7dig", "blie\u00b7be", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "$,", "PWS", "PPER", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es kann nur ", "tokens": ["Es", "kann", "nur"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV"], "meter": "++-", "measure": "unknown.measure.di"}, "line.6": {"text": "Wie es nur ", "tokens": ["Wie", "es", "nur"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PPER", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Und beide sind vereint im Leben", "tokens": ["Und", "bei\u00b7de", "sind", "ver\u00b7eint", "im", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VAFIN", "VVPP", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dann, wenn der Mensch den Menschen liebt.", "tokens": ["Dann", ",", "wenn", "der", "Mensch", "den", "Men\u00b7schen", "liebt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Nun steig empor ins Himmelreich,", "tokens": ["Nun", "steig", "em\u00b7por", "ins", "Him\u00b7mel\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und bring herab den V\u00f6lkerfrieden!", "tokens": ["Und", "bring", "her\u00b7ab", "den", "V\u00f6l\u00b7ker\u00b7frie\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er ist dem Dort und Hier zugleich,", "tokens": ["Er", "ist", "dem", "Dort", "und", "Hier", "zu\u00b7gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Erde nicht allein, beschieden.", "tokens": ["Der", "Er\u00b7de", "nicht", "al\u00b7lein", ",", "be\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADV", "$,", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Hol uns den ", "tokens": ["Hol", "uns", "den"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPER", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Der auch nur ", "tokens": ["Der", "auch", "nur"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADV", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Dann schwebt mit ihm der Engel nieder,", "tokens": ["Dann", "schwebt", "mit", "ihm", "der", "En\u00b7gel", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Den man den V\u00f6lkerfrieden nennt.", "tokens": ["Den", "man", "den", "V\u00f6l\u00b7ker\u00b7frie\u00b7den", "nennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}