{"textgrid.poem.35163": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Der V\u00f6lkerfriede", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Trag nicht empor ins Himmelreich,", "tokens": ["Trag", "nicht", "em\u00b7por", "ins", "Him\u00b7mel\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "PTKVZ", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was auf der Erde hat zu bleiben!", "tokens": ["Was", "auf", "der", "Er\u00b7de", "hat", "zu", "blei\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du bist noch lange Gott nicht gleich", "tokens": ["Du", "bist", "noch", "lan\u00b7ge", "Gott", "nicht", "gleich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "NN", "PTKNEG", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und willst dich ihm doch einverleiben.", "tokens": ["Und", "willst", "dich", "ihm", "doch", "ein\u00b7ver\u00b7lei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du wirfst ihm alle irdschen Fragen", "tokens": ["Du", "wirfst", "ihm", "al\u00b7le", "ird\u00b7schen", "Fra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zur pflichtgemachten L\u00f6sung hin;", "tokens": ["Zur", "pflicht\u00b7ge\u00b7mach\u00b7ten", "L\u00f6\u00b7sung", "hin", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Allmacht soll sich f\u00fcr dich plagen;", "tokens": ["Die", "All\u00b7macht", "soll", "sich", "f\u00fcr", "dich", "pla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das ist des Glaubens Zweck und Sinn.", "tokens": ["Das", "ist", "des", "Glau\u00b7bens", "Zweck", "und", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Erscheint dir eine Last zu schwer,", "tokens": ["Er\u00b7scheint", "dir", "ei\u00b7ne", "Last", "zu", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Will Etwas dir nicht gleich gelingen,", "tokens": ["Will", "Et\u00b7was", "dir", "nicht", "gleich", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So sorgt dich das nicht allzusehr,", "tokens": ["So", "sorgt", "dich", "das", "nicht", "all\u00b7zu\u00b7sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDS", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du kannst es ja dem Vater bringen.", "tokens": ["Du", "kannst", "es", "ja", "dem", "Va\u00b7ter", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du bist von ihm einst ausgegangen", "tokens": ["Du", "bist", "von", "ihm", "einst", "aus\u00b7ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und kehrest einst zu ihm zur\u00fcck;", "tokens": ["Und", "keh\u00b7rest", "einst", "zu", "ihm", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du brauchst von ihm nur zu verlangen,", "tokens": ["Du", "brauchst", "von", "ihm", "nur", "zu", "ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Dein Heil ist ja sein eignes Gl\u00fcck.", "tokens": ["Dein", "Heil", "ist", "ja", "sein", "eig\u00b7nes", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So soll Gott Alles f\u00fcr dich thun;", "tokens": ["So", "soll", "Gott", "Al\u00b7les", "f\u00fcr", "dich", "thun", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NN", "PIS", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er soll sogar auch f\u00fcr dich lieben.", "tokens": ["Er", "soll", "so\u00b7gar", "auch", "f\u00fcr", "dich", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Auf seiner G\u00fcte auszuruhn,", "tokens": ["Auf", "sei\u00b7ner", "G\u00fc\u00b7te", "aus\u00b7zu\u00b7ruhn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist dir verbrieft, ist dir verschrieben.", "tokens": ["Ist", "dir", "ver\u00b7brieft", ",", "ist", "dir", "ver\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du brauchst nichts weiter, als zu glauben,", "tokens": ["Du", "brauchst", "nichts", "wei\u00b7ter", ",", "als", "zu", "glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PTKVZ", "$,", "KOUS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df er die Welt zum Besten lenkt,", "tokens": ["Da\u00df", "er", "die", "Welt", "zum", "Bes\u00b7ten", "lenkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und eifrig gegen den zu schnauben,", "tokens": ["Und", "eif\u00b7rig", "ge\u00b7gen", "den", "zu", "schnau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Gottes Reich sich anders denkt.", "tokens": ["Der", "Got\u00b7tes", "Reich", "sich", "an\u00b7ders", "denkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und gl\u00e4ubig schnaubend, l\u00e4chelst du,", "tokens": ["Und", "gl\u00e4u\u00b7big", "schnau\u00b7bend", ",", "l\u00e4\u00b7chelst", "du", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erf\u00fcllt von heilgem Himmelsfeuer,", "tokens": ["Er\u00b7f\u00fcllt", "von", "heil\u00b7gem", "Him\u00b7mels\u00b7feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dem N\u00e4chsten Gottes Liebe zu \u2013 \u2013", "tokens": ["Dem", "N\u00e4chs\u00b7ten", "Got\u00b7tes", "Lie\u00b7be", "zu", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "APPR", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die deinige ist dir zu theuer.", "tokens": ["Die", "dei\u00b7ni\u00b7ge", "ist", "dir", "zu", "theu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSS", "VAFIN", "PPER", "PTKA", "ADJD", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Die g\u00f6ttliche reicht f\u00fcr die Schaaren", "tokens": ["Die", "g\u00f6tt\u00b7li\u00b7che", "reicht", "f\u00fcr", "die", "Schaa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Der Ungez\u00e4hlten ewig aus;", "tokens": ["Der", "Un\u00b7ge\u00b7z\u00e4hl\u00b7ten", "e\u00b7wig", "aus", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die menschliche hat man zu sparen;", "tokens": ["Die", "menschli\u00b7che", "hat", "man", "zu", "spa\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Sie geht nicht \u00fcbers Ich hinaus.", "tokens": ["Sie", "geht", "nicht", "\u00fc\u00b7bers", "Ich", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und dieser Glaube will der Welt", "tokens": ["Und", "die\u00b7ser", "Glau\u00b7be", "will", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "VMFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch diese Liebe Frieden bringen", "tokens": ["Durch", "die\u00b7se", "Lie\u00b7be", "Frie\u00b7den", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und l\u00e4\u00dft als Herrscher und als Held", "tokens": ["Und", "l\u00e4\u00dft", "als", "Herr\u00b7scher", "und", "als", "Held"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOUS", "NN", "KON", "KOUS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein ", "tokens": ["Sein"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Und dieser Glaube, viel zerrissen,", "tokens": ["Und", "die\u00b7ser", "Glau\u00b7be", ",", "viel", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Stets mit sich selbst in Zank und Streit,", "tokens": ["Stets", "mit", "sich", "selbst", "in", "Zank", "und", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRF", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er will allein zu finden wissen", "tokens": ["Er", "will", "al\u00b7lein", "zu", "fin\u00b7den", "wis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "PTKZU", "VVINF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das, was ihm fehlt, die Einigkeit!", "tokens": ["Das", ",", "was", "ihm", "fehlt", ",", "die", "Ei\u00b7nig\u00b7keit", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "$,", "PWS", "PPER", "VVFIN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "O, glaub an diesen Glauben nicht!", "tokens": ["O", ",", "glaub", "an", "die\u00b7sen", "Glau\u00b7ben", "nicht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "APPR", "PDAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Glaub nur allein an Gottes Liebe.", "tokens": ["Glaub", "nur", "al\u00b7lein", "an", "Got\u00b7tes", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Was er der Menschheit auch verspricht,", "tokens": ["Was", "er", "der", "Menschheit", "auch", "ver\u00b7spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Nichts ist, was er nicht schuldig bliebe.", "tokens": ["Nichts", "ist", ",", "was", "er", "nicht", "schul\u00b7dig", "blie\u00b7be", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "$,", "PWS", "PPER", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es kann nur ", "tokens": ["Es", "kann", "nur"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV"], "meter": "++-", "measure": "unknown.measure.di"}, "line.6": {"text": "Wie es nur ", "tokens": ["Wie", "es", "nur"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PPER", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Und beide sind vereint im Leben", "tokens": ["Und", "bei\u00b7de", "sind", "ver\u00b7eint", "im", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VAFIN", "VVPP", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dann, wenn der Mensch den Menschen liebt.", "tokens": ["Dann", ",", "wenn", "der", "Mensch", "den", "Men\u00b7schen", "liebt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Nun steig empor ins Himmelreich,", "tokens": ["Nun", "steig", "em\u00b7por", "ins", "Him\u00b7mel\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und bring herab den V\u00f6lkerfrieden!", "tokens": ["Und", "bring", "her\u00b7ab", "den", "V\u00f6l\u00b7ker\u00b7frie\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er ist dem Dort und Hier zugleich,", "tokens": ["Er", "ist", "dem", "Dort", "und", "Hier", "zu\u00b7gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Erde nicht allein, beschieden.", "tokens": ["Der", "Er\u00b7de", "nicht", "al\u00b7lein", ",", "be\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADV", "$,", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Hol uns den ", "tokens": ["Hol", "uns", "den"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPER", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Der auch nur ", "tokens": ["Der", "auch", "nur"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADV", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Dann schwebt mit ihm der Engel nieder,", "tokens": ["Dann", "schwebt", "mit", "ihm", "der", "En\u00b7gel", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Den man den V\u00f6lkerfrieden nennt.", "tokens": ["Den", "man", "den", "V\u00f6l\u00b7ker\u00b7frie\u00b7den", "nennt", "."], "token_info": ["word", "word", "word", "w