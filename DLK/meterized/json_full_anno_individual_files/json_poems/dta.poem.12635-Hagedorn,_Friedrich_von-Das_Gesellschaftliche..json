{"dta.poem.12635": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Das Gesellschaftliche.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905199128", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr Freunde! zecht bey freudenvollen Ch\u00f6ren!", "tokens": ["Ihr", "Freun\u00b7de", "!", "zecht", "bey", "freu\u00b7den\u00b7vol\u00b7len", "Ch\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf! stimmt ein freyes Scherz-Lied an.", "tokens": ["Auf", "!", "stimmt", "ein", "frey\u00b7es", "Scher\u00b7zLied", "an", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Trink ich so viel, so trink ich euch zu Ehren,", "tokens": ["Trink", "ich", "so", "viel", ",", "so", "trink", "ich", "euch", "zu", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$,", "ADV", "VVFIN", "PPER", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und da\u00df ich heller singen kann.", "tokens": ["Und", "da\u00df", "ich", "hel\u00b7ler", "sin\u00b7gen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Rund-Trunk mu\u00df der Stimmen Bund beleben,", "tokens": ["Der", "Run\u00b7dTrunk", "mu\u00df", "der", "Stim\u00b7men", "Bund", "be\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So schmeckt der Wein uns doppelt sch\u00f6n;", "tokens": ["So", "schmeckt", "der", "Wein", "uns", "dop\u00b7pelt", "sch\u00f6n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ein Gesetz, nur eines, will ich geben:", "tokens": ["Und", "ein", "Ge\u00b7setz", ",", "nur", "ei\u00b7nes", ",", "will", "ich", "ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADV", "PIS", "$,", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "La\u00dft nicht das Glas zu lange stehn.", "tokens": ["La\u00dft", "nicht", "das", "Glas", "zu", "lan\u00b7ge", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "ART", "NN", "APPR", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Jhr Freunde! zecht, wie unsre V\u00e4ter zechten:", "tokens": ["Ihr", "Freun\u00b7de", "!", "zecht", ",", "wie", "uns\u00b7re", "V\u00e4\u00b7ter", "zech\u00b7ten", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie waren alt und klug genung,", "tokens": ["Sie", "wa\u00b7ren", "alt", "und", "klug", "ge\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und manchen Zank, bey dem wir S\u00f6hne rechten,", "tokens": ["Und", "man\u00b7chen", "Zank", ",", "bey", "dem", "wir", "S\u00f6h\u00b7ne", "rech\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "APPR", "PRELS", "PPER", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ertr\u00e4nkten sie im Reihen-Trunk.", "tokens": ["Er\u00b7tr\u00e4nk\u00b7ten", "sie", "im", "Rei\u00b7hen\u00b7Trunk", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie thaten mehr: Sa\u00df nur an ihrer Seite", "tokens": ["Sie", "tha\u00b7ten", "mehr", ":", "Sa\u00df", "nur", "an", "ih\u00b7rer", "Sei\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$.", "VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein Kind voll holder Freundlichkeit;", "tokens": ["Ein", "Kind", "voll", "hol\u00b7der", "Freund\u00b7lich\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So gab dem Wein ein Schm\u00e4tzchen das Geleite,", "tokens": ["So", "gab", "dem", "Wein", "ein", "Schm\u00e4tz\u00b7chen", "das", "Ge\u00b7lei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So ward ein Glas dem Ku\u00df geweiht.", "tokens": ["So", "ward", "ein", "Glas", "dem", "Ku\u00df", "ge\u00b7weiht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wie trostlos war der Zeiten erste Jugend,", "tokens": ["Wie", "trost\u00b7los", "war", "der", "Zei\u00b7ten", "ers\u00b7te", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als Thyrsis einer Phyllis sang,", "tokens": ["Als", "Thyr\u00b7sis", "ei\u00b7ner", "Phyl\u00b7lis", "sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und zum Geseufz von Leidenschaft und Tugend", "tokens": ["Und", "zum", "Ge\u00b7seufz", "von", "Lei\u00b7den\u00b7schaft", "und", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit ihr aus schwachen Wassern trank!", "tokens": ["Mit", "ihr", "aus", "schwa\u00b7chen", "Was\u00b7sern", "trank", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die N\u00fcchternheit, die Einfalt bl\u00f6der Liebe", "tokens": ["Die", "N\u00fcch\u00b7tern\u00b7heit", ",", "die", "Ein\u00b7falt", "bl\u00f6\u00b7der", "Lie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verl\u00e4ngerten der Sch\u00e4fer M\u00fch;", "tokens": ["Ver\u00b7l\u00e4n\u00b7ger\u00b7ten", "der", "Sch\u00e4\u00b7fer", "M\u00fch", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NE", "NE", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Wir trinken Wein, befeuren unsre Triebe", "tokens": ["Wir", "trin\u00b7ken", "Wein", ",", "be\u00b7fe\u00b7u\u00b7ren", "uns\u00b7re", "Trie\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und k\u00fcssen muthiger, als sie.", "tokens": ["Und", "k\u00fcs\u00b7sen", "mut\u00b7hi\u00b7ger", ",", "als", "sie", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,", "KOUS", "PPER", "$."], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}}, "stanza.7": {"line.1": {"text": "Lockt uns kein Laub in ungewisse Schatten;", "tokens": ["Lockt", "uns", "kein", "Laub", "in", "un\u00b7ge\u00b7wis\u00b7se", "Schat\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So baut man Dach und Zimmer an,", "tokens": ["So", "baut", "man", "Dach", "und", "Zim\u00b7mer", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die manchem Ku\u00df mehr Sicherheit verstatten,", "tokens": ["Die", "man\u00b7chem", "Ku\u00df", "mehr", "Si\u00b7cher\u00b7heit", "ver\u00b7stat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als Forst und Busch ihm leisten kann.", "tokens": ["Als", "Forst", "und", "Busch", "ihm", "leis\u00b7ten", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Der s\u00fcsse Reitz der ewig jungen Freude", "tokens": ["Der", "s\u00fcs\u00b7se", "Reitz", "der", "e\u00b7wig", "jun\u00b7gen", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wird stets durch Lieb und Wein vermehrt.", "tokens": ["Wird", "stets", "durch", "Lieb", "und", "Wein", "ver\u00b7mehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn ich den Scherz und den Tockayer meide,", "tokens": ["Wenn", "ich", "den", "Scherz", "und", "den", "Tock\u00b7a\u00b7yer", "mei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+++-+-", "measure": "unknown.measure.hexa"}, "line.4": {"text": "So sagt: bin ich der Jugend wehrt?", "tokens": ["So", "sagt", ":", "bin", "ich", "der", "Ju\u00b7gend", "wehrt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "VAFIN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}