{"dta.poem.17048": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "149.  Das Hochzeit Kleyd ist noth.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Der Him\u0304el thut sich auf/ der Br\u00e4utgam komt gegangen!", "tokens": ["Der", "Him\u0304el", "thut", "sich", "auf", "/", "der", "Br\u00e4ut\u00b7gam", "komt", "ge\u00b7gan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "$(", "ART", "NE", "VVFIN", "VVPP", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "O Braut wie wiltu jhn ohns Hochzeit Kleyd embfangen!", "tokens": ["O", "Braut", "wie", "wil\u00b7tu", "jhn", "ohns", "Hoch\u00b7zeit", "Kleyd", "emb\u00b7fan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KOKOM", "VMFIN", "PPER", "APPR", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}