{"textgrid.poem.47914": {"metadata": {"author": {"name": "Klaj, Johann", "birth": "N.A.", "death": "N.A."}, "title": "3. Hochfeierliche Empf\u00e4ngnu\u00df Christi", "genre": "verse", "period": "N.A.", "pub_year": 1636, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der H\u00f6chste zu der Zeit/ als Eva gantz vermessen/", "tokens": ["Der", "H\u00f6chs\u00b7te", "zu", "der", "Zeit", "/", "als", "E\u00b7va", "gantz", "ver\u00b7mes\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$(", "KOUS", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "den H\u00f6chsten und sich selbst/ gestellet in vergessen/", "tokens": ["den", "H\u00f6chs\u00b7ten", "und", "sich", "selbst", "/", "ge\u00b7stel\u00b7let", "in", "ver\u00b7ges\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PRF", "ADV", "$(", "VVPP", "APPR", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "durch allzu k\u00fchne Hand auff einmal Obst vnd Tod", "tokens": ["durch", "all\u00b7zu", "k\u00fch\u00b7ne", "Hand", "auff", "ein\u00b7mal", "Obst", "vnd", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PTKA", "ADJA", "NN", "APPR", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "gebrochen/ abgest\u00fcrtzt sich/ mich und dich in Noth/", "tokens": ["ge\u00b7bro\u00b7chen", "/", "ab\u00b7ge\u00b7st\u00fcrtzt", "sich", "/", "mich", "und", "dich", "in", "Noth", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$(", "VVPP", "PRF", "$(", "PPER", "KON", "PRF", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sprach seine Diener an: Wie/ meine jhr meine B\u00fcrger/", "tokens": ["Sprach", "sei\u00b7ne", "Die\u00b7ner", "an", ":", "Wie", "/", "mei\u00b7ne", "jhr", "mei\u00b7ne", "B\u00fcr\u00b7ger", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "$.", "PWAV", "$(", "VVFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Soll der Verleumder dann/ der H\u00f6llen Mohr/ der W\u00fcrger/", "tokens": ["Soll", "der", "Ver\u00b7leum\u00b7der", "dann", "/", "der", "H\u00f6l\u00b7len", "Mohr", "/", "der", "W\u00fcr\u00b7ger", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADV", "$(", "ART", "NN", "NE", "$(", "ART", "NN", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.7": {"text": "so Meister spielen? Nein; soll denn der Apffelbi\u00df", "tokens": ["so", "Meis\u00b7ter", "spie\u00b7len", "?", "Nein", ";", "soll", "denn", "der", "Apf\u00b7fel\u00b7bi\u00df"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VVINF", "$.", "PTKANT", "$.", "VMFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "mir in der Sternenburg/ thun einen solchen Ri\u00df?", "tokens": ["mir", "in", "der", "Ster\u00b7nen\u00b7burg", "/", "thun", "ei\u00b7nen", "sol\u00b7chen", "Ri\u00df", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$(", "VVFIN", "ART", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich will den! Gabriel/ auff/ setze dich zu Wagen/", "tokens": ["Ich", "will", "den", "!", "Ga\u00b7bri\u00b7el", "/", "auff", "/", "set\u00b7ze", "dich", "zu", "Wa\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "$.", "NE", "$(", "APPR", "$(", "VVFIN", "PRF", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "La\u00df dich den Wolckensturm durch Dampff und D\u00fcnste tragen/", "tokens": ["La\u00df", "dich", "den", "Wol\u00b7cken\u00b7sturm", "durch", "Dampff", "und", "D\u00fcns\u00b7te", "tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "in Pfeilgeschwinder Eil/ hin in Idumens Stadt", "tokens": ["in", "Pfeil\u00b7ge\u00b7schwin\u00b7der", "Eil", "/", "hin", "in", "I\u00b7du\u00b7mens", "Stadt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "NN", "$(", "ADV", "APPR", "NE", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "die von dem gr\u00fcnen Zweig/ den schlechten Namen hat/", "tokens": ["die", "von", "dem", "gr\u00fc\u00b7nen", "Zweig", "/", "den", "schlech\u00b7ten", "Na\u00b7men", "hat", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Da wohnt die/ gegen der die Gottheit ist entflammet/", "tokens": ["Da", "wohnt", "die", "/", "ge\u00b7gen", "der", "die", "Got\u00b7theit", "ist", "ent\u00b7flam\u00b7met", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "$(", "APPR", "ART", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "die von Vhranen her/ und K\u00f6nigs Blut gestammet/", "tokens": ["die", "von", "Vhra\u00b7nen", "her", "/", "und", "K\u00f6\u00b7nigs", "Blut", "ge\u00b7stam\u00b7met", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PTKVZ", "$(", "KON", "NN", "NN", "VVPP", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.15": {"text": "sag/ da\u00df sie tragen soll/ den/ der sie selber tr\u00e4gt/", "tokens": ["sag", "/", "da\u00df", "sie", "tra\u00b7gen", "soll", "/", "den", "/", "der", "sie", "sel\u00b7ber", "tr\u00e4gt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "KOUS", "PPER", "VVINF", "VMFIN", "$(", "ART", "$(", "PRELS", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "das Menschen Hau\u00df/ und alls was sich darinnen regt;", "tokens": ["das", "Men\u00b7schen", "Hau\u00df", "/", "und", "alls", "was", "sich", "da\u00b7rin\u00b7nen", "regt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "KON", "ADV", "PRELS", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Genug! der Herold fleugt in G\u00f6tter hoher gr\u00f6sse/", "tokens": ["Ge\u00b7nug", "!", "der", "He\u00b7rold", "fleugt", "in", "G\u00f6t\u00b7ter", "ho\u00b7her", "gr\u00f6s\u00b7se", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ART", "NN", "VVFIN", "APPR", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "es lebt durch Seidnen Flor/ die wolerziemte Bl\u00f6sse/", "tokens": ["es", "lebt", "durch", "Seid\u00b7nen", "Flor", "/", "die", "wo\u00b7ler\u00b7ziem\u00b7te", "Bl\u00f6s\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "das Stirngestirne Liecht/ die Wangenzier bestralt/", "tokens": ["das", "Stirn\u00b7ges\u00b7tir\u00b7ne", "Liecht", "/", "die", "Wan\u00b7gen\u00b7zier", "be\u00b7stralt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "den Rosengleichen Mund/ mit Lilgen untermahlt.", "tokens": ["den", "Ro\u00b7sen\u00b7glei\u00b7chen", "Mund", "/", "mit", "Lil\u00b7gen", "un\u00b7ter\u00b7mahlt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Das Wollenweiche Haar/ sich von der Scheitel strecket/", "tokens": ["Das", "Wol\u00b7len\u00b7wei\u00b7che", "Haar", "/", "sich", "von", "der", "Schei\u00b7tel", "stre\u00b7cket", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "PRF", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "den Alabaster Hals/ wie klares Flie\u00dfgold lecket;", "tokens": ["den", "A\u00b7la\u00b7bas\u00b7ter", "Hals", "/", "wie", "kla\u00b7res", "Flie\u00df\u00b7gold", "le\u00b7cket", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "KOKOM", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "So blinckt der Tag der Nacht/ das liechtentlehnte Liecht/", "tokens": ["So", "blinckt", "der", "Tag", "der", "Nacht", "/", "das", "liech\u00b7tent\u00b7lehn\u00b7te", "Liecht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "neigt n\u00e4chtlich Wolcken ab/ sein Sichel Angesicht/", "tokens": ["neigt", "n\u00e4cht\u00b7lich", "Wol\u00b7cken", "ab", "/", "sein", "Si\u00b7chel", "An\u00b7ge\u00b7sicht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "PTKVZ", "$(", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "beleuchtet sich im Meer/ bespiegelt sich im Wellen/", "tokens": ["be\u00b7leuch\u00b7tet", "sich", "im", "Meer", "/", "be\u00b7spie\u00b7gelt", "sich", "im", "Wel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "NN", "$(", "VVFIN", "PRF", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "die jhrer F\u00fcrstin gleich/ bald ab/ bald zuwerts prellen;", "tokens": ["die", "jhrer", "F\u00fcrs\u00b7tin", "gleich", "/", "bald", "ab", "/", "bald", "zu\u00b7werts", "prel\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "$(", "ADV", "PTKVZ", "$(", "ADV", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.27": {"text": "In seiner Rechten gl\u00e4ntzt die Blumenk\u00f6nigin/", "tokens": ["In", "sei\u00b7ner", "Rech\u00b7ten", "gl\u00e4ntzt", "die", "Blu\u00b7men\u00b7k\u00f6\u00b7ni\u00b7gin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "die Kron vnd Sceptern gleicht; die/ die in jhren Sinn/", "tokens": ["die", "Kron", "vnd", "Scep\u00b7tern", "gleicht", ";", "die", "/", "die", "in", "jhren", "Sinn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$.", "ART", "$(", "ART", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.29": {"text": "Dem Trauring abgesagt; durchlas damal die Bl\u00e4tter/", "tokens": ["Dem", "Trau\u00b7ring", "ab\u00b7ge\u00b7sagt", ";", "durch\u00b7las", "da\u00b7mal", "die", "Bl\u00e4t\u00b7ter", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$.", "VVFIN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "die jhr und jhres Sohns hertzallerliebster Vetter", "tokens": ["die", "jhr", "und", "jhres", "Sohns", "hert\u00b7zal\u00b7ler\u00b7liebs\u00b7ter", "Vet\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "KON", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.31": {"text": "vorl\u00e4ngsten auffgesetzt; der Abgesandte schwingt", "tokens": ["vor\u00b7l\u00e4ngs\u00b7ten", "auff\u00b7ge\u00b7setzt", ";", "der", "Ab\u00b7ge\u00b7sand\u00b7te", "schwingt"], "token_inf