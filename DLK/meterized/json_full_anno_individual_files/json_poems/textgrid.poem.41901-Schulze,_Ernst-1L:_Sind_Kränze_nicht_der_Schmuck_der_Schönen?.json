{"textgrid.poem.41901": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sind Kr\u00e4nze nicht der Schmuck der Sch\u00f6nen?", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sind Kr\u00e4nze nicht der Schmuck der Sch\u00f6nen?", "tokens": ["Sind", "Kr\u00e4n\u00b7ze", "nicht", "der", "Schmuck", "der", "Sch\u00f6\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PTKNEG", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fchrt nicht bekr\u00e4nzt die Braut den Tanz?", "tokens": ["F\u00fchrt", "nicht", "be\u00b7kr\u00e4nzt", "die", "Braut", "den", "Tanz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mu\u00df nicht ein Kranz den Helden kr\u00f6nen?", "tokens": ["Mu\u00df", "nicht", "ein", "Kranz", "den", "Hel\u00b7den", "kr\u00f6\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist nicht des Liedes Lohn ein Kranz?", "tokens": ["Ist", "nicht", "des", "Lie\u00b7des", "Lohn", "ein", "Kranz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Drum sollen stets auch diese Stunden,", "tokens": ["Drum", "sol\u00b7len", "stets", "auch", "die\u00b7se", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "ADV", "ADV", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Weil sie so Sch\u00f6nes uns verliehn,", "tokens": ["Weil", "sie", "so", "Sch\u00f6\u00b7nes", "uns", "ver\u00b7liehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Vom duft'gen Blumenkranz umwunden,", "tokens": ["Vom", "duft'\u00b7gen", "Blu\u00b7men\u00b7kranz", "um\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sich schm\u00fccken mit des Fr\u00fchlings Gr\u00fcn.", "tokens": ["Sich", "schm\u00fc\u00b7cken", "mit", "des", "Fr\u00fch\u00b7lings", "Gr\u00fcn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mag auch der Winter sein sie nennen,", "tokens": ["Mag", "auch", "der", "Win\u00b7ter", "sein", "sie", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "PPOSAT", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Weil er noch rauh die Fl\u00fcgel schwingt,", "tokens": ["Weil", "er", "noch", "rauh", "die", "Fl\u00fc\u00b7gel", "schwingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie sollen wir den Lenz erkennen,", "tokens": ["Wie", "sol\u00b7len", "wir", "den", "Lenz", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als an den Gaben, die er bringt?", "tokens": ["Als", "an", "den", "Ga\u00b7ben", ",", "die", "er", "bringt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und sagt nicht jedes Herz sich immer,", "tokens": ["Und", "sagt", "nicht", "je\u00b7des", "Herz", "sich", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "PIAT", "NN", "PRF", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn ihm dein Auge freundlich lacht:", "tokens": ["Wenn", "ihm", "dein", "Au\u00b7ge", "freund\u00b7lich", "lacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der sch\u00f6nste May, wohl hat er nimmer", "tokens": ["Der", "sch\u00f6ns\u00b7te", "May", ",", "wohl", "hat", "er", "nim\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So holde Blumen uns gebracht!", "tokens": ["So", "hol\u00b7de", "Blu\u00b7men", "uns", "ge\u00b7bracht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Des Lebens leichte Fl\u00fcgel bindet", "tokens": ["Des", "Le\u00b7bens", "leich\u00b7te", "Fl\u00fc\u00b7gel", "bin\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "An Raum und Zeit der stolze Wahn;", "tokens": ["An", "Raum", "und", "Zeit", "der", "stol\u00b7ze", "Wahn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch was den Gott in sich empfindet,", "tokens": ["Doch", "was", "den", "Gott", "in", "sich", "emp\u00b7fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das schwebt auf selbsterkohrner Bahn.", "tokens": ["Das", "schwebt", "auf", "selbs\u00b7ter\u00b7kohr\u00b7ner", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hell flammt ein himmlischer Gedanke", "tokens": ["Hell", "flammt", "ein", "himm\u00b7li\u00b7scher", "Ge\u00b7dan\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Auch aus des Kummers Nacht hervor,", "tokens": ["Auch", "aus", "des", "Kum\u00b7mers", "Nacht", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und bl\u00fchend schlingt die zarte Ranke", "tokens": ["Und", "bl\u00fc\u00b7hend", "schlingt", "die", "zar\u00b7te", "Ran\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Am rauhen Felsen sich empor.", "tokens": ["Am", "rau\u00b7hen", "Fel\u00b7sen", "sich", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So zog mit seinen Dienern allen,", "tokens": ["So", "zog", "mit", "sei\u00b7nen", "Die\u00b7nern", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "PIAT", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Duft, Gesang und Sonnenschein,", "tokens": ["Mit", "Duft", ",", "Ge\u00b7sang", "und", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Einst in des Winters dunkle Hallen", "tokens": ["Einst", "in", "des", "Win\u00b7ters", "dunk\u00b7le", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der sch\u00f6ne Fr\u00fchling prangend ein,", "tokens": ["Der", "sch\u00f6\u00b7ne", "Fr\u00fch\u00b7ling", "pran\u00b7gend", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und lie\u00df aus seinen Siegeszweigen,", "tokens": ["Und", "lie\u00df", "aus", "sei\u00b7nen", "Sie\u00b7ges\u00b7zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Um jenen holden Augenblick", "tokens": ["Um", "je\u00b7nen", "hol\u00b7den", "Au\u00b7gen\u00b7blick"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nach fernen Jahren zu bezeugen,", "tokens": ["Nach", "fer\u00b7nen", "Jah\u00b7ren", "zu", "be\u00b7zeu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die sch\u00f6nste Blume, dich, zur\u00fcck.", "tokens": ["Die", "sch\u00f6ns\u00b7te", "Blu\u00b7me", ",", "dich", ",", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Drum schm\u00fccke stets mit allen Bl\u00fcthen,", "tokens": ["Drum", "schm\u00fc\u00b7cke", "stets", "mit", "al\u00b7len", "Bl\u00fc\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Allem, was im jungen Jahr", "tokens": ["Mit", "Al\u00b7lem", ",", "was", "im", "jun\u00b7gen", "Jahr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "$,", "PRELS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Wiesen und die Haine bieten,", "tokens": ["Die", "Wie\u00b7sen", "und", "die", "Hai\u00b7ne", "bie\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sich dieser Tag sein goldnes Haar.", "tokens": ["Sich", "die\u00b7ser", "Tag", "sein", "gold\u00b7nes", "Haar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PDAT", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den Sieger mag der Lorbeer kr\u00f6nen,", "tokens": ["Den", "Sie\u00b7ger", "mag", "der", "Lor\u00b7beer", "kr\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Liebe sich der Myrte freun;", "tokens": ["Die", "Lie\u00b7be", "sich", "der", "Myr\u00b7te", "freun", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dem holden Geber ", "tokens": ["Dem", "hol\u00b7den", "Ge\u00b7ber"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Mu\u00df ", "tokens": ["Mu\u00df"], "token_info": ["word"], "pos": ["VMFIN"], "meter": "+", "measure": "single.up"}}, "stanza.6": {"line.1": {"text": "Sind Kr\u00e4nze nicht der Schmuck der Sch\u00f6nen?", "tokens": ["Sind", "Kr\u00e4n\u00b7ze", "nicht", "der", "Schmuck", "der", "Sch\u00f6\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PTKNEG", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fchrt nicht bekr\u00e4nzt die Braut den Tanz?", "tokens": ["F\u00fchrt", "nicht", "be\u00b7kr\u00e4nzt", "die", "Braut", "den", "Tanz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mu\u00df nicht