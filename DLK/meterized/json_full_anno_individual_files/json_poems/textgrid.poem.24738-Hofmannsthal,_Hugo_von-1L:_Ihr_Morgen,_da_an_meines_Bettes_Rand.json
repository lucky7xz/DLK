{"textgrid.poem.24738": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr Morgen, da an meines Bettes Rand", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Morgen, da an meines Bettes Rand", "tokens": ["Ihr", "Mor\u00b7gen", ",", "da", "an", "mei\u00b7nes", "Bet\u00b7tes", "Rand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das Licht aus hellen Muschelwolken flog", "tokens": ["Das", "Licht", "aus", "hel\u00b7len", "Mu\u00b7schel\u00b7wol\u00b7ken", "flog"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und leuchtend, den ich sp\u00e4ter niemals fand,", "tokens": ["Und", "leuch\u00b7tend", ",", "den", "ich", "sp\u00e4\u00b7ter", "nie\u00b7mals", "fand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "$,", "PRELS", "PPER", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Felsenpfad sch\u00f6n in die Weite bog,", "tokens": ["Der", "Fel\u00b7sen\u00b7pfad", "sch\u00f6n", "in", "die", "Wei\u00b7te", "bog", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ihr Mittagsstunden! gro\u00dfer dunkler Baum,", "tokens": ["Ihr", "Mit\u00b7tags\u00b7stun\u00b7den", "!", "gro\u00b7\u00dfer", "dunk\u00b7ler", "Baum", ","], "token_info": ["word", "word", "punct", "word",