{"textgrid.poem.66831": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Riesig rollst du mir zu F\u00fc\u00dfen \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Riesig rollst du mir zu F\u00fc\u00dfen \u2013", "tokens": ["Rie\u00b7sig", "rollst", "du", "mir", "zu", "F\u00fc\u00b7\u00dfen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PPER", "APPR", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df vom Buchenwipfel gr\u00fc\u00dfen", "tokens": ["La\u00df", "vom", "Bu\u00b7chen\u00b7wip\u00b7fel", "gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "APPRART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dich, du dr\u00f6hnend Wogenheer!", "tokens": ["Dich", ",", "du", "dr\u00f6h\u00b7nend", "Wo\u00b7ge\u00b7nheer", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00fcchtern in dein Donnerklingen", "tokens": ["Sch\u00fcch\u00b7tern", "in", "dein", "Don\u00b7ner\u00b7klin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wag' ich kaum mein Lied zu singen,", "tokens": ["Wag'", "ich", "kaum", "mein", "Lied", "zu", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nachtigall am Zukunftsmeer.", "tokens": ["Nach\u00b7ti\u00b7gall", "am", "Zu\u00b7kunfts\u00b7meer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dickichtnistend mu\u00dft' ich lauschen", "tokens": ["Di\u00b7ckicht\u00b7nis\u00b7tend", "mu\u00dft'", "ich", "lau\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lang schon deinem fernen Rauschen,", "tokens": ["Lang", "schon", "dei\u00b7nem", "fer\u00b7nen", "Rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zitternd meine Brust dir schwoll.", "tokens": ["Zit\u00b7ternd", "mei\u00b7ne", "Brust", "dir", "schwoll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPOSAT", "NN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Leis im Traum ist mir entquollen", "tokens": ["Leis", "im", "Traum", "ist", "mir", "ent\u00b7quol\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "VAFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Widerhall von deinem Grollen,", "tokens": ["Wi\u00b7der\u00b7hall", "von", "dei\u00b7nem", "Grol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schluchzend schlug ich sehnsuchtsvoll.", "tokens": ["Schluch\u00b7zend", "schlug", "ich", "sehn\u00b7suchts\u00b7voll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Pl\u00f6tzlich hat mich's ganz gezogen,", "tokens": ["Pl\u00f6tz\u00b7lich", "hat", "mich's", "ganz", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bin vom Dickicht aufgeflogen,", "tokens": ["Bin", "vom", "Di\u00b7ckicht", "auf\u00b7ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bin geflogen bis hieher.", "tokens": ["Bin", "ge\u00b7flo\u00b7gen", "bis", "hie\u00b7her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "KON", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mu\u00df nun all mein bittres Klagen,", "tokens": ["Mu\u00df", "nun", "all", "mein", "bitt\u00b7res", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PIAT", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "All mein s\u00fc\u00dfes Jauchzen schlagen", "tokens": ["All", "mein", "s\u00fc\u00b7\u00dfes", "Jauch\u00b7zen", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dir im Takte, neues Meer.", "tokens": ["Dir", "im", "Tak\u00b7te", ",", "neu\u00b7es", "Meer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Meer der Menschheit, b\u00e4umende Fluten,", "tokens": ["Meer", "der", "Menschheit", ",", "b\u00e4u\u00b7men\u00b7de", "Flu\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Meiner Seele sch\u00e4umende Gluten", "tokens": ["Mei\u00b7ner", "See\u00b7le", "sch\u00e4u\u00b7men\u00b7de", "Glu\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Spr\u00fchn euch gischtend in den Scho\u00df.", "tokens": ["Spr\u00fchn", "euch", "gischtend", "in", "den", "Scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Aus den Wolken rieselnde Strahlen,", "tokens": ["Aus", "den", "Wol\u00b7ken", "rie\u00b7seln\u00b7de", "Strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Blauer Segen aus den Qualen,", "tokens": ["Blau\u00b7er", "Se\u00b7gen", "aus", "den", "Qua\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aus der Not ein menschlich Los.", "tokens": ["Aus", "der", "Not", "ein", "menschlich", "Los", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJD", "NE", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Auf dem Maienfeld des Werde,", "tokens": ["Auf", "dem", "Mai\u00b7en\u00b7feld", "des", "Wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heil dir, junger Held der Erde,", "tokens": ["Heil", "dir", ",", "jun\u00b7ger", "Held", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Siegfried Proletariat!", "tokens": ["Sieg\u00b7fried", "Pro\u00b7le\u00b7ta\u00b7ri\u00b7at", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "++--+-+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "St\u00e4hlern in der Kraft des Sch\u00f6nen", "tokens": ["St\u00e4h\u00b7lern", "in", "der", "Kraft", "des", "Sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Tritt einher du, Spott und H\u00f6hnen", "tokens": ["Tritt", "ein\u00b7her", "du", ",", "Spott", "und", "H\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PIS", "PPER", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schweige, wo dein Morgen naht.", "tokens": ["Schwei\u00b7ge", ",", "wo", "dein", "Mor\u00b7gen", "naht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Heute mu\u00dft du h\u00e4\u00dflich darben,", "tokens": ["Heu\u00b7te", "mu\u00dft", "du", "h\u00e4\u00df\u00b7lich", "dar\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "PAV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schleppen alle Lust zu Garben", "tokens": ["Schlep\u00b7pen", "al\u00b7le", "Lust", "zu", "Gar\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PIAT", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fcr ein Rudel, sch\u00f6nheitfremd.", "tokens": ["F\u00fcr", "ein", "Ru\u00b7del", ",", "sch\u00f6n\u00b7heit\u00b7fremd", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00f6nheit mit der Seele suchend,", "tokens": ["Sch\u00f6n\u00b7heit", "mit", "der", "See\u00b7le", "su\u00b7chend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Webst du, deine Not verfluchend,", "tokens": ["Webst", "du", ",", "dei\u00b7ne", "Not", "ver\u00b7flu\u00b7chend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Grob des Lebens Sorgenhemd.", "tokens": ["Grob", "des", "Le\u00b7bens", "Sor\u00b7gen\u00b7hemd", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Neue Kr\u00e4fte seh ich gl\u00fchen,", "tokens": ["Neu\u00b7e", "Kr\u00e4f\u00b7te", "seh", "ich", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Neue S\u00e4fte seh ich bl\u00fchen,", "tokens": ["Neu\u00b7e", "S\u00e4f\u00b7te", "seh", "ich", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lichtwarm steigt die neue Welt.", "tokens": ["Licht\u00b7warm", "steigt", "die", "neu\u00b7e", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Das Gemeine weicht von Erden,", "tokens": ["Das", "Ge\u00b7mei\u00b7ne", "weicht", "von", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Was nie war, nun will es werden,", "tokens": ["Was", "nie", "war", ",", "nun", "will", "es", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "$,", "ADV", "VMFIN", "PPER", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und das Sklavenschiff zerschellt.", "tokens": ["Und", "das", "Skla\u00b7ven\u00b7schiff", "zer\u00b7schellt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Br\u00fcder, Menschheit, b\u00e4umende Fluten,", "tokens": ["Br\u00fc\u00b7der", ",", "Menschheit", ",", "b\u00e4u\u00b7men\u00b7de", "Flu\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Meines Geistes sch\u00e4umende Gluten", "tokens": ["Mei\u00b7nes", "Geis\u00b7tes", "sch\u00e4u\u00b7men\u00b7de", "Glu\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Spr\u00fchn euch gischte