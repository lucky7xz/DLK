{"dta.poem.20076": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Um Dich .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Herz, la\u00df dies Zweifeln, la\u00df dies Klauben,", "tokens": ["Herz", ",", "la\u00df", "dies", "Zwei\u00b7feln", ",", "la\u00df", "dies", "Klau\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PDS", "NN", "$,", "VVIMP", "PDS", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vor dem das Beste selbst zerf\u00e4llt,", "tokens": ["Vor", "dem", "das", "Bes\u00b7te", "selbst", "zer\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wahre Dir den Rest von Glauben", "tokens": ["Und", "wah\u00b7re", "Dir", "den", "Rest", "von", "Glau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "An Gutes noch in dieser Welt.", "tokens": ["An", "Gu\u00b7tes", "noch", "in", "die\u00b7ser", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Schau hin auf eines Weibes Z\u00fcge,", "tokens": ["Schau", "hin", "auf", "ei\u00b7nes", "Wei\u00b7bes", "Z\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Das l\u00e4chelnd auf den S\u00e4ugling blickt,", "tokens": ["Das", "l\u00e4\u00b7chelnd", "auf", "den", "S\u00e4ug\u00b7ling", "blickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und f\u00fchl\u2019s, es ist nicht alles L\u00fcge,", "tokens": ["Und", "f\u00fchl's", ",", "es", "ist", "nicht", "al\u00b7les", "L\u00fc\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VAFIN", "PTKNEG", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was uns das Leben bringt und schickt.", "tokens": ["Was", "uns", "das", "Le\u00b7ben", "bringt", "und", "schickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und Herze, willst du ganz genesen,", "tokens": ["Und", "Her\u00b7ze", ",", "willst", "du", "ganz", "ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VMFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sei selber wahr, sei selber rein!", "tokens": ["Sei", "sel\u00b7ber", "wahr", ",", "sei", "sel\u00b7ber", "rein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "$,", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was wir in Welt und Menschen lesen", "tokens": ["Was", "wir", "in", "Welt", "und", "Men\u00b7schen", "le\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist nur der eigne Wiederschein.", "tokens": ["Ist", "nur", "der", "eig\u00b7ne", "Wie\u00b7der\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}