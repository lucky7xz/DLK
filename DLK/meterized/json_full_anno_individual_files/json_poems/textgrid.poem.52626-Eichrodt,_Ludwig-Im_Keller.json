ri"}}, "stanza.3": {"line.1": {"text": "Das Horn herbei! vom Wein das Singen", "tokens": ["Das", "Horn", "her\u00b7bei", "!", "vom", "Wein", "das", "Sin\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$.", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist keine That noch und Vollbringen.", "tokens": ["Ist", "kei\u00b7ne", "That", "noch", "und", "Voll\u00b7brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADV", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das Trinken, wenn die Schale kreist,", "tokens": ["Das", "Trin\u00b7ken", ",", "wenn", "die", "Scha\u00b7le", "kreist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erkenntni\u00df schafft und Lust,", "tokens": ["Er\u00b7kennt\u00b7ni\u00df", "schafft", "und", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es ruht in jeder Form ein Geist \u2013", "tokens": ["Es", "ruht", "in", "je\u00b7der", "Form", "ein", "Geist", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O macht ihn euch bewu\u00dft!", "tokens": ["O", "macht", "ihn", "euch", "be\u00b7wu\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Das Horn herbei! wem soll er gelten,", "tokens": ["Das", "Horn", "her\u00b7bei", "!", "wem", "soll", "er", "gel\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$.", "PWS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der tiefe Trunk, den Thoren schelten?", "tokens": ["Der", "tie\u00b7fe", "Trunk", ",", "den", "Tho\u00b7ren", "schel\u00b7ten", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "O prahlet nur mit Seelenruh", "tokens": ["O", "prah\u00b7let", "nur", "mit", "See\u00b7len\u00b7ruh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und anderer Sympathie,", "tokens": ["Und", "an\u00b7de\u00b7rer", "Sym\u00b7pa\u00b7thie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wir trinken hoher Liebe zu", "tokens": ["Wir", "trin\u00b7ken", "ho\u00b7her", "Lie\u00b7be", "zu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und ihrer Poesie!", "tokens": ["Und", "ih\u00b7rer", "Poe\u00b7sie", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Das Horn herbei! und leert es muthig!", "tokens": ["Das", "Horn", "her\u00b7bei", "!", "und", "leert", "es", "mut\u00b7hig", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$.", "KON", "VVFIN", "PPER"