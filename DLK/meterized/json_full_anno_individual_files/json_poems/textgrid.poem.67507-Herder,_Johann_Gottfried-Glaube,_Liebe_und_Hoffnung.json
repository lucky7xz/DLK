{"textgrid.poem.67507": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Glaube, Liebe und Hoffnung", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heilige Grazien Ihr, Ihr Huldg\u00f6ttinnen der Menschheit,", "tokens": ["Hei\u00b7li\u00b7ge", "Gra\u00b7zi\u00b7en", "Ihr", ",", "Ihr", "Huld\u00b7g\u00f6t\u00b7tin\u00b7nen", "der", "Menschheit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "$,", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "+--+-+--+-+--+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Welch ein fr\u00f6hliches Bild malte der K\u00fcnstler in Euch!", "tokens": ["Welch", "ein", "fr\u00f6h\u00b7li\u00b7ches", "Bild", "mal\u00b7te", "der", "K\u00fcnst\u00b7ler", "in", "Euch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "APPR", "PPER", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Nicht mit Blumen, er band Euch mit der Kette des Lebens;", "tokens": ["Nicht", "mit", "Blu\u00b7men", ",", "er", "band", "Euch", "mit", "der", "Ket\u00b7te", "des", "Le\u00b7bens", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "$,", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+---+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Muntere Kinder ziehn wallend hinauf und hinab.", "tokens": ["Mun\u00b7te\u00b7re", "Kin\u00b7der", "ziehn", "wal\u00b7lend", "hin\u00b7auf", "und", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADJD", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.5": {"text": "Hoher ", "tokens": ["Ho\u00b7her"], "token_info": ["word"], "pos": ["NE"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Und aus Deinen H\u00e4nden empf\u00e4ngt sie liebende", "tokens": ["Und", "aus", "Dei\u00b7nen", "H\u00e4n\u00b7den", "emp\u00b7f\u00e4ngt", "sie", "lie\u00b7ben\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "PPER", "VVFIN"], "meter": "--+-+--+-+--", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "M\u00fche, z\u00e4rtliche M\u00fch hat die Geliebten gekn\u00fcpft.", "tokens": ["M\u00fc\u00b7he", ",", "z\u00e4rt\u00b7li\u00b7che", "M\u00fch", "hat", "die", "Ge\u00b7lieb\u00b7ten", "ge\u00b7kn\u00fcpft", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Seid getrennet uns nie, Ihr Pflegerinnen der Menschheit!", "tokens": ["Seid", "ge\u00b7tren\u00b7net", "uns", "nie", ",", "Ihr", "Pfle\u00b7ge\u00b7rin\u00b7nen", "der", "Menschheit", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "PPER", "ADV", "$,", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "Himmel wird es um uns, wo Ihr auf Erden erscheint.", "tokens": ["Him\u00b7mel", "wird", "es", "um", "uns", ",", "wo", "Ihr", "auf", "Er\u00b7den", "er\u00b7scheint", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPR", "PPER", "$,", "PWAV", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "+-+--+---+--+", "measure": "tro