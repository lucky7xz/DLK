{"dta.poem.7731": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Im heil'gen Weda hat sein Wort Gott offenbart;", "tokens": ["Im", "heil'\u00b7gen", "We\u00b7da", "hat", "sein", "Wort", "Gott", "of\u00b7fen\u00b7bart", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch sein Verst\u00e4ndnis nun, wo ist es aufbewahrt?", "tokens": ["Doch", "sein", "Ver\u00b7st\u00e4nd\u00b7nis", "nun", ",", "wo", "ist", "es", "auf\u00b7be\u00b7wahrt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "$,", "PWAV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Im Weda selber, der, in sich verst\u00e4ndlich klar,", "tokens": ["Im", "We\u00b7da", "sel\u00b7ber", ",", "der", ",", "in", "sich", "ver\u00b7st\u00e4nd\u00b7lich", "klar", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$,", "PRELS", "$,", "APPR", "PRF", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zureichend sich aus sich erkl\u00e4ret immerdar.", "tokens": ["Zu\u00b7rei\u00b7chend", "sich", "aus", "sich", "er\u00b7kl\u00e4\u00b7ret", "im\u00b7mer\u00b7dar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PRF", "APPR", "PRF", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wol so von Ursprung klar ist Gottes Wort entfaltet,", "tokens": ["Wol", "so", "von", "Ur\u00b7sprung", "klar", "ist", "Got\u00b7tes", "Wort", "ent\u00b7fal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "ADJD", "VAFIN", "NN", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Allein die Sprach', in der es spricht, ist nun veraltet.", "tokens": ["Al\u00b7lein", "die", "Sprach'", ",", "in", "der", "es", "spricht", ",", "ist", "nun", "ver\u00b7al\u00b7tet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Du, um sie zu verstehn, mu\u00dft sie erst \u00fcbertragen;", "tokens": ["Du", ",", "um", "sie", "zu", "ver\u00b7stehn", ",", "mu\u00dft", "sie", "erst", "\u00fc\u00b7ber\u00b7tra\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "$,", "VMFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ob den rechten Sinn du trafst, wer kann dirs sagen?", "tokens": ["Und", "ob", "den", "rech\u00b7ten", "Sinn", "du", "trafst", ",", "wer", "kann", "dirs", "sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,", "PWS", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "So scheint das heil'ge Wort zu rechten Sinns Erbeutung", "tokens": ["So", "scheint", "das", "heil'\u00b7ge", "Wort", "zu", "rech\u00b7ten", "Sinns", "Er\u00b7beu\u00b7tung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu fordern fort und fort ein heil'ges Amt der Deutung.", "tokens": ["Zu", "for\u00b7dern", "fort", "und", "fort", "ein", "heil'\u00b7ges", "Amt", "der", "Deu\u00b7tung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PTKVZ", "KON", "PTKVZ", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Wer aber kann und darf nun f\u00fchren dieses Amt,", "tokens": ["Wer", "a\u00b7ber", "kann", "und", "darf", "nun", "f\u00fch\u00b7ren", "die\u00b7ses", "Amt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VMFIN", "KON", "VMFIN", "ADV", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df irdisch nicht entweiht sei, was vom Himmel stammt?", "tokens": ["Da\u00df", "ir\u00b7disch", "nicht", "ent\u00b7weiht", "sei", ",", "was", "vom", "Him\u00b7mel", "stammt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVPP", "VAFIN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Zu Richtern wirft sich auf der Schriftgelehrten Zunft;", "tokens": ["Zu", "Rich\u00b7tern", "wirft", "sich", "auf", "der", "Schrift\u00b7ge\u00b7lehr\u00b7ten", "Zunft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch wir empfehlen dir Schiedsrichterin Vernunft.", "tokens": ["Doch", "wir", "emp\u00b7feh\u00b7len", "dir", "Schieds\u00b7rich\u00b7te\u00b7rin", "Ver\u00b7nunft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Und wer unf\u00e4hig mit Vernunft ist zu vernehmen,", "tokens": ["Und", "wer", "un\u00b7f\u00e4\u00b7hig", "mit", "Ver\u00b7nunft", "ist", "zu", "ver\u00b7neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADJD", "APPR", "NN", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mag unvern\u00fcnftiger Auslegung sich bequemen.", "tokens": ["Mag", "un\u00b7ver\u00b7n\u00fcnf\u00b7ti\u00b7ger", "Aus\u00b7le\u00b7gung", "sich", "be\u00b7que\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJA", "NN", "PRF", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}