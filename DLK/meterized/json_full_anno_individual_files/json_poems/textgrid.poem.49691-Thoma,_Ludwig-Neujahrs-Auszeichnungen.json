, "pos": ["VAFIN", "PIAT", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Gnadenstrahl hat nichts vergessen", "tokens": ["Der", "Gna\u00b7den\u00b7strahl", "hat", "nichts", "ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PIS", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und schien ins kleinste Schreibb\u00fcro.", "tokens": ["Und", "schien", "ins", "kleins\u00b7te", "Schreib\u00b7b\u00fc\u00b7ro", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wer immer hielt der Treue Banner,", "tokens": ["Wer", "im\u00b7mer", "hielt", "der", "Treu\u00b7e", "Ban\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der kriegt den Michel eins bis vier,", "tokens": ["Der", "kriegt", "den", "Mi\u00b7chel", "eins", "bis", "vier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NE", "PIS", "APPR", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vom Hofrat bis zum B\u00fcchsenspanner", "tokens": ["Vom", "Hof\u00b7rat", "bis", "zum", "B\u00fcch\u00b7sen\u00b7span\u00b7ner"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und vom Minister zum Hatschier.", "tokens": ["Und", "vom", "Mi\u00b7nis\u00b7ter", "zum", "Hat\u00b7schier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure"