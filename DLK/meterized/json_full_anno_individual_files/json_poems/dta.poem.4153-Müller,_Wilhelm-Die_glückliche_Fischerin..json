{"dta.poem.4153": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die gl\u00fcckliche Fischerin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.85", "id:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sah's vom Ufer her:", "tokens": ["Ich", "sah's", "vom", "U\u00b7fer", "her", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "In's Netz die Fischlein sprangen,", "tokens": ["In's", "Netz", "die", "Fisc\u00b7hlein", "spran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als ob's zum Tanze w\u00e4r';", "tokens": ["Als", "ob's", "zum", "Tan\u00b7ze", "w\u00e4r'", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wollt' keins im Meere bleiben,", "tokens": ["Wollt'", "keins", "im", "Mee\u00b7re", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das Netz war viel zu klein:", "tokens": ["Das", "Netz", "war", "viel", "zu", "klein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Sie lie\u00df es sich gefallen,", "tokens": ["Sie", "lie\u00df", "es", "sich", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und dacht', es mu\u00df so sein.", "tokens": ["Und", "dacht'", ",", "es", "mu\u00df", "so", "sein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VMFIN", "ADV", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie stieg aus ihrem Boote,", "tokens": ["Sie", "stieg", "aus", "ih\u00b7rem", "Boo\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Am Strande blieb sie stehn,", "tokens": ["Am", "Stran\u00b7de", "blieb", "sie", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da schwoll das Meer und wogte,", "tokens": ["Da", "schwoll", "das", "Meer", "und", "wog\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als m\u00f6cht' es mit ihr gehn;", "tokens": ["Als", "m\u00f6cht'", "es", "mit", "ihr", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und Muscheln und Korallen", "tokens": ["Und", "Mu\u00b7scheln", "und", "Ko\u00b7ral\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Trieb es ihr hinterdrein:", "tokens": ["Trieb", "es", "ihr", "hin\u00b7ter\u00b7drein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Sie hob sie auf vom Boden,", "tokens": ["Sie", "hob", "sie", "auf", "vom", "Bo\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und dacht', es mu\u00df so sein.", "tokens": ["Und", "dacht'", ",", "es", "mu\u00df", "so", "sein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VMFIN", "ADV", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich armer Hirtenbube,", "tokens": ["Ich", "ar\u00b7mer", "Hir\u00b7ten\u00b7bu\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was frommt mein Werben mir", "tokens": ["Was", "frommt", "mein", "Wer\u00b7ben", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit Blumen und mit B\u00e4ndern?", "tokens": ["Mit", "Blu\u00b7men", "und", "mit", "B\u00e4n\u00b7dern", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Welt geh\u00f6ret ihr.", "tokens": ["Die", "Welt", "ge\u00b7h\u00f6\u00b7ret", "ihr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ihr schlagen alle Herzen,", "tokens": ["Ihr", "schla\u00b7gen", "al\u00b7le", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und w\u00e4ren sie von Stein:", "tokens": ["Und", "w\u00e4\u00b7ren", "sie", "von", "Stein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sie nimmt's wie Wogenrauschen,", "tokens": ["Sie", "nimmt's", "wie", "Wo\u00b7gen\u00b7rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und denkt, es mu\u00df so sein.", "tokens": ["Und", "denkt", ",", "es", "mu\u00df", "so", "sein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VMFIN", "ADV", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "K\u00f6nnt' ich ihr selber bringen", "tokens": ["K\u00f6nnt'", "ich", "ihr", "sel\u00b7ber", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Sterne Silberlicht,", "tokens": ["Der", "Ster\u00b7ne", "Sil\u00b7ber\u00b7licht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des Himmels Abendbl\u00e4ue,", "tokens": ["Des", "Him\u00b7mels", "A\u00b7bend\u00b7bl\u00e4u\u00b7e", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was Neues w\u00e4r' es nicht.", "tokens": ["Was", "Neu\u00b7es", "w\u00e4r'", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie hielt's an ihre Augen,", "tokens": ["Sie", "hielt's", "an", "ih\u00b7re", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und spr\u00e4ch': es ist ja mein!", "tokens": ["Und", "spr\u00e4ch'", ":", "es", "ist", "ja", "mein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPER", "VAFIN", "ADV", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Verg\u00e4\u00dfe mir zu danken,", "tokens": ["Ver\u00b7g\u00e4\u00b7\u00dfe", "mir", "zu", "dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und d\u00e4cht', es mu\u00df so sein.", "tokens": ["Und", "d\u00e4cht'", ",", "es", "mu\u00df", "so", "sein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VMFIN", "ADV", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Was frommt dein bl\u00f6des Klingen,", "tokens": ["Was", "frommt", "dein", "bl\u00f6\u00b7des", "Klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein kleines Saitenspiel?", "tokens": ["Mein", "klei\u00b7nes", "Sai\u00b7ten\u00b7spiel", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ist auch ihr Fenster offen,", "tokens": ["Ist", "auch", "ihr", "Fens\u00b7ter", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie h\u00f6rt dich doch nicht viel,", "tokens": ["Sie", "h\u00f6rt", "dich", "doch", "nicht", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vor allen J\u00e4gerh\u00f6rnern", "tokens": ["Vor", "al\u00b7len", "J\u00e4\u00b7ger\u00b7h\u00f6r\u00b7nern"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und Fl\u00f6ten und Schalmei'n;", "tokens": ["Und", "Fl\u00f6\u00b7ten", "und", "Schal\u00b7mei'n", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Sie tanzt dazu den Reigen,", "tokens": ["Sie", "tanzt", "da\u00b7zu", "den", "Rei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und denkt, es mu\u00df so sein.", "tokens": ["Und", "denkt", ",", "es", "mu\u00df", "so", "sein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VMFIN", "ADV", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}