{"textgrid.poem.48680": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1. An meinen Erl\u00f6ser", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erh\u00f6re meine Not, du aller Not Erh\u00f6rer,", "tokens": ["Er\u00b7h\u00f6\u00b7re", "mei\u00b7ne", "Not", ",", "du", "al\u00b7ler", "Not", "Er\u00b7h\u00f6\u00b7rer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "PPER", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "hilf Helfer aller Welt, hilf mir auch, der ich mir", "tokens": ["hilf", "Hel\u00b7fer", "al\u00b7ler", "Welt", ",", "hilf", "mir", "auch", ",", "der", "ich", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "NN", "PIAT", "NN", "$,", "VVIMP", "PPER", "ADV", "$,", "PRELS", "PPER", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "selbselbst nicht helfen kan; ich suche Trost bei dir.", "tokens": ["selb\u00b7selbst", "nicht", "hel\u00b7fen", "kan", ";", "ich", "su\u00b7che", "Trost", "bei", "dir", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VVINF", "VMFIN", "$.", "PPER", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Herr, du hast Rat und Tat. Dich preisen deine Lehrer,", "tokens": ["Herr", ",", "du", "hast", "Rat", "und", "Tat", ".", "Dich", "prei\u00b7sen", "dei\u00b7ne", "Leh\u00b7rer", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "NN", "KON", "NN", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "wie du es denn auch bist, f\u00fcr einen Glaubensmehrer.", "tokens": ["wie", "du", "es", "denn", "auch", "bist", ",", "f\u00fcr", "ei\u00b7nen", "Glau\u00b7bens\u00b7meh\u00b7rer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "ADV", "VAFIN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich bin desselben leer. Hier steh' ich: ich steh' hier.", "tokens": ["Ich", "bin", "des\u00b7sel\u00b7ben", "leer", ".", "Hier", "steh'", "ich", ":", "ich", "steh'", "hier", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PDS", "ADJD", "$.", "ADV", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Erf\u00fclle mich mit dir und deines Geistes Zier.", "tokens": ["Er\u00b7f\u00fcl\u00b7le", "mich", "mit", "dir", "und", "dei\u00b7nes", "Geis\u00b7tes", "Zier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPER", "KON", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er ist es, er dein Geist, der rechte Glaubensmehrer.", "tokens": ["Er", "ist", "es", ",", "er", "dein", "Geist", ",", "der", "rech\u00b7te", "Glau\u00b7bens\u00b7meh\u00b7rer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "PPER", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.ia