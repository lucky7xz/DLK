{"dta.poem.13133": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Neuntes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eseht diesen Griffel an! der ist der Thaten Feder!", "tokens": ["\u201e", "seht", "die\u00b7sen", "Grif\u00b7fel", "an", "!", "der", "ist", "der", "Tha\u00b7ten", "Fe\u00b7der", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PDAT", "NN", "PTKVZ", "$.", "ART", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201eder gr\u00e4bt, was je geschicht, in ewig gr\u00fcne Ceder.", "tokens": ["\u201e", "der", "gr\u00e4bt", ",", "was", "je", "ge\u00b7schicht", ",", "in", "e\u00b7wig", "gr\u00fc\u00b7ne", "Ce\u00b7der", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "VVFIN", "$,", "PRELS", "ADV", "VVPP", "$,", "APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "25\u201dDer Baum ist euch bekannt: er ist das Wunder-Holz;", "tokens": ["\"", "Der", "Baum", "ist", "euch", "be\u00b7kannt", ":", "er", "ist", "das", "Wun\u00b7der\u00b7Holz", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "PPER", "PTKVZ", "$.", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201ees \u00fcbertrifft des Steins, des Erzes Macht und Stolz.", "tokens": ["\u201e", "es", "\u00fc\u00b7bert\u00b7rifft", "des", "Steins", ",", "des", "Er\u00b7zes", "Macht", "und", "Stolz", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201edie Zeit erk\u00fchnt sich nicht an ihm den Zahn zu wezen;", "tokens": ["\u201e", "die", "Zeit", "er\u00b7k\u00fchnt", "sich", "nicht", "an", "ihm", "den", "Zahn", "zu", "we\u00b7zen", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PRF", "PTKNEG", "APPR", "PPER", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201ekein Sturm kein Donner-Streich kann dessen Stam\u0303 verlezen.", "tokens": ["\u201e", "kein", "Sturm", "kein", "Don\u00b7ner\u00b7Streich", "kann", "des\u00b7sen", "Stam\u0303", "ver\u00b7le\u00b7zen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIAT", "NN", "PIAT", "NN", "VMFIN", "PDS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201eso folgt die Frag\u2019, ob ich zu diesem Vorsaz taug,", "tokens": ["\u201e", "so", "folgt", "die", "Frag'", ",", "ob", "ich", "zu", "die\u00b7sem", "Vor\u00b7saz", "taug", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "30\u201dIch, der Geschichten Stimm'; ich, aller Zeiten Aug;", "tokens": ["\"", "Ich", ",", "der", "Ge\u00b7schich\u00b7ten", "Stimm'", ";", "ich", ",", "al\u00b7ler", "Zei\u00b7ten", "Aug", ";"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "$,", "ART", "NN", "NN", "$.", "PPER", "$,", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201eman wird das, was ich schreib, so lang die Welt steht, lesen,", "tokens": ["\u201e", "man", "wird", "das", ",", "was", "ich", "schreib", ",", "so", "lang", "die", "Welt", "steht", ",", "le\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PIS", "VAFIN", "PDS", "$,", "PWS", "PPER", "VVFIN", "$,", "ADV", "ADJD", "ART", "NN", "VVFIN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201eund wissen wer das Paar, das Kronen-Paar gewesen.", "tokens": ["\u201e", "und", "wis\u00b7sen", "wer", "das", "Paar", ",", "das", "Kro\u00b7nen\u00b7Paar", "ge\u00b7we\u00b7sen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVFIN", "PWS", "ART", "NN", "$,", "ART", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201ees waltet nur in mir ein so befli\u00dfner Geist,", "tokens": ["\u201e", "es", "wal\u00b7tet", "nur", "in", "mir", "ein", "so", "be\u00b7fli\u00df\u00b7ner", "Geist", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "APPR", "PPER", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201eder den Nachk\u00f6mmlingen den Ruhm der Vorwelt weist.", "tokens": ["\u201e", "der", "den", "Nach\u00b7k\u00f6mm\u00b7lin\u00b7gen", "den", "Ruhm", "der", "Vor\u00b7welt", "weist", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ART", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "35\u201dIch kann denselbigen die l\u00e4ngst verflo\u00dfnen Sachen", "tokens": ["\"", "Ich", "kann", "den\u00b7sel\u00b7bi\u00b7gen", "die", "l\u00e4ngst", "ver\u00b7flo\u00df\u00b7nen", "Sa\u00b7chen"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VMFIN", "PDS", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "\u201edurch meine Kunst so viel als gegenw\u00e4rtig machen.", "tokens": ["\u201e", "durch", "mei\u00b7ne", "Kunst", "so", "viel", "als", "ge\u00b7gen\u00b7w\u00e4r\u00b7tig", "ma\u00b7chen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "ADV", "PIAT", "KOKOM", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}