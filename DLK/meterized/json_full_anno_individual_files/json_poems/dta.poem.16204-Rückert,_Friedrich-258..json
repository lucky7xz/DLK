{"dta.poem.16204": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "258.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sei m\u00e4\u00dfig im Genu\u00df, nicht blo\u00df gew\u00fcrzter Speisen,", "tokens": ["Sei", "m\u00e4\u00b7\u00dfig", "im", "Ge\u00b7nu\u00df", ",", "nicht", "blo\u00df", "ge\u00b7w\u00fcrz\u00b7ter", "Spei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "APPRART", "NN", "$,", "PTKNEG", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Geistiger W\u00fcrzen auch in B\u00fcchern deiner Weisen.", "tokens": ["Geis\u00b7ti\u00b7ger", "W\u00fcr\u00b7zen", "auch", "in", "B\u00fc\u00b7chern", "dei\u00b7ner", "Wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "Mit Speisen wirst du nur den Magen \u00fcberladen,", "tokens": ["Mit", "Spei\u00b7sen", "wirst", "du", "nur", "den", "Ma\u00b7gen", "\u00fc\u00b7berl\u00b7a\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch fremdes Denken kann dem eignen Denken schaden.", "tokens": ["Doch", "frem\u00b7des", "Den\u00b7ken", "kann", "dem", "eig\u00b7nen", "Den\u00b7ken", "scha\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Drum, wie du issest nur soviel du kanst verdauen,", "tokens": ["Drum", ",", "wie", "du", "is\u00b7sest", "nur", "so\u00b7viel", "du", "kanst", "ver\u00b7dau\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PWAV", "PPER", "VAFIN", "ADV", "PIS", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So lis auch mehr nicht als du brauchst dich zu erbauen.", "tokens": ["So", "lis", "auch", "mehr", "nicht", "als", "du", "brauchst", "dich", "zu", "er\u00b7bau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "ADV", "ADV", "PTKNEG", "KOUS", "PPER", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}