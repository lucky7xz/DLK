{"dta.poem.5587": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Weltweisen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ja, in dem grossen Werck fasst meine Seele Muth.", "tokens": ["Ja", ",", "in", "dem", "gros\u00b7sen", "Werck", "fasst", "mei\u00b7ne", "See\u00b7le", "Muth", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "APPR", "ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn klingt gleich mein Gesang so rein nicht, nicht so sch\u00f6ne;", "tokens": ["Denn", "klingt", "gleich", "mein", "Ge\u00b7sang", "so", "rein", "nicht", ",", "nicht", "so", "sch\u00f6\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "NN", "ADV", "ADJD", "PTKNEG", "$,", "PTKNEG", "ADV", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So sch\u00f6pff\u2019 ich doch den Inhalt meiner Th\u00f6ne", "tokens": ["So", "sch\u00f6pf\u00b7f'", "ich", "doch", "den", "In\u00b7halt", "mei\u00b7ner", "Th\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "PPOSAT", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "In einer sch\u00f6nern Quell\u2019, in einer reinern Fluth.", "tokens": ["In", "ei\u00b7ner", "sch\u00f6\u00b7nern", "Quell'", ",", "in", "ei\u00b7ner", "rei\u00b7nern", "Fluth", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich wandere beym Glanz von einem heitern Licht,", "tokens": ["Ich", "wan\u00b7de\u00b7re", "beym", "Glanz", "von", "ei\u00b7nem", "hei\u00b7tern", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vor mir steigt die Natur aus ihren Dunckelheiten,", "tokens": ["Vor", "mir", "steigt", "die", "Na\u00b7tur", "aus", "ih\u00b7ren", "Dun\u00b7ckel\u00b7hei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Ein Mann, aus unserm Volck verspricht mich zu begleiten.", "tokens": ["Ein", "Mann", ",", "aus", "un\u00b7serm", "Volck", "ver\u00b7spricht", "mich", "zu", "be\u00b7glei\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Man nimmt von diesen kaum so bald den Unterricht;", "tokens": ["Man", "nimmt", "von", "die\u00b7sen", "kaum", "so", "bald", "den", "Un\u00b7ter\u00b7richt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PDAT", "ADV", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So f\u00fchrt ein kurtzer Weg uns zu der Wahrheit hin.", "tokens": ["So", "f\u00fchrt", "ein", "kurt\u00b7zer", "Weg", "uns", "zu", "der", "Wahr\u00b7heit", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}