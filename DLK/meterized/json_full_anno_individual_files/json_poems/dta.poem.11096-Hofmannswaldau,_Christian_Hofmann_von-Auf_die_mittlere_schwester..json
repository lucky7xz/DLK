{"dta.poem.11096": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf die mittlere schwester.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du mittelpunct von meinem hertzen!", "tokens": ["Du", "mit\u00b7tel\u00b7punct", "von", "mei\u00b7nem", "hert\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Komm, sage mir fein mit bedacht,", "tokens": ["Komm", ",", "sa\u00b7ge", "mir", "fein", "mit", "be\u00b7dacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "ADJD", "APPR", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Was mich mit solchen liebes-schmertzen", "tokens": ["Was", "mich", "mit", "sol\u00b7chen", "lie\u00b7bes\u00b7schmert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hat gegen dich entz\u00fcckt gemacht?", "tokens": ["Hat", "ge\u00b7gen", "dich", "ent\u00b7z\u00fcckt", "ge\u00b7macht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ob das vielleicht die ursach ist,", "tokens": ["Ob", "das", "viel\u00b7leicht", "die", "ur\u00b7sach", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ADV", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Weil du die mittle schwester bist?", "tokens": ["Weil", "du", "die", "mitt\u00b7le", "schwes\u00b7ter", "bist", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach ja, es steckt in diesem worte", "tokens": ["Ach", "ja", ",", "es", "steckt", "in", "die\u00b7sem", "wor\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ITJ", "$,", "PPER", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gantz ohne zweifel solche krafft,", "tokens": ["Gantz", "oh\u00b7ne", "zwei\u00b7fel", "sol\u00b7che", "krafft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die auch bey mir an meinem orte,", "tokens": ["Die", "auch", "bey", "mir", "an", "mei\u00b7nem", "or\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-++--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dergleichen wunder-w\u00fcrckung schafft:", "tokens": ["Derg\u00b7lei\u00b7chen", "wun\u00b7der\u00b7w\u00fcr\u00b7ckung", "schafft", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Denn du alleine bleibst mein licht,", "tokens": ["Denn", "du", "al\u00b7lei\u00b7ne", "bleibst", "mein", "licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die andern schwestern acht ich nicht.", "tokens": ["Die", "an\u00b7dern", "schwes\u00b7tern", "acht", "ich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "CARD", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich dencke so, dieweil der kuchen", "tokens": ["Ich", "den\u00b7cke", "so", ",", "die\u00b7weil", "der", "ku\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Am besten in der mitten schmeckt;", "tokens": ["Am", "bes\u00b7ten", "in", "der", "mit\u00b7ten", "schmeckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "APPR", "ART", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So mu\u00df ich ebenfalls versuchen,", "tokens": ["So", "mu\u00df", "ich", "e\u00b7ben\u00b7falls", "ver\u00b7su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie sich die mittle schwester leckt,", "tokens": ["Wie", "sich", "die", "mitt\u00b7le", "schwes\u00b7ter", "leckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "ADJA", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und ob es dann zu glauben sey,", "tokens": ["Und", "ob", "es", "dann", "zu", "glau\u00b7ben", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df fisch und jungfern einerley?", "tokens": ["Da\u00df", "fisch", "und", "jung\u00b7fern", "ei\u00b7ner\u00b7ley", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der mittle finger laugt am weitsten;", "tokens": ["Der", "mitt\u00b7le", "fin\u00b7ger", "laugt", "am", "weits\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ach, mittle schwester! greiff nach mir!", "tokens": ["Ach", ",", "mitt\u00b7le", "schwes\u00b7ter", "!", "greiff", "nach", "mir", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "ADJD", "$.", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die mittel-wege sind am breitsten;", "tokens": ["Die", "mit\u00b7tel\u00b7we\u00b7ge", "sind", "am", "breits\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist es nun eben so bey dir?", "tokens": ["Ist", "es", "nun", "e\u00b7ben", "so", "bey", "dir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So glaub ich, da\u00df kein ehren-mann", "tokens": ["So", "glaub", "ich", ",", "da\u00df", "kein", "eh\u00b7ren\u00b7mann"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Durch dich zu falle kommen kan.", "tokens": ["Durch", "dich", "zu", "fal\u00b7le", "kom\u00b7men", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKZU", "VVFIN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Man geht doch auf den mittel-wegen", "tokens": ["Man", "geht", "doch", "auf", "den", "mit\u00b7tel\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gantz sicher nach der tugend zu;", "tokens": ["Gantz", "si\u00b7cher", "nach", "der", "tu\u00b7gend", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun ist mir viel daran gelegen,", "tokens": ["Nun", "ist", "mir", "viel", "da\u00b7ran", "ge\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich fein tugendhafftig thu.", "tokens": ["Da\u00df", "ich", "fein", "tu\u00b7gend\u00b7haff\u00b7tig", "thu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Drum, mittle schwester! gieb dich drein,", "tokens": ["Drum", ",", "mitt\u00b7le", "schwes\u00b7ter", "!", "gieb", "dich", "drein", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "VVFIN", "ADJD", "$.", "VVIMP", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df ich ein ", "tokens": ["Da\u00df", "ich", "ein"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "ART"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.6": {"line.1": {"text": "Mich deucht, als w\u00e4ren deine thaten", "tokens": ["Mich", "deucht", ",", "als", "w\u00e4\u00b7ren", "dei\u00b7ne", "tha\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOKOM", "VAFIN", "PPOSAT", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Fein mittelm\u00e4\u00dfig eingericht:", "tokens": ["Fein", "mit\u00b7tel\u00b7m\u00e4\u00b7\u00dfig", "ein\u00b7ge\u00b7richt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du bist nicht gar zu fromm gerathen,", "tokens": ["Du", "bist", "nicht", "gar", "zu", "fromm", "ge\u00b7ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "PTKA", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und doch auch gar zu lose nicht;", "tokens": ["Und", "doch", "auch", "gar", "zu", "lo\u00b7se", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "APPR", "NE", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nun bin ich eben so, mein blut!", "tokens": ["Nun", "bin", "ich", "e\u00b7ben", "so", ",", "mein", "blut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es schickt sich alles trefflich gut.", "tokens": ["Es", "schickt", "sich", "al\u00b7les", "treff\u00b7lich", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PIS", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Nicht gar zu keusch, nicht voller liebe,", "tokens": ["Nicht", "gar", "zu", "keusch", ",", "nicht", "vol\u00b7ler", "lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PTKA", "ADJD", "$,", "PTKNEG", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht gar zu arm, und nicht zu reich,", "tokens": ["Nicht", "gar", "zu", "arm", ",", "und", "nicht", "zu", "reich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PTKA", "ADJD", "$,", "KON", "PTKNEG", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht gar zu munter, nicht zu tr\u00fcbe,", "tokens": ["Nicht", "gar", "zu", "mun\u00b7ter", ",", "nicht", "zu", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PTKA", "ADJD", "$,", "PTKNEG", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht engeln, auch nicht teufeln gleich,", "tokens": ["Nicht", "en\u00b7geln", ",", "auch", "nicht", "teu\u00b7feln", "gleich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$,", "ADV", "PTKNEG", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nicht alt, nicht jung, so hin, so hin,", "tokens": ["Nicht", "alt", ",", "nicht", "jung", ",", "so", "hin", ",", "so", "hin", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "PTKNEG", "ADJD", "$,", "ADV", "PTKVZ", "$,", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So in der mitten bleibt mein sinn.", "tokens": ["So", "in", "der", "mit\u00b7ten", "bleibt", "mein", "sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Dann, wer ihn gar zu hoch will treiben,", "tokens": ["Dann", ",", "wer", "ihn", "gar", "zu", "hoch", "will", "trei\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWS", "PPER", "ADV", "PTKA", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Thut meistentheils den gr\u00f6sten fall;", "tokens": ["Thut", "meis\u00b7ten\u00b7theils", "den", "gr\u00f6s\u00b7ten", "fall", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer in der tieffe sucht zu bleiben,", "tokens": ["Wer", "in", "der", "tief\u00b7fe", "sucht", "zu", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den spottet man nur \u00fcberall;", "tokens": ["Den", "spot\u00b7tet", "man", "nur", "\u00fc\u00b7be\u00b7rall", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIS", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wer sich zu mittel-sachen h\u00e4lt,", "tokens": ["Wer", "sich", "zu", "mit\u00b7tel\u00b7sa\u00b7chen", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der dient am besten in die welt.", "tokens": ["Der", "dient", "am", "bes\u00b7ten", "in", "die", "welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKA", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Ich halte mich an dich, mein leben!", "tokens": ["Ich", "hal\u00b7te", "mich", "an", "dich", ",", "mein", "le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPER", "$,", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nur sage: Ja! Es ahnt mir wohl:", "tokens": ["Nur", "sa\u00b7ge", ":", "Ja", "!", "Es", "ahnt", "mir", "wohl", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "PTKANT", "$.", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Du wirst dich bald darein ergeben,", "tokens": ["Du", "wirst", "dich", "bald", "da\u00b7rein", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich dein eigen bleiben soll;", "tokens": ["Da\u00df", "ich", "dein", "ei\u00b7gen", "blei\u00b7ben", "soll", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Denn sonsten w\u00fcrdest du allein", "tokens": ["Denn", "sons\u00b7ten", "w\u00fcr\u00b7dest", "du", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ohn anfang