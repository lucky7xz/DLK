{"dta.poem.2387": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Mein T H Y R S J S/ du l\u00e4ssest M Y R T J L L E N zu", "tokens": ["Mein", "T", "H", "Y", "R", "S", "J", "S", "/", "du", "l\u00e4s\u00b7sest", "M", "Y", "R", "T", "J", "L", "L", "E", "N", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NE", "XY", "XY", "XY", "XY", "XY", "XY", "$(", "PPER", "VVFIN", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.2": {"text": "Ehren/", "tokens": ["Eh\u00b7ren", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Dem \u00e4delen Sch\u00e4fer dein Seitenspiel h\u00f6ren/", "tokens": ["Dem", "\u00e4\u00b7de\u00b7len", "Sch\u00e4\u00b7fer", "dein", "Sei\u00b7ten\u00b7spiel", "h\u00f6\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Du singest sein Hassen/ du singest sein Lieben", "tokens": ["Du", "sin\u00b7gest", "sein", "Has\u00b7sen", "/", "du", "sin\u00b7gest", "sein", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$(", "PPER", "VVFIN", "PPOSAT", "ADJA"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Sein Hertzenserfreuen und Seelen betr\u00fcben:", "tokens": ["Sein", "Hert\u00b7zen\u00b7ser\u00b7freu\u00b7en", "und", "See\u00b7len", "be\u00b7tr\u00fc\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Er hasset\u2019 verfluchet und scheuet die Seelen", "tokens": ["Er", "has\u00b7set'", "ver\u00b7flu\u00b7chet", "und", "scheu\u00b7et", "die", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "VVFIN", "KON", "VVFIN", "ART", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "So ihnen zur Liebe den Wankelmuht wehlen;", "tokens": ["So", "ih\u00b7nen", "zur", "Lie\u00b7be", "den", "Wan\u00b7kel\u00b7muht", "weh\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.8": {"text": "Die \u00e4dele Seel\u2019 E V F R O S J L L E N zu lieben", "tokens": ["Die", "\u00e4\u00b7de\u00b7le", "Seel'", "E", "V", "F", "R", "O", "S", "J", "L", "L", "E", "N", "zu", "lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "CARD", "XY", "XY", "XY", "XY", "XY", "XY", "XY", "XY", "XY", "PTKZU", "VVINF"], "meter": "-+--+-+-+-+--+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.9": {"text": "Dis stehet M Y R T J L L E N im Hertzen geschrieben.", "tokens": ["Dis", "ste\u00b7het", "M", "Y", "R", "T", "J", "L", "L", "E", "N", "im", "Hert\u00b7zen", "ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NE", "XY", "XY", "XY", "XY", "XY", "XY", "XY", "XY", "APPRART", "NN", "VVPP", "$."], "meter": "+--+-+---+-+-+--+-", "measure": "iambic.septa.invert"}, "line.10": {"text": "Halt deine T H A L J A mein T H Y R S J S im Hertzen/", "tokens": ["Halt", "dei\u00b7ne", "T", "H", "A", "L", "J", "A", "mein", "T", "H", "Y", "R", "S", "J", "S", "im", "Hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NE", "XY", "NE", "NE", "NE", "NE", "PPOSAT", "NE", "XY", "XY", "XY", "XY", "XY", "XY", "APPRART", "NN", "$("], "meter": "+--+--+-+-+-+-+-+-+-", "measure": "dactylic.di.plus"}, "line.11": {"text": "Daneben der Keuschheit hochr\u00fchmliches Schertzen/", "tokens": ["Da\u00b7ne\u00b7ben", "der", "Keuschheit", "hoch\u00b7r\u00fchm\u00b7li\u00b7ches", "Schert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Sie lohnen dir wieder des Momus sein g\u00f6nnen.", "tokens": ["Sie", "loh\u00b7nen", "dir", "wie\u00b7der", "des", "Mo\u00b7mus", "sein", "g\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "PPOSAT", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Und S Y L V J V S bleibet in Warheit zu nennen:", "tokens": ["Und", "S", "Y", "L", "V", "J", "V", "S", "blei\u00b7bet", "in", "War\u00b7heit", "zu", "nen\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "XY", "XY", "XY", "XY", "XY", "XY", "VVFIN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}}}}}