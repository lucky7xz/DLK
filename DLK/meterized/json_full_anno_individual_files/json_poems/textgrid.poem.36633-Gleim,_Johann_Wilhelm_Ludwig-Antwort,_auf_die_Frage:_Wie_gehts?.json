, "die", "lie\u00b7be", "hei\u00b7li\u00b7ge"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "ITJ", "$.", "ART", "ADJA", "ADJA"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Familie, uns nahm!", "tokens": ["Fa\u00b7mi\u00b7lie", ",", "uns", "nahm", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Wo weinen sie den Abschied noch?", "tokens": ["Wo", "wei\u00b7nen", "sie", "den", "Ab\u00b7schied", "noch", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo macht der Wagner Halt?", "tokens": ["Wo", "macht", "der", "Wag\u00b7ner", "Halt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo jammert Ihnen Menschenjoch?", "tokens": ["Wo", "jam\u00b7mert", "Ih\u00b7nen", "Men\u00b7schen\u00b7joch", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo sehn sie Hermanns Wald?", "tokens": ["Wo", "sehn", "sie", "Her\u00b7manns", "Wald", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "NE", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Von unsrer Herzen Sympathie", "tokens": ["Von", "uns\u00b7rer", "Her\u00b7zen", "Sym\u00b7pa\u00b7thie"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sprach ihr bethr\u00e4nter Blick!", "tokens": ["Sprach", "ihr