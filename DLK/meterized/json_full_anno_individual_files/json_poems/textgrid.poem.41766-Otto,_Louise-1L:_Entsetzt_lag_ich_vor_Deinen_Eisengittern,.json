{"textgrid.poem.41766": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Entsetzt lag ich vor Deinen Eisengittern,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Entsetzt lag ich vor Deinen Eisengittern,", "tokens": ["Ent\u00b7setzt", "lag", "ich", "vor", "Dei\u00b7nen", "Ei\u00b7sen\u00b7git\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Weil ich umsonst gestrebt Dich zu erretten,", "tokens": ["Weil", "ich", "um\u00b7sonst", "ge\u00b7strebt", "Dich", "zu", "er\u00b7ret\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Indes sie Dich auf hartem Pf\u00fchle betten.", "tokens": ["In\u00b7des", "sie", "Dich", "auf", "har\u00b7tem", "Pf\u00fch\u00b7le", "bet\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Trank ich den Kelch der Leiden still, den bittern.", "tokens": ["Trank", "ich", "den", "Kelch", "der", "Lei\u00b7den", "still", ",", "den", "bit\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ART", "NN", "PTKVZ", "$,", "ART", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch h\u00f6rt ich auf zu bangen und zu zittern,", "tokens": ["Doch", "h\u00f6rt", "ich", "auf", "zu", "ban\