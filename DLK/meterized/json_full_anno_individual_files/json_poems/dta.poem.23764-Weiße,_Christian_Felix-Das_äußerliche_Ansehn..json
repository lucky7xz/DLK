{"dta.poem.23764": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "Das \u00e4u\u00dferliche Ansehn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20643-5", "language": ["de:0.99"], "booktitle": "Wei\u00dfe, Christian Felix: Lieder f\u00fcr Kinder. Leipzig, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Unter sch\u00f6n gewachsnen B\u00e4umen", "tokens": ["Un\u00b7ter", "sch\u00f6n", "ge\u00b7wachs\u00b7nen", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stand ein niedrer krummer Baum:", "tokens": ["Stand", "ein", "nie\u00b7drer", "krum\u00b7mer", "Baum", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie in ihrer Hoheit Tr\u00e4umen,", "tokens": ["Sie", "in", "ih\u00b7rer", "Ho\u00b7heit", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "G\u00f6nnten ihm das Leben kaum:", "tokens": ["G\u00f6nn\u00b7ten", "ihm", "das", "Le\u00b7ben", "kaum", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "O k\u00f6mmt nur der Zimmermann,", "tokens": ["O", "k\u00f6mmt", "nur", "der", "Zim\u00b7mer\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.6": {"text": "Sprachen sie, so mu\u00df du dran!", "tokens": ["Spra\u00b7chen", "sie", ",", "so", "mu\u00df", "du", "dran", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "VMFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch schon k\u00f6mmt er angestiegen, &#8212;", "tokens": ["Doch", "schon", "k\u00f6mmt", "er", "an\u00b7ge\u00b7stie\u00b7gen", ",", "&#8212;"], "token_info": ["word", "word", "word", "word", "word", "punct", "XML_entity"], "pos": ["KON", "ADV", "VVFIN", "PPER", "VVPP", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie? was f\u00e4llt dem Thoren ein?", "tokens": ["Wie", "?", "was", "f\u00e4llt", "dem", "Tho\u00b7ren", "ein", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PWS", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie bemerkt er mit Vergn\u00fcgen,", "tokens": ["Sie", "be\u00b7merkt", "er", "mit", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sollts auf sie gem\u00fcnzet seyn?", "tokens": ["Sollts", "auf", "sie", "ge\u00b7m\u00fcn\u00b7zet", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Himmel! alle haut er um,", "tokens": ["Him\u00b7mel", "!", "al\u00b7le", "haut", "er", "um", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PIS", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dieser bleibt, denn er war krumm.", "tokens": ["Die\u00b7ser", "bleibt", ",", "denn", "er", "war", "krumm", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "KON", "PPER", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "O man trotze nicht auf Erden", "tokens": ["O", "man", "trot\u00b7ze", "nicht", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "PIS", "VVFIN", "PTKNEG", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf Gestalt und \u00e4u\u00dfre Pracht:", "tokens": ["Auf", "Ge\u00b7stalt", "und", "\u00e4u\u00df\u00b7re", "Pracht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das kann oft zum Fall uns werden,", "tokens": ["Das", "kann", "oft", "zum", "Fall", "uns", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "APPRART", "NN", "PPER", "VAINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Was uns stolz und eitel macht.", "tokens": ["Was", "uns", "stolz", "und", "ei\u00b7tel", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer nicht sehr ins Auge f\u00e4llt,", "tokens": ["Wer", "nicht", "sehr", "ins", "Au\u00b7ge", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Den beneidet nicht die Welt.", "tokens": ["Den", "be\u00b7nei\u00b7det", "nicht", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}