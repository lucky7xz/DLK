{"dta.poem.9950": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "59.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Niemahls la\u00df ich mich nicht schauen/ lufft die ist mein element/", "tokens": ["Nie\u00b7mahls", "la\u00df", "ich", "mich", "nicht", "schau\u00b7en", "/", "lufft", "die", "ist", "mein", "e\u00b7le\u00b7ment", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PRF", "PTKNEG", "VVINF", "$(", "VVFIN", "PDS", "VAFIN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-+--+--+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Sterb und lebe wieder auff tausendmahl in einem tage.", "tokens": ["Sterb", "und", "le\u00b7be", "wie\u00b7der", "auff", "tau\u00b7send\u00b7mahl", "in", "ei\u00b7nem", "ta\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "ADV", "APPR", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Red und habe keine zunge/ antwort iedem der mich nennt/", "tokens": ["Red", "und", "ha\u00b7be", "kei\u00b7ne", "zun\u00b7ge", "/", "ant\u00b7wort", "ie\u00b7dem", "der", "mich", "nennt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VAFIN", "PIAT", "NN", "$(", "ADV", "PIAT", "ART", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Diesem/ der mich reden macht/ alle w\u00f6rter nach ich sage.", "tokens": ["Die\u00b7sem", "/", "der", "mich", "re\u00b7den", "macht", "/", "al\u00b7le", "w\u00f6r\u00b7ter", "nach", "ich", "sa\u00b7ge", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "$(", "PRELS", "PPER", "VVINF", "VVFIN", "$(", "PIAT", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+++-+-+-+-", "measure": "unknown.measure.octa.plus"}}}}}