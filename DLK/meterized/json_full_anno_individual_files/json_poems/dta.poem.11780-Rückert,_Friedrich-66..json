{"dta.poem.11780": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "66.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das irdische an dir, Gesch\u00f6pf, sind deine Glieder,", "tokens": ["Das", "ir\u00b7di\u00b7sche", "an", "dir", ",", "Ge\u00b7sch\u00f6pf", ",", "sind", "dei\u00b7ne", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "PPER", "$,", "NN", "$,", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+++-+-+-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "Vom Himmel hast du, sollst du haben dein Gefieder.", "tokens": ["Vom", "Him\u00b7mel", "hast", "du", ",", "sollst", "du", "ha\u00b7ben", "dein", "Ge\u00b7fie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "$,", "VMFIN", "PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dein Vorbild sei, o Mensch, so lang du Raupe bist,", "tokens": ["Dein", "Vor\u00b7bild", "sei", ",", "o", "Mensch", ",", "so", "lang", "du", "Rau\u00b7pe", "bist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$,", "FM", "NN", "$,", "ADV", "ADJD", "PPER", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Schmetterling, der ganz Fl\u00fcgel geworden ist.", "tokens": ["Der", "Schmet\u00b7ter\u00b7ling", ",", "der", "ganz", "Fl\u00fc\u00b7gel", "ge\u00b7wor\u00b7den", "ist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "NN", "VAPP", "VAFIN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Die edle Pflanze hat ein Baum sich ausgegliedert,", "tokens": ["Die", "ed\u00b7le", "Pflan\u00b7ze", "hat", "ein", "Baum", "sich", "aus\u00b7ge\u00b7glie\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und oben schwebt das Blatt im Sonnenschein gefiedert.", "tokens": ["Und", "o\u00b7ben", "schwebt", "das", "Blatt", "im", "Son\u00b7nen\u00b7schein", "ge\u00b7fie\u00b7dert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Sei von des Himmels Thau, der Pflanze gleich, begossen,", "tokens": ["Sei", "von", "des", "Him\u00b7mels", "Thau", ",", "der", "Pflan\u00b7ze", "gleich", ",", "be\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "NN", "$,", "ART", "NN", "ADV", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df wie an ihr das Blatt, an dir die Fl\u00fcgel sprossen!", "tokens": ["Da\u00df", "wie", "an", "ihr", "das", "Blatt", ",", "an", "dir", "die", "Fl\u00fc\u00b7gel", "spros\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "APPR", "PPER", "ART", "NN", "$,", "APPR", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ums Haupt der Sch\u00f6nheit wallt dem Laube gleich die Locke,", "tokens": ["Ums", "Haupt", "der", "Sch\u00f6n\u00b7heit", "wallt", "dem", "Lau\u00b7be", "gleich", "die", "Lo\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "ART", "NN", "VVFIN", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Himmelsl\u00fcfte sie zum Spiel herniederlocke.", "tokens": ["Das", "Him\u00b7mel\u00b7sl\u00fcf\u00b7te", "sie", "zum", "Spiel", "her\u00b7nie\u00b7der\u00b7lo\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Und wenn dich selbst es lockt zu spielen mit dem Duft", "tokens": ["Und", "wenn", "dich", "selbst", "es", "lockt", "zu", "spie\u00b7len", "mit", "dem", "Duft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "PPER", "VVFIN", "PTKZU", "VVINF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Locken, spiele fein mit ihm wie Himmelsluft.", "tokens": ["Der", "Lo\u00b7cken", ",", "spie\u00b7le", "fein", "mit", "ihm", "wie", "Him\u00b7mel\u00b7sluft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "ADJD", "APPR", "PPER", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Der Lock' ermangelt ein behaarter Thieretro\u00df;", "tokens": ["Der", "Lock'", "er\u00b7man\u00b7gelt", "ein", "be\u00b7haar\u00b7ter", "Thie\u00b7re\u00b7tro\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bem\u00e4hnt ist edel nur der Leu und stolz das Ro\u00df.", "tokens": ["Be\u00b7m\u00e4hnt", "ist", "e\u00b7del", "nur", "der", "Leu", "und", "stolz", "das", "Ro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ADJD", "ADV", "ART", "NN", "KON", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Den V\u00f6geln aber sind die Fl\u00fcgel angeboren,", "tokens": ["Den", "V\u00f6\u00b7geln", "a\u00b7ber", "sind", "die", "Fl\u00fc\u00b7gel", "an\u00b7ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die V\u00f6gel haben sie behalten, wir verloren.", "tokens": ["Die", "V\u00f6\u00b7gel", "ha\u00b7ben", "sie", "be\u00b7hal\u00b7ten", ",", "wir", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVINF", "$,", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Da\u00df du sie hattest, mahnt gefl\u00fcgelt dich der Traum,", "tokens": ["Da\u00df", "du", "sie", "hat\u00b7test", ",", "mahnt", "ge\u00b7fl\u00fc\u00b7gelt", "dich", "der", "Traum", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VAFIN", "$,", "VVFIN", "VVPP", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Beschwingten G\u00f6ttern gleich dich fl\u00fcgelnd \u00fcbern Raum.", "tokens": ["Be\u00b7schwing\u00b7ten", "G\u00f6t\u00b7tern", "gleich", "dich", "fl\u00fc\u00b7gelnd", "\u00fc\u00b7bern", "Raum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "PPER", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Nicht ehr behalten dort dich G\u00f6tter zum Genossen,", "tokens": ["Nicht", "ehr", "be\u00b7hal\u00b7ten", "dort", "dich", "G\u00f6t\u00b7ter", "zum", "Ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "VVFIN", "ADV", "PPER", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus innrer G\u00f6ttlichkeit bis dir die Fl\u00fcgel sprossen;", "tokens": ["Aus", "inn\u00b7rer", "G\u00f6tt\u00b7lich\u00b7keit", "bis", "dir", "die", "Fl\u00fc\u00b7gel", "spros\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Bis \u2014 also kreist in sich mein Lied \u2014 ins Morgenroth", "tokens": ["Bis", "al\u00b7so", "kreist", "in", "sich", "mein", "Lied", "ins", "Mor\u00b7gen\u00b7roth"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "$(", "ADV", "VVFIN", "APPR", "PRF", "PPOSAT", "NN", "$(", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Entschwebt der Schmetterling, dem Eins ist Lieb' und Tod.", "tokens": ["Ent\u00b7schwebt", "der", "Schmet\u00b7ter\u00b7ling", ",", "dem", "Eins", "ist", "Lieb'", "und", "Tod", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ART", "NN", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}