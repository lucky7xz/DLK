{"dta.poem.8370": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Martin Opitzen Sch\u00e4fferey  \n von der Nimfen Hercinie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1630", "urn": "urn:nbn:de:kobv:b4-200905197863", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Sch\u00e4fferey Von der Nimfen Hercinie. Breslau, 1630."}, "poem": {"stanza.1": {"line.1": {"text": "Vertrawe dich der see/ dem Frawenzimmer nicht;", "tokens": ["Ver\u00b7tra\u00b7we", "dich", "der", "see", "/", "dem", "Fra\u00b7wen\u00b7zim\u00b7mer", "nicht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$(", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die weil kein gla\u00df so baldt als jhre gunst zerbricht.", "tokens": ["Die", "weil", "kein", "gla\u00df", "so", "baldt", "als", "jhre", "gunst", "zer\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOUS", "PIAT", "NN", "ADV", "ADV", "KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Kein weib ist guet/ vndt ist ja eines oder zwey/", "tokens": ["Kein", "weib", "ist", "guet", "/", "vndt", "ist", "ja", "ei\u00b7nes", "o\u00b7der", "zwey", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADJD", "$(", "KON", "VAFIN", "ADV", "ART", "KON", "CARD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So wei\u00df ich nicht wie guet au\u00df b&#242;sem worden sey.", "tokens": ["So", "wei\u00df", "ich", "nicht", "wie", "guet", "au\u00df", "b", "&#242;", "sem", "wor\u00b7den", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "XML_entity", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "KOKOM", "VVFIN", "APPR", "XY", "XY", "XY", "VAPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}