{"textgrid.poem.53475": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Deutscher Abend", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun g\u00f6nnt die Firma stillen Abendfrieden", "tokens": ["Nun", "g\u00f6nnt", "die", "Fir\u00b7ma", "stil\u00b7len", "A\u00b7bend\u00b7frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "dem Arbeitsmann, den M\u00e4dels, dem Kommis \u2013", "tokens": ["dem", "Ar\u00b7beits\u00b7mann", ",", "den", "M\u00e4\u00b7dels", ",", "dem", "Kom\u00b7mis", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "nun sitzt ganz Deutschland um den runden, lieben", "tokens": ["nun", "sitzt", "ganz", "Deutschland", "um", "den", "run\u00b7den", ",", "lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "ADV", "NE", "APPR", "ART", "ADJA", "$,", "VVINF"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "gedeckten Tisch und sieht aufs Visavis.", "tokens": ["ge\u00b7deck\u00b7ten", "Tisch", "und", "sieht", "aufs", "Vi\u00b7sa\u00b7vis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da liegt das Land: ganz schwarz und blau und dunkel.", "tokens": ["Da", "liegt", "das", "Land", ":", "ganz", "schwarz", "und", "blau", "und", "dun\u00b7kel", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "ADV", "ADJD", "KON", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es klirrt der Wind im Telegrafendraht.", "tokens": ["Es", "klirrt", "der", "Wind", "im", "Te\u00b7le\u00b7gra\u00b7fen\u00b7draht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein gelbes Fenster gr\u00fc\u00dft dich mit Gefunkel:", "tokens": ["Ein", "gel\u00b7bes", "Fens\u00b7ter", "gr\u00fc\u00dft", "dich", "mit", "Ge\u00b7fun\u00b7kel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "hier spielt der F\u00f6rster seinen Dauerskat.", "tokens": ["hier", "spielt", "der", "F\u00f6rs\u00b7ter", "sei\u00b7nen", "Dau\u00b7ers\u00b7kat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Man hebt die Zeitung, l\u00e4\u00dft sie wieder sinken,", "tokens": ["Man", "hebt", "die", "Zei\u00b7tung", ",", "l\u00e4\u00dft", "sie", "wie\u00b7der", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "$,", "VVFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "die Welt, ihr Lieben, geht den alten Lauf \u2013", "tokens": ["die", "Welt", ",", "ihr", "Lie\u00b7ben", ",", "geht", "den", "al\u00b7ten", "Lauf", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "ADJA", "$,", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "hieraufbez\u00fcglich kann man einen trinken,", "tokens": ["hier\u00b7auf\u00b7be\u00b7z\u00fcg\u00b7lich", "kann", "man", "ei\u00b7nen", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ART", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "die Pfeife qualmt, nun steigt der Mond herauf.", "tokens": ["die", "Pfei\u00b7fe", "qualmt", ",", "nun", "steigt", "der", "Mond", "her\u00b7auf", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und hundert Mimen spreizen ihre Glieder,", "tokens": ["Und", "hun\u00b7dert", "Mi\u00b7men", "sprei\u00b7zen", "ih\u00b7re", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und hundert B\u00fcrger f\u00fcllen sich mit Bier . . .", "tokens": ["und", "hun\u00b7dert", "B\u00fcr\u00b7ger", "f\u00fcl\u00b7len", "sich", "mit", "Bier", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "CARD", "NN", "VVFIN", "PRF", "APPR", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und hundert M\u00e4dchen summen kleine Lieder,", "tokens": ["Und", "hun\u00b7dert", "M\u00e4d\u00b7chen", "sum\u00b7men", "klei\u00b7ne", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["K