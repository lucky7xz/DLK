{"textgrid.poem.34423": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1885, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stahlblauer Aether trage meine Schwingen,", "tokens": ["Stahl\u00b7blau\u00b7er", "A\u00b7e\u00b7ther", "tra\u00b7ge", "mei\u00b7ne", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "++-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "goldrothe Sonn hast meine Kraft erregt!", "tokens": ["gol\u00b7dro\u00b7the", "Sonn", "hast", "mei\u00b7ne", "Kraft", "er\u00b7regt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Nun will ich laut und immer lauter singen", "tokens": ["Nun", "will", "ich", "laut", "und", "im\u00b7mer", "lau\u00b7ter", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "KON", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "das Lied, das mir der Dank ins Herz gelegt!", "tokens": ["das", "Lied", ",", "das", "mir", "der", "Dank", "ins", "Herz", "ge\u00b7legt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein frommer Heide, will ich Opfer bringen,", "tokens": ["Ein", "from\u00b7mer", "Hei\u00b7de", ",", "will", "ich", "Op\u00b7fer", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "dir G\u00f6ttin, die den Busen mir bewegt,", "tokens": ["dir", "G\u00f6t\u00b7tin", ",", "die", "den", "Bu\u00b7sen", "mir", "be\u00b7wegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "und wenn im Gl\u00fccke meine Lippen schweigen,", "tokens": ["und", "wenn", "im", "Gl\u00fc\u00b7cke", "mei\u00b7ne", "Lip\u00b7pen", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPRART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "will ich mein Haupt, von Gnaden schwer, dir neigen!", "tokens": ["will", "ich", "mein", "Haupt", ",", "von", "Gna\u00b7den", "schwer", ",", "dir", "nei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "$,", "APPR", "NN", "ADJD", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Im Jugendrausche stamml ich deinen Namen,", "tokens": ["Im", "Ju\u00b7gen\u00b7drau\u00b7sche", "stamml", "ich", "dei\u00b7nen", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "beseelende, o Venus, Herrscherin,", "tokens": ["be\u00b7see\u00b7len\u00b7de", ",", "o", "Ve\u00b7nus", ",", "Herr\u00b7sche\u00b7rin", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "$,", "FM", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und mein Gebet beschliesst kein kaltes Amen,", "tokens": ["und", "mein", "Ge\u00b7bet", "be\u00b7schliesst", "kein", "kal\u00b7tes", "A\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "mein Kuss bezeugt, wie tief ich dankbar bin!", "tokens": ["mein", "Kuss", "be\u00b7zeugt", ",", "wie", "tief", "ich", "dank\u00b7bar", "bin", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PWAV", "ADJD", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Tage gingen und die Tage kamen:", "tokens": ["Die", "Ta\u00b7ge", "gin\u00b7gen", "und", "die", "Ta\u00b7ge", "ka\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "ich war so still \u2013 was fiebert heut mein Sinn?", "tokens": ["ich", "war", "so", "still", "\u2013", "was", "fie\u00b7bert", "heut", "mein", "Sinn", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$(", "PWS", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Was weckt in meiner Brust den Strom der Lieder?", "tokens": ["Was", "weckt", "in", "mei\u00b7ner", "Brust", "den", "Strom", "der", "Lie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "ART", "NN", "$."], 