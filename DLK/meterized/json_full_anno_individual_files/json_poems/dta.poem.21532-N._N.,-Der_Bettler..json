{"dta.poem.21532": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Der Bettler.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Das Leben ist sch\u00f6n, ein Scherzen, ein Singen,", "tokens": ["Das", "Le\u00b7ben", "ist", "sch\u00f6n", ",", "ein", "Scher\u00b7zen", ",", "ein", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein Necken, ein Kosen, Gewinnen, Gelingen,", "tokens": ["Ein", "Ne\u00b7cken", ",", "ein", "Ko\u00b7sen", ",", "Ge\u00b7win\u00b7nen", ",", "Ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ein duftiger Fr\u00fchlingssonnenglanz!", "tokens": ["Ein", "duf\u00b7ti\u00b7ger", "Fr\u00fch\u00b7lings\u00b7son\u00b7nen\u00b7glanz", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Noch nimmer hatte das Gl\u00fcck mir getrogen,", "tokens": ["Noch", "nim\u00b7mer", "hat\u00b7te", "das", "Gl\u00fcck", "mir", "ge\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ich hab\u2019s von den purpurnen Lippen gesogen,", "tokens": ["Ich", "hab's", "von", "den", "pur\u00b7pur\u00b7nen", "Lip\u00b7pen", "ge\u00b7so\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Ein L\u00e4cheln von dir \u2014 und ich traute ihm ganz.", "tokens": ["Ein", "L\u00e4\u00b7cheln", "von", "dir", "und", "ich", "trau\u00b7te", "ihm", "ganz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "$(", "KON", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Die sonnigen Tage, die heiteren Stunden,", "tokens": ["Die", "son\u00b7ni\u00b7gen", "Ta\u00b7ge", ",", "die", "hei\u00b7te\u00b7ren", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wie sind sie so hastig dahingeschwunden \u2014", "tokens": ["Wie", "sind", "sie", "so", "has\u00b7tig", "da\u00b7hin\u00b7ge\u00b7schwun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADJD", "VVINF", "$("], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Wie war doch so wenig im Leben gegl\u00fcckt:", "tokens": ["Wie", "war", "doch", "so", "we\u00b7nig", "im", "Le\u00b7ben", "ge\u00b7gl\u00fcckt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "ADV", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Nicht konnt\u2019 ich euch halten, nicht wieder fassen,", "tokens": ["Nicht", "konnt'", "ich", "euch", "hal\u00b7ten", ",", "nicht", "wie\u00b7der", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "PPER", "PRF", "VVINF", "$,", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Die Geliebte mu\u00dft\u2019 aus den Armen ich lassen,", "tokens": ["Die", "Ge\u00b7lieb\u00b7te", "mu\u00dft'", "aus", "den", "Ar\u00b7men", "ich", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Habe die Augen ihr zugedr\u00fcckt.", "tokens": ["Ha\u00b7be", "die", "Au\u00b7gen", "ihr", "zu\u00b7ge\u00b7dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Im w\u00fcsten Leben, im Taumel der Lust,", "tokens": ["Im", "w\u00fcs\u00b7ten", "Le\u00b7ben", ",", "im", "Tau\u00b7mel", "der", "Lust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "VVFIN", "NN", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mein wundes Herz in der todtkranken Brust,", "tokens": ["Mein", "wun\u00b7des", "Herz", "in", "der", "todt\u00b7kran\u00b7ken", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das wollte nimmer genesen. \u2014", "tokens": ["Das", "woll\u00b7te", "nim\u00b7mer", "ge\u00b7ne\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VMFIN", "ADV", "VVPP", "$.", "$("], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Verspielt, verjubelt mein Gl\u00fcck und mein Geld", "tokens": ["Ver\u00b7spielt", ",", "ver\u00b7ju\u00b7belt", "mein", "Gl\u00fcck", "und", "mein", "Geld"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "VVFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und getauscht daf\u00fcr ein die Schande der Welt \u2014", "tokens": ["Und", "ge\u00b7tauscht", "da\u00b7f\u00fcr", "ein", "die", "Schan\u00b7de", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "PAV", "ART", "ART", "NN", "ART", "NN", "$("], "meter": "--+----+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Und Disteln von Dornen gelesen!", "tokens": ["Und", "Dis\u00b7teln", "von", "Dor\u00b7nen", "ge\u00b7le\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Mich hungert und friert an der kalten Wand", "tokens": ["Mich", "hun\u00b7gert", "und", "friert", "an", "der", "kal\u00b7ten", "Wand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Den kahlen Hut in der zitternden Hand;", "tokens": ["Den", "kah\u00b7len", "Hut", "in", "der", "zit\u00b7tern\u00b7den", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Was kann mich erquicken, was letzen?", "tokens": ["Was", "kann", "mich", "er\u00b7qui\u00b7cken", ",", "was", "let\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VVINF", "$,", "PWS", "VVINF", "$."], "meter": "++--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Und wie mir\u2019s ged\u00e4mmert, so kam\u2019s, so kam\u2019s,", "tokens": ["Und", "wie", "mir's", "ge\u00b7d\u00e4m\u00b7mert", ",", "so", "kam's", ",", "so", "kam's", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWAV", "NE", "VVPP", "$,", "ADV", "VVFIN", "$,", "ADV", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Gebrochen mein Herze und wie mein Wamms", "tokens": ["Ge\u00b7bro\u00b7chen", "mein", "Her\u00b7ze", "und", "wie", "mein", "Wamms"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "VVFIN", "KON", "KOKOM", "PPOSAT", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Zerrissen in Lumpen und Fetzen!", "tokens": ["Zer\u00b7ris\u00b7sen", "in", "Lum\u00b7pen", "und", "Fet\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}