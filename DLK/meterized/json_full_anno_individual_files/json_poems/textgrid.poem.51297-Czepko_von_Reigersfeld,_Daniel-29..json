{"textgrid.poem.51297": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "29.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist wohl, so lang' ein Mensch der Welt noch ist ergeben,", "tokens": ["Ist", "wohl", ",", "so", "lang'", "ein", "Mensch", "der", "Welt", "noch", "ist", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "ADV", "ADV", "ART", "NN", "ART", "NN", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In allem, was er thut, im Tod und auch im Leben,", "tokens": ["In", "al\u00b7lem", ",", "was", "er", "thut", ",", "im", "Tod", "und", "auch", "im", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PWS", "PPER", "VVFIN", "$,", "APPRART", "NN", "KON", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er schl\u00e4ffet oder wacht, in Wollust oder Pein,", "tokens": ["Er", "schl\u00e4f\u00b7fet", "o\u00b7der", "wacht", ",", "in", "Wol\u00b7lust", "o\u00b7der", "Pein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er hoffet oder w\u00fcnscht Ruh anzutreffen? Nein.", "tokens": ["Er", "hof\u00b7fet", "o\u00b7der", "w\u00fcnscht", "Ruh", "an\u00b7zu\u00b7tref\u00b7fen", "?", "Nein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "NN", "VVIZU", "$.", "PTKANT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jedoch begehret er nicht einen Tritt zu schreiten,", "tokens": ["Je\u00b7doch", "be\u00b7ge\u00b7hret", "er", "nicht", "ei\u00b7nen", "Tritt", "zu", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Er sieht sich keinmal umb, wendt sich zu keiner Seiten,", "tokens": ["Er", "sieht", "sich", "kein\u00b7mal", "umb", ",", "wendt", "sich", "zu", "kei\u00b7ner", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "PTKVZ", "$,", "VVFIN", "PRF", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Kein E\u00dfen stillet ihn, kein Auge macht er zu,", "tokens": ["Kein", "E\u00b7\u00dfen", "stil\u00b7let", "ihn", ",", "kein", "Au\u00b7ge", "macht", "er", "zu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "$,", "PIAT", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Streckt keine Hand nicht aus, sucht er nicht drinnen Ruh.", "tokens": ["Streckt", "kei\u00b7ne", "Hand", "nicht", "aus", ",", "sucht", "er", "nicht", "drin\u00b7nen", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PTKNEG", "PTKVZ", "$,", "VVFIN", "PPER", "PTKNEG", "ADV", "NN", "$."], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.9": {"text": "Ruh schreyt ein iedes Ding. Ruh ist ein Ziel der Sachen,", "tokens": ["Ruh", "schreyt", "ein", "ie\u00b7des", "Ding", ".", "Ruh", "ist", "ein", "Ziel", "der", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "PIAT", "NN", "$.", "NN", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und was das gr\u00f6ste, Got wil selbst zur Ruh sich machen.", "tokens": ["Und", "was", "das", "gr\u00f6s\u00b7te", ",", "Got", "wil", "selbst", "zur", "Ruh", "sich", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "ADJA", "$,", "NN", "VMFIN", "ADV", "APPRART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ja sprichst du, welcher ist, der Gott erforschen kan?", "tokens": ["Ja", "sprichst", "du", ",", "wel\u00b7cher", "ist", ",", "der", "Gott", "er\u00b7for\u00b7schen", "kan", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "PPER", "$,", "PWAT", "VAFIN", "$,", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "O Mensch, du triffst ihn ja in allen Dingen an.", "tokens": ["O", "Mensch", ",", "du", "triffst", "ihn", "ja", "in", "al\u00b7len", "Din\u00b7gen", "an", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich sehe, du wilt fort, bleib wilt du Ruh ergr\u00fcnden,", "tokens": ["Ic