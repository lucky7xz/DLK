{"dta.poem.5621": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vom Geist und C\u00f6rper", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Erwachsen, dencke man: man k\u00e4m' erst auf die Welt.", "tokens": ["Er\u00b7wach\u00b7sen", ",", "den\u00b7cke", "man", ":", "man", "k\u00e4m'", "erst", "auf", "die", "Welt", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "PIS", "$.", "PIS", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gleich wird uns eine Welt, die neu ist, vorgestellt.", "tokens": ["Gleich", "wird", "uns", "ei\u00b7ne", "Welt", ",", "die", "neu", "ist", ",", "vor\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "$,", "PRELS", "ADJD", "VAFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}