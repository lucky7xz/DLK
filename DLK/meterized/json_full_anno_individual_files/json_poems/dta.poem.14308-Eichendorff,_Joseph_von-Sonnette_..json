{"dta.poem.14308": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Sonnette .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ein M\u00e4dchen, schlicht und fromm wie jene Zeiten,", "tokens": ["Ein", "M\u00e4d\u00b7chen", ",", "schlicht", "und", "fromm", "wie", "je\u00b7ne", "Zei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "KON", "ADJD", "KOKOM", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das, von den Abendscheinen angeflogen,", "tokens": ["Das", ",", "von", "den", "A\u00b7bend\u00b7schei\u00b7nen", "an\u00b7ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Versenkt in solcher Stille tiefen Wogen \u2014", "tokens": ["Ver\u00b7senkt", "in", "sol\u00b7cher", "Stil\u00b7le", "tie\u00b7fen", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIAT", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das mocht' auf Bildern oft das Herz mir weiten.", "tokens": ["Das", "mocht'", "auf", "Bil\u00b7dern", "oft", "das", "Herz", "mir", "wei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPR", "NN", "ADV", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}