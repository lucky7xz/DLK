{"textgrid.poem.41521": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am Feste Mari\u00e4 Lichtme\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch die Gassen geht Maria,", "tokens": ["Durch", "die", "Gas\u00b7sen", "geht", "Ma\u00b7ria", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem Arm den Sohn, den lieben,", "tokens": ["In", "dem", "Arm", "den", "Sohn", ",", "den", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00e4lt ihn fest, und h\u00e4lt ihn linde,", "tokens": ["H\u00e4lt", "ihn", "fest", ",", "und", "h\u00e4lt", "ihn", "lin\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ihr Auge schaut auf ihn;", "tokens": ["Und", "ihr", "Au\u00b7ge", "schaut", "auf", "ihn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie die Englein ihn gesungen,", "tokens": ["Wie", "die", "En\u00b7glein", "ihn", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihn die Hirten angebetet,", "tokens": ["Ihn", "die", "Hir\u00b7ten", "an\u00b7ge\u00b7be\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Huldigten die grauen Weisen,", "tokens": ["Hul\u00b7dig\u00b7ten", "die", "grau\u00b7en", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "L\u00e4\u00dft sie still vor\u00fcberziehn.", "tokens": ["L\u00e4\u00dft", "sie", "still", "vor\u00b7\u00fc\u00b7ber\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aber Josef ihr zur Seiten", "tokens": ["A\u00b7ber", "Jo\u00b7sef", "ihr", "zur", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NE", "PPOSAT", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist in Sorgfalt ganz befangen,", "tokens": ["Ist", "in", "Sorg\u00b7falt", "ganz", "be\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Pr\u00fcfend fr\u00e4gt er alle Steine,", "tokens": ["Pr\u00fc\u00b7fend", "fr\u00e4gt", "er", "al\u00b7le", "Stei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ob ihr Fu\u00df zu k\u00fchn sich wagt;", "tokens": ["Ob", "ihr", "Fu\u00df", "zu", "k\u00fchn", "sich", "wagt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKA", "ADJD", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wei\u00df nicht was er wird erleben,", "tokens": ["Wei\u00df", "nicht", "was", "er", "wird", "er\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PWS", "PPER", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aber wunderbare Dinge", "tokens": ["A\u00b7ber", "wun\u00b7der\u00b7ba\u00b7re", "Din\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Haben aus des Kindleins Augen", "tokens": ["Ha\u00b7ben", "aus", "des", "Kin\u00b7dleins", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sich ihm heimlich angesagt.", "tokens": ["Sich", "ihm", "heim\u00b7lich", "an\u00b7ge\u00b7sagt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "O Maria, Mutter Christi,", "tokens": ["O", "Ma\u00b7ria", ",", "Mut\u00b7ter", "Chris\u00b7ti", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NN", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nicht zu dir will ich mich wagen,", "tokens": ["Nicht", "zu", "dir", "will", "ich", "mich", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPER", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn du bist mir viel zu helle,", "tokens": ["Denn", "du", "bist", "mir", "viel", "zu", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ADV", "APPR", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine Seel' ergraut vor dir,", "tokens": ["Mei\u00b7ne", "Seel'", "er\u00b7graut", "vor", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bist mir fast wie zum Entsetzen", "tokens": ["Bist", "mir", "fast", "wie", "zum", "Ent\u00b7set\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "KOKOM", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In der fleckenlosen Reine,", "tokens": ["In", "der", "fle\u00b7cken\u00b7lo\u00b7sen", "Rei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Die du siegreich hast bewahret,", "tokens": ["Die", "du", "sieg\u00b7reich", "hast", "be\u00b7wah\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da du wandeltest gleich mir.", "tokens": ["Da", "du", "wan\u00b7del\u00b7test", "gleich", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADV", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Will viel lieber vor dein Kindlein", "tokens": ["Will", "viel", "lie\u00b7ber", "vor", "dein", "Kin\u00b7dlein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Treten, weinend und zerschlagen.", "tokens": ["Tre\u00b7ten", ",", "wei\u00b7nend", "und", "zer\u00b7schla\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVPP", "KON", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist er wohl mein Herr und Richter,", "tokens": ["Ist", "er", "wohl", "mein", "Herr", "und", "Rich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und du stehst mir minder weit,", "tokens": ["Und", "du", "stehst", "mir", "min\u00b7der", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Einer Torheit mu\u00df ich zollen,", "tokens": ["Ei\u00b7ner", "Tor\u00b7heit", "mu\u00df", "ich", "zol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Soll ich nicht in Furcht zerst\u00e4uben,", "tokens": ["Soll", "ich", "nicht", "in", "Furcht", "zer\u00b7st\u00e4u\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Hat er doch nicht \u00fcberwunden,", "tokens": ["Hat", "er", "doch", "nicht", "\u00fc\u00b7berw\u00b7un\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ist der Held von Ewigkeit!", "tokens": ["Ist", "der", "Held", "von", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Liebster Herr, du hast geschaffen", "tokens": ["Liebs\u00b7ter", "Herr", ",", "du", "hast", "ge\u00b7schaf\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine arme kranke Seele,", "tokens": ["Mei\u00b7ne", "ar\u00b7me", "kran\u00b7ke", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie den Reiz, den vielgestalten,", "tokens": ["Wie", "den", "Reiz", ",", "den", "viel\u00b7ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der auf breite Stra\u00dfen f\u00fchrt;", "tokens": ["Der", "auf", "brei\u00b7te", "Stra\u00b7\u00dfen", "f\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und du wei\u00dft, da\u00df wie vor andern", "tokens": ["Und", "du", "wei\u00dft", ",", "da\u00df", "wie", "vor", "an\u00b7dern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "KOKOM", "APPR", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Frischer Hauch in meiner Seele,", "tokens": ["Fri\u00b7scher", "Hauch", "in", "mei\u00b7ner", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "So mich auch vor andern gl\u00fchend", "tokens": ["So", "mich", "auch", "vor", "an\u00b7dern", "gl\u00fc\u00b7hend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "ADV", "APPR", "PIS", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Jede Erdenlust ber\u00fchrt!", "tokens": ["Je\u00b7de", "Er\u00b7den\u00b7lust", "be\u00b7r\u00fchrt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Hast du mir zu reichen Kr\u00e4ften", "tokens": ["Hast", "du", "mir", "zu", "rei\u00b7chen", "Kr\u00e4f\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch ein reiches Amt verliehen,", "tokens": ["Auch", "ein", "rei\u00b7ches", "Amt", "ver\u00b7lie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Reiche G\u00fcter zu verwalten", "tokens": ["Rei\u00b7che", "G\u00fc\u00b7ter", "zu", "ver\u00b7wal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ein hohes, reiches Schlo\u00df,", "tokens": ["Und", "ein", "ho\u00b7hes", ",", "rei\u00b7ches", "Schlo\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und nun liegt es in Zerst\u00f6rung,", "tokens": ["Und", "nun", "liegt", "es", "in", "Zer\u00b7st\u00f6\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Graunvoll in der \u00f6den Gr\u00f6\u00dfe,", "tokens": ["Graun\u00b7voll", "in", "der", "\u00f6\u00b7den", "Gr\u00f6\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie ein kn\u00f6chern Ungeheuer,", "tokens": ["Wie", "ein", "kn\u00f6\u00b7chern", "Un\u00b7ge\u00b7heu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie ein toter Meerkolo\u00df.", "tokens": ["Wie", "ein", "to\u00b7ter", "Meer\u00b7ko\u00b7lo\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und da ich nach vielen Tagen,", "tokens": ["Und", "da", "ich", "nach", "vie\u00b7len", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sonder Glauben, voll der Liebe,", "tokens": ["Son\u00b7der", "Glau\u00b7ben", ",", "voll", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Angstvoll pr\u00fcfte seine Mauern,", "tokens": ["Angst\u00b7voll", "pr\u00fcf\u00b7te", "sei\u00b7ne", "Mau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Siehe da, sie standen fest!", "tokens": ["Sie\u00b7he", "da", ",", "sie", "stan\u00b7den", "fest", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "O mein Herr willst du mich h\u00f6ren,", "tokens": ["O", "mein", "Herr", "willst", "du", "mich", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auftun deine Gnadensch\u00e4tze,", "tokens": ["Auf\u00b7tun", "dei\u00b7ne", "Gna\u00b7den\u00b7sch\u00e4t\u00b7ze", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sieh ich will getreulich bauen", "tokens": ["Sieh", "ich", "will", "ge\u00b7treu\u00b7lich", "bau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "VMFIN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Meines Lebens tr\u00fcben Rest.", "tokens": ["Mei\u00b7nes", "Le\u00b7bens", "tr\u00fc\u00b7ben", "Rest", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Mu\u00df mein Haus gleich stehen eine", "tokens": ["Mu\u00df", "mein", "Haus", "gleich", "ste\u00b7hen", "ei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "VVFIN", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00d6de warnende Ruine,", "tokens": ["\u00d6\u00b7de", "war\u00b7nen\u00b7de", "Ru\u00b7i\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Ach, nur dort mag sich gestalten,", "tokens": ["Ach", ",", "nur", "dort", "mag", "sich", "ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "ADV", "VMFIN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was so rettungslos zerst\u00f6rt.", "tokens": ["Was", "so", "ret\u00b7tungs\u00b7los", "zer\u00b7st\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Kann ich nur ein St\u00fcbchen bauen,", "tokens": ["Kann", "ich", "nur", "ein", "St\u00fcb\u00b7chen", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ausgeschm\u00fcckt mit stillen Werken,", "tokens": ["Aus\u00b7ge\u00b7schm\u00fcckt", "mit", "stil\u00b7len", "Wer\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wo ich, Herr, dich kann bewirten,", "tokens": ["Wo", "ich", ",", "Herr", ",", "dich", "kann", "be\u00b7wir\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "NN", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wenn du bei mir eingekehrt.", "tokens": ["Wenn", "du", "bei", "mir", "ein\u00b7ge\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Aus den Hallen tritt Maria,", "tokens": ["Aus", "den", "Hal\u00b7len", "tritt", "Ma\u00b7ria", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem Arm den Sohn, den lieben,", "tokens": ["In", "dem", "Arm", "den", "Sohn", ",", "den", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00e4lt ihn fest und halt ihn linde,", "tokens": ["H\u00e4lt", "ihn", "fest", "und", "halt", "ihn", "lin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "PPER", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und auf ihm ihr Auge ruht.", "tokens": ["Und", "auf", "ihm", "ihr", "Au\u00b7ge", "ruht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "O sie hat das Gl\u00fcck getragen", "tokens": ["O", "sie", "hat", "das", "Gl\u00fcck", "ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "VAFIN", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Durch neun wonnevolle Monde;", "tokens": ["Durch", "neun", "won\u00b7ne\u00b7vol\u00b7le", "Mon\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Was verk\u00fcndet jene Frommen,", "tokens": ["Was", "ver\u00b7k\u00fcn\u00b7det", "je\u00b7ne", "From\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Trug sie l\u00e4ngst im gl\u00fchnden Mut.", "tokens": ["Trug", "sie", "l\u00e4ngst", "im", "gl\u00fchn\u00b7den", "Mut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Aber Josef stillen Schrittes,", "tokens": ["A\u00b7ber", "Jo\u00b7sef", "stil\u00b7len", "Schrit\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tritt nicht mehr an ihre Seite,", "tokens": ["Tritt", "nicht", "mehr", "an", "ih\u00b7re", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Da das liebe, liebe Kindlein", "tokens": ["Da", "das", "lie\u00b7be", ",", "lie\u00b7be", "Kin\u00b7dlein"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PDS", "VVFIN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nun der Herr der ganzen Welt,", "tokens": ["Nun", "der", "Herr", "der", "gan\u00b7zen", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch wie h\u00f6her steigt die Sonne,", "tokens": ["Doch", "wie", "h\u00f6\u00b7her", "steigt", "die", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schleicht er leis an ihre Schulter,", "tokens": ["Schleicht", "er", "leis", "an", "ih\u00b7re", "Schul\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und er zupft an ihrem Mantel,", "tokens": ["Und", "er", "zupft", "an", "ih\u00b7rem", "Man\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df der Schleier niederf\u00e4llt.", "tokens": ["Da\u00df", "der", "Schlei\u00b7er", "nie\u00b7der\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Durch die Gassen geht Maria,", "tokens": ["Durch", "die", "Gas\u00b7sen", "geht", "Ma\u00b7ria", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem Arm den Sohn, den lieben,", "tokens": ["In", "dem", "Arm", "den", "Sohn", ",", "den", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00e4lt ihn fest, und h\u00e4lt ihn linde,", "tokens": ["H\u00e4lt", "ihn", "fest", ",", "und", "h\u00e4lt", "ihn", "lin\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ihr Auge schaut auf ihn;", "tokens": ["Und", "ihr", "Au\u00b7ge", "schaut", "auf", "ihn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie die Englein ihn gesungen,", "tokens": ["Wie", "die", "En\u00b7glein", "ihn", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihn die Hirten angebetet,", "tokens": ["Ihn", "die", "Hir\u00b7ten", "an\u00b7ge\u00b7be\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Huldigten die grauen Weisen,", "tokens": ["Hul\u00b7dig\u00b7ten", "die", "grau\u00b7en", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "L\u00e4\u00dft sie still vor\u00fcberziehn.", "tokens": ["L\u00e4\u00dft", "sie", "still", "vor\u00b7\u00fc\u00b7ber\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Aber Josef ihr zur Seiten", "tokens": ["A\u00b7ber", "Jo\u00b7sef", "ihr", "zur", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NE", "PPOSAT", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist in Sorgfalt ganz befangen,", "tokens": ["Ist", "in", "Sorg\u00b7falt", "ganz", "be\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Pr\u00fcfend fr\u00e4gt er alle Steine,", "tokens": ["Pr\u00fc\u00b7fend", "fr\u00e4gt", "er", "al\u00b7le", "Stei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ob ihr Fu\u00df zu k\u00fchn sich wagt;", "tokens": ["Ob", "ihr", "Fu\u00df", "zu", "k\u00fchn", "sich", "wagt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKA", "ADJD", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wei\u00df nicht was er wird erleben,", "tokens": ["Wei\u00df", "nicht", "was", "er", "wird", "er\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PWS", "PPER", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aber wunderbare Dinge", "tokens": ["A\u00b7ber", "wun\u00b7der\u00b7ba\u00b7re", "Din\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Haben aus des Kindleins Augen", "tokens": ["Ha\u00b7ben", "aus", "des", "Kin\u00b7dleins", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sich ihm heimlich angesagt.", "tokens": ["Sich", "ihm", "heim\u00b7lich", "an\u00b7ge\u00b7sagt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "O Maria, Mutter Christi,", "tokens": ["O", "Ma\u00b7ria", ",", "Mut\u00b7ter", "Chris\u00b7ti", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NN", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nicht zu dir will ich mich wagen,", "tokens": ["Nicht", "zu", "dir", "will", "ich", "mich", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPER", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn du bist mir viel zu helle,", "tokens": ["Denn", "du", "bist", "mir", "viel", "zu", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ADV", "APPR", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine Seel' ergraut vor dir,", "tokens": ["Mei\u00b7ne", "Seel'", "er\u00b7graut", "vor", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bist mir fast wie zum Entsetzen", "tokens": ["Bist", "mir", "fast", "wie", "zum", "Ent\u00b7set\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "KOKOM", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In der fleckenlosen Reine,", "tokens": ["In", "der", "fle\u00b7cken\u00b7lo\u00b7sen", "Rei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Die du siegreich hast bewahret,", "tokens": ["Die", "du", "sieg\u00b7reich", "hast", "be\u00b7wah\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da du wandeltest gleich mir.", "tokens": ["Da", "du", "wan\u00b7del\u00b7test", "gleich", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADV", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Will viel lieber vor dein Kindlein", "tokens": ["Will", "viel", "lie\u00b7ber", "vor", "dein", "Kin\u00b7dlein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Treten, weinend und zerschlagen.", "tokens": ["Tre\u00b7ten", ",", "wei\u00b7nend", "und", "zer\u00b7schla\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVPP", "KON", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist er wohl mein Herr und Richter,", "tokens": ["Ist", "er", "wohl", "mein", "Herr", "und", "Rich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und du stehst mir minder weit,", "tokens": ["Und", "du", "stehst", "mir", "min\u00b7der", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Einer Torheit mu\u00df ich zollen,", "tokens": ["Ei\u00b7ner", "Tor\u00b7heit", "mu\u00df", "ich", "zol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Soll ich nicht in Furcht zerst\u00e4uben,", "tokens": ["Soll", "ich", "nicht", "in", "Furcht", "zer\u00b7st\u00e4u\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Hat er doch nicht \u00fcberwunden,", "tokens": ["Hat", "er", "doch", "nicht", "\u00fc\u00b7berw\u00b7un\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ist der Held von Ewigkeit!", "tokens": ["Ist", "der", "Held", "von", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Liebster Herr, du hast geschaffen", "tokens": ["Liebs\u00b7ter", "Herr", ",", "du", "hast", "ge\u00b7schaf\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine arme kranke Seele,", "tokens": ["Mei\u00b7ne", "ar\u00b7me", "kran\u00b7ke", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie den Reiz, den vielgestalten,", "tokens": ["Wie", "den", "Reiz", ",", "den", "viel\u00b7ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der auf breite Stra\u00dfen f\u00fchrt;", "tokens": ["Der", "auf", "brei\u00b7te", "Stra\u00b7\u00dfen", "f\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und du wei\u00dft, da\u00df wie vor andern", "tokens": ["Und", "du", "wei\u00dft", ",", "da\u00df", "wie", "vor", "an\u00b7dern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "KOKOM", "APPR", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Frischer Hauch in meiner Seele,", "tokens": ["Fri\u00b7scher", "Hauch", "in", "mei\u00b7ner", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "So mich auch vor andern gl\u00fchend", "tokens": ["So", "mich", "auch", "vor", "an\u00b7dern", "gl\u00fc\u00b7hend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "ADV", "APPR", "PIS", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Jede Erdenlust ber\u00fchrt!", "tokens": ["Je\u00b7de", "Er\u00b7den\u00b7lust", "be\u00b7r\u00fchrt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Hast du mir zu reichen Kr\u00e4ften", "tokens": ["Hast", "du", "mir", "zu", "rei\u00b7chen", "Kr\u00e4f\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch ein reiches Amt verliehen,", "tokens": ["Auch", "ein", "rei\u00b7ches", "Amt", "ver\u00b7lie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Reiche G\u00fcter zu verwalten", "tokens": ["Rei\u00b7che", "G\u00fc\u00b7ter", "zu", "ver\u00b7wal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ein hohes, reiches Schlo\u00df,", "tokens": ["Und", "ein", "ho\u00b7hes", ",", "rei\u00b7ches", "Schlo\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und nun liegt es in Zerst\u00f6rung,", "tokens": ["Und", "nun", "liegt", "es", "in", "Zer\u00b7st\u00f6\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Graunvoll in der \u00f6den Gr\u00f6\u00dfe,", "tokens": ["Graun\u00b7voll", "in", "der", "\u00f6\u00b7den", "Gr\u00f6\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie ein kn\u00f6chern Ungeheuer,", "tokens": ["Wie", "ein", "kn\u00f6\u00b7chern", "Un\u00b7ge\u00b7heu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie ein toter Meerkolo\u00df.", "tokens": ["Wie", "ein", "to\u00b7ter", "Meer\u00b7ko\u00b7lo\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Und da ich nach vielen Tagen,", "tokens": ["Und", "da", "ich", "nach", "vie\u00b7len", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sonder Glauben, voll der Liebe,", "tokens": ["Son\u00b7der", "Glau\u00b7ben", ",", "voll", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Angstvoll pr\u00fcfte seine Mauern,", "tokens": ["Angst\u00b7voll", "pr\u00fcf\u00b7te", "sei\u00b7ne", "Mau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Siehe da, sie standen fest!", "tokens": ["Sie\u00b7he", "da", ",", "sie", "stan\u00b7den", "fest", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "O mein Herr willst du mich h\u00f6ren,", "tokens": ["O", "mein", "Herr", "willst", "du", "mich", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auftun deine Gnadensch\u00e4tze,", "tokens": ["Auf\u00b7tun", "dei\u00b7ne", "Gna\u00b7den\u00b7sch\u00e4t\u00b7ze", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sieh ich will getreulich bauen", "tokens": ["Sieh", "ich", "will", "ge\u00b7treu\u00b7lich", "bau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "VMFIN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Meines Lebens tr\u00fcben Rest.", "tokens": ["Mei\u00b7nes", "Le\u00b7bens", "tr\u00fc\u00b7ben", "Rest", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "Mu\u00df mein Haus gleich stehen eine", "tokens": ["Mu\u00df", "mein", "Haus", "gleich", "ste\u00b7hen", "ei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "VVFIN", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00d6de warnende Ruine,", "tokens": ["\u00d6\u00b7de", "war\u00b7nen\u00b7de", "Ru\u00b7i\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Ach, nur dort mag sich gestalten,", "tokens": ["Ach", ",", "nur", "dort", "mag", "sich", "ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "ADV", "VMFIN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was so rettungslos zerst\u00f6rt.", "tokens": ["Was", "so", "ret\u00b7tungs\u00b7los", "zer\u00b7st\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Kann ich nur ein St\u00fcbchen bauen,", "tokens": ["Kann", "ich", "nur", "ein", "St\u00fcb\u00b7chen", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ausgeschm\u00fcckt mit stillen Werken,", "tokens": ["Aus\u00b7ge\u00b7schm\u00fcckt", "mit", "stil\u00b7len", "Wer\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wo ich, Herr, dich kann bewirten,", "tokens": ["Wo", "ich", ",", "Herr", ",", "dich", "kann", "be\u00b7wir\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "NN", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wenn du bei mir eingekehrt.", "tokens": ["Wenn", "du", "bei", "mir", "ein\u00b7ge\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.19": {"line.1": {"text": "Aus den Hallen tritt Maria,", "tokens": ["Aus", "den", "Hal\u00b7len", "tritt", "Ma\u00b7ria", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem Arm den Sohn, den lieben,", "tokens": ["In", "dem", "Arm", "den", "Sohn", ",", "den", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00e4lt ihn fest und halt ihn linde,", "tokens": ["H\u00e4lt", "ihn", "fest", "und", "halt", "ihn", "lin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "PPER", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und auf ihm ihr Auge ruht.", "tokens": ["Und", "auf", "ihm", "ihr", "Au\u00b7ge", "ruht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "O sie hat das Gl\u00fcck getragen", "tokens": ["O", "sie", "hat", "das", "Gl\u00fcck", "ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "VAFIN", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Durch neun wonnevolle Monde;", "tokens": ["Durch", "neun", "won\u00b7ne\u00b7vol\u00b7le", "Mon\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Was verk\u00fcndet jene Frommen,", "tokens": ["Was", "ver\u00b7k\u00fcn\u00b7det", "je\u00b7ne", "From\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Trug sie l\u00e4ngst im gl\u00fchnden Mut.", "tokens": ["Trug", "sie", "l\u00e4ngst", "im", "gl\u00fchn\u00b7den", "Mut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.20": {"line.1": {"text": "Aber Josef stillen Schrittes,", "tokens": ["A\u00b7ber", "Jo\u00b7sef", "stil\u00b7len", "Schrit\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tritt nicht mehr an ihre Seite,", "tokens": ["Tritt", "nicht", "mehr", "an", "ih\u00b7re", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Da das liebe, liebe Kindlein", "tokens": ["Da", "das", "lie\u00b7be", ",", "lie\u00b7be", "Kin\u00b7dlein"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PDS", "VVFIN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nun der Herr der ganzen Welt,", "tokens": ["Nun", "der", "Herr", "der", "gan\u00b7zen", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch wie h\u00f6her steigt die Sonne,", "tokens": ["Doch", "wie", "h\u00f6\u00b7her", "steigt", "die", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schleicht er leis an ihre Schulter,", "tokens": ["Schleicht", "er", "leis", "an", "ih\u00b7re", "Schul\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und er zupft an ihrem Mantel,", "tokens": ["Und", "er", "zupft", "an", "ih\u00b7rem", "Man\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df der Schleier niederf\u00e4llt.", "tokens": ["Da\u00df", "der", "Schlei\u00b7er", "nie\u00b7der\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}