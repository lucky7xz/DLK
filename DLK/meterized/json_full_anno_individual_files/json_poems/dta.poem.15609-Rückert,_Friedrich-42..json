{"dta.poem.15609": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "42.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was zu beweisen ist, ist auch zu widerlegen,", "tokens": ["Was", "zu", "be\u00b7wei\u00b7sen", "ist", ",", "ist", "auch", "zu", "wi\u00b7der\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKZU", "VVINF", "VAFIN", "$,", "VAFIN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Drum sollst du jegliches Beweisen niederlegen.", "tokens": ["Drum", "sollst", "du", "jeg\u00b7li\u00b7ches", "Be\u00b7wei\u00b7sen", "nie\u00b7der\u00b7le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Auf Ueberzeugung steh, da stehst du unbeweglich,", "tokens": ["Auf", "Ue\u00b7ber\u00b7zeu\u00b7gung", "steh", ",", "da", "stehst", "du", "un\u00b7be\u00b7weg\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die unbeweislich ist, darum unwiderleglich.", "tokens": ["Die", "un\u00b7be\u00b7weis\u00b7lich", "ist", ",", "da\u00b7rum", "un\u00b7wi\u00b7der\u00b7leg\u00b7lich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "$,", "PAV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}