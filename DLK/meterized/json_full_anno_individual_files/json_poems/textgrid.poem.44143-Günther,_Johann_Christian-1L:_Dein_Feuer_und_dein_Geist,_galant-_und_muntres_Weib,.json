 "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und, weil ihm alles geht, aus tollem Wahne meint,", "tokens": ["Und", ",", "weil", "ihm", "al\u00b7les", "geht", ",", "aus", "tol\u00b7lem", "Wah\u00b7ne", "meint", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "PIS", "VVFIN", "$,", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es k\u00f6nn' ein jeder Mensch sein eigen Gl\u00fccke machen,", "tokens": ["Es", "k\u00f6nn'", "ein", "je\u00b7der", "Mensch", "sein", "ei\u00b7gen", "Gl\u00fc\u00b7cke", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "PIAT", "NN", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dar\u00fcber mu\u00df ich so als wie die Warheit lachen.", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "mu\u00df", "ich", "so", "als", "wie", "die", "War\u00b7heit", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "KOUS", "KOKOM", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.14": {"line.1": {"text": "Die Polsche Barbarey hat mich ganz fremden Gast", "tokens": ["Die", "Pol\u00b7sche", "Bar\u00b7ba\u00b7rey", "hat", "mich", "ganz", "frem\u00b7den", "Gast"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit eu\u00dferlicher Gunst ein Vierthel Jahr umfast", "tokens": ["Mit", "eu\u00b7\u00dfer\u00b7li\u00b7cher", "Gunst", "ein", "Vier\u00b7thel", "Jahr", "um\u