{"textgrid.poem.44143": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dein Feuer und dein Geist, galant- und muntres Weib,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Feuer und dein Geist, galant- und muntres Weib,", "tokens": ["Dein", "Feu\u00b7er", "und", "dein", "Geist", ",", "ga\u00b7lant", "und", "mun\u00b7tres", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,", "TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sind Kennern rechter Kunst ein s\u00fc\u00dfer Zeitvertreib", "tokens": ["Sind", "Ken\u00b7nern", "rech\u00b7ter", "Kunst", "ein", "s\u00fc\u00b7\u00dfer", "Zeit\u00b7ver\u00b7treib"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und haben neulich erst ein Meisterst\u00fcck erwiesen,", "tokens": ["Und", "ha\u00b7ben", "neu\u00b7lich", "erst", "ein", "Meis\u00b7ter\u00b7st\u00fcck", "er\u00b7wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das zwar den F\u00fcrsten ehrt, doch dich selbst mehr gepriesen.", "tokens": ["Das", "zwar", "den", "F\u00fcrs\u00b7ten", "ehrt", ",", "doch", "dich", "selbst", "mehr", "ge\u00b7prie\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "VVFIN", "$,", "KON", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So fahre munter fort und las der Misgunst Schreyn", "tokens": ["So", "fah\u00b7re", "mun\u00b7ter", "fort", "und", "las", "der", "Mis\u00b7gunst", "Schreyn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "PTKVZ", "KON", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein nettes Lautenspiel nur immerhin entweihn;", "tokens": ["Dein", "net\u00b7tes", "Lau\u00b7ten\u00b7spiel", "nur", "im\u00b7mer\u00b7hin", "ent\u00b7weihn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Nachruhm ist ein Schaz, den hohe Seelen finden", "tokens": ["Der", "Nach\u00b7ruhm", "ist", "ein", "Schaz", ",", "den", "ho\u00b7he", "See\u00b7len", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und kluge Dichter stets um ihren Lorbeer winden.", "tokens": ["Und", "klu\u00b7ge", "Dich\u00b7ter", "stets", "um", "ih\u00b7ren", "Lor\u00b7beer", "win\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich, den zum Theil der Neid, zum Theil auch Schwachheitsschuld", "tokens": ["Ich", ",", "den", "zum", "Theil", "der", "Neid", ",", "zum", "Theil", "auch", "Schwach\u00b7heits\u00b7schuld"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "APPRART", "NN", "ART", "NN", "$,", "APPRART", "NN", "ADV", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ins Labyrinth gebracht, geh jezo mit Gedult,", "tokens": ["Ins", "La\u00b7by\u00b7rinth", "ge\u00b7bracht", ",", "geh", "je\u00b7zo", "mit", "Ge\u00b7dult", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wohin die Schickung winckt, und will mit Sehn und Schweigen", "tokens": ["Wo\u00b7hin", "die", "Schi\u00b7ckung", "winckt", ",", "und", "will", "mit", "Sehn", "und", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,", "KON", "VMFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem Gl\u00fccke, das mich \u00e4ft, ein starck Gem\u00fcthe zeigen.", "tokens": ["Dem", "Gl\u00fc\u00b7cke", ",", "das", "mich", "\u00e4ft", ",", "ein", "starck", "Ge\u00b7m\u00fc\u00b7the", "zei\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ich sehe so vorl\u00e4ngst, was man nicht \u00e4ndern kan,", "tokens": ["Ich", "se\u00b7he", "so", "vor\u00b7l\u00e4ngst", ",", "was", "man", "nicht", "\u00e4n\u00b7dern", "kan", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "PRELS", "PIS", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie alles in der Welt mit gleichen Augen an,", "tokens": ["Wie", "al\u00b7les", "in", "der", "Welt", "mit", "glei\u00b7chen", "Au\u00b7gen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da Leute be\u00dfrer Kunst und von weit reichern Gaben", "tokens": ["Da", "Leu\u00b7te", "be\u00df\u00b7rer", "Kunst", "und", "von", "weit", "rei\u00b7chern", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ADJA", "NN", "KON", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vor diesem noch wohl mehr als ich gelidten haben.", "tokens": ["Vor", "die\u00b7sem", "noch", "wohl", "mehr", "als", "ich", "ge\u00b7lid\u00b7ten", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADV", "ADV", "ADV", "KOUS", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Da\u00df mancher unsrer Zeit so gro\u00df als altklug scheint", "tokens": ["Da\u00df", "man\u00b7cher", "uns\u00b7rer", "Zeit", "so", "gro\u00df", "als", "alt\u00b7klug", "scheint"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "PPOSAT", "NN", "ADV", "ADJD", "KOKOM", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und, weil ihm alles geht, aus tollem Wahne meint,", "tokens": ["Und", ",", "weil", "ihm", "al\u00b7les", "geht", ",", "aus", "tol\u00b7lem", "Wah\u00b7ne", "meint", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "PIS", "VVFIN", "$,", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es k\u00f6nn' ein jeder Mensch sein eigen Gl\u00fccke machen,", "tokens": ["Es", "k\u00f6nn'", "ein", "je\u00b7der", "Mensch", "sein", "ei\u00b7gen", "Gl\u00fc\u00b7cke", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "PIAT", "NN", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dar\u00fcber mu\u00df ich so als wie die Warheit lachen.", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "mu\u00df", "ich", "so", "als", "wie", "die", "War\u00b7heit", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "KOUS", "KOKOM", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Die Polsche Barbarey hat mich ganz fremden Gast", "tokens": ["Die", "Pol\u00b7sche", "Bar\u00b7ba\u00b7rey", "hat", "mich", "ganz", "frem\u00b7den", "Gast"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit eu\u00dferlicher Gunst ein Vierthel Jahr umfast", "tokens": ["Mit", "eu\u00b7\u00dfer\u00b7li\u00b7cher", "Gunst", "ein", "Vier\u00b7thel", "Jahr", "um\u00b7fast"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und will mich durch das Joch verliebter Po\u00dfen zwingen,", "tokens": ["Und", "will", "mich", "durch", "das", "Joch", "ver\u00b7lieb\u00b7ter", "Po\u00b7\u00dfen", "zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "APPR", "ART", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den unruhvollen Stand bald an das Ziel zu bringen.", "tokens": ["Den", "un\u00b7ruh\u00b7vol\u00b7len", "Stand", "bald", "an", "das", "Ziel", "zu", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Gott weis, wie lang es w\u00e4hrt. L\u00f6scht meiner Eltern Haus,", "tokens": ["Gott", "weis", ",", "wie", "lang", "es", "w\u00e4hrt", ".", "L\u00f6scht", "mei\u00b7ner", "El\u00b7tern", "Haus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PWAV", "ADJD", "PPER", "VVFIN", "$.", "NN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wohin ich jezo geh, des Feuers Ursprung aus,", "tokens": ["Wo\u00b7hin", "ich", "je\u00b7zo", "geh", ",", "des", "Feu\u00b7ers", "Ur\u00b7sprung", "aus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "$,", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So hof ich hier noch Trost, wo nicht, so soll mein Leben", "tokens": ["So", "hof", "ich", "hier", "noch", "Trost", ",", "wo", "nicht", ",", "so", "soll", "mein", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "NN", "$,", "PWAV", "PTKNEG", "$,", "ADV", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In weit entfernter Luft nach be\u00dfrem Gl\u00fccke streben.", "tokens": ["In", "weit", "ent\u00b7fern\u00b7ter", "Luft", "nach", "be\u00df\u00b7rem", "Gl\u00fc\u00b7cke", "stre\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Von dir, gelehrte Frau, verlang ich weiter nichts", "tokens": ["Von", "dir", ",", "ge\u00b7lehr\u00b7te", "Frau", ",", "ver\u00b7lang", "ich", "wei\u00b7ter", "nichts"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "ADJA", "NN", "$,", "VVFIN", "PPER", "ADV", "PIS"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als nur noch diesen Strahl des holden Gnadenlichts,", "tokens": ["Als", "nur", "noch", "die\u00b7sen", "Strahl", "des", "hol\u00b7den", "Gna\u00b7den\u00b7lichts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "PDAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Fehler junger Zeit mit Gro\u00dfmuth zu ertragen", "tokens": ["Die", "Feh\u00b7ler", "jun\u00b7ger", "Zeit", "mit", "Gro\u00df\u00b7muth", "zu", "er\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "APPR", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und meiner Musenschaar bisweilen Trost zu sagen.", "tokens": ["Und", "mei\u00b7ner", "Mu\u00b7sen\u00b7schaar", "bis\u00b7wei\u00b7len", "Trost", "zu", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Dein Feuer und dein Geist, galant- und muntres Weib,", "tokens": ["Dein", "Feu\u00b7er", "und", "dein", "Geist", ",", "ga\u00b7lant", "und", "mun\u00b7tres", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,", "TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sind Kennern rechter Kunst ein s\u00fc\u00dfer Zeitvertreib", "tokens": ["Sind", "Ken\u00b7nern", "rech\u00b7ter", "Kunst", "ein", "s\u00fc\u00b7\u00dfer", "Zeit\u00b7ver\u00b7treib"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und haben neulich erst ein Meisterst\u00fcck erwiesen,", "tokens": ["Und", "ha\u00b7ben", "neu\u00b7lich", "erst", "ein", "Meis\u00b7ter\u00b7st\u00fcck", "er\u00b7wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das zwar den F\u00fcrsten ehrt, doch dich selbst mehr gepriesen.", "tokens": ["Das", "zwar", "den", "F\u00fcrs\u00b7ten", "ehrt", ",", "doch", "dich", "selbst", "mehr", "ge\u00b7prie\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "VVFIN", "$,", "KON", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "So fahre munter fort und las der Misgunst Schreyn", "tokens": ["So", "fah\u00b7re", "mun\u00b7ter", "fort", "und", "las", "der", "Mis\u00b7gunst", "Schreyn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "PTKVZ", "KON", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein nettes Lautenspiel nur immerhin entweihn;", "tokens": ["Dein", "net\u00b7tes", "Lau\u00b7ten\u00b7spiel", "nur", "im\u00b7mer\u00b7hin", "ent\u00b7weihn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Nachruhm ist ein Schaz, den hohe Seelen finden", "tokens": ["Der", "Nach\u00b7ruhm", "ist", "ein", "Schaz", ",", "den", "ho\u00b7he", "See\u00b7len", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und kluge Dichter stets um ihren Lorbeer winden.", "tokens": ["Und", "klu\u00b7ge", "Dich\u00b7ter", "stets", "um", "ih\u00b7ren", "Lor\u00b7beer", "win\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Ich, den zum Theil der Neid, zum Theil auch Schwachheitsschuld", "tokens": ["Ich", ",", "den", "zum", "Theil", "der", "Neid", ",", "zum", "Theil", "auch", "Schwach\u00b7heits\u00b7schuld"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "APPRART", "NN", "ART", "NN", "$,", "APPRART", "NN", "ADV", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ins Labyrinth gebracht, geh jezo mit Gedult,", "tokens": ["Ins", "La\u00b7by\u00b7rinth", "ge\u00b7bracht", ",", "geh", "je\u00b7zo", "mit", "Ge\u00b7dult", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wohin die Schickung winckt, und will mit Sehn und Schweigen", "tokens": ["Wo\u00b7hin", "die", "Schi\u00b7ckung", "winckt", ",", "und", "will", "mit", "Sehn", "und", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,", "KON", "VMFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem Gl\u00fccke, das mich \u00e4ft, ein starck Gem\u00fcthe zeigen.", "tokens": ["Dem", "Gl\u00fc\u00b7cke", ",", "das", "mich", "\u00e4ft", ",", "ein", "starck", "Ge\u00b7m\u00fc\u00b7the", "zei\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Ich sehe so vorl\u00e4ngst, was man nicht \u00e4ndern kan,", "tokens": ["Ich", "se\u00b7he", "so", "vor\u00b7l\u00e4ngst", ",", "was", "ma