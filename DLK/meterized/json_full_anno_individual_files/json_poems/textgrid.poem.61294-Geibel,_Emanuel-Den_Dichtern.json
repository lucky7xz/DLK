{"textgrid.poem.61294": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Den Dichtern", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr S\u00e4nger, denen auf die Brauen", "tokens": ["Ihr", "S\u00e4n\u00b7ger", ",", "de\u00b7nen", "auf", "die", "Brau\u00b7en"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Einst s\u00fc\u00dfer Tau des Himmels fiel,", "tokens": ["Einst", "s\u00fc\u00b7\u00dfer", "Tau", "des", "Him\u00b7mels", "fiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ihr im dunkeln Heut zu schauen", "tokens": ["Da\u00df", "ihr", "im", "dun\u00b7keln", "Heut", "zu", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verm\u00f6gt der Zukunft Farbenspiel,", "tokens": ["Ver\u00b7m\u00f6gt", "der", "Zu\u00b7kunft", "Far\u00b7ben\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auf, jetzt gedenkt, wie euch gegeben", "tokens": ["Auf", ",", "jetzt", "ge\u00b7denkt", ",", "wie", "euch", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "$,", "ADV", "VVPP", "$,", "PWAV", "PPER", "VVPP"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.6": {"text": "Ein Heilsamt aller S\u00fchnung voll,", "tokens": ["Ein", "Heil\u00b7samt", "al\u00b7ler", "S\u00fch\u00b7nung", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und la\u00dft das Lied erhabner schweben", "tokens": ["Und", "la\u00dft", "das", "Lied", "er\u00b7hab\u00b7ner", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "ART", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Als dieser Tage Lieb' und Groll!", "tokens": ["Als", "die\u00b7ser", "Ta\u00b7ge", "Lieb'", "und", "Groll", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Zum w\u00fcsten Kampf nicht, der die Stufen", "tokens": ["Zum", "w\u00fcs\u00b7ten", "Kampf", "nicht", ",", "der", "die", "Stu\u00b7fen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "PTKNEG", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Noch blind umtobt mit Schwert und Brand,", "tokens": ["Noch", "blind", "um\u00b7tobt", "mit", "Schwert", "und", "Brand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zur Tempelwacht seid ihr berufen,", "tokens": ["Zur", "Tem\u00b7pel\u00b7wacht", "seid", "ihr", "be\u00b7ru\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und auf den H\u00f6hn ist euer Stand.", "tokens": ["Und", "auf", "den", "H\u00f6hn", "ist", "eu\u00b7er", "Stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn alle schwanken, trutzen, zagen", "tokens": ["Wenn", "al\u00b7le", "schwan\u00b7ken", ",", "trut\u00b7zen", ",", "za\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KOUS", "PIS", "VVINF", "$,", "VVFIN", "$,", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Beim j\u00e4hen Wetterschlag der Zeit,", "tokens": ["Beim", "j\u00e4\u00b7hen", "Wet\u00b7ter\u00b7schlag", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sollt ihr in freier Seele tragen", "tokens": ["Sollt", "ihr", "in", "frei\u00b7er", "See\u00b7le", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das Ma\u00df und die Gerechtigkeit.", "tokens": ["Das", "Ma\u00df", "und", "die", "Ge\u00b7rech\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die heil'gen Sch\u00e4tze sollt ihr h\u00fcten,", "tokens": ["Die", "heil'\u00b7gen", "Sch\u00e4t\u00b7ze", "sollt", "ihr", "h\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die fromm die V\u00e4ter angeh\u00e4uft,", "tokens": ["Die", "fromm", "die", "V\u00e4\u00b7ter", "an\u00b7ge\u00b7h\u00e4uft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Herzens keusche Wunderbl\u00fcten,", "tokens": ["Des", "Her\u00b7zens", "keu\u00b7sche", "Wun\u00b7der\u00b7bl\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Glauben, der von Frieden tr\u00e4uft.", "tokens": ["Den", "Glau\u00b7ben", ",", "der", "von", "Frie\u00b7den", "tr\u00e4uft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr sollt durch diese Zeit von Eisen", "tokens": ["Ihr", "sollt", "durch", "die\u00b7se", "Zeit", "von", "Ei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PDAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Forttragen im gediegnen Wort", "tokens": ["Fort\u00b7tra\u00b7gen", "im", "ge\u00b7dieg\u00b7nen", "Wort"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Als hochbegnadigte Templeisen", "tokens": ["Als", "hoch\u00b7be\u00b7gna\u00b7dig\u00b7te", "Tem\u00b7plei\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADJA", "NN"], "meter": "-+-+--++-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Der Sch\u00f6nheit Licht, des Geistes Hort.", "tokens": ["Der", "Sch\u00f6n\u00b7heit", "Licht", ",", "des", "Geis\u00b7tes", "Hort", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Nicht d\u00fcrft ihr euch vor Thronen beugen", "tokens": ["Nicht", "d\u00fcrft", "ihr", "euch", "vor", "Thro\u00b7nen", "beu\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VMFIN", "PPER", "PRF", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Noch knieen, wo der P\u00f6bel kniet;", "tokens": ["Noch", "kni\u00b7e\u00b7en", ",", "wo", "der", "P\u00f6\u00b7bel", "kniet", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Die ew'ge Wahrheit braucht der Zeugen,", "tokens": ["Die", "ew'\u00b7ge", "Wahr\u00b7heit", "braucht", "der", "Zeu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Opferfeuer sei das Lied,", "tokens": ["Und", "Op\u00b7fer\u00b7feu\u00b7er", "sei", "das", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df, wenn dereinst nach Sturm und Fluten", "tokens": ["Da\u00df", ",", "wenn", "de\u00b7reinst", "nach", "Sturm", "und", "Flu\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "KOUS", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Erscheint des Friedensbogens Tag,", "tokens": ["Er\u00b7scheint", "des", "Frie\u00b7dens\u00b7bo\u00b7gens", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das Volk an euern ", "tokens": ["Das", "Volk", "an", "eu\u00b7ern"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Der Freiheit Fackel z\u00fcnden mag.", "tokens": ["Der", "Frei\u00b7heit", "Fa\u00b7ckel", "z\u00fcn\u00b7den", "mag."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["ART", "NN", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Hinweg drum mit des Grimmes Falten,", "tokens": ["Hin\u00b7weg", "drum", "mit", "des", "Grim\u00b7mes", "Fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Schellenklang und Brunst und Lug!", "tokens": ["Mit", "Schel\u00b7len\u00b7klang", "und", "Brunst", "und", "Lug", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie mag der Arm die Wage halten,", "tokens": ["Wie", "mag", "der", "Arm", "die", "Wa\u00b7ge", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der mit dem Schwert den Bruder schlug?", "tokens": ["Der", "mit", "dem", "Schwert", "den", "Bru\u00b7der", "schlug", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie mag den Kelch des Segens spenden,", "tokens": ["Wie", "mag", "den", "Kelch", "des", "Se\u00b7gens", "spen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wer selbst am Mahl der S\u00fcnde zecht?", "tokens": ["Wer", "selbst", "am", "Mahl", "der", "S\u00fcn\u00b7de", "zecht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Rein sollt ihr sein an Herz und H\u00e4nden,", "tokens": ["Rein", "sollt", "ihr", "sein", "an", "Herz", "und", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPOSAT", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ihr seid ein priesterlich Geschlecht.", "tokens": ["Ihr", "seid", "ein", "pries\u00b7ter\u00b7lich", "Ge\u00b7schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und will euch schier die Kraft versiegen,", "tokens": ["Und", "will", "euch", "schier", "die", "Kraft", "ver\u00b7sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und schwankt euch in der Brust das Herz:", "tokens": ["Und", "schwankt", "euch", "in", "der", "Brust", "das", "Herz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gebete, die zum Himmel fliegen,", "tokens": ["Ge\u00b7be\u00b7te", ",", "die", "zum", "Him\u00b7mel", "flie\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ziehn Feuerzungen niederw\u00e4rts;", "tokens": ["Ziehn", "Feu\u00b7er\u00b7zun\u00b7gen", "nie\u00b7der\u00b7w\u00e4rts", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "PTKVZ", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.5": {"text": "Und aus der Sch\u00f6pfung heil'gem Leben,", "tokens": ["Und", "aus", "der", "Sch\u00f6p\u00b7fung", "heil'\u00b7gem", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Aus ihrer ewig heitern Ruh'", "tokens": ["Aus", "ih\u00b7rer", "e\u00b7wig", "hei\u00b7tern", "Ruh'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Str\u00f6mt mit geheimnisvollem Weben", "tokens": ["Str\u00f6mt", "mit", "ge\u00b7heim\u00b7nis\u00b7vol\u00b7lem", "We\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Verj\u00fcngung euch und Klarheit zu.", "tokens": ["Ver\u00b7j\u00fcn\u00b7gung", "euch", "und", "Klar\u00b7heit", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Geht hin zum Meer in Abendgluten,", "tokens": ["Geht", "hin", "zum", "Meer", "in", "A\u00b7bend\u00b7glu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Geht hin zum Wald und r\u00fcstet euch!", "tokens": ["Geht", "hin", "zum", "Wald", "und", "r\u00fcs\u00b7tet", "euch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Geist schwebt heut noch auf den Fluten,", "tokens": ["Der", "Geist", "schwebt", "heut", "noch", "auf", "den", "Flu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch heute flammt's im Dorngestr\u00e4uch;", "tokens": ["Noch", "heu\u00b7te", "flammt's", "im", "Dorn\u00b7ge\u00b7str\u00e4uch", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da wird in ahnungsvollem Segen", "tokens": ["Da", "wird", "in", "ah\u00b7nungs\u00b7vol\u00b7lem", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Herr euch nah sein, nah und hold,", "tokens": ["Der", "Herr", "euch", "nah", "sein", ",", "nah", "und", "hold", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADJD", "VAINF", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und wird euch auf die Lippen legen,", "tokens": ["Und", "wird", "eu