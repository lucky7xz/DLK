 "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "NE", "$.", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "So dir die Majest\u00e4t, der drey Personen zeigt,", "tokens": ["So", "dir", "die", "Ma\u00b7jes\u00b7t\u00e4t", ",", "der", "drey", "Per\u00b7so\u00b7nen", "zeigt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "$,", "PRELS", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Durch sie wird Sattans Trutz, der H\u00f6llen Macht gebeugt.", "tokens": ["Durch", "sie", "wird", "Sat\u00b7tans", "Trutz", ",", "der", "H\u00f6l\u00b7len", "Macht", "ge\u00b7beugt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "NE", "NN", "$,", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Du machst jetzt mit dem Herrn im Himmel einen Bund,", "tokens": ["Du", "machst", "jetzt", "mit", "dem", "Herrn", "im", "Him\u00b7mel", "ei\u00b7nen", "Bund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du schw\u00f6rst dem Teufel ab, und giebst dich Gott zu eigen:", "tokens": ["Du", "schw\u00f6rst", "dem", "Teu\u00b7fel", "ab", ",", "und", "giebst", "dich", "Gott", "zu", "ei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "NN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die\u00df Wort mu\u00df ich vor ihm und seiner Kirch bezeugen.", "tokens": ["Die\u00df", "Wort", "mu\u00df", "ich", "vor", "ihm", "und", "sei\u00b7ner", "Kirch", "be\u00b7zeu\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VMFIN", "PPER", "APPR", "PPER", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So denk mein Pathgen doch allzeit an diese Stund,", "tokens": ["So", "denk", "mein", "Path\u00b7gen", "doch", "all\u00b7zeit", "an", "die\u00b7se", "Stund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADV", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und halte deinem Gott/ was du durch mich gesagt,", "tokens": ["Und", "hal\u00b7te", "dei\u00b7nem", "Gott", "/", "was", "du", "durch", "mich", "ge\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$(", "PWS", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Damit dich nicht dereinst der Fluch des Meineids plagt.", "tokens": ["Da\u00b7mit", "dich", "nicht", "de\u00b7reinst", "der", "Fluch", "des", "Mein\u00b7eids", "plagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}