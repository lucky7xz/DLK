{"textgrid.poem.62573": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen Taufzettel, bey Gevatterschaft einer Gro\u00dfmutter", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hat Adams S\u00fcndenfall es gleich dahin gebracht,", "tokens": ["Hat", "A\u00b7dams", "S\u00fcn\u00b7den\u00b7fall", "es", "gleich", "da\u00b7hin", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "NN", "PPER", "ADV", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df wir nun allesamt auf diesem Rund der Erden,", "tokens": ["Da\u00df", "wir", "nun", "al\u00b7le\u00b7samt", "auf", "die\u00b7sem", "Rund", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "APPR", "PDAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In S\u00fcnden erst gezeugt, hernach gebohren werden,", "tokens": ["In", "S\u00fcn\u00b7den", "erst", "ge\u00b7zeugt", ",", "her\u00b7nach", "ge\u00b7boh\u00b7ren", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVPP", "$,", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie solches Davids-Psalm uns l\u00e4ngst bekannt gemacht;", "tokens": ["Wie", "sol\u00b7ches", "Da\u00b7vids\u00b7P\u00b7salm", "uns", "l\u00e4ngst", "be\u00b7kannt", "ge\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Doch unsers Jesus Tod w\u00e4scht uns von S\u00fcnden rein,", "tokens": ["Doch", "un\u00b7sers", "Je\u00b7sus", "Tod", "w\u00e4scht", "uns", "von", "S\u00fcn\u00b7den", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NE", "NN", "VVFIN", "PPER", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und l\u00e4\u00dft uns durch die Tauf davon gesaubert seyn.", "tokens": ["Und", "l\u00e4\u00dft", "uns", "durch", "die", "Tauf", "da\u00b7von", "ge\u00b7sau\u00b7bert", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "PAV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du aber zartes Kind! mein Enkel, ach! du bist", "tokens": ["Du", "a\u00b7ber", "zar\u00b7tes", "Kind", "!", "mein", "En\u00b7kel", ",", "ach", "!", "du", "bist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "ADJA", "NN", "$.", "PPOSAT", "NN", "$,", "ITJ", "$.", "PPER", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In S\u00fcnden ebenfals gezeuget und gebohren:", "tokens": ["In", "S\u00fcn\u00b7den", "e\u00b7ben\u00b7fals", "ge\u00b7zeu\u00b7get", "und", "ge\u00b7boh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und wo dich Gott nicht w\u00e4scht; so bleibest du verlohren.", "tokens": ["Und", "wo", "dich", "Gott", "nicht", "w\u00e4scht", ";", "so", "blei\u00b7best", "du", "ver\u00b7loh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "NN", "PTKNEG", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch nein, dein Seelen-Freund, dein Heyland Jesus Christ,", "tokens": ["Doch", "nein", ",", "dein", "See\u00b7len\u00b7Freund", ",", "dein", "Hey\u00b7land", "Je\u00b7sus", "Christ", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "NE", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Hat deine Seele lieb, drum ruft er dich zur Tauf,", "tokens": ["Hat", "dei\u00b7ne", "See\u00b7le", "lieb", ",", "drum", "ruft", "er", "dich", "zur", "Tauf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJD", "$,", "PAV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und nimt dich nun dadurch zum Kind der Gnaden auf.", "tokens": ["Und", "nimt", "dich", "nun", "da\u00b7durch", "zum", "Kind", "der", "Gna\u00b7den", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PAV", "APPRART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "O Freudenreicher Tag! o seelges Wasserbad!", "tokens": ["O", "Freu\u00b7den\u00b7rei\u00b7cher", "Tag", "!", "o", "seel\u00b7ges", "Was\u00b7ser\u00b7bad", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Gottes Zorn-Glut l\u00f6scht; er schreibet deinen Namen", "tokens": ["Das", "Got\u00b7tes", "Zorn\u00b7Glut", "l\u00f6scht", ";", "er", "schrei\u00b7bet", "dei\u00b7nen", "Na\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "$.", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ins Buch des Lebens ein, und bringt dich zu den Saamen", "tokens": ["Ins", "Buch", "des", "Le\u00b7bens", "ein", ",", "und", "bringt", "dich", "zu", "den", "Saa\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der heilgen Christenheit. O! merke doch die Gnad,", "tokens": ["Der", "heil\u00b7gen", "Chris\u00b7ten\u00b7heit", ".", "O", "!", "mer\u00b7ke", "doch", "die", "Gnad", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "NE", "$.", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "So dir die Majest\u00e4t, der drey Personen zeigt,", "tokens": ["So", "dir", "die", "Ma\u00b7jes\u00b7t\u00e4t", ",", "der", "drey", "Per\u00b7so\u00b7nen", "zeigt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "$,", "PRELS", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Durch sie wird Sattans Trutz, der H\u00f6llen Macht gebeugt.", "tokens": ["Durch", "sie", "wird", "Sat\u00b7tans", "Trutz", ",", "der", "H\u00f6l\u00b7len", "Macht", "ge\u00b7beugt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "NE", "NN", "$,", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Du machst jetzt mit dem Herrn im Himmel einen Bund,", "tokens": ["Du", "machst", "jetzt", "mit", "dem", "Herrn", "im", "Him\u00b7mel", "ei\u00b7nen", "Bund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du schw\u00f6rst dem Teufel ab, und giebst dich Gott zu eigen:", "tokens": ["Du", "schw\u00f6rst", "dem", "Teu\u00b7fel", "ab", ",", "und", "giebst", "dich", "Gott", "zu", "ei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "NN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die\u00df Wort mu\u00df ich vor ihm und seiner Kirch bezeugen.", "tokens": ["Die\u00df", "Wort", "mu\u00df", "ich", "vor", "ihm", "und", "sei\u00b7ner", "Kirch", "be\u00b7zeu\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VMFIN", "PPER", "APPR", "PPER", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So denk mein Pathgen doch allzeit an diese Stund,", "tokens": ["So", "denk", "mein", "Path\u00b7gen", "doch", "all\u00b7zeit", "an", "die\u00b7se", "Stund", ","], "token_info": ["word", "word", "word", "word",