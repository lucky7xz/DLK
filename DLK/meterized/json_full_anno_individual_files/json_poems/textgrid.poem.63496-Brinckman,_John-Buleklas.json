{"textgrid.poem.63496": {"metadata": {"author": {"name": "Brinckman, John", "birth": "N.A.", "death": "N.A."}, "title": "Buleklas", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.71", "af:0.28"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbl\u00fctt Riking, l\u00fctt Riking, wat l\u00f6ppst du so dull?", "tokens": ["\u00bb", "l\u00fctt", "Ri\u00b7king", ",", "l\u00fctt", "Ri\u00b7king", ",", "wat", "l\u00f6ppst", "du", "so", "dull", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "NN", "$,", "VVFIN", "NE", "$,", "PWS", "VVFIN", "PPER", "ADV", "VMFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wat is di, wat hest du, l\u00fctt Dirn?", "tokens": ["Wat", "is", "di", ",", "wat", "hest", "du", ",", "l\u00fctt", "Dirn", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,", "PWS", "VVFIN", "PPER", "$,", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dat is jo nich d\u00fcster, de Mand is jo vull,", "tokens": ["Dat", "is", "jo", "nich", "d\u00fcs\u00b7ter", ",", "de", "Mand", "is", "jo", "vull", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "un ball dor l\u00fcchten de Stirn.", "tokens": ["un", "ball", "dor", "l\u00fcch\u00b7ten", "de", "Stirn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Dat Brok d\u00f6rch, dat Mur lank, v\u00f6rbi dor de oll Wir,", "tokens": ["Dat", "Brok", "d\u00f6rch", ",", "dat", "Mur", "lank", ",", "v\u00f6r\u00b7bi", "dor", "de", "oll", "Wir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,", "ART", "NN", "ADJD", "$,", "FM", "FM", "FM", "FM", "PPER", "$,"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "de B\u00e4k noch, denn b\u00fcst du to Hus.", "tokens": ["de", "B\u00e4k", "noch", ",", "denn", "b\u00fcst", "du", "to", "Hus", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "$,", "KON", "VVFIN", "PPER", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du l\u00f6ppst jo, as l\u00f6p dor wen achter di her,", "tokens": ["Du", "l\u00f6ppst", "jo", ",", "as", "l\u00f6p", "dor", "wen", "ach\u00b7ter", "di", "her", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "PTKVZ", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "as v\u00f6r den Kater de Mus!\u00ab", "tokens": ["as", "v\u00f6r", "den", "Ka\u00b7ter", "de", "Mus", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NE", "ART", "NN", "NE", "NE", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "L\u00fctt Riking Spr\u00fcng den \u00d6wer hendal,", "tokens": ["L\u00fctt", "Ri\u00b7king", "Spr\u00fcng", "den", "\u00d6\u00b7wer", "hen\u00b7dal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "ART", "NN", "NE", "$,"], "meter": "-+-+-+--+", "measure": "iambic.te