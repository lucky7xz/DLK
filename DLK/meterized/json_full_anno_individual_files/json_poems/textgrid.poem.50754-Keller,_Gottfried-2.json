{"textgrid.poem.50754": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "2", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durchs Fr\u00fchrot zog das Wolkenschiff", "tokens": ["Durchs", "Fr\u00fch\u00b7rot", "zog", "das", "Wol\u00b7ken\u00b7schiff"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "vor einem hellen Fr\u00fchlingstag,", "tokens": ["vor", "ei\u00b7nem", "hel\u00b7len", "Fr\u00fch\u00b7lings\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als ich, ein tr\u00e4umend Sch\u00fclerkind,", "tokens": ["Als", "ich", ",", "ein", "tr\u00e4u\u00b7mend", "Sch\u00fc\u00b7ler\u00b7kind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "im morgenstillen Felde lag;", "tokens": ["im", "mor\u00b7gen\u00b7stil\u00b7len", "Fel\u00b7de", "lag", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Falter streifte meine Stirn,", "tokens": ["Ein", "Fal\u00b7ter", "streif\u00b7te", "mei\u00b7ne", "Stirn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und vor mir eine Lilie stand;", "tokens": ["und", "vor", "mir", "ei\u00b7ne", "Li\u00b7lie", "stand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich aber schaute dr\u00fcber hin", "tokens": ["Ich", "a\u00b7ber", "schau\u00b7te", "dr\u00fc\u00b7ber", "hin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PAV", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "ins tiefe blaue Morgenland.", "tokens": ["ins", "tie\u00b7fe", "blau\u00b7e", "Mor\u00b7gen\u00b7land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das ganze Erdreich schwoll empor", "tokens": ["Das", "gan\u00b7ze", "Er\u00b7dreich", "schwoll", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "in tausendfacher Bl\u00fctenlust;", "tokens": ["in", "tau\u00b7send\u00b7fa\u00b7cher", "Bl\u00fc\u00b7ten\u00b7lust", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch m\u00e4chtiger schwoll Traum an Traum", "tokens": ["Doch", "m\u00e4ch\u00b7ti\u00b7ger", "schwoll", "Traum", "an", "Traum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "ADJD", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und Bild an Bild aus meiner Brust:", "tokens": ["und", "Bild", "an", "Bild", "aus", "mei\u00b7ner", "Brust", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das war die duftige Kinderwelt,", "tokens": ["Das", "war", "die", "duf\u00b7ti\u00b7ge", "Kin\u00b7der\u00b7welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "an deren Scheide ich mich fand,", "tokens": ["an", "de\u00b7ren", "Schei\u00b7de", "ich", "mich", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PPER", "PRF", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Die wie die erste Bl\u00fcte sich,", "tokens": ["Die", "wie", "die", "ers\u00b7te", "Bl\u00fc\u00b7te", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "ART", "ADJA", "NN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "am Lebensbaume, mir entwand!", "tokens": ["am", "Le\u00b7bens\u00b7bau\u00b7me", ",", "mir", "ent\u00b7wand", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie baute sich noch ", "tokens": ["Sie", "bau\u00b7te", "sich", "noch"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "mit letztem Glanz, im letzten Flor;", "tokens": ["mit", "letz\u00b7tem", "Glanz", ",", "im", "letz\u00b7ten", "Flor", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein lieblich wunderlicher Bau,", "tokens": ["Ein", "lieb\u00b7lich", "wun\u00b7der\u00b7li\u00b7cher", "Bau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ein Feentempel stieg empor", "tokens": ["ein", "Fe\u00b7en\u00b7tem\u00b7pel", "stieg", "em\u00b7por"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von hundert S\u00e4ulchen, zart wie Glas,", "tokens": ["Von", "hun\u00b7dert", "S\u00e4ul\u00b7chen", ",", "zart", "wie", "Glas", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$,", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Alt\u00e4rlein, Nischen \u2013 Bildchen drin,", "tokens": ["Al\u00b7t\u00e4r\u00b7lein", ",", "Ni\u00b7schen", "\u2013", "Bild\u00b7chen", "drin", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$(", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bepriestert war das Wunderhaus", "tokens": ["Be\u00b7pries\u00b7tert", "war", "das", "Wun\u00b7der\u00b7haus"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "nach mystisch heil'gem Kindersinn.", "tokens": ["nach", "mys\u00b7tisch", "heil'\u00b7gem", "Kin\u00b7der\u00b7sinn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und mitten in dem Tempel stand,", "tokens": ["Und", "mit\u00b7ten", "in", "dem", "Tem\u00b7pel", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "durchsichtig, ein kristallner Sarg,", "tokens": ["durch\u00b7sich\u00b7tig", ",", "ein", "kris\u00b7tall\u00b7ner", "Sarg", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der eine rosenrote Frau,", "tokens": ["Der", "ei\u00b7ne", "ro\u00b7sen\u00b7ro\u00b7te", "Frau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "auf Feuerlilien schlafend, barg.", "tokens": ["auf", "Feu\u00b7er\u00b7li\u00b7li\u00b7en", "schla\u00b7fend", ",", "barg", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Vier Riesen lagen um den Schrein", "tokens": ["Vier", "Rie\u00b7sen", "la\u00b7gen", "um", "den", "Schrein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "mit schlummernden Falken auf der Faust;", "tokens": ["mit", "schlum\u00b7mern\u00b7den", "Fal\u00b7ken", "auf", "der", "Faust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Sie nickten oft im Morgenwind,", "tokens": ["Sie", "nick\u00b7ten", "oft", "im", "Mor\u00b7gen\u00b7wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "der ihnen um die Schl\u00e4fe braust'.", "tokens": ["der", "ih\u00b7nen", "um", "die", "Schl\u00e4\u00b7fe", "braust'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Da ging die Sonne flammend auf", "tokens": ["Da", "ging", "die", "Son\u00b7ne", "flam\u00b7mend", "auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVPP", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und schmolz den Tempel auf den Grund,", "tokens": ["und", "schmolz", "den", "Tem\u00b7pel", "auf", "den", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur in der wehenden Asche noch", "tokens": ["Nur", "in", "der", "we\u00b7hen\u00b7den", "A\u00b7sche", "noch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "der Schrein mit seinen H\u00fctern stund;", "tokens": ["der", "Schrein", "mit", "sei\u00b7nen", "H\u00fc\u00b7tern", "stund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Worauf der w\u00e4rmste Sonnenstrahl", "tokens": ["Wo\u00b7rauf", "der", "w\u00e4rms\u00b7te", "Son\u00b7nen\u00b7strahl"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "den Deckel von Kristall erschlo\u00df,", "tokens": ["den", "De\u00b7ckel", "von", "Kris\u00b7tall", "er\u00b7schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "So da\u00df der rosigen Schl\u00e4ferin", "tokens": ["So", "da\u00df", "der", "ro\u00b7si\u00b7gen", "Schl\u00e4\u00b7fe\u00b7rin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "der Tag sich in die Augen go\u00df.", "tokens": ["der", "Tag", "sich", "in", "die", "Au\u00b7gen", "go\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und auch die Riesen wachten auf", "tokens": ["Und", "auch", "die", "Rie\u00b7sen", "wach\u00b7ten", "auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "die sandten ihre Falkenzucht", "tokens": ["die", "sand\u00b7ten", "ih\u00b7re", "Fal\u00b7ken\u00b7zucht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus in den goldenen Morgenschein", "tokens": ["Aus", "in", "den", "gol\u00b7de\u00b7nen", "Mor\u00b7gen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "nach aller Winde fr\u00f6hlicher Flucht.", "tokens": ["nach", "al\u00b7ler", "Win\u00b7de", "fr\u00f6h\u00b7li\u00b7cher", "Flucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Sie stiegen auf ins \u00c4therblau", "tokens": ["Sie", "stie\u00b7gen", "auf", "ins", "\u00c4\u00b7ther\u00b7blau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und brachten in ", "tokens": ["und", "brach\u00b7ten", "in"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "APPR"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Der Dame im kristallnen Sarg", "tokens": ["Der", "Da\u00b7me", "im", "kris\u00b7tall\u00b7nen", "Sarg"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "eine scheue wei\u00dfe Taube zur\u00fcck.", "tokens": ["ei\u00b7ne", "scheu\u00b7e", "wei\u00b7\u00dfe", "Tau\u00b7be", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.7": {"line.1": {"text": "Halb Kind, halb J\u00fcngling, tr\u00e4umend noch,", "tokens": ["Halb", "Kind", ",", "halb", "J\u00fcng\u00b7ling", ",", "tr\u00e4u\u00b7mend", "noch", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "ADJD", "NN", "$,", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "fand ich die Liebe im Morgentau;", "tokens": ["fand", "ich", "die", "Lie\u00b7be", "im", "Mor\u00b7gen\u00b7tau", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Ich trug sie singend in der Brust,", "tokens": ["Ich", "trug", "sie", "sin\u00b7gend", "in", "der", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "heimkehrend von der funkelnden Au.", "tokens": ["heim\u00b7keh\u00b7rend", "von", "der", "fun\u00b7keln\u00b7den", "Au", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Ein neuer Mensch, trat ich ins Haus", "tokens": ["Ein", "neu\u00b7er", "Mensch", ",", "trat", "ich", "ins", "Haus"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und fand das lockige M\u00e4dchen da,", "tokens": ["und", "fand", "das", "lo\u00b7cki\u00b7ge", "M\u00e4d\u00b7chen", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Das sch\u00fcchtern mir und ungewohnt,", "tokens": ["Das", "sch\u00fcch\u00b7tern", "mir", "und", "un\u00b7ge\u00b7wohnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "wegfliehend in die Augen sah.", "tokens": ["weg\u00b7flie\u00b7hend", "in", "die", "Au\u00b7gen", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "O s\u00fc\u00dfe Stunde, die das Herz", "tokens": ["O", "s\u00fc\u00b7\u00dfe", "Stun\u00b7de", ",", "die", "das", "Herz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "vom Herzen voller Sehnsucht