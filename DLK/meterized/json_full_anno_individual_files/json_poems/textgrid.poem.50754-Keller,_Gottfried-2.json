.tetra"}, "line.6": {"text": "und brachten in ", "tokens": ["und", "brach\u00b7ten", "in"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "APPR"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Der Dame im kristallnen Sarg", "tokens": ["Der", "Da\u00b7me", "im", "kris\u00b7tall\u00b7nen", "Sarg"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "eine scheue wei\u00dfe Taube zur\u00fcck.", "tokens": ["ei\u00b7ne", "scheu\u00b7e", "wei\u00b7\u00dfe", "Tau\u00b7be", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.15": {"line.1": {"text": "Halb Kind, halb J\u00fcngling, tr\u00e4umend noch,", "tokens": ["Halb", "Kind", ",", "halb", "J\u00fcng\u00b7ling", ",", "tr\u00e4u\u00b7mend", "noch", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "ADJD", "NN", "$,", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "fand ich die Liebe im Morgentau;", "tokens": ["fand", "ich", "die", "Lie\u00b7be", "im", "Mor\u00b7gen\u00b7tau", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Ich trug sie singend in der Brust,", "tokens": ["Ich", "trug", "sie", "sin\u00b7gend", "in", "der", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "heimkehrend von der funkelnden Au.", "tokens": ["heim\u00b7keh\u00b7rend", "von", "der", "fun\u00b7keln\u00b7den", "Au", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Ein neuer Mensch, trat ich ins Haus", "tokens": ["Ein", "neu\u00b7er", "Mensch", ",", "trat", "ich", "ins", "Haus"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und fand das lockige M\u00e4dchen da,", "tokens": ["und", "fand", "das", "lo\u00b7cki\u00b7ge", "M\u00e4d\u00b7chen", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Das sch\u00fcchtern mir und ungewohnt,", "tokens": ["Das", "sch\u00fcch\u00b7tern", "mir", "und", "un\u00b7ge\u00b7wohnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "wegfliehend in die Augen sah.", "tokens": ["weg\u00b7flie\u00b7hend", "in", "die", "Au\u00b7gen", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "O s\u00fc\u00dfe Stunde, die das Herz", "tokens": ["O", "s\u00fc\u00b7\u00dfe", "Stun\u00b7de", ",", "die", "das", "Herz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "vom Herzen voller Sehnsucht rei\u00dft!", "tokens": ["vom", "Her\u00b7zen", "vol\u00b7ler", "Sehn\u00b7sucht", "rei\u00dft", "!"], "