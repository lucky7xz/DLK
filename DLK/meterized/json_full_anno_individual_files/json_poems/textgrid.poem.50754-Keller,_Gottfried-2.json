word", "punct"], "pos": ["ADJD", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der eine rosenrote Frau,", "tokens": ["Der", "ei\u00b7ne", "ro\u00b7sen\u00b7ro\u00b7te", "Frau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "auf Feuerlilien schlafend, barg.", "tokens": ["auf", "Feu\u00b7er\u00b7li\u00b7li\u00b7en", "schla\u00b7fend", ",", "barg", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Vier Riesen lagen um den Schrein", "tokens": ["Vier", "Rie\u00b7sen", "la\u00b7gen", "um", "den", "Schrein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "mit schlummernden Falken auf der Faust;", "tokens": ["mit", "schlum\u00b7mern\u00b7den", "Fal\u00b7ken", "auf", "der", "Faust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Sie nickten oft im Morgenwind,", "tokens": ["Sie", "nick\u00b7ten", "oft", "im", "Mor\u00b7gen\u00b7wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "der ihnen um die Schl\u00e4fe braust'.", "tokens": ["der", "ih\u00b7nen", "um", "die", "Schl\u00e4\u00b7fe", "braust'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {