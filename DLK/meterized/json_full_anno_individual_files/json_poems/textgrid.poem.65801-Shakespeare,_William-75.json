{"textgrid.poem.65801": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "75", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was Brot dem Leben, was lauwarmer Regen", "tokens": ["Was", "Brot", "dem", "Le\u00b7ben", ",", "was", "lau\u00b7war\u00b7mer", "Re\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "NN", "ART", "NN", "$,", "PWS", "ADJA", "NN"], "meter": "-+-+-+++-+-", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Dem Erdreich ist, das bist du meinem Geist:", "tokens": ["Dem", "Er\u00b7dreich", "ist", ",", "das", "bist", "du", "mei\u00b7nem", "Geist", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PDS", "VAFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und solches K\u00e4mpfen f\u00fchr' ich deines Friedens wegen,", "tokens": ["Und", "sol\u00b7ches", "K\u00e4mp\u00b7fen", "f\u00fchr'", "ich", "dei\u00b7nes", "Frie\u00b7dens", "we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie zwischen Geizigen und ihrem Gut sich weist.", "tokens": ["Wie", "zwi\u00b7schen", "Gei\u00b7zi\u00b7gen", "und", "ih\u00b7rem", "Gut", "sich", "weist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "KON", "PPOSAT", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bald jubl' ich im Genu\u00df, bald mu\u00df ich sorgen,", "tokens": ["Bald", "jubl'", "ich", "im", "Ge\u00b7nu\u00df", ",", "bald", "mu\u00df", "ich", "sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$,", "ADV", "VMFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ob nicht ein Dieb mein Kleinod mir entr\u00fcckt:", "tokens": ["Ob", "nicht", "ein", "Dieb", "mein", "Klei\u00b7nod", "mir", "ent\u00b7r\u00fcckt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Bald w\u00e4r ich gern allein mit dir geborgen,", "tokens": ["Bald", "w\u00e4r", "ich", "gern", "al\u00b7lein", "mit", "dir", "ge\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Bald wollt' ich, jeder s\u00e4h was mich entz\u00fcckt.", "tokens": ["Bald", "wollt'", "ich", ",", "je\u00b7der", "s\u00e4h", "was", "mich", "ent\u00b7z\u00fcckt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "PIS", "VVFIN", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Von deinem Augenschmaus bisweilen vollgef\u00fcllt;", "tokens": ["Von", "dei\u00b7nem", "Au\u00b7gen\u00b7schmaus", "bis\u00b7wei\u00b7len", "voll\u00b7ge\u00b7f\u00fcllt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Um einen einz'gen Blick dann wieder wi