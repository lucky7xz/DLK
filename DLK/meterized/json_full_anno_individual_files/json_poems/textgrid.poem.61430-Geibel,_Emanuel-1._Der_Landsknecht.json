{"textgrid.poem.61430": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1. Der Landsknecht", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Landsknecht bin ich worden", "tokens": ["Ein", "Lands\u00b7knecht", "bin", "ich", "wor\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "VAPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In des Feldhauptmanns Heer,", "tokens": ["In", "des", "Feld\u00b7haupt\u00b7manns", "Heer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Dem frommen Landsknechtsorden,", "tokens": ["Dem", "from\u00b7men", "Lands\u00b7knech\u00b7tsor\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dem sing' ich Preis und Ehr'.", "tokens": ["Dem", "sing'", "ich", "Preis", "und", "Ehr'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wer f\u00e4hrt so gut mit frischem Mut", "tokens": ["Wer", "f\u00e4hrt", "so", "gut", "mit", "fri\u00b7schem", "Mut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In diesen b\u00f6sen Zeiten,", "tokens": ["In", "die\u00b7sen", "b\u00f6\u00b7sen", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Als wie der Kriegsmann tut!", "tokens": ["Als", "wie", "der", "Kriegs\u00b7mann", "tut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Fahne soll mich f\u00fchren,", "tokens": ["Die", "Fah\u00b7ne", "soll", "mich", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Fahne, meine Braut.", "tokens": ["Die", "Fah\u00b7ne", ",", "mei\u00b7ne", "Braut", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn sich die Trommeln r\u00fchren,", "tokens": ["Wenn", "sich", "die", "Trom\u00b7meln", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie ruft sie da so laut!", "tokens": ["Wie", "ruft", "sie", "da", "so", "laut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Kein' be\u00dfre Lust, als fest im Sturm", "tokens": ["Kein'", "be\u00df\u00b7re", "Lust", ",", "als", "fest", "im", "Sturm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "$,", "KOUS", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "F\u00fcr sie den Feind erschlagen", "tokens": ["F\u00fcr", "sie", "den", "Feind", "er\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und stehen als ein Turm.", "tokens": ["Und", "ste\u00b7hen", "als", "ein", "Turm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich hab' nicht viel zu sparen", "tokens": ["Ich", "hab'", "nicht", "viel", "zu", "spa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als wie ein reicher Gauch;", "tokens": ["Als", "wie", "ein", "rei\u00b7cher", "Gauch", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wohin wir m\u00f6gen fahren,", "tokens": ["Wo\u00b7hin", "wir", "m\u00f6\u00b7gen", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da nehm' ich, was ich brauch'.", "tokens": ["Da", "nehm'", "ich", ",", "was", "ich", "brauch'", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "He, B\u00e4uerlein, B\u00e4uerlein, sch\u00fcrz' dich nun!", "tokens": ["He", ",", "B\u00e4u\u00b7er\u00b7lein", ",", "B\u00e4u\u00b7er\u00b7lein", ",", "sch\u00fcrz'", "dich", "nun", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$,", "NN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Den Krug tu aus dem Keller,", "tokens": ["Den", "Krug", "tu", "aus", "dem", "Kel\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Tu an den Spie\u00df das Huhn!", "tokens": ["Tu", "an", "den", "Spie\u00df", "das", "Huhn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Drei W\u00fcrfel und ein' Karten,", "tokens": ["Drei", "W\u00fcr\u00b7fel", "und", "ein'", "Kar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die sind in jedem Schank;", "tokens": ["Die", "sind", "in", "je\u00b7dem", "Schank", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es kommt, mir aufzuwarten,", "tokens": ["Es", "kommt", ",", "mir", "auf\u00b7zu\u00b7war\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Dirnlein schlank und blank.", "tokens": ["Ein", "Dirn\u00b7lein", "schlank", "und", "blank", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mein Feinslieb das hei\u00dft Braun und Blond,", "tokens": ["Mein", "Feins\u00b7lieb", "das", "hei\u00dft", "Braun", "und", "Blond", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PDS", "VVFIN", "NE", "KON", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Schneewei\u00df und Rot-wie-Rosen,", "tokens": ["Schnee\u00b7wei\u00df", "und", "Rot\u00b7wie\u00b7Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Ein andres jeden Mond.", "tokens": ["Ein", "and\u00b7res", "je\u00b7den", "Mond", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und rei\u00dfen mir die Kleider,", "tokens": ["Und", "rei\u00b7\u00dfen", "mir", "die", "Klei\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das schafft mir wenig Harm;", "tokens": ["Das", "schafft", "mir", "we\u00b7nig", "Harm", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mir macht der Wein, der Schneider,", "tokens": ["Mir", "macht", "der", "Wein", ",", "der", "Schnei\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Einen Rauschemantel warm;", "tokens": ["Ei\u00b7nen", "Rau\u00b7schem\u00b7an\u00b7tel", "warm", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der deckt mich zu vor aller Plag'", "tokens": ["Der", "deckt", "mich", "zu", "vor", "al\u00b7ler", "Plag'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "PTKZU", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Im Graben und auf der Schanzen", "tokens": ["Im", "Gra\u00b7ben", "und", "auf", "der", "Schan\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "APPR", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Bis an den jungen Tag.", "tokens": ["Bis", "an", "den", "jun\u00b7gen", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und kommt eine Kugel balde", "tokens": ["Und", "kommt", "ei\u00b7ne", "Ku\u00b7gel", "bal\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und nimmt mir fort ein Bein:", "tokens": ["Und", "nimmt", "mir", "fort", "ein", "Bein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es w\u00e4chst viel Holz im Walde,", "tokens": ["Es", "w\u00e4chst", "viel", "Holz", "im", "Wal\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich darf nicht traurig sein.", "tokens": ["Ich", "darf", "nicht", "trau\u00b7rig", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ei, was mich Str\u00fcmpf' und Schuh' gekost,", "tokens": ["Ei", ",", "was", "mich", "Str\u00fcmpf'", "und", "Schuh'", "ge\u00b7kost", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "PPER", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Nun mag ich's ba\u00df vertrinken;", "tokens": ["Nun", "mag", "ich's", "ba\u00df", "ver\u00b7trin\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Das ist ein tapfrer Trost.", "tokens": ["Das", "ist", "ein", "tapf\u00b7rer", "Trost", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und werd' ich gar erschlagen,", "tokens": ["Und", "werd'", "ich", "gar", "er\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Erschlagen auf breiter Heid':", "tokens": ["Er\u00b7schla\u00b7gen", "auf", "brei\u00b7ter", "Heid'", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Vier Spie\u00dfe m\u00fcssen mich tragen,", "tokens": ["Vier", "Spie\u00b7\u00dfe", "m\u00fcs\u00b7sen", "mich", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ein Grab steht gleich bereit.", "tokens": ["Ein", "Grab", "steht", "gleich", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So schl\u00e4gt man mir den Pummerlein pum,", "tokens": ["So", "schl\u00e4gt", "man", "mir", "den", "Pum\u00b7mer\u00b7lein", "pum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Der ist mir neunmal lieber", "tokens": ["Der", "ist", "mir", "neun\u00b7mal", "lie\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Als aller Pfaffen Gebrumm.", "tokens": ["Als", "al\u00b7ler", "Pfaf\u00b7fen", "Ge\u00b7brumm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.8": {"line.1": {"text": "Wer hat dies Lied gesungen", "tokens": ["Wer", "hat", "dies", "Lied", "ge\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PDS", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu Pfeif' und Trommelschlag?", "tokens": ["Zu", "Pfeif'", "und", "Trom\u00b7mel\u00b7schlag", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Einem Landsknecht ist's gelungen,", "tokens": ["Ei\u00b7nem", "Lands\u00b7knecht", "ist's", "ge\u00b7lun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da er zu Augsburg lag.", "tokens": ["Da", "er", "zu", "Augs\u00b7burg", "lag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Im gr\u00fcnen Baum da kehrt' er ein", "tokens": ["Im", "gr\u00fc\u00b7nen", "Baum", "da", "kehrt'", "er", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ADV", "VVFIN", "PPER", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und k\u00fc\u00dft' ein schwarzbraun M\u00e4del", "tokens": ["Und", "k\u00fc\u00dft'", "ein", "schwarz\u00b7braun", "M\u00e4\u00b7del"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und trank einen k\u00fchlen Wein.", "tokens": ["Und", "trank", "ei\u00b7nen", "k\u00fch\u00b7len", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Ein Landsknecht bin ich worden", "tokens": ["Ein", "Lands\u00b7knecht", "bin", "ich", "wor\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "VAPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In des Feldhauptmanns Heer,", "tokens": ["In", "des", "Feld\u00b7haupt\u00b7manns", "Heer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Dem frommen Landsknechtsorden,", "tokens": ["Dem", "from\u00b7men", "Lands\u00b7knech\u00b7tsor\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dem sing' ich Preis und Ehr'.", "tokens": ["Dem", "sing'", "ich", "Preis", "und", "Ehr'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wer f\u00e4hrt so gut mit frischem Mut", "tokens": ["Wer", "f\u00e4hrt", "so", "gut", "mit", "fri\u00b7schem", "Mut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In diesen b\u00f6sen Zeiten,", "tokens": ["In", "die\u00b7sen", "b\u00f6\u00b7sen", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Als wie der Kriegsmann tut!", "tokens": ["Als", "wie", "der", "Kriegs\u00b7mann", "tut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Die Fahne soll mich f\u00fchren,", "tokens": ["Die", "Fah\u00b7ne", "soll", "mich", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Fahne, meine Braut.", "tokens": ["Die", "Fah\u00b7ne", ",", "mei\u00b7ne", "Braut", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn sich die Trommeln r\u00fchren,", "tokens": ["Wenn", "sich", "die", "Trom\u00b7meln", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie ruft sie da so laut!", "tokens": ["Wie", "ruft", "sie", "da", "so", "laut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Kein' be\u00dfre Lust, als fest im Sturm", "tokens": ["Kein'", "be\u00df\u00b7re", "Lust", ",", "als", "fest", "im", "Sturm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "$,", "KOUS", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "F\u00fcr sie den Feind erschlagen", "tokens": ["F\u00fcr", "sie", "den", "Feind", "er\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und stehen als ein Turm.", "tokens": ["Und", "ste\u00b7hen", "als", "ein", "Turm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Ich hab' nicht viel zu sparen", "tokens": ["Ich", "hab'", "nicht", "viel", "zu", "spa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als wie ein reicher Gauch;", "tokens": ["Als", "wie", "ein", "rei\u00b7cher", "Gauch", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wohin wir m\u00f6gen fahren,", "tokens": ["Wo\u00b7hin", "wir", "m\u00f6\u00b7gen", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da nehm' ich, was ich brauch'.", "tokens": ["Da", "nehm'", "ich", ",", "was", "ich", "brauch'", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "He, B\u00e4uerlein, B\u00e4uerlein, sch\u00fcrz' dich nun!", "tokens": ["He", ",", "B\u00e4u\u00b7er\u00b7lein", ",", "B\u00e4u\u00b7er\u00b7lein", ",", "sch\u00fcrz'", "dich", "nun", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$,", "NN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Den Krug tu aus dem Keller,", "tokens": ["Den", "Krug", "tu", "aus", "dem", "Kel\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Tu an den Spie\u00df das Huhn!", "tokens": ["Tu", "an", "den", "Spie\u00df", "das", "Huhn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.12": {"line.1": {"text": "Drei W\u00fcrfel und ein' Karten,", "tokens": ["Drei", "W\u00fcr\u00b7fel", "und", "ein'", "Kar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die sind in jedem Schank;", "tokens": ["Die", "sind", "in", "je\u00b7dem", "Schank", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es kommt, mir aufzuwarten,", "tokens": ["Es", "kommt", ",", "mir", "auf\u00b7zu\u00b7war\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Dirnlein schlank und blank.", "tokens": ["Ein", "Dirn\u00b7lein", "schlank", "und", "blank", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mein Feinslieb das hei\u00dft Braun und Blond,", "tokens": ["Mein", "Feins\u00b7lieb", "das", "hei\u00dft", "Braun", "und", "Blond", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PDS", "VVFIN", "NE", "KON", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Schneewei\u00df und Rot-wie-Rosen,", "tokens": ["Schnee\u00b7wei\u00df", "und", "Rot\u00b7wie\u00b7Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Ein andres jeden Mond.", "tokens": ["Ein", "and\u00b7res", "je\u00b7den", "Mond", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Und rei\u00dfen mir die Kleider,", "tokens": ["Und", "rei\u00b7\u00dfen", "mir", "die", "Klei\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das schafft mir wenig Harm;", "tokens": ["Das", "schafft", "mir", "we\u00b7nig", "Harm", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mir macht der Wein, der Schneider,", "tokens": ["Mir", "macht", "der", "Wein", ",", "der", "Schnei\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Einen Rauschemantel warm;", "tokens": ["Ei\u00b7nen", "Rau\u00b7schem\u00b7an\u00b7tel", "warm", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der deckt mich zu vor aller Plag'", "tokens": ["Der", "deckt", "mich", "zu", "vor", "al\u00b7ler", "Plag'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "PTKZU", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Im Graben und auf der Schanzen", "tokens": ["Im", "Gra\u00b7ben", "und", "auf", "der", "Schan\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "APPR", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Bis an den jungen Tag.", "tokens": ["Bis", "an", "den", "jun\u00b7gen", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Und kommt eine Kugel balde", "tokens": ["Und", "kommt", "ei\u00b7ne", "Ku\u00b7gel", "bal\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und nimmt mir fort ein Bein:", "tokens": ["Und", "nimmt", "mir", "fort", "ein", "Bein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es w\u00e4chst viel Holz im Walde,", "tokens": ["Es", "w\u00e4chst", "viel", "Holz", "im", "Wal\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich darf nicht traurig sein.", "tokens": ["Ich", "darf", "nicht", "trau\u00b7rig", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ei, was mich Str\u00fcmpf' und Schuh' gekost,", "tokens": ["Ei", ",", "was", "mich", "Str\u00fcmpf'", "und", "Schuh'", "ge\u00b7kost", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "PPER", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Nun mag ich's ba\u00df vertrinken;", "tokens": ["Nun", "mag", "ich's", "ba\u00df", "ver\u00b7trin\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Das ist ein tapfrer Trost.", "tokens": ["Das", "ist", "ein", "tapf\u00b7rer", "Trost", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Und werd' ich gar erschlagen,", "tokens": ["Und", "werd'", "ich", "gar", "er\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Erschlagen auf breiter Heid':", "tokens": ["Er\u00b7schla\u00b7gen", "auf", "brei\u00b7ter", "Heid'", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Vier Spie\u00dfe m\u00fcssen mich tragen,", "tokens": ["Vier", "Spie\u00b7\u00dfe", "m\u00fcs\u00b7sen", "mich", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ein Grab steht gleich bereit.", "tokens": ["Ein", "Grab", "steht", "gleich", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So schl\u00e4gt man mir den Pummerlein pum,", "tokens": ["So", "schl\u00e4gt", "man", "mir", "den", "Pum\u00b7mer\u00b7lein", "pum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Der ist mir neunmal lieber", "tokens": ["Der", "ist", "mir", "neun\u00b7mal", "lie\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Als aller Pfaffen Gebrumm.", "tokens": ["Als", "al\u00b7ler", "Pfaf\u00b7fen", "Ge\u00b7brumm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.16": {"line.1": {"text": "Wer hat dies Lied gesungen", "tokens": ["Wer", "hat", "dies", "Lied", "ge\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PDS", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu Pfeif' und Trommelschlag?", "tokens": ["Zu", "Pfeif'", "und", "Trom\u00b7mel\u00b7schlag", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Einem Landsknecht ist's gelungen,", "tokens": ["Ei\u00b7nem", "Lands\u00b7knecht", "ist's", "ge\u00b7lun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da er zu Augsburg lag.", "tokens": ["Da", "er", "zu", "Augs\u00b7burg", "lag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Im gr\u00fcnen Baum da kehrt' er ein", "tokens": ["Im", "gr\u00fc\u00b7nen", "Baum", "da", "kehrt'", "er", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ADV", "VVFIN", "PPER", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und k\u00fc\u00dft' ein schwarzbraun M\u00e4del", "tokens": ["Und", "k\u00fc\u00dft'", "ein", "schwarz\u00b7braun", "M\u00e4\u00b7del"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und trank einen k\u00fchlen Wein.", "tokens": ["Und", "trank", "ei\u00b7nen", "k\u00fch\u00b7len", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}