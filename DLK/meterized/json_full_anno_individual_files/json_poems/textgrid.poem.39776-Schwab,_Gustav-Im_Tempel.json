{"textgrid.poem.39776": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Im Tempel", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Priester schweigt, es sendet die Gemeine", "tokens": ["Der", "Pries\u00b7ter", "schweigt", ",", "es", "sen\u00b7det", "die", "Ge\u00b7mei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von halbbewegten Lippen stumme Bitte;", "tokens": ["Von", "halb\u00b7be\u00b7weg\u00b7ten", "Lip\u00b7pen", "stum\u00b7me", "Bit\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verkl\u00e4rend gie\u00dfet ihre Heil'genscheine", "tokens": ["Ver\u00b7kl\u00e4\u00b7rend", "gie\u00b7\u00dfet", "ih\u00b7re", "Heil'\u00b7gen\u00b7schei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Sonne nieder in der Beter Mitte.", "tokens": ["Die", "Son\u00b7ne", "nie\u00b7der", "in", "der", "Be\u00b7ter", "Mit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dort steht von ihrem Glanz umwallt, die Meine,", "tokens": ["Dort", "steht", "von", "ih\u00b7rem", "Glanz", "um\u00b7wallt", ",", "die", "Mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "VVPP", "$,", "PRELS", "PPOSAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die H\u00e4nde faltet sie nach frommer Sitte,", "tokens": ["Die", "H\u00e4n\u00b7de", "fal\u00b7tet", "sie", "nach", "from\u00b7mer", "Sit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und neiget jetzt mit friedlicher Geberde", "tokens": ["Und", "nei\u00b7get", "jetzt", "mit", "fried\u00b7li\u00b7cher", "Ge\u00b7ber\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ihr sch\u00f6nes Haupt dem\u00fctiglich zur Erde.", "tokens": ["Ihr", "sch\u00f6\u00b7nes", "Haupt", "de\u00b7m\u00fc\u00b7tig\u00b7lich", "zur", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du sel'ges Kind! wie f\u00fchl' ich deine N\u00e4he!", "tokens": ["Du", "sel'\u00b7ges", "Kind", "!", "wie", "f\u00fchl'", "ich", "dei\u00b7ne", "N\u00e4\u00b7he", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kommt doch des Geistes Stral auf mich hernieder;", "tokens": ["Kommt", "doch", "des", "Geis\u00b7tes", "Stral", "auf", "mich", "her\u00b7nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In meiner Brust, so oft ich nach dir sehe,", "tokens": ["In", "mei\u00b7ner", "Brust", ",", "so", "oft", "ich", "nach", "dir", "se\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADV", "ADV", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Thut sich der Himmel auf und quellen Lieder;", "tokens": ["Thut", "sich", "der", "Him\u00b7mel", "auf", "und", "quel\u00b7len", "Lie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "PTKVZ", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und wie ich ganz in dich verloren stehe,", "tokens": ["Und", "wie", "ich", "ganz", "in", "dich", "ver\u00b7lo\u00b7ren", "ste\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "APPR", "PPER", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Gebiert dein heil'ger Sinn in mir sich wieder;", "tokens": ["Ge\u00b7biert", "dein", "heil'\u00b7ger", "Sinn", "in", "mir", "sich", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPOSAT", "ADJA", "NN", "APPR", "PPER", "PRF", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Mein Auge senkt, mein Haupt sich, wie das deine,", "tokens": ["Mein", "Au\u00b7ge", "senkt", ",", "mein", "Haupt", "sich", ",", "wie", "das", "dei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PPOSAT", "NN", "PRF", "$,", "PWAV", "ART", "PPOSAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und dein Gebet, dein Wesen wird das meine.", "tokens": ["Und", "dein", "Ge\u00b7bet", ",", "dein", "We\u00b7sen", "wird", "das", "mei\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "VAFIN", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da weckt mich wunderbar aus meiner Stille", "tokens": ["Da", "weckt", "mich", "wun\u00b7der\u00b7bar", "aus", "mei\u00b7ner", "Stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Glocken Klang und des Gesanges Wogen:", "tokens": ["Der", "Glo\u00b7cken", "Klang", "und", "des", "Ge\u00b7san\u00b7ges", "Wo\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es kommt dein Bild in unnennbarer F\u00fclle", "tokens": ["Es", "kommt", "dein", "Bild", "in", "un\u00b7nenn\u00b7ba\u00b7rer", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf allen T\u00f6nen nach mir zugeflogen,", "tokens": ["Auf", "al\u00b7len", "T\u00f6\u00b7nen", "nach", "mir", "zu\u00b7ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mein Geist ergie\u00dft sich durch die ird'sche H\u00fclle,", "tokens": ["Mein", "Geist", "er\u00b7gie\u00dft", "sich", "durch", "die", "ird'\u00b7sche", "H\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Von Liedern und Gebeten hingezogen;", "tokens": ["Von", "Lie\u00b7dern", "und", "Ge\u00b7be\u00b7ten", "hin\u00b7ge\u00b7zo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Von ", "tokens": ["Von"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.8": {"text": "Die Engel h\u00f6rt am Thron den Herrn er loben.", "tokens": ["Die", "En\u00b7gel", "h\u00f6rt", "am", "Thron", "den", "Herrn", "er", "lo\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und wie nun schweigen Glocken und Ges\u00e4nge,", "tokens": ["Und", "wie", "nun", "schwei\u00b7gen", "Glo\u00b7cken", "und", "Ge\u00b7s\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Blick' ich, erwacht, hinab, Sie noch zu finden;", "tokens": ["Blick'", "ich", ",", "er\u00b7wacht", ",", "hin\u00b7ab", ",", "Sie", "noch", "zu", "fin\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVPP", "$,", "PTKVZ", "$,", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dort wandelt Sie zur Th\u00fcre mit der Menge \u2013", "tokens": ["Dort", "wan\u00b7delt", "Sie", "zur", "Th\u00fc\u00b7re", "mit", "der", "Men\u00b7ge", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Froh, ohne Sehnsucht, seh' ich sie verschwinden;", "tokens": ["Froh", ",", "oh\u00b7ne", "Sehn\u00b7sucht", ",", "seh'", "ich", "sie", "ver\u00b7schwin\u00b7den", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUI", "NN", "$,", "VVFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "In meinem Ohr ja hallen noch die Kl\u00e4nge,", "tokens": ["In", "mei\u00b7nem", "Ohr", "ja", "hal\u00b7len", "noch", "die", "Kl\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die mich an sie, wie Priestersegen, binden.", "tokens": ["Die", "mich", "an", "sie", ",", "wie", "Pries\u00b7ter\u00b7se\u00b7gen", ",", "bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPER", "$,", "PWAV", "NN", "$,", "VVINF", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Ich bin mit ihr vor Gottes Stuhl getreten,", "tokens": ["Ich", "bin", "mit", "ihr", "vor", "Got\u00b7tes", "Stuhl", "ge\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "APPR", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und mir war klar: erh\u00f6ret sei mein Beten.", "tokens": ["Und", "mir", "war", "klar", ":", "er\u00b7h\u00f6\u00b7ret", "sei", "mein", "Be\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "$.", "VVFIN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Der Priester schweigt, es sendet die Gemeine", "tokens": ["Der", "Pries\u00b7ter", "schweigt", ",", "es", "sen\u00b7det", "die", "Ge\u00b7mei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von halbbewegten Lippen stumme Bitte;", "tokens": ["Von", "halb\u00b7be\u00b7weg\u00b7ten", "Lip\u00b7pen", "stum\u00b7me", "Bit\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verkl\u00e4rend gie\u00dfet ihre Heil'genscheine", "tokens": ["Ver\u00b7kl\u00e4\u00b7rend", "gie\u00b7\u00dfet", "ih\u00b7re", "Heil'\u00b7gen\u00b7schei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Sonne nieder in der Beter Mitte.", "tokens": ["Die", "Son\u00b7ne", "nie\u00b7der", "in", "der", "Be\u00b7ter", "Mit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dort steht von ihrem Glanz umwallt, die Meine,", "tokens": ["Dort", "steht", "von", "ih\u00b7rem", "Glanz", "um\u00b7wallt", ",", "die", "Mei