{"dta.poem.20341": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "An Chloris.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ist meine Chloris kranck/ so mu\u00df Orontes sterben.", "tokens": ["Ist", "mei\u00b7ne", "Chlo\u00b7ris", "kranck", "/", "so", "mu\u00df", "O\u00b7ron\u00b7tes", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NE", "ADJD", "$(", "ADV", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was dir beschwerlich ist/ greifft mich nicht minder an/", "tokens": ["Was", "dir", "be\u00b7schwer\u00b7lich", "ist", "/", "greifft", "mich", "nicht", "min\u00b7der", "an", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "VAFIN", "$(", "VVFIN", "PPER", "PTKNEG", "ADV", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Aus deinem ungemach qvillt endlich mein verderben/", "tokens": ["Aus", "dei\u00b7nem", "un\u00b7ge\u00b7mach", "qvillt", "end\u00b7lich", "mein", "ver\u00b7der\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJD", "VVFIN", "ADV", "PPOSAT", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Indem bey deiner pein ich nicht gesund seyn kan.", "tokens": ["In\u00b7dem", "bey", "dei\u00b7ner", "pein", "ich", "nicht", "ge\u00b7sund", "seyn", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "PPER", "PTKNEG", "VVPP", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Soll ich/ o sch\u00f6nes licht! dich nicht nach wunsche schauen?", "tokens": ["Soll", "ich", "/", "o", "sch\u00f6\u00b7nes", "licht", "!", "dich", "nicht", "nach", "wun\u00b7sche", "schau\u00b7en", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$(", "FM", "ADJA", "NN", "$.", "PPER", "PTKNEG", "APPR", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vernehm\u2019 ich/ da\u00df dein haupt mit schmertzen ist gekr\u00e4nckt/", "tokens": ["Ver\u00b7nehm'", "ich", "/", "da\u00df", "dein", "haupt", "mit", "schmert\u00b7zen", "ist", "ge\u00b7kr\u00e4nckt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$(", "KOUS", "PPOSAT", "NN", "APPR", "VVINF", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So mu\u00df ich mir ein hau\u00df von lauter dornen bauen/", "tokens": ["So", "mu\u00df", "ich", "mir", "ein", "hau\u00df", "von", "lau\u00b7ter", "dor\u00b7nen", "bau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "ART", "NN", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Daran die traurigkeit ihr schwartzes zeichen hengt.", "tokens": ["Da\u00b7ran", "die", "trau\u00b7rig\u00b7keit", "ihr", "schwart\u00b7zes", "zei\u00b7chen", "hengt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich will mich in den schoo\u00df der einsamkeit verschliessen/", "tokens": ["Ich", "will", "mich", "in", "den", "schoo\u00df", "der", "ein\u00b7sam\u00b7keit", "ver\u00b7schlies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "ART", "VVFIN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und unmuth soll mein freund/ verdru\u00df mein nachbar seyn/", "tokens": ["Und", "un\u00b7muth", "soll", "mein", "freund", "/", "ver\u00b7dru\u00df", "mein", "nach\u00b7bar", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VMFIN", "PPOSAT", "NN", "$(", "VVIMP", "PPOSAT", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ich will in angst vergehn/ in thr\u00e4nen gantz zufliessen/", "tokens": ["Ich", "will", "in", "angst", "ver\u00b7gehn", "/", "in", "thr\u00e4\u00b7nen", "gantz", "zu\u00b7flies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ADJD", "VVINF", "$(", "APPR", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ich suche finsterni\u00df/ und keinen sonnenschein.", "tokens": ["Ich", "su\u00b7che", "fins\u00b7ter\u00b7ni\u00df", "/", "und", "kei\u00b7nen", "son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "$(", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Doch schreibet Chloris mir: Mein haupt-weh ist vergangen/", "tokens": ["Doch", "schrei\u00b7bet", "Chlo\u00b7ris", "mir", ":", "Mein", "haup\u00b7tweh", "ist", "ver\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "PPER", "$.", "PPOSAT", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So zeigt der himmel mir den alten \u00fcberflu\u00df/", "tokens": ["So", "zeigt", "der", "him\u00b7mel", "mir", "den", "al\u00b7ten", "\u00fc\u00b7berf\u00b7lu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ich w\u00fcnsche diesen tag nichts anders zu erlangen", "tokens": ["Ich", "w\u00fcn\u00b7sche", "die\u00b7sen", "tag", "nichts", "an\u00b7ders", "zu", "er\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "PIS", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Als meiner Chloris blick und ihren s\u00fcssen ku\u00df.", "tokens": ["Als", "mei\u00b7ner", "Chlo\u00b7ris", "blick", "und", "ih\u00b7ren", "s\u00fcs\u00b7sen", "ku\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NE", "NN", "KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}