{"dta.poem.19956": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Waldleben .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.57", "no:0.42"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sey willkommen, Wandersmann,", "tokens": ["Sey", "will\u00b7kom\u00b7men", ",", "Wan\u00b7ders\u00b7mann", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "PWAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In des Waldes Einsamkeit!", "tokens": ["In", "des", "Wal\u00b7des", "Ein\u00b7sam\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was ein armes Leben freut,", "tokens": ["Was", "ein", "ar\u00b7mes", "Le\u00b7ben", "freut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hier man einzig finden kann.", "tokens": ["Hier", "man", "ein\u00b7zig", "fin\u00b7den", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "ADJD", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "An der Quelle ruht das Reh,", "tokens": ["An", "der", "Quel\u00b7le", "ruht", "das", "Reh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Drossel \u00fcbet freien Sang;", "tokens": ["Dros\u00b7sel", "\u00fc\u00b7bet", "frei\u00b7en", "Sang", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Waldesnacht mach' dir nicht bang,", "tokens": ["Wal\u00b7des\u00b7nacht", "mach'", "dir", "nicht", "bang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gr\u00fcn thut keinem Auge weh.", "tokens": ["Gr\u00fcn", "thut", "kei\u00b7nem", "Au\u00b7ge", "weh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Bach und Thau giebt k\u00fchlen Schein,", "tokens": ["Bach", "und", "Thau", "giebt", "k\u00fch\u00b7len", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Blume bl\u00fchet ungepfl\u00fcckt,", "tokens": ["Blu\u00b7me", "bl\u00fc\u00b7het", "un\u00b7ge\u00b7pfl\u00fcckt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tief in Kl\u00fcften, nie erblickt,", "tokens": ["Tief", "in", "Kl\u00fcf\u00b7ten", ",", "nie", "er\u00b7blickt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$,", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schlummert Gold und Edelstein.", "tokens": ["Schlum\u00b7mert", "Gold", "und", "E\u00b7del\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Eile nicht zu Stadt und Thal!", "tokens": ["Ei\u00b7le", "nicht", "zu", "Stadt", "und", "Thal", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Eine M\u00fchle treibt der Quell:", "tokens": ["Ei\u00b7ne", "M\u00fch\u00b7le", "treibt", "der", "Quell", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Drossel, so gesungen hell,", "tokens": ["Dros\u00b7sel", ",", "so", "ge\u00b7sun\u00b7gen", "hell", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADJD", "ADJD", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.4": {"text": "Sizt im Bauer stumm und kahl.", "tokens": ["Sizt", "im", "Bau\u00b7er", "stumm", "und", "kahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Aus der Erde stillem Schoo\u00df", "tokens": ["Aus", "der", "Er\u00b7de", "stil\u00b7lem", "Schoo\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Rei\u00dfen sie den Edelstein;", "tokens": ["Rei\u00b7\u00dfen", "sie", "den", "E\u00b7del\u00b7stein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie ein Auge giebt er Schein,", "tokens": ["Wie", "ein", "Au\u00b7ge", "giebt", "er", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Das von Thr\u00e4nen \u00fcberflo\u00df.", "tokens": ["Das", "von", "Thr\u00e4\u00b7nen", "\u00fc\u00b7ber\u00b7flo\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Armer, armer Wandersmann!", "tokens": ["Ar\u00b7mer", ",", "ar\u00b7mer", "Wan\u00b7ders\u00b7mann", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil' o weil' in Waldesnacht!", "tokens": ["Weil'", "o", "weil'", "in", "Wal\u00b7des\u00b7nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Drau\u00dfen Mond und Sonne wacht,", "tokens": ["Drau\u00b7\u00dfen", "Mond", "und", "Son\u00b7ne", "wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sieht dich jeder fragend an.", "tokens": ["Sieht", "dich", "je\u00b7der", "fra\u00b7gend", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Aber hier in Waldesschoo\u00df", "tokens": ["A\u00b7ber", "hier", "in", "Wal\u00b7des\u00b7schoo\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gehst du einsam mit dem Quell,", "tokens": ["Gehst", "du", "ein\u00b7sam", "mit", "dem", "Quell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Siehet dich kein Auge hell,", "tokens": ["Sie\u00b7het", "dich", "kein", "Au\u00b7ge", "hell", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Als der Thau auf Blum' und Moos.", "tokens": ["Als", "der", "Thau", "auf", "Blum'", "und", "Moos", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}}}}