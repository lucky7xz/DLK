{"textgrid.poem.66627": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Himmelfahrt", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bunte Blumen, gr\u00fcne B\u00fcsche,", "tokens": ["Bun\u00b7te", "Blu\u00b7men", ",", "gr\u00fc\u00b7ne", "B\u00fc\u00b7sche", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Burschen, M\u00e4dchen Arm in Arm,", "tokens": ["Bur\u00b7schen", ",", "M\u00e4d\u00b7chen", "Arm", "in", "Arm", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In der k\u00fchlen Morgenfrische", "tokens": ["In", "der", "k\u00fch\u00b7len", "Mor\u00b7gen\u00b7fri\u00b7sche"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Locker schlendernder Fr\u00fchlingsschwarm.", "tokens": ["Lo\u00b7cker", "schlen\u00b7dern\u00b7der", "Fr\u00fch\u00b7lings\u00b7schwarm", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "\u00dcbern\u00e4chtige Gesichter,", "tokens": ["\u00dc\u00b7bern\u00b7\u00e4ch\u00b7ti\u00b7ge", "Ge\u00b7sich\u00b7ter", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lange, lange noch nicht matt,", "tokens": ["Lan\u00b7ge", ",", "lan\u00b7ge", "noch", "nicht", "matt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "ADV", "PTKNEG", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Lebenslustiges Gelichter,", "tokens": ["Le\u00b7bens\u00b7lus\u00b7ti\u00b7ges", "Ge\u00b7lich\u00b7ter", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Lange, lange noch nicht satt.", "tokens": ["Lan\u00b7ge", ",", "lan\u00b7ge", "noch", "nicht", "satt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "ADV", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "\u00bbheut lieb' ich die Susanne", "tokens": ["\u00bb", "heut", "lieb'", "ich", "die", "Su\u00b7san\u00b7ne"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Und morgen die Marianne,", "tokens": ["Und", "mor\u00b7gen", "die", "Ma\u00b7ri\u00b7an\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Halli, Hallo!", "tokens": ["Hal\u00b7li", ",", "Hal\u00b7lo", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.12": {"text": "Wir leben so \u2013", "tokens": ["Wir", "le\u00b7ben", "so", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Vom lustigen Berge in die lustige Stadt.\u00ab", "tokens": ["Vom", "lus\u00b7ti\u00b7gen", "Ber\u00b7ge", "in", "die", "lus\u00b7ti\u00b7ge", "Stadt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Der da mit ihren zerlockerten Haaren", "tokens": ["Der", "da", "mit", "ih\u00b7ren", "zer\u00b7lo\u00b7cker\u00b7ten", "Haa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Ist wohl die Unschuld gen Himmel gefahren", "tokens": ["Ist", "wohl", "die", "Un\u00b7schuld", "gen", "Him\u00b7mel", "ge\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "APPR", "NN", "VVPP"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Heut in dieser selbigen Nacht;", "tokens": ["Heut", "in", "die\u00b7ser", "sel\u00b7bi\u00b7gen", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.17": {"text": "Maienn\u00e4chte sind Liebesschulen,", "tokens": ["Mai\u00b7en\u00b7n\u00e4ch\u00b7te", "sind", "Lie\u00b7bes\u00b7schu\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.18": {"text": "Lieblich ist es im Gr\u00fcnen buhlen,", "tokens": ["Lieb\u00b7lich", "ist", "es", "im", "Gr\u00fc\u00b7nen", "buh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.19": {"text": "Und kein W\u00e4chter der Sitte wacht.", "tokens": ["Und", "kein", "W\u00e4ch\u00b7ter", "der", "Sit\u00b7te", "wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.20": {"text": "In den schw\u00e4rzlichen Augenringen", "tokens": ["In", "den", "schw\u00e4rz\u00b7li\u00b7chen", "Au\u00b7gen\u00b7rin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.21": {"text": "Kauert schl\u00e4frig geb\u00fc\u00dfte Lust,", "tokens": ["Kau\u00b7ert", "schl\u00e4f\u00b7rig", "ge\u00b7b\u00fc\u00df\u00b7te", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.22": {"text": "T\u00fcchtig hat das Feuer geru\u00dft.", "tokens": ["T\u00fcch\u00b7tig", "hat", "das", "Feu\u00b7er", "ge\u00b7ru\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.23": {"text": "Aber mit silberreinem Singen", "tokens": ["A\u00b7ber", "mit", "sil\u00b7ber\u00b7rei\u00b7nem", "Sin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.24": {"text": "Sittige Dirnen vor\u00fcberspringen,", "tokens": ["Sit\u00b7ti\u00b7ge", "Dir\u00b7nen", "vor\u00b7\u00fc\u00b7bers\u00b7prin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.25": {"text": "Maiengl\u00f6ckchen an keuscher Brust.", "tokens": ["Mai\u00b7en\u00b7gl\u00f6\u00b7ck\u00b7chen", "an", "keu\u00b7scher", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+---+-+", "measure": "unknown.measure.tetra"}, "line.26": {"text": "Aus dem offenen Bierhaus dringen,", "tokens": ["Aus", "dem", "of\u00b7fe\u00b7nen", "Bier\u00b7haus", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.27": {"text": "Klingen Schalmei und Harmonika,", "tokens": ["Klin\u00b7gen", "Schal\u00b7mei", "und", "Har\u00b7mo\u00b7ni\u00b7ka", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NE", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.28": {"text": "Klingen Harmonika und Schalmei;", "tokens": ["Klin\u00b7gen", "Har\u00b7mo\u00b7ni\u00b7ka", "und", "Schal\u00b7mei", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "KON", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.29": {"text": "Italiener mit lautem Geschrei", "tokens": ["I\u00b7ta\u00b7li\u00b7e\u00b7ner", "mit", "lau\u00b7tem", "Ge\u00b7schrei"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.30": {"text": "Feigenkr\u00e4nze zu H\u00e4upten schwingen,", "tokens": ["Fei\u00b7gen\u00b7kr\u00e4n\u00b7ze", "zu", "H\u00e4up\u00b7ten", "schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.31": {"text": "Brezelweiber schleifen vorbei.", "tokens": ["Bre\u00b7zel\u00b7wei\u00b7ber", "schlei\u00b7fen", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.32": {"text": "Sch\u00fcchterne Sonnenstrahlen blinken,", "tokens": ["Sch\u00fcch\u00b7ter\u00b7ne", "Son\u00b7nen\u00b7strah\u00b7len", "blin\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.33": {"text": "Schimmerwellen am Waldesrand,", "tokens": ["Schim\u00b7mer\u00b7wel\u00b7len", "am", "Wal\u00b7des\u00b7rand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.34": {"text": "Ach, wie herrlich die Wipfel winken,", "tokens": ["Ach", ",", "wie", "herr\u00b7lich", "die", "Wip\u00b7fel", "win\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.35": {"text": "Lichte Buchen im Brautgewand!", "tokens": ["Lich\u00b7te", "Bu\u00b7chen", "im", "Braut\u00b7ge\u00b7wand", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPRART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.36": {"text": "Und noch ehe mit breitem Strahl", "tokens": ["Und", "noch", "e\u00b7he", "mit", "brei\u00b7tem", "Strahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "KOUS", "APPR", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.37": {"text": "Siegreich mich die Sonne bestreicht,", "tokens": ["Sieg\u00b7reich", "mich", "die", "Son\u00b7ne", "be\u00b7streicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.38": {"text": "Seh' ich in der Tiefe das Tal,", "tokens": ["Seh'", "ich", "in", "der", "Tie\u00b7fe", "das", "Tal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.39": {"text": "Habe des Berges Kulm erreicht.", "tokens": ["Ha\u00b7be", "des", "Ber\u00b7ges", "Kulm", "er\u00b7reicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.40": {"text": "Wolkenspiegelnd und funkenwiegelnd,", "tokens": ["Wol\u00b7ken\u00b7spie\u00b7gelnd", "und", "fun\u00b7ken\u00b7wie\u00b7gelnd", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVPP", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.41": {"text": "Perlgrau zittert der See.", "tokens": ["Perl\u00b7grau", "zit\u00b7tert", "der", "See", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.42": {"text": "Dicker Sonnenduft", "tokens": ["Di\u00b7cker", "Son\u00b7nen\u00b7duft"], "token_info": ["word", "word"], "pos": ["PDAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.43": {"text": "H\u00fcllt die ferne Luft,", "tokens": ["H\u00fcllt", "die", "fer\u00b7ne", "Luft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.44": {"text": "Tief im Flor versinkt der Firnenschnee.", "tokens": ["Tief", "im", "Flor", "ver\u00b7sinkt", "der", "Fir\u00b7nen\u00b7schnee", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Bunte Blumen, gr\u00fcne B\u00fcsche,", "tokens": ["Bun\u00b7te", "Blu\u00b7men", ",", "gr\u00fc\u00b7ne", "B\u00fc\u00b7sche", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Burschen, M\u00e4dchen Arm in Arm,", "tokens": ["Bur\u00b7schen", ",", "M\u00e4d\u00b7chen", "Arm", "in", "Arm", ","]