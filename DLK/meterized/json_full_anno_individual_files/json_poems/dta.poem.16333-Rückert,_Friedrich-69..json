{"dta.poem.16333": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "69.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Dankbarkeit ergeht nicht in des Handelns Schranken,", "tokens": ["Die", "Dank\u00b7bar\u00b7keit", "er\u00b7geht", "nicht", "in", "des", "Han\u00b7delns", "Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Dankbarkeit besteht, das Wort sagts, im Gedanken.", "tokens": ["Die", "Dank\u00b7bar\u00b7keit", "be\u00b7steht", ",", "das", "Wort", "sagts", ",", "im", "Ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mein Denken dankt, es ist mein Dank euch zugedacht,", "tokens": ["Mein", "Den\u00b7ken", "dankt", ",", "es", "ist", "mein", "Dank", "euch", "zu\u00b7ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn auch ihn weder Wort noch Werk bemerklich macht.", "tokens": ["Wenn", "auch", "ihn", "we\u00b7der", "Wort", "noch", "Werk", "be\u00b7mer\u00b7klich", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "KON", "NN", "ADV", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Undankbar w\u00e4r' ich sonst in einem wicht'gen Falle;", "tokens": ["Un\u00b7dank\u00b7bar", "w\u00e4r'", "ich", "sonst", "in", "ei\u00b7nem", "wicht'\u00b7gen", "Fal\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn wem am meisten Dank ich schulde, todt sind alle.", "tokens": ["Denn", "wem", "am", "meis\u00b7ten", "Dank", "ich", "schul\u00b7de", ",", "todt", "sind", "al\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPRART", "ADJA", "NN", "PPER", "VVFIN", "$,", "ADJD", "VAFIN", "PIS", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Mit Worten kann ich mich bei ihnen nicht bedanken,", "tokens": ["Mit", "Wor\u00b7ten", "kann", "ich", "mich", "bei", "ih\u00b7nen", "nicht", "be\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "PRF", "APPR", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch sie begn\u00fcgen sich mit dankenden Gedanken.", "tokens": ["Doch", "sie", "be\u00b7gn\u00fc\u00b7gen", "sich", "mit", "dan\u00b7ken\u00b7den", "Ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}