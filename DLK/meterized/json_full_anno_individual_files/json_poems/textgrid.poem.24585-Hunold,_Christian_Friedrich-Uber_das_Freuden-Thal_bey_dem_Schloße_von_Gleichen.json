{"textgrid.poem.24585": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Uber das Freuden-Thal/ bey dem Schlo\u00dfe von Gleichen", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo hat di\u00df Freuden-Thal den Uhrsprung hergenommen?", "tokens": ["Wo", "hat", "di\u00df", "Freu\u00b7den\u00b7Thal", "den", "Uhrs\u00b7prung", "her\u00b7ge\u00b7nom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PDS", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Davon/ weil Lieb' und Treu zusammen da gekommen.", "tokens": ["Da\u00b7von", "/", "weil", "Lieb'", "und", "Treu", "zu\u00b7sam\u00b7men", "da", "ge\u00b7kom\u00b7men", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$(", "KOUS", "NN", "KON", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Di\u00df Angedencken wird im Fr\u00fchling wieder neu/", "tokens": ["Di\u00df", "An\u00b7ge\u00b7den\u00b7cken", "wird", "im", "Fr\u00fch\u00b7ling", "wie\u00b7der", "neu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VAFIN", "APPRART", "NN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bey einem Kegel-Spiel/ und wahrer Freundschaffts-Treu.", "tokens": ["Bey", "ei\u00b7nem", "Ke\u00b7gel\u00b7Spiel", "/", "und", "wah\u00b7rer", "Freund\u00b7schaffts\u00b7Treu", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$(", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexa