{"textgrid.poem.59936": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "22.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sollen gut meine Lieder der Liebe gesungen werden:", "tokens": ["Sol\u00b7len", "gut", "mei\u00b7ne", "Lie\u00b7der", "der", "Lie\u00b7be", "ge\u00b7sun\u00b7gen", "wer\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "PPOSAT", "NN", "ART", "NN", "VVPP", "VAINF", "$."], "meter": "+-+--+--+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "M\u00fcssen perlende Becher in Liebe geschwungen werden,", "tokens": ["M\u00fcs\u00b7sen", "per\u00b7len\u00b7de", "Be\u00b7cher", "in", "Lie\u00b7be", "ge\u00b7schwun\u00b7gen", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "APPR", "NN", "VVPP", "VAINF", "$,"], "meter": "+-+--+--+--+-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Bis die Freude in uns wie eine Sonne aufgeht,", "tokens": ["Bis", "die", "Freu\u00b7de", "in", "uns", "wie", "ei\u00b7ne", "Son\u00b7ne", "auf\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PPER", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Davon die Sorgen, die Nebel des Geistes, bezwungen werden.", "tokens": ["Da\u00b7von", "die", "Sor\u00b7gen", ",", "die", "Ne\u00b7bel", "des", "Geis\u00b7tes", ",", "be\u00b7zwun\u00b7gen", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$,", "VVPP", "VAINF", "$."], "meter": "-+-+--+--+--+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Rosen netzet der Tau, rosige Lippen der Wein \u2013", "tokens": ["Ro\u00b7sen", "net\u00b7zet", "der", "Tau", ",", "ro\u00b7si\u00b7ge", "Lip\u00b7pen", "der", "Wein", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "ADJA", "NN", "ART", "NN", "$("], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "So mu\u00df der Sch\u00f6nheit Geheimnis errungen werden!", "tokens": ["So", "mu\u00df", "der", "Sch\u00f6n\u00b7heit", "Ge\u00b7heim\u00b7nis", "er\u00b7run\u00b7gen", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Nur wo Liebe und Witz mit den Becher sie schleift,", "tokens": ["Nur", "wo", "Lie\u00b7be", "und", "Witz", "mit", "den", "Be\u00b7cher", "sie", "schleift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "NN", "KON", "NN", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Mag der Schliff echter Versdiamanten gelungen werden,", "tokens": ["Mag", "der", "Schliff", "ech\u00b7ter", "Vers\u00b7di\u00b7a\u00b7man\u00b7ten", "ge\u00b7lun\u00b7gen", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADJA", "NN", "VVPP", "VAINF", "$,"], "meter": "+--+-+--+--+-+-", "measure": "iambic.hexa.invert"}}, "stanza.5": {"line.1": {"text": "Da\u00df von der s\u00fc\u00dfen Gewalt ihrer blendenden Glut", "tokens": ["Da\u00df", "von", "der", "s\u00fc\u00b7\u00dfen", "Ge\u00b7walt", "ih\u00b7rer", "blen\u00b7den\u00b7den", "Glut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "PPOSAT", "ADJA", "NN"], "meter": "---+--+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Alle f\u00fchlenden Herzen in Liebe umschlungen werden!", "tokens": ["Al\u00b7le", "f\u00fch\u00b7len\u00b7den", "Her\u00b7zen", "in", "Lie\u00b7be", "um\u00b7schlun\u00b7gen", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "APPR", "NN", "VVPP", "VAINF", "$."], "meter": "--+--+--+--+-+-", "measure": "anapaest.tetra.plus"}}, "stanza.6": {"line.1": {"text": "Also schufst du dein Lied, o Mirza-Schaffy!", "tokens": ["Al\u00b7so", "schufst", "du", "dein", "Lied", ",", "o", "Mir\u00b7za\u00b7Schaffy", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "FM", "NE", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Wie es geschaffen, so mu\u00df es gesungen werden:", "tokens": ["Wie", "es", "ge\u00b7schaf\u00b7fen", ",", "so", "mu\u00df", "es", "ge\u00b7sun\u00b7gen", "wer\u00b7den", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "$,", "ADV", "VMFIN", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Da\u00df vor lauter Entz\u00fccken und Wonnegef\u00fchl", "tokens": ["Da\u00df", "vor", "lau\u00b7ter", "Ent\u00b7z\u00fc\u00b7cken", "und", "Won\u00b7ne\u00b7ge\u00b7f\u00fchl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PIAT", "NN", "KON", "NN"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "N\u00e4rrisch die Alten u