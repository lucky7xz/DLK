{"textgrid.poem.40219": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Jugendweisheit", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nehmen wir ", "tokens": ["Neh\u00b7men", "wir"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "haben wir's nicht recht verstanden:", "tokens": ["ha\u00b7ben", "wir's", "nicht", "recht", "ver\u00b7stan\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PTKNEG", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Menschenleben ist das Leben", "tokens": ["Men\u00b7schen\u00b7le\u00b7ben", "ist", "das", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "so nur, wie wir es ", "tokens": ["so", "nur", ",", "wie", "wir", "es"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "PWAV", "PPER", "PPER"], "meter": "---+-", "measure": "unknown.measure.single"}}, "stanza.2": {"line.1": {"text": "Ja, so schw\u00e4rmt' ich seelentrunken!", "tokens": ["Ja", ",", "so", "schw\u00e4rmt'", "ich", "see\u00b7len\u00b7trun\u00b7ken", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "VVFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie mir Alles wohlbehagte,", "tokens": ["Wie", "mir", "Al\u00b7les", "wohl\u00b7be\u00b7hag\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "was ich f\u00fchlte, was ich sagte:", "tokens": ["was", "ich", "f\u00fchl\u00b7te", ",", "was", "ich", "sag\u00b7te", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "in mein Spiegelbild versunken!", "tokens": ["in", "mein", "Spie\u00b7gel\u00b7bild", "ver\u00b7sun\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch ", "tokens": ["Doch"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "mit den Wegen sich beraten!", "tokens": ["mit", "den", "We\u00b7gen", "sich", "be\u00b7ra\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zwar den J\u00fcngling ehrt sein F\u00fchlen,", "tokens": ["Zwar", "den", "J\u00fcng\u00b7ling", "ehrt", "sein", "F\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "doch dem Manne ziemen Thaten!", "tokens": ["doch", "dem", "Man\u00b7ne", "zie\u00b7men", "Tha\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nehmen wir ", "tokens": ["Neh\u00b7men", "wir"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "haben wir's nicht recht verstanden:", "tokens": ["ha\u00b7ben", "wir's", "nicht", "recht", "ver\u00b7stan\u00b7den", ":"]