{"textgrid.poem.55601": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Natur und Kunst, sie scheinen sich zu fliehen ...", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Natur und Kunst, sie scheinen sich zu fliehen", "tokens": ["Na\u00b7tur", "und", "Kunst", ",", "sie", "schei\u00b7nen", "sich", "zu", "flie\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "PPER", "VVFIN", "PRF", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und haben sich, eh man es denkt, gefunden;", "tokens": ["Und", "ha\u00b7ben", "sich", ",", "eh", "man", "es", "denkt", ",", "ge\u00b7fun\u00b7den", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VAFIN", "PRF", "$,", "KOUS", "PIS", "PPER", "VVFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Widerwille ist auch mir verschwunden,", "tokens": ["Der", "Wi\u00b7der\u00b7wil\u00b7le", "ist", "auch", "mir", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und beide scheinen gleich mich anzuziehen.", "tokens": ["Und", "bei\u00b7de", "schei\u00b7nen", "gleich", "mich", "an\u00b7zu\u00b7zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADV", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es gilt wohl nur ein redliches Bem\u00fchen!", "tokens": ["Es", "gilt", "wohl", "nur", "ein", "red\u00b7li\u00b7ches", "Be\u00b7m\u00fc\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wenn wir erst in abgeme\u00dfnen Stunden", "tokens": ["Und", "wenn", "wir", "erst", "in", "ab\u00b7ge\u00b7me\u00df\u00b7nen", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit Geist und Flei\u00df uns an die Kunst gebunden,", "tokens": ["Mit", "Geist", "und", "Flei\u00df", "uns", "an", "die", "Kunst", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mag frei Natur im Herzen wieder gl\u00fchen.", "tokens": ["Mag", "frei", "Na\u00b7tur", "im", "Her\u00b7zen", "wie\u00b7der", "gl\u00fc\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "NN", "APPRART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So ist's mit aller Bildung auch beschaffen:", "tokens": ["So", "ist's", "mit", "al\u00b7ler", "Bil\u00b7dung", "auch", "be\u00b7schaf\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PIAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vergebens werden ungebundne Geister", "tokens": ["Ver\u00b7ge\u00b7bens", "wer\u00b7den", "un\u00b7ge\u00b7bund\u00b7ne", "Geis\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nach der Vollendung reiner H\u00f6he streben.", "tokens": ["Nach", "der", "Vol\u00b7len\u00b7dung", "rei\u00b7ner", "H\u00f6\u00b7he", "stre\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Wer Gro\u00dfes will, mu\u00df sich zusammenraffen;", "tokens": ["Wer", "Gro\u00b7\u00dfes", "will", ",", "mu\u00df", "sich", "zu\u00b7sam\u00b7men\u00b7raf\u00b7fen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VMFIN", "$,", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In der Beschr\u00e4nkung zeigt sich erst der Meister,", "tokens": ["In", "der", "Be\u00b7schr\u00e4n\u00b7kung", "zeigt", "sich", "erst", "der", "Meis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PRF", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und das Gesetz nur kann uns Freiheit geben.", "tokens": ["Und", "das", "Ge\u00b7setz", "nur", "kann", "uns", "Frei\u00b7heit", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Natur und Kunst, sie scheinen sich zu fliehen", "tokens": ["Na\u00b7tur", "und", "Kunst", ",", "sie", "schei\u00b7nen", "sich", "zu", "flie\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "PPER", "VVFIN", "PRF", "PTKZU", "VVINF"], "meter": "-