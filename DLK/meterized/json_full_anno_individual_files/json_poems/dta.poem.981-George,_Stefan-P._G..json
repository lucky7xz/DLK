{"dta.poem.981": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "P. G.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.85", "nl:0.14"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Im offnen leben wo ihr all euch gleichet                 ", "tokens": ["Im", "off\u00b7nen", "le\u00b7ben", "wo", "ihr", "all", "euch", "glei\u00b7chet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "VVINF", "PWAV", "PPER", "PIAT", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo ihr fast niemals wie ihr f&#252;hlet saget                 ", "tokens": ["Wo", "ihr", "fast", "nie\u00b7mals", "wie", "ihr", "f", "&#252;", "hlet", "sa\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "XML_entity", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ADV", "KOKOM", "PPOSAT", "NN", "$(", "VVFIN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "War manches kommen doch von starkem zittern                 ", "tokens": ["War", "man\u00b7ches", "kom\u00b7men", "doch", "von", "star\u00b7kem", "zit\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIS", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "War manche trennung voll zerdr&#252;ckter thr&#228;nen                 ", "tokens": ["War", "man\u00b7che", "tren\u00b7nung", "voll", "zerdr", "&#252;", "ck\u00b7ter", "thr", "&#228;", "nen"], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word", "word", "XML_entity", "word"], "pos": ["VAFIN", "PIAT", "NN", "ADJD", "FM", "FM", "FM", "FM", "$(", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Es waren tage gross wo ihr euch gabet                 ", "tokens": ["Es", "wa\u00b7ren", "ta\u00b7ge", "gross", "wo", "ihr", "euch", "ga\u00b7bet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "ADJA", "PWAV", "PPER", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wo ihr die schleier eurer klugheit risset                 ", "tokens": ["Wo", "ihr", "die", "schlei\u00b7er", "eu\u00b7rer", "klug\u00b7heit", "ris\u00b7set"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "ADJA", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und abende wo nichts geschah doch t\u00f6ne                 ", "tokens": ["Und", "a\u00b7ben\u00b7de", "wo", "nichts", "ge\u00b7schah", "doch", "t\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "PWAV", "PIS", "VVFIN", "ADV", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und blicke fielen ewgen angedenkens.               ", "tokens": ["Und", "bli\u00b7cke", "fie\u00b7len", "ew\u00b7gen", "an\u00b7ge\u00b7den\u00b7kens", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}