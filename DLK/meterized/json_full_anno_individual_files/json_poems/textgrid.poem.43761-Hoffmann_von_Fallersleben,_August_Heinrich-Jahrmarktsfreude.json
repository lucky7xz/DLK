{"textgrid.poem.43761": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Jahrmarktsfreude", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jahrmarkt ist in jenem St\u00e4dtchen:", "tokens": ["Jahr\u00b7markt", "ist", "in", "je\u00b7nem", "St\u00e4dt\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00f6nnt' auch ich dort heute sein!", "tokens": ["K\u00f6nnt'", "auch", "ich", "dort", "heu\u00b7te", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "ADV", "ADV", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Junge Burschen geh'n und M\u00e4dchen,", "tokens": ["Jun\u00b7ge", "Bur\u00b7schen", "geh'n", "und", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles geht zur Stadt hinein.", "tokens": ["Al\u00b7les", "geht", "zur", "Stadt", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und mein Herz, wie w\u00fcrd' es lachen,", "tokens": ["Und", "mein", "Herz", ",", "wie", "w\u00fcrd'", "es", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PWAV", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ja, ich w\u00e4r' ein gl\u00fccklich Kind