rd", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PIS", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sprach nicht dr\u00fcben an der Pforte,", "tokens": ["Sprach", "nicht", "dr\u00fc\u00b7ben", "an", "der", "Pfor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Als er schied, der Gro\u00dfpapa \u2013", "tokens": ["Als", "er", "schied", ",", "der", "Gro\u00df\u00b7pa\u00b7pa", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ei, das waren s\u00fc\u00dfe Worte!", "tokens": ["Ei", ",", "das", "wa\u00b7ren", "s\u00fc\u00b7\u00dfe", "Wor\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PDS", "VAFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "W\u00e4r' er bald nur wieder da!", "tokens": ["W\u00e4r'", "er", "bald", "nur", "wie\u00b7der", "da", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Er versprach vor allen Dingen,", "tokens": ["Er", "ver\u00b7sprach", "vor", "al\u00b7len", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die mein Herz schon lang begehrt,", "tokens": ["Die", "mein", "Herz", "schon", "lang", "be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "