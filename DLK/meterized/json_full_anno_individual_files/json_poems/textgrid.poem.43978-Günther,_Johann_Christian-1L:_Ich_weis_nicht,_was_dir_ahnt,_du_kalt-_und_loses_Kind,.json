{"textgrid.poem.43978": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich weis nicht, was dir ahnt, du kalt- und loses Kind,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich weis nicht, was dir ahnt, du kalt- und loses Kind,", "tokens": ["Ich", "weis", "nicht", ",", "was", "dir", "ahnt", ",", "du", "kal\u00b7t", "und", "lo\u00b7ses", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "PTKNEG", "$,", "PWS", "PPER", "VVFIN", "$,", "PPER", "TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "-+---+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da\u00df meine Lieb und Treu so gar nicht f\u00e4hig sind,", "tokens": ["Da\u00df", "mei\u00b7ne", "Lieb", "und", "Treu", "so", "gar", "nicht", "f\u00e4\u00b7hig", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "KON", "NN", "ADV", "ADV", "PTKNEG", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die G\u00fcte deiner Brust auch in geringen Sachen", "tokens": ["Die", "G\u00fc\u00b7te", "dei\u00b7ner", "Brust", "auch", "in", "ge\u00b7rin\u00b7gen", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mir zur Erg\u00f6zligkeit recht ofenbahr zu machen.", "tokens": ["Mir", "zur", "Er\u00b7g\u00f6z\u00b7lig\u00b7keit", "recht", "o\u00b7fen\u00b7bahr", "zu", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du sagest alles zu und h\u00e4ltst doch keinmahl Wort;", "tokens": ["Du", "sa\u00b7gest", "al\u00b7les", "zu", "und", "h\u00e4ltst", "doch", "kein\u00b7mahl", "Wort", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PTKVZ", "KON", "VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wir gehn mit Lust zu dir und ziehn so traurig fort,", "tokens": ["Wir", "gehn", "mit", "Lust", "zu", "dir", "und", "ziehn", "so", "trau\u00b7rig", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "PPER", "KON", "VVFIN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Als ob dein Herz noch nichts von meiner Liebe w\u00fcste", "tokens": ["Als", "ob", "dein", "Herz", "noch", "nichts", "von", "mei\u00b7ner", "Lie\u00b7be", "w\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPOSAT", "NN", "ADV", "PIS", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und ich erst vierzehn Jahr wie Jacob dienen m\u00fcste.", "tokens": ["Und", "ich", "erst", "vier\u00b7zehn", "Jahr", "wie", "Ja\u00b7cob", "die\u00b7nen", "m\u00fcs\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "CARD", "NN", "KOKOM", "NE", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich weis nicht, welcher Fall dir das Gehirn verr\u00fcckt", "tokens": ["Ich", "weis", "nicht", ",", "wel\u00b7cher", "Fall", "dir", "das", "Ge\u00b7hirn", "ver\u00b7r\u00fcckt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PTKVZ", "PTKNEG", "$,", "PWAT", "NN", "PPER", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und deinen muntern Geist in Eigensinn erstickt.", "tokens": ["Und", "dei\u00b7nen", "mun\u00b7tern", "Geist", "in", "Ei\u00b7gen\u00b7sinn", "er\u00b7stickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Bald geh ich mit der Furcht im Zimmer auf und nieder,", "tokens": ["Bald", "geh", "ich", "mit", "der", "Furcht", "im", "Zim\u00b7mer", "auf", "und", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "APPRART", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Als k\u00e4men jezt bey dir viel alte Funcken wieder,", "tokens": ["Als", "k\u00e4\u00b7men", "jezt", "bey", "dir", "viel", "al\u00b7te", "Fun\u00b7cken", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "ADV", "APPR", "PPER", "PIAT", "ADJA", "NN", "ADV", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.13": {"text": "Womit ein falsches M., das lezt auch au\u00dfen blieb,", "tokens": ["Wo\u00b7mit", "ein", "fal\u00b7sches", "M.", ",", "das", "lezt", "auch", "au\u00b7\u00dfen", "blieb", ","], "token_info": ["word", "word", "word", "abbreviation", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NE", "$,", "PRELS", "ADV", "ADV", "ADV", "VVFIN", "$,"], "meter": "---+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Den ersten Liebeszug dir in das Herze schrieb.", "tokens": ["Den", "ers\u00b7ten", "Lie\u00b7bes\u00b7zug", "dir", "in", "das", "Her\u00b7ze", "schrieb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Bald aber mach ich mir von neuem Trostgedancken,", "tokens": ["Bald", "a\u00b7ber", "mach", "ich", "mir", "von", "neu\u00b7em", "Trost\u00b7ge\u00b7dan\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Als w\u00fcrd ich nur probiert und durch verstelltes Wancken", "tokens": ["Als", "w\u00fcrd", "ich", "nur", "pro\u00b7biert", "und", "durch", "ver\u00b7stell\u00b7tes", "Wan\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PPER", "ADV", "VVPP", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "In meiner Treu ge\u00fcbt; bald f\u00e4llt was \u00c4rgers ein;", "tokens": ["In", "mei\u00b7ner", "Treu", "ge\u00b7\u00fcbt", ";", "bald", "f\u00e4llt", "was", "\u00c4r\u00b7gers", "ein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$.", "ADV", "VVFIN", "PIS", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Weil meiner Feind jezt viel und zwar auch gro\u00dfe seyn,", "tokens": ["Weil", "mei\u00b7ner", "Feind", "jezt", "viel", "und", "zwar", "auch", "gro\u00b7\u00dfe", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADV", "KON", "ADV", "ADV", "ADJA", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Die, da ich nie geschont, die Warheit frey zu sagen,", "tokens": ["Die", ",", "da", "ich", "nie", "ge\u00b7schont", ",", "die", "War\u00b7heit", "frey", "zu", "sa\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "ADV", "VVPP", "$,", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Mich stets auch \u00fcberall mit L\u00e4sterworten schlagen,", "tokens": ["Mich", "stets", "auch", "\u00fc\u00b7be\u00b7rall", "mit", "L\u00e4s\u00b7ter\u00b7wor\u00b7ten", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "So ri\u00dfe mir vielleicht ihr grob- und b\u00f6ser Sinn", "tokens": ["So", "ri\u00b7\u00dfe", "mir", "viel\u00b7leicht", "ihr", "grob", "und", "b\u00f6\u00b7ser", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "TRUNC", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die Fr\u00fcchte deiner Gunst noch in den Bl\u00fcthen hin", "tokens": ["Die", "Fr\u00fcch\u00b7te", "dei\u00b7ner", "Gunst", "noch", "in", "den", "Bl\u00fc\u00b7then", "hin"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und d\u00fcrfte mir, an statt dein Herze zuzuneigen,", "tokens": ["Und", "d\u00fcrf\u00b7te", "mir", ",", "an", "statt", "dein", "Her\u00b7ze", "zu\u00b7zu\u00b7nei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "$,", "APPR", "APPR", "PPOSAT", "NN", "VVIZU", "$,"], "meter": "-+--++-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Den Eckel gegen mich in deiner Seele zeugen.", "tokens": ["Den", "E\u00b7ckel", "ge\u00b7gen", "mich", "in", "dei\u00b7ner", "See\u00b7le", "zeu\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ich weis wohl, da\u00df dein Geist, du halb verwirrtes Kind,", "tokens": ["Ich", "weis", "wohl", ",", "da\u00df", "dein", "Geist", ",", "du", "halb", "ver\u00b7wirr\u00b7tes", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "ADV", "$,", "KOUS", "PPOSAT", "NN", "$,", "PPER", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Fast all' und jedes Mahl den besten Grund ersinnt,", "tokens": ["Fast", "all'", "und", "je\u00b7des", "Mahl", "den", "bes\u00b7ten", "Grund", "er\u00b7sinnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "KON", "PIAT", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Wenn du beschuldigt wirst, die Ohnmacht vorzusch\u00fczen;", "tokens": ["Wenn", "du", "be\u00b7schul\u00b7digt", "wirst", ",", "die", "Ohn\u00b7macht", "vor\u00b7zu\u00b7sch\u00fc\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Ich stell es auch dahin, doch ob es dir auch n\u00fczen", "tokens": ["Ich", "stell", "es", "auch", "da\u00b7hin", ",", "doch", "ob", "es", "dir", "auch", "n\u00fc\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PAV", "$,", "ADV", "KOUS", "PPER", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Und immer gelten wird, das lehrt einmahl die Zeit.", "tokens": ["Und", "im\u00b7mer", "gel\u00b7ten", "wird", ",", "das", "lehrt", "ein\u00b7mahl", "die", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVINF", "VAFIN", "$,", "PDS", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Ich kan zum wenigsten mit meiner Redligkeit,", "tokens": ["Ich", "kan", "zum", "we\u00b7nigs\u00b7ten", "mit", "mei\u00b7ner", "Red\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "PIS", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.31": {"text": "Die du so schlecht erkennst, dem Himmel wohlgefallen,", "tokens": ["Die", "du", "so", "schlecht", "er\u00b7kennst", ",", "dem", "Him\u00b7mel", "wohl\u00b7ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVFIN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Der meine Fehler tr\u00e4gt und mir auch noch bey allen,", "tokens": ["Der", "mei\u00b7ne", "Feh\u00b7ler", "tr\u00e4gt", "und", "mir", "auch", "noch", "bey", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "KON", "PPER", "ADV", "ADV", "APPR", "PIS", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Die Kunst und Ehrligkeit und Wi\u00dfenschaft erg\u00f6zt,", "tokens": ["Die", "Kunst", "und", "Ehr\u00b7lig\u00b7keit", "und", "Wi\u00b7\u00dfen\u00b7schaft", "er\u00b7g\u00f6zt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Den angenehmsten Lohn der besten Freundschaft sezt,", "tokens": ["Den", "an\u00b7ge\u00b7nehms\u00b7ten", "Lohn", "der", "bes\u00b7ten", "Freund\u00b7schaft", "sezt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Und wird dich einer so wie G\u00fcnther lieben k\u00f6nnen,", "tokens": ["Und", "wird", "dich", "ei\u00b7ner", "so", "wie", "G\u00fcn\u00b7ther", "lie\u00b7ben", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PRF", "ART", "ADV", "KOKOM", "NE", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "So will ich deiner Gunst mich gleich nicht w\u00fcrdig nennen.", "tokens": ["So", "will", "ich", "dei\u00b7ner", "Gunst", "mich", "gleich", "nicht", "w\u00fcr\u00b7dig", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "PPER", "ADV", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich weis nicht, was dir ahnt, du kalt- und loses Kind,", "tokens": ["Ich", "weis", "nicht", ",", "was", "dir", "ahnt", ",", "du", "kal\u00b7t", "und", "lo\u00b7ses", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "PTKNEG", "$,", "PWS", "PPER", "VVFIN", "$,", "PPER", "TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "-+---+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da\u00df meine Lieb und Treu so gar nicht f\u00e4hig sind,", "tokens": ["Da\u00df", "mei\u00b7ne", "Lieb", "und", "Treu", "so", "gar", "nicht", "f\u00e4\u00b7hig", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "KON", "NN", "ADV", "ADV", "PTKNEG", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die G\u00fcte deiner Brust auch in geringen Sachen", "tokens": ["Die", "G\u00fc\u00b7te", "dei\u00b7ner", "Brust", "auch", "in", "ge\u00b7rin\u00b7gen", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mir zur Erg\u00f6zligkeit recht ofenbahr zu machen.", "tokens": ["Mir", "zur", "Er\u00b7g\u00f6z\u00b7lig\u00b7keit", "recht", "o\u00b7fen\u00b7bahr", "zu", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", 