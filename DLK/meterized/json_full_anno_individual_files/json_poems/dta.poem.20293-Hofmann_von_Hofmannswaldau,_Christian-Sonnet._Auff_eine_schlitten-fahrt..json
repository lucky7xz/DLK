{"dta.poem.20293": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet.  \n Auff eine schlitten-fahrt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ist das nicht Flavia/ die sich bey tr\u00fcber nacht", "tokens": ["Ist", "das", "nicht", "Fla\u00b7via", "/", "die", "sich", "bey", "tr\u00fc\u00b7ber", "nacht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PDS", "PTKNEG", "NE", "$(", "PRELS", "PRF", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "L\u00e4st in der rauhen lufft und auff dem schlitten f\u00fchren?", "tokens": ["L\u00e4st", "in", "der", "rau\u00b7hen", "lufft", "und", "auff", "dem", "schlit\u00b7ten", "f\u00fch\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "KON", "APPR", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Will sie den weissen schnee mit ihren rosen zieren?", "tokens": ["Will", "sie", "den", "weis\u00b7sen", "schnee", "mit", "ih\u00b7ren", "ro\u00b7sen", "zie\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und wird zu winters-zeit der lentz herf\u00fcr gebracht?", "tokens": ["Und", "wird", "zu", "win\u00b7ter\u00b7szeit", "der", "lentz", "her\u00b7f\u00fcr", "ge\u00b7bracht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "NN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie ists/ ich kenne sie aus ihrer augen pracht/", "tokens": ["Sie", "ists", "/", "ich", "ken\u00b7ne", "sie", "aus", "ih\u00b7rer", "au\u00b7gen", "pracht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die strahlen lassen sich als neue sterne sp\u00fcren/", "tokens": ["Die", "strah\u00b7len", "las\u00b7sen", "sich", "als", "neu\u00b7e", "ster\u00b7ne", "sp\u00fc\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "KOUS", "ADJA", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und was mir stets mit recht zu loben will geb\u00fchren/", "tokens": ["Und", "was", "mir", "stets", "mit", "recht", "zu", "lo\u00b7ben", "will", "ge\u00b7b\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "APPR", "ADJD", "PTKZU", "VVINF", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Hat meine feder stumpff/ und mich itzt stumm gemacht.", "tokens": ["Hat", "mei\u00b7ne", "fe\u00b7der", "stumpff", "/", "und", "mich", "itzt", "stumm", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVFIN", "$(", "KON", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wird aber auch der schnee vor deinen augen fliessen?", "tokens": ["Wird", "a\u00b7ber", "auch", "der", "schnee", "vor", "dei\u00b7nen", "au\u00b7gen", "flies\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Den augen/ welchen geist und hertzen schmeltzen m\u00fcssen/", "tokens": ["Den", "au\u00b7gen", "/", "wel\u00b7chen", "geist", "und", "hert\u00b7zen", "schmelt\u00b7zen", "m\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PWAT", "NN", "KON", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "F\u00fcr denen ei\u00df zergeht/ und eisen selbst zerbricht?", "tokens": ["F\u00fcr", "de\u00b7nen", "ei\u00df", "zer\u00b7geht", "/", "und", "ei\u00b7sen", "selbst", "zer\u00b7bricht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADJD", "VVFIN", "$(", "KON", "VVFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Nein. Ist der dr\u00fcste schnee so lange liegen blieben/", "tokens": ["Nein", ".", "Ist", "der", "dr\u00fcs\u00b7te", "schnee", "so", "lan\u00b7ge", "lie\u00b7gen", "blie\u00b7ben", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "VAFIN", "ART", "ADJA", "NN", "ADV", "ADV", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und hat den weissen glantz dein auge nicht vertrieben/", "tokens": ["Und", "hat", "den", "weis\u00b7sen", "glantz", "dein", "au\u00b7ge", "nicht", "ver\u00b7trie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "PPOSAT", "NN", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So schmeltzet es gewi\u00df den schnee der strasse nicht.", "tokens": ["So", "schmelt\u00b7zet", "es", "ge\u00b7wi\u00df", "den", "schnee", "der", "stras\u00b7se", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "ART", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}