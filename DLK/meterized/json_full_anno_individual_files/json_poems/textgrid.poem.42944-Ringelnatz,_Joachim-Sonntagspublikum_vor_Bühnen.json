{"textgrid.poem.42944": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Sonntagspublikum vor B\u00fchnen", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sonntagskinder sind Arbeitsfreie,", "tokens": ["Sonn\u00b7tags\u00b7kin\u00b7der", "sind", "Ar\u00b7beits\u00b7frei\u00b7e", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Ungew\u00f6hnte. \u2013 Der K\u00fcnstler verzeihe", "tokens": ["Un\u00b7ge\u00b7w\u00f6hn\u00b7te", ".", "\u2013", "Der", "K\u00fcnst\u00b7ler", "ver\u00b7zei\u00b7he"], "token_info": ["word", "punct", "punct", "word", "word", "word"], "pos": ["NN", "$.", "$(", "ART", "NN", "VVFIN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Ihnen ihr fremdes Geschau.", "tokens": ["Ih\u00b7nen", "ihr", "frem\u00b7des", "Ge\u00b7schau", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Sonntagskinder sind pl\u00f6tzliche F\u00fcrsten,", "tokens": ["Sonn\u00b7tags\u00b7kin\u00b7der", "sind", "pl\u00f6tz\u00b7li\u00b7che", "F\u00fcrs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Gl\u00fccklich an Sonne, D\u00fcnnbier und W\u00fcrsten.", "tokens": ["Gl\u00fcck\u00b7lich", "an", "Son\u00b7ne", ",", "D\u00fcnn\u00b7bier", "und", "W\u00fcrs\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "li