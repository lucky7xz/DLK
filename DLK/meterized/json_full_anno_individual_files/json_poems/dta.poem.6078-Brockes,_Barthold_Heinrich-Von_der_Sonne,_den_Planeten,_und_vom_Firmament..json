{"dta.poem.6078": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Sonne, den Planeten,  \n und vom Firmament.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wovon jedoch auch viele meinen,", "tokens": ["Wo\u00b7von", "je\u00b7doch", "auch", "vie\u00b7le", "mei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "PIS", "VVFIN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Da\u00df, zu gewisser Zeit, sie uns erscheinen.", "tokens": ["Da\u00df", ",", "zu", "ge\u00b7wis\u00b7ser", "Zeit", ",", "sie", "uns", "er\u00b7schei\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "APPR", "ADJA", "NN", "$,", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Jhr langes Wegseyn k\u00e4m\u2019 aus der Entfernung her,", "tokens": ["Ihr", "lan\u00b7ges", "Weg\u00b7seyn", "k\u00e4m'", "aus", "der", "Ent\u00b7fer\u00b7nung", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und wenn sie ihren Lauff, der unbekannt ist, enden,", "tokens": ["Und", "wenn", "sie", "ih\u00b7ren", "Lauff", ",", "der", "un\u00b7be\u00b7kannt", "ist", ",", "en\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN", "$,", "PRELS", "ADJD", "VAFIN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So s\u00e4h\u2019 man sie sich wieder zu uns wenden,", "tokens": ["So", "s\u00e4h'", "man", "sie", "sich", "wie\u00b7der", "zu", "uns", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "PRF", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und wundert sich so dann ein jeder sehr.", "tokens": ["Und", "wun\u00b7dert", "sich", "so", "dann", "ein", "je\u00b7der", "sehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "ADV", "ART", "PIAT", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}