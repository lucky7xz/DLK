{"textgrid.poem.54164": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Recht mu\u00df Recht bleiben \u2013!", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwir k\u00f6nnen nicht zahlen! Wir werden nichts zahlen!", "tokens": ["\u00bb", "wir", "k\u00f6n\u00b7nen", "nicht", "zah\u00b7len", "!", "Wir", "wer\u00b7den", "nichts", "zah\u00b7len", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "PTKNEG", "VVINF", "$.", "PPER", "VAFIN", "PIS", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die Gl\u00e4ubiger sollen uns was malen!\u00ab", "tokens": ["Die", "Gl\u00e4u\u00b7bi\u00b7ger", "sol\u00b7len", "uns", "was", "ma\u00b7len", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PIS", "VVINF", "$.", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Das geht gegen Welschland und gegen New York.", "tokens": ["Das", "geht", "ge\u00b7gen", "Wel\u00b7schland", "und", "ge\u00b7gen", "New", "Y\u00b7ork", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "KON", "APPR", "NE", "NE", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Vertr\u00e4ge nehmen wir leicht wie Kork.", "tokens": ["Ver\u00b7tr\u00e4\u00b7ge", "neh\u00b7men", "wir", "leicht", "wie", "Kork", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "KOKOM", "NE", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Nur nicht gegen die, die uns beherrschten:", "tokens": ["Nur", "nicht", "ge\u00b7gen", "die", ",", "die", "uns", "be\u00b7herrschten", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "APPR", "ART", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Wie steht denn die Sache mit unsern Ferschten \u2013?", "tokens": ["Wie", "steht", "denn", "die", "Sa\u00b7che", "mit", "un\u00b7sern", "Ferschten", "\u2013", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$(", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Sagt da einer: Gro\u00df ist die deutsche Not?", "tokens": ["Sagt", "da", "ei\u00b7ner", ":", "Gro\u00df", "ist", "die", "deut\u00b7sche", "Not", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIS", "$.", "ADJD", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sagt da einer: Sparen hei\u00dft das Gebot?", "tokens": ["Sagt", "da", "ei\u00b7ner", ":", "Spa\u00b7ren", "hei\u00dft", "das", "Ge\u00b7bot", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIS", "$.", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Ruft da der Nazi: Tyrannei?", "tokens": ["Ruft", "da", "der", "Na\u00b7zi", ":", "Ty\u00b7ran\u00b7nei", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NE", "$.", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Rundfunkt da der Groener: Ein Volk sei frei?", "tokens": ["Rund\u00b7funkt", "da", "der", "Gro\u00b7e\u00b7ner", ":", "Ein", "Volk", "sei", "frei", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$.", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "+---++--+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Stehn da die B\u00fcrger auf wie ein Mann,", "tokens": ["Stehn", "da", "die", "B\u00fcr\u00b7ger", "auf", "wie", "ein", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKVZ", "KOKOM", "ART", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "weil keiner zahlen will und kann?", "tokens": ["weil", "kei\u00b7ner", "zah\u00b7len", "will", "und", "kann", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVINF", "VMFIN", "KON", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Kriegen die F\u00fcrsten, was andre suchen?", "tokens": ["Krie\u00b7gen", "die", "F\u00fcrs\u00b7ten", ",", "was", "and\u00b7re", "su\u00b7chen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "PIS", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.8": {"text": "Brot \u2013? Ja, Kuchen.", "tokens": ["Brot", "\u2013", "?", "Ja", ",", "Ku\u00b7chen", "."], "token_info": ["word", "punct", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$(", "$.", "PTKANT", "$,", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Die bekommen Millionen und Millionen.", "tokens": ["Die", "be\u00b7kom\u00b7men", "Mil\u00b7lion\u00b7en", "und", "Mil\u00b7lion\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+---+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Die d\u00fcrfen in weiten Schl\u00f6ssern wohnen.", "tokens": ["Die", "d\u00fcr\u00b7fen", "in", "wei\u00b7ten", "Schl\u00f6s\u00b7sern", "woh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die kassieren f\u00fcr Kind und Kindeskind,", "tokens": ["Die", "kas\u00b7sie\u00b7ren", "f\u00fcr", "Kind", "und", "Kin\u00b7des\u00b7kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "weil wir brave Untertanen sind.", "tokens": ["weil", "wir", "bra\u00b7ve", "Un\u00b7ter\u00b7ta\u00b7nen", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Der in Doorn, der den Ha\u00df einer Welt gesammelt,", "tokens": ["Der", "in", "Do\u00b7orn", ",", "der", "den", "Ha\u00df", "ei\u00b7ner", "Welt", "ge\u00b7sam\u00b7melt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "$,", "PRELS", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "der hat noch nie so viel Geld gesammelt.", "tokens": ["der", "hat", "noch", "nie", "so", "viel", "Geld", "ge\u00b7sam\u00b7melt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "ADV", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Die Burschen k\u00f6nnen in Dollars baden,", "tokens": ["Die", "Bur\u00b7schen", "k\u00f6n\u00b7nen", "in", "Dol\u00b7lars", "ba\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "ihre Konten sind von Gottes Gnaden.", "tokens": ["ih\u00b7re", "Kon\u00b7ten", "sind", "von", "Got\u00b7tes", "Gna\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Wirft die einer zum Tempel hinaus?", "tokens": ["Wirft", "die", "ei\u00b7ner", "zum", "Tem\u00b7pel", "hin\u00b7aus", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ART", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "So sehn wir aus.", "tokens": ["So", "sehn", "wir", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Kein Geld f\u00fcr Kr\u00fcppel. Kein Geld f\u00fcr Proleten.", "tokens": ["Kein", "Geld", "f\u00fcr", "Kr\u00fcp\u00b7pel", ".", "Kein", "Geld", "f\u00fcr", "Pro\u00b7le\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "$.", "PIAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kein Geld f\u00fcr die, die der Krieg zertreten.", "tokens": ["Kein", "Geld", "f\u00fcr", "die", ",", "die", "der", "Krieg", "zer\u00b7tre\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "$,", "PRELS", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der Wind pfeift durch den Hosenri\u00df.", "tokens": ["Der", "Wind", "pfeift", "durch", "den", "Ho\u00b7sen\u00b7ri\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Dank des Vaterlands ist euch gewi\u00df.", "tokens": ["Der", "Dank", "des", "Va\u00b7ter\u00b7lands", "ist", "euch", "ge\u00b7wi\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Die leiden. Die hungern. Und die d\u00fcrsten.", "tokens": ["Die", "lei\u00b7den", ".", "Die", "hun\u00b7gern", ".", "Und", "die", "d\u00fcrs\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "VVINF", "$.", "ART", "NN", "$.", "KON", "ART", "ADJA", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Aber immer feste f\u00fcr die F\u00fcrsten!", "tokens": ["A\u00b7ber", "im\u00b7mer", "fes\u00b7te", "f\u00fcr", "die", "F\u00fcrs\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Sie zapfen an deutschem Gut und Blute.", "tokens": ["Sie", "zap\u00b7fen", "an", "deut\u00b7schem", "Gut", "und", "Blu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da hei\u00dft es nicht: Tribute! Tribute!", "tokens": ["Da", "hei\u00dft", "es", "nicht", ":", "Tri\u00b7bu\u00b7te", "!", "Tri\u00b7bu\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "$.", "NN", "$.", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da zahlt der Deutsche, getreu seinem Eid,", "tokens": ["Da", "zahlt", "der", "Deut\u00b7sche", ",", "ge\u00b7treu", "sei\u00b7nem", "Eid", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "an die gottgewollte Obrigkeit.", "tokens": ["an", "die", "gott\u00b7ge\u00b7woll\u00b7te", "Ob\u00b7rig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Aber der kleine Mann, der in Land und Stadt", "tokens": ["A\u00b7ber", "der", "klei\u00b7ne", "Mann", ",", "der", "in", "Land", "und", "Stadt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "NN", "KON", "NN"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "seine Kriegsanleihe gezeichnet hat,", "tokens": ["sei\u00b7ne", "Kriegs\u00b7an\u00b7lei\u00b7he", "ge\u00b7zeich\u00b7net", "hat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "VAFIN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "der kann sich sein Geld in den Schornstein schreiben.", "tokens": ["der", "kann", "sich", "sein", "Geld", "in", "den", "Schorn\u00b7stein", "schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PRF", "PPOSAT", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+