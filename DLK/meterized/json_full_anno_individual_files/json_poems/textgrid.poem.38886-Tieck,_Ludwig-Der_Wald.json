{"textgrid.poem.38886": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der Wald", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der frische Morgenwind", "tokens": ["Der", "fri\u00b7sche", "Mor\u00b7gen\u00b7wind"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Durch unsre Zweige geht,", "tokens": ["Durch", "uns\u00b7re", "Zwei\u00b7ge", "geht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "R\u00fchrt jedes Blatt geschwind,", "tokens": ["R\u00fchrt", "je\u00b7des", "Blatt", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wenn er so wohlgemuth durch alle Aeste weht.", "tokens": ["Wenn", "er", "so", "wohl\u00b7ge\u00b7muth", "durch", "al\u00b7le", "A\u00b7es\u00b7te", "weht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.5": {"text": "R\u00fchr' dich, o Menschenkind,", "tokens": ["R\u00fchr'", "dich", ",", "o", "Men\u00b7schen\u00b7kind", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "FM", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Was soll die Bangigkeit!", "tokens": ["Was", "soll", "die", "Ban\u00b7gig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wirf ab dein kleines Leid,", "tokens": ["Wirf", "ab", "dein", "klei\u00b7nes", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Komm, komm in unsern Schatten gr\u00fcn,", "tokens": ["Komm", ",", "komm", "in", "un\u00b7sern", "Schat\u00b7ten", "gr\u00fcn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wirf alle Sorgen hin,", "tokens": ["Wirf", "al\u00b7le", "Sor\u00b7gen", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Erschlie\u00df dein Herz der Freudigkeit.", "tokens": ["Er\u00b7schlie\u00df", "dein", "Herz", "der", "Freu\u00b7dig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wir r\u00fchren mit Zweigen", "tokens": ["Wir", "r\u00fch\u00b7ren", "mit", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "In den Himmel hinein,", "tokens": ["In", "den", "Him\u00b7mel", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und sp\u00fcren so eigen", "tokens": ["Und", "sp\u00fc\u00b7ren", "so", "ei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADJD"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Den gl\u00e4nzenden Schein:", "tokens": ["Den", "gl\u00e4n\u00b7zen\u00b7den", "Schein", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Mit Fingern, mit Zweigen, mit Aesten,", "tokens": ["Mit", "Fin\u00b7gern", ",", "mit", "Zwei\u00b7gen", ",", "mit", "A\u00b7es\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+--+--+--", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Durchrauscht von spielenden Westen,", "tokens": ["Durc\u00b7hrauscht", "von", "spie\u00b7len\u00b7den", "Wes\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Durchsungen von V\u00f6gelein,", "tokens": ["Durch\u00b7sun\u00b7gen", "von", "V\u00f6\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Freun wir uns frisch bis in die Wurzeln hinein.", "tokens": ["Freun", "wir", "uns", "frisch", "bis", "in", "die", "Wur\u00b7zeln", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.9": {"text": "Wir rauschen, wir fl\u00fcstern, wir wogen,", "tokens": ["Wir", "rau\u00b7schen", ",", "wir", "fl\u00fcs\u00b7tern", ",", "wir", "wo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVINF", "$,", "PPER", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.10": {"text": "Geschirmt vom blauen Himmelsbogen,", "tokens": ["Ge\u00b7schirmt", "vom", "blau\u00b7en", "Him\u00b7mels\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Von freundlichen L\u00fcften durchzogen.", "tokens": ["Von", "freund\u00b7li\u00b7chen", "L\u00fcf\u00b7ten", "durch\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.12": {"text": "Fr\u00fchlingsglanz!", "tokens": ["Fr\u00fch\u00b7lings\u00b7glanz", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "Fr\u00fchlingsglanz!", "tokens": ["Fr\u00fch\u00b7lings\u00b7glanz", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.14": {"text": "Sey gegr\u00fc\u00dft, sey gegr\u00fc\u00dft von Abend zu Morgen,", "tokens": ["Sey", "ge\u00b7gr\u00fc\u00dft", ",", "sey", "ge\u00b7gr\u00fc\u00dft", "von", "A\u00b7bend", "zu", "Mor\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "VAFIN", "VVPP", "APPR", "NN", "APPR", "NN", "$,"], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.15": {"text": "Von Morgen zu Abend:", "tokens": ["Von", "Mor\u00b7gen", "zu", "A\u00b7bend", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.16": {"text": "Komm, Mensch, sei frei von Sorgen", "tokens": ["Komm", ",", "Mensch", ",", "sei", "frei", "von", "Sor\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "NN", "$,", "VAFIN", "ADJD", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "In unserm Schatten, der br\u00fcderlich labend. \u2013", "tokens": ["In", "un\u00b7serm", "Schat\u00b7ten", ",", "der", "br\u00fc\u00b7der\u00b7lich", "la\u00b7bend", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "ADJD", "VVPP", "$.", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Jeder sein eigen,", "tokens": ["Je\u00b7der", "sein", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "PPOSAT", "ADJD", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Birken, Tannen, Eichen,", "tokens": ["Bir\u00b7ken", ",", "Tan\u00b7nen", ",", "Ei\u00b7chen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Stehn wir durchsammen verwirrt,", "tokens": ["Stehn", "wir", "durch\u00b7sam\u00b7men", "ver\u00b7wirrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "ADJD", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Doch keiner den andern irrt;", "tokens": ["Doch", "kei\u00b7ner", "den", "an\u00b7dern", "irrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ART", "ADJA", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Der streckt die Zweig' in die Weite,", "tokens": ["Der", "streckt", "die", "Zweig'", "in", "die", "Wei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "R\u00fchrt schirmend das Gras mit der Hand,", "tokens": ["R\u00fchrt", "schir\u00b7mend", "das", "Gras", "mit", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Der steht zum Himmel gewandt,", "tokens": ["Der", "steht", "zum", "Him\u00b7mel", "ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "F\u00fchrt jeder ein Rauschen, sein eigen,", "tokens": ["F\u00fchrt", "je\u00b7der", "ein", "Rau\u00b7schen", ",", "sein", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "$,", "PPOSAT", "ADJD", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.9": {"text": "Und sch\u00fcttelt sich frisch in den Zweigen;", "tokens": ["Und", "sch\u00fct\u00b7telt", "sich", "frisch", "in", "den", "Zwei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.10": {"text": "Doch flie\u00dft der mannigfalt'ge Klang", "tokens": ["Doch", "flie\u00dft", "der", "man\u00b7nig\u00b7falt'\u00b7ge", "Klang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "In Einen br\u00fcderlichen Chorgesang.", "tokens": ["In", "Ei\u00b7nen", "br\u00fc\u00b7der\u00b7li\u00b7chen", "Chor\u00b7ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "So auch die Menschen mitsammen", "tokens": ["So", "auch", "die", "Men\u00b7schen", "mit\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "Die verschieden von Einem nur stammen,", "tokens": ["Die", "ver\u00b7schie\u00b7den", "von", "Ei\u00b7nem", "nur", "stam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "PIS", "ADV", "VVFIN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.14": {"text": "Jeder r\u00fchrt sich in seinen Zweigen,", "tokens": ["Je\u00b7der", "r\u00fchrt", "sich", "in", "sei\u00b7nen", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.15": {"text": "Doch alle streben zum Licht zu steigen,", "tokens": ["Doch", "al\u00b7le", "stre\u00b7ben", "zum", "Licht", "zu", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Wenn sich auch viele gegen die Erde neigen,", "tokens": ["Wenn", "sich", "auch", "vie\u00b7le", "ge\u00b7gen", "die", "Er\u00b7de", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADV", "PIS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Sie alle Br\u00fcder sein,", "tokens": ["Sie", "al\u00b7le", "Br\u00fc\u00b7der", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "Verschiede