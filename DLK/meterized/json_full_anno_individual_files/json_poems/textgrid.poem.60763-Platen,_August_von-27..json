{"textgrid.poem.60763": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "27.", "genre": "verse", "period": "N.A.", "pub_year": 1832, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dich erfleht das Land als Segen,", "tokens": ["Dich", "er\u00b7fleht", "das", "Land", "als", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KOUS", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schn\u00f6der, unwillkommner Regen!", "tokens": ["Schn\u00f6\u00b7der", ",", "un\u00b7will\u00b7komm\u00b7ner", "Re\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mich nur st\u00f6rst du sehr auf meinen", "tokens": ["Mich", "nur", "st\u00f6rst", "du", "sehr", "auf", "mei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Abendlichen Liebeswegen.", "tokens": ["A\u00b7bend\u00b7li\u00b7chen", "Lie\u00b7bes\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.5": {"text": "Nach der Feder mu\u00df ich greifen,", "tokens": ["Nach", "der", "Fe\u00b7der", "mu\u00df", "ich", "grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie ein Held nach seinem Degen,", "tokens": ["Wie", "ein", "Held", "nach", "sei\u00b7nem", "De\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Weil die Helden wie die Dichter", "tokens": ["Weil", "die", "Hel\u00b7den", "wie", "die", "Dich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "KOKOM", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Langeweile macht verlegen;", "tokens": ["Lan\u00b7ge\u00b7wei\u00b7le", "macht", "ver\u00b7le\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Eitle Reime mu\u00df ich schmieden,", "tokens": ["Eit\u00b7le", "Rei\u00b7me", "mu\u00df", "ich", "schmie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Statt der Liebe Gunst zu pflegen:", "tokens": ["Statt", "der", "Lie\u00b7be", "Gunst", "zu", "pfle\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Sonst erheitert kein Gesch\u00e4ft mich", "tokens": ["Sonst", "er\u00b7hei\u00b7tert", "kein", "Ge\u00b7sch\u00e4ft", "mich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Meiner tiefen Wunde wegen.", "tokens": ["Mei\u00b7ner", "tie\u00b7fen", "Wun\u00b7de", "we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dich erfleht das Land als Segen,", "tokens": ["Dich", "e