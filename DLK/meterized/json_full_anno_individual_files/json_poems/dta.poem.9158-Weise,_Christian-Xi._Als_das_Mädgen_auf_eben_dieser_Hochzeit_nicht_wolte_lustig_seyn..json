{"dta.poem.9158": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Xi.  \n Als das M\u00e4dgen auf eben dieser Hochzeit  \n nicht wolte lustig seyn.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Hab ich mich nicht gespitzt", "tokens": ["Hab", "ich", "mich", "nicht", "ge\u00b7spitzt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PRF", "PTKNEG", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Auf dieses Hochzeit-spiel/", "tokens": ["Auf", "die\u00b7ses", "Hoch\u00b7zeit\u00b7spiel", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und gleichwol da ich itzt", "tokens": ["Und", "gleich\u00b7wol", "da", "ich", "itzt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "KOUS", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die lust betrachten wil/", "tokens": ["Die", "lust", "be\u00b7trach\u00b7ten", "wil", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So ist sie warlich schlecht/", "tokens": ["So", "ist", "sie", "war\u00b7lich", "schlecht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Das m\u00e4dgen wil nicht l\u00e4chen/", "tokens": ["Das", "m\u00e4d\u00b7gen", "wil", "nicht", "l\u00e4\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVINF", "VMFIN", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und mir ist auch nicht recht/", "tokens": ["Und", "mir", "ist", "auch", "nicht", "recht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "PTKNEG", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "So stehen meine sachen.", "tokens": ["So", "ste\u00b7hen", "mei\u00b7ne", "sa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "2. Ich h\u00e4tte mir den spa\u00df", "tokens": ["Ich", "h\u00e4t\u00b7te", "mir", "den", "spa\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Viel besser eingebildt/", "tokens": ["Viel", "bes\u00b7ser", "ein\u00b7ge\u00b7bildt", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch nun wird di\u00df und das", "tokens": ["Doch", "nun", "wird", "di\u00df", "und", "das"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PDS", "KON", "ART"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die helffte kaum erf\u00fcllt/", "tokens": ["Die", "helff\u00b7te", "kaum", "er\u00b7f\u00fcllt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich war von hertzen froh/", "tokens": ["Ich", "war", "von", "hert\u00b7zen", "froh", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Dann schickt ich mich auf minen/", "tokens": ["Dann", "schickt", "ich", "mich", "auf", "mi\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ADJA", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da wolt ich so und so/", "tokens": ["Da", "wolt", "ich", "so", "und", "so", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "KON", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Das lose kind bedienen.", "tokens": ["Das", "lo\u00b7se", "kind", "be\u00b7die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "3. Es hat sich wol bedient/", "tokens": ["Es", "hat", "sich", "wol", "be\u00b7dient", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "ADV", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wann sie aus lauter list", "tokens": ["Wann", "sie", "aus", "lau\u00b7ter", "list"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sich nicht so viel erk\u00fchnt", "tokens": ["Sich", "nicht", "so", "viel", "er\u00b7k\u00fchnt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "PTKNEG", "ADV", "ADV", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und selber lustig ist;", "tokens": ["Und", "sel\u00b7ber", "lus\u00b7tig", "ist", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wann sie den sch\u00f6nen rest", "tokens": ["Wann", "sie", "den", "sch\u00f6\u00b7nen", "rest"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der zeit nur wil verliehren/", "tokens": ["Der", "zeit", "nur", "wil", "ver\u00b7lieh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und den Saturnus l\u00e4st", "tokens": ["Und", "den", "Sa\u00b7tur\u00b7nus", "l\u00e4st"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.8": {"text": "In ihren sinn regieren.", "tokens": ["In", "ih\u00b7ren", "sinn", "re\u00b7gie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "4. Ich bin zu wunderlich", "tokens": ["Ich", "bin", "zu", "wun\u00b7der\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und werde flugs betr\u00fcbt/", "tokens": ["Und", "wer\u00b7de", "flugs", "be\u00b7tr\u00fcbt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wann nur ein m\u00e4dgen sich", "tokens": ["Wann", "nur", "ein", "m\u00e4d\u00b7gen", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "ADJA", "PRF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ein wenig theuer giebt:", "tokens": ["Ein", "we\u00b7nig", "theu\u00b7er", "giebt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es darff ein eintzig blick", "tokens": ["Es", "darff", "ein", "eint\u00b7zig", "blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "ADJD", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Sich wider mich erregen/", "tokens": ["Sich", "wi\u00b7der", "mich", "er\u00b7re\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "So prall ich schon zur\u00fcck/", "tokens": ["So", "prall", "ich", "schon", "zu\u00b7r\u00fcck", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PPER", "ADV", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und ist mir alls entgegen.", "tokens": ["Und", "ist", "mir", "alls", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "5. Was hilffts/ ich wil hinfort", "tokens": ["Was", "hilffts", "/", "ich", "wil", "hin\u00b7fort"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$(", "PPER", "VMFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ein bi\u00dfgen kl\u00fcger seyn/", "tokens": ["Ein", "bi\u00df\u00b7gen", "kl\u00fc\u00b7ger", "seyn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VAINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und mich auff keinen ort", "tokens": ["Und", "mich", "auff", "kei\u00b7nen", "ort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So \u00fcberm\u00e4ssig freu\u2019n:", "tokens": ["So", "\u00fc\u00b7berm\u00b7\u00e4s\u00b7sig", "freu'n", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es ist doch mein gebrauch/", "tokens": ["Es", "ist", "doch", "mein", "ge\u00b7brauch", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Je \u00e4rger ich mich spitze/", "tokens": ["Je", "\u00e4r\u00b7ger", "ich", "mich", "spit\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PRF", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Je weniger wird mir auch", "tokens": ["Je", "we\u00b7ni\u00b7ger", "wird", "mir", "auch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Der freude selber n\u00fctze.", "tokens": ["Der", "freu\u00b7de", "sel\u00b7ber", "n\u00fct\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "6. Hingegen so beh\u00e4lt", "tokens": ["Hin\u00b7ge\u00b7gen", "so", "be\u00b7h\u00e4lt"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dieselbe lust den prei\u00df/", "tokens": ["Die\u00b7sel\u00b7be", "lust", "den", "prei\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn da\u00df man auff der welt", "tokens": ["Wenn", "da\u00df", "man", "auff", "der", "welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PIS", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Nichts von den h\u00e4ndeln wei\u00df", "tokens": ["Nichts", "von", "den", "h\u00e4n\u00b7deln", "wei\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Denn also k\u00f6mmt man offt", "tokens": ["Denn", "al\u00b7so", "k\u00f6mmt", "man", "offt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PIS", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Zu angenehmen leuten/", "tokens": ["Zu", "an\u00b7ge\u00b7neh\u00b7men", "leu\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die einem unverhofft", "tokens": ["Die", "ei\u00b7nem", "un\u00b7ver\u00b7hofft"], "token_info": ["word", "word", "word"], "pos": ["ART", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Den sch\u00f6nsten spa\u00df bereiten.", "tokens": ["Den", "sch\u00f6ns\u00b7ten", "spa\u00df", "be\u00b7rei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "7. Dem sey nun wie ihm sey/", "tokens": ["Dem", "sey", "nun", "wie", "ihm", "sey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "KOKOM", "PPER", "VAFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Jtzt bin ich wol vexirt/", "tokens": ["Jtzt", "bin", "ich", "wol", "ve\u00b7xirt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und kan doch nicht vorbey/", "tokens": ["Und", "kan", "doch", "nicht", "vor\u00b7bey", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "PTKNEG", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df so ein stern regiert/", "tokens": ["Da\u00df", "so", "ein", "stern", "re\u00b7giert", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ist mein gel\u00fccke gut/", "tokens": ["Ist", "mein", "ge\u00b7l\u00fc\u00b7cke", "gut", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "So will ichs bald erleben/", "tokens": ["So", "will", "ichs", "bald", "er\u00b7le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADV", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df mir das lose blut/", "tokens": ["Da\u00df", "mir", "das", "lo\u00b7se", "blut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Soll bessre blicke geben.", "tokens": ["Soll", "bess\u00b7re", "bli\u00b7cke", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}