{"dta.poem.20454": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Schwangerer Jungfern Trost-  \n Gedancken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "La\u00df/ grosse Venus/ dir ja nicht zu wider seyn/", "tokens": ["La\u00df", "/", "gros\u00b7se", "Ve\u00b7nus", "/", "dir", "ja", "nicht", "zu", "wi\u00b7der", "seyn", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$(", "ADJA", "NN", "$(", "PPER", "ADV", "PTKNEG", "PTKZU", "APPR", "PPOSAT", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df wir f\u00fcr dein altar mit schweren fusse treten!", "tokens": ["Da\u00df", "wir", "f\u00fcr", "dein", "al\u00b7tar", "mit", "schwe\u00b7ren", "fus\u00b7se", "tre\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wirff einen strahl auff uns von deiner gottheit schein/", "tokens": ["Wirff", "ei\u00b7nen", "strahl", "auff", "uns", "von", "dei\u00b7ner", "got\u00b7theit", "schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "APPR", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die wir in demuth itzt dich kommen anzubeten:", "tokens": ["Die", "wir", "in", "de\u00b7muth", "itzt", "dich", "kom\u00b7men", "an\u00b7zu\u00b7be\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PRELS", "ADV", "PPER", "VVFIN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nimm unsern ehren-krantz zu einem opffer an/", "tokens": ["Nimm", "un\u00b7sern", "eh\u00b7ren\u00b7krantz", "zu", "ei\u00b7nem", "opf\u00b7fer", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "APPR", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "La\u00df dieses trauer-pfand an deinen w\u00e4nden hangen/", "tokens": ["La\u00df", "die\u00b7ses", "trau\u00b7e\u00b7rpfand", "an", "dei\u00b7nen", "w\u00e4n\u00b7den", "han\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PDAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und so es ewig nicht erhalten werden kan/", "tokens": ["Und", "so", "es", "e\u00b7wig", "nicht", "er\u00b7hal\u00b7ten", "wer\u00b7den", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "ADJD", "PTKNEG", "VVPP", "VAINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So la\u00df die asche nur in deinem tempel prangen.", "tokens": ["So", "la\u00df", "die", "asc\u00b7he", "nur", "in", "dei\u00b7nem", "tem\u00b7pel", "pran\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ART", "ADJA", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Es r\u00fchme Pallas sich mit ihrer jungferschafft/", "tokens": ["Es", "r\u00fch\u00b7me", "Pal\u00b7las", "sich", "mit", "ih\u00b7rer", "jung\u00b7fer\u00b7schafft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "PRF", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So mag auch Vesta sich f\u00fcr allen m\u00e4nnern wehren/", "tokens": ["So", "mag", "auch", "Ves\u00b7ta", "sich", "f\u00fcr", "al\u00b7len", "m\u00e4n\u00b7nern", "weh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "NE", "PRF", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Diana f\u00fchle nicht der starcken liebe krafft;", "tokens": ["Di\u00b7a\u00b7na", "f\u00fch\u00b7le", "nicht", "der", "star\u00b7cken", "lie\u00b7be", "krafft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wir wollen insgesammt zu deiner fahne schwehren.", "tokens": ["Wir", "wol\u00b7len", "ins\u00b7ge\u00b7sammt", "zu", "dei\u00b7ner", "fah\u00b7ne", "schweh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wir bitten jenen auch mit ihrem wesen trutz/", "tokens": ["Wir", "bit\u00b7ten", "je\u00b7nen", "auch", "mit", "ih\u00b7rem", "we\u00b7sen", "trutz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und wollen uns die zunfft der schwangern jungfern nennen.", "tokens": ["Und", "wol\u00b7len", "uns", "die", "zunfft", "der", "schwan\u00b7gern", "jung\u00b7fern", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ART", "NN", "ART", "ADJA", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Nimst du uns willig auff in deinen schirm und schutz/", "tokens": ["Nimst", "du", "uns", "wil\u00b7lig", "auff", "in", "dei\u00b7nen", "schirm", "und", "schutz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "APPR", "APPR", "PPOSAT", "NN", "KON", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So sucht das volck umsonst uns flecken anzubrennen.", "tokens": ["So", "sucht", "das", "volck", "um\u00b7sonst", "uns", "fle\u00b7cken", "an\u00b7zu\u00b7bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVFIN", "PPER", "VVINF", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wir sch\u00e4tzen den verlust der jungferschafft nicht gro\u00df/", "tokens": ["Wir", "sch\u00e4t\u00b7zen", "den", "ver\u00b7lust", "der", "jung\u00b7fer\u00b7schafft", "nicht", "gro\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "VVFIN", "ART", "NN", "PTKNEG", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und f\u00fchlen immer noch das angenehme jucken/", "tokens": ["Und", "f\u00fch\u00b7len", "im\u00b7mer", "noch", "das", "an\u00b7ge\u00b7neh\u00b7me", "ju\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Als der beperlte thau in unsre muschel flo\u00df/", "tokens": ["Als", "der", "be\u00b7perl\u00b7te", "thau", "in", "uns\u00b7re", "mu\u00b7schel", "flo\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und die sich \u00f6ffnete denselben einzuschlucken.", "tokens": ["Und", "die", "sich", "\u00f6ff\u00b7ne\u00b7te", "den\u00b7sel\u00b7ben", "ein\u00b7zu\u00b7schlu\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PRF", "VVFIN", "PDS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Es war/ als h\u00e4tte sich uns Jupiter gezeigt/", "tokens": ["Es", "war", "/", "als", "h\u00e4t\u00b7te", "sich", "uns", "Ju\u00b7pi\u00b7ter", "ge\u00b7zeigt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "KOKOM", "VAFIN", "PRF", "PPER", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und wolte wiederum mit menschen liebe pflegen;", "tokens": ["Und", "wol\u00b7te", "wie\u00b7de\u00b7rum", "mit", "men\u00b7schen", "lie\u00b7be", "pfle\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "APPR", "ADJA", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Als h\u00e4tte sich zu uns der himmel selbst geneigt/", "tokens": ["Als", "h\u00e4t\u00b7te", "sich", "zu", "uns", "der", "him\u00b7mel", "selbst", "ge\u00b7neigt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PRF", "APPR", "PPER", "ART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und wolte sich hinfort auff unserm schoo\u00df bewegen.", "tokens": ["Und", "wol\u00b7te", "sich", "hin\u00b7fort", "auff", "un\u00b7serm", "schoo\u00df", "be\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "ADV", "APPR", "PPOSAT", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Die lenden huben sich/ da uns die lust empfieng/", "tokens": ["Die", "len\u00b7den", "hu\u00b7ben", "sich", "/", "da", "uns", "die", "lust", "emp\u00b7fi\u00b7eng", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$(", "KOUS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.26": {"text": "Als wenn der gantze leib gen himmel fliegen wolte/", "tokens": ["Als", "wenn", "der", "gant\u00b7ze", "leib", "gen", "him\u00b7mel", "flie\u00b7gen", "wol\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "ADJA", "NN", "APPR", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "So da\u00df die seele fast uns mit zugleich entgieng/", "tokens": ["So", "da\u00df", "die", "see\u00b7le", "fast", "uns", "mit", "zu\u00b7gleich", "ent\u00b7gieng", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "VVFIN", "ADV", "PPER", "APPR", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Indem die jungferschafft den abschied nehmen solte.", "tokens": ["In\u00b7dem", "die", "jung\u00b7fer\u00b7schafft", "den", "ab\u00b7schied", "neh\u00b7men", "sol\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Cupido hatte schon ein labsal zubereit/", "tokens": ["Cu\u00b7pi\u00b7do", "hat\u00b7te", "schon", "ein", "lab\u00b7sal", "zu\u00b7be\u00b7reit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Die geister wiederum vom schlaffe zuerwecken:", "tokens": ["Die", "geis\u00b7ter", "wie\u00b7de\u00b7rum", "vom", "schlaf\u00b7fe", "zu\u00b7er\u00b7we\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "APPRART", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Er kam uns h\u00f6chst-erw\u00fcnscht zu eben rechter zeit/", "tokens": ["Er", "kam", "uns", "h\u00f6chst\u00b7er\u00b7w\u00fcnscht", "zu", "e\u00b7ben", "rech\u00b7ter", "zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und lie\u00df uns Ambrosin aus rothen schaalen lecken.", "tokens": ["Und", "lie\u00df", "uns", "A\u00b7mbro\u00b7sin", "aus", "ro\u00b7then", "schaa\u00b7len", "le\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "APPR", "ADJA", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Drum achten wir nicht sehr der sp\u00f6tter grosse zahl/", "tokens": ["Drum", "ach\u00b7ten", "wir", "nicht", "sehr", "der", "sp\u00f6t\u00b7ter", "gros\u00b7se", "zahl", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PTKNEG", "ADV", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und lassen andere vor jungfern gerne lauffen/", "tokens": ["Und", "las\u00b7sen", "an\u00b7de\u00b7re", "vor", "jung\u00b7fern", "ger\u00b7ne", "lauf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "APPR", "ADJA", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Ja wolten unger\u00fchmt/ wo m\u00f6glich/ tausendmahl", "tokens": ["Ja", "wol\u00b7ten", "un\u00b7ge\u00b7r\u00fchmt", "/", "wo", "m\u00f6g\u00b7lich", "/", "tau\u00b7send\u00b7mahl"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PTKANT", "VMFIN", "ADJD", "$(", "PWAV", "ADJD", "$(", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Um einen schn\u00f6den krantz dergleichen wollust kauffen.", "tokens": ["Um", "ei\u00b7nen", "schn\u00f6\u00b7den", "krantz", "derg\u00b7lei\u00b7chen", "wol\u00b7lust", "kauf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "PIS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Wir fragen alle welt/ was ist der jungfer-stand?", "tokens": ["Wir", "fra\u00b7gen", "al\u00b7le", "welt", "/", "was", "ist", "der", "jung\u00b7fer\u00b7stand", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$(", "PWS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Was ist die jungferschafft? Ein buch/ so nicht zulesen/", "tokens": ["Was", "ist", "die", "jung\u00b7fer\u00b7schafft", "?", "Ein", "buch", "/", "so", "nicht", "zu\u00b7le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$.", "ART", "NN", "$(", "ADV", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Ein sch\u00fcler freyer kunst/ ein blosser w\u00f6rter-tand/", "tokens": ["Ein", "sch\u00fc\u00b7ler", "frey\u00b7er", "kunst", "/", "ein", "blos\u00b7ser", "w\u00f6r\u00b7ter\u00b7tand", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Ein kind der phantasie/ ein wesen ohne wesen.", "tokens": ["Ein", "kind", "der", "phan\u00b7ta\u00b7sie", "/", "ein", "we\u00b7sen", "oh\u00b7ne", "we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$(", "ART", "NN", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Es heget das gehirn und mehret diese zucht/", "tokens": ["Es", "he\u00b7get", "das", "ge\u00b7hirn", "und", "meh\u00b7ret", "die\u00b7se", "zucht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "PDS", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Ihr gantzes wesen st\u00fctzt der pfeiler der gedancken/", "tokens": ["Ihr", "gant\u00b7zes", "we\u00b7sen", "st\u00fctzt", "der", "pfei\u00b7ler", "der", "ge\u00b7dan\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Warum/ ist unbekandt. Gewi\u00df da\u00df ohne frucht", "tokens": ["Wa\u00b7rum", "/", "ist", "un\u00b7be\u00b7kandt", ".", "Ge\u00b7wi\u00df", "da\u00df", "oh\u00b7ne", "frucht"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "$(", "VAFIN", "ADJD", "$.", "ADV", "KOUS", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Man der natur hierdurch will schm\u00e4lern ihre schrancken.", "tokens": ["Man", "der", "na\u00b7tur", "hier\u00b7durch", "will", "schm\u00e4\u00b7lern", "ih\u00b7re", "schran\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "PAV", "VMFIN", "VVINF", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Doch wird durch diesen wahn ein grosser theil beth\u00f6rt/", "tokens": ["Doch", "wird", "durch", "die\u00b7sen", "wahn", "ein", "gros\u00b7ser", "theil", "be\u00b7th\u00f6rt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PDAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Und abgeschreckt von dem/ was die natur wohl g\u00f6nnte/", "tokens": ["Und", "ab\u00b7ge\u00b7schreckt", "von", "dem", "/", "was", "die", "na\u00b7tur", "wohl", "g\u00f6nn\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "APPR", "ART", "$(", "PWS", "ART", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Es w\u00fcrde gar um viel der menschen zahl vermehrt/", "tokens": ["Es", "w\u00fcr\u00b7de", "gar", "um", "viel", "der", "men\u00b7schen", "zahl", "ver\u00b7mehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ADV", "ART", "ADJA", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Wenn iede sonder schimpff nur mutter heissen k\u00f6nte.", "tokens": ["Wenn", "ie\u00b7de", "son\u00b7der", "schimpff", "nur", "mut\u00b7ter", "heis\u00b7sen", "k\u00f6n\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VVFIN", "ADV", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Es ist die jungferschafft/ wer sie zu etwas macht/", "tokens": ["Es", "ist", "die", "jung\u00b7fer\u00b7schafft", "/", "wer", "sie", "zu", "et\u00b7was", "macht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$(", "PWS", "PPER", "PTKA", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Ein unvollkommner stand/ gleich ungef\u00f6rmter erden/", "tokens": ["Ein", "un\u00b7voll\u00b7komm\u00b7ner", "stand", "/", "gleich", "un\u00b7ge\u00b7f\u00f6rm\u00b7ter", "er\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$(", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Der zur vollkommenheit nicht eher wird gebracht/", "tokens": ["Der", "zur", "voll\u00b7kom\u00b7men\u00b7heit", "nicht", "e\