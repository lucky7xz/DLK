{"textgrid.poem.41976": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Seidenes Bett, bald hegst du den reizenden Leib der Geliebten,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seidenes Bett, bald hegst du den reizenden Leib der Geliebten,", "tokens": ["Sei\u00b7de\u00b7nes", "Bett", ",", "bald", "hegst", "du", "den", "rei\u00b7zen\u00b7den", "Leib", "der", "Ge\u00b7lieb\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Ach, schon harret dein Schoo\u00df auf die begl\u00fcckende Last,", "tokens": ["Ach", ",", "schon", "har\u00b7ret", "dein", "Schoo\u00df", "auf", "die", "be\u00b7gl\u00fc\u00b7cken\u00b7de", "Last", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Ueppiger schwillst du empor, in den Flaum sank friedliche Ruhe,", "tokens": ["Uep\u00b7pi\u00b7ger", "schwillst", "du", "em\u00b7por", ",", "in", "den", "Flaum", "sank", "fried\u00b7li\u00b7che", "Ru\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$,", "APPR", "ART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Still durchs d\u00e4mmernde Zelt schl\u00fcpfen die Tr\u00e4ume dahin.", "tokens": ["Still", "durchs", "d\u00e4m\u00b7mern\u00b7de", "Zelt", "schl\u00fcp\u00b7fen", "die", "Tr\u00e4u\u00b7me", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "PAV", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Darf ich dir nahn, unheilig dem heiligen? Wandle mir, Sehnsucht,", "tokens": ["Darf", "ich", "dir", "nahn", ",", "un\u00b7hei\u00b7lig", "dem", "hei\u00b7li\u00b7gen", "?", "Wand\u00b7le", "mir", ",", "Sehn\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADJA", "$,", "ADJD", "ART", "ADJA", "$.", "VVFIN", "PPER", "$,", "NN", "$,"], "meter": "+--+-+--+--+-+++", "measure": "iambic.octa.plus.invert"}, "line.6": {"text": "Wandle zum rosigen Jetzt magisch die kommende Zeit.", "tokens": ["Wand\u00b7le", "zum", "ro\u00b7si\u00b7gen", "Jetzt", "ma\u00b7gisch", "die", "kom\u00b7men\u00b7de", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "ADV", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Hier wird leis' an die H\u00fclle geschmiegt aufathmen der Busen;", "tokens": ["Hier", "wird", "leis'", "an", "die", "H\u00fcl\u00b7le", "ge\u00b7schmiegt", "auf\u00b7ath\u00b7men", "der", "Bu\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "APPR", "ART", "NN", "VVPP", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Flattern in ihr nicht schon Funken der \u00fcppigen Gluth?", "tokens": ["Flat\u00b7tern", "in", "ihr", "nicht", "schon", "Fun\u00b7ken", "der", "\u00fcp\u00b7pi\u00b7gen", "Gluth", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "PTKNEG", "ADV", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+--++-+", "measure": "trochaic.septa.relaxed"}, "line.9": {"text": "H\u00f6r' ich nicht schon das \u00e4therische Wehn des bl\u00fchenden Mundes?", "tokens": ["H\u00f6r'", "ich", "nicht", "schon", "das", "\u00e4t\u00b7he\u00b7ri\u00b7sche", "Wehn", "des", "bl\u00fc\u00b7hen\u00b7den", "Mun\u00b7des", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "---+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Winkt durch die D\u00e4mmrung nicht scheu und err\u00f6thend ihr Bild?", "tokens": ["Winkt", "durch", "die", "D\u00e4mm\u00b7rung", "nicht", "scheu", "und", "er\u00b7r\u00f6\u00b7thend", "ihr", "Bild", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKNEG", "ADJD", "KON", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "K\u00fcssen will ich den Ort, wo dem Bett mit der Wange sie nahn wird,", "tokens": ["K\u00fcs\u00b7sen", "will", "ich", "den", "Ort", ",", "wo", "dem", "Bett", "mit", "der", "Wan\u00b7ge", "sie", "nahn", "wird", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ART", "NN", "$,", "PWAV", "ART", "NN", "APPR", "ART", "NN", "PPER", "ADJA", "VAFIN", "$,"], "meter": "+-+--+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.12": {"text": "Und im seligen Traum w\u00e4hnen, ich k\u00fcsse sie selbst,", "tokens": ["Und", "im", "se\u00b7li\u00b7gen", "Traum", "w\u00e4h\u00b7nen", ",", "ich", "k\u00fcs\u00b7se", "sie", "selbst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "VVINF", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.13": {"text": "Will fest schlingen den sehnenden Arm um's wallende Lager;", "tokens": ["Will", "fest", "schlin\u00b7gen", "den", "seh\u00b7nen\u00b7den", "Arm", "um's", "wal\u00b7len\u00b7de", "La\u00b7ger", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "VVFIN", "ART", "ADJA", "NN", "NE", "ADJA", "NN", "$."], "meter": "-++--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Bl\u00fchet das Gl\u00fcck doch allein Jenem, der gl\u00fccklich sich w\u00e4hnt.", "tokens": ["Bl\u00fc\u00b7het", "das", "Gl\u00fcck", "doch", "al\u00b7lein", "Je\u00b7nem", ",", "der", "gl\u00fcck\u00b7lich", "sich", "w\u00e4hnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADV", "PDAT", "$,", "PRELS", "ADJD", "PRF", "VVFIN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.15": {"text": "Also k\u00fc\u00dft der Verlobte das Bild der Geliebten mit Inbrunst,", "tokens": ["Al\u00b7so", "k\u00fc\u00dft", "der", "Ver\u00b7lob\u00b7te", "das", "Bild", "der", "Ge\u00b7lieb\u00b7ten", "mit", "In\u00b7brunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+--+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.16": {"text": "Wenn noch fern in dem Arm holder Gespielen sie weilt.", "tokens": ["Wenn", "noch", "fern", "in", "dem", "Arm", "hol\u00b7der", "Ge\u00b7spie\u00b7len", "sie", "weilt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "APPR", "ART", "NN", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.17": {"text": "Ruhen soll sie, wo ich jetzt ruhete! Nehmet, ihr Tr\u00e4ume,", "tokens": ["Ru\u00b7hen", "soll", "sie", ",", "wo", "ich", "jetzt", "ru\u00b7he\u00b7te", "!", "Neh\u00b7met", ",", "ihr", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$.", "VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.18": {"text": "Huldvoll, was ich gef\u00fchlt, auf in den magischen Schoo\u00df;", "tokens": ["Huld\u00b7voll", ",", "was", "ich", "ge\u00b7f\u00fchlt", ",", "auf", "in", "den", "ma\u00b7gi\u00b7schen", "Schoo\u00df", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "PPER", "VVPP", "$,", "APPR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.19": {"text": "Hegt mit z\u00e4rtlicher Sorge das Pfand, und, wenn sie herannaht,", "tokens": ["Hegt", "mit", "z\u00e4rt\u00b7li\u00b7cher", "Sor\u00b7ge", "das", "Pfand", ",", "und", ",", "wenn", "sie", "her\u00b7an\u00b7naht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "$,", "KON", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.20": {"text": "In die entschlummerte Brust gie\u00dfet es Alles hinab:", "tokens": ["In", "die", "ent\u00b7schlum\u00b7mer\u00b7te", "Brust", "gie\u00b7\u00dfet", "es", "Al\u00b7les", "hin\u00b7ab", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "PIS", "PTKVZ", "$."], "meter": "---+--++--+--+", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "Doch nicht ich, sie f\u00fchle nun Jegliches, und die Bezaubrung", "tokens": ["Doch", "nicht", "ich", ",", "sie", "f\u00fch\u00b7le", "nun", "Jeg\u00b7li\u00b7ches", ",", "und", "die", "Be\u00b7zaub\u00b7rung"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PTKNEG", "PPER", "$,", "PPER", "VVFIN", "ADV", "PIS", "$,", "KON", "ART", "NN"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.22": {"text": "Mische mein hei\u00dfes Gef\u00fchl leise mit ihrem Gef\u00fchl.", "tokens": ["Mi\u00b7sche", "mein", "hei\u00b7\u00dfes", "Ge\u00b7f\u00fchl", "lei\u00b7se", "mit", "ih\u00b7rem", "Ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.23": {"text": "La\u00dft sie schw\u00e4rmen und hoffen wie ich, und gleiche Verkl\u00e4rung", "tokens": ["La\u00dft", "sie", "schw\u00e4r\u00b7men", "und", "hof\u00b7fen", "wie", "ich", ",", "und", "glei\u00b7che", "Ver\u00b7kl\u00e4\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "VVFIN", "KON", "VVFIN", "KOKOM", "PPER", "$,", "KON", "ADJA", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.24": {"text": "Leucht' ihr im Blicke wie mir, wenn ihr Gedanke mich nennt;", "tokens": ["Leucht'", "ihr", "im", "Bli\u00b7cke", "wie", "mir", ",", "wenn", "ihr", "Ge\u00b7dan\u00b7ke", "mich", "nennt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "KOKOM", "PPER", "$,", "KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.25": {"text": "La\u00dft sie gl\u00fchen wie ich, und la\u00dft, ich opfre die Sehnsucht", "tokens": ["La\u00dft", "sie", "gl\u00fc\u00b7hen", "wie", "ich", ",", "und", "la\u00dft", ",", "ich", "opf\u00b7re", "die", "Sehn\u00b7sucht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "VVFIN", "KOKOM", "PPER", "$,", "KON", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.26": {"text": "Willig den Grazien auf, lasset sie zagen wie ich.", "tokens": ["Wil\u00b7lig", "den", "Gra\u00b7zi\u00b7en", "auf", ",", "las\u00b7set", "sie", "za\u00b7gen", "wie", "ich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "PTKVZ", "$,", "VVFIN", "PPER", "VVINF", "KOKOM", "PPER", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Seidenes Bett, bald hegst du den reizenden Leib der Geliebten,", "tokens": ["Sei\u00b7de\u00b7nes", "Bett", ",", "bald", "hegst", "du", "den", "rei\u00b7zen\u00b7den", "Leib", "der", "Ge\u00b7lieb\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Ach, schon harret dein Schoo\u00df auf die begl\u00fcckende Last,", "tokens": ["Ach", ",", "schon", "har\u00b7ret", "dein", "Schoo\u00df", "auf", "die", "be\u00b7gl\u00fc\u00b7cken\u00b7de", "Last", ","], "token_info": ["word", "punct", "word", "word", "wor