{"textgrid.poem.58632": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "64.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Komm, kehr doch wieder ein, du ausgekehrte Seele,", "tokens": ["Komm", ",", "kehr", "doch", "wie\u00b7der", "ein", ",", "du", "aus\u00b7ge\u00b7kehr\u00b7te", "See\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "ADV", "ADV", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und bleib nicht l\u00e4nger fern im Land der Mitternacht,", "tokens": ["Und", "bleib", "nicht", "l\u00e4n\u00b7ger", "fern", "im", "Land", "der", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADJD", "ADJD", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Komm und auch ja dich nicht mit Furcht und Unglaub' qu\u00e4le,", "tokens": ["Komm", "und", "auch", "ja", "dich", "nicht", "mit", "Furcht", "und", "Un\u00b7glaub'", "qu\u00e4\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "ADV", "ADV", "PPER", "PTKNEG", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gott ruft und wartet schon, bis du zurecht gebracht!", "tokens": ["Gott", "ruft", "und", "war\u00b7tet", "schon", ",", "bis", "du", "zu\u00b7recht", "ge\u00b7bracht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "VVFIN", "ADV", "$,", "KOUS", "PPER", "PTKVZ", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bekenn nur deine Schuld und beuge dich dem\u00fctig,", "tokens": ["Be\u00b7kenn", "nur", "dei\u00b7ne", "Schuld", "und", "beu\u00b7ge", "dich", "de\u00b7m\u00fc\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "KON", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Kehr dich vertraulich so in seinen Scho\u00df hinein;", "tokens": ["Kehr", "dich", "ver\u00b7trau\u00b7lich", "so", "in", "sei\u00b7nen", "Scho\u00df", "hin\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.7": {"text": "Ach nein, er z\u00fcrnet nicht, er ist so fromm und g\u00fctig,", "tokens": ["Ach", "nein", ",", "er", "z\u00fcr\u00b7net", "nicht", ",", "er", "ist", "so", "fromm", "und", "g\u00fc\u00b7tig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKANT", "$,", "PPER", "VVFIN", "PTKNEG", "$,", "PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mit armen Kindern will der Herr vertrauet sein!", "tokens": ["Mit", "ar\u00b7men", "Kin\u00b7dern", "will", "der", "Herr", "ver\u00b7trau\u00b7et", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "ART", "NN", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Komm, kehr doch wieder ein, du au