{"dta.poem.2810": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das rechte Ma\u00df, wie man den Lehrling vorwerts treibt,", "tokens": ["Das", "rech\u00b7te", "Ma\u00df", ",", "wie", "man", "den", "Lehr\u00b7ling", "vor\u00b7werts", "treibt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "PIS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So da\u00df er doch dabei in rechten Schranken bleibt,", "tokens": ["So", "da\u00df", "er", "doch", "da\u00b7bei", "in", "rech\u00b7ten", "Schran\u00b7ken", "bleibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "PAV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ist, einen Fortschritt, den er that, ihn lassen merken,", "tokens": ["Ist", ",", "ei\u00b7nen", "Fort\u00b7schritt", ",", "den", "er", "that", ",", "ihn", "las\u00b7sen", "mer\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "PPER", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um zu dem weitern, den er thun soll, ihn zu st\u00e4rken,", "tokens": ["Um", "zu", "dem", "wei\u00b7tern", ",", "den", "er", "thun", "soll", ",", "ihn", "zu", "st\u00e4r\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "ART", "ADJA", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nicht da\u00df er glaube, schon ein Gro\u00dfes sei gethan,", "tokens": ["Nicht", "da\u00df", "er", "glau\u00b7be", ",", "schon", "ein", "Gro\u00b7\u00dfes", "sei", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOUS", "PPER", "VVFIN", "$,", "ADV", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch f\u00fchle, da\u00df er thun das Gr\u00f6ste soll und kan;", "tokens": ["Doch", "f\u00fch\u00b7le", ",", "da\u00df", "er", "thun", "das", "Gr\u00f6s\u00b7te", "soll", "und", "kan", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "ART", "NN", "VMFIN", "KON", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dazwischen unvermerkt, ihn nicht im Weiterschreiten", "tokens": ["Da\u00b7zwi\u00b7schen", "un\u00b7ver\u00b7merkt", ",", "ihn", "nicht", "im", "Wei\u00b7ter\u00b7schrei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "ADJD", "$,", "PPER", "PTKNEG", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu st\u00f6ren, aus dem Weg zu r\u00e4umen Schwierigkeiten,", "tokens": ["Zu", "st\u00f6\u00b7ren", ",", "aus", "dem", "Weg", "zu", "r\u00e4u\u00b7men", "Schwie\u00b7rig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Doch ihm zu g\u00f6nnen auch dabei von Zeit zu Zeit", "tokens": ["Doch", "ihm", "zu", "g\u00f6n\u00b7nen", "auch", "da\u00b7bei", "von", "Zeit", "zu", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "PTKZU", "VVINF", "ADV", "PAV", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das lohnende Gef\u00fchl besiegter Schwierigkeit.", "tokens": ["Das", "loh\u00b7nen\u00b7de", "Ge\u00b7f\u00fchl", "be\u00b7sieg\u00b7ter", "Schwie\u00b7rig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}