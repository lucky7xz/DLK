{"dta.poem.11408": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Amor, ein fischer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.57", "af:0.42"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die liebe, so allhier die fl\u00fcgel weggelegt,", "tokens": ["Die", "lie\u00b7be", ",", "so", "all\u00b7hier", "die", "fl\u00fc\u00b7gel", "weg\u00b7ge\u00b7legt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "$,", "ADV", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Stellt sich, als einen fischer ein:", "tokens": ["Stellt", "sich", ",", "als", "ei\u00b7nen", "fi\u00b7scher", "ein", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "KOUS", "ART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die tr\u00fcbe bach beperlter z\u00e4hren", "tokens": ["Die", "tr\u00fc\u00b7be", "bach", "be\u00b7perl\u00b7ter", "z\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VVFIN", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Soll mit gewalt ihm einen fang gew\u00e4hren.", "tokens": ["Soll", "mit", "ge\u00b7walt", "ihm", "ei\u00b7nen", "fang", "ge\u00b7w\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "De\u00dfwegen mu\u00df ein haar, so meine Phyllis tr\u00e4gt,", "tokens": ["De\u00df\u00b7we\u00b7gen", "mu\u00df", "ein", "haar", ",", "so", "mei\u00b7ne", "Phyl\u00b7lis", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "ART", "NN", "$,", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und ein blick, der in den seelen eine k\u00fchne lust erregt,", "tokens": ["Und", "ein", "blick", ",", "der", "in", "den", "see\u00b7len", "ei\u00b7ne", "k\u00fch\u00b7ne", "lust", "er\u00b7regt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Der k\u00f6der und das garn, ihr aber fische, seyn.", "tokens": ["Der", "k\u00f6\u00b7der", "und", "das", "garn", ",", "ihr", "a\u00b7ber", "fi\u00b7sche", ",", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ART", "NN", "$,", "PPER", "ADV", "ADJA", "$,", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Doch, hertzen! last euch nicht nach dieser kost verlangen!", "tokens": ["Doch", ",", "hert\u00b7zen", "!", "last", "euch", "nicht", "nach", "die\u00b7ser", "kost", "ver\u00b7lan\u00b7gen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$.", "VVFIN", "PPER", "PTKNEG", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich kenn\u2019 ihn schon, den kleinen b\u00f6sewicht,", "tokens": ["Ich", "kenn'", "ihn", "schon", ",", "den", "klei\u00b7nen", "b\u00f6\u00b7se\u00b7wicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Er locket euch nur an, und kommt wahrhafftig nicht,", "tokens": ["Er", "lo\u00b7cket", "euch", "nur", "an", ",", "und", "kommt", "wahr\u00b7haff\u00b7tig", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "KON", "VVFIN", "ADJD", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Um euch zu s\u00e4ttigen, er kommt nur euch zu fangen.", "tokens": ["Um", "euch", "zu", "s\u00e4t\u00b7ti\u00b7gen", ",", "er", "kommt", "nur", "euch", "zu", "fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "PTKZU", "VVINF", "$,", "PPER", "VVFIN", "ADV", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}