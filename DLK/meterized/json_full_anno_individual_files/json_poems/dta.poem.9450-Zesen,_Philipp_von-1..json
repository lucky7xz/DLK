{"dta.poem.9450": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mich hab\u2019 ich dem Liebsten zu eigen gegeben/", "tokens": ["Mich", "hab'", "ich", "dem", "Liebs\u00b7ten", "zu", "ei\u00b7gen", "ge\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "PTKA", "ADJD", "VVPP", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Er bleibet mein Sch\u00f6nster/ ich bleibe sein Leben.", "tokens": ["Er", "blei\u00b7bet", "mein", "Sch\u00f6ns\u00b7ter", "/", "ich", "blei\u00b7be", "sein", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$(", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Kom\u0303/ Bruder/ und la\u00df uns aufs Acker-feld gehn/", "tokens": ["Kom\u0303", "/", "Bru\u00b7der", "/", "und", "la\u00df", "uns", "aufs", "A\u00b7cke\u00b7rfeld", "gehn", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$(", "NN", "$(", "KON", "VVIMP", "PPER", "APPRART", "NN", "VVINF", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Damit wir des morgens bey zeiten aufstehn/", "tokens": ["Da\u00b7mit", "wir", "des", "mor\u00b7gens", "bey", "zei\u00b7ten", "auf\u00b7stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADV", "APPR", "NN", "VVINF", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "und sehen ob unsere Reben auch bl\u00fchen/", "tokens": ["und", "se\u00b7hen", "ob", "un\u00b7se\u00b7re", "Re\u00b7ben", "auch", "bl\u00fc\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOUS", "PPOSAT", "NN", "ADV", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "und augen gewonnen", "tokens": ["und", "au\u00b7gen", "ge\u00b7won\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Von hitzen der Sonnen;", "tokens": ["Von", "hit\u00b7zen", "der", "Son\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Was wiltu verzihen?", "tokens": ["Was", "wil\u00b7tu", "ver\u00b7zi\u00b7hen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Komm eylend mein Licht/", "tokens": ["Komm", "ey\u00b7lend", "mein", "Licht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "und seume dich nicht!", "tokens": ["und", "seu\u00b7me", "dich", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Wir wollen den \u00e4pfelbaum heute beschauen/", "tokens": ["Wir", "wol\u00b7len", "den", "\u00e4p\u00b7fel\u00b7baum", "heu\u00b7te", "be\u00b7schau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADV", "ADV", "VVINF", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Daselbsten auf wollust und fr\u00f6ligkeit bauen/", "tokens": ["Da\u00b7selbs\u00b7ten", "auf", "wol\u00b7lust", "und", "fr\u00f6\u00b7lig\u00b7keit", "bau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Da will ich dier geben und zeigen die Brust/", "tokens": ["Da", "will", "ich", "dier", "ge\u00b7ben", "und", "zei\u00b7gen", "die", "Brust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PDS", "VVINF", "KON", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Da will ich dich k\u00fcssen und hertzen mit lust;", "tokens": ["Da", "will", "ich", "dich", "k\u00fcs\u00b7sen", "und", "hert\u00b7zen", "mit", "lust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF", "KON", "NN", "APPR", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Da will ich dier unsere Lilien geben/", "tokens": ["Da", "will", "ich", "dier", "un\u00b7se\u00b7re", "Li\u00b7li\u00b7en", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+--+--+-+--", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Da soltu dich laben", "tokens": ["Da", "sol\u00b7tu", "dich", "la\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PRF", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Mit allerley gaben/", "tokens": ["Mit", "al\u00b7ler\u00b7ley", "ga\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "VVFIN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Mein einiges Leben/", "tokens": ["Mein", "ei\u00b7ni\u00b7ges", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Dr\u00fcmb eyle/ mein Licht/", "tokens": ["Dr\u00fcmb", "ey\u00b7le", "/", "mein", "Licht", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$(", "PPOSAT", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "und seume dich nicht!", "tokens": ["und", "seu\u00b7me", "dich", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}