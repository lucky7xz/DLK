{"textgrid.poem.60556": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Mit meinen Gedichten", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mich lie\u00df die Gunst des Augenblickes,", "tokens": ["Mich", "lie\u00df", "die", "Gunst", "des", "Au\u00b7gen\u00b7bli\u00b7ckes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein fl\u00fcchtig L\u00e4cheln des Geschickes,", "tokens": ["Ein", "fl\u00fcch\u00b7tig", "L\u00e4\u00b7cheln", "des", "Ge\u00b7schi\u00b7ckes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie bis ins Herz du sch\u00f6n, erkennen;", "tokens": ["Wie", "bis", "ins", "Herz", "du", "sch\u00f6n", ",", "er\u00b7ken\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADV", "APPRART", "NN", "PPER", "ADJD", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Leb wohl! ich mu\u00df von dir mich trennen!", "tokens": ["Leb", "wohl", "!", "ich", "mu\u00df", "von", "dir", "mich", "tren\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$.", "PPER", "VMFIN", "APPR", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch milderts mir dein fr\u00fches Scheiden,", "tokens": ["Doch", "mil\u00b7derts", "mir", "dein", "fr\u00fc\u00b7hes", "Schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn ich vom Gl\u00fcck, das mir entschwunden \u2013", "tokens": ["Wenn", "ich", "vom", "Gl\u00fcck", ",", "das", "mir", "ent\u00b7schwun\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "$,", "PRELS", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So schnell wie du! \u2013, die heitern Kunden", "tokens": ["So", "schnell", "wie", "du", "!", "\u2013", ",", "die", "hei\u00b7tern", "Kun\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "KOKOM", "PPER", "$.", "$(", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und wenn ich darf den Ruf der Leiden,", "tokens": ["Und", "wenn", "ich", "darf", "den", "Ruf", "der", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VMFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die singend mir das Herz zerrissen,", "tokens": ["Die", "sin\u00b7gend", "mir", "das", "Herz", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "In deinen lieben H\u00e4nden wissen.", "tokens": ["In", "dei\u00b7nen", "lie\u00b7ben", "H\u00e4n\u00b7den", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mich lie\u00df die Gunst des Augenblickes,", "tokens": ["Mich", "lie