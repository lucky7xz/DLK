{"textgrid.poem.64379": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "102. Unter einem niederl\u00e4ndischen Gem\u00e4lde eines fr\u00f6hlichen Trinkers und Tabakrauchers", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mir ist, ihr Herrn, doch gar zu wohl!", "tokens": ["Mir", "ist", ",", "ihr", "Herrn", ",", "doch", "gar", "zu", "wohl", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PPOSAT", "NN", "$,", "ADV", "ADV", "APPR", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie sch\u00e4umt das liebe Glas so voll!", "tokens": ["Wie", "sch\u00e4umt", "das", "lie\u00b7be", "Glas", "so", "voll", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hab' ich ein Pfeifchen ausgeraucht,", "tokens": ["Hab'", "ich", "ein", "Pfei\u00b7fchen", "aus\u00b7ge\u00b7raucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Flugs wird das andre angeschmaucht!", "tokens": ["Flugs", "wird", "das", "and\u00b7re", "an\u00b7ge\u00b7schmaucht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Vergr\u00e4melt mir die Freude nicht!", "tokens": ["Ver\u00b7gr\u00e4\u00b7melt", "mir", "die", "Freu\u00b7de", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "