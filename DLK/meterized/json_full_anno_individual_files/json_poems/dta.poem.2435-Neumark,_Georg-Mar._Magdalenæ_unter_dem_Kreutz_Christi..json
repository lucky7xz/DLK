{"dta.poem.2435": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Mar. Magdalen\u00e6 unter dem Kreutz  \n Christi.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Mich d\u00fcrstet/ schreistu/ hier aber wo nur Steine/", "tokens": ["Mich", "d\u00fcrs\u00b7tet", "/", "schreis\u00b7tu", "/", "hier", "a\u00b7ber", "wo", "nur", "Stei\u00b7ne", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "VVFIN", "$(", "ADV", "ADV", "PWAV", "ADV", "NN", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und rauhe Klippen sind da ist kein klarer Flu\u00df/", "tokens": ["Und", "rau\u00b7he", "Klip\u00b7pen", "sind", "da", "ist", "kein", "kla\u00b7rer", "Flu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "ADV", "VAFIN", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Noch einger Labsatstrunk als nur ein Thr\u00e4nengu\u00df", "tokens": ["Noch", "ein\u00b7ger", "Lab\u00b7sats\u00b7trunk", "als", "nur", "ein", "Thr\u00e4\u00b7nen\u00b7gu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "KOKOM", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der durch mein Augen fleust/ den ich im Hertzen weine.", "tokens": ["Der", "durch", "mein", "Au\u00b7gen", "fleust", "/", "den", "ich", "im", "Hert\u00b7zen", "wei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$(", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "La\u00df dieser seyn dein Trunk/ O grosser Lebensf\u00fcr\u00df/", "tokens": ["La\u00df", "die\u00b7ser", "seyn", "dein", "Trunk", "/", "O", "gros\u00b7ser", "Le\u00b7bens\u00b7f\u00fcr\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PDAT", "VAINF", "PPOSAT", "NN", "$(", "NE", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich schenke reichlich ein/ weil dich so hefftig d\u00fcrst.", "tokens": ["Ich", "schen\u00b7ke", "reich\u00b7lich", "ein", "/", "weil", "dich", "so", "heff\u00b7tig", "d\u00fcrst", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "$(", "KOUS", "PPER", "ADV", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}