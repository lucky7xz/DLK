, "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKZU", "VVINF", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.29": {"text": "Wahrlich, Bester! nur von f\u00fcnf Minuten", "tokens": ["Wahr\u00b7lich", ",", "Bes\u00b7ter", "!", "nur", "von", "f\u00fcnf", "Mi\u00b7nu\u00b7ten"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "NN", "$.", "ADV", "APPR", "CARD", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.30": {"text": "Alles Elend dieser Erde sehn,", "tokens": ["Al\u00b7les", "E\u00b7lend", "die\u00b7ser", "Er\u00b7de", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PDAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.31": {"text": "W\u00e4re schrecklicher, als in den Fluten", "tokens": ["W\u00e4\u00b7re", "schreck\u00b7li\u00b7cher", ",", "als", "in", "den", "Flu\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.32": {"text": "Des erz\u00fcrnten Meeres untergehn.", "tokens": ["Des", "er\u00b7z\u00fcrn\u00b7ten", "Mee\u00b7res", "un\u00b7ter\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.33": {"text": "Ehe noch am fr\u00f6hlichen Pokale", "tokens": ["E\u00b7he", "noch", "am", "fr\u00f6h\u00b7li\u00b7chen", "Po\u00b7ka\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.34": {"text": "Dieser Kranz von Rosen wird verbl\u00fchn,", "tokens": ["Die\u00b7ser", "Kranz", "von", "Ro\u00b7sen", "wird", "ver\u00b7bl\u00fchn", ","], "token_info": ["word", "