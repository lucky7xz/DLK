 {"text": "Sieh denn auch, des Lebens froh zu werden,", "tokens": ["Sieh", "denn", "auch", ",", "des", "Le\u00b7bens", "froh", "zu", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADV", "$,", "ART", "NN", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "Um dich her, wo stille Weisheit wohnt,", "tokens": ["Um", "dich", "her", ",", "wo", "stil\u00b7le", "Weis\u00b7heit", "wohnt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "PTKVZ", "$,", "PWAV", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.19": {"text": "Und das Gl\u00fcck zuweilen schon auf Erden", "tokens": ["Und", "das", "Gl\u00fcck", "zu\u00b7wei\u00b7len", "schon", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADV", "ADV", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "Edler Seelen Tugenden belohnt.", "tokens": ["Ed\u00b7ler", "See\u00b7len", "Tu\u00b7gen\u00b7den", "be\u00b7lohnt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.21": {"text": "Aber da, wo Menschen, Menschen braten,", "tokens": ["A\u00b7ber", "da", ",", "wo", "Men\u00b7schen", ",", "Men\u00b7schen", "bra\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "NN", "$,", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "Weil sie andres Glaubens sind als du,", "tokens": ["Weil", "sie", "and\u00b7res", "Glau\u00b7bens", "sind", "als", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "p