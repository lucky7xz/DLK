{"textgrid.poem.25726": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Um den Reitz der Erde zu genie\u00dfen,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Um den Reitz der Erde zu genie\u00dfen,", "tokens": ["Um", "den", "Reitz", "der", "Er\u00b7de", "zu", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Siehest du dich um, mein lieber Freund,", "tokens": ["Sie\u00b7hest", "du", "dich", "um", ",", "mein", "lie\u00b7ber", "Freund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wo aus ihrem Schoo\u00dfe Veilchen sprie\u00dfen,", "tokens": ["Wo", "aus", "ih\u00b7rem", "Schoo\u00b7\u00dfe", "Veil\u00b7chen", "sprie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und der Vollmond durch die Birken scheint.", "tokens": ["Und", "der", "Voll\u00b7mond", "durch", "die", "Bir\u00b7ken", "scheint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Aber, wo die Distel ihren spitzen", "tokens": ["A\u00b7ber", ",", "wo", "die", "Dis\u00b7tel", "ih\u00b7ren", "spit\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "PWAV", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Stachel in des Wandrers Ferse sticht,", "tokens": ["Sta\u00b7chel", "in", "des", "Wand\u00b7rers", "Fer\u00b7se", "sticht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Und die Wolken auf ihn niederblitzen,", "tokens": ["Und", "die", "Wol\u00b7ken", "auf", "ihn", "nie\u00b7der\u00b7blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Diesen Irrweg sucht dein Auge nicht.", "tokens": ["Die\u00b7sen", "Irr\u00b7weg", "sucht", "dein", "Au\u00b7ge", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Und weshalb auch suchen? Um die H\u00e4nde", "tokens": ["Und", "we\u00b7shalb", "auch", "su\u00b7chen", "?", "Um", "die", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWAV", "ADV", "VVINF", "$.", "KOUI", "ART", "NN"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.10": {"text": "Wund zu rei\u00dfen an dem Distelheer'?", "tokens": ["Wund", "zu", "rei\u00b7\u00dfen", "an", "dem", "Dis\u00b7tel\u00b7heer'", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "(eine von zehntausend!) Oder f\u00e4nde", "tokens": ["(", "ei\u00b7ne", "von", "zehn\u00b7tau\u00b7send", "!", ")", "O\u00b7der", "f\u00e4n\u00b7de"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["$(", "ART", "APPR", "CARD", "$.", "$(", "KON", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Nun der Blitz des Wandrers Haupt nicht mehr?", "tokens": ["Nun", "der", "Blitz", "des", "Wand\u00b7rers", "Haupt", "nicht", "mehr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "NN", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.13": {"text": "Aber jede Nessel in den G\u00e4ngen", "tokens": ["A\u00b7ber", "je\u00b7de", "Nes\u00b7sel", "in", "den", "G\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Deines Gartens, die uns t\u00fcckisch sticht,", "tokens": ["Dei\u00b7nes", "Gar\u00b7tens", ",", "die", "uns", "t\u00fc\u00b7ckisch", "sticht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.15": {"text": "Und den Busch voll Rosen will verdr\u00e4ngen,", "tokens": ["Und", "den", "Busch", "voll", "Ro\u00b7sen", "will", "ver\u00b7dr\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Auszureuten: machst du dir zur Pflicht.", "tokens": ["Aus\u00b7zu\u00b7reu\u00b7ten", ":", "machst", "du", "dir", "zur", "Pflicht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.17": {"text": "Sieh denn auch, des Lebens froh zu werden,", "tokens": ["Sieh", "denn", "auch", ",", "des", "Le\u00b7bens", "froh", "zu", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADV", "$,", "ART", "NN", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "Um dich her, wo stille Weisheit wohnt,", "tokens": ["Um", "dich", "her", ",", "wo", "stil\u00b7le", "Weis\u00b7heit", "wohnt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "PTKVZ", "$,", "PWAV", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.19": {"text": "Und das Gl\u00fcck zuweilen schon auf Erden", "tokens": ["Und", "das", "Gl\u00fcck", "zu\u00b7wei\u00b7len", "schon", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADV", "ADV", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "Edler Seelen Tugenden belohnt.", "tokens": ["Ed\u00b7ler", "See\u00b7len", "Tu\u00b7gen\u00b7den", "be\u00b7lohnt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.21": {"text": "Aber da, wo Menschen, Menschen braten,", "tokens": ["A\u00b7ber", "da", ",", "wo", "Men\u00b7schen", ",", "Men\u00b7schen", "bra\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "NN", "$,", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "Weil sie andres Glaubens sind als du,", "tokens": ["Weil", "sie", "and\u00b7res", "Glau\u00b7bens", "sind", "als", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VAFIN", "KOUS", "PPER", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.23": {"text": "Und nach ihrem Glauben Gutes thaten:", "tokens": ["Und", "nach", "ih\u00b7rem", "Glau\u00b7ben", "Gu\u00b7tes", "tha\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.24": {"text": "O da dr\u00fccke fest dein Auge zu.", "tokens": ["O", "da", "dr\u00fc\u00b7cke", "fest", "dein", "Au\u00b7ge", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "ADJD", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.25": {"text": "Immer neue Lechzende auf Erden", "tokens": ["Im\u00b7mer", "neu\u00b7e", "Lech\u00b7zen\u00b7de", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "APPR", "NN"], "meter": "+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.26": {"text": "Aufzusuchen, macht die Welt zur Last,", "tokens": ["Auf\u00b7zu\u00b7su\u00b7chen", ",", "macht", "die", "Welt", "zur", "Last", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.27": {"text": "Da dir mehr schon itzt begegnen werden,", "tokens": ["Da", "dir", "mehr", "schon", "itzt", "be\u00b7geg\u00b7nen", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADV", "VVINF", "VAFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.28": {"text": "Als du zu erquicken Wasser hast.", "tokens": ["Als", "du", "zu", "er\u00b7qui\u00b7cken", "Was\u00b7ser", "hast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKZU", "VVINF", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.29": {"text": "Wahrlich, Bester! nur von f\u00fcnf Minuten", "tokens": ["Wahr\u00b7lich", ",", "Bes\u00b7ter", "!", "nur", "von", "f\u00fcnf", "Mi\u00b7nu\u00b7ten"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "NN", "$.", "ADV", "APPR", "CARD", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.30": {"text": "Alles Elend dieser Erde sehn,", "tokens": ["Al\u00b7les", "E\u00b7lend", "die\u00b7ser", "Er\u00b7de", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PDAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.31": {"text": "W\u00e4re schrecklicher, als in den Fluten", "tokens": ["W\u00e4\u00b7re", "schreck\u00b7li\u00b7cher", ",", "als", "in", "den", "Flu\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.32": {"text": "Des erz\u00fcrnten Meeres untergehn.", "tokens": ["Des", "er\u00b7z\u00fcrn\u00b7ten", "Mee\u00b7res", "un\u00b7ter\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.33": {"text": "Ehe noch am fr\u00f6hlichen Pokale", "tokens": ["E\u00b7he", "noch", "am", "fr\u00f6h\u00b7li\u00b7chen", "Po\u00b7ka\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.34": {"text": "Dieser Kranz von Rosen wird verbl\u00fchn,", "tokens": ["Die\u00b7ser", "Kranz", "von", "Ro\u00b7sen", "wird", "ver\u00b7bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "NE", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.35": {"text": "Werden Tausende zum letztenmale", "tokens": ["Wer\u00b7den", "Tau\u00b7sen\u00b7de", "zum", "letz\u00b7ten\u00b7ma\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "NN", "APPRART", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.36": {"text": "R\u00f6chelnd ihren Athem in sich ziehn.", "tokens": ["R\u00f6\u00b7chelnd", "ih\u00b7ren", "A\u00b7them", "in", "sich", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "APPR", "PRF", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.37": {"text": "Eh' wir unser Rheinweinlied gesungen,", "tokens": ["Eh'", "wir", "un\u00b7ser", "Rhein\u00b7wein\u00b7lied", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.38": {"text": "Und f\u00fcr ", "tokens": ["Und", "f\u00fcr"], "token_info": ["word", "word"], "pos": ["KON", "APPR"], "meter": "-+", "measure": "iambic.single"}, "line.39": {"text": "Ist gewi\u00df bereits ein Schiff verschlungen,", "tokens": ["Ist", "ge\u00b7wi\u00df", "be\u00b7reits", "ein", "Schiff", "ver\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.40": {"text": "Und durch Brand ein halbes Dorf verheert.", "tokens": ["Und", "durch", "Brand", "ein", "hal\u00b7bes", "Dorf", "ver\u00b7heert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.41": {"text": "Auch an uns wird einst die Reihe kommen,", "tokens": ["Auch", "an", "uns", "wird", "einst", "die", "Rei\u00b7he", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VAFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.42": {"text": "In der Zahl der Weinenden zu seyn,", "tokens": ["In", "der", "Zahl", "der", "Wei\u00b7nen\u00b7den", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PTKZU", "VAINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.43": {"text": "Ehe wir im stillen Thal' der Frommen", "tokens": ["E\u00b7he", "wir", "im", "stil\u00b7len", "Thal'", "der", "From\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.44": {"text": "Uns der wahren Freud' auf ewig weihn.", "tokens": ["Uns", "der", "wah\u00b7ren", "Freud'", "auf", "e\u00b7wig", "weihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "APPR", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.45": {"text": "Wirst du fr\u00fcher zu dem Thale wallen,", "tokens": ["Wirst", "du", "fr\u00fc\u00b7her", "zu", "dem", "Tha\u00b7le", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.46": {"text": "Als der Freund, der auch hin\u00fcber strebt?", "tokens": ["Als", "der", "Freund", ",", "der", "auch", "hin\u00b7\u00fc\u00b7ber", "strebt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "ADV", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.47": {"text": "Nie in eine von den Gruben fallen,", "tokens": ["Nie", "in", "ei\u00b7ne", "von", "den", "Gru\u00b7ben", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.48": {"text": "Die der B\u00f6se hier dem Guten gr\u00e4bt?", "tokens": ["Die", "der", "B\u00f6\u00b7se", "hier", "dem", "Gu\u00b7ten", "gr\u00e4bt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.49": {"text": "Nein! dem Schmerze werden alle beide,", "tokens": ["Nein", "!", "dem", "Schmer\u00b7ze", "wer\u00b7den", "al\u00b7le", "bei\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ART", "NN", "VAFIN", "PIAT", "PIS", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.50": {"text": "Wie dem Tode, nimmer wir entfliehn.", "tokens": ["Wie", "dem", "To\u00b7de", ",", "nim\u00b7mer", "wir", "ent\u00b7fliehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ADV", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.51": {"text": "Darum la\u00df uns hurtig aus der Freude", "tokens": ["Da\u00b7rum", "la\u00df", "uns", "hur\u00b7tig", "aus", "der", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.52": {"text": "Spin