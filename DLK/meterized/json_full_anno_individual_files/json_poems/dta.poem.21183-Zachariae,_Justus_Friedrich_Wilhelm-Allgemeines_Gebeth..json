{"dta.poem.21183": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Allgemeines Gebeth.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Allm\u00e4chtiger, der seinen Thron", "tokens": ["All\u00b7m\u00e4ch\u00b7ti\u00b7ger", ",", "der", "sei\u00b7nen", "Thron"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Himmeln hoch erh\u00f6het;", "tokens": ["In", "Him\u00b7meln", "hoch", "er\u00b7h\u00f6\u00b7het", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O h\u00f6re mich, der Erde Sohn,", "tokens": ["O", "h\u00f6\u00b7re", "mich", ",", "der", "Er\u00b7de", "Sohn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der dir im Staube flehet!", "tokens": ["Der", "dir", "im", "Stau\u00b7be", "fle\u00b7het", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du schufst mich Staub, und lie\u00dfest Staub", "tokens": ["Du", "schufst", "mich", "Staub", ",", "und", "lie\u00b7\u00dfest", "Staub"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$,", "KON", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Engel sich erheben;", "tokens": ["Zum", "En\u00b7gel", "sich", "er\u00b7he\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hier unten der Verwesung Raub,", "tokens": ["Hier", "un\u00b7ten", "der", "Ver\u00b7we\u00b7sung", "Raub", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um ewig dort zu leben.", "tokens": ["Um", "e\u00b7wig", "dort", "zu", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ein denkend Thier! Wie arm, wie blo\u00df,", "tokens": ["Ein", "den\u00b7kend", "Thier", "!", "Wie", "arm", ",", "wie", "blo\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$.", "PWAV", "ADJD", "$,", "PWAV", "ADV", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Ist es, der Herr der Erden!", "tokens": ["Ist", "es", ",", "der", "Herr", "der", "Er\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein denkend Thier! Wie frey, wie gro\u00df,", "tokens": ["Ein", "den\u00b7kend", "Thier", "!", "Wie", "frey", ",", "wie", "gro\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$.", "PWAV", "ADJD", "$,", "PWAV", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Unsterblich soll es werden!", "tokens": ["U\u00b7nsterb\u00b7lich", "soll", "es", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "VAINF", "$."], "meter": "++-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Welch ein Geschenk gabst du mir nicht,", "tokens": ["Welch", "ein", "Ge\u00b7schenk", "gabst", "du", "mir", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "VVFIN", "PPER", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da du Vernunft mir schenktest,", "tokens": ["Da", "du", "Ver\u00b7nunft", "mir", "schenk\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und der Erkenntni\u00df g\u00f6ttlichs Licht", "tokens": ["Und", "der", "Er\u00b7kennt\u00b7ni\u00df", "g\u00f6tt\u00b7lichs", "Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In meine Seele senktest!", "tokens": ["In", "mei\u00b7ne", "See\u00b7le", "senk\u00b7test", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Verleih mir doch die Wissenschaft,", "tokens": ["Ver\u00b7leih", "mir", "doch", "die", "Wis\u00b7sen\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein ewges Gl\u00fcck zu finden;", "tokens": ["Mein", "ew\u00b7ges", "Gl\u00fcck", "zu", "fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und gieb mir Willen, Muth, und Kraft,", "tokens": ["Und", "gieb", "mir", "Wil\u00b7len", ",", "Muth", ",", "und", "Kraft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "NN", "$,", "NN", "$,", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich selbst zu \u00fcberwinden.", "tokens": ["Mich", "selbst", "zu", "\u00fc\u00b7berw\u00b7in\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Lehr mich, was mein Gewissen sagt,", "tokens": ["Lehr", "mich", ",", "was", "mein", "Ge\u00b7wis\u00b7sen", "sagt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Himmel vorzuziehen!", "tokens": ["Dem", "Him\u00b7mel", "vor\u00b7zu\u00b7zie\u00b7hen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und la\u00df mich, was es untersagt,", "tokens": ["Und", "la\u00df", "mich", ",", "was", "es", "un\u00b7ter\u00b7sagt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mehr als die H\u00f6lle fliehen.", "tokens": ["Mehr", "als", "die", "H\u00f6l\u00b7le", "flie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Mach f\u00fchlend dieses harte Herz,", "tokens": ["Mach", "f\u00fch\u00b7lend", "die\u00b7ses", "har\u00b7te", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn meine Br\u00fcder leiden;", "tokens": ["Wenn", "mei\u00b7ne", "Br\u00fc\u00b7der", "lei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und la\u00df an meines Ha\u00dfers Schmerz", "tokens": ["Und", "la\u00df", "an", "mei\u00b7nes", "Ha\u00b7\u00dfers", "Schmerz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich nie mein Auge weiden.", "tokens": ["Sich", "nie", "mein", "Au\u00b7ge", "wei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "La\u00df mich nie mit verwegner Hand", "tokens": ["La\u00df", "mich", "nie", "mit", "ver\u00b7weg\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Nach deinem Donner trachten;", "tokens": ["Nach", "dei\u00b7nem", "Don\u00b7ner", "trach\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Noch jeden, der dich nicht erkannt", "tokens": ["Noch", "je\u00b7den", ",", "der", "dich", "nicht", "er\u00b7kannt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "$,", "PRELS", "PPER", "PTKNEG", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der H\u00f6lle w\u00fcrdig achten.", "tokens": ["Der", "H\u00f6l\u00b7le", "w\u00fcr\u00b7dig", "ach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Jm Gl\u00fccke Furcht, im Ungl\u00fcck Muth", "tokens": ["Jm", "Gl\u00fc\u00b7cke", "Furcht", ",", "im", "Un\u00b7gl\u00fcck", "Muth"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "NN", "$,", "APPRART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sey alles, was ich flehe.", "tokens": ["Sey", "al\u00b7les", ",", "was", "ich", "fle\u00b7he", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was du, mein Sch\u00f6pfer willst, ist gut,", "tokens": ["Was", "du", ",", "mein", "Sch\u00f6p\u00b7fer", "willst", ",", "ist", "gut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "$,", "PPOSAT", "NN", "VMFIN", "$,", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und was du willst, geschehe!", "tokens": ["Und", "was", "du", "willst", ",", "ge\u00b7sche\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VMFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "La\u00df mich mein Brodt durch deine Gunst", "tokens": ["La\u00df", "mich", "mein", "Brodt", "durch", "dei\u00b7ne", "Gunst"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht ohne M\u00fch erwerben.", "tokens": ["Nicht", "oh\u00b7ne", "M\u00fch", "er\u00b7wer\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und lehre mich die gro\u00dfe Kunst", "tokens": ["Und", "leh\u00b7re", "mich", "die", "gro\u00b7\u00dfe", "Kunst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu leben, und zu sterben.", "tokens": ["Zu", "le\u00b7ben", ",", "und", "zu", "ster\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "O du, vor dem der Seraph kniet,", "tokens": ["O", "du", ",", "vor", "dem", "der", "Se\u00b7raph", "kniet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "APPR", "ART", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Cherubim umringen,", "tokens": ["Den", "Che\u00b7ru\u00b7bim", "um\u00b7rin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NE", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von allen Sternen schallt das Lied,", "tokens": ["Von", "al\u00b7len", "Ster\u00b7nen", "schallt", "das", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So deine Heilgen singen.", "tokens": ["So", "dei\u00b7ne", "Heil\u00b7gen", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Ich beuge, Herr, vor dir mein Knie;", "tokens": ["Ich", "beu\u00b7ge", ",", "Herr", ",", "vor", "dir", "mein", "Knie", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "$,", "APPR", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du hast den Staub erhoben!", "tokens": ["Du", "hast", "den", "Staub", "er\u00b7ho\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Heil mir! ich bin ein Geist, wie sie,", "tokens": ["Heil", "mir", "!", "ich", "bin", "ein", "Geist", ",", "wie", "sie", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "PPER", "VAFIN", "ART", "NN", "$,", "PWAV", "PPER", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Der Mensch darf, Herr, dich loben!", "tokens": ["Der", "Mensch", "darf", ",", "Herr", ",", "dich", "lo\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "$,", "NN", "$,", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}