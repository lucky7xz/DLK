{"textgrid.poem.67316": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Die Feldheimen", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Menschen waren einst, so lehret Plato,", "tokens": ["Men\u00b7schen", "wa\u00b7ren", "einst", ",", "so", "leh\u00b7ret", "Pla\u00b7to", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "$,", "ADV", "VVFIN", "NE", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Gute Menschen waren einst die Heimchen,", "tokens": ["Gu\u00b7te", "Men\u00b7schen", "wa\u00b7ren", "einst", "die", "Heim\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Die ihr Tagewerk mit Flei\u00dfe trieben,", "tokens": ["Die", "ihr", "Ta\u00b7ge\u00b7werk", "mit", "Flei\u00b7\u00dfe", "trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Kinder zeugten und den Acker bauten;", "tokens": ["Kin\u00b7der", "zeug\u00b7ten", "und", "den", "A\u00b7cker", "bau\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Bis mit ihren zauberischen T\u00f6nen", "tokens": ["Bis", "mit", "ih\u00b7ren", "zau\u00b7be\u00b7ri\u00b7schen", "T\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dreimal drei der Musen niederstiegen", "tokens": ["Drei\u00b7mal", "drei", "der", "Mu\u00b7sen", "nie\u00b7ders\u00b7tie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "CARD", "ART", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und die Fluren mit Gesang erf\u00fcllten", "tokens": ["Und", "die", "Flu\u00b7ren", "mit", "Ge\u00b7sang", "er\u00b7f\u00fcll\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und sogar die V\u00f6gel singen lehrten.", "tokens": ["Und", "so\u00b7gar", "die", "V\u00f6\u00b7gel", "sin\u00b7gen", "lehr\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Ach, da standen sprachlos und entz\u00fccket", "tokens": ["Ach", ",", "da", "stan\u00b7den", "sprach\u00b7los", "und", "ent\u00b7z\u00fc\u00b7cket"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "ADJD", "KON", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Unsre flei\u00dfig-guten Ackerseelen", "tokens": ["Uns\u00b7re", "flei\u00b7\u00dfig\u00b7gu\u00b7ten", "A\u00b7cker\u00b7see\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und verga\u00dfen ob der neuen Wollust", "tokens": ["Und", "ver\u00b7ga\u00b7\u00dfen", "ob", "der", "neu\u00b7en", "Wol\u00b7lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Arbeit, Kinder, Speis' und Trank und Schlummer.", "tokens": ["Ar\u00b7beit", ",", "Kin\u00b7der", ",", "Speis'", "und", "Trank", "und", "Schlum\u00b7mer", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Offnen Ohres, offnen Mundes hingen", "tokens": ["Off\u00b7nen", "Oh\u00b7res", ",", "off\u00b7nen", "Mun\u00b7des", "hin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["FM.la", "FM.la", "$,", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Am Gesange der G\u00f6ttinnen Alle,", "tokens": ["Am", "Ge\u00b7san\u00b7ge", "der", "G\u00f6t\u00b7tin\u00b7nen", "Al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PIAT", "$,"], "meter": "+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Wurden Amatoren, Virtuosen,", "tokens": ["Wur\u00b7den", "A\u00b7ma\u00b7to\u00b7ren", ",", "Vir\u00b7tu\u00b7o\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "NE", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Famuli und Famul\u00e4 der Musen.", "tokens": ["Fa\u00b7mu\u00b7li", "und", "Fa\u00b7mu\u00b7l\u00e4", "der", "Mu\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Wenig Tage w\u00e4hrete die Freude,", "tokens": ["We\u00b7nig", "Ta\u00b7ge", "w\u00e4h\u00b7re\u00b7te", "die", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und das Chor der horchenden Entz\u00fcckten", "tokens": ["Und", "das", "Chor", "der", "hor\u00b7chen\u00b7den", "Ent\u00b7z\u00fcck\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Stand von Hunger, Durst und von Ges\u00e4ngen", "tokens": ["Stand", "von", "Hun\u00b7ger", ",", "Durst", "und", "von", "Ge\u00b7s\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "$,", "NN", "KON", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Matt und welk und eingeschrumpft und sterbend.", "tokens": ["Matt", "und", "welk", "und", "ein\u00b7ge\u00b7schrumpft", "und", "ster\u00b7bend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "KON", "VVPP", "KON", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Und die Musen halfen ihren treuen", "tokens": ["Und", "die", "Mu\u00b7sen", "hal\u00b7fen", "ih\u00b7ren", "treu\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PPOSAT", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "M\u00e4rtyrern noch in den letzten N\u00f6then;", "tokens": ["M\u00e4r\u00b7ty\u00b7rern", "noch", "in", "den", "letz\u00b7ten", "N\u00f6\u00b7then", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "S\u00fc\u00dfen Todes f\u00fchrten sie die armen", "tokens": ["S\u00fc\u00b7\u00dfen", "To\u00b7des", "f\u00fchr\u00b7ten", "sie", "die", "ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "ART", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Singend-sterbenden ins Land der Dichter,", "tokens": ["Sin\u00b7gen\u00b7dster\u00b7ben\u00b7den", "ins", "Land", "der", "Dich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Wo sie jetzt auf allen gr\u00fcnen B\u00e4umen", "tokens": ["Wo", "sie", "jetzt", "auf", "al\u00b7len", "gr\u00fc\u00b7nen", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wie die K\u00f6nige der Erde thronen,", "tokens": ["Wie", "die", "K\u00f6\u00b7ni\u00b7ge", "der", "Er\u00b7de", "thro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ohne Sorgen, ohne M\u00fch und Arbeit,", "tokens": ["Oh\u00b7ne", "Sor\u00b7gen", ",", "oh\u00b7ne", "M\u00fch", "und", "Ar\u00b7beit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUI", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ohne Fleisch und Blut, den G\u00f6ttern \u00e4hnlich.", "tokens": ["Oh\u00b7ne", "Fleisch", "und", "Blut", ",", "den", "G\u00f6t\u00b7tern", "\u00e4hn\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Nun und nimmer dr\u00fccket sie das Alter,", "tokens": ["Nun", "und", "nim\u00b7mer", "dr\u00fc\u00b7cket", "sie", "das", "Al\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Nun und nimmer \u00e4ngstet sie die Nahrung;", "tokens": ["Nun", "und", "nim\u00b7mer", "\u00e4ngs\u00b7tet", "sie", "die", "Nah\u00b7rung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Trunken, von ein Wenig Thaue trunken,", "tokens": ["Trun\u00b7ken", ",", "von", "ein", "We\u00b7nig", "Thau\u00b7e", "trun\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "PIAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Singen sie geh\u00f6rt und ungeh\u00f6ret.", "tokens": ["Sin\u00b7gen", "sie", "ge\u00b7h\u00f6rt", "und", "un\u00b7ge\u00b7h\u00f6\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "KON", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Wie sie denn auch, also lehret Plato,", "tokens": ["Wie", "sie", "denn", "auch", ",", "al\u00b7so", "leh\u00b7ret", "Pla\u00b7to", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "$,", "ADV", "VVFIN", "NE", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Ihren Musen treue Nachricht bringen,", "tokens": ["Ih\u00b7ren", "Mu\u00b7sen", "treu\u00b7e", "Nach\u00b7richt", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Was hier dieser Knabe, jener Sch\u00e4fer", "tokens": ["Was", "hier", "die\u00b7ser", "Kna\u00b7be", ",", "je\u00b7ner", "Sch\u00e4\u00b7fer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "ADV", "PDAT", "NN", "$,", "PDAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Singt und sang und k\u00fcnftig singen werde.", "tokens": ["Singt", "und", "sang", "und", "k\u00fcnf\u00b7tig", "sin\u00b7gen", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "ADJD", "KON", "ADJD", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Ach, Ihr s\u00fc\u00dfen Landverw\u00fcsterinnen,", "tokens": ["Ach", ",", "Ihr", "s\u00fc\u00b7\u00dfen", "Land\u00b7ver\u00b7w\u00fcs\u00b7te\u00b7rin\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Steiget noch einmal vom Himmel nieder,", "tokens": ["Stei\u00b7get", "noch", "ein\u00b7mal", "vom", "Him\u00b7mel", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Holde Musen, steigt herab und hemmet", "tokens": ["Hol\u00b7de", "Mu\u00b7sen", ",", "steigt", "her\u00b7ab", "und", "hem\u00b7met"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "VVFIN", "ADV", "KON", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Eurer ew'gen Lieder ew'ge Wirkung!", "tokens": ["Eu\u00b7rer", "ew'\u00b7gen", "Lie\u00b7der", 