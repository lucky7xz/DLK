anten,", "tokens": ["Rei\u00b7sen\u00b7de", "Kund\u00b7schaf\u00b7ter", ",", "De\u00b7cla\u00b7man\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "$,"], "meter": "+--++-+-+-", "measure": "dactylic.init"}}, "stanza.24": {"line.1": {"text": "Seht, o sehet ihre M\u00fch und Arbeit,", "tokens": ["Seht", ",", "o", "se\u00b7het", "ih\u00b7re", "M\u00fch", "und", "Ar\u00b7beit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "FM", "VVFIN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ihren Hunger, ihre hei\u00dfe Sanglust!", "tokens": ["Ih\u00b7ren", "Hun\u00b7ger", ",", "ih\u00b7re", "hei\u00b7\u00dfe", "Sang\u00b7lust", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wandelt sie! \u2013 Jedoch wozu die Wandlung?", "tokens": ["Wan\u00b7delt", "sie", "!", "\u2013", "Je\u00b7doch", "wo\u00b7zu", "die", "Wand\u00b7lung", "?"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "$(", "ADV", "PWAV", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sie sind jetzt schon wie die Heimchen selig.", "tokens": ["Sie", "sind", "jetzt", "schon", "wie", "die", "Heim\u00b7chen", "se\u00b7lig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "KOKOM", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}