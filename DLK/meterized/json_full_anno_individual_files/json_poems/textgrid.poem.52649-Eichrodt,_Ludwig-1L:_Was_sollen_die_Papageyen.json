{"textgrid.poem.52649": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was sollen die Papageyen", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was sollen die Papageyen", "tokens": ["Was", "sol\u00b7len", "die", "Pa\u00b7pa\u00b7ge\u00b7yen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im deutschen Dichterwald?", "tokens": ["Im", "deut\u00b7schen", "Dich\u00b7ter\u00b7wald", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie w\u00e4lschen und sie schreien", "tokens": ["Sie", "w\u00e4l\u00b7schen", "und", "sie", "schrei\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ganz kannibalisch bald.", "tokens": ["Ganz", "kan\u00b7ni\u00b7ba\u00b7lisch", "bald", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wollt ihr den Verstand verlieren,", "tokens": ["Wollt", "ihr", "den", "Ver\u00b7stand", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So gehet ihr gar nicht irr,", "tokens": ["So", "ge\u00b7het", "ihr", "gar", "nicht", "irr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Geht nur in den Wald spazieren", "tokens": ["Geht", "nur", "in", "den", "Wald", "spa\u00b7zie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und h\u00f6rt das Stimmengewirr!", "tokens": ["Und", "h\u00f6rt", "das", "Stim\u00b7men\u00b7ge\u00b7wirr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Sie holen die fremden Laute", "tokens": ["Sie", "ho\u00b7len", "die", "frem\u00b7den", "Lau\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Aus allen Winkeln der Welt,", "tokens": ["Aus", "al\u00b7len", "Win\u00b7keln", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und wer sich wenig erbaute,", "tokens": ["Und", "wer", "sich", "we\u00b7nig", "er\u00b7bau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wird mit Kommentaren gequ\u00e4lt.", "tokens": ["Wird", "mit", "Kom\u00b7men\u00b7ta\u00b7ren", "ge\u00b7qu\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Ihr wolltet euch wohl erquicken", "tokens": ["Ihr", "woll\u00b7tet", "euch", "wohl", "er\u00b7qui\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+--+