, "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ihr m\u00fcsset die Kunst erlernen,", "tokens": ["Ihr", "m\u00fcs\u00b7set", "die", "Kunst", "er\u00b7ler\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Zu gehn aus euch heraus,", "tokens": ["Zu", "gehn", "aus", "euch", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr m\u00fc\u00dft euch erst entfernen,", "tokens": ["Ihr", "m\u00fc\u00dft", "euch", "erst", "ent\u00b7fer\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dann seid ihr recht zu Haus!", "tokens": ["Dann", "seid", "ihr", "recht", "zu", "Haus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Dort thun es die heimischen Spatzen", "tokens": ["Dort", "thun", "es", "die", "hei\u00b7mi\u00b7schen", "Spat\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Nachzwitschern mit saurer M\u00fch,", "tokens": ["Nach\u00b7zwit\u00b7schern", "mit", "sau\u00b7rer", "M\u00fch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und weil sie Unsinn schwatzen,