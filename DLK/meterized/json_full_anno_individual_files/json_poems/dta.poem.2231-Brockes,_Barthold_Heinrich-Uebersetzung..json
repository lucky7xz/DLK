{"dta.poem.2231": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Uebersetzung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was eigentlich uns die Natur geschenket, um begl\u00fcckt", "tokens": ["Was", "ei\u00b7gent\u00b7lich", "uns", "die", "Na\u00b7tur", "ge\u00b7schen\u00b7ket", ",", "um", "be\u00b7gl\u00fcckt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "ADV", "PPER", "ART", "NN", "VVPP", "$,", "KOUI", "VVPP"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "zu seyn,", "tokens": ["zu", "seyn", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VAINF", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Und da\u00df wir es genie\u00dfen sollen,", "tokens": ["Und", "da\u00df", "wir", "es", "ge\u00b7nie\u00b7\u00dfen", "sol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das ist die Gegenwart allein.", "tokens": ["Das", "ist", "die", "Ge\u00b7gen\u00b7wart", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir gleichen widersinn\u2019schen Kindern, die dieses Gut", "tokens": ["Wir", "glei\u00b7chen", "wi\u00b7der\u00b7sinn'\u00b7schen", "Kin\u00b7dern", ",", "die", "die\u00b7ses", "Gut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADJA", "ADJA", "NN", "$,", "PRELS", "PDAT", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "nicht kosten wollen.", "tokens": ["nicht", "kos\u00b7ten", "wol\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}