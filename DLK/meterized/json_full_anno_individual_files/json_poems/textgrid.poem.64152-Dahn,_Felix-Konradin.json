{"textgrid.poem.64152": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Konradin", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was steigt herab der Alpen Hang,", "tokens": ["Was", "steigt", "her\u00b7ab", "der", "Al\u00b7pen", "Hang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Waffenglanz, mit Harfenklang,", "tokens": ["Im", "Waf\u00b7fen\u00b7glanz", ",", "mit", "Har\u00b7fen\u00b7klang", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das jugendsch\u00f6ne Haupt umrollt", "tokens": ["Das", "ju\u00b7gend\u00b7sch\u00f6\u00b7ne", "Haupt", "um\u00b7rollt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von sonnenheller Locken Gold?", "tokens": ["Von", "son\u00b7nen\u00b7hel\u00b7ler", "Lo\u00b7cken", "Gold", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wer ist der J\u00fcngling ohne Fehl?", "tokens": ["Wer", "ist", "der", "J\u00fcng\u00b7ling", "oh\u00b7ne", "Fehl", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ist's Sankt Georg, ist's Gabriel?", "tokens": ["Ist's", "Sankt", "Ge\u00b7org", ",", "ist's", "Ga\u00b7bri\u00b7el", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct