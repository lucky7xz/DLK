{"textgrid.poem.41815": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "Zur Kriegserkl\u00e4rung", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schon in der Jugend Morgentagen", "tokens": ["Schon", "in", "der", "Ju\u00b7gend", "Mor\u00b7gen\u00b7ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fchlt ich mich als ein Kind der Zeit", "tokens": ["F\u00fchlt", "ich", "mich", "als", "ein", "Kind", "der", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "KOUS", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ihrem Hoffen, ihren Fragen", "tokens": ["Und", "ih\u00b7rem", "Hof\u00b7fen", ",", "ih\u00b7ren", "Fra\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "War stets mein Wort, mein Lied geweiht.", "tokens": ["War", "stets", "mein", "Wort", ",", "mein", "Lied", "ge\u00b7weiht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mein ganzes Herz, mein ganzes Leben", "tokens": ["Mein", "gan\u00b7zes", "Herz", ",", "mein", "gan\u00b7zes", "Le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "War nur erf\u00fcllt von einem Ziel:", "tokens": ["War", "nur", "er\u00b7f\u00fcllt", "von", "ei\u00b7nem", "Ziel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mich an mein Volk dahin zu geben,", "tokens": ["Mich", "an", "mein", "Volk", "da\u00b7hin", "zu", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "PAV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Welch' Schicksalslos mir sonst auch fiel.", "tokens": ["Welch'", "Schick\u00b7sals\u00b7los", "mir", "sonst", "auch", "fiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Schon als dies Volk noch harmlos tr\u00e4umte,", "tokens": ["Schon", "als", "dies", "Volk", "noch", "harm\u00b7los", "tr\u00e4um\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PDS", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Noch still in Ketten lag und schlief \u2013", "tokens": ["Noch", "still", "in", "Ket\u00b7ten", "lag", "und", "schlief", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als Morgenrot den Himmel s\u00e4umte", "tokens": ["Als", "Mor\u00b7gen\u00b7rot", "den", "Him\u00b7mel", "s\u00e4um\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Lerchenlied nach Freiheit rief: \u2013", "tokens": ["Und", "Ler\u00b7chen\u00b7lied", "nach", "Frei\u00b7heit", "rief", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "NN", "APPR", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da rief ich mit und rief so lange", "tokens": ["Da", "rief", "ich", "mit", "und", "rief", "so", "lan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "KON", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bis da\u00df er kam, der Freiheit Tag \u2013", "tokens": ["Bis", "da\u00df", "er", "kam", ",", "der", "Frei\u00b7heit", "Tag", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "VVFIN", "$,", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch nicht zertreten ward die Schlange,", "tokens": ["Doch", "nicht", "zer\u00b7tre\u00b7ten", "ward", "die", "Schlan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die zuckend nur zu Boden lag.", "tokens": ["Die", "zu\u00b7ckend", "nur", "zu", "Bo\u00b7den", "lag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "In einer Zeit voll Schmach und Leiden,", "tokens": ["In", "ei\u00b7ner", "Zeit", "voll", "Schmach", "und", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die f\u00fcr uns all' verh\u00e4ngnisvoll,", "tokens": ["Die", "f\u00fcr", "uns", "all'", "ver\u00b7h\u00e4ng\u00b7nis\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu Harfenspiel an Trauerweiden", "tokens": ["Zu", "Har\u00b7fen\u00b7spiel", "an", "Trau\u00b7er\u00b7wei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch noch mein tr\u00f6stend Lied erscholl!", "tokens": ["Doch", "noch", "mein", "tr\u00f6s\u00b7tend", "Lied", "er\u00b7scholl", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "ADJD", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und nun, da Deutschland sich erhoben,", "tokens": ["Und", "nun", ",", "da", "Deutschland", "sich", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "NE", "PRF", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ein ", "tokens": ["Ein"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Die deutschen Klingen zu erproben", "tokens": ["Die", "deut\u00b7schen", "Klin\u00b7gen", "zu", "er\u00b7pro\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Feinde stolz entgegengeht,", "tokens": ["Dem", "Fein\u00b7de", "stolz", "ent\u00b7ge\u00b7gen\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Dem Feind, vom zitternden Tyrannen", "tokens": ["Dem", "Feind", ",", "vom", "zit\u00b7tern\u00b7den", "Ty\u00b7ran\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu einem frechen Krieg gehetzt,", "tokens": ["Zu", "ei\u00b7nem", "fre\u00b7chen", "Krieg", "ge\u00b7hetzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der, um die Freiheit neu zu bannen", "tokens": ["Der", ",", "um", "die", "Frei\u00b7heit", "neu", "zu", "ban\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "$,", "KOUI", "ART", "NN", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sogar das V\u00f6lkerrecht verletzt:", "tokens": ["So\u00b7gar", "das", "V\u00f6l\u00b7ker\u00b7recht", "ver\u00b7letzt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Nun gilt es Schlachtenlieder singen", "tokens": ["Nun", "gilt", "es", "Schlach\u00b7ten\u00b7lie\u00b7der", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem deutschen Vaterland geweiht,", "tokens": ["Dem", "deut\u00b7schen", "Va\u00b7ter\u00b7land", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun gilt's, die deutschen Fahnen schwingen", "tokens": ["Nun", "gilt's", ",", "die", "deut\u00b7schen", "Fah\u00b7nen", "schwin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Dienst der neuen, bessern Zeit.", "tokens": ["Im", "Dienst", "der", "neu\u00b7en", ",", "bes\u00b7sern", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "O, da\u00df sie endlich, endlich nahte", "tokens": ["O", ",", "da\u00df", "sie", "end\u00b7lich", ",", "end\u00b7lich", "nah\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "KOUS", "PPER", "ADV", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und sei sie auch mit Blut getauft,", "tokens": ["Und", "sei", "sie", "auch", "mit", "Blut", "ge\u00b7tauft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Freiheit wird auf keinem Pfade", "tokens": ["Die", "Frei\u00b7heit", "wird", "auf", "kei\u00b7nem", "Pfa\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu teuer und zu schwer erkauft.", "tokens": ["Zu", "teu\u00b7er", "und", "zu", "schwer", "er\u00b7kauft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "KON", "PTKA", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Die Freiheit \u2013 wollt' es nie vergessen \u2013", "tokens": ["Die", "Frei\u00b7heit", "\u2013", "wollt'", "es", "nie", "ver\u00b7ges\u00b7sen", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "VMFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auch Deutschlands Einheit hat nur Wert,", "tokens": ["Auch", "Deutschlands", "Ein\u00b7heit", "hat", "nur", "Wert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "NN", "VAFIN", "ADV", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn wir sie nach dem Segen messen,", "tokens": ["Wenn", "wir", "sie", "nach", "dem", "Se\u00b7gen", "mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den sie dem ganzen Volk beschert.", "tokens": ["Den", "sie", "dem", "gan\u00b7zen", "Volk", "be\u00b7schert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "O, deutsches Volk \u2013 auf, auf zum Siege", "tokens": ["O", ",", "deut\u00b7sches", "Volk", "\u2013", "auf", ",", "auf", "zum", "Sie\u00b7ge"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "ADJA", "NN", "$(", "PTKVZ", "$,", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und st\u00fcrze Frankreichs Kaiserthron", "tokens": ["Und", "st\u00fcr\u00b7ze", "Fran\u00b7kreichs", "Kai\u00b7ser\u00b7thron"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text":