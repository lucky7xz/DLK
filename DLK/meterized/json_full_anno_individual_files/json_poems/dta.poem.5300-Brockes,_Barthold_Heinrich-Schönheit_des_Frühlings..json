{"dta.poem.5300": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Sch\u00f6nheit des Fr\u00fchlings.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Indem ich hier von einer H\u00f6he,", "tokens": ["In\u00b7dem", "ich", "hier", "von", "ei\u00b7ner", "H\u00f6\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und zwar zur holden Fr\u00fchlings-Zeit,", "tokens": ["Und", "zwar", "zur", "hol\u00b7den", "Fr\u00fch\u00b7lings\u00b7Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verschiedne G\u00e4rten \u00fcbersehe,", "tokens": ["Ver\u00b7schied\u00b7ne", "G\u00e4r\u00b7ten", "\u00fc\u00b7ber\u00b7se\u00b7he", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erstaun\u2019 ich ob der Lieblichkeit,", "tokens": ["Er\u00b7staun'", "ich", "ob", "der", "Lieb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und ob dem bl\u00fchenden Gepr\u00e4nge", "tokens": ["Und", "ob", "dem", "bl\u00fc\u00b7hen\u00b7den", "Ge\u00b7pr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Des Fr\u00fchlings Kleides der Natur,", "tokens": ["Des", "Fr\u00fch\u00b7lings", "Klei\u00b7des", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In so verschiedner Pracht, Glantz, Farben und Figur.", "tokens": ["In", "so", "ver\u00b7schied\u00b7ner", "Pracht", ",", "Glantz", ",", "Far\u00b7ben", "und", "Fi\u00b7gur", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Es zeigt die ungezehlte Menge", "tokens": ["Es", "zeigt", "die", "un\u00b7ge\u00b7zehl\u00b7te", "Men\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Der Wipfel, die ich von dem Schnee,", "tokens": ["Der", "Wip\u00b7fel", ",", "die", "ich", "von", "dem", "Schnee", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.10": {"text": "Der holden Bl\u00fchte schimmern seh,", "tokens": ["Der", "hol\u00b7den", "Bl\u00fch\u00b7te", "schim\u00b7mern", "seh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ein angenehm mit gr\u00fcn vermischt Gewebe,", "tokens": ["Ein", "an\u00b7ge\u00b7nehm", "mit", "gr\u00fcn", "ver\u00b7mischt", "Ge\u00b7we\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ADJD", "VVPP", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Worin das Weisse, bald das Gr\u00fcn,", "tokens": ["Wo\u00b7rin", "das", "Weis\u00b7se", ",", "bald", "das", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Das Gr\u00fcn hinwiederum, nicht minder k\u00fchn,", "tokens": ["Das", "Gr\u00fcn", "hin\u00b7wie\u00b7de\u00b7rum", ",", "nicht", "min\u00b7der", "k\u00fchn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Das Weisse zu besiegen schien.", "tokens": ["Das", "Weis\u00b7se", "zu", "be\u00b7sie\u00b7gen", "schien", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Nicht zu beschreiben ist, wie s\u00fc\u00df", "tokens": ["Nicht", "zu", "be\u00b7schrei\u00b7ben", "ist", ",", "wie", "s\u00fc\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "PTKZU", "VVINF", "VAFIN", "$,", "PWAV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Die angenehm gemischte Sch\u00f6nheit lie\u00df:", "tokens": ["Die", "an\u00b7ge\u00b7nehm", "ge\u00b7mischte", "Sch\u00f6n\u00b7heit", "lie\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Ach wie so lieblich, glatt und zart und frisch und sch\u00f6n", "tokens": ["Ach", "wie", "so", "lieb\u00b7lich", ",", "glatt", "und", "zart", "und", "frisch", "und", "sch\u00f6n"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "ADV", "ADJD", "$,", "ADJD", "KON", "ADJD", "KON", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ist dort das junge Laub auf Linden,", "tokens": ["Ist", "dort", "das", "jun\u00b7ge", "Laub", "auf", "Lin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Und andern B\u00e4umen, anzusehn!", "tokens": ["Und", "an\u00b7dern", "B\u00e4u\u00b7men", ",", "an\u00b7zu\u00b7sehn", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "VVIZU", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Seht, wie die Wipfel sich so zierlich r\u00fcnden,", "tokens": ["Seht", ",", "wie", "die", "Wip\u00b7fel", "sich", "so", "zier\u00b7lich", "r\u00fcn\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "NN", "PRF", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Indem von allererst geschlossnen zarten Zweigen,", "tokens": ["In\u00b7dem", "von", "al\u00b7le\u00b7rerst", "ge\u00b7schloss\u00b7nen", "zar\u00b7ten", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADV", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die gr\u00fcnen Spitzen, noch so schwach", "tokens": ["Die", "gr\u00fc\u00b7nen", "Spit\u00b7zen", ",", "noch", "so", "schwach"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Durch ihre Bl\u00e4tter-Last herab gezogen,", "tokens": ["Durch", "ih\u00b7re", "Bl\u00e4t\u00b7ter\u00b7Last", "her\u00b7ab", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Und recht als runde gr\u00fcne Bogen,", "tokens": ["Und", "recht", "als", "run\u00b7de", "gr\u00fc\u00b7ne", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Sich wie gew\u00f6lbet abwerts beugen,", "tokens": ["Sich", "wie", "ge\u00b7w\u00f6l\u00b7bet", "ab\u00b7werts", "beu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "KOKOM", "VVFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Sich Wolcken-f\u00f6rmig zeigen,", "tokens": ["Sich", "Wol\u00b7cken\u00b7f\u00f6r\u00b7mig", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "NE", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Und unvermercket nach und nach", "tokens": ["Und", "un\u00b7ver\u00b7mer\u00b7cket", "nach", "und", "nach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "KON", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Nur allererst sich aufwerts lencken.", "tokens": ["Nur", "al\u00b7le\u00b7rerst", "sich", "auf\u00b7werts", "len\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Erweget, wie sie sich so angenehm verschrencken,", "tokens": ["Er\u00b7we\u00b7get", ",", "wie", "sie", "sich", "so", "an\u00b7ge\u00b7nehm", "ver\u00b7schren\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "PRF", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}