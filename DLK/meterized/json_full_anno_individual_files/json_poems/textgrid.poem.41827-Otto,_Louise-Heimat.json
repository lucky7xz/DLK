{"textgrid.poem.41827": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "Heimat", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Viel tausend Menschen hat es sonst gegeben,", "tokens": ["Viel", "tau\u00b7send", "Men\u00b7schen", "hat", "es", "sonst", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die nie hinaus aus Stadt und Dorf gekommen,", "tokens": ["Die", "nie", "hin\u00b7aus", "aus", "Stadt", "und", "Dorf", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APZR", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im selben Haus da sie erwacht zum Leben,", "tokens": ["Im", "sel\u00b7ben", "Haus", "da", "sie", "er\u00b7wacht", "zum", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "KOUS", "PPER", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ist ihnen auch sein letzter Strahl verglommen.", "tokens": ["Ist", "ih\u00b7nen", "auch", "sein", "letz\u00b7ter", "Strahl", "ver\u00b7glom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Einf\u00f6rmig ward der Faden fortgesponnen", "tokens": ["Ein\u00b7f\u00f6r\u00b7mig", "ward", "der", "Fa\u00b7den", "fort\u00b7ge\u00b7spon\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und reichte niemals \u00fcber weite Grenzen,", "tokens": ["Und", "reich\u00b7te", "nie\u00b7mals", "\u00fc\u00b7ber", "wei\u00b7te", "Gren\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch eine Welt von Leiden und von Wonnen", "tokens": ["Doch", "ei\u00b7ne", "Welt", "von", "Lei\u00b7den", "und", "von", "Won\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Fand darin Raum in bunten Wechselt\u00e4nzen.", "tokens": ["Fand", "da\u00b7rin", "Raum", "in", "bun\u00b7ten", "Wech\u00b7sel\u00b7t\u00e4n\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das enge Haus hielt selbst den Sinn gebunden,", "tokens": ["Das", "en\u00b7ge", "Haus", "hielt", "selbst", "den", "Sinn", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Haus und Hof war allem lieb und teuer,", "tokens": ["Und", "Haus", "und", "Hof", "war", "al\u00b7lem", "lieb", "und", "teu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VAFIN", "PIS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Man f\u00fcrchtete des Heimweh's tiefe Wunden,", "tokens": ["Man", "f\u00fcrch\u00b7te\u00b7te", "des", "Heim\u00b7weh's", "tie\u00b7fe", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn man verlie\u00df des trauten Herdes Feuer. \u2013", "tokens": ["Wenn", "man", "ver\u00b7lie\u00df", "des", "trau\u00b7ten", "Her\u00b7des", "Feu\u00b7er", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "ART", "ADJA", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Das war vordem \u2013 da gab's noch weite Fernen \u2013", "tokens": ["Das", "war", "vor\u00b7dem", "\u2013", "da", "gab's", "noch", "wei\u00b7te", "Fer\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "$(", "ADV", "VVFIN", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da gab's noch enge festgeschlossne Kreise,", "tokens": ["Da", "gab's", "noch", "en\u00b7ge", "fest\u00b7ge\u00b7schloss\u00b7ne", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es mochte kaum ein Volk vom andren lernen", "tokens": ["Es", "moch\u00b7te", "kaum", "ein", "Volk", "vom", "an\u00b7dren", "ler\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "APPRART", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und jeder Gau bewahrte seine Weise.", "tokens": ["Und", "je\u00b7der", "Gau", "be\u00b7wahr\u00b7te", "sei\u00b7ne", "Wei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Jetzt aber dr\u00e4ngt's die Heimat zu verlassen", "tokens": ["Jetzt", "a\u00b7ber", "dr\u00e4ngt's", "die", "Hei\u00b7mat", "zu", "ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So jung als alt und zieht sie weit und weiter,", "tokens": ["So", "jung", "als", "alt", "und", "zieht", "sie", "weit", "und", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ADJD", "KON", "VVFIN", "PPER", "ADJD", "KON", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es wird der Dampf f\u00fcr ganze V\u00f6lkermassen", "tokens": ["Es", "wird", "der", "Dampf", "f\u00fcr", "gan\u00b7ze", "V\u00f6l\u00b7ker\u00b7mas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zum ruhelosen lockenden Begleiter.", "tokens": ["Zum", "ru\u00b7he\u00b7lo\u00b7sen", "lo\u00b7cken\u00b7den", "Be\u00b7glei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wohl ist es sch\u00f6n, die sch\u00f6ne Welt durchfliegen,", "tokens": ["Wohl", "ist", "es", "sch\u00f6n", ",", "die", "sch\u00f6\u00b7ne", "Welt", "durch\u00b7flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo immer neue Wunder sich erschlie\u00dfen", "tokens": ["Wo", "im\u00b7mer", "neu\u00b7e", "Wun\u00b7der", "sich", "er\u00b7schlie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADJA", "NN", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im Schauen und im Staunen sich zu wiegen,", "tokens": ["Im", "Schau\u00b7en", "und", "im", "Stau\u00b7nen", "sich", "zu", "wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Natur und Kunst begeistert zu genie\u00dfen.", "tokens": ["Na\u00b7tur", "und", "Kunst", "be\u00b7geis\u00b7tert", "zu", "ge\u00b7nie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Wohl ist es sch\u00f6n im fernen Land zu weilen", "tokens": ["Wohl", "ist", "es", "sch\u00f6n", "im", "fer\u00b7nen", "Land", "zu", "wei\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "APPRART", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Edle auch im fremden Volk erkennen.", "tokens": ["Das", "Ed\u00b7le", "auch", "im", "frem\u00b7den", "Volk", "er\u00b7ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit ihm das Streben nach dem H\u00f6chsten teilen", "tokens": ["Mit", "ihm", "das", "Stre\u00b7ben", "nach", "dem", "H\u00f6chs\u00b7ten", "tei\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Dienst der Menschheit sich verbunden nennen", "tokens": ["Im", "Dienst", "der", "Menschheit", "sich", "ver\u00b7bun\u00b7den", "nen\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "PRF", "VVPP", "VVINF"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Doch dreimal sch\u00f6ner wenn Erinnerungen,", "tokens": ["Doch", "drei\u00b7mal", "sch\u00f6\u00b7ner", "wenn", "E\u00b7rin\u00b7ne\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An solche Zeiten uns die Heimath schm\u00fccken", "tokens": ["An", "sol\u00b7che", "Zei\u00b7ten", "uns", "die", "Hei\u00b7math", "schm\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wenn der Boden, den wir selbst entsprungen", "tokens": ["Und", "wenn", "der", "Bo\u00b7den", ",", "den", "wir", "selbst", "ent\u00b7sprun\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Uns noch vermag wie einstens zu begl\u00fccken", "tokens": ["Uns", "noch", "ver\u00b7mag", "wie", "eins\u00b7tens", "zu", "be\u00b7gl\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "KOKOM", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Wohl ist's ein E l\u00fcck, das Wen'gen, ist beschieden", "tokens": ["Wohl", "ist's", "ein", "E", "l\u00fcck", ",", "das", "Wen'\u00b7gen", ",", "ist", "be\u00b7schie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKVZ", "$,", "ART", "NN", "$,", "VAFIN", "VVPP"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Wenn uns die St\u00e4tte wo wir einst geboren,", "tokens": ["Wenn", "uns", "die", "St\u00e4t\u00b7te", "wo", "wir", "einst", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PWAV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo wir getr\u00e4umt im holden Jugendfrieden,", "tokens": ["Wo", "wir", "ge\u00b7tr\u00e4umt", "im", "hol\u00b7den", "Ju\u00b7gend\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Alter noch als Heimat unverloren.", "tokens": ["Im", "Al\u00b7ter", "noch", "als", "Hei\u00b7mat", "un\u00b7ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "KOUS", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Wenn wir, was da wir strebten und empfunden,", "tokens": ["Wenn", "wir", ",", "was", "da", "wir", "streb\u00b7ten", "und", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PWS", "KOUS", "PPER", "VVFIN", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auch in der Ferne weiter gro\u00df gezogen", "tokens": ["Auch", "in", "der", "Fer\u00b7ne", "wei\u00b7ter", "gro\u00df", "ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn, was wir hier gelobt in heilgen Stunden", "tokens": ["Wenn", ",", "was", "wir", "hier", "ge\u00b7lobt", "in", "heil\u00b7gen", "Stun\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "PRELS", "PPER", "ADV", "VVPP", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verleugnet nie in st\u00fcrmschen Lebenswogen.", "tokens": ["Ver\u00b7leug\u00b7net", "nie", "in", "st\u00fcrm\u00b7schen", "Le\u00b7bens\u00b7wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Wenn wir die Heimat so bewu\u00dft betreten,", "tokens": ["Wenn", "wir", "die", "Hei\u00b7mat", "so", "be\u00b7wu\u00dft", "be\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gedenkend gern so alt als neuer Zeiten,", "tokens": ["Ge\u00b7den\u00b7kend", "gern", "so", "alt", "als", "neu\u00b7er", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], 