{"textgrid.poem.26528": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Weh' dem, der da sein eig'nes Thun zu richten", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weh' dem, der da sein eig'nes Thun zu richten", "tokens": ["Weh'", "dem", ",", "der", "da", "sein", "eig'\u00b7nes", "Thun", "zu", "rich\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "$,", "PRELS", "ADV", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Begonnen hat! Dann z\u00e4hlt er zu den Kranken", "tokens": ["Be\u00b7gon\u00b7nen", "hat", "!", "Dann", "z\u00e4hlt", "er", "zu", "den", "Kran\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "$.", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und schaudernd f\u00fchlt er keimen den Gedanken:", "tokens": ["Und", "schau\u00b7dernd", "f\u00fchlt", "er", "kei\u00b7men", "den", "Ge\u00b7dan\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sich selbst erkennen, hei\u00dft sich selbst vernichten.", "tokens": ["Sich", "selbst", "er\u00b7ken\u00b7nen", ",", "hei\u00dft", "sich", "selbst", "ver\u00b7nich\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVINF", "$,", "VVFIN", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Denn auf sein Wesen mu\u00df er stumm verzichten,", "tokens": ["Denn", "auf", "sein", "We\u00b7sen", "mu\u00df", "er", "stumm", "ver\u00b7zich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wie die liebsten Hoffnungen ihm sanken,", "tokens": ["Und", "wie", "die", "liebs\u00b7ten", "Hoff\u00b7nun\u00b7gen", "ihm", "san\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Lebt er dahin in haltlos \u00f6dem Schwanken", "tokens": ["Lebt", "er", "da\u00b7hin", "in", "halt\u00b7los", "\u00f6\u00b7dem", "Schwan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PAV", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und w\u00fcnscht den Tod herbei, die Qual zu schlichten.", "tokens": ["Und", "w\u00fcnscht", "den", "Tod", "her\u00b7bei", ",", "die", "Qual", "zu", "schlich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Darum frohlockt nicht so beim Weiterschreiten!", "tokens": ["Da\u00b7rum", "froh\u00b7lockt", "nicht", "so", "beim", "Wei\u00b7ter\u00b7schrei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PTKNEG", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Dasein ist ein gro\u00dfes Sichbesinnen \u2013", "tokens": ["Das", "D