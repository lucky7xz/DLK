{"textgrid.poem.33660": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Erste Lieder", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Manches Lied von meinem Lieben,", "tokens": ["Man\u00b7ches", "Lied", "von", "mei\u00b7nem", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das hervordrang ungehemmt,", "tokens": ["Das", "her\u00b7vor\u00b7drang", "un\u00b7ge\u00b7hemmt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eh' ich's ganz noch aufgeschrieben,", "tokens": ["Eh'", "ich's", "ganz", "noch", "auf\u00b7ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "War's von Tr\u00e4nen weggeschwemmt.", "tokens": ["Wa\u00b7r's", "von", "Tr\u00e4\u00b7nen", "weg\u00b7ge\u00b7schwemmt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Manches hab' ich hingegeben", "tokens": ["Man\u00b7ches", "hab'", "ich", "hin\u00b7ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dem und jenem Menschenkind,", "tokens": ["Dem", "und", "je\u00b7nem", "Men\u00b7schen\u00b7kind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "KON", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Manches Blatt aus meinem Leben", "tokens": ["Man\u00b7ches", "Blatt", "aus", "mei\u00b7nem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch zum Spiele f\u00fcr den Wind.", "tokens": ["Auch", "zum", "Spie\u00b7le", "f\u00fcr", "den", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Die der ersten Sehnsuchtflamme", "tokens": ["Die", "der", "ers\u00b7ten", "Sehn\u00b7sucht\u00b7flam\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wurden Asche, Glut ihr Grab;", "tokens": ["Wur\u00b7den", "A\u00b7sche", ",", "Glut", "ihr", "Grab", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "An des Lorbeers jungem Stamme", "tokens": ["An", "des", "Lor\u00b7beers", "jun\u00b7gem", "Stam\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Flog das Feuer auf und ab.", "tokens": ["Flog", "das", "Feu\u00b7er", "auf", "und", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Flog empor und leuchtet wieder", "tokens": ["Flog", "em\u00b7por", "und", "leuch\u00b7tet", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos"