{"textgrid.poem.46234": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "An H. Martin Opitzen, teutschen poeten", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jedem mein ohr, hand, mund schier m\u00fcd, die schwere plagen", "tokens": ["Je\u00b7dem", "mein", "ohr", ",", "hand", ",", "mund", "schier", "m\u00fcd", ",", "die", "schwe\u00b7re", "pla\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN", "$,", "NN", "$,", "FM.la", "FM.la", "FM.la", "$,", "ART", "ADJA", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die diser grosse krieg mit schwert, pest, hunger, brand", "tokens": ["die", "di\u00b7ser", "gros\u00b7se", "krieg", "mit", "schwert", ",", "pest", ",", "hun\u00b7ger", ",", "brand"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "APPR", "VVFIN", "$,", "VVFIN", "$,", "ADJA", "$,", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und unerh\u00f6rter wut auf unser vatterland", "tokens": ["und", "un\u00b7er\u00b7h\u00f6r\u00b7ter", "wut", "auf", "un\u00b7ser", "vat\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "ausgie\u00dfet, ohn abla\u00df zu h\u00f6ren, schreiben, klagen:", "tokens": ["aus\u00b7gie\u00b7\u00dfet", ",", "ohn", "ab\u00b7la\u00df", "zu", "h\u00f6\u00b7ren", ",", "schrei\u00b7ben", ",", "kla\u00b7gen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "KOUI", "ADJD", "PTKZU", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+++-+-+-+-", "measure": "unknown.measure.septa"}, "line.5": {"text": "Da ward mit wunder mir und mit wohn f\u00fcrgetragen,", "tokens": ["Da", "ward", "mit", "wun\u00b7der", "mir", "und", "mit", "wohn", "f\u00fcr\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "NN", "PPER", "KON", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "mein Opitz, deiner lieb und freindschaft wertes pfand,", "tokens": ["mein", "O\u00b7pitz", ",", "dei\u00b7ner", "lieb", "und", "freind\u00b7schaft", "wer\u00b7tes", "pfand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJD", "KON", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "pfand, welches mir alsbald die feder aus der hand", "tokens": ["pfand", ",", "wel\u00b7ches", "mir", "als\u00b7bald", "die", "fe\u00b7der", "aus", "der", "hand"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PRELS", "PPER", "ADV", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "und aus dem mund und geist die klag und leid geschlagen;", "tokens": ["und", "aus", "dem", "mund", "und", "geist", "die", "klag", "und", "leid", "ge\u00b7schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "KON", "VVFIN", "ART", "NN", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dan ja dein orgelstreich und deiner harpfen klang", "tokens": ["Dan", "ja", "dein", "or\u00b7gel\u00b7streich", "und", "dei\u00b7ner", "harp\u00b7fen", "klang"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPOSAT", "ADJD", "KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "so lieblich das geh\u00f6r und herz zugleich ber\u00fchren,", "tokens": ["so", "lieb\u00b7lich", "das", "ge\u00b7h\u00f6r", "und", "herz", "zu\u00b7gleich", "be\u00b7r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "KON", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "da\u00df, wer (sinnreich) mit mir erforschet ihren zwang,", "tokens": ["da\u00df", ",", "wer", "(", "sinn\u00b7reich", ")", "mit", "mir", "er\u00b7for\u00b7schet", "ih\u00b7ren", "zwang", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWS", "$(", "ADJD", "$(", "APPR", "PPER", "VVFIN", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der kan nichts dan dein werk und wort zu herzen f\u00fchren", "tokens": ["Der", "kan", "nichts", "dan", "dein", "werk", "und", "wort", "zu", "her\u00b7zen", "f\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PIS", "ADV", "PPOSAT", "NN", "KON", "NN", "PTKZU", "VVINF", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "und sein mund mu\u00df dich bald mit einem lobgesang", "tokens": ["und", "sein", "mund", "mu\u00df", "dich", "bald", "mit", "ei\u00b7nem", "lob\u00b7ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-++--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "und seine hand dein haupt mit lorberzweigen zieren.", "tokens": ["und", "sei\u00b7ne", "hand", "dein", "haupt", "mit", "lor\u00b7ber\u00b7zwei\u00b7gen", "zie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PPOSAT", "NN", "APPR", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Jedem mein ohr, hand, mund schier m\u00fcd, die schwere plagen", "tokens": ["Je\u00b7dem", "mein", "ohr", ",", "hand", ",", "mund", "schier", "m\u00fcd", ",", "die", "schwe\u00b7re", "pla\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN", "$,", "NN", "$,", "FM.la", "FM.la", "FM.la", "$,", "ART", "ADJA", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die diser grosse krieg mit schwert, pest, hunger, brand", "tokens": ["die", "di\u00b7ser", "gros\u00b7se", "krieg", "mit", "schwert", ",", "pest", ",", "hun\u00b7ger", ",", "brand"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "APPR", "VVFIN", "$,", "VVFIN", "$,", "ADJA", "$,", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und unerh\u00f6rter wut auf unser vatterland", "tokens": ["und", "un\u00b7er\u00b7h\u00f6r\u00b7ter", "wut", "auf", "un\u00b7ser", "vat\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "ausg