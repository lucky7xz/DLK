{"textgrid.poem.63826": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Sermione", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbvon allen Inseln, Sirmio, und Halbinseln", "tokens": ["\u00bb", "von", "al\u00b7len", "In\u00b7seln", ",", "Sir\u00b7mio", ",", "und", "Hal\u00b7bin\u00b7seln"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["$(", "APPR", "PIAT", "NN", "$,", "NE", "$,", "KON", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mein Augenstern, so viel' in klaren Landseen", "tokens": ["Mein", "Au\u00b7gens\u00b7tern", ",", "so", "viel'", "in", "kla\u00b7ren", "Land\u00b7seen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ADV", "PIS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Meeresweite rings der Wassergott h\u00fctet,", "tokens": ["Und", "Mee\u00b7res\u00b7wei\u00b7te", "rings", "der", "Was\u00b7ser\u00b7gott", "h\u00fc\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wie froh erblick' ich, wie zufrieden dich wieder!\u00ab", "tokens": ["Wie", "froh", "er\u00b7blick'", "ich", ",", "wie", "zu\u00b7frie\u00b7den", "dich", "wie\u00b7der", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "$,", "PWAV", "ADJD", "PPER", "ADV", "$.", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Und wem, im Fr\u00fchling landend an Catulls Eiland,", "tokens": ["Und", "wem", ",", "im", "Fr\u00fch\u00b7ling", "lan\u00b7dend", "an", "Ca\u00b7tulls", "Ei\u00b7land", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "$,", "APPRART", "NN", "ADJD", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Entschl\u00fcpfte nicht gleich ihm ein Wonnesto\u00dfseufzer,", "tokens": ["Ent\u00b7schl\u00fcpf\u00b7te", "nicht", "gleich", "ihm", "ein", "Won\u00b7ne\u00b7sto\u00df\u00b7seuf\u00b7zer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "PPER", "ART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Darf er auch nicht wie er \u00bbim eignen Bett ausruhn\u00ab!", "tokens": ["Darf", "er", "auch", "nicht", "wie", "er", "\u00bb", "im", "eig\u00b7nen", "Bett", "aus\u00b7ruhn", "\u00ab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PTKNEG", "PWAV", "PPER", "$(", "APPRART", "ADJA", "NN", "VVIZU", "$(", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wie lieblich, hier im lichtgepflanzten \u00d6lwalde", "tokens": ["Wie", "lieb\u00b7lich", ",", "hier", "im", "licht\u00b7ge\u00b7pflanz\u00b7ten", "\u00d6l\u00b7wal\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "$,", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.9": {"text": "Hinwandeln, oder unter schattigen Laubkronen", "tokens": ["Hin\u00b7wan\u00b7deln", ",", "o\u00b7der", "un\u00b7ter", "schat\u00b7ti\u00b7gen", "Laub\u00b7kro\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+--++-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Des dunklen Lorbeers sich behaglich hinstrecken,", "tokens": ["Des", "dunk\u00b7len", "Lor\u00b7beers", "sich", "be\u00b7hag\u00b7lich", "hins\u00b7tre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Von Anemonen rings umbl\u00fcht und W\u00fcrzkr\u00e4utern!", "tokens": ["Von", "A\u00b7ne\u00b7mo\u00b7nen", "rings", "um\u00b7bl\u00fcht", "und", "W\u00fcrz\u00b7kr\u00e4u\u00b7tern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVPP", "KON", "NN", "$."], "meter": "-+-+-+-+-++-", "measure": "unknown.measure.hexa"}, "line.12": {"text": "Wie sch\u00f6n, durch Tr\u00fcmmer hochgew\u00f6lbter Torbogen,", "tokens": ["Wie", "sch\u00f6n", ",", "durch", "Tr\u00fcm\u00b7mer", "hoch\u00b7ge\u00b7w\u00f6lb\u00b7ter", "Tor\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.13": {"text": "Durch die der See heraufgl\u00e4nzt in Smaragdbl\u00e4ue,", "tokens": ["Durch", "die", "der", "See", "her\u00b7auf\u00b7gl\u00e4nzt", "in", "Sma\u00b7ragd\u00b7bl\u00e4u\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "VVPP", "APPR", "NN", "$,"], "meter": "---+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Hin\u00fcberschaun zu schneebedeckten Bergh\u00e4uptern,", "tokens": ["Hin\u00b7\u00fc\u00b7bersc\u00b7haun", "zu", "schnee\u00b7be\u00b7deck\u00b7ten", "Berg\u00b7h\u00e4up\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.15": {"text": "Die kahle Stirnen baden in Aprilsonne!", "tokens": ["Die", "kah\u00b7le", "Stir\u00b7nen", "ba\u00b7den", "in", "Ap\u00b7ril\u00b7son\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Nur die Zikade singt ihr schrilles Volksliedchen", "tokens": ["Nur", "die", "Zi\u00b7ka\u00b7de", "singt", "ihr", "schril\u00b7les", "Volks\u00b7lied\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.17": {"text": "Im Gras verborgen, sonst kein Laut, als einlullend", "tokens": ["Im", "Gras", "ver\u00b7bor\u00b7gen", ",", "sonst", "kein", "Laut", ",", "als", "ein\u00b7lul\u00b7lend"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "VVPP", "$,", "ADV", "PIAT", "NN", "$,", "KOUS", "VVPP"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Des gro\u00dfen Pans eint\u00f6nig tiefe Schnarcht\u00f6ne.", "tokens": ["Des", "gro\u00b7\u00dfen", "Pans", "ein\u00b7t\u00f6\u00b7nig", "tie\u00b7fe", "Schnarch\u00b7t\u00f6\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.19": {"text": "Hier mochte wohl der Dichter friedlich ausrasten", "tokens": ["Hier", "moch\u00b7te", "wohl", "der", "Dich\u00b7ter", "fried\u00b7lich", "aus\u00b7ras\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Vom Fieber Roms und zweifelhaften Liebschaften", "tokens": ["Vom", "Fie\u00b7ber", "Roms", "und", "zwei\u00b7fel\u00b7haf\u00b7ten", "Lieb\u00b7schaf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "NE", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-++-", "measure": "unknown.measure.hexa"}, "line.21": {"text": "Und seiner Lesbia Wankelmut und Untreue.", "tokens": ["Und", "sei\u00b7ner", "Les\u00b7bia", "Wan\u00b7kel\u00b7mut", "und", "Un\u00b7treu\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Doch diese Villa, ungeheuren Umfanges,", "tokens": ["Doch", "die\u00b7se", "Vil\u00b7la", ",", "un\u00b7ge\u00b7heu\u00b7ren", "Um\u00b7fan\u00b7ges", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Mit Hallen, B\u00e4dern, Pfeilern, des August w\u00fcrdig,", "tokens": ["Mit", "Hal\u00b7len", ",", "B\u00e4\u00b7dern", ",", "Pfei\u00b7lern", ",", "des", "Au\u00b7gust", "w\u00fcr\u00b7dig", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Die h\u00e4ttest du, Poete, dir erbaun lassen,", "tokens": ["Die", "h\u00e4t\u00b7test", "du", ",", "Poe\u00b7te", ",", "dir", "er\u00b7baun", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "$,", "NN", "$,", "PPER", "VVINF", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.25": {"text": "Der, nicht an Gunst der Gro\u00dfen dem Horaz \u00e4hnlich,", "tokens": ["Der", ",", "nicht", "an", "Gunst", "der", "Gro\u00b7\u00dfen", "dem", "Ho\u00b7raz", "\u00e4hn\u00b7lich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PTKNEG", "APPR", "NN", "ART", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "Den Freund Fabullus du zu Tisch dir einludest,", "tokens": ["Den", "Freund", "Fa\u00b7bul\u00b7lus", "du", "zu", "Tisch", "dir", "ein\u00b7lu\u00b7dest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "PPER", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.27": {"text": "Ein k\u00f6stlich Mahl verhei\u00dfend, wenn er selbst n\u00e4mlich", "tokens": ["Ein", "k\u00f6st\u00b7lich", "Mahl", "ver\u00b7hei\u00b7\u00dfend", ",", "wenn", "er", "selbst", "n\u00e4m\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "VVPP", "$,", "KOUS", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.28": {"text": "Das Essen und den Wein dazu sich mitbr\u00e4chte,", "tokens": ["Das", "Es\u00b7sen", "und", "den", "Wein", "da\u00b7zu", "sich", "mit\u00b7br\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "PAV", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.29": {"text": "Indem dein Beutel nur gef\u00fcllt mit Spinnweben?", "tokens": ["In\u00b7dem", "dein", "Beu\u00b7tel", "nur", "ge\u00b7f\u00fcllt", "mit", "Spinn\u00b7we\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "Man wei\u00df, wie damals Publikum und Buchh\u00e4ndler", "tokens": ["Man", "wei\u00df", ",", "wie", "da\u00b7mals", "Pub\u00b7li\u00b7kum", "und", "Buch\u00b7h\u00e4nd\u00b7ler"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "PWAV", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.31": {"text": "Ein B\u00e4ndchen Lyrik schlechter noch als heut zahlten,", "tokens": ["Ein", "B\u00e4nd\u00b7chen", "Ly\u00b7rik", "schlech\u00b7ter", "noch", "als", "heut", "zahl\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "ADV", "KOKOM", "ADV", "ADJA", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.32": {"text": "Und du zumal warst nimmer ein Erfolgj\u00e4ger.", "tokens": ["Und", "du", "zu\u00b7mal", "warst", "nim\u00b7mer", "ein", "Er\u00b7folg\u00b7j\u00e4\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+---+-++-+-", "measure": "dactylic.init"}, "line.33": {"text": "Nein, hier an deiner Lieblingsinsel Nordspitze", "tokens": ["Nein", ",", "hier", "an", "dei\u00b7ner", "Lieb\u00b7lings\u00b7in\u00b7sel", "Nord\u00b7spit\u00b7ze"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADV", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-++-+-", "measure": "unknown.measure.hexa"}, "line.34": {"text": "Besa\u00dfest du vielleicht ein schlichtes Landh\u00e4uschen,", "tokens": ["Be\u00b7sa\u00b7\u00dfest", "du", "viel\u00b7leicht", "ein", "schlich\u00b7tes", "Land\u00b7h\u00e4usc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+---+-+-+--", "measure": "dactylic.init"}, "line.35": {"text": "Im Erdgescho\u00df zwei Zimmer oder drei h\u00f6chstens,", "tokens": ["Im", "Erd\u00b7ge\u00b7scho\u00df", "zwei", "Zim\u00b7mer", "o\u00b7der", "drei", "h\u00f6chs\u00b7tens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "CARD", "NN", "KON", "CARD", "ADV", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.36": {"text": "Und eine Loggia mit den sch\u00f6nsten Ausblicken,", "tokens": ["Und", "ei\u00b7ne", "Log\u00b7gia", "mit", "den", "sch\u00f6ns\u00b7ten", "Aus\u00b7bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.37": {"text": "Und stiegst zum Bad hinunter in die Seefluten,", "tokens": ["Und", "stiegst", "zum", "Bad", "hin\u00b7un\u00b7ter", "in", "die", "See\u00b7flu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NE", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.38": {"text": "Die von den Gliedern wie vom Geist die Gluthitze", "tokens": ["Die", "von", "den", "Glie\u00b7dern", "wie", "vom", "Geist", "die", "Glut\u00b7hit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "KOKOM", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-++-", "measure": "unknown.measure.hexa"}, "line.39": {"text": "Der Weltstadt Rom samt ihrem Ehrgeiz absp\u00fclten.", "tokens": ["Der", "Welt\u00b7stadt", "Rom", "samt", "ih\u00b7rem", "Ehr\u00b7geiz", "ab\u00b7sp\u00fcl\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-++-", "measure": "unknown.measure.hexa"}, "line.40": {"text": "Doch was an Wohllaut dir die Welle zurauschte,", "tokens": ["Doch", "was", "an", "Wohl\u00b7laut", "dir", "die", "Wel\u00b7le", "zu\u00b7rauschte", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "NN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.41": {"text": "An reinem Balsam dir Natur ins Blut fl\u00f6\u00dfte,", "tokens": ["An", "rei\u00b7nem", "Bal\u00b7sam", "dir", "Na\u00b7tur", "ins", "Blut", "fl\u00f6\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Um das beneiden konnte dich dein Nachfolger,", "tokens": ["Um", "das", "be\u00b7nei\u00b7den", "konn\u00b7te", "dich", "dein", "Nach\u00b7fol\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "VMFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.43": {"text": "Der niederri\u00df dein H\u00e4uschen und die Prachtbauten", "tokens": ["Der", "nie\u00b7der\u00b7ri\u00df", "dein", "H\u00e4usc\u00b7hen", "und", "die", "Pracht\u00b7bau\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-++-", "measure": "unknown.measure.hexa"}, "line.44": {"text": "Mit hundert Sklaven prahlerisch hier auff\u00fchrte", "tokens": ["Mit", "hun\u00b7dert", "Skla\u00b7ven", "prah\u00b7le\u00b7risch", "hier", "auf\u00b7f\u00fchr\u00b7te"], "token_info": ["word", "word", "word", "word", 