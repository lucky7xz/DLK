e": "trochaic.tetra"}, "line.3": {"text": "Fromm gebeugt steht Jedermann;", "tokens": ["Fromm", "ge\u00b7beugt", "steht", "Je\u00b7der\u00b7mann", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "VVFIN", "PIS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit dem Christusbild, dem blassen,", "tokens": ["Mit", "dem", "Chris\u00b7tus\u00b7bild", ",", "dem", "blas\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kommen Knaben, Glocken klingen", "tokens": ["Kom\u00b7men", "Kna\u00b7ben", ",", "Glo\u00b7cken", "klin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "NN", "$,", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und Gott selber naht heran;", "tokens": ["Und", "Gott", "sel\u00b7ber", "naht", "he\u00b7ran", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-++-+-+", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Aber meine Nachbarn fassen", "tokens": ["A\u00b7ber", "mei\u00b7ne", "Nach\u00b7barn", "fas\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nach den Lampen rings und bringen", "tokens": ["Nach", "den", "Lam\u00b7pen", "rings", "und", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Sie zum Fenster, knieen dann.", "tokens": ["Sie", "zum", "Fens\u00b7ter", ",", "kni\u00b7e\u00b7en", "dann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "$,", "VVFIN", "ADV", "$."], "meter": "--+--+--", "measure": "anapaest.di.plus"}}, "stanza.8": {"line.1": {"text": "An die junge Brust sich schlagend,", "tokens": ["An", "die", "jun\u00b7ge", "Brust", "sich", "schla\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PRF", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sinken zu des Ew'gen Preise", "tokens": ["Sin\u00b7ken", "zu", "des", "Ew'\u00b7gen", "Prei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auch die Schwestern auf das Knie;", "tokens": ["Auch", "die", "Schwes\u00b7tern", "auf", "das", "Knie", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und, die helle Lampe tragend,", "tokens": ["Und", ",", "die", "hel\u00b7le", "Lam\u00b7pe", "tra\u00b7gend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kommt die Mutter still, die greise,", "tokens": ["Kommt", "die", "Mut\u00b7ter", "still", ",", "die", "grei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$,", "PRELS", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und sie stellt sie zwischen sie;", "tokens": ["Und", "sie", "stellt", "sie", "zwi\u00b7schen", "sie", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch der Baum, sie \u00fcberragend,", "tokens": ["Doch", "der", "Baum", ",", "sie", "\u00fc\u00b7berr\u00b7a\u00b7gend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Streut auf sie die Bl\u00fcten leise,", "tokens": ["Streut", "auf", "sie", "die", "Bl\u00fc\u00b7ten", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Die der Sommer ihm verlieh.", "tokens": ["Die", "der", "Som\u00b7mer", "ihm", "ver\u00b7lieh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}