{"textgrid.poem.33972": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Venerabile in der Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf benachbartem Balcone", "tokens": ["Auf", "be\u00b7nach\u00b7bar\u00b7tem", "Bal\u00b7co\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Sah ich, wenn die Nacht sich senkte,", "tokens": ["Sah", "ich", ",", "wenn", "die", "Nacht", "sich", "senk\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Oft zwei Schwestern traurig geh'n;", "tokens": ["Oft", "zwei", "Schwes\u00b7tern", "trau\u00b7rig", "geh'n", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch, wie nah' ich ihnen wohne,", "tokens": ["Doch", ",", "wie", "nah'", "ich", "ih\u00b7nen", "woh\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "VVFIN", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und wie d'rob mein Herz sich kr\u00e4nkte:", "tokens": ["Und", "wie", "d'\u00b7rob", "mein", "Herz", "sich", "kr\u00e4nk\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "PPOSAT", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Tags hab' ich sie nie geseh'n;", "tokens": ["Tags", "hab'", "ich", "sie", "nie", "ge\u00b7seh'n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nur mit seiner Flammenkrone,", "tokens": ["Nur", "mit", "sei\u00b7ner", "Flam\u00b7men\u00b7kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die er, wie in Feuer, tr\u00e4nkte,", "tokens": ["Die", "er", ",", "wie", "in", "Feu\u00b7er", ",", "tr\u00e4nk\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "$,", "PWAV", "APPR", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Sah ich den Granatbaum steh'n.", "tokens": ["Sah", "ich", "den", "Gra\u00b7nat\u00b7baum", "steh'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVFIN", "NE"], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Heute auch sind sie erschienen,", "tokens": ["Heu\u00b7te", "auch", "sind", "sie", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihre Kleider, ihre wei\u00dfen,", "tokens": ["Ih\u00b7re", "Klei\u00b7der", ",", "ih\u00b7re", "wei\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schimmern durch die Nacht, wie Licht;", "tokens": ["Schim\u00b7mern", "durch", "die", "Nacht", ",", "wie", "Licht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "PWAV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die D\u00fcfte zieh'n von ihnen", "tokens": ["Und", "die", "D\u00fcf\u00b7te", "zieh'n", "von", "ih\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Her zu mir, die sich beflei\u00dfen,", "tokens": ["Her", "zu", "mir", ",", "die", "sich", "be\u00b7flei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,", "PRELS", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zu erfrischen ihr Gesicht;", "tokens": ["Zu", "er\u00b7fri\u00b7schen", "ihr", "Ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nur die s\u00fc\u00dfen M\u00e4dchenmienen,", "tokens": ["Nur", "die", "s\u00fc\u00b7\u00dfen", "M\u00e4d\u00b7chen\u00b7mie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die den Himmel uns verhei\u00dfen,", "tokens": ["Die", "den", "Him\u00b7mel", "uns", "ver\u00b7hei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Nur ihr Antlitz, seh' ich nicht.", "tokens": ["Nur", "ihr", "Ant\u00b7litz", ",", "seh'", "ich", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Horch'! da zieht es durch die Gassen,", "tokens": ["Horch'", "!", "da", "zieht", "es", "durch", "die", "Gas\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Beten h\u00f6re ich und singen,", "tokens": ["Be\u00b7ten", "h\u00f6\u00b7re", "ich", "und", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Fromm gebeugt steht Jedermann;", "tokens": ["Fromm", "ge\u00b7beugt", "steht", "Je\u00b7der\u00b7mann", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "VVFIN", "PIS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit dem Christusbild, dem blassen,", "tokens": ["Mit", "dem", "Chris\u00b7tus\u00b7bild", ",", "dem", "blas\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kommen Knaben, Glocken klingen", "tokens": ["Kom\u00b7men", "Kna\u00b7ben", ",", "Glo\u00b7cken", "klin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "NN", "$,", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und Gott selber naht heran;", "tokens": ["Und", "Gott", "sel\u00b7ber", "naht", "he\u00b7ran", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-++-+-+", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Aber meine Nachbarn fassen", "tokens": ["A\u00b7ber", "mei\u00b7ne", "Nach\u00b7barn", "fas\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nach den Lampen rings und bringen", "tokens": ["Nach", "den", "Lam\u00b7pen", "rings", "und", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Sie zum Fenster, knieen dann.", "tokens": ["Sie", "zum", "Fens\u00b7ter", ",", "kni\u00b7e\u00b7en", "dann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "$,", "VVFIN", "ADV", "$."], "meter": "--+--+--", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "An die junge Brust sich schlagend,", "tokens": ["An", "die", "jun\u00b7ge", "Brust", "sich", "schla\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PRF", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sinken zu des Ew'gen Preise", "tokens": ["Sin\u00b7ken", "zu", "des", "Ew'\u00b7gen", "Prei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auch die Schwestern auf das Knie;", "tokens": ["Auch", "die", "Schwes\u00b7tern", "auf", "das", "Knie", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und, die helle Lampe tragend,", "tokens": ["Und", ",", "die", "hel\u00b7le", "Lam\u00b7pe", "tra\u00b7gend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kommt die Mutter still, die greise,", "tokens": ["Kommt", "die", "Mut\u00b7ter", "still", ",", "die", "grei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$,", "PRELS", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und sie stellt sie zwischen sie;", "tokens": ["Und", "sie", "stellt", "sie", "zwi\u00b7schen", "sie", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch der Baum, sie \u00fcberragend,", "tokens": ["Doch", "der", "Baum", ",", "sie", "\u00fc\u00b7berr\u00b7a\u00b7gend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Streut auf sie die Bl\u00fcten leise,", "tokens": ["Streut", "auf", "sie", "die", "Bl\u00fc\u00b7ten", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Die der Sommer ihm verlieh.", "tokens": ["Die", "der", "Som\u00b7mer", "ihm", "ver\u00b7lieh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Auf benachbartem Balcone", "tokens": ["Auf", "be\u00b7nach\u00b7bar\u00b7tem", "Bal\u00b7co\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Sah ich, wenn die Nacht sich senkte,", "tokens": ["Sah", "ich", ",", "wenn", "die", "Nacht", "sich", "senk