{"textgrid.poem.67573": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Die Erdbeeren", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Holde Erdent\u00f6chter,", "tokens": ["Hol\u00b7de", "Er\u00b7den\u00b7t\u00f6ch\u00b7ter", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Fr\u00fchlings fr\u00fche Kinder,", "tokens": ["Fr\u00fch\u00b7lings", "fr\u00fc\u00b7he", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Schon aus Sonnenvaters", "tokens": ["Schon", "aus", "Son\u00b7nen\u00b7va\u00b7ters"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Warmem Lebenshauche", "tokens": ["War\u00b7mem", "Le\u00b7bens\u00b7hau\u00b7che"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Und aus Mutter-Erden", "tokens": ["Und", "aus", "Mut\u00b7ter\u00b7Er\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "K\u00fchlem Schoo\u00df empfangen,", "tokens": ["K\u00fch\u00b7lem", "Schoo\u00df", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "K\u00fchle, s\u00fc\u00dfe Beeren!", "tokens": ["K\u00fch\u00b7le", ",", "s\u00fc\u00b7\u00dfe", "Bee\u00b7ren", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Wie sie dort im Grase", "tokens": ["Wie", "sie", "dort", "im", "Gra\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "APPRART", "NN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.2": {"text": "H\u00fcgelaufw\u00e4rts gl\u00fchen", "tokens": ["H\u00fc\u00b7gel\u00b7auf\u00b7w\u00e4rts", "gl\u00fc\u00b7hen"], "token_info": ["word", "word"], "pos": ["ADV", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und ins Gr\u00fcn err\u00f6then,", "tokens": ["Und", "ins", "Gr\u00fcn", "er\u00b7r\u00f6\u00b7then", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Jetzt den Wandrer lieblich", "tokens": ["Jetzt", "den", "Wand\u00b7rer", "lieb\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Locken, jetzt entschl\u00fcpfend", "tokens": ["Lo\u00b7cken", ",", "jetzt", "ent\u00b7schl\u00fcp\u00b7fend"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADV", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "T\u00e4uschen \u2013 Buhlerinnen,", "tokens": ["T\u00e4u\u00b7schen", "\u2013", "Buh\u00b7le\u00b7rin\u00b7nen", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$(", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Wie die Erdent\u00f6chter!", "tokens": ["Wie", "die", "Er\u00b7den\u00b7t\u00f6ch\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Ha, wie Vater Fr\u00fchlings", "tokens": ["Ha", ",", "wie", "Va\u00b7ter", "Fr\u00fch\u00b7lings"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Odem sie durchbalsamt,", "tokens": ["O\u00b7dem", "sie", "durch\u00b7bal\u00b7samt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und der Mutter Erde", "tokens": ["Und", "der", "Mut\u00b7ter", "Er\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "K\u00fchle sie erfrischet!", "tokens": ["K\u00fch\u00b7le", "sie", "er\u00b7fri\u00b7schet", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wie aus niederm Grase", "tokens": ["Wie", "aus", "nie\u00b7derm", "Gra\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Labung auf sie duften!", "tokens": ["La\u00b7bung", "auf", "sie", "duf\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Gl\u00fchen da wie Sterne!", "tokens": ["Gl\u00fc\u00b7hen", "da", "wie", "Ster\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KOKOM", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Sollet bald in Schaaren", "tokens": ["Sol\u00b7let", "bald", "in", "Schaa\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Lieblich schwimmen! \u2013 Sterne,", "tokens": ["Lieb\u00b7lich", "schwim\u00b7men", "!", "\u2013", "Ster\u00b7ne", ","], "token_info": ["word", "word", "punct", "punct", "word", "punct"], "pos": ["ADJD", "VVINF", "$.", "$(", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Jetzt in wei\u00dfer Unschuld,", "tokens": ["Jetzt", "in", "wei\u00b7\u00dfer", "Un\u00b7schuld", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Jetzt in goldnem Feuer", "tokens": ["Jetzt", "in", "gold\u00b7nem", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Sch\u00f6ngepaaret! Feuer,", "tokens": ["Sch\u00f6n\u00b7ge\u00b7paa\u00b7ret", "!", "Feu\u00b7er", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$.", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Unschuld! und der Liebe", "tokens": ["Un\u00b7schuld", "!", "und", "der", "Lie\u00b7be"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$.", "KON", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Und der Freude T\u00f6chter!", "tokens": ["Und", "der", "Freu\u00b7de", "T\u00f6ch\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Mir ein ganzer Fr\u00fchling,", "tokens": ["Mir", "ein", "gan\u00b7zer", "Fr\u00fch\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Mir ein ganzes Leben!", "tokens": ["Mir", "ein", "gan\u00b7zes", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Unschuld, Kraft und Freude,", "tokens": ["Un\u00b7schuld", ",", "Kraft", "und", "Freu\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "K\u00fchl' und S\u00fc\u00dfe! Rose", "tokens": ["K\u00fchl'", "und", "S\u00fc\u00b7\u00dfe", "!", "Ro\u00b7se"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["NE", "KON", "NN", "$.", "NE"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Ohne Stachel, Labung", "tokens": ["Oh\u00b7ne", "Sta\u00b7chel", ",", "La\u00b7bung"], "token_info": ["word", "word", "punct", "word"], "pos": ["APPR", "NN", "$,", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Ohne Felsenschlaube!", "tokens": ["Oh\u00b7ne", "Fel\u00b7sen\u00b7schlau\u00b7be", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Sch\u00f6n und tief im Grase!", "tokens": ["Sch\u00f6n", "und", "tief", "im", "Gra\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Mir ein ganzer Fr\u00fchling,", "tokens": ["Mir", "ein", "gan\u00b7zer", "Fr\u00fch\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Mir ein Duft aus Eden!", "tokens": ["Mir", "ein", "Duft", "aus", "E\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Als einst Paradieses", "tokens": ["Als", "einst", "Pa\u00b7ra\u00b7die\u00b7ses"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADV", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Sel'ge Fluren schwanden,", "tokens": ["Sel'\u00b7ge", "Flu\u00b7ren", "schwan\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Waren's Manns Gebete,", "tokens": ["Wa\u00b7ren's", "Manns", "Ge\u00b7be\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Waren's Eva's Thr\u00e4nen,", "tokens": ["Wa\u00b7ren's", "E\u00b7va's", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Die zu Duft da blieben?", "tokens": ["Die", "zu", "Duft", "da", "blie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Oder bracht' ein Bruder-", "tokens": ["O\u00b7der", "bracht'", "ein", "Bru\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "TRUNC"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Engel Euch hinieden", "tokens": ["En\u00b7gel", "Euch", "hi\u00b7nie\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["NE", "PPER", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "In die Wilde? \u2013 Labung", "tokens": ["In", "die", "Wil\u00b7de", "?", "\u2013", "La\u00b7bung"], "token_info": ["word", "word", "word", "punct", "punct", "word"], "pos": ["APPR", "ART", "NN", "$.", "$(", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wo dem matten Wandrer", "tokens": ["Wo", "dem", "mat\u00b7ten", "Wand\u00b7rer"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Zu bereiten, Labung,", "tokens": ["Zu", "be\u00b7rei\u00b7ten", ",", "La\u00b7bung", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Als er, halb verschmachtet,", "tokens": ["Als", "er", ",", "halb", "ver\u00b7schmach\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Traurig abw\u00e4rts blickte?", "tokens": ["Trau\u00b7rig", "ab\u00b7w\u00e4rts", "blick\u00b7te", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Kommt dem matten Wandrer", "tokens": ["Kommt", "dem", "mat\u00b7ten", "Wand\u00b7rer"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Auch in w\u00fcster Wilde", "tokens": ["Auch", "in", "w\u00fcs\u00b7ter", "Wil\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Labung! Wenn er traurig", "tokens": ["La\u00b7bung", "!", "Wenn", "er", "trau\u00b7rig"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "KOUS", "PPER", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Pfadverloren abw\u00e4rts", "tokens": ["Pfad\u00b7ver\u00b7lo\u00b7ren", "ab\u00b7w\u00e4rts"], "token_info": ["word", "word"], "pos": ["NN", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Blicket \u2013 dann erscheint ihm", "tokens": ["Bli\u00b7cket", "\u2013", "dann", "er\u00b7scheint", "ihm"], "token_info": ["word", "punct", "word"