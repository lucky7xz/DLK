{"textgrid.poem.38527": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Laura betet! Engelharfen hallen", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Laura betet! Engelharfen hallen", "tokens": ["Lau\u00b7ra", "be\u00b7tet", "!", "En\u00b7gel\u00b7har\u00b7fen", "hal\u00b7len"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "$.", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Tr\u00f6stung Gottes in ihr krankes Herz,", "tokens": ["Tr\u00f6s\u00b7tung", "Got\u00b7tes", "in", "ihr", "kran\u00b7kes", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und, wie Abels Opferd\u00fcfte, wallen", "tokens": ["Und", ",", "wie", "A\u00b7bels", "Op\u00b7fer\u00b7d\u00fcf\u00b7te", ",", "wal\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "$,", "PWAV", "NN", "NN", "$,", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ihre Seufzer himmelw\u00e4rts!", "tokens": ["Ih\u00b7re", "Seuf\u00b7zer", "him\u00b7mel\u00b7w\u00e4rts", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.2": {"line.1": {"text": "Wie sie kniet! in Andacht hingegossen,", "tokens": ["Wie", "sie", "kniet", "!", "in", "An\u00b7dacht", "hin\u00b7ge\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$.", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sch\u00f6