{"dta.poem.14766": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Br\u00e4utigam.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Von allen Bergen nieder", "tokens": ["Von", "al\u00b7len", "Ber\u00b7gen", "nie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So fr\u00f6hlich Gr\u00fc\u00dfen schallt \u2014", "tokens": ["So", "fr\u00f6h\u00b7lich", "Gr\u00fc\u00b7\u00dfen", "schallt"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das ist der Fr\u00fchling wieder,", "tokens": ["Das", "ist", "der", "Fr\u00fch\u00b7ling", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der ruft zum gr\u00fcnen Wald!", "tokens": ["Der", "ruft", "zum", "gr\u00fc\u00b7nen", "Wald", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}