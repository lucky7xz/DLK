{"dta.poem.3524": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Auch ein Schicksal .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich habe mein Feinsliebchen               ", "tokens": ["Ich", "ha\u00b7be", "mein", "Feins\u00b7lieb\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So lange nicht gesehn,", "tokens": ["So", "lan\u00b7ge", "nicht", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich sah sie gestern Abend,", "tokens": ["Ich", "sah", "sie", "ge\u00b7stern", "A\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wohl vor der Th\u00fcre stehn.", "tokens": ["Wohl", "vor", "der", "Th\u00fc\u00b7re", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie sagt, ich sollt sie k\u00fcssen,", "tokens": ["Sie", "sagt", ",", "ich", "sollt", "sie", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als ich vorbey wollt gehn;", "tokens": ["Als", "ich", "vor\u00b7bey", "wollt", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Mutter sollts nicht wissen,", "tokens": ["Die", "Mut\u00b7ter", "sollts", "nicht", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Mutter hats gesehn.", "tokens": ["Die", "Mut\u00b7ter", "hats", "ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ach Tochter, du willst freyen,", "tokens": ["Ach", "Toch\u00b7ter", ",", "du", "willst", "frey\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie wird es dir ergehn;", "tokens": ["Wie", "wird", "es", "dir", "er\u00b7gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es wird dich bald gereuen,", "tokens": ["Es", "wird", "dich", "bald", "ge\u00b7reu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn du wirst andre sehn.", "tokens": ["Wenn", "du", "wirst", "and\u00b7re", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wenn alle junge M\u00e4dchen", "tokens": ["Wenn", "al\u00b7le", "jun\u00b7ge", "M\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohlauf zum Tanzboden gehn,", "tokens": ["Wohl\u00b7auf", "zum", "Tanz\u00b7bo\u00b7den", "gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Mit ihren gr\u00fcnen Kr\u00e4nzerchen", "tokens": ["Mit", "ih\u00b7ren", "gr\u00fc\u00b7nen", "Kr\u00e4n\u00b7zer\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Im Reihentanze stehn.", "tokens": ["Im", "Rei\u00b7hen\u00b7tan\u00b7ze", "stehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Dann mu\u00dft du junges Weibchen", "tokens": ["Dann", "mu\u00dft", "du", "jun\u00b7ges", "Weib\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohl bey der Wiege stehn,", "tokens": ["Wohl", "bey", "der", "Wie\u00b7ge", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit deinem schneeweissen Leibchen,", "tokens": ["Mit", "dei\u00b7nem", "schnee\u00b7weis\u00b7sen", "Leib\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der Kopf thut dir so weh.", "tokens": ["Der", "Kopf", "thut", "dir", "so", "weh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u201edas Feuer kann man l\u00f6schen,", "tokens": ["\u201e", "das", "Feu\u00b7er", "kann", "man", "l\u00f6\u00b7schen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u201edes Feuer brennt so sehr;", "tokens": ["\u201e", "des", "Feu\u00b7er", "brennt", "so", "sehr", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201edie Liebe nicht vergessen,", "tokens": ["\u201e", "die", "Lie\u00b7be", "nicht", "ver\u00b7ges\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201eje nun und nimmermehr.\u201c", "tokens": ["\u201e", "je", "nun", "und", "nim\u00b7mer\u00b7mehr", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "ADV", "KON", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}