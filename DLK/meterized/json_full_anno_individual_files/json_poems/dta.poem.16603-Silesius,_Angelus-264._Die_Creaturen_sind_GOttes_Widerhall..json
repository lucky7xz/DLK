{"dta.poem.16603": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "264.  Die Creaturen sind GOttes  \n Widerhall.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Nichts weset ohne Stimm: Gott h\u00f6ret \u00fcberall/", "tokens": ["Nichts", "we\u00b7set", "oh\u00b7ne", "Stimm", ":", "Gott", "h\u00f6\u00b7ret", "\u00fc\u00b7be\u00b7rall", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NN", "$.", "NN", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In allen Creaturn/ sein Lob und Widerhall.", "tokens": ["In", "al\u00b7len", "Crea\u00b7turn", "/", "sein", "Lob", "und", "Wi\u00b7der\u00b7hall", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$(", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}}}}