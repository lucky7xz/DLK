{"textgrid.poem.40169": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Vision", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein wei\u00dfer Schwan, du ziehest vor mir leise", "tokens": ["Mein", "wei\u00b7\u00dfer", "Schwan", ",", "du", "zie\u00b7hest", "vor", "mir", "lei\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPER", "VVFIN", "APPR", "PPER", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "durch meiner Dichtung d\u00e4mmerblaue Fluten, \u2013", "tokens": ["durch", "mei\u00b7ner", "Dich\u00b7tung", "d\u00e4m\u00b7mer\u00b7blau\u00b7e", "Flu\u00b7ten", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "zur Ferne ziehst du, wo die Wellenkreise", "tokens": ["zur", "Fer\u00b7ne", "ziehst", "du", ",", "wo", "die", "Wel\u00b7len\u00b7krei\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "sich zitternd mischen sonnengoldnen Gluten.", "tokens": ["sich", "zit\u00b7ternd", "mi\u00b7schen", "son\u00b7nen\u00b7gold\u00b7nen", "Glu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wir lassen tragen uns und weiter tragen,", "tokens": ["Wir", "las\u00b7sen", "tra\u00b7gen", "uns", "und", "wei\u00b7ter", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VVFIN", "PPER", "KON", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "vom stillen Scheine so dahingezogen:", "tokens": ["vom", "stil\u00b7len", "Schei\u00b7ne", "so", "da\u00b7hin\u00b7ge\u00b7zo\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "PAV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "bis wir die sel'gen Inseln sehen ragen,", "tokens": ["bis", "wir", "die", "sel'\u00b7gen", "In\u00b7seln", "se\u00b7hen", "ra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "vom Ew'gen Licht umleuchtet, aus den Wogen.", "tokens": ["vom", "Ew'\u00b7gen", "Licht", "um\u00b7leuch\u00b7tet", ",", "aus", "den", "Wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dann nehme auf ich dich in meinen Nachen,", "tokens": ["Dann", "neh\u00b7me", "auf", "ich", "dich", "in", "mei\u00b7nen", "Na\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "dann berg' ich uns am heimatlichen Strande;", "tokens": ["dann", "ber\u00b7g'", "ich", "uns", "am", "hei\u00b7mat\u00b7li\u00b7chen", "Stran\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPRART", "ADJA", "NN", "$."], "m