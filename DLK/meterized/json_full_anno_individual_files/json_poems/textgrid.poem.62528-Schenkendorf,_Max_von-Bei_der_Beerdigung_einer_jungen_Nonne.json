{"textgrid.poem.62528": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Bei der Beerdigung einer jungen Nonne", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unter Blumen eingeschlafen", "tokens": ["Un\u00b7ter", "Blu\u00b7men", "ein\u00b7ge\u00b7schla\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruht hier eine Gottesbraut,", "tokens": ["Ruht", "hier", "ei\u00b7ne", "Got\u00b7tes\u00b7braut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deren Herz nicht St\u00fcrme trafen", "tokens": ["De\u00b7ren", "Herz", "nicht", "St\u00fcr\u00b7me", "tra\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "NN", "PTKNEG", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ihr Ohr kein Frevellaut.", "tokens": ["Und", "ihr", "Ohr", "kein", "Fre\u00b7vel\u00b7laut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Die ihr in dem Sarge schaut,", "tokens": ["Die", "ihr", "in", "dem", "Sar\u00b7ge", "schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "War des Klosters j\u00fcngste Bl\u00fcte,", "tokens": ["War", "des", "Klos\u00b7ters", "j\u00fcngs\u00b7te", "Bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat sich fr\u00fch dem Herrn vertraut,", "tokens": ["Hat", "sich", "fr\u00fch", "dem", "Herrn", "ver\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ADJD", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df er ihre Seele h\u00fcte.", "tokens": ["Da\u00df", "er", "ih\u00b7re", "See\u00b7le", "h\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Die ihr noch die kalten Wangen", "tokens": ["Die", "ihr", "noch", "die", "kal\u00b7ten", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jetzt in Rosenschein verkl\u00e4rt.", "tokens": ["Jetzt", "in", "Ro\u00b7sen\u00b7schein", "ver\u00b7kl\u00e4rt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Traurig h\u00e4ngt ihr Saitenspiel,", "tokens": ["Trau\u00b7rig", "h\u00e4ngt", "ihr", "Sai\u00b7ten\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00f6chte gern sein Lied vollenden;", "tokens": ["M\u00f6ch\u00b7te", "gern", "sein", "Lied", "voll\u00b7en\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Rosenkranz und Zither fiel,", "tokens": ["Ro\u00b7sen\u00b7kranz", "und", "Zi\u00b7ther", "fiel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da sie starb, aus ihren H\u00e4nden.", "tokens": ["Da", "sie", "starb", ",", "aus", "ih\u00b7ren", "H\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sollt' ihr ganz und gar entschweben,", "tokens": ["Sollt'", "ihr", "ganz", "und", "gar", "ent\u00b7schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "KON", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "S\u00fc\u00dfes Lied und zarter Ton?", "tokens": ["S\u00fc\u00b7\u00dfes", "Lied", "und", "zar\u00b7ter", "Ton", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist mit jenem holden Leben", "tokens": ["Ist", "mit", "je\u00b7nem", "hol\u00b7den", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alle Melodie entflohn?", "tokens": ["Al\u00b7le", "Me\u00b7lo\u00b7die", "ent\u00b7flohn", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "In dem Himmel klingt es fort,", "tokens": ["In", "dem", "Him\u00b7mel", "klingt", "es", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Was hienieden abgebrochen,", "tokens": ["Was", "hien\u00b7ie\u00b7den", "ab\u00b7ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVPP", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.3": {"text": "R\u00e4thselhaftes halbes Wort", "tokens": ["R\u00e4th\u00b7sel\u00b7haf\u00b7tes", "hal\u00b7bes", "Wort"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird einst v\u00f6llig ausgesprochen.", "tokens": ["Wird", "einst", "v\u00f6l\u00b7lig", "aus\u00b7ge\u00b7spro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Aus des heil'gen Gartens Mauern", "tokens": ["Aus", "des", "heil'\u00b7gen", "Gar\u00b7tens", "Mau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Hat sie G\u00e4rtners Huld versetzt,", "tokens": ["Hat", "sie", "G\u00e4rt\u00b7ners", "Huld", "ver\u00b7setzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo sie unter Wonneschauern", "tokens": ["Wo", "sie", "un\u00b7ter", "Won\u00b7ne\u00b7schau\u00b7ern"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nun im h\u00f6hern Licht sich letzt.", "tokens": ["Nun", "im", "h\u00f6\u00b7hern", "Licht", "sich", "letzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "PRF", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Die gegr\u00fcnt im Klosterthal,", "tokens": ["Die", "ge\u00b7gr\u00fcnt", "im", "Klos\u00b7ter\u00b7thal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "VVPP", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie der heil'ge Zweig des Aaron,", "tokens": ["Wie", "der", "heil'\u00b7ge", "Zweig", "des", "Aa\u00b7ron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Trinket Gottes reinen Strahl,", "tokens": ["Trin\u00b7ket", "Got\u00b7tes", "rei\u00b7nen", "Strahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bl\u00fcht nun auf im Thale Saron.", "tokens": ["Bl\u00fcht", "nun", "auf", "im", "Tha\u00b7le", "Sa\u00b7ron", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "APPRART", "NN", "NE", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.9": {"line.1": {"text": "K\u00fcndet's laut, ihr Glockenkl\u00e4nge,", "tokens": ["K\u00fcn\u00b7det's", "laut", ",", "ihr", "Glo\u00b7cken\u00b7kl\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Glaub' es dankend, frommer Christ,", "tokens": ["Glaub'", "es", "dan\u00b7kend", ",", "from\u00b7mer", "Christ", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df die Erd' uns viel zu enge,", "tokens": ["Da\u00df", "die", "Erd'", "uns", "viel", "zu", "en\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "ADV", "APPR", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df sie keine Heimat ist.", "tokens": ["Da\u00df", "sie", "kei\u00b7ne", "Hei\u00b7mat", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "H\u00f6her lodre, Andachtsglut,", "tokens": ["H\u00f6\u00b7her", "lod\u00b7re", ",", "An\u00b7dachts\u00b7glut", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Tr\u00f6st' uns in Triumphges\u00e4ngen,", "tokens": ["Tr\u00f6st'", "uns", "in", "Tri\u00b7umph\u00b7ge\u00b7s\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Wenn wir mit geweihter Flut", "tokens": ["Wenn", "wir", "mit", "ge\u00b7weih\u00b7ter", "Flut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nun das theure Bild besprengen.", "tokens": ["Nun", "das", "theu\u00b7re", "Bild", "be\u00b7spren\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Erste Gabe, letzte Gabe,", "tokens": ["Ers\u00b7te", "Ga\u00b7be", ",", "letz\u00b7te", "Ga\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Opferwasser, Thr\u00e4nenquell,", "tokens": ["Op\u00b7fer\u00b7was\u00b7ser", ",", "Thr\u00e4\u00b7nen\u00b7quell", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ein Menschenherz sich labe,", "tokens": ["Da\u00df", "ein", "Men\u00b7schen\u00b7herz", "sich", "la\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Flie\u00dfet ewig fromm und hell.", "tokens": ["Flie\u00b7\u00dfet", "e\u00b7wig", "fromm", "und", "hell", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Senkt den Leichnam in das Grab,", "tokens": ["Senkt", "den", "Leich\u00b7nam", "in", "das", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwestern, haltet ein mit Weinen!", "tokens": ["Schwes\u00b7tern", ",", "hal\u00b7tet", "ein", "mit", "Wei\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Reife Fr\u00fcchte fallen ab,", "tokens": ["Rei\u00b7fe", "Fr\u00fcch\u00b7te", "fal\u00b7len", "ab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Euer Tag wird auch erscheinen.", "tokens": ["Eu\u00b7er", "Tag", "wird", "auch", "er\u00b7schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Zu der Schaar der heil'gen Frauen,", "tokens": ["Zu", "der", "Schaar", "der", "heil'\u00b7gen", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu der Himmelsk\u00f6nigin,", "tokens": ["Zu", "der", "Him\u00b7mels\u00b7k\u00f6\u00b7ni\u00b7gin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zu den ewiggr\u00fcnen Auen", "tokens": ["Zu", "den", "e\u00b7wig\u00b7gr\u00fc\u00b7nen", "Au\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fchrt euch einst ein Engel hin.", "tokens": ["F\u00fchrt", "euch", "einst", "ein", "En\u00b7gel", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Und die Gnadenmutter wird", "tokens": ["Und", "die", "Gna\u00b7den\u00b7mut\u00b7ter", "wird"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In ein neu Gewand euch kleiden,", "tokens": ["In", "ein", "neu", "Ge\u00b7wand", "euch", "klei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Herr, der treue Hirt,", "tokens": ["Und", "der", "Herr", ",", "der", "treu\u00b7e", "Hirt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Euch an Lebensb\u00e4chen weiden.", "tokens": ["Euch", "an", "Le\u00b7bens\u00b7b\u00e4\u00b7chen", "wei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Wir im Thal der Thr\u00e4nen beten:", "tokens": ["Wir", "im", "Thal", "der", "Thr\u00e4\u00b7nen", "be\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00f6nig, segne deine Schaar,", "tokens": ["K\u00f6\u00b7nig", ",", "seg\u00b7ne", "dei\u00b7ne", "Schaar", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "St\u00e4rk' uns in den letzten N\u00f6then,", "tokens": ["St\u00e4rk'", "uns", "in", "den", "letz\u00b7ten", "N\u00f6\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Opfer auf dem S\u00fchnaltar!", "tokens": ["Op\u00b7fer", "auf", "dem", "S\u00fch\u00b7nal\u00b7tar", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Miserere Kyrie!", "tokens": ["Mi\u00b7se\u00b7re\u00b7re", "Ky\u00b7rie", "!"], "token_info": ["word", "word", "punct"], "pos":