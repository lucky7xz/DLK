{"dta.poem.13744": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Heimkehr .  \n 1810.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Doch warum auf diesen Wegen", "tokens": ["Doch", "wa\u00b7rum", "auf", "die\u00b7sen", "We\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sieht's so still und einsam aus?", "tokens": ["Sieht's", "so", "still", "und", "ein\u00b7sam", "aus", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kommt denn keiner mir entgegen,", "tokens": ["Kommt", "denn", "kei\u00b7ner", "mir", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIS", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bin ich nicht mehr Sohn vom Haus?", "tokens": ["Bin", "ich", "nicht", "mehr", "Sohn", "vom", "Haus", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "PIAT", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Kein' Hoboen h\u00f6r' ich schallen,", "tokens": ["Kein'", "Ho\u00b7boen", "h\u00f6r'", "ich", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Keine bunte Truppe mehr", "tokens": ["Kei\u00b7ne", "bun\u00b7te", "Trup\u00b7pe", "mehr"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Seh' ich froh den Burgpfad wallen \u2014", "tokens": ["Seh'", "ich", "froh", "den", "Burg\u00b7pfad", "wal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Damals ging es lust'ger her.", "tokens": ["Da\u00b7mals", "ging", "es", "lust'\u00b7ger", "her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}