{"textgrid.poem.42028": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein glasgeschnittner W\u00fcrfel f\u00fcllte das Zimmer", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein glasgeschnittner W\u00fcrfel f\u00fcllte das Zimmer", "tokens": ["Ein", "glas\u00b7ge\u00b7schnitt\u00b7ner", "W\u00fcr\u00b7fel", "f\u00fcll\u00b7te", "das", "Zim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sobald es wieder Abend war, den trug", "tokens": ["So\u00b7bald", "es", "wie\u00b7der", "A\u00b7bend", "war", ",", "den", "trug"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "NN", "VAFIN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er oft und stand umtaut von Schimmer.", "tokens": ["Er", "oft", "und", "stand", "um\u00b7taut", "von", "Schim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "KON", "VVFIN", "APPR", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Drau\u00dfen ging dunkler V\u00f6gel schwerer Flug", "tokens": ["Drau\u00b7\u00dfen", "ging", "dunk\u00b7ler", "V\u00f6\u00b7gel", "schwe\u00b7rer", "Flug"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "ADJA", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Flatternd vorbei und war wie k\u00fchles Wehn", "tokens": ["Flat\u00b7ternd", "vor\u00b7bei", "und", "war", "wie", "k\u00fch\u00b7les", "Wehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "PTKVZ", "KON", "VAFIN", "KOKOM", "ADJA", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.6": {"text": "Um seine Stirne. Manchmal, wunderbar,", "tokens": ["Um", "sei\u00b7ne", "Stir\u00b7ne", ".", "Manch\u00b7mal", ",", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "$.", "ADV", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Blieben die fremden Lieder um ihn stehn", "tokens": ["Blie\u00b7ben", "die", "frem\u00b7den", "Lie\u00b7der", "um", "ihn", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "APPR", "PPER", "VVINF"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.8": {"text": "Und eine Blume sang in seinem Haar.", "tokens": ["Und", "ei\u00b7ne", "Blu\u00b7me", "sang", "in", "sei\u00b7nem", "Haar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Oft schlich er scheu, geb\u00fcckt in dumpfer Last,", "tokens": ["Oft", "schlich", "er", "scheu", ",", "ge\u00b7b\u00fcckt", "in", "dum\u00b7pfer", "Last", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADJD", "$,", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und sah sich wie ein Feuer, das entlohte.", "tokens": ["Und", "sah", "sich", "wie", "ein", "Feu\u00b7er", ",", "das", "ent\u00b7loh\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "KOKOM", "ART", "NN", "$,", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"},