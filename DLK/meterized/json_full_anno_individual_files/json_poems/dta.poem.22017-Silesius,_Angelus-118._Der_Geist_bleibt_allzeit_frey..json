{"dta.poem.22017": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "118. Der Geist bleibt allzeit frey.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Schleu\u00df mich so streng du wilt in tausendt Eysen ein/", "tokens": ["Schleu\u00df", "mich", "so", "streng", "du", "wilt", "in", "tau\u00b7sendt", "Ey\u00b7sen", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "VVFIN", "PPER", "VMFIN", "APPR", "CARD", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich werde doch gantz srey/ und ungef\u00e4sselt seyn.", "tokens": ["Ich", "wer\u00b7de", "doch", "gantz", "srey", "/", "und", "un\u00b7ge\u00b7f\u00e4s\u00b7selt", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADJD", "$(", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}