ord", "punct", "word", "punct", "word"], "pos": ["KON", "VVFIN", "ADV", "NN", "$,", "ADJA", "$,", "ADV"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Die Bahn zu versperren den Booten,", "tokens": ["Die", "Bahn", "zu", "ver\u00b7sper\u00b7ren", "den", "Boo\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Geschaut nicht nach rechts, geschaut nicht nach links!", "tokens": ["Ge\u00b7schaut", "nicht", "nach", "rechts", ",", "ge\u00b7schaut", "nicht", "nach", "links", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "ADV", "$,", "VVPP", "PTKNEG", "APPR", "ADV", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Brecht durch! Sonst seid ihr Heloten.", "tokens": ["Brecht", "durch", "!", "Sonst", "seid", "ihr", "He\u00b7lo\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Was rauschen die Wogen, was rinnt in der Luft?", "tokens": ["Was", "rau\u00b7schen", "die", "Wo\u00b7gen", ",", "was", "rinnt", "in", "der", "Luft", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$,", "PWS", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Was zittert von Lande zu Lande?", "tokens": ["Was", "zit\u00b7tert", "von", "Lan\u00b7de", "zu", "Lan\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+--+--+-", "me