{"textgrid.poem.43283": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich mu\u00df die Menschen immer wieder segnen,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich mu\u00df die Menschen immer wieder segnen,", "tokens": ["Ich", "mu\u00df", "die", "Men\u00b7schen", "im\u00b7mer", "wie\u00b7der", "seg\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die g\u00fctevoll mir einst mein Handwerk lehrten.", "tokens": ["Die", "g\u00fc\u00b7te\u00b7voll", "mir", "einst", "mein", "Hand\u00b7werk", "lehr\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bin ich doch einer von den Vielbegehrten!", "tokens": ["Bin", "ich", "doch", "ei\u00b7ner", "von", "den", "Viel\u00b7be\u00b7gehr\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und jedem Meister darf ich stolz begegnen.", "tokens": ["Und", "je\u00b7dem", "Meis\u00b7ter", "darf", "ich", "stolz", "be\u00b7geg\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nur Tr\u00e4ge schreien stets von M\u00fche, Frohne,", "tokens": ["Nur", "Tr\u00e4\u00b7ge", "schrei\u00b7en", "stets", "von", "M\u00fc\u