{"dta.poem.1654": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der Gl\u00fccks-Topff.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn wir der finstern H\u00f6l und Wohnung sind entgangen/", "tokens": ["Wenn", "wir", "der", "fins\u00b7tern", "H\u00f6l", "und", "Woh\u00b7nung", "sind", "ent\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "KON", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Darinn uns die Natur neun Monat h\u00e4lt gefangen.", "tokens": ["Da\u00b7rinn", "uns", "die", "Na\u00b7tur", "neun", "Mo\u00b7nat", "h\u00e4lt", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ART", "NN", "CARD", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "on solcher d\u00fcstern Nacht und tieffem Schlaff entrissen/", "tokens": ["on", "sol\u00b7cher", "d\u00fcs\u00b7tern", "Nacht", "und", "tief\u00b7fem", "Schlaff", "ent\u00b7ris\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "KON", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "es hellen Tages Licht zum ersten mahl begr\u00fcssen/", "tokens": ["es", "hel\u00b7len", "Ta\u00b7ges", "Licht", "zum", "ers\u00b7ten", "mahl", "be\u00b7gr\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "NN", "APPRART", "ADJA", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "o pfleget uns alsbald der Himmel vorzuschreiben/", "tokens": ["o", "pfle\u00b7get", "uns", "als\u00b7bald", "der", "Him\u00b7mel", "vor\u00b7zu\u00b7schrei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "VVFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "as der und jener Mensch vor Lebens-Art soll treiben.", "tokens": ["as", "der", "und", "je\u00b7ner", "Mensch", "vor", "Le\u00b7bens\u00b7Art", "soll", "trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "KON", "PDAT", "NN", "APPR", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es hat die Ewigkeit den Sternen eingepr\u00e4get/", "tokens": ["Es", "hat", "die", "E\u00b7wig\u00b7keit", "den", "Ster\u00b7nen", "ein\u00b7ge\u00b7pr\u00e4\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Was ein iedweder Sinn vor Neigung bey sich tr\u00e4get/", "tokens": ["Was", "ein", "ied\u00b7we\u00b7der", "Sinn", "vor", "Nei\u00b7gung", "bey", "sich", "tr\u00e4\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "PIAT", "NN", "APPR", "NN", "APPR", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Man siehet dannenher aus schwacher Reiser Bl\u00fchen/", "tokens": ["Man", "sie\u00b7het", "dan\u00b7nen\u00b7her", "aus", "schwa\u00b7cher", "Rei\u00b7ser", "Bl\u00fc\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PAV", "APPR", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Was ihr bejahrter Stamm vor Fr\u00fcchte wird erziehen.", "tokens": ["Was", "ihr", "be\u00b7jahr\u00b7ter", "Stamm", "vor", "Fr\u00fcch\u00b7te", "wird", "er\u00b7zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "APPR", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Bey diesem sehn wir Lust zu Wissenschafft entspringen/", "tokens": ["Bey", "die\u00b7sem", "sehn", "wir", "Lust", "zu", "Wis\u00b7sen\u00b7schafft", "ent\u00b7sprin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVFIN", "PPER", "NN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und unterschiedner Art verschiedne Fr\u00fcchte bringen/", "tokens": ["Und", "un\u00b7ter\u00b7schied\u00b7ner", "Art", "ver\u00b7schied\u00b7ne", "Fr\u00fcch\u00b7te", "brin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der Eine folget nach dem Triebe seiner Sinnen/", "tokens": ["Der", "Ei\u00b7ne", "fol\u00b7get", "nach", "dem", "Trie\u00b7be", "sei\u00b7ner", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "APPR", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Sucht seinen Auffenthalt durch K\u00fcnste zu gewinnen/", "tokens": ["Sucht", "sei\u00b7nen", "Auf\u00b7fent\u00b7halt", "durch", "K\u00fcns\u00b7te", "zu", "ge\u00b7win\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Lehrt die verkehrte Welt nach Gottes Willen leben/", "tokens": ["Lehrt", "die", "ver\u00b7kehr\u00b7te", "Welt", "nach", "Got\u00b7tes", "Wil\u00b7len", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Beym Richter einen Spruch vor seinen Theil erheben/", "tokens": ["Beym", "Rich\u00b7ter", "ei\u00b7nen", "Spruch", "vor", "sei\u00b7nen", "Theil", "er\u00b7he\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Der eingefallnen Hand den schwachen Puls ber\u00fchren/", "tokens": ["Der", "ein\u00b7ge\u00b7fall\u00b7nen", "Hand", "den", "schwa\u00b7chen", "Puls", "be\u00b7r\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Durch zweiffelhafften Satz das Gegentheil verf\u00fchren/", "tokens": ["Durch", "zweif\u00b7fel\u00b7haff\u00b7ten", "Satz", "das", "Ge\u00b7gen\u00b7theil", "ver\u00b7f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Sich um ein jedes Wort in tausend andern zweyen/", "tokens": ["Sich", "um", "ein", "je\u00b7des", "Wort", "in", "tau\u00b7send", "an\u00b7dern", "zwe\u00b7yen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "PIAT", "NN", "APPR", "CARD", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.20": {"text": "Durch s\u00fcsser Reime Schall ein zartes Ohr erfreuen/", "tokens": ["Durch", "s\u00fcs\u00b7ser", "Rei\u00b7me", "Schall", "ein", "zar\u00b7tes", "Ohr", "er\u00b7freu\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Und durch was Mittel mehr die Sicherheit vom Sterben", "tokens": ["Und", "durch", "was", "Mit\u00b7tel", "mehr", "die", "Si\u00b7cher\u00b7heit", "vom", "Ster\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PRELS", "NN", "ADV", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Ein Wei\u00dfheit-schwangres Hirn gedencket zu erwerben.", "tokens": ["Ein", "Wei\u00df\u00b7heit\u00b7schwan\u00b7gres", "Hirn", "ge\u00b7den\u00b7cket", "zu", "er\u00b7wer\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ein andrer mag nicht stets in fauler Ruhe sitzen/", "tokens": ["Ein", "an\u00b7drer", "mag", "nicht", "stets", "in", "fau\u00b7ler", "Ru\u00b7he", "sit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VMFIN", "PTKNEG", "ADV", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Bey seiner Mutter Schos und hinterm Ofen schwitzen/", "tokens": ["Bey", "sei\u00b7ner", "Mut\u00b7ter", "Schos", "und", "hin\u00b7term", "O\u00b7fen", "schwit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NE", "KON", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Will lieber in die Welt durch Hagel/ Ey\u00df und Eisen/", "tokens": ["Will", "lie\u00b7ber", "in", "die", "Welt", "durch", "Ha\u00b7gel", "/", "Ey\u00df", "und", "Ei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "ART", "NN", "APPR", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Auch durch erz\u00fcrnte See/ durch Wind und Wellen reisen/", "tokens": ["Auch", "durch", "er\u00b7z\u00fcrn\u00b7te", "See", "/", "durch", "Wind", "und", "Wel\u00b7len", "rei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$(", "APPR", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und gegenw\u00e4rtig selbst erfahren/ sehn und h\u00f6ren/", "tokens": ["Und", "ge\u00b7gen\u00b7w\u00e4r\u00b7tig", "selbst", "er\u00b7fah\u00b7ren", "/", "sehn", "und", "h\u00f6\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "VVINF", "$(", "VVINF", "KON", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Was ungewisser uns die stummen B\u00fccher lehren.", "tokens": ["Was", "un\u00b7ge\u00b7wis\u00b7ser", "uns", "die", "stum\u00b7men", "B\u00fc\u00b7cher", "leh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ein andrer/ voll von Mutt und k\u00fchner Helden Hitze/", "tokens": ["Ein", "an\u00b7drer", "/", "voll", "von", "Mutt", "und", "k\u00fch\u00b7ner", "Hel\u00b7den", "Hit\u00b7ze", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$(", "ADJD", "APPR", "NE", "KON", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Beut seinem Feinde dar die kriegerische Spitze/", "tokens": ["Beut", "sei\u00b7nem", "Fein\u00b7de", "dar", "die", "krie\u00b7ge\u00b7ri\u00b7sche", "Spit\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wo Trommel und Trompet im freyen Felde klingen/", "tokens": ["Wo", "Trom\u00b7mel", "und", "Trom\u00b7pet", "im", "frey\u00b7en", "Fel\u00b7de", "klin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "APPRART", "ADJA", "NN", "VVINF", "$("], "meter": "-+--++-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.32": {"text": "Wo ihren rauhen Pa\u00df die groben St\u00fccke singen/", "tokens": ["Wo", "ih\u00b7ren", "rau\u00b7hen", "Pa\u00df", "die", "gro\u00b7ben", "St\u00fc\u00b7cke", "sin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Wo man ein festes Ort best\u00fcrmet und besch\u00fctzet/", "tokens": ["Wo", "man", "ein", "fes\u00b7tes", "Ort", "be\u00b7st\u00fcr\u00b7met", "und", "be\u00b7sch\u00fct\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Wo man das hei\u00dfe Blutt aus hundert Wunden schwitzet/", "tokens": ["Wo", "man", "das", "hei\u00b7\u00dfe", "Blutt", "aus", "hun\u00b7dert", "Wun\u00b7den", "schwit\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "ADJA", "NN", "APPR", "CARD", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Da hat er einen Sitz und Wohnung auffgeschlagen/", "tokens": ["Da", "hat", "er", "ei\u00b7nen", "Sitz", "und", "Woh\u00b7nung", "auff\u00b7ge\u00b7schla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Bereit vor Ehr und Gutt das Leben hinzuwagen.", "tokens": ["Be\u00b7reit", "vor", "Ehr", "und", "Gutt", "das", "Le\u00b7ben", "hin\u00b7zu\u00b7wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Ein andrer/ den sein Sinn zu Ruh und Friede tr\u00e4get/", "tokens": ["Ein", "an\u00b7drer", "/", "den", "sein", "Sinn", "zu", "Ruh", "und", "Frie\u00b7de", "tr\u00e4\u00b7get", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$(", "ART", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Denckt emsig nach/ wie er der stillen Wirthschafft pfleget/", "tokens": ["Denckt", "em\u00b7sig", "nach", "/", "wie", "er", "der", "stil\u00b7len", "Wirth\u00b7schafft", "pfle\u00b7get", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "$(", "PWAV", "PPER", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.39": {"text": "Wo er den T\u00fcnger soll dick oder d\u00fcnne breiten/", "tokens": ["Wo", "er", "den", "T\u00fcn\u00b7ger", "soll", "dick", "o\u00b7der", "d\u00fcn\u00b7ne", "brei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VMFIN", "ADJD", "KON", "VVFIN", "ADJA", "$("], "meter": "---+--+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.40": {"text": "Wie er das fette Feld soll seicht und tieff bereiten/", "tokens": ["Wie", "er", "das", "fet\u00b7te", "Feld", "soll", "seicht", "und", "tieff", "be\u00b7rei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "VMFIN", "ADJD", "KON", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Ob er die reiffe Frucht l\u00e4st hauen oder schneiden/", "tokens": ["Ob", "er", "die", "reif\u00b7fe", "Frucht", "l\u00e4st", "hau\u00b7en", "o\u00b7der", "schnei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", 