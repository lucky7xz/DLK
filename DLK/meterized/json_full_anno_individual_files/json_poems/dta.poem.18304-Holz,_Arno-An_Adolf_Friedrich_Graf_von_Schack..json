{"dta.poem.18304": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "An Adolf Friedrich Graf von Schack.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Ist doch der Quark von all den Versverbrechern,", "tokens": ["Ist", "doch", "der", "Quark", "von", "all", "den", "Vers\u00b7ver\u00b7bre\u00b7chern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "APPR", "PIAT", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die heut mit selbstgef\u00e4lligem Behagen", "tokens": ["Die", "heut", "mit", "selbst\u00b7ge\u00b7f\u00e4l\u00b7li\u00b7gem", "Be\u00b7ha\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Tretrad schwingen und das Tamtam schlagen!", "tokens": ["Das", "Tre\u00b7trad", "schwin\u00b7gen", "und", "das", "Tam\u00b7tam", "schla\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nur du schwingst nicht das Weihrauchfa\u00df der Mode", "tokens": ["Nur", "du", "schwingst", "nicht", "das", "Weih\u00b7rauch\u00b7fa\u00df", "der", "Mo\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "PTKNEG", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und beugst vor deinem Publikum das Knie,", "tokens": ["Und", "beugst", "vor", "dei\u00b7nem", "Pub\u00b7li\u00b7kum", "das", "Knie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du weihst dich als begeisterter Rhapsode", "tokens": ["Du", "weihst", "dich", "als", "be\u00b7geis\u00b7ter\u00b7ter", "Rhap\u00b7so\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "KOUS", "ADJA", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dem Hohenpriesterdienst der Poesie!", "tokens": ["Dem", "Ho\u00b7hen\u00b7pries\u00b7ter\u00b7dienst", "der", "Poe\u00b7sie", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Zeit ist eisern, eisern ihr Beruf,", "tokens": ["Die", "Zeit", "ist", "ei\u00b7sern", ",", "ei\u00b7sern", "ihr", "Be\u00b7ruf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "O, da\u00df sie endlich ihres Sohns ged\u00e4chte,", "tokens": ["O", ",", "da\u00df", "sie", "end\u00b7lich", "ih\u00b7res", "Sohns", "ge\u00b7d\u00e4ch\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des Sohns, der ihr die \u201eWeihges\u00e4nge\u201c schuf,", "tokens": ["Des", "Sohns", ",", "der", "ihr", "die", "\u201e", "Weih\u00b7ge\u00b7s\u00e4n\u00b7ge", "\u201c", "schuf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ART", "$(", "NN", "$(", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sie und des Orients wundervolle \u201eN\u00e4chte\u201c!", "tokens": ["Sie", "und", "des", "O\u00b7rients", "wun\u00b7der\u00b7vol\u00b7le", "\u201e", "N\u00e4ch\u00b7te", "\u201c", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["PPER", "KON", "ART", "NN", "ADJA", "$(", "NN", "$(", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Seit mir die Muse l\u00e4chelnd zugenickt,", "tokens": ["Seit", "mir", "die", "Mu\u00b7se", "l\u00e4\u00b7chelnd", "zu\u00b7ge\u00b7nickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hab ich mit Staunen zu dir aufgeblickt", "tokens": ["Hab", "ich", "mit", "Stau\u00b7nen", "zu", "dir", "auf\u00b7ge\u00b7blickt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "NN", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und winde dir nun in dein Kranzgeflecht:", "tokens": ["Und", "win\u00b7de", "dir", "nun", "in", "dein", "Kranz\u00b7ge\u00b7flecht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "\u201eich danke dir!\u201c", "tokens": ["\u201e", "ich", "dan\u00b7ke", "dir", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}}}}