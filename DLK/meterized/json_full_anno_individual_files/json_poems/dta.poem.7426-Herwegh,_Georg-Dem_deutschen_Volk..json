{"dta.poem.7426": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Dem   deutschen   Volk.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Deutschland, o zerrissen Herz,", "tokens": ["Deutschland", ",", "o", "zer\u00b7ris\u00b7sen", "Herz", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Das zu Ende bald geschlagen,", "tokens": ["Das", "zu", "En\u00b7de", "bald", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur um Dich noch will ich klagen", "tokens": ["Nur", "um", "Dich", "noch", "will", "ich", "kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPER", "ADV", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und in einer Brust von Erz", "tokens": ["Und", "in", "ei\u00b7ner", "Brust", "von", "Erz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schweigend meinen kleinen Schmerz,", "tokens": ["Schwei\u00b7gend", "mei\u00b7nen", "klei\u00b7nen", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Meinen kleinen Jammer tragen,", "tokens": ["Mei\u00b7nen", "klei\u00b7nen", "Jam\u00b7mer", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Vaterland, um Dich nur klagen.", "tokens": ["Va\u00b7ter\u00b7land", ",", "um", "Dich", "nur", "kla\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUI", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Lustig gr\u00fcnt Dein Nadelholz,", "tokens": ["Lus\u00b7tig", "gr\u00fcnt", "Dein", "Na\u00b7del\u00b7holz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Lustig rauschen Deine Eichen;", "tokens": ["Lus\u00b7tig", "rau\u00b7schen", "Dei\u00b7ne", "Ei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In den sechs und drei\u00dfig Reichen", "tokens": ["In", "den", "sechs", "und", "drei\u00b7\u00dfig", "Rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "CARD", "KON", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fehlt ein einzig K\u00f6rnchen Golds:", "tokens": ["Fehlt", "ein", "ein\u00b7zig", "K\u00f6rn\u00b7chen", "Golds", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJD", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Freier B\u00fcrger hoher Stolz", "tokens": ["Frei\u00b7er", "B\u00fcr\u00b7ger", "ho\u00b7her", "Stolz"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Fehlt im Lande sonder Gleichen,", "tokens": ["Fehlt", "im", "Lan\u00b7de", "son\u00b7der", "Glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "In den sechs und drei\u00dfig Reichen.", "tokens": ["In", "den", "sechs", "und", "drei\u00b7\u00dfig", "Rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "CARD", "KON", "CARD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn ein S\u00e4nger f\u00fcr Dich focht,", "tokens": ["Wenn", "ein", "S\u00e4n\u00b7ger", "f\u00fcr", "Dich", "focht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn ein Mann ein Schwert geschwungen,", "tokens": ["Wenn", "ein", "Mann", "ein", "Schwert", "ge\u00b7schwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hast Du scheu nur mitgesungen,", "tokens": ["Hast", "Du", "scheu", "nur", "mit\u00b7ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hast Du sch\u00fcchtern mitgepocht;", "tokens": ["Hast", "Du", "sch\u00fcch\u00b7tern", "mit\u00b7ge\u00b7pocht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVINF", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und man hat Dich unterjocht,", "tokens": ["Und", "man", "hat", "Dich", "un\u00b7ter\u00b7jocht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Hat Dich in den Staub gezwungen,", "tokens": ["Hat", "Dich", "in", "den", "Staub", "ge\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Weil Du gar so still gesungen.", "tokens": ["Weil", "Du", "gar", "so", "still", "ge\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ihr beweinet's und bereut's \u2014", "tokens": ["Ihr", "be\u00b7wei\u00b7net's", "und", "be\u00b7reut's"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "KON", "NE", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und das nennt ihr deutsche Treue?", "tokens": ["Und", "das", "nennt", "ihr", "deut\u00b7sche", "Treu\u00b7e", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00dft die Thr\u00e4nen, la\u00dft die Reue,", "tokens": ["La\u00dft", "die", "Thr\u00e4\u00b7nen", ",", "la\u00dft", "die", "Reu\u00b7e", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Soll nicht einst der Enkel Teut's", "tokens": ["Soll", "nicht", "einst", "der", "En\u00b7kel", "Teut's"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PTKNEG", "ADV", "ART", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sterben an der Zwietracht Kreuz,", "tokens": ["Ster\u00b7ben", "an", "der", "Zwiet\u00b7racht", "Kreuz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "K\u00e4mpf' und handle, Volk, auf's Neue,", "tokens": ["K\u00e4mpf'", "und", "hand\u00b7le", ",", "Volk", ",", "auf's", "Neu\u00b7e", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "$,", "NN", "$,", "APPRART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn der Teufel ist die Reue!", "tokens": ["Denn", "der", "Teu\u00b7fel", "ist", "die", "Reu\u00b7e", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Tritt in Deiner F\u00fcrsten Reihn!", "tokens": ["Tritt", "in", "Dei\u00b7ner", "F\u00fcrs\u00b7ten", "Reihn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sprich: die sechs und drei\u00dfig Lappen", "tokens": ["Sprich", ":", "die", "sechs", "und", "drei\u00b7\u00dfig", "Lap\u00b7pen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$.", "ART", "CARD", "KON", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sollen wieder besser klappen", "tokens": ["Sol\u00b7len", "wie\u00b7der", "bes\u00b7ser", "klap\u00b7pen"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Helf' uns Gott \u2014 so soll es klappen.", "tokens": ["Hel\u00b7f'", "uns", "Gott", "so", "soll", "es", "klap\u00b7pen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "$(", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}