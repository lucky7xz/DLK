{"textgrid.poem.57647": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was frag ich nach den Menschen", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was frag ich nach den Menschen", "tokens": ["Was", "frag", "ich", "nach", "den", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und nach der lauten Stadt,", "tokens": ["Und", "nach", "der", "lau\u00b7ten", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn mich die Bergwaldwildnis,", "tokens": ["Wenn", "mich", "die", "Berg\u00b7wald\u00b7wild\u00b7nis", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die wei\u00dfe Stille hat.", "tokens": ["Die", "wei\u00b7\u00dfe", "Stil\u00b7le", "hat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Buchenst\u00e4mme stehen", "tokens": ["Die", "Bu\u00b7chen\u00b7st\u00e4m\u00b7me", "ste\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So schwarz im wei\u00dfen Schnee,", "tokens": ["So", "schwarz", "im", "wei\u00b7\u00dfen", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Seinen Schlafbaum sucht der Bussard,", "tokens": ["Sei\u00b7nen", "Schlaf\u00b7baum", "sucht", "der", "Bus\u00b7sard", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu Felde zieht das Reh.", "tokens": ["Zu", "Fel\u00b7de", "zieht", "das", "Reh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Fuchs bellt unten im Grunde,", "tokens": ["Der", "Fuchs", "bellt", "un\u00b7ten", "im", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Eule gibt keine Ruh,", "tokens": ["Die", "Eu\u00b7le", "gibt", "kei\u00b7ne", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der Abendwind r\u00fchrt an den Zweigen,", "tokens": ["Der", "Ab\u00b7end\u00b7wind", "r\u00fchrt", "an", "den", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Der Schnee f\u00e4llt immerzu.", "tokens": ["Der", "Schnee", "f\u00e4llt", "im\u00b7mer\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Im Tale funkeln die Lichter,", "tokens": ["Im", "Ta\u00b7le", "fun\u00b7keln", "die", "Lich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "