{"dta.poem.15733": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "66.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Viel Worte hast du, Sohn, das Kind nur einen Schrei,", "tokens": ["Viel", "Wor\u00b7te", "hast", "du", ",", "Sohn", ",", "das", "Kind", "nur", "ei\u00b7nen", "Schrei", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "$,", "NN", "$,", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur einen, der ihm mu\u00df ausdr\u00fccken vielerlei,", "tokens": ["Nur", "ei\u00b7nen", ",", "der", "ihm", "mu\u00df", "aus\u00b7dr\u00fc\u00b7cken", "vie\u00b7ler\u00b7lei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$,", "PRELS", "PPER", "VMFIN", "VVINF", "PIAT", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Lust, Unlust, Hunger, Durst, Begier nach Schlaf und Spiel;", "tokens": ["Lust", ",", "Un\u00b7lust", ",", "Hun\u00b7ger", ",", "Durst", ",", "Be\u00b7gier", "nach", "Schlaf", "und", "Spiel", ";"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Es hat beisammen, was dir auseinander fiel.", "tokens": ["Es", "hat", "bei\u00b7sam\u00b7men", ",", "was", "dir", "aus\u00b7ein\u00b7an\u00b7der", "fiel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "PWS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Entfaltetes l\u00e4\u00dft sich nicht mehr zusammenfalten;", "tokens": ["Ent\u00b7fal\u00b7te\u00b7tes", "l\u00e4\u00dft", "sich", "nicht", "mehr", "zu\u00b7sam\u00b7men\u00b7fal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Du lerne reicher stets die F\u00fclle zu gestalten.", "tokens": ["Du", "ler\u00b7ne", "rei\u00b7cher", "stets", "die", "F\u00fcl\u00b7le", "zu", "ge\u00b7stal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Gib Sprache dem Gef\u00fchl in jedem Ton, und sei", "tokens": ["Gib", "Spra\u00b7che", "dem", "Ge\u00b7f\u00fchl", "in", "je\u00b7dem", "Ton", ",", "und", "sei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "NN", "ART", "NN", "APPR", "PIAT", "NN", "$,", "KON", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wahr in jedem, wie das Kind in seinem Schrei.", "tokens": ["So", "wahr", "in", "je\u00b7dem", ",", "wie", "das", "Kind", "in", "sei\u00b7nem", "Schrei", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PIS", "$,", "PWAV", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}