{"dta.poem.8132": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1773", "urn": "urn:nbn:de:kobv:b4-200905199504", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wehklagen, und bang Senfzen vom Graunthale des Abgrunds her,", "tokens": ["Weh\u00b7kla\u00b7gen", ",", "und", "bang", "Senf\u00b7zen", "vom", "Graunt\u00b7ha\u00b7le", "des", "Ab\u00b7grunds", "her", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "ADJD", "NN", "APPRART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+---+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sturmheulen, und Strombr\u00fcllen, und Felskrachen, das laut", "tokens": ["Sturm\u00b7heu\u00b7len", ",", "und", "Strom\u00b7br\u00fcl\u00b7len", ",", "und", "Fels\u00b7kra\u00b7chen", ",", "das", "laut"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "KON", "NN", "$,", "KON", "NN", "$,", "PRELS", "ADJD"], "meter": "+---++--++--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "niederst\u00fcrzt',", "tokens": ["nie\u00b7der\u00b7st\u00fcrzt'", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Und Wutschreyn, und Rachausrufen erschell dumpf auf!", "tokens": ["Und", "Wut\u00b7schreyn", ",", "und", "Rach\u00b7aus\u00b7ru\u00b7fen", "er\u00b7schell", "dumpf", "auf", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "KON", "NN", "ADJD", "ADJD", "PTKVZ", "$."], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Wie der Strahl eilt, schwebten wir schnell, und in Wehmut fort.", "tokens": ["Wie", "der", "Strahl", "eilt", ",", "schweb\u00b7ten", "wir", "schnell", ",", "und", "in", "Weh\u00b7mut", "fort", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "ADJD", "$,", "KON", "APPR", "NN", "PTKVZ", "$."], "meter": "--+-+--++-+-+", "measure": "iambic.hexa.relaxed"}}}}}