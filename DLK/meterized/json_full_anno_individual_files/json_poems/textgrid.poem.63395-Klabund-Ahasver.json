{"textgrid.poem.63395": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Ahasver", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ewig bist du Meer und rinnst ins Meer,", "tokens": ["E\u00b7wig", "bist", "du", "Meer", "und", "rinnst", "ins", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "NN", "KON", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Quelle, Wolke, Regen \u2013 Ahasver...", "tokens": ["Quel\u00b7le", ",", "Wol\u00b7ke", ",", "Re\u00b7gen", "\u2013", "A\u00b7has\u00b7ver", "..."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$(", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Tor, wer um vertane Stunden tr\u00e4umt,", "tokens": ["Tor", ",", "wer", "um", "ver\u00b7ta\u00b7ne", "Stun\u00b7den", "tr\u00e4umt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Weiser, wer die Jahre weit vers\u00e4umt.", "tokens": ["Wei\u00b7ser", ",", "wer", "die", "Jah\u00b7re", "weit", "ver\u00b7s\u00e4umt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Trage so die ewige Last der Erde", "tokens": ["Tra\u00b7ge", "so", "die", "e\u00b7wi\u00b7ge", "Last", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.6": {"text": "Und den Dornenkranz mit Frohgeb\u00e4rde.", "tokens": ["Und", "den", "Dor\u00b7nen\u00b7kranz", "mit", "Froh\u00b7ge\u00b7b\u00e4r\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Schl\u00e4gst du deine Welt und dich zusammen,", "tokens": ["Schl\u00e4gst", "du", "dei\u00b7ne", "Welt", "und", "dich", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "KON", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Aus den Tr\u00fcmmern brechen neue Flammen...", "tokens": ["Aus", "den", "Tr\u00fcm\u00b7mern", "bre\u00b7chen", "neu\u00b7e", "Flam\u00b7men", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Tod ist nur ein Wort, damit man sich vergi\u00dft...", "tokens": ["Tod", "ist", "nur", "ein", "Wort", ",", "da\u00b7mit", "man", "sich", "ver\u00b7gi\u00dft", "..."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ART", "NN", "$,", "KOUS", "PIS", "PRF", "VVFIN", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.10": {"text": "Weh, Sterblicher, da\u00df du unsterblich bist!", "tokens": ["Weh", ",", "Sterb\u00b7li\u00b7cher", ",", "da\u00df", "du", "uns\u00b7terb\u00b7lich", "bist", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ewig bist du Meer un