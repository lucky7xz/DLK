en brennt\u00ab \u2013", "tokens": ["Das", "grel\u00b7le", "gr\u00fcn", ",", "das", "mei\u00b7nen", "Ra\u00b7sen", "brennt", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.22": {"line.1": {"text": "halt! das war die rechte Stimmung f\u00fcr den Spanierwein. Ich las und las.", "tokens": ["halt", "!", "das", "war", "die", "rech\u00b7te", "Stim\u00b7mung", "f\u00fcr", "den", "Spa\u00b7ni\u00b7er\u00b7wein", ".", "Ich", "las", "und", "las", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PDS", "VAFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$.", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+-+--+-+-+", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.23": {"line.1": {"text": "\u00bb", "tokens": ["\u00bb"], "token_info": ["punct"], "pos": ["$("]}, "line.2": {"text": "Mit meinem bindet \u2013 \u2013", "tokens": ["Mit", "mei\u00b7nem", "bin\u00b7det", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "VVFIN", "$(", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Verachtung um die Lippen sch\u00fcrzend\u00ab \u2013", "tokens": ["Ver\u00b7ach\u00b7tung", "um", "die", "Lip\u00b7pen", "sch\u00fcr\u00b7zend", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJD", "$(", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.24": {"line.1": {"text": "hatte Er das ", "tokens": ["hat\u00b7te", "Er", "das"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PPER", "ART"], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.25": {"line.1": {"text": "\