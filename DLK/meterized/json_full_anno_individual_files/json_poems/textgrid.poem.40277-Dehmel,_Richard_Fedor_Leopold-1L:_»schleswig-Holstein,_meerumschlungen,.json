{"textgrid.poem.40277": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbschleswig-Holstein, meerumschlungen,", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbschleswig-Holstein, meerumschlungen,", "tokens": ["\u00bb", "schles\u00b7wig\u00b7Hol\u00b7stein", ",", "meer\u00b7um\u00b7schlun\u00b7gen", ","], "token_info": ["punct", "word", "punct", "word", "punct"], "pos": ["$(", "VAINF", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schleswig-Holstein, stammverwandt!\u00ab", "tokens": ["Schles\u00b7wig\u00b7Hol\u00b7stein", ",", "stamm\u00b7ver\u00b7wandt", "!", "\u00ab"], "token_info": ["word", "punct", "word", "punct", "punct"], "pos": ["NN", "$,", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Herrlich: ja, wir Seelenschreiber!", "tokens": ["Herr\u00b7lich", ":", "ja", ",", "wir", "See\u00b7len\u00b7schrei\u00b7ber", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$.", "PTKANT", "$,", "PPER", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Herrlich, ja: dies Brotgequ\u00e4le!", "tokens": ["Herr\u00b7lich", ",", "ja", ":", "dies", "Brot\u00b7ge\u00b7qu\u00e4\u00b7le", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADV", "$.", "PDS", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn die Kinder; denn die Weiber.", "tokens": ["Denn", "die", "Kin\u00b7der", ";", "denn", "die", "Wei\u00b7ber", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$.", "KON", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und so wird man Schreiberseele.", "tokens": ["Und", "so", "wird", "man", "Schrei\u00b7ber\u00b7see\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PIS", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbnoch Einmal la\u00df mich deine Hand", "tokens": ["\u00bb", "noch", "Ein\u00b7mal", "la\u00df", "mich", "dei\u00b7ne", "Hand"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "ADV", "VVIMP", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "inbr\u00fcnstig k\u00fcssen, hei\u00df und schwer,", "tokens": ["in\u00b7br\u00fcns\u00b7tig", "k\u00fcs\u00b7sen", ",", "hei\u00df", "und", "schwer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "nicht deinen Mund, nicht deinen Mund,", "tokens": ["nicht", "dei\u00b7nen", "Mund", ",", "nicht", "dei\u00b7nen", "Mund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "$,", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ich lie\u00dfe dich sonst nimmermehr.\u00ab", "tokens": ["ich", "lie\u00b7\u00dfe", "dich", "sonst", "nim\u00b7mer\u00b7mehr", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Freilich, wenn wir schliefen", "tokens": ["Frei\u00b7lich", ",", "wenn", "wir", "schlie\u00b7fen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "in dem viel zu tiefen", "tokens": ["in", "dem", "viel", "zu", "tie\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "PIS", "PTKZU", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wasser zwischen uns:", "tokens": ["Was\u00b7ser", "zwi\u00b7schen", "uns", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "schliefe auch das B\u00fc\u00dfen", "tokens": ["schlie\u00b7fe", "auch", "das", "B\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "dieser s\u00fcndig s\u00fc\u00dfen", "tokens": ["die\u00b7ser", "s\u00fcn\u00b7dig", "s\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJD", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Unschuld zwischen uns.", "tokens": ["Un\u00b7schuld", "zwi\u00b7schen", "uns", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Aber mit dem Leben", "tokens": ["A\u00b7ber", "mit", "dem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "schliefe auch \u2013 das Streben.", "tokens": ["schlie\u00b7fe", "auch", "\u2013", "das", "Stre\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$(", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "\u00bbdie himmelblauen Schmetterlinge leuchten,", "tokens": ["\u00bb", "die", "him\u00b7mel\u00b7blau\u00b7en", "Schmet\u00b7ter\u00b7lin\u00b7ge", "leuch\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der schwefelgelbe Pfau, der mich umschweift,", "tokens": ["Der", "schwe\u00b7fel\u00b7gel\u00b7be", "Pfau", ",", "der", "mich", "um\u00b7schweift", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das grelle gr\u00fcn, das meinen Rasen brennt\u00ab \u2013", "tokens": ["Das", "grel\u00b7le", "gr\u00fcn", ",", "das", "mei\u00b7nen", "Ra\u00b7sen", "brennt", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "halt! das war die rechte Stimmung f\u00fcr den Spanierwein. Ich las und las.", "tokens": ["halt", "!", "das", "war", "die", "rech\u00b7te", "Stim\u00b7mung", "f\u00fcr", "den", "Spa\u00b7ni\u00b7er\u00b7wein", ".", "Ich", "las", "und", "las", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PDS", "VAFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$.", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+-+--+-+-+", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.7": {"line.1": {"text": "\u00bb", "tokens": ["\u00bb"], "token_info": ["punct"], "pos": ["$("]}, "line.2": {"text": "Mit meinem bindet \u2013 \u2013", "tokens": ["Mit", "mei\u00b7nem", "bin\u00b7det", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "VVFIN", "$(", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Verachtung um die Lippen sch\u00fcrzend\u00ab \u2013", "tokens": ["Ver\u00b7ach\u00b7tung", "um", "die", "Lip\u00b7pen", "sch\u00fcr\u00b7zend", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJD", "$(", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "hatte Er das ", "tokens": ["hat\u00b7te", "Er", "das"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PPER", "ART"], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.9": {"line.1": {"text": "\u00bbund ich, ein F\u00fcrst hier auf dem Aldebaran \u2013 \u2013", "tokens": ["\u00bb", "und", "ich", ",", "ein", "F\u00fcrst", "hier", "auf", "dem", "A\u00b7lde\u00b7ba\u00b7ran", "\u2013", "\u2013"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "PPER", "$,", "ART", "NN", "ADV", "APPR", "ART", "NN", "$(", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "H\u00f6rst du's? ich w\u00fcnsche, nein ich will, ich will,", "tokens": ["H\u00f6rst", "du's", "?", "ich", "w\u00fcn\u00b7sche", ",", "nein", "ich", "will", ",", "ich", "will", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PIS", "$.", "PPER", "VVFIN", "$,", "PTKANT", "PPER", "VMFIN", "$,", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df du mich liebst auf diesem roten Stern!\u00ab", "tokens": ["Da\u00df", "du", "mich", "liebst", "auf", "die\u00b7sem", "ro\u00b7ten", "Stern", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "APPR", "PDAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Ob das wol sein Eheweib gewesen war? \u2013", "tokens": ["Ob", "das", "wol", "sein", "E\u00b7he\u00b7weib", "ge\u00b7we\u00b7sen", "war", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PDS", "ADV", "PPOSAT", "NN", "VAPP", "VAFIN", "$.", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.11": {"line.1": {"text": "\u00bbviel besser sind die Menschen hier als unten!", "tokens": ["\u00bb", "viel", "bes\u00b7ser", "sind", "die", "Men\u00b7schen", "hier", "als", "un\u00b7ten", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "VAFIN", "ART", "NN", "ADV", "KOKOM", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mehr Liebe, mehr Verzeihung und Geduld,", "tokens": ["Mehr", "Lie\u00b7be", ",", "mehr", "Ver\u00b7zei\u00b7hung", "und", "Ge\u00b7duld", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kein Mi\u00dfverst\u00e4ndnis mehr \u2013 \u2013", "tokens": ["Kein", "Mi\u00df\u00b7ver\u00b7st\u00e4nd\u00b7nis", "mehr", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "ADV", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "\u00bb \u2013 \u2013 und langsam", "tokens": ["\u00bb", "\u2013", "\u2013", "und", "lang\u00b7sam"], "token_info": ["punct", "punct", "punct", "word", "word"], "pos": ["$(", "$(", "$(", "KON", "ADJD"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Auf Moor und Brachfeld welkt der Tag\u00ab.", "tokens": ["Auf", "Moor", "und", "Brach\u00b7feld", "welkt", "der", "Tag", "\u00ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "ART", "NN", "$(", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "\u00bbein gr\u00fcner Turban schm\u00fcckt das Haupt mir,", "tokens": ["\u00bb", "ein", "gr\u00fc\u00b7ner", "Tur\u00b7ban", "schm\u00fcckt", "das", "Haupt", "mir", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von Seide knittert mein Gewand;", "tokens": ["Von", "Sei\u00b7de", "knit\u00b7tert", "mein", "Ge\u00b7wand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und jeder Muselmensch hier glaubt mir,", "tokens": ["Und", "je\u00b7der", "Mu\u00b7sel\u00b7mensch", "hier", "glaubt", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich sei der F\u00fcrst von Samarkand\u00ab.", "tokens": ["Ich", "sei", "der", "F\u00fcrst", "von", "Sa\u00b7mar\u00b7kand", "\u00ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NE", "$(", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "wie die Woge heimlich sch\u00e4umt", "tokens": ["wie", "die", "Wo\u00b7ge", "heim\u00b7lich", "sch\u00e4umt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJD", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "und von ihrer Tiefe tr\u00e4umt;", "tokens": ["und", "von", "ih\u00b7rer", "Tie\u00b7fe", "tr\u00e4umt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Gieb mir deine Hand,", "tokens": ["Gieb", "mir", "dei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Einmal noch ein Schmerz,", "tokens": ["Ein\u00b7mal", "noch", "ein", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Einmal noch ein deutsches Herz \u2013", "tokens": ["Ein\u00b7mal", "noch", "ein", "deut\u00b7sches", "Herz", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dann leb wohl, mein Weib, mein Vaterland! \u2013 \u2013", "tokens": ["Dann", "leb", "wohl", ",", "mein", "Weib", ",", "mein", "Va\u00b7ter\u00b7land", "!", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$.", "$(", "$("], "meter": "--+-+-+-+", "measure": "anapaest.init"}}, "stanza.16": {"line.1": {"text": "Ach, wie s\u00fc\u00df die Veilchen duften ...", "tokens": ["Ach", ",", "wie", "s\u00fc\u00df", "die", "Veil\u00b7chen", "duf\u00b7ten", "..."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "ADJD", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "\u00bbschleswig-Holstein, meerumschlungen,", "tokens": ["\u00bb", "schles\u00b7wig\u00b7Hol\u00b7stein", ",", "meer\u00b7um\u00b7schlun\u00b7gen", ","], "token_info": ["punct", "word", "punct", "word", "punct"], "pos": ["$(", "VAINF", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schleswig-Holstein, stammverwandt!\u00ab", "tokens": ["Schles\u00b7wig\u00b7Hol\u00b7stein", ",", "stamm\u00b7ver\u00b7wandt", "!", "\u00ab"], "token_info": ["word", "punct", "word", "punct", "punct"], "pos": ["NN", "$,", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "Herrlich: ja, wir Seelenschreiber!", "tokens": ["Herr\u00b7lich", ":", "ja", ",", "wir", "See\u00b7len\u00b7schrei\u00b7ber", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$.", "PTKANT", "$,", "PPER", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Herrlich, ja: dies Brotgequ\u00e4le!", "tokens": ["Herr\u00b7lich", ",", "ja", ":", "dies", "Brot\u00b7ge\u00b7qu\u00e4\u00b7le", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADV", "$.", "PDS", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn die Kinder; denn die Weiber.", "tokens": ["Denn", "die", "Kin\u00b7der", ";", "denn", "die", "Wei\u00b7ber", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$.", "KON", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und so wird man Schreiberseele.", "tokens": ["Und", "so", "wird", "man", "Schrei\u00b7ber\u00b7see\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PIS", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.19": {"line.1": {"text": "\u00bbnoch Einmal la\u00df mich deine Hand", "tokens": ["\u00bb", "noch", "Ein\u00b7mal", "la\u00df", "mich", "dei\u00b7ne", "Hand"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV"