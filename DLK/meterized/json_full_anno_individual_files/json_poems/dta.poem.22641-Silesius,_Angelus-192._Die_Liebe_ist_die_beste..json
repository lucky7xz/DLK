{"dta.poem.22641": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "192. Die Liebe ist die beste.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Ich mag mich auf der Welt in keiner Kunst so \u00fcben/", "tokens": ["Ich", "mag", "mich", "auf", "der", "Welt", "in", "kei\u00b7ner", "Kunst", "so", "\u00fc\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "ART", "NN", "APPR", "PIAT", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als wie ich meinen GOtt aufs innigste sol lieben.", "tokens": ["Als", "wie", "ich", "mei\u00b7nen", "Gott", "aufs", "in\u00b7nigs\u00b7te", "sol", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "PPER", "PPOSAT", "NN", "APPRART", "ADJA", "VMFIN", "VVINF", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}}}}