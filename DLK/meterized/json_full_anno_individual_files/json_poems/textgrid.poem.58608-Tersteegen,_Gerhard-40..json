{"textgrid.poem.58608": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "40.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, man glaubt es nimmermehr, wie man sich in allem meinet;", "tokens": ["Ach", ",", "man", "glaubt", "es", "nim\u00b7mer\u00b7mehr", ",", "wie", "man", "sich", "in", "al\u00b7lem", "mei\u00b7net", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PIS", "VVFIN", "PPER", "ADV", "$,", "PWAV", "PIS", "PRF", "APPR", "PIS", "VVFIN", "$."], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Traue deiner Tugend nicht, ob sie noch so lauter scheinet!", "tokens": ["Trau\u00b7e", "dei\u00b7ner", "Tu\u00b7gend", "nicht", ",", "ob", "sie", "noch", "so", "lau\u00b7ter", "schei\u00b7net", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKNEG", "$,", "KOUS", "PPER", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Sollst du werden auserw\u00e4hlt und von allen Schlacken rein,", "tokens": ["Sollst", "du", "wer\u00b7den", "au\u00b7ser\u00b7w\u00e4hlt", "und", "von", "al\u00b7len", "Schla\u00b7cken", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VAFIN", "VVPP", "KON", "APPR", "PIAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "So mu\u00dft du in Kreuz und Not in den Elendsofen ein.", "tokens": ["So", "mu\u00dft", "du", "in", "Kreuz", "und", "Not", "in", "den", "E\u00b7len\u00b7dso\u00b7fen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NN", "KON", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Silber wird gel\u00e4utert bald; dies kommt nicht so leicht zu Ende,", "tokens": ["Sil\u00b7ber", "wird", "ge\u00b7l\u00e4u\u00b7tert", "bald", ";", "dies", "kommt", "nicht", "so", "leicht", "zu", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "ADV", "$.", "PDS", "VVFIN", "PTKNEG", "ADV", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Da man in sich f\u00fchlet nur S\u00fcnde, Jammer und Elende.", "tokens": ["Da", "man", "in", "sich", "f\u00fch\u00b7let", "nur", "S\u00fcn\u00b7de", ",", "Jam\u00b7mer", "und", "E\u00b7len\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PRF", "VVFIN", "ADV", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Wer in diese Schule kommt, lernet wie man reine liebt", "tokens": ["Wer", "in", "die\u00b7se", "Schu\u00b7le", "kommt", ",", "ler\u00b7net", "wie", "man", "rei\u00b7ne", "liebt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "PDAT", "NN", "VVFIN", "$,", "VVFIN", "KOKOM", "PIS", "ADJA", "VVFIN"], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Und von eigner Absicht frei, Gott allein die Ehre gibt.", "tokens": ["Und", "von", "eig\u00b7ner", "Ab\u00b7sicht", "frei", ",", "Gott", "al\u00b7lein", "die", "Eh\u00b7re", "gibt", "."], "token_info": ["word", "word", "word", "word