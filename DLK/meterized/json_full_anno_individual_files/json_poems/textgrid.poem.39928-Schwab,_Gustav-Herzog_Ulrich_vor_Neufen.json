{"textgrid.poem.39928": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Herzog Ulrich vor Neufen", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "M\u00fcd vom Schlagen und vom Siegen", "tokens": ["M\u00fcd", "vom", "Schla\u00b7gen", "und", "vom", "Sie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "KON", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zieht der Herzog durch sein Land,", "tokens": ["Zieht", "der", "Her\u00b7zog", "durch", "sein", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Droben sieht er Neufen liegen", "tokens": ["Dro\u00b7ben", "sieht", "er", "Neu\u00b7fen", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf der dr\u00e4u'nden Felsenwand.", "tokens": ["Auf", "der", "dr\u00e4u'n\u00b7den", "Fel\u00b7sen\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hei\u00dfer Strahl der Fr\u00fchlingssonnen", "tokens": ["Hei\u00b7\u00dfer", "Strahl", "der", "Fr\u00fch\u00b7lings\u00b7son\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Brennt auf Reiter und auf Ro\u00df \u2013", "tokens": ["Brennt", "auf", "Rei\u00b7ter", "und", "auf", "Ro\u00df", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "KON", "APPR", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "\u00bbw\u00e4re doch das Nest gewonnen!\u00ab", "tokens": ["\u00bb", "w\u00e4\u00b7re", "doch", "das", "Nest", "ge\u00b7won\u00b7nen", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VAFIN", "ADV", "ART", "NN", "VVPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ruft der Landgraf, sein Geno\u00df.", "tokens": ["Ruft", "der", "Land\u00b7graf", ",", "sein", "Ge\u00b7no\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und so reiten sie die Stege", "tokens": ["Und", "so", "rei\u00b7ten", "sie", "die", "Ste\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch den k\u00fchlen Wald hinauf;", "tokens": ["Durch", "den", "k\u00fch\u00b7len", "Wald", "hin\u00b7auf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lauscht kein Hinterhalt im Wege?", "tokens": ["Lauscht", "kein", "Hin\u00b7ter\u00b7halt", "im", "We\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Regnen keine Kugeln drauf?", "tokens": ["Reg\u00b7nen", "kei\u00b7ne", "Ku\u00b7geln", "drauf", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nein, es ist kein Feind zu sp\u00fcren,", "tokens": ["Nein", ",", "es", "ist", "kein", "Feind", "zu", "sp\u00fc\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Alle Zinnen stehen leer,", "tokens": ["Al\u00b7le", "Zin\u00b7nen", "ste\u00b7hen", "leer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Auf bequemen Br\u00fccken f\u00fchren", "tokens": ["Auf", "be\u00b7que\u00b7men", "Br\u00fc\u00b7cken", "f\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Durch den Burgwall sie das Heer.", "tokens": ["Durch", "den", "Burg\u00b7wall", "sie", "das", "Heer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Aus dem Schlosse t\u00f6nt entgegen", "tokens": ["Aus", "dem", "Schlos\u00b7se", "t\u00f6nt", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihnen nicht Gesch\u00fctzes Knall,", "tokens": ["Ih\u00b7nen", "nicht", "Ge\u00b7sch\u00fct\u00b7zes", "Knall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sondern Priesters Wort und Segen,", "tokens": ["Son\u00b7dern", "Pries\u00b7ters", "Wort", "und", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ein heller Orgelschall.", "tokens": ["Und", "ein", "hel\u00b7ler", "Or\u00b7gel\u00b7schall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und von mehr als Einer Sch\u00fcssel", "tokens": ["Und", "von", "mehr", "als", "Ei\u00b7ner", "Sch\u00fcs\u00b7sel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "KOKOM", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "S\u00fc\u00dfer Dampf her\u00fcber weht,", "tokens": ["S\u00fc\u00b7\u00dfer", "Dampf", "her\u00b7\u00fc\u00b7ber", "weht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und der Burgvogt mit dem Schl\u00fcssel", "tokens": ["Und", "der", "Burg\u00b7vogt", "mit", "dem", "Schl\u00fcs\u00b7sel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Vor dem offnen Thore steht.", "tokens": ["Vor", "dem", "off\u00b7nen", "Tho\u00b7re", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbritter Berthold, du Verwegner,", "tokens": ["\u00bb", "rit\u00b7ter", "Bert\u00b7hold", ",", "du", "Ver\u00b7weg\u00b7ner", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sprich, was macht denn dich so zahm?", "tokens": ["Sprich", ",", "was", "macht", "denn", "dich", "so", "zahm", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PWS", "VVFIN", "KON", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Du mein Feind und ew'ger Gegner,", "tokens": ["Du", "mein", "Feind", "und", "ew'\u00b7ger", "Geg\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bist du worden blind und lahm?", "tokens": ["Bist", "du", "wor\u00b7den", "blind", "und", "lahm", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VAPP", "ADJD", "KON", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber deine Blicke gl\u00e4nzen,", "tokens": ["A\u00b7ber", "dei\u00b7ne", "Bli\u00b7cke", "gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie kein blindes Auge gl\u00fcht!", "tokens": ["Wie", "kein", "blin\u00b7des", "Au\u00b7ge", "gl\u00fcht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und dein Haus schickt sich zu T\u00e4nzen,", "tokens": ["Und", "dein", "Haus", "schickt", "sich", "zu", "T\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Wie kein Lahmer drum sich m\u00fcht!\u00ab", "tokens": ["Wie", "kein", "Lah\u00b7mer", "drum", "sich", "m\u00fcht", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PIAT", "NN", "PAV", "PRF", "ADJD", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbherr!\u00ab erwiedert ihm der Ritter,", "tokens": ["\u00bb", "herr", "!", "\u00ab", "er\u00b7wie\u00b7dert", "ihm", "der", "Rit\u00b7ter", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$.", "$(", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Warf sich vor des Herzogs Fu\u00df:", "tokens": ["Warf", "sich", "vor", "des", "Her\u00b7zogs", "Fu\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbseid nicht Eurem Knechte bitter,", "tokens": ["\u00bb", "seid", "nicht", "Eu\u00b7rem", "Knech\u00b7te", "bit\u00b7ter", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PTKNEG", "PPOSAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nennt auch feig nicht seinen Gru\u00df.", "tokens": ["Nennt", "auch", "feig", "nicht", "sei\u00b7nen", "Gru\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mir ist heut ein Sohn geboren,", "tokens": ["Mir", "ist", "heut", "ein", "Sohn", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Meines Hauses erster Stern;", "tokens": ["Mei\u00b7nes", "Hau\u00b7ses", "ers\u00b7ter", "Stern", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wird mir der, hab' ich geschworen, \u2013", "tokens": ["Wird", "mir", "der", ",", "hab'", "ich", "ge\u00b7schwo\u00b7ren", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "ART", "$,", "VAFIN", "PPER", "VVPP", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Will ich huld'gen meinem Herrn.", "tokens": ["Will", "ich", "huld'\u00b7gen", "mei\u00b7nem", "Herrn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "In der Kirche, den zu taufen,", "tokens": ["In", "der", "Kir\u00b7che", ",", "den", "zu", "tau\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stehet mir der Burgpfaff schon.", "tokens": ["Ste\u00b7het", "mir", "der", "Burg\u00b7pfaff", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seid Ihr nicht zu m\u00fcd vom Raufen,", "tokens": ["Seid", "Ihr", "nicht", "zu", "m\u00fcd", "vom", "Rau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "PTKNEG", "PTKA", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Werdet Pathen meinem Sohn!", "tokens": ["Wer\u00b7det", "Pa\u00b7then", "mei\u00b7nem", "Sohn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nicht vergessen solche Gnade", "tokens": ["Nicht", "ver\u00b7ges\u00b7sen", "sol\u00b7che", "Gna\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "VVPP", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird der Vater und das Kind,", "tokens": ["Wird", "der", "Va\u00b7ter", "und", "das", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Die zu Neufens steilem Pfade", "tokens": ["Die", "zu", "Neu\u00b7fens", "stei\u00b7lem", "Pfa\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NE", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Hundert Jahr lang W\u00e4chter sind!\u00ab", "tokens": ["Hun\u00b7dert", "Jahr", "lang", "W\u00e4ch\u00b7ter", "sind", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["CARD", "NN", "ADJD", "NE", "VAFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ei, gelegen kommt den F\u00fcrsten", "tokens": ["Ei", ",", "ge\u00b7le\u00b7gen", "kommt", "den", "F\u00fcrs\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVPP", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Solche Ladung nach dem Kampf,", "tokens": ["Sol\u00b7che", "La\u00b7dung", "nach", "dem", "Kampf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die nach k\u00fchlem Weine d\u00fcrsten,", "tokens": ["Die", "nach", "k\u00fch\u00b7lem", "Wei\u00b7ne", "d\u00fcrs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schielen nach der Sch\u00fcsseln Dampf.", "tokens": ["Schie\u00b7len", "nach", "der", "Sch\u00fcs\u00b7seln", "Dampf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und der Herzog reicht dem Degen", "tokens": ["Und", "der", "Her\u00b7zog", "reicht", "dem", "De\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "--+---+-", "measure": "anapaest.init"}, "line.6": {"text": "Freundlich die Vers\u00f6hnungshand,", "tokens": ["Freund\u00b7lich", "die", "Ver\u00b7s\u00f6h\u00b7nungs\u00b7hand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Schenkt dem Knaben seinen Segen,", "tokens": ["Schenkt", "dem", "Kna\u00b7ben", "sei\u00b7nen", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und ein sch\u00f6n St\u00fcck Ackerland.", "tokens": ["Und", "ein", "sch\u00f6n", "St\u00fcck", "A\u00b7cker\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJD", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "M\u00fcd vom Schlagen und vom Siegen", "tokens": ["M\u00fcd", "vom", "Schla\u00b7gen", "und", "vom", "Sie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["N