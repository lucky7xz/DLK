{"textgrid.poem.57238": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Selbst der Engel entschwebt Wonnegefilden, l\u00e4sst", "genre": "verse", "period": "N.A.", "pub_year": 1750, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Selbst der Engel entschwebt Wonnegefilden, l\u00e4sst", "tokens": ["Selbst", "der", "En\u00b7gel", "ent\u00b7schwebt", "Won\u00b7ne\u00b7ge\u00b7fil\u00b7den", ",", "l\u00e4sst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "NN", "$,", "VVFIN"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Seine Krone voll Glanz unter den Himlischen,", "tokens": ["Sei\u00b7ne", "Kro\u00b7ne", "voll", "Glanz", "un\u00b7ter", "den", "Him\u00b7li\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "NN", "APPR", "ART", "NN", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Wandelt, unter den Menschen", "tokens": ["Wan\u00b7delt", ",", "un\u00b7ter", "den", "Men\u00b7schen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "APPR", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Mensch, in J\u00fcnglingsgestalt umher.", "tokens": ["Mensch", ",", "in", "J\u00fcng\u00b7lings\u00b7ge\u00b7stalt", "um\u00b7her", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Lass denn, Muse, den Hain, wo du das Weltgericht,", "tokens": ["Lass", "denn", ",", "Mu\u00b7se", ",", "den", "Hain", ",", "wo", "du", "das", "Welt\u00b7ge\u00b7richt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "NN", "$,", "ART", "NN", "$,", "PWAV", "PPER", "ART", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Und die K\u00f6nige singst, welche verworfen sind!", "tokens": ["Und", "die", "K\u00f6\u00b7ni\u00b7ge", "singst", ",", "wel\u00b7che", "ver\u00b7wor\u00b7fen", "sind", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,", "PRELS", "VVPP", "VAFIN", "$."], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Kom, hier winken, dich Th\u00e4ler", "tokens": ["Kom", ",", "hier", "win\u00b7ken", ",", "dich", "Th\u00e4\u00b7ler"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "ADV", "VVINF", "$,", "PPER", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "In ihr Tempe zur Erd' herab.", "tokens": ["In", "ihr", "Tem\u00b7pe", "zur", "Erd'", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "NN", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Kom, es hoffet ihr Wink! Wo du der Ceder Haupt", "tokens": ["Kom", ",", "es", "hof\u00b7fet", "ihr", "Wink", "!", "Wo", "du", "der", "Ce\u00b7der", "Haupt"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$.", "PWAV", "PPER", "ART", "NN", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Durch den steigenden Schall deines Gesangs bewegst,", "tokens": ["Durch", "den", "stei\u00b7gen\u00b7den", "Schall", "dei\u00b7nes", "Ge\u00b7sangs", "be\u00b7wegst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "--+--+---+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Nicht nur jene Gefilde", "tokens": ["Nicht", "nur", "je\u00b7ne", "Ge\u00b7fil\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "PDAT", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Sind mit lachendem Reiz bekr\u00e4nzt;", "tokens": ["Sind", "mit", "la\u00b7chen\u00b7dem", "Reiz", "be\u00b7kr\u00e4nzt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Auch hier stand die Natur, da sie aus reicher Hand", "tokens": ["Auch", "hier", "stand", "die", "Na\u00b7tur", ",", "da", "sie", "aus", "rei\u00b7cher", "Hand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u00dcber H\u00fcgel und Thal lebende Sch\u00f6nheit goss,", "tokens": ["\u00dc\u00b7ber", "H\u00fc\u00b7gel", "und", "Thal", "le\u00b7ben\u00b7de", "Sch\u00f6n\u00b7heit", "goss", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJA", "NN", "NE", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Mit verweilendem Tritte,", "tokens": ["Mit", "ver\u00b7wei\u00b7len\u00b7dem", "Trit\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Diese Th\u00e4ler zu schm\u00fccken, still.", "tokens": ["Die\u00b7se", "Th\u00e4\u00b7ler", "zu", "schm\u00fc\u00b7cken", ",", "still", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDAT", "NN", "PTKZU", "VVINF", "$,", "ADJD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "Sieh den ruhenden See, wie sein Gestade sich,", "tokens": ["Sieh", "den", "ru\u00b7hen\u00b7den", "See", ",", "wie", "sein", "Ge\u00b7sta\u00b7de", "sich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$,", "PWAV", "PPOSAT", "NN", "PRF", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Dicht vom Walde bedeckt, sanfter erhoben hat,", "tokens": ["Dicht", "vom", "Wal\u00b7de", "be\u00b7deckt", ",", "sanf\u00b7ter", "er\u00b7ho\u00b7ben", "hat", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVPP", "$,", "ADJD", "VVPP", "VAFIN", "$,"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und den schimmernden Abend", "tokens": ["Und", "den", "schim\u00b7mern\u00b7den", "A\u00b7bend"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "In der gr\u00fcnlichen D\u00e4mrung birgt.", "tokens": ["In", "der", "gr\u00fcn\u00b7li\u00b7chen", "D\u00e4m\u00b7rung", "birgt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.6": {"line.1": {"text": "Sieh des schattenden Walds Wipfel. Sie neigen sich,", "tokens": ["Sieh", "des", "schat\u00b7ten\u00b7den", "Walds", "Wip\u00b7fel", ".", "Sie", "nei\u00b7gen", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "NN", "$.", "PPER", "VVFIN", "PRF", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Vor dem kommenden Hauch lauterer L\u00fcfte? Nein,", "tokens": ["Vor", "dem", "kom\u00b7men\u00b7den", "Hauch", "lau\u00b7te\u00b7rer", "L\u00fcf\u00b7te", "?", "Nein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN", "$.", "PTKANT", "$,"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Friedrich k\u00f6mt in den Schatten!", "tokens": ["Fried\u00b7rich", "k\u00f6mt", "in", "den", "Schat\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Darum neigen die Wipfel sich.", "tokens": ["Da\u00b7rum", "nei\u00b7gen", "die", "Wip\u00b7fel", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "PRF", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "Warum l\u00e4chelt dein Blick? warum ergiesset sich", "tokens": ["Wa\u00b7rum", "l\u00e4\u00b7chelt", "dein", "Blick", "?", "wa\u00b7rum", "er\u00b7gies\u00b7set", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "$.", "PWAV", "VVFIN", "PRF"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Diese Freude, der Reiz heller vom Aug' herab?", "tokens": ["Die\u00b7se", "Freu\u00b7de", ",", "der", "Reiz", "hel\u00b7ler", "vom", "Aug'", "her\u00b7ab", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "ART", "NN", "ADJD", "APPRART", "NN", "ADV", "$."], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Wird sein festlicher Name", "tokens": ["Wird", "sein", "fest\u00b7li\u00b7cher", "Na\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Schon genannt, wo die Palme weht?", "tokens": ["Schon", "ge\u00b7nannt", ",", "wo", "die", "Pal\u00b7me", "weht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.8": {"line.1": {"text": "\u00bbglaubest du, dass auf das, so auf der Erd' ihr thut,", "tokens": ["\u00bb", "glau\u00b7best", "du", ",", "dass", "auf", "das", ",", "so", "auf", "der", "Erd'", "ihr", "thut", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "$,", "KOUS", "APPR", "ART", "$,", "ADV", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wir mit forschendem Blick wachsam nicht niedersehn?", "tokens": ["Wir", "mit", "for\u00b7schen\u00b7dem", "Blick", "wach\u00b7sam", "nicht", "nie\u00b7der\u00b7sehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "ADJD", "PTKNEG", "VVINF", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und die Edlen nicht kennen,", "tokens": ["Und", "die", "Ed\u00b7len", "nicht", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Die so einsam hier unten sind?", "tokens": ["Die", "so", "ein\u00b7sam", "hier", "un\u00b7ten", "sind", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "ADV", "ADV", "VAFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.9": {"line.1": {"text": "Da wir, wenn er kaum reift, schon den Gedanken sehn,", "tokens": ["Da", "wir", ",", "wenn", "er", "kaum", "reift", ",", "schon", "den", "Ge\u00b7dan\u00b7ken", "sehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und die werdende That, eh sie hin\u00fcbertrit", "tokens": ["Und", "die", "wer\u00b7den\u00b7de", "That", ",", "eh", "sie", "hin\u00b7\u00fc\u00b7bert\u00b7rit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "KOUS", "PPER", "VVFIN"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Vor das Auge des Schauers,", "tokens": ["Vor", "das", "Au\u00b7ge", "des", "Schau\u00b7ers", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Und nun andre Geberden hat!", "tokens": ["Und", "nun", "and\u00b7re", "Ge\u00b7ber\u00b7den", "hat", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "VAFIN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.10": {"line.1": {"text": "Kann was heiliger uns, als ein Gebieter seyn,", "tokens": ["Kann", "was", "hei\u00b7li\u00b7ger", "uns", ",", "als", "ein", "Ge\u00b7bie\u00b7ter", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADJA", "PPER", "$,", "KOUS", "ART", "NN", "VAINF", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Der zwar feurig und jung, dennoch ein Weiser ist,", "tokens": ["Der", "zwar", "feu\u00b7rig", "und", "jung", ",", "den\u00b7noch", "ein", "Wei\u00b7ser", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "KON", "ADJD", "$,", "ADV", "ART", "NN", "VAFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Und, die h\u00f6chste der W\u00fcrden,", "tokens": ["Und", ",", "die", "h\u00f6chs\u00b7te", "der", "W\u00fcr\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ART", "ADJA", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Durch sich selber, noch mehr erh\u00f6ht?", "tokens": ["Durch", "sich", "sel\u00b7ber", ",", "noch", "mehr", "er\u00b7h\u00f6ht", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ADV", "$,", "ADV", "ADV", "VVPP", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.11": {"line.1": {"text": "Heil dem K\u00f6nig! er h\u00f6rt, rufet die Stund' ihm einst,", "tokens": ["Heil", "dem", "K\u00f6\u00b7nig", "!", "er", "h\u00f6rt", ",", "ru\u00b7fet", "die", "Stund'", "ihm", "einst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "PPER", "VVFIN", "$,", "VVFIN", "ART", "NN", "PPER", "ADV", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Die auch Kronen vom Haupt, wenn sie ert\u00f6net, wirft,", "tokens": ["Die", "auch", "Kro\u00b7nen", "vom", "Haupt", ",", "wenn", "sie", "er\u00b7t\u00f6\u00b7net", ",", "wirft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADV", "NN", "APPRART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Unerschrocken ihr Rufen,", "tokens": ["Un\u00b7er\u00b7schro\u00b7cken", "ihr", "Ru\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "L\u00e4chelt, schlummert zu Gl\u00fccklichen", "tokens": ["L\u00e4\u00b7chelt", ",", "schlum\u00b7mert", "zu", "Gl\u00fcck\u00b7li\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "APPR", "NN"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "Still hin\u00fcber! Um ihn stehn in Versamlungen", "tokens": ["Still", "hin\u00b7\u00fc\u00b7ber", "!", "Um", "ihn", "stehn", "in", "Ver\u00b7sam\u00b7lun\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "$.", "KOUI", "PPER", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Seine Thaten umher, jede mit Licht gekr\u00f6nt,", "tokens": ["Sei\u00b7ne", "Tha\u00b7ten", "um\u00b7her", ",", "je\u00b7de", "mit", "Licht", "ge\u00b7kr\u00f6nt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$,", "PIAT", "APPR", "NN", "VVPP", "$,"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Jede bis zu dem Richter", "tokens": ["Je\u00b7de", "bis", "zu", "dem", "Rich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "ADV", "APPR", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Seine sanfte Begleiterin.\u00ab", "tokens": ["Sei\u00b7ne", "sanf\u00b7te", "Be\u00b7glei\u00b7te\u00b7rin", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.13": {"line.1": {"text": "Se