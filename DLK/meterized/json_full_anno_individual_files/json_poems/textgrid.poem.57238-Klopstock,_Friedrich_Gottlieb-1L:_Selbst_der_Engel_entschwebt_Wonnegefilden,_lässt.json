, "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Friedrich k\u00f6mt in den Schatten!", "tokens": ["Fried\u00b7rich", "k\u00f6mt", "in", "den", "Schat\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Darum neigen die Wipfel sich.", "tokens": ["Da\u00b7rum", "nei\u00b7gen", "die", "Wip\u00b7fel", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "PRF", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.19": {"line.1": {"text": "Warum l\u00e4chelt dein Blick? warum ergiesset sich", "tokens": ["Wa\u00b7rum", "l\u00e4\u00b7chelt", "dein", "Blick", "?", "wa\u00b7rum", "er\u00b7gies\u00b7set", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "$.", "PWAV", "VVFIN", "PRF"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Diese Freude, der Reiz heller vom Aug' herab?", "tokens": ["Die\u00b7se", "Freu\u00b7de", ",", "der", "Reiz", "hel\u00b7ler", "vom", "Aug'", "her\u00b7ab", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "ART", "NN", "ADJD", "APPRART", "NN", "ADV", "$."], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Wird sein festlicher Name", "tokens": ["Wird", "sein", "fest\u00b7li\u00b7cher", "Na\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Schon genannt, wo die Palme weht?", "tokens": ["Schon", "ge\u00b7nannt", ",", "wo", "die", "Pal\u00b7me", "weht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.20": {"line.1": {"text": "\u00bbglaubest du, dass auf das, so auf der Erd' ihr thut,", "tokens": ["\u00bb", "glau\u00b7best", "du", ",", "dass", "auf", "das", ",", "so", "auf", "der", "Erd'", "ihr", "thut", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "$,", "KOUS", "APPR", "ART", "$,", "ADV", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wir mit forschendem Blick wachsam nicht niedersehn?", "tokens": ["Wir", "mit", "for\u00b7schen\u00b7dem", "Blick", "wach\u00b7sam", "nicht", "nie\u00b7der\u00b7sehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "ADJD", "PTKNEG", "VVINF", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und die Edlen nicht kennen,", "tokens": ["Und", "die", "Ed\u00b7len", "nicht", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Die so einsam hier unten sind?", "tokens": ["Die", "so", "ein\u00b7sam", "hier", "un\u00b7ten", "sind", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "ADV", "ADV", "VAFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.21": {"line.1": {"text": "Da wir, wenn er kaum reift, schon den Gedanken sehn,", "tokens": ["Da", "wir", ",", "wenn", "er", "kaum", "reift", ",", "schon", "den", "Ge\u00b7dan\u00b7ken", "sehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und die werdende That, eh sie hin\u00fcbertrit", "tokens": ["Und", "die", "wer\u00b7den\u00b7de", "That", ",", "eh", "sie", "hin\u00b7\u00fc\u00b7bert\u00b7rit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "KOUS", "PPER", "VVFIN"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Vor das Auge des Schauers,", "tokens": ["Vor", "das", "Au\u00b7ge", "des", "Schau\u00b7ers", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Und nun andre Geberden hat!", "tokens": ["Und", "nun", "and\u00b7re", "Ge\u00b7ber\u00b7den", "hat", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "VAFIN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.22": {"line.1": {"text": "Kann was heiliger uns, als ein Gebieter seyn,", "tokens": ["Kann", "was", "hei\u00b7li\u00b7ger", "uns", ",", "als", "ein", "Ge\u00b7bie\u00b7ter", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADJA", "PPER", "$,", "KOUS", "ART", "NN", "VAINF", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Der zwar feurig und jung, dennoch ein Weiser ist,", "tokens": ["Der", "zwar", "feu\u00b7rig", "und", "jung", ",", "den\u00b7noch", "ein", "Wei\u00b7ser", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "KON", "ADJD", "$,", "ADV", "ART", "NN", "VAFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Und, die h\u00f6chste der W\u00fcrden,", "tokens": ["Und", ",", "die", "h\u00f6chs\u00b7te", "der", "W\u00fcr\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ART", "ADJA", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Durch sich selber, noch mehr erh\u00f6ht?", "tokens": ["Durch", "sich", "sel\u00b7ber", ",", "noch", "mehr", "er\u00b7h\u00f6ht", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ADV", "$,", "ADV", "ADV", "VVPP", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.23": {"line.1": {"text": "Heil dem K\u00f6nig! er h\u00f6rt, rufet die Stund' ihm einst,", "tokens": ["Heil", "dem", "K\u00f6\u00b7nig", "!", "er", "h\u00f6rt", ",", "ru\u00b7fet", "die", "Stund'", "ihm", "einst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "PPER", "VVFIN", "$,", "VVFIN", "ART", "NN", "PPER", "ADV", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Die auch Kronen vom Haupt, wenn sie ert\u00f6net, wirft,", "tokens": ["Die", "auch", "Kro\u00b7nen", "vom", "Haupt", ",", "wenn", "sie", "er\u00b7t\u00f6\u00b7net", ",", "wirft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADV", "NN", "APPRART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Unerschrocken ihr Rufen,", "tokens": ["Un\u00b7er\u00b7schro\u00b7cken", "ihr", "Ru\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "L\u00e4chelt, schlummert zu Gl\u00fccklichen", "tokens": ["L\u00e4\u00b7chelt", ",", "schlum\u00b7mert", "zu", "Gl\u00fcck\u00b7li\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "APPR", "NN"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}}, "stanza.24": {"line.1": {"text": "Still hin\u00fcber! Um ihn stehn in Versamlungen", "tokens": ["Still", "hin\u00b7\u00fc\u00b7ber", "!", "Um", "ihn", "stehn", "in", "Ver\u00b7sam\u00b7lun\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "$.", "KOUI", "PPER", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Seine Thaten umher, jede mit Licht gekr\u00f6nt,", "tokens": ["Sei\u00b7ne", "Tha\u00b7ten", "um\u00b7her", ",", "je\u00b7de", "mit", "Licht", "ge\u00b7kr\u00f6nt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$,", "PIAT", "APPR", "NN", "VVPP", "$,"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Jede bis zu dem Richter", "tokens": ["Je\u00b7de", "bis", "zu", "dem", "Rich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "ADV", "APPR", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Seine sanfte Begleiterin.\u00ab", "tokens": ["Sei\u00b7ne", "sanf\u00b7te", "Be\u00b7glei\u00b7te\u00b7rin", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}