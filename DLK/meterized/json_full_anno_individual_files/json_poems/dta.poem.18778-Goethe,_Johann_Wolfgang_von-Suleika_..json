{"dta.poem.18778": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Suleika .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.71", "nl:0.28"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Sag du hast wohl viel gedichtet?", "tokens": ["Sag", "du", "hast", "wohl", "viel", "ge\u00b7dich\u00b7tet", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hin und her dein Lied gerichtet? \u2014", "tokens": ["Hin", "und", "her", "dein", "Lied", "ge\u00b7rich\u00b7tet", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "KON", "ADV", "PPOSAT", "NN", "VVPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00f6ngeschrieben, deine Hand,", "tokens": ["Sch\u00f6n\u00b7ge\u00b7schrie\u00b7ben", ",", "dei\u00b7ne", "Hand", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Prachtgebunden, goldger\u00e4ndet,", "tokens": ["Pracht\u00b7ge\u00b7bun\u00b7den", ",", "gold\u00b7ge\u00b7r\u00e4n\u00b7det", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bis auf Punkt und Strich vollendet,", "tokens": ["Bis", "auf", "Punkt", "und", "Strich", "voll\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Zierlichlockend manchen Band.", "tokens": ["Zier\u00b7lich\u00b7lo\u00b7ckend", "man\u00b7chen", "Band", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Stets wo du sie hingewendet", "tokens": ["Stets", "wo", "du", "sie", "hin\u00b7ge\u00b7wen\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PWAV", "PPER", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "War\u2019s gewiss ein Liebespfand.", "tokens": ["Wa\u00b7r's", "ge\u00b7wiss", "ein", "Lie\u00b7be\u00b7spfand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}