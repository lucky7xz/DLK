{"dta.poem.6435": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Geruch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man stelle sich demnach die C\u00f6rper f\u00fcr,", "tokens": ["Man", "stel\u00b7le", "sich", "dem\u00b7nach", "die", "C\u00f6r\u00b7per", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "PAV", "ART", "NN", "APPR", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So mit Geruch erf\u00fcllet seyn,", "tokens": ["So", "mit", "Ge\u00b7ruch", "er\u00b7f\u00fcl\u00b7let", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als wenn sie zart und kleine C\u00f6rperlein", "tokens": ["Als", "wenn", "sie", "zart", "und", "klei\u00b7ne", "C\u00f6r\u00b7per\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "ADJD", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In grosser Menge von sich schicken,", "tokens": ["In", "gros\u00b7ser", "Men\u00b7ge", "von", "sich", "schi\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So k\u00fctzelnd die so zart-als kleinen H\u00e4utchen dr\u00fccken,", "tokens": ["So", "k\u00fct\u00b7zelnd", "die", "so", "zar\u00b7tals", "klei\u00b7nen", "H\u00e4ut\u00b7chen", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "ADV", "ADJA", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die aus dem Hirn bis in die Nase gehn.", "tokens": ["Die", "aus", "dem", "Hirn", "bis", "in", "die", "Na\u00b7se", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "APPR", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "An diesem Ort, woselbst die kleine Nervgen stehn,", "tokens": ["An", "die\u00b7sem", "Ort", ",", "wo\u00b7selbst", "die", "klei\u00b7ne", "Nerv\u00b7gen", "stehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PWAV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Empfangen sie durch das durchbohrte Bein,", "tokens": ["Emp\u00b7fan\u00b7gen", "sie", "durch", "das", "durch\u00b7bohr\u00b7te", "Bein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "Das wie ein Sieb daselbst in Netzen eingetheilet,", "tokens": ["Das", "wie", "ein", "Sieb", "da\u00b7selbst", "in", "Net\u00b7zen", "ein\u00b7ge\u00b7thei\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "KOKOM", "ART", "NN", "PAV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Den Druck der C\u00f6rper, die so klein,", "tokens": ["Den", "Druck", "der", "C\u00f6r\u00b7per", ",", "die", "so", "klein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "So leicht, da\u00df jeglicher schnell in die L\u00fcffte eilet.", "tokens": ["So", "leicht", ",", "da\u00df", "jeg\u00b7li\u00b7cher", "schnell", "in", "die", "L\u00fcff\u00b7te", "ei\u00b7let", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PIAT", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Woselbst denn vom Geruch der erste Druck entspringt,", "tokens": ["Wo\u00b7selbst", "denn", "vom", "Ge\u00b7ruch", "der", "ers\u00b7te", "Druck", "ent\u00b7springt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der denn darauf bis ins Gehirne dringt.", "tokens": ["Der", "denn", "da\u00b7rauf", "bis", "ins", "Ge\u00b7hir\u00b7ne", "dringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PAV", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}