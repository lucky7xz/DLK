{"textgrid.poem.43879": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "[hab ich mich einmahl vergangen]", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hab ich mich einmahl vergangen,", "tokens": ["Hab", "ich", "mich", "ein\u00b7mahl", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mach ich es doch wieder gut,", "tokens": ["Mach", "ich", "es", "doch", "wie\u00b7der", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da mein stumm und still Verlangen", "tokens": ["Da", "mein", "stumm", "und", "still", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJD", "KON", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deiner Sch\u00f6nheit Opfer thut,", "tokens": ["Dei\u00b7ner", "Sch\u00f6n\u00b7heit", "Op\u00b7fer", "thut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Deiner Sch\u00f6nheit am Verstande,", "tokens": ["Dei\u00b7ner", "Sch\u00f6n\u00b7heit", "am", "Ver\u00b7stan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die sich auch durch Mienen zeigt,", "tokens": ["Die", "sich", "auch", "durch", "Mie\u00b7nen", "zeigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die ungewohnten Bande", "tokens": ["Und", "die", "un\u00b7ge\u00b7wohn\u00b7ten", "Ban\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Machen, da\u00df mein Herze schweigt.", "tokens": ["Ma\u00b7chen", ",", "da\u00df", "mein", "Her\u00b7ze", "schweigt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Schweigen will ich mit dem Munde,", "tokens": ["Schwei\u00b7gen", "will", "ich", "mit", "dem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da das Herz nicht reden darf;", "tokens": ["Da", "das", "Herz", "nicht", "re\u00b7den", "darf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das Verh\u00e4ngn\u00fc\u00df dieser Stunde", "tokens": ["Das", "Ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", "die\u00b7ser", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Handelt etwas gar zu scharf;", "tokens": ["Han\u00b7delt", "et\u00b7was", "gar", "zu", "scharf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "PTKA", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich soll reimen und nicht wi\u00dfen,", "tokens": ["Ich", "soll", "rei\u00b7men", "und", "nicht", "wi\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "KON", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Was sich diesmahl reimen soll.", "tokens": ["Was", "sich", "dies\u00b7mahl", "rei\u00b7men", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "F\u00fclle nur mit deinen K\u00fc\u00dfen", "tokens": ["F\u00fcl\u00b7le", "nur", "mit", "dei\u00b7nen", "K\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die gesuchte Strophe voll.", "tokens": ["Die", "ge\u00b7such\u00b7te", "Stro\u00b7phe", "voll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "K\u00fc\u00dfe sind der Weg zum Lieben", "tokens": ["K\u00fc\u00b7\u00dfe", "sind", "der", "Weg", "zum", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "APPRART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der Geist der Poesie;", "tokens": ["Und", "der", "Geist", "der", "Poe\u00b7sie", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$."], "meter": "--+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Blindlings wird man oft getrieben,", "tokens": ["Blind\u00b7lings", "wird", "man", "oft", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIS", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df uns eine Sch\u00f6nheit zieh.", "tokens": ["Da\u00df", "uns", "ei\u00b7ne", "Sch\u00f6n\u00b7heit", "zieh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sch\u00f6nheit, B\u00e4ume, Gra\u00df und Nelcken,", "tokens": ["Sch\u00f6n\u00b7heit", ",", "B\u00e4u\u00b7me", ",", "Gra\u00df", "und", "Nel\u00b7cken", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Welche Lenz und Jugend zieht,", "tokens": ["Wel\u00b7che", "Lenz", "und", "Ju\u00b7gend", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "M\u00fc\u00dfen nach und nach verwelcken,", "tokens": ["M\u00fc\u00b7\u00dfen", "nach", "und", "nach", "ver\u00b7wel\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "KON", "APPR", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bis der Baum voll Mandeln bl\u00fcht.", "tokens": ["Bis", "der", "Baum", "voll", "Man\u00b7deln", "bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Bl\u00fchn schon einmahl diese Fr\u00fcchte,", "tokens": ["Bl\u00fchn", "schon", "ein\u00b7mahl", "die\u00b7se", "Fr\u00fcch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach, so ist es warlich aus,", "tokens": ["Ach", ",", "so", "ist", "es", "war\u00b7lich", "aus", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VAFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und des Alters Schaugerichte", "tokens": ["Und", "des", "Al\u00b7ters", "Schau\u00b7ge\u00b7rich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind ein erlner Blumenstrau\u00df,", "tokens": ["Sind", "ein", "erl\u00b7ner", "Blu\u00b7men\u00b7strau\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Welcher Mund und Augen locket,", "tokens": ["Wel\u00b7cher", "Mund", "und", "Au\u00b7gen", "lo\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aber, wenn er tragen soll,", "tokens": ["A\u00b7ber", ",", "wenn", "er", "tra\u00b7gen", "soll", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "So wie die Granaten stocket,", "tokens": ["So", "wie", "die", "Gra\u00b7na\u00b7ten", "sto\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Die nur sind zum Ansehn voll.", "tokens": ["Die", "nur", "sind", "zum", "An\u00b7sehn", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VAFIN", "APPRART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Mag's doch seyn! Ich will verehren,", "tokens": ["Mag's", "doch", "seyn", "!", "Ich", "will", "ver\u00b7eh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VAINF", "$.", "PPER", "VMFIN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was ich nicht genie\u00dfen kan;", "tokens": ["Was", "ich", "nicht", "ge\u00b7nie\u00b7\u00dfen", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wiltu meine Lieder h\u00f6ren,", "tokens": ["Wil\u00b7tu", "mei\u00b7ne", "Lie\u00b7der", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "O so h\u00f6r auch dieses an,", "tokens": ["O", "so", "h\u00f6r", "auch", "die\u00b7ses", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "ADV", "PDAT", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df der Strahl von deinem Glanze,", "tokens": ["Da\u00df", "der", "Strahl", "von", "dei\u00b7nem", "Glan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Welcher dich vor andern ziert,", "tokens": ["Wel\u00b7cher", "dich", "vor", "an\u00b7dern", "ziert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "PRF", "APPR", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Auch den Ruhm von meinem Kranze", "tokens": ["Auch", "den", "Ruhm", "von", "mei\u00b7nem", "Kran\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mit sich auf die Nachwelt f\u00fchrt.", "tokens": ["Mit", "sich", "auf", "die", "Nach\u00b7welt", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Hab ich mich einmahl vergangen,", "tokens": ["Hab", "ich", "mich", "ein\u00b7mahl", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mach ich es doch wieder gut,", "tokens": ["Mach", "ich", "es", "doch", "wie\u00b7der", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da mein stumm und still Verlangen", "tokens": ["Da", "mein", "stumm", "und", "still", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJD", "KON", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deiner Sch\u00f6nheit Opfer thut,", "tokens": ["Dei\u00b7ner", "Sch\u00f6n\u00b7heit", "Op\u00b7fer", "thut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Deiner Sch\u00f6nheit am Verstande,", "tokens": ["Dei\u00b7ner", "Sch\u00f6n\u00b7heit", "am", "Ver\u00b7stan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die sich auch durch Mienen zeigt,", "tokens": ["Die", "sich", "auch", "durch", "Mie\u00b7nen", "zeigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die ungewohnten Bande", "tokens": ["Und", "die", "un\u00b7ge\u00b7wohn\u00b7ten", "Ban\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Machen, da\u00df mein Herze schweigt.", "tokens": ["Ma\u00b7chen", ",", "da\u00df", "mein", "Her\u00b7ze", "schweigt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Schweigen will ich mit dem Munde,", "tokens": ["Schwei\u00b7gen", "will", "ich", "mit", "dem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da das Herz nicht reden darf;", "tokens": ["Da", "das", "Herz", "nicht", "re\u00b7den", "darf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das Verh\u00e4ngn\u00fc\u00df dieser Stunde", "tokens": ["Das", "Ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", "die\u00b7ser", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hand