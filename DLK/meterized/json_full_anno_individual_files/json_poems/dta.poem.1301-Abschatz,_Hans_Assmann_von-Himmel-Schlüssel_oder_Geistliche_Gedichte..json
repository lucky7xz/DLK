{"dta.poem.1301": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Himmel-Schl\u00fcssel  \n oder  \n Geistliche Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Die Nacht tritt nunmehr ein/ die mich ans Licht gebracht/", "tokens": ["Die", "Nacht", "tritt", "nun\u00b7mehr", "ein", "/", "die", "mich", "ans", "Licht", "ge\u00b7bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "$(", "PRELS", "PRF", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Jahr von meiner Zeit ist wiederum verschwunden/", "tokens": ["Ein", "Jahr", "von", "mei\u00b7ner", "Zeit", "ist", "wie\u00b7de\u00b7rum", "ver\u00b7schwun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie bald verfl\u00fcgen sich des Lebens kurtze Stunden/", "tokens": ["Wie", "bald", "ver\u00b7fl\u00fc\u00b7gen", "sich", "des", "Le\u00b7bens", "kurt\u00b7ze", "Stun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PRF", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die uns doch manche Noth so lang und bitter macht!", "tokens": ["Die", "uns", "doch", "man\u00b7che", "Noth", "so", "lang", "und", "bit\u00b7ter", "macht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PIAT", "NN", "ADV", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich habe manchen Tag und manche Nacht gewacht/", "tokens": ["Ich", "ha\u00b7be", "man\u00b7chen", "Tag", "und", "man\u00b7che", "Nacht", "ge\u00b7wacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "KON", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Betr\u00fcbni\u00df/ Kummer/ Sorg\u2019 und M\u00fche gnug empfunden:", "tokens": ["Be\u00b7tr\u00fcb\u00b7ni\u00df", "/", "Kum\u00b7mer", "/", "Sor\u00b7g'", "und", "M\u00fc\u00b7he", "gnug", "emp\u00b7fun\u00b7den", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NN", "KON", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die mich ohn Unterla\u00df zum Trauren angebunden/", "tokens": ["Die", "mich", "ohn", "Un\u00b7ter\u00b7la\u00df", "zum", "Trau\u00b7ren", "an\u00b7ge\u00b7bun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und dennoch zehl ich nun des Jahres lezte Nacht.", "tokens": ["Und", "den\u00b7noch", "zehl", "ich", "nun", "des", "Jah\u00b7res", "lez\u00b7te", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Des H\u00f6chsten Gunst hat mir verk\u00fcrtzet solche Zeit/", "tokens": ["Des", "H\u00f6chs\u00b7ten", "Gunst", "hat", "mir", "ver\u00b7k\u00fcrt\u00b7zet", "sol\u00b7che", "Zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "VVFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die auffgelegte Last auch selber helffen tragen/", "tokens": ["Die", "auff\u00b7ge\u00b7leg\u00b7te", "Last", "auch", "sel\u00b7ber", "helf\u00b7fen", "tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADV", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Durch Zeichen seiner Huld vers\u00fcsset alle Plagen/", "tokens": ["Durch", "Zei\u00b7chen", "sei\u00b7ner", "Huld", "ver\u00b7s\u00fcs\u00b7set", "al\u00b7le", "Pla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "VVFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was aber hab ich ihm erzeigt vor Danckbarkeit?", "tokens": ["Was", "a\u00b7ber", "hab", "ich", "ihm", "er\u00b7zeigt", "vor", "Dan\u00b7ck\u00b7bar\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "PPER", "PPER", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}, "stanza.4": {"line.1": {"text": "Der enge Lebens-Rest ist viel zu kurtz darzu/", "tokens": ["Der", "en\u00b7ge", "Le\u00b7bens\u00b7Rest", "ist", "viel", "zu", "kurtz", "dar\u00b7zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "PAV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was ich bi\u00dfher vers\u00e4umt/ geb\u00fchrlich einzubringen.", "tokens": ["Was", "ich", "bi\u00df\u00b7her", "ver\u00b7s\u00e4umt", "/", "ge\u00b7b\u00fchr\u00b7lich", "ein\u00b7zu\u00b7brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVPP", "$(", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Herr/ hilff mir bald dahin/ wo tausend Engel singen/", "tokens": ["Herr", "/", "hilff", "mir", "bald", "da\u00b7hin", "/", "wo", "tau\u00b7send", "En\u00b7gel", "sin\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "PPER", "ADV", "PAV", "$(", "PWAV", "CARD", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Damit ich meiner Pflicht ein volles Gn\u00fcgen thu.", "tokens": ["Da\u00b7mit", "ich", "mei\u00b7ner", "Pflicht", "ein", "vol\u00b7les", "Gn\u00fc\u00b7gen", "thu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Indessen weil ich hier noch Stunden z\u00e4hlen soll/", "tokens": ["In\u00b7des\u00b7sen", "weil", "ich", "hier", "noch", "Stun\u00b7den", "z\u00e4h\u00b7len", "soll", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "ADV", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So la\u00df mich solche Zeit zu deinem Dienst anwenden;", "tokens": ["So", "la\u00df", "mich", "sol\u00b7che", "Zeit", "zu", "dei\u00b7nem", "Dienst", "an\u00b7wen\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Seele forder dann zu deinen sichern H\u00e4nden/", "tokens": ["Die", "See\u00b7le", "for\u00b7der", "dann", "zu", "dei\u00b7nen", "si\u00b7chern", "H\u00e4n\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und la\u00df den m\u00fcden Leib im Grabe ruhen wohl.", "tokens": ["Und", "la\u00df", "den", "m\u00fc\u00b7den", "Leib", "im", "Gra\u00b7be", "ru\u00b7hen", "wohl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}