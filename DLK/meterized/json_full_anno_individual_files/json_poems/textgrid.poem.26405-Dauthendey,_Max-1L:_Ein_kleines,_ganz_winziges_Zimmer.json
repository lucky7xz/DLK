chen", "ver\u00b7zei\u00b7hen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ADJD", "ADV", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ach, Lieb' ist nicht nur ein Gedicht!", "tokens": ["Ach", ",", "Lieb'", "ist", "nicht", "nur", "ein", "Ge\u00b7dicht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "VAFIN", "PTKNEG", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.25": {"line.1": {"text": "Die Lieb' ist entsetzlich zu tragen!", "tokens": ["Die", "Lieb'", "ist", "ent\u00b7setz\u00b7lich", "zu", "tra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und haupts\u00e4chlich, wenn was entsteht.", "tokens": ["Und", "haupt\u00b7s\u00e4ch\u00b7lich", ",", "wenn", "was", "ent\u00b7steht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KOUS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Niemandem darf man es sagen,", "tokens": ["Und", "Nie\u00b7man\u00b7dem", "darf", "man", "es", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "PIS", "PPER", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Wenn's nicht standesamtlich hergeht.", "tokens": ["Wenn's", "nicht", "stan\u00b7des\u00b7amt\u00b7lich", "her\u00b7geht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.26": {"line.1": {"text": "Denn alle es st\u00fcndlich betonen:", "tokens": ["Denn", "al\u00b7le", "es", "st\u00fcnd\u00b7lich", "be\u00b7to\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PPER", "ADJD", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Vermeide Geschichten mit Herrn!", "tokens": ["Ver\u00b7mei\u00b7de", "Ge\u00b7schich\u00b7ten", "mit", "Herrn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und hier gleich danebenan wohnen", "tokens": ["Und", "hier", "gleich", "da\u00b7ne\u00b7be\u00b7nan", "woh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "N\u00e4hm\u00e4dchen, die horchen so gern.", "tokens": ["N\u00e4h\u00b7m\u00e4d\u00b7chen", ",", "die", "hor\u00b7chen", "so", "gern", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "VVFIN", "ADV", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.27": {"line.1": {"text": "Kein Seufzer, kein Schrei darf hindringen.", "tokens": ["Kein", "Seuf\u00b7zer", ",", "kein", "Schrei", "darf", "hin\u00b7drin\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die Zungen, die schl\u00fcgen mich tot. \u2013", "tokens": ["Die", "Zun\u00b7gen", ",", "die", "schl\u00fc\u00b7gen", "mich", "tot", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "PPER", "ADJD", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Die Esther, sie f\u00e4ngt an zu singen,", "tokens": ["Die", "E\u00b7sther", ",", "sie", "f\u00e4ngt", "an", "zu", "sin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie singt im Gesang sich blaurot.", "tokens": ["Sie", "singt", "im", "Ge\u00b7sang", "sich", "blau\u00b7rot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PRF", "ADJD", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.28": {"line.1": {"text": "Sie singt hohe Coloraturen", "tokens": ["Sie", "singt", "ho\u00b7he", "Co\u00b7lo\u00b7ra\u00b7tu\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und singt viele Volkslieder laut;", "tokens": ["Und", "singt", "vie\u00b7le", "Volks\u00b7lie\u00b7der", "laut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Erleidet geb\u00e4rend Torturen,", "tokens": ["Er\u00b7lei\u00b7det", "ge\u00b7b\u00e4\u00b7rend", "Tor\u00b7tu\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "VVPP", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Weil sie sich zu schreien nicht traut.", "tokens": ["Weil", "sie", "sich", "zu", "schrei\u00b7en", "nicht", "traut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKZU", "VVFIN", "PTKNEG", "VVFIN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.29": {"line.1": {"text": "Daneben die N\u00e4htermaschinen,", "tokens": ["Da\u00b7ne\u00b7ben", "die", "N\u