{"textgrid.poem.26405": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein kleines, ganz winziges Zimmer", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein kleines, ganz winziges Zimmer", "tokens": ["Ein", "klei\u00b7nes", ",", "ganz", "win\u00b7zi\u00b7ges", "Zim\u00b7mer"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADV", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Sieht oben vom Dach auf die Welt.", "tokens": ["Sieht", "o\u00b7ben", "vom", "Dach", "auf", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Im Fenster liegt Abendgeflimmer.", "tokens": ["Im", "Fens\u00b7ter", "liegt", "A\u00b7bend\u00b7ge\u00b7flim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Keins ahnt, was das Zimmer enth\u00e4lt.", "tokens": ["Keins", "ahnt", ",", "was", "das", "Zim\u00b7mer", "ent\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Im Zimmer da sitzt Fr\u00e4ulein Esther,", "tokens": ["Im", "Zim\u00b7mer", "da", "sitzt", "Fr\u00e4u\u00b7lein", "E\u00b7sther", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "NN", "NE", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Gouvernante von unten im Haus.", "tokens": ["Gou\u00b7ver\u00b7nan\u00b7te", "von", "un\u00b7ten", "im", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADV", "APPRART", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Sie pre\u00dft ihre Z\u00e4hne noch fester,", "tokens": ["Sie", "pre\u00dft", "ih\u00b7re", "Z\u00e4h\u00b7ne", "noch", "fes\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Denn traurig sieht's rings um sie aus.", "tokens": ["Denn", "trau\u00b7rig", "sieht's", "rings", "um", "sie", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ADV", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Zum Teufel man soll's ja nicht sehen!", "tokens": ["Zum", "Teu\u00b7fel", "man", "soll's", "ja", "nicht", "se\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIS", "VMFIN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Leute sind so ideal!", "tokens": ["Die", "Leu\u00b7te", "sind", "so", "i\u00b7deal", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.3": {"text": "Die Esther liegt n\u00e4mlich in Wehen,", "tokens": ["Die", "E\u00b7sther", "liegt", "n\u00e4m\u00b7lich", "in", "We\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die Wehen, die werden fatal.", "tokens": ["Die", "We\u00b7hen", ",", "die", "wer\u00b7den", "fa\u00b7tal", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VAFIN", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Die Esther, die pre\u00dft ihre H\u00e4nde,", "tokens": ["Die", "E\u00b7sther", ",", "die", "pre\u00dft", "ih\u00b7re", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die Esther verzerrt ihr Gesicht.", "tokens": ["Die", "E\u00b7sther", "ver\u00b7zerrt", "ihr", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "O Gott, wenn so Jemand sie f\u00e4nde!", "tokens": ["O", "Gott", ",", "wenn", "so", "Je\u00b7mand", "sie", "f\u00e4n\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KOUS", "ADV", "PIS", "PPER", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Auf Sitte legt jeder Gewicht.", "tokens": ["Auf", "Sit\u00b7te", "legt", "je\u00b7der", "Ge\u00b7wicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Besonders beim Stande der Lehrer \u2013", "tokens": ["Be\u00b7son\u00b7ders", "beim", "Stan\u00b7de", "der", "Leh\u00b7rer", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die Esther, sie pre\u00dft auf den Mund", "tokens": ["Die", "E\u00b7sther", ",", "sie", "pre\u00dft", "auf", "den", "Mund"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Aus Marmor den Briefebeschwerer,", "tokens": ["Aus", "Mar\u00b7mor", "den", "Brie\u00b7fe\u00b7be\u00b7schwe\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Der erst auf dem Schreibtische stund.", "tokens": ["Der", "erst", "auf", "dem", "Schreib\u00b7ti\u00b7sche", "stund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Gottvater, sie darf ja nicht schreien!", "tokens": ["Gott\u00b7va\u00b7ter", ",", "sie", "darf", "ja", "nicht", "schrei\u00b7en", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VMFIN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "O Himmel, Du ahnst es wohl nicht,", "tokens": ["O", "Him\u00b7mel", ",", "Du", "ahnst", "es", "wohl", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Wie schwer heut' die Menschen verzeihen. \u2013", "tokens": ["Wie", "schwer", "heut'", "die", "Men\u00b7schen", "ver\u00b7zei\u00b7hen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ADJD", "ADV", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ach, Lieb' ist nicht nur ein Gedicht!", "tokens": ["Ach", ",", "Lieb'", "ist", "nicht", "nur", "ein", "Ge\u00b7dicht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "VAFIN", "PTKNEG", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Die Lieb' ist entsetzlich zu tragen!", "tokens": ["Die", "Lieb'", "ist", "ent\u00b7setz\u00b7lich", "zu", "tra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und haupts\u00e4chlich, wenn was entsteht.", "tokens": ["Und", "haupt\u00b7s\u00e4ch\u00b7lich", ",", "wenn", "was", "ent\u00b7steht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KOUS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Niemandem darf man es sagen,", "tokens": ["Und", "Nie\u00b7man\u00b7dem", "darf", "man", "es", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "PIS", "PPER", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Wenn's nicht standesamtlich hergeht.", "tokens": ["Wenn's", "nicht", "stan\u00b7des\u00b7amt\u00b7lich", "her\u00b7geht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.8": {"line.1": {"text": "Denn alle es st\u00fcndlich betonen:", "tokens": ["Denn", "al\u00b7le", "es", "st\u00fcnd\u00b7lich", "be\u00b7to\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PPER", "ADJD", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Vermeide Geschichten mit Herrn!", "tokens": ["Ver\u00b7mei\u00b7de", "Ge\u00b7schich\u00b7ten", "mit", "Herrn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und hier gleich danebenan wohnen", "tokens": ["Und", "hier", "gleich", "da\u00b7ne\u00b7be\u00b7nan", "woh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "N\u00e4hm\u00e4dchen, die horchen so gern.", "tokens": ["N\u00e4h\u00b7m\u00e4d\u00b7chen", ",", "die", "hor\u00b7chen", "so", "gern", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "VVFIN", "ADV", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.9": {"line.1": {"text": "Kein Seufzer, kein Schrei darf hindringen.", "tokens": ["Kein", "Seuf\u00b7zer", ",", "kein", "Schrei", "darf", "hin\u00b7drin\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die Zungen, die schl\u00fcgen mich tot. \u2013", "tokens": ["Die", "Zun\u00b7gen", ",", "die", "schl\u00fc\u00b7gen", "mich", "tot", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "PPER", "ADJD", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Die Esther, sie f\u00e4ngt an zu singen,", "tokens": ["Die", "E\u00b7sther", ",", "sie", "f\u00e4ngt", "an", "zu", "sin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie singt im Gesang sich blaurot.", "tokens": ["Sie", "singt", "im", "Ge\u00b7sang", "sich", "blau\u00b7rot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PRF", "ADJD", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "Sie singt hohe Coloraturen", "tokens": ["Sie", "singt", "ho\u00b7he", "Co\u00b7lo\u00b7ra\u00b7tu\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und singt viele Volkslieder laut;", "tokens": ["Und", "singt", "vie\u00b7le", "Volks\u00b7lie\u00b7der", "laut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Erleidet geb\u00e4rend Torturen,", "tokens": ["Er\u00b7lei\u00b7det", "ge\u00b7b\u00e4\u00b7rend", "Tor\u00b7tu\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "VVPP", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Weil sie sich zu schreien nicht traut.", "tokens": ["Weil", "sie", "sich", "zu", "schrei\u00b7en", "nicht", "traut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKZU", "VVFIN", "PTKNEG", "VVFIN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.11": {"line.1": {"text": "Daneben die N\u00e4htermaschinen,", "tokens": ["Da\u00b7ne\u00b7ben", "die", "N\u00e4h\u00b7ter\u00b7ma\u00b7schi\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die poltern und stampfen, gottlob!", "tokens": ["Die", "pol\u00b7tern", "und", "stamp\u00b7fen", ",", "gott\u00b7lob", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "KON", "VVFIN", "$,", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Sie rasseln wild \u00fcberm Verdienen,", "tokens": ["Sie", "ras\u00b7seln", "wild", "\u00fc\u00b7berm", "Ver\u00b7die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Da\u00df leis' sich manch' Seufzer dreinschob.", "tokens": ["Da\u00df", "leis'", "sich", "man\u00b7ch'", "Seuf\u00b7zer", "drein\u00b7schob", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PRF", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.12": {"line.1": {"text": "Und dann singt sie wieder hinrei\u00dfend,", "tokens": ["Und", "dann", "singt", "sie", "wie\u00b7der", "hin\u00b7rei\u00b7\u00dfend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sie denkt: ach, die Lieb' war es wert,", "tokens": ["Sie", "denkt", ":", "ach", ",", "die", "Lieb'", "war", "es", "wert", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "XY", "$,", "ART", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+++-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Wenn man, seine Schmerzen verbei\u00dfend,", "tokens": ["Wenn", "man", ",", "sei\u00b7ne", "Schmer\u00b7zen", "ver\u00b7bei\u00b7\u00dfend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Im Singen die Kinder geb\u00e4rt.", "tokens": ["Im", "Sin\u00b7gen", "die", "Kin\u00b7der", "ge\u00b7b\u00e4rt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.13": {"line.1": {"text": "Denn Liebe kann m\u00e4chtig erheben", "tokens": ["Denn", "Lie\u00b7be", "kann", "m\u00e4ch\u00b7tig", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VMFIN", "ADJD", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und klein ist dagegen das Leid.", "tokens": ["Und", "klein", "ist", "da\u00b7ge\u00b7gen", "das", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PAV", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Die Menschen, die nie was erleben,", "tokens": ["Die", "Men\u00b7schen", ",", "die", "nie", "was", "er\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "PIS", "VVINF", "$,"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die urteilen auch nicht gescheit.", "tokens": ["Die", "ur\u00b7tei\u00b7len", "auch", "nicht", "ge\u00b7scheit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Nur sp\u00e4t erst in n\u00e4chtlichen Stunden,", "tokens": ["Nur", "sp\u00e4t", "erst", "in", "n\u00e4cht\u00b7li\u00b7chen", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Da haben, voll t\u00f6richter Scham,", "tokens": ["Da", "ha\u00b7ben", ",", "voll", "t\u00f6\u00b7rich\u00b7ter", "Scham", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die N\u00e4hm\u00e4dchen Esther gefunden,", "tokens": ["Die", "N\u00e4h\u00b7m\u00e4d\u00b7chen", "E\u00b7sther", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-++--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Als ihr schon das Sterben ankam.", "tokens": ["Als", "ihr", "schon", "das", "Ster\u00b7ben", "an\u00b7kam", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.15": {"line.1": {"text": "Die M\u00e4dchen, sie h\u00f6rten im Winde,", "tokens": ["Die", "M\u00e4d\u00b7chen", ",", "sie", "h\u00f6r\u00b7ten", "im", "Win\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Der nachts um die Dachstuben strich,", "tokens": ["Der", "nachts", "um", "die", "Dach\u00b7stu\u00b7ben", "strich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Die Stimme wie von einem Kinde,", "tokens": ["Die", "Stim\u00b7me", "wie", "von", "ei\u00b7nem", "Kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die nicht von den Ohren mehr wich.", "tokens": ["Die", "nicht", "von", "den", "Oh\u00b7ren", "mehr", "wich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "APPR", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.16": {"line.1": {"text": "Erst glaubte man, 's k\u00e4m vom Kamine.", "tokens": ["Erst", "glaub\u00b7te", "man", ",", "'s", "k\u00e4m", "vom", "Ka\u00b7mi\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da horchte man bald an der Wand.", "tokens": ["Da", "horch\u00b7te", "man", "bald", "an", "der", "Wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die J\u00fcngste, genannt Josephine,", "tokens": ["Die", "J\u00fcngs\u00b7te", ",", "ge\u00b7nannt", "Jo\u00b7se\u00b7phi\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "NE", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Am schnellsten die Sache verstand.", "tokens": ["Am", "schnells\u00b7ten", "die", "Sa\u00b7che", "ver\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.17": {"line.1": {"text": "\u00bbdaneben hats Fr\u00e4ulein geboren,", "tokens": ["\u00bb", "da\u00b7ne\u00b7ben", "hats", "Fr\u00e4u\u00b7lein", "ge\u00b7bo\u00b7ren", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PAV", "VAFIN", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Durchs Schl\u00fcsselloch sah ich's im Gang.\u00ab", "tokens": ["Durchs", "Schl\u00fcs\u00b7sel\u00b7loch", "sah", "ich's", "im", "Gang", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIS", "APPRART", "NN", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Sie rief es, rot bis in die Ohren,", "tokens": ["Sie", "rief", "es", ",", "rot", "bis", "in", "die", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADJD", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weil's doch gar so aufregend klang.", "tokens": ["Weil's", "doch", "gar", "so", "auf\u00b7re\u00b7gend", "klang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.18": {"line.1": {"text": "Die Esther, gestorben im Singen,", "tokens": ["Die", "E\u00b7sther", ",", "ge\u00b7stor\u00b7ben", "im", "Sin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Lag neben dem Sopha in Ruh'.", "tokens": ["Lag", "ne\u00b7ben", "dem", "So\u00b7pha", "in", "Ruh'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Im Hause die Klatschm\u00e4uler gingen,", "tokens": ["Im", "Hau\u00b7se", "die", "Klatsc\u00b7hm\u00e4u\u00b7ler", "gin\u00b7gen", ","], "token_i