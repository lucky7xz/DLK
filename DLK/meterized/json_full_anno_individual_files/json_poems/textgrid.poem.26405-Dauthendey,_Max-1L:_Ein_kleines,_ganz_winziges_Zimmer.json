, "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die Wehen, die werden fatal.", "tokens": ["Die", "We\u00b7hen", ",", "die", "wer\u00b7den", "fa\u00b7tal", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VAFIN", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.22": {"line.1": {"text": "Die Esther, die pre\u00dft ihre H\u00e4nde,", "tokens": ["Die", "E\u00b7sther", ",", "die", "pre\u00dft", "ih\u00b7re", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die Esther verzerrt ihr Gesicht.", "tokens": ["Die", "E\u00b7sther", "ver\u00b7zerrt", "ihr", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "O Gott, wenn so Jemand sie f\u00e4nde!", "tokens": ["O", "Gott", ",", "wenn", "so", "Je\u00b7mand", "sie", "f\u00e4n\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KOUS", "ADV", "PIS", "PPER", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Auf Sitte legt jeder Gewicht.", "tokens": ["Auf", "Sit\u00b7te", "legt", "je\u00b7der", "Ge\u00b7wicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.23": {"line.1": {"text": "Besonders beim Stande der Lehrer \u2013", "tokens": ["Be\u00b7son\u00b7ders", "beim", "Stan\u00b7de", "der", "Leh\u0