{"textgrid.poem.65313": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Violen und Rosen", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich ging in's Feld und wollte Blumen holen:", "tokens": ["Ich", "ging", "in's", "Feld", "und", "woll\u00b7te", "Blu\u00b7men", "ho\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "KON", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da sah ich stehn ein Kind in den Violen", "tokens": ["Da", "sah", "ich", "stehn", "ein", "Kind", "in", "den", "Vi\u00b7o\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit Lippen, wie zwei junge Rosenknospen.", "tokens": ["Mit", "Lip\u00b7pen", ",", "wie", "zwei", "jun\u00b7ge", "Ro\u00b7sen\u00b7knos\u00b7pen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOKOM", "CARD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nun waren die Violen gleich verschwunden,", "tokens": ["Nun", "wa\u00b7ren", "die", "Vi\u00b7o\u00b7len", "gleich", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich konnte meine Augen nimmer wenden", "tokens": ["Ich", "konn\u00b7te", "mei\u00b7ne", "Au\u00b7gen", "nim\u00b7mer", "wen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von jenen Rosenknospen ihres Mundes.", "tokens": ["Von", "je\u00b7nen", "Ro\u00b7sen\u00b7knos\u00b7pen", "ih\u00b7res", "Mun\u00b7des", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und als die Mutter frug: Wo sind die Blumen,", "tokens": ["Und", "als", "die", "Mut\u00b7ter", "frug", ":", "Wo", "sind", "die", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$.", "PWAV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da\u00df wir sie morgen mit zu Markte nehmen?", "tokens": ["Da\u00df", "wir", "sie", "mor\u00b7gen", "mit", "zu", "Mark\u00b7te", "neh\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "APPR", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sprach ich: Die Knospen sind noch nicht gesprungen.", "tokens": ["Sprach", "ich", ":", "Die", "Knos\u00b7pen", "sind", "noch", "nicht", "ge\u00b7sprun\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "ART", "NN", "VAFIN", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich ging in's Feld und wollte Blumen holen:", "tokens": ["Ich", "ging", "in's", "Feld", "und", "woll\u00b7te", "Blu\u00b7men", "ho\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN