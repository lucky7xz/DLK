{"dta.poem.13130": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Achtes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201edie Schlange, dieser Hahn bedeuten meine Kraft,", "tokens": ["\u201e", "die", "Schlan\u00b7ge", ",", "die\u00b7ser", "Hahn", "be\u00b7deu\u00b7ten", "mei\u00b7ne", "Kraft", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "PDAT", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "660\u201dWeil meine Macht so viel als ihre Tugend schafft,", "tokens": ["\"", "Weil", "mei\u00b7ne", "Macht", "so", "viel", "als", "ih\u00b7re", "Tu\u00b7gend", "schafft", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPOSAT", "NN", "ADV", "PIAT", "KOKOM", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201eda diese Thiere nichts, was Schwachheit heisset, bl\u00f6det.", "tokens": ["\u201e", "da", "die\u00b7se", "Thie\u00b7re", "nichts", ",", "was", "Schwach\u00b7heit", "heis\u00b7set", ",", "bl\u00f6\u00b7det", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "KOUS", "PDAT", "NN", "PIS", "$,", "PWS", "NN", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201ewo man von Wachsamkeit f\u00fcr die Gesundheit redet;", "tokens": ["\u201e", "wo", "man", "von", "Wach\u00b7sam\u00b7keit", "f\u00fcr", "die", "Ge\u00b7sund\u00b7heit", "re\u00b7det", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PIS", "APPR", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201eder Schlange gleicht mein Herz; es ist, wie sie, gesund;", "tokens": ["\u201e", "der", "Schlan\u00b7ge", "gleicht", "mein", "Herz", ";", "es", "ist", ",", "wie", "sie", ",", "ge\u00b7sund", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "$,", "PWAV", "PPER", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201edem Hahne jener Arzt, den meine Vorsicht fund;", "tokens": ["\u201e", "dem", "Hah\u00b7ne", "je\u00b7ner", "Arzt", ",", "den", "mei\u00b7ne", "Vor\u00b7sicht", "fund", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "PDAT", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "665\u201dDamit, wann ich vielleicht in Wanckelmuth verfalle,", "tokens": ["\"", "Da\u00b7mit", ",", "wann", "ich", "viel\u00b7leicht", "in", "Wan\u00b7ckel\u00b7muth", "ver\u00b7fal\u00b7le", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PAV", "$,", "PWAV", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201e(Der Sachen Unbestand und Wechsel trifft uns alle)", "tokens": ["\u201e", "(", "Der", "Sa\u00b7chen", "Un\u00b7be\u00b7stand", "und", "Wech\u00b7sel", "trifft", "uns", "al\u00b7le", ")"], "token_info": ["punct", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "ART", "NN", "NN", "KON", "NN", "VVFIN", "PPER", "PIAT", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201eer meine Sorge trag\u2019 und kluge Wachsamkeit;", "tokens": ["\u201e", "er", "mei\u00b7ne", "Sor\u00b7ge", "trag'", "und", "klu\u00b7ge", "Wach\u00b7sam\u00b7keit", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "PPOSAT", "NN", "VVFIN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201ef\u00fcr mich, was mir ersprie\u00dft, ergr\u00fcnd\u2019 und zubereit.", "tokens": ["\u201e", "f\u00fcr", "mich", ",", "was", "mir", "er\u00b7sprie\u00dft", ",", "er\u00b7gr\u00fcnd'", "und", "zu\u00b7be\u00b7reit", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPER", "$,", "PWS", "PPER", "VVFIN", "$,", "ADJD", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201eich hab aus Tausenden denselbigen erkohren,", "tokens": ["\u201e", "ich", "hab", "aus", "Tau\u00b7sen\u00b7den", "den\u00b7sel\u00b7bi\u00b7gen", "er\u00b7koh\u00b7ren", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "APPR", "NN", "PDS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "670\u201dFlei\u00df und Natur hat ihn zu diesem Ziel gebohren.", "tokens": ["\"", "Flei\u00df", "und", "Na\u00b7tur", "hat", "ihn", "zu", "die\u00b7sem", "Ziel", "ge\u00b7boh\u00b7ren", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "KON", "NN", "VAFIN", "PPER", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.13": {"text": "\u201ewer jene Kr\u00e4fte st\u00e4rckt, womit man Kronen tr\u00e4gt,", "tokens": ["\u201e", "wer", "je\u00b7ne", "Kr\u00e4f\u00b7te", "st\u00e4rckt", ",", "wo\u00b7mit", "man", "Kro\u00b7nen", "tr\u00e4gt", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "PDAT", "NN", "VVFIN", "$,", "PWAV", "PIS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "\u201edem wird auch der Verdienst der B\u00fcrde beygelegt.", "tokens": ["\u201e", "dem", "wird", "auch", "der", "Ver\u00b7dienst", "der", "B\u00fcr\u00b7de", "bey\u00b7ge\u00b7legt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "ADV", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "\u201eder Arm, der ohne mich mu\u00df einen Zepter tragen,", "tokens": ["\u201e", "der", "Arm", ",", "der", "oh\u00b7ne", "mich", "mu\u00df", "ei\u00b7nen", "Zep\u00b7ter", "tra\u00b7gen", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "PRELS", "APPR", "PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "\u201eist, wie ein schwaches Schiff bey bestem Wind, zu klagen.", "tokens": ["\u201e", "ist", ",", "wie", "ein", "schwa\u00b7ches", "Schiff", "bey", "bes\u00b7tem", "Wind", ",", "zu", "kla\u00b7gen", "."], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VAFIN", "$,", "PWAV", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}