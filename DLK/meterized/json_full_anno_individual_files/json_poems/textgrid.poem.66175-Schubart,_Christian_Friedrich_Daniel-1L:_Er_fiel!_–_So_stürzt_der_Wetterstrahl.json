{"textgrid.poem.66175": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Er fiel! \u2013 So st\u00fcrzt der Wetterstrahl", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er fiel! \u2013 So st\u00fcrzt der Wetterstrahl", "tokens": ["Er", "fiel", "!", "\u2013", "So", "st\u00fcrzt", "der", "Wet\u00b7ter\u00b7strahl"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "$(", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Berges Tanne nieder!", "tokens": ["Des", "Ber\u00b7ges", "Tan\u00b7ne", "nie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ach, ", "tokens": ["Ach", ","], "token_info": ["word", "punct"], "pos": ["ITJ", "$,"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Denn solchen Vater, solchen Freund,", "tokens": ["Denn", "sol\u00b7chen", "Va\u00b7ter", ",", "sol\u00b7chen", "Freund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wo finden wir ihn wieder?", "tokens": ["Wo", "fin\u00b7den", "wir", "ihn", "wie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Gott! welch ein Anblick war's, als wir", "tokens": ["Gott", "!", "welch", "ein", "An\u00b7blick", "wa\u00b7r's", ",", "als", "wir"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$.", "PWAT", "ART", "NN", "VAFIN", "$,", "KOUS", "PPER"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Um seine Leiche standen!", "tokens": ["Um", "sei\u00b7ne", "Lei\u00b7che", "stan\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und ach! den schrecklichen Verlust,", "tokens": ["Und", "ach", "!", "den", "schreck\u00b7li\u00b7chen", "Ver\u00b7lust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Dolch gedreht in unsre Brust,", "tokens": ["Den", "Dolch", "ge\u00b7dreht", "in", "uns\u00b7re", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit vollem Schmerz empfanden!", "tokens": ["Mit", "vol\u00b7lem", "Schmerz", "emp\u00b7fan\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der graue Krieger zitterte,", "tokens": ["Der", "grau\u00b7e", "Krie\u00b7ger", "zit\u00b7ter\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Stand stumm und weinte lange!", "tokens": ["Stand", "stumm", "und", "wein\u00b7te", "lan\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "VVFIN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dem j\u00fcngern Krieger brannt' das Herz;", "tokens": ["Dem", "j\u00fcn\u00b7gern", "Krie\u00b7ger", "brannt'", "das", "Herz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In hei\u00dfen Tropfen quoll sein Schmerz", "tokens": ["In", "hei\u00b7\u00dfen", "Trop\u00b7fen", "quoll", "sein", "Schmerz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von sonnenbrauner Wange.", "tokens": ["Von", "son\u00b7nen\u00b7brau\u00b7ner", "Wan\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ein Todtenacker scheint der Berg,", "tokens": ["Ein", "Tod\u00b7te\u00b7na\u00b7cker", "scheint", "der", "Berg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Begl\u00e4nzt vom Mondenlichte.", "tokens": ["Be\u00b7gl\u00e4nzt", "vom", "Mon\u00b7den\u00b7lich\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Soldaten stehen weggewandt,", "tokens": ["Sol\u00b7da\u00b7ten", "ste\u00b7hen", "weg\u00b7ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und trocknen sich mit rauher Hand", "tokens": ["Und", "trock\u00b7nen", "sich", "mit", "rau\u00b7her", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Thr\u00e4nen vom Gesichte.", "tokens": ["Die", "Thr\u00e4\u00b7nen", "vom", "Ge\u00b7sich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ha, Kamerad! m\u00f6cht' heulen laut,", "tokens": ["Ha", ",", "Ka\u00b7me\u00b7rad", "!", "m\u00f6cht'", "heu\u00b7len", "laut", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$.", "VMFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So sprechen harte Krieger;", "tokens": ["So", "spre\u00b7chen", "har\u00b7te", "Krie\u00b7ger", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Denn unser General ist todt,", "tokens": ["Denn", "un\u00b7ser", "Ge\u00b7ne\u00b7ral", "ist", "todt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der liebreich uns die Rechte bot,", "tokens": ["Der", "lieb\u00b7reich", "uns", "die", "Rech\u00b7te", "bot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach! unser Vater ", "tokens": ["Ach", "!", "un\u00b7ser", "Va\u00b7ter"], "token_info": ["word", "punct", "word", "word"], "pos": ["ITJ", "$.", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Gott wei\u00df, er war Soldatenfreund,", "tokens": ["Gott", "wei\u00df", ",", "er", "war", "Sol\u00b7da\u00b7ten\u00b7freund", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PPER", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Half Leidenden und Armen;", "tokens": ["Half", "Lei\u00b7den\u00b7den", "und", "Ar\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zwar hagelscharf dem B\u00f6sewicht,", "tokens": ["Zwar", "ha\u00b7gel\u00b7scharf", "dem", "B\u00f6\u00b7se\u00b7wicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch Braven mild, wie Maienlicht,", "tokens": ["Doch", "Bra\u00b7ven", "mild", ",", "wie", "Mai\u00b7en\u00b7licht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "$,", "PWAV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Voll Mitleid und Erbarmen.", "tokens": ["Voll", "Mit\u00b7leid", "und", "Er\u00b7bar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "Einsamer Asperg! traure nur,", "tokens": ["Ein\u00b7sa\u00b7mer", "As\u00b7perg", "!", "trau\u00b7re", "nur", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Vater ist geschieden!", "tokens": ["Dein", "Va\u00b7ter", "ist", "ge\u00b7schie\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der deine Scheitel so geziert,", "tokens": ["Der", "dei\u00b7ne", "Schei\u00b7tel", "so", "ge\u00b7ziert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der so mit Weisheit dich regiert,", "tokens": ["Der", "so", "mit", "Weis\u00b7heit", "dich", "re\u00b7giert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dein Vater ist geschieden!", "tokens": ["Dein", "Va\u00b7ter", "ist", "ge\u00b7schie\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ha, Tr\u00e4ger! nehmt die Leiche nur", "tokens": ["Ha", ",", "Tr\u00e4\u00b7ger", "!", "nehmt", "die", "Lei\u00b7che", "nur"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "NN", "$.", "VVFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verstummend auf den R\u00fccken;", "tokens": ["Ver\u00b7stum\u00b7mend", "auf", "den", "R\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir schreiten schweigend hinterher", "tokens": ["Wir", "schrei\u00b7ten", "schwei\u00b7gend", "hin\u00b7ter\u00b7her"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit grabgesunkenem Gewehr", "tokens": ["Mit", "grab\u00b7ge\u00b7sun\u00b7ke\u00b7nem", "Ge\u00b7wehr"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und thr\u00e4nenschweren Blicken.", "tokens": ["Und", "thr\u00e4\u00b7nen\u00b7schwe\u00b7ren", "Bli\u00b7cken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "O, lauter als Kanonenschu\u00df", "tokens": ["O", ",", "lau\u00b7ter", "als", "Ka\u00b7no\u00b7nen\u00b7schu\u00df"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PIAT", "KOKOM", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Krachen kleiner Wehre,", "tokens": ["Und", "Kra\u00b7chen", "klei\u00b7ner", "Weh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Schickt, Vater! dein Batallion", "tokens": ["Schickt", ",", "Va\u00b7ter", "!", "dein", "Ba\u00b7tal\u00b7li\u00b7on"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "NN", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Dank vom Grab zu Gottes Thron,", "tokens": ["Den", "Dank", "vom", "Grab", "zu", "Got\u00b7tes", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Getaucht in manche Z\u00e4hre.", "tokens": ["Ge\u00b7taucht", "in", "man\u00b7che", "Z\u00e4h\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Ja, tausend Dank! Ach tausend Dank,", "tokens": ["Ja", ",", "tau\u00b7send", "Dank", "!", "Ach", "tau\u00b7send", "Dank", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "CARD", "NN", "$.", "ITJ", "CARD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Vater f\u00fcr uns alle!", "tokens": ["O", "Va\u00b7ter", "f\u00fcr", "uns", "al\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "PPER", "PIS", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ja wohl, ein Vater warst uns du!", "tokens": ["Ja", "wohl", ",", "ein", "Va\u00b7ter", "warst", "uns", "du", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "$,", "ART", "NN", "VAFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zieh' ein, zieh' ein zur ew'gen Ruh'", "tokens": ["Zieh'", "ein", ",", "zieh'", "ein", "zur", "ew'\u00b7gen", "Ruh'"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PTKVZ", "$,", "VVFIN", "ART", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In deine Todtenhalle.", "tokens": ["In", "dei\u00b7ne", "Tod\u00b7ten\u00b7hal\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Gott selbst im Himmel lohn' es dir,", "tokens": ["Gott", "selbst", "im", "Him\u00b7mel", "lohn'", "es", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was du uns hier erwiesen!", "tokens": ["Was", "du", "uns", "hier", "er\u00b7wie\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit Himmelsruh' und Herrlichkeit,", "tokens": ["Mit", "Him\u00b7mels\u00b7ruh'", "und", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit ew'gem Frieden nach dem Streit \u2013", "tokens": ["Mit", "ew'\u00b7gem", "Frie\u00b7den", "nach", "dem", "Streit", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O, tausend Thr\u00e4nen flie\u00dfen!", "tokens": ["O", ",", "tau\u00b7send", "Thr\u00e4\u00b7nen", "flie\u00b7\u00dfen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Geist ", "tokens": ["Geist"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Aus deiner Wolkenh\u00fclle,", "tokens": ["Aus", "dei\u00b7ner", "Wol\u00b7ken\u00b7h\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und h\u00f6r' des Weinens Stimme, h\u00f6r'", "tokens": ["Und", "h\u00f6r'", "des", "Wei\u00b7nens", "Stim\u00b7me", ",", "h\u00f6r'"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Deinen Sehnsucht, tief und schwer!", "tokens": ["Der", "Dei\u00b7nen", "Sehn\u00b7sucht", ",", "tief", "und", "schwer", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den Dank aus Herzensf\u00fclle!", "tokens": ["Den", "Dank", "aus", "Her\u00b7zens\u00b7f\u00fcl\u00b7le", "!"], "token_info": [