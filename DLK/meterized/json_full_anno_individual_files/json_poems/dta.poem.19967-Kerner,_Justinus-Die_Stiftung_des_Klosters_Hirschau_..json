{"dta.poem.19967": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Die Stiftung des Klosters Hirschau .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Helicena eine Wittwe war,", "tokens": ["He\u00b7li\u00b7ce\u00b7na", "ei\u00b7ne", "Witt\u00b7we", "war", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Reich, fromm vor andern Frauen,", "tokens": ["Reich", ",", "fromm", "vor", "an\u00b7dern", "Frau\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie strebte br\u00fcnstig, ganz und gar", "tokens": ["Sie", "streb\u00b7te", "br\u00fcns\u00b7tig", ",", "ganz", "und", "gar"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "ADV", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich Jesum anzutrauen.", "tokens": ["Sich", "Je\u00b7sum", "an\u00b7zu\u00b7trau\u00b7en", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Drum warf sie oft sich auf die Knie',", "tokens": ["Drum", "warf", "sie", "oft", "sich", "auf", "die", "Knie'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er m\u00f6cht' ihr offenbaren:", "tokens": ["Er", "m\u00f6cht'", "ihr", "of\u00b7fen\u00b7ba\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wie ihre Erdeng\u00fcter sie,", "tokens": ["Wie", "ih\u00b7re", "Er\u00b7den\u00b7g\u00fc\u00b7ter", "sie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ihm treulich k\u00f6nnt' bewahren.", "tokens": ["Ihm", "treu\u00b7lich", "k\u00f6nnt'", "be\u00b7wah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da lag sie in der Nacht einmal,", "tokens": ["Da", "lag", "sie", "in", "der", "Nacht", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gewiegt in fromme Tr\u00e4ume,", "tokens": ["Ge\u00b7wiegt", "in", "from\u00b7me", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und sah ein seltsam fremdes Thal,", "tokens": ["Und", "sah", "ein", "selt\u00b7sam", "frem\u00b7des", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Darin drei Fichtenb\u00e4ume.", "tokens": ["Da\u00b7rin", "drei", "Fich\u00b7ten\u00b7b\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die B\u00e4ume waren wundersam", "tokens": ["Die", "B\u00e4u\u00b7me", "wa\u00b7ren", "wun\u00b7der\u00b7sam"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Aus einem Stamm gesprossen;", "tokens": ["Aus", "ei\u00b7nem", "Stamm", "ge\u00b7spros\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Aus ihren duft'gen Wurzeln kam", "tokens": ["Aus", "ih\u00b7ren", "duft'\u00b7gen", "Wur\u00b7zeln", "kam"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein klarer Born geflossen.", "tokens": ["Ein", "kla\u00b7rer", "Born", "ge\u00b7flos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und ob der fremden Wunderau", "tokens": ["Und", "ob", "der", "frem\u00b7den", "Wun\u00b7de\u00b7rau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sah sie am Himmel wallen,", "tokens": ["Sah", "sie", "am", "Him\u00b7mel", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein hohes Dom auf Wolken blau,", "tokens": ["Ein", "ho\u00b7hes", "Dom", "auf", "Wol\u00b7ken", "blau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00f6rt eine Stimme schallen:", "tokens": ["H\u00f6rt", "ei\u00b7ne", "Stim\u00b7me", "schal\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u201edie\u00df Gotteshaus, du fromme Braut!", "tokens": ["\u201e", "die\u00df", "Got\u00b7tes\u00b7haus", ",", "du", "from\u00b7me", "Braut", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sey, wo die B\u00e4ume stehen,", "tokens": ["Sey", ",", "wo", "die", "B\u00e4u\u00b7me", "ste\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "In festen Grund von dir gebaut!", "tokens": ["In", "fes\u00b7ten", "Grund", "von", "dir", "ge\u00b7baut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nimm's aus geweihten H\u00f6hen!\u201c", "tokens": ["Nim\u00b7m's", "aus", "ge\u00b7weih\u00b7ten", "H\u00f6\u00b7hen", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sieh! da erwacht die fromme Frau,", "tokens": ["Sieh", "!", "da", "er\u00b7wacht", "die", "from\u00b7me", "Frau", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus ihren s\u00fcssen Tr\u00e4umen,", "tokens": ["Aus", "ih\u00b7ren", "s\u00fcs\u00b7sen", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Noch steht vor ihr die fremde Au,", "tokens": ["Noch", "steht", "vor", "ihr", "die", "frem\u00b7de", "Au", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Born mit den drei B\u00e4umen.", "tokens": ["Der", "Born", "mit", "den", "drei", "B\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "APPR", "ART", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie ist in hoher Freudigkeit", "tokens": ["Sie", "ist", "in", "ho\u00b7her", "Freu\u00b7dig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bereit zu Gottes Ruhme,", "tokens": ["Be\u00b7reit", "zu", "Got\u00b7tes", "Ruh\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Zieht an ein pr\u00e4chtig Feierkleid,", "tokens": ["Zieht", "an", "ein", "pr\u00e4ch\u00b7tig", "Fei\u00b7er\u00b7kleid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Schm\u00fcckt sich mit duft'ger Blume.", "tokens": ["Schm\u00fcckt", "sich", "mit", "duft'\u00b7ger", "Blu\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "In tiefer Demuth geht sie aus", "tokens": ["In", "tie\u00b7fer", "De\u00b7muth", "geht", "sie", "aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit ihrer Magd, der treuen,", "tokens": ["Mit", "ih\u00b7rer", "Magd", ",", "der", "treu\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als gieng sie in das Gotteshaus,", "tokens": ["Als", "gieng", "sie", "in", "das", "Got\u00b7tes\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Oder zur Lust im Maien.", "tokens": ["O\u00b7der", "zur", "Lust", "im", "Mai\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch weiter wandte sich ihr Fu\u00df,", "tokens": ["Doch", "wei\u00b7ter", "wand\u00b7te", "sich", "ihr", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PRF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Wolken zogen schnelle,", "tokens": ["Die", "Wol\u00b7ken", "zo\u00b7gen", "schnel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die V\u00f6gel sangen Morgengru\u00df,", "tokens": ["Die", "V\u00f6\u00b7gel", "san\u00b7gen", "Mor\u00b7gen\u00b7gru\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Fraue ward gar helle.", "tokens": ["Der", "Frau\u00b7e", "ward", "gar", "hel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ein D\u00fcften f\u00fcllte rings die Au,", "tokens": ["Ein", "D\u00fcf\u00b7ten", "f\u00fcll\u00b7te", "rings", "die", "Au", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als sie dar\u00fcber gangen!", "tokens": ["Als", "sie", "da\u00b7r\u00fc\u00b7ber", "gan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zu gehen mit der hohen Frau,", "tokens": ["Zu", "ge\u00b7hen", "mit", "der", "ho\u00b7hen", "Frau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fchlt jede Blum' Verlangen.", "tokens": ["F\u00fchlt", "je\u00b7de", "Blum'", "Ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie ging wohl in ein fremdes Thal,", "tokens": ["Sie", "ging", "wohl", "in", "ein", "frem\u00b7des", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Stieg auf des Berges R\u00fccken,", "tokens": ["Stieg", "auf", "des", "Ber\u00b7ges", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und alles th\u00e4t' im Sonnenstrahl", "tokens": ["Und", "al\u00b7les", "th\u00e4t'", "im", "Son\u00b7nen\u00b7strahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ihr klar entgegen blicken.", "tokens": ["Ihr", "klar", "ent\u00b7ge\u00b7gen", "bli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Da steh'n drei B\u00e4um' auf gr\u00fcner Au", "tokens": ["Da", "steh'n", "drei", "B\u00e4um'", "auf", "gr\u00fc\u00b7ner", "Au"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "CARD", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus einem Stamm gesprossen,", "tokens": ["Aus", "ei\u00b7nem", "Stamm", "ge\u00b7spros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da ist ein Born von Himmelsthau,", "tokens": ["Da", "ist", "ein", "Born", "von", "Him\u00b7mel\u00b7sthau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ueber Blumen hell geflossen.", "tokens": ["Ue\u00b7ber", "Blu\u00b7men", "hell", "ge\u00b7flos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Die Fraue kann nicht l\u00e4nger stehn,", "tokens": ["Die", "Frau\u00b7e", "kann", "nicht", "l\u00e4n\u00b7ger", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu den B\u00e4umen mu\u00df sie eilen,", "tokens": ["Zu", "den", "B\u00e4u\u00b7men", "mu\u00df", "sie", "ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ein heil'ger Hauch th\u00e4t sie umweh'n,", "tokens": ["Ein", "heil'\u00b7ger", "Hauch", "th\u00e4t", "sie", "um\u00b7weh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da m\u00f6cht sie ewig weilen.", "tokens": ["Da", "m\u00f6cht", "sie", "e\u00b7wig", "wei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Sie leget ab ihr Feierkleid,", "tokens": ["Sie", "le\u00b7get", "ab", "ihr", "Fei\u00b7er\u00b7kleid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Blumen und Edelsteine;", "tokens": ["Blu\u00b7men", "und", "E\u00b7del\u00b7stei\u00b7ne", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Den heiligen drei B\u00e4umen weiht", "tokens": ["Den", "hei\u00b7li\u00b7gen", "drei", "B\u00e4u\u00b7men", "weiht"], "token_info": ["word", "word", "word", "word", 