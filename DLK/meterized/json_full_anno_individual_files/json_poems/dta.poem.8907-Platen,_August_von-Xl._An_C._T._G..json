{"dta.poem.8907": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Xl.  \n  An C. T. G.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcr so verletzende Beleidigungen,", "tokens": ["F\u00fcr", "so", "ver\u00b7let\u00b7zen\u00b7de", "Be\u00b7lei\u00b7di\u00b7gun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Das f\u00fchl' ich tief, doch thu' ich's blos gezwungen,", "tokens": ["Das", "f\u00fchl'", "ich", "tief", ",", "doch", "thu'", "ich's", "blos", "ge\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "$,", "ADV", "VVFIN", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn ich mein Herz an diesem Recht erlabe.", "tokens": ["Wenn", "ich", "mein", "Herz", "an", "die\u00b7sem", "Recht", "er\u00b7la\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Denn ich verw\u00fcnsch' es als die schlimmste Gabe,", "tokens": ["Denn", "ich", "ver\u00b7w\u00fcn\u00b7sch'", "es", "als", "die", "schlimms\u00b7te", "Ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Vom Schicksal unserer noch allzujungen,", "tokens": ["Vom", "Schick\u00b7sal", "un\u00b7se\u00b7rer", "noch", "all\u00b7zu\u00b7jun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Noch zarten Liebe feindlich aufgedrungen,", "tokens": ["Noch", "zar\u00b7ten", "Lie\u00b7be", "feind\u00b7lich", "auf\u00b7ge\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da es die kaum geborne tr\u00e4gt zu Grabe.", "tokens": ["Da", "es", "die", "kaum", "ge\u00b7bor\u00b7ne", "tr\u00e4gt", "zu", "Gra\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADV", "ADJA", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Beginnst du so, was soll ich k\u00fcnftig hoffen,", "tokens": ["Be\u00b7ginnst", "du", "so", ",", "was", "soll", "ich", "k\u00fcnf\u00b7tig", "hof\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PWS", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn schon am Morgen unsres neuen Bundes", "tokens": ["Wenn", "schon", "am", "Mor\u00b7gen", "uns\u00b7res", "neu\u00b7en", "Bun\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPRART", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mich solch ein Schlag aus blauer Luft getroffen?", "tokens": ["Mich", "solch", "ein", "Schlag", "aus", "blau\u00b7er", "Luft", "ge\u00b7trof\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "ART", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch ach, mein Recht begiebt sich jedes Grundes,", "tokens": ["Doch", "ach", ",", "mein", "Recht", "be\u00b7giebt", "sich", "je\u00b7des", "Grun\u00b7des", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "PPOSAT", "NN", "VVFIN", "PRF", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es sieht geformt dich aus zu sch\u00f6nen Stoffen,", "tokens": ["Es", "sieht", "ge\u00b7formt", "dich", "aus", "zu", "sch\u00f6\u00b7nen", "Stof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVPP", "PRF", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und lebt ja nur vom Hauche deines Mundes!", "tokens": ["Und", "lebt", "ja", "nur", "vom", "Hau\u00b7che", "dei\u00b7nes", "Mun\u00b7des", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}