{"dta.poem.1869": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ubersezte Spruch-Reimen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Was ist doch seliger/ als sorgen ohne Sorgen/", "tokens": ["Was", "ist", "doch", "se\u00b7li\u00b7ger", "/", "als", "sor\u00b7gen", "oh\u00b7ne", "Sor\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADJD", "$(", "KOUS", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn das Gem\u00fctte nun die Last beyseite legt.", "tokens": ["Wenn", "das", "Ge\u00b7m\u00fct\u00b7te", "nun", "die", "Last", "bey\u00b7sei\u00b7te", "legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Arbeits-m\u00fcde Leib/ der fremde B\u00fcrde tr\u00e4gt/", "tokens": ["Der", "Ar\u00b7beits\u00b7m\u00fc\u00b7de", "Leib", "/", "der", "frem\u00b7de", "B\u00fcr\u00b7de", "tr\u00e4gt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sein Eigenthum vers\u00e4umt/ zu seiner H\u00fctte k\u00fcmmt/", "tokens": ["Sein", "Ei\u00b7gen\u00b7thum", "ver\u00b7s\u00e4umt", "/", "zu", "sei\u00b7ner", "H\u00fct\u00b7te", "k\u00fcmmt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$(", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die l\u00e4ngst-gew\u00fcnschte Ruh auff seinem Lager nimmt.", "tokens": ["Die", "l\u00e4ngs\u00b7tge\u00b7w\u00fcnschte", "Ruh", "auff", "sei\u00b7nem", "La\u00b7ger", "nimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Und drinnen ungest\u00f6rt erwart den lichten Morgen.", "tokens": ["Und", "drin\u00b7nen", "un\u00b7ge\u00b7st\u00f6rt", "er\u00b7wart", "den", "lich\u00b7ten", "Mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}