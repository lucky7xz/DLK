{"dta.poem.4087": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Ungeduld.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich gr\u00fcb' es gern in jeden Kieselstein,", "tokens": ["Ich", "gr\u00fcb'", "es", "gern", "in", "je\u00b7den", "Kie\u00b7sel\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich m\u00f6cht' es s\u00e4'n auf jedes frische Beet", "tokens": ["Ich", "m\u00f6cht'", "es", "s\u00e4'n", "auf", "je\u00b7des", "fri\u00b7sche", "Beet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit Kressensamen, der es schnell verr\u00e4th,", "tokens": ["Mit", "Kres\u00b7sen\u00b7sa\u00b7men", ",", "der", "es", "schnell", "ver\u00b7r\u00e4\u00b7th", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf jeden wei\u00dfen Zettel m\u00f6cht' ich's schreiben:", "tokens": ["Auf", "je\u00b7den", "wei\u00b7\u00dfen", "Zet\u00b7tel", "m\u00f6cht'", "ich's", "schrei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dein ist mein Herz, und soll es ewig bleiben.", "tokens": ["Dein", "ist", "mein", "Herz", ",", "und", "soll", "es", "e\u00b7wig", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "PPOSAT", "NN", "$,", "KON", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich m\u00f6cht' mir ziehen einen jungen Staar,", "tokens": ["Ich", "m\u00f6cht'", "mir", "zie\u00b7hen", "ei\u00b7nen", "jun\u00b7gen", "Staar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bis da\u00df er spr\u00e4ch' die Worte rein und klar,", "tokens": ["Bis", "da\u00df", "er", "spr\u00e4ch'", "die", "Wor\u00b7te", "rein", "und", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bis er sie spr\u00e4ch' mit meines Mundes Klang,", "tokens": ["Bis", "er", "sie", "spr\u00e4ch'", "mit", "mei\u00b7nes", "Mun\u00b7des", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit meines Herzens vollem, hei\u00dfen Drang;", "tokens": ["Mit", "mei\u00b7nes", "Her\u00b7zens", "vol\u00b7lem", ",", "hei\u00b7\u00dfen", "Drang", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dann s\u00e4ng' er hell durch ihre Fensterscheiben:", "tokens": ["Dann", "s\u00e4ng'", "er", "hell", "durch", "ih\u00b7re", "Fens\u00b7ter\u00b7schei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dein ist mein Herz, und soll es ewig bleiben.", "tokens": ["Dein", "ist", "mein", "Herz", ",", "und", "soll", "es", "e\u00b7wig", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "PPOSAT", "NN", "$,", "KON", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Den Morgenwinden m\u00f6cht' ichs hauchen ein,", "tokens": ["Den", "Mor\u00b7gen\u00b7win\u00b7den", "m\u00f6cht'", "ichs", "hau\u00b7chen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PIS", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich m\u00f6cht' es s\u00e4useln durch den regen Hain;", "tokens": ["Ich", "m\u00f6cht'", "es", "s\u00e4u\u00b7seln", "durch", "den", "re\u00b7gen", "Hain", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "O, leuchtet' es aus jedem Blumenstern!", "tokens": ["O", ",", "leuch\u00b7tet'", "es", "aus", "je\u00b7dem", "Blu\u00b7mens\u00b7tern", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Tr\u00fcg' es der Duft zu ihr von nah und fern!", "tokens": ["Tr\u00fcg'", "es", "der", "Duft", "zu", "ihr", "von", "nah", "und", "fern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "APPR", "ADJD", "KON", "ADJD", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Ihr Wogen, k\u00f6nnt ihr nichts als R\u00e4der treiben?", "tokens": ["Ihr", "Wo\u00b7gen", ",", "k\u00f6nnt", "ihr", "nichts", "als", "R\u00e4\u00b7der", "trei\u00b7ben", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "PIS", "KOKOM", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dein ist mein Herz, und soll es ewig bleiben.", "tokens": ["Dein", "ist", "mein", "Herz", ",", "und", "soll", "es", "e\u00b7wig", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "PPOSAT", "NN", "$,", "KON", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich meint', es m\u00fc\u00dft' in meinen Augen stehn,", "tokens": ["Ich", "meint'", ",", "es", "m\u00fc\u00dft'", "in", "mei\u00b7nen", "Au\u00b7gen", "stehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf meinen Wangen m\u00fc\u00dft' man's brennen sehn,", "tokens": ["Auf", "mei\u00b7nen", "Wan\u00b7gen", "m\u00fc\u00dft'", "man's", "bren\u00b7nen", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PIS", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zu lesen w\u00e4r's auf meinem stummen Mund,", "tokens": ["Zu", "le\u00b7sen", "w\u00e4r's", "auf", "mei\u00b7nem", "stum\u00b7men", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VAFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein jeder Athemzug g\u00e4b's laut ihr kund;", "tokens": ["Ein", "je\u00b7der", "A\u00b7them\u00b7zug", "g\u00e4b's", "laut", "ihr", "kund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "NE", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und sie merkt nichts von all' dem bangen Treiben:", "tokens": ["Und", "sie", "merkt", "nichts", "von", "all'", "dem", "ban\u00b7gen", "Trei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIS", "APPR", "PIS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dein ist mein Herz, und soll es ewig bleiben!", "tokens": ["Dein", "ist", "mein", "Herz", ",", "und", "soll", "es", "e\u00b7wig", "blei\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "PPOSAT", "NN", "$,", "KON", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}