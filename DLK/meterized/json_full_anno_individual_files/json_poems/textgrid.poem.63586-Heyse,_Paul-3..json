{"textgrid.poem.63586": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schl\u00e4fst du? Es ist schon Tag. \u2013 Ist's wirklich Tag?", "tokens": ["Schl\u00e4fst", "du", "?", "Es", "ist", "schon", "Tag", ".", "\u2013", "Ist's", "wirk\u00b7lich", "Tag", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "PPER", "VAFIN", "ADV", "NN", "$.", "$(", "NE", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mich d\u00fcnkt, die Nacht ist eben angebrochen.", "tokens": ["Mich", "d\u00fcnkt", ",", "die", "Nacht", "ist", "e\u00b7ben", "an\u00b7ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mein Ohr ist taub dem fr\u00fchen Stundenschlag.", "tokens": ["Mein", "Ohr", "ist", "taub", "dem", "fr\u00fc\u00b7hen", "Stun\u00b7den\u00b7schlag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Es lauscht, wie es getan seit so viel Wochen,", "tokens": ["Es", "lauscht", ",", "wie", "es", "ge\u00b7tan", "seit", "so", "viel", "Wo\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "VVPP", "APPR", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ob noch das Stimmchen uns nicht rufen will,", "tokens": ["Ob", "noch", "das", "Stimm\u00b7chen", "uns", "nicht", "ru\u00b7fen", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PPER", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Das Fingerchen an unsre T\u00fcre pochen.", "tokens": ["Das", "Fin\u00b7ge\u00b7rchen", "an", "uns\u00b7re", "T\u00fc\u00b7re", "po\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Rief es nicht da? Nein; alles totenstill,", "tokens": ["Rief", "es", "nicht", "da", "?", "Nein", ";", "al\u00b7les", "to\u00b7tens\u00b7till", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "$.", "PTKANT", "$.", "PIS", "ADJD", "$,"], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.8": {"text": "Und nur der Gram, der \u00fcber Nacht geruht,", "tokens": ["Und", "nur", "der", "Gram", ",", "der", "\u00fc\u00b7ber", "Nacht", "ge\u00b7ruht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "PRELS", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Schreit pl\u00f6tzlich auf mit St\u00f6hnen, bang und schrill.", "tokens": ["Schreit", "pl\u00f6tz\u00b7lich", "auf", "mit", "St\u00f6h\u00b7nen", ",", "bang", "und", "schrill", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "APPR", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Ist's m\u00f6glich? Nie mehr wird es uns so gut,", "tokens": ["Ist's", "m\u00f6g\u00b7lich", "?", "Nie", "mehr", "wird", "es", "uns", "so", "gut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$.", "ADV", "ADV", "VAFIN", "PPER", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Durch unsres Kindes Weckruf zu erwachen?", "tokens": ["Durch", "uns\u00b7res", "Kin\u00b7des", "Weck\u00b7ruf", "zu", "er\u00b7wa\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "O wie das wehe, wie das wehe tut!", "tokens": ["O", "wie", "das", "we\u00b7he", ",", "wie", "das", "we\u00b7he", "tut", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "PDS", "VVFIN", "$,", "PWAV", "ART", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Nie mehr zu h\u00f6ren, wie mit leisem Lachen", "tokens": ["Nie", "mehr", "zu", "h\u00f6\u00b7ren", ",", "wie", "mit", "lei\u00b7sem", "La\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PTKZU", "VVINF", "$,", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Im Zwielicht etwas tappt an unser Bette", "tokens": ["Im", "Zwie\u00b7licht", "et\u00b7was", "tappt", "an", "un\u00b7ser", "Bet\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Bis uns gef\u00e4llt, die Augen aufzumachen,", "tokens": ["Bis", "uns", "ge\u00b7f\u00e4llt", ",", "die", "Au\u00b7gen", "auf\u00b7zu\u00b7ma\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$,", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Und dann, nie rastend an derselben St\u00e4tte,", "tokens": ["Und", "dann", ",", "nie", "ras\u00b7tend", "an", "der\u00b7sel\u00b7ben", "St\u00e4t\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADV", "ADJD", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Sich in die Decke wickelt und versteckt,", "tokens": ["Sich", "in", "die", "De\u00b7cke", "wi\u00b7ckelt", "und", "ver\u00b7steckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Als ob die Schnecke nun ihr H\u00e4uschen h\u00e4tte;", "tokens": ["Als", "ob", "die", "Schne\u00b7cke", "nun", "ihr", "H\u00e4usc\u00b7hen", "h\u00e4t\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "ADV", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Bald neben uns sich wie zum Schlafen streckt,", "tokens": ["Bald", "ne\u00b7ben", "uns", "sich", "wie", "zum", "Schla\u00b7fen", "streckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "PRF", "KOKOM", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Und wenn es eben m\u00e4uschenstille lag,", "tokens": ["Und", "wenn", "es", "e\u00b7ben", "m\u00e4u\u00b7schen\u00b7stil\u00b7le", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Mit neuer Schelmerei uns jauchzend neckt.", "tokens": ["Mit", "neu\u00b7er", "Schel\u00b7me\u00b7rei", "uns", "jauch\u00b7zend", "neckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Das soll nie wiederkommen, und den Tag,", "tokens": ["Das", "soll", "nie", "wie\u00b7der\u00b7kom\u00b7men", ",", "und", "den", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "VVINF", "$,", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Den sonnenlosen, soll man \u00fcberleben,", "tokens": ["Den", "son\u00b7nen\u00b7lo\u00b7sen", ",", "soll", "man", "\u00fc\u00b7berl\u00b7e\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Wo man erwacht ist ohne Lerchenschlag?", "tokens": ["Wo", "man", "er\u00b7wacht", "ist", "oh\u00b7ne", "Ler\u00b7chen\u00b7schlag", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVPP", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Wohl! ins Notwend'ge gilt's sich zu ergeben;", "tokens": ["Wohl", "!", "ins", "Not\u00b7wen\u00b7d'\u00b7ge", "gilt's", "sich", "zu", "er\u00b7ge\u00b7ben", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "APPRART", "NN", "VVFIN", "PRF", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.26": {"text": "Wir werden's, du und ich. Doch