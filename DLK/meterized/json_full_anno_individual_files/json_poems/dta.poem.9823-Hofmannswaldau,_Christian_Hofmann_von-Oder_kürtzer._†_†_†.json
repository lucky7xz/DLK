{"dta.poem.9823": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Oder k\u00fcrtzer.  \n \u2020 \u2020 \u2020", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was unsern ruhestand verlohr/", "tokens": ["Was", "un\u00b7sern", "ru\u00b7he\u00b7stand", "ver\u00b7lohr", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist freylich Evens fall gewesen!", "tokens": ["Ist", "frey\u00b7lich", "E\u00b7vens", "fall", "ge\u00b7we\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NE", "NN", "VAPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Allein was konte sie davor/", "tokens": ["Al\u00b7lein", "was", "kon\u00b7te", "sie", "da\u00b7vor", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VMFIN", "PPER", "PAV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df man so schwachen zeug erlesen?", "tokens": ["Da\u00df", "man", "so", "schwa\u00b7chen", "zeug", "er\u00b7le\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie war von Adams fleisch und bein/", "tokens": ["Sie", "war", "von", "A\u00b7dams", "fleisch", "und", "bein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NE", "ADJD", "KON", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So konte sie nicht st\u00e4rcker seyn.", "tokens": ["So", "kon\u00b7te", "sie", "nicht", "st\u00e4r\u00b7cker", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}