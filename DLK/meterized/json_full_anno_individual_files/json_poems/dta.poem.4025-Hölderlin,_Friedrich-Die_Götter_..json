{"dta.poem.4025": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die G\u00f6tter .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Du stiller Aether! immer bewahrst du sch\u00f6n", "tokens": ["Du", "stil\u00b7ler", "A\u00b7e\u00b7ther", "!", "im\u00b7mer", "be\u00b7wahrst", "du", "sch\u00f6n"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$.", "ADV", "VVFIN", "PPER", "ADJD"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Seele mir im Schmerz, und es adelt sih", "tokens": ["Die", "See\u00b7le", "mir", "im", "Schmerz", ",", "und", "es", "a\u00b7delt", "sih"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "APPRART", "NN", "$,", "KON", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Zur Tapferkeit vor deinen Stralen,", "tokens": ["Zur", "Tap\u00b7fer\u00b7keit", "vor", "dei\u00b7nen", "Stra\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Helios! oft die emp\u00f6rte Brust mir.", "tokens": ["He\u00b7li\u00b7os", "!", "oft", "die", "em\u00b7p\u00f6r\u00b7te", "Brust", "mir", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "ART", "ADJA", "NN", "PPER", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Ihr guten G\u00f6tter! arm ist, wer euch nicht kennt,", "tokens": ["Ihr", "gu\u00b7ten", "G\u00f6t\u00b7ter", "!", "arm", "ist", ",", "wer", "euch", "nicht", "kennt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "ADJD", "VAFIN", "$,", "PWS", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Im rohen Busen ruhet der Zwist ihm nie,", "tokens": ["Im", "ro\u00b7hen", "Bu\u00b7sen", "ru\u00b7het", "der", "Zwist", "ihm", "nie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "PPER", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und Nacht ist ihm die Welt, und keine", "tokens": ["Und", "Nacht", "ist", "ihm", "die", "Welt", ",", "und", "kei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "NN", "VAFIN", "PPER", "ART", "NN", "$,", "KON", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Freude gedeihet und kein Gesang ihm.", "tokens": ["Freu\u00b7de", "ge\u00b7dei\u00b7het", "und", "kein", "Ge\u00b7sang", "ihm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "KON", "PIAT", "NN", "PPER", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Nur ihr, mit euer ewigen Jugend, n\u00e4hrt", "tokens": ["Nur", "ihr", ",", "mit", "eu\u00b7er", "e\u00b7wi\u00b7gen", "Ju\u00b7gend", ",", "n\u00e4hrt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "PPER", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In Herzen, die euch lieben, den Kindersinn,", "tokens": ["In", "Her\u00b7zen", ",", "die", "euch", "lie\u00b7ben", ",", "den", "Kin\u00b7der\u00b7sinn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "VVINF", "$,", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und la\u00dft in Sorgen und in Irren", "tokens": ["Und", "la\u00dft", "in", "Sor\u00b7gen", "und", "in", "Ir\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nimmer den Genius sich vertrauern.", "tokens": ["Nim\u00b7mer", "den", "Ge\u00b7nius", "sich", "ver\u00b7trau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PRF", "VVINF", "$."], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}}}}}