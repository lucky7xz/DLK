{"textgrid.poem.62356": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Himmel offenbart sich nur in Tr\u00e4umen,", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Himmel offenbart sich nur in Tr\u00e4umen,", "tokens": ["Der", "Him\u00b7mel", "of\u00b7fen\u00b7bart", "sich", "nur", "in", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So lang wir noch im Erdenthale wallen:", "tokens": ["So", "lang", "wir", "noch", "im", "Er\u00b7dent\u00b7ha\u00b7le", "wal\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Entfesselt schweben wir durch Wolkenhallen", "tokens": ["Ent\u00b7fes\u00b7selt", "schwe\u00b7ben", "wir", "durch", "Wol\u00b7ken\u00b7hal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und brechen Bl\u00fcten von den Lebensb\u00e4umen.", "tokens": ["Und", "bre\u00b7chen", "Bl\u00fc\u00b7ten", "von", "den", "Le\u00b7bens\u00b7b\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "H\u00f6rst du das Kind entz\u00fcckt im Traume lallen?", "tokens": ["H\u00f6rst", "du", "das", "Kind", "ent\u00b7z\u00fcckt", "im", "Trau\u00b7me", "lal\u00b7len", "?"], "token_info": ["word", "word", "word", "wor