{"dta.poem.20327": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gesteh es nur mein kind/ und l\u00e4chle nicht zu viel/", "tokens": ["Ge\u00b7steh", "es", "nur", "mein", "kind", "/", "und", "l\u00e4ch\u00b7le", "nicht", "zu", "viel", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$(", "KON", "VVFIN", "PTKNEG", "PTKA", "PIS", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gewi\u00df/ du weisest mir das erste liebes-spiel;", "tokens": ["Ge\u00b7wi\u00df", "/", "du", "wei\u00b7sest", "mir", "das", "ers\u00b7te", "lie\u00b7bes\u00b7spiel", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dann als dein s\u00fcsser mund ein wort von w\u00fcrffeln sprach/", "tokens": ["Dann", "als", "dein", "s\u00fcs\u00b7ser", "mund", "ein", "wort", "von", "w\u00fcrf\u00b7feln", "sprach", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPOSAT", "ADJA", "NN", "ART", "NN", "APPR", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da dacht ich allererst den sachen weiter nach.", "tokens": ["Da", "dacht", "ich", "al\u00b7le\u00b7rerst", "den", "sa\u00b7chen", "wei\u00b7ter", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Er w\u00fcrffelt gar zu wohl mit seiner augen-paar/", "tokens": ["Er", "w\u00fcrf\u00b7felt", "gar", "zu", "wohl", "mit", "sei\u00b7ner", "au\u00b7gen\u00b7paar", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich h\u00f6rt und wuste nicht/ was das geredet war.", "tokens": ["Ich", "h\u00f6rt", "und", "wus\u00b7te", "nicht", "/", "was", "das", "ge\u00b7re\u00b7det", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PTKNEG", "$(", "PWS", "PDS", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Indem so blickest du mich gar zu freundlich an/", "tokens": ["In\u00b7dem", "so", "bli\u00b7ckest", "du", "mich", "gar", "zu", "freund\u00b7lich", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVFIN", "PPER", "PRF", "ADV", "PTKA", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da dacht ich allererst/ wie einer w\u00fcrffeln kan.", "tokens": ["Da", "dacht", "ich", "al\u00b7le\u00b7rerst", "/", "wie", "ei\u00b7ner", "w\u00fcrf\u00b7feln", "kan", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$(", "PWAV", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ist di\u00df die w\u00fcrffel-art/ wo mag das bretspiel seyn?", "tokens": ["Ist", "di\u00df", "die", "w\u00fcrf\u00b7fel\u00b7art", "/", "wo", "mag", "das", "bret\u00b7spiel", "seyn", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "NN", "$(", "PWAV", "VMFIN", "PDS", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Indem so f\u00fchrtest du mich bey der hand hinein.", "tokens": ["In\u00b7dem", "so", "f\u00fchr\u00b7test", "du", "mich", "bey", "der", "hand", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es lag mit flor bedeckt/ ich macht es sanffte lo\u00df/", "tokens": ["Es", "lag", "mit", "flor", "be\u00b7deckt", "/", "ich", "macht", "es", "sanff\u00b7te", "lo\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "VVFIN", "ADJD", "$(", "PPER", "VVFIN", "PPER", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und satzte mich damit auff deinen s\u00fcssen schoo\u00df.", "tokens": ["Und", "satz\u00b7te", "mich", "da\u00b7mit", "auff", "dei\u00b7nen", "s\u00fcs\u00b7sen", "schoo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PAV", "APPR", "PPOSAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ach das geliebte bret/ das mir gezeiget ward/", "tokens": ["Ach", "das", "ge\u00b7lieb\u00b7te", "bret", "/", "das", "mir", "ge\u00b7zei\u00b7get", "ward", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ART", "ADJA", "VVFIN", "$(", "PRELS", "PPER", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "War doppelt/ rund und zart/ wie marmor wei\u00df und hart/", "tokens": ["War", "dop\u00b7pelt", "/", "rund", "und", "zart", "/", "wie", "mar\u00b7mor", "wei\u00df", "und", "hart", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$(", "ADJD", "KON", "ADJD", "$(", "KOKOM", "NE", "VVFIN", "KON", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die augen gaben mir den rechten w\u00fcrffel-lauff/", "tokens": ["Die", "au\u00b7gen", "ga\u00b7ben", "mir", "den", "rech\u00b7ten", "w\u00fcrf\u00b7fel\u00b7lauff", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der mund den besten stein/ den setzt ich k\u00fcssend drauff.", "tokens": ["Der", "mund", "den", "bes\u00b7ten", "stein", "/", "den", "setzt", "ich", "k\u00fcs\u00b7send", "drauff", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$(", "ART", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wie wohl war mir darbey/ voraus mein liebgen dir/", "tokens": ["Wie", "wohl", "war", "mir", "dar\u00b7bey", "/", "vo\u00b7raus", "mein", "lieb\u00b7gen", "dir", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "PAV", "$(", "ADV", "PPOSAT", "VVFIN", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn du/ du suchtest selbst die besten spiel herf\u00fcr:", "tokens": ["Denn", "du", "/", "du", "such\u00b7test", "selbst", "die", "bes\u00b7ten", "spiel", "her\u00b7f\u00fcr", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$(", "PPER", "VVFIN", "ADV", "ART", "ADJA", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dick dack und contra puff/ verkehren/ aus und ein/", "tokens": ["Dick", "dack", "und", "cont\u00b7ra", "puff", "/", "ver\u00b7keh\u00b7ren", "/", "aus", "und", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$(", "VVFIN", "$(", "PTKVZ", "KON", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die solten unsre kurtz- und lange weile seyn.", "tokens": ["Die", "sol\u00b7ten", "uns\u00b7re", "kurtz", "und", "lan\u00b7ge", "wei\u00b7le", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPOSAT", "TRUNC", "KON", "ADV", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Indem so ruffestu: Ach still! Ich h\u00f6re was!", "tokens": ["In\u00b7dem", "so", "ruf\u00b7fes\u00b7tu", ":", "Ach", "still", "!", "Ich", "h\u00f6\u00b7re", "was", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVFIN", "$.", "NN", "PTKVZ", "$.", "PPER", "VVFIN", "PIS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die frau/ frau mutter k\u00f6mmt/ sie sieht/ sie mercket das.", "tokens": ["Die", "frau", "/", "frau", "mut\u00b7ter", "k\u00f6mmt", "/", "sie", "sieht", "/", "sie", "mer\u00b7cket", "das", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADJD", "NN", "VVFIN", "$(", "PPER", "VVFIN", "$(", "PPER", "VVFIN", "PDS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ach wie entsetzt ich mich! Ach wie erschrackest du!", "tokens": ["Ach", "wie", "ent\u00b7setzt", "ich", "mich", "!", "Ach", "wie", "er\u00b7schra\u00b7ckest", "du", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "VVFIN", "PPER", "PRF", "$.", "NN", "KOKOM", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da deckten wir in eil das bretspiel wieder zu.", "tokens": ["Da", "deck\u00b7ten", "wir", "in", "eil", "das", "bret\u00b7spiel", "wie\u00b7der", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "KOUS", "PDS", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "So war das spiel verst\u00f6rt. Trag aber keinen groll/", "tokens": ["So", "war", "das", "spiel", "ver\u00b7st\u00f6rt", ".", "Trag", "a\u00b7ber", "kei\u00b7nen", "groll", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PDS", "VVFIN", "VVPP", "$.", "NN", "ADV", "PIAT", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zeig mir die w\u00fcrffel nur/ im fall ich spielen soll.", "tokens": ["Zeig", "mir", "die", "w\u00fcrf\u00b7fel", "nur", "/", "im", "fall", "ich", "spie\u00b7len", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADV", "$(", "APPRART", "NN", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ihr m\u00e4dgen lernet di\u00df/ die ihr mich spielen seht/", "tokens": ["Ihr", "m\u00e4d\u00b7gen", "ler\u00b7net", "di\u00df", "/", "die", "ihr", "mich", "spie\u00b7len", "seht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVFIN", "PDS", "$(", "PRELS", "PPER", "PRF", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich hab den besten stein in meiner liebsten bret.", "tokens": ["Ich", "hab", "den", "bes\u00b7ten", "stein", "in", "mei\u00b7ner", "liebs\u00b7ten", "bret."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}