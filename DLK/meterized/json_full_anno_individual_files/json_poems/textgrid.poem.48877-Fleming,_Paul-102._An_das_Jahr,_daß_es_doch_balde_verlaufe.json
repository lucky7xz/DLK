{"textgrid.poem.48877": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "102. An das Jahr, da\u00df es doch balde verlaufe", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zw\u00f6lf F\u00fcrsten dienen dir, vier H\u00e4uptern untertan,", "tokens": ["Zw\u00f6lf", "F\u00fcrs\u00b7ten", "die\u00b7nen", "dir", ",", "vier", "H\u00e4up\u00b7tern", "un\u00b7ter\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "$,", "CARD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die Wochen sind dein Heer, als welche du aus Tagen,", "tokens": ["die", "Wo\u00b7chen", "sind", "dein", "Heer", ",", "als", "wel\u00b7che", "du", "aus", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,", "KOUS", "PRELS", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "aus Stunden diese machst. So f\u00e4hrst du auf dem Wagen,", "tokens": ["aus", "Stun\u00b7den", "die\u00b7se", "machst", ".", "So", "f\u00e4hrst", "du", "auf", "dem", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PDS", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "den Mon und Sonne ziehn. Die Zeit, die fleucht voran,", "tokens": ["den", "Mon", "und", "Son\u00b7ne", "ziehn", ".", "Die", "Zeit", ",", "die", "fleucht", "vo\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVINF", "$.", "ART", "NN", "$,", "PRELS", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "h\u00e4ut Alles vor ihm um und macht dir reinen Plan.", "tokens": ["h\u00e4ut", "Al\u00b7les", "vor", "ihm", "um", "und", "macht", "dir", "rei\u00b7nen", "Plan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "PPER", "PTKVZ", "KON", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So sieht man weit und breit des Sternen P\u00f6vel jagen", "tokens": ["So", "sieht", "man", "weit", "und", "breit", "des", "Ster\u00b7nen", "P\u00f6\u00b7vel", "ja\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ADJD", "KON", "ADJD", "ART", "NN", "NE", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "um, neben und nach dir. So wirst du hingetragen", "tokens": ["um", ",", "ne\u00b7ben", "und", "nach", "dir", ".", "So", "wirst", "du", "hin\u00b7ge\u00b7tra\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUI", "$,", "APPR", "KON", "APPR", "PPER", "$.", "ADV", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "ins Haus der Ewigkeit, der Niemand folgen kan.", "tokens": ["ins", "Haus", "der", "E\u00b7wig\u00b7keit", ",", "der", "Nie\u00b7mand", "fol\u00b7gen", "kan", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "PRELS", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Lauf, Vater Jahr, di\u00df Jahr lauf mehr als sonst behende", "tokens": ["Lauf", ",", "Va\u00b7ter", "Jahr", ",", "di\u00df", "Jahr", "lauf", "mehr", "als", "sonst", "be\u00b7hen\u00b7de"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "NN", "$,", "PDS", "NN", "APPR", "PIAT", "KOKOM", "ADV", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und komme noch einmal so balde zu dem Ende,", "tokens": ["und", "kom\u00b7me", "noch", "ein\u00b7mal", "so", "bal\u00b7de", "zu", "dem", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "o meiner Arbeit Trost, da\u00df ich das sch\u00f6ne Tun,", "tokens": ["o", "mei\u00b7ner", "Ar\u00b7beit", "Trost", ",", "da\u00df", "ich", "das", "sch\u00f6\u00b7ne", "Tun", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "PPOSAT", "NN", "NN", "$,", "KOUS", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "auf das mein Vaterland in langer Hoffnung denket,", "tokens": ["auf", "das", "mein", "Va\u00b7ter\u00b7land", "in", "lan\u00b7ger", "Hoff\u00b7nung", "den\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "recht f\u00fchre wol hinaus, und, die sich itzt so krenket,", "tokens": ["recht", "f\u00fch\u00b7re", "wol", "hin\u00b7aus", ",", "und", ",", "die", "sich", "itzt", "so", "kren\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PTKVZ", "$,", "KON", "$,", "PRELS", "PRF", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "alsdenn mit Freuden m\u00f6g' in diesen Armen ruhn.", "tokens": ["als\u00b7denn", "mit", "Freu\u00b7den", "m\u00f6g'", "in", "die\u00b7sen", "Ar\u00b7men", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VMFIN", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Zw\u00f6lf F\u00fcrsten dienen dir, vier H\u00e4uptern untertan,", "tokens": ["Zw\u00f6lf", "F\u00fcrs\u00b7ten", "die\u00b7nen", "dir", ",", "vier", "H\u00e4up\u00b7tern", "un\u00b7ter\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "$,", "CARD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die Wochen sind dein Heer, als welche du aus Tagen,", "tokens": ["die", "Wo\u00b7chen", "sind", "dein", "Heer", ",", "als", "wel\u00b7che", "du", "aus", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,", "KOUS", "PRELS", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "aus Stunden diese machst. So f\u00e4hrst du auf dem Wagen,", "tokens": ["aus", "Stun\u00b7den", "die\u00b7se", "machst", ".", "So", "f\u00e4hrst", "du", "auf", "dem", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos":