{"dta.poem.403": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Sie an ihm/ als er zu frey redete.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Die Finger auf den Mund/ und still davon geschwiegen", "tokens": ["Die", "Fin\u00b7ger", "auf", "den", "Mund", "/", "und", "still", "da\u00b7von", "ge\u00b7schwie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$(", "KON", "ADJD", "PAV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jyr redet allzu frey/ ihr treibet losen Schertz/", "tokens": ["Jyr", "re\u00b7det", "all\u00b7zu", "frey", "/", "ihr", "trei\u00b7bet", "lo\u00b7sen", "Schertz", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKA", "ADJD", "$(", "PPER", "VVFIN", "ADJA", "NN", "$("], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Das Hertze lasset ihr in eurem Munde liegen/", "tokens": ["Das", "Hert\u00b7ze", "las\u00b7set", "ihr", "in", "eu\u00b7rem", "Mun\u00b7de", "lie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich aber h\u00fcll den Mund hinunter in mein Hertz.", "tokens": ["Ich", "a\u00b7ber", "h\u00fcll", "den", "Mund", "hin\u00b7un\u00b7ter", "in", "mein", "Hertz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "ART", "NN", "APPO", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Man kan zuweilen wol ein Wort von Lieben nennen/", "tokens": ["Man", "kan", "zu\u00b7wei\u00b7len", "wol", "ein", "Wort", "von", "Lie\u00b7ben", "nen\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "ADV", "ART", "NN", "APPR", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch man mu\u00df nicht zu weit in ", "tokens": ["Doch", "man", "mu\u00df", "nicht", "zu", "weit", "in"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VMFIN", "PTKNEG", "PTKA", "ADJD", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Den Eintritt mag man wol gern einem jeden g\u00f6nnen", "tokens": ["Den", "Ein\u00b7tritt", "mag", "man", "wol", "gern", "ei\u00b7nem", "je\u00b7den", "g\u00f6n\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PIS", "ADV", "ADV", "ART", "PIAT", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Allein ihr Heiligthum will keinen Sp\u00f6tter sehn.", "tokens": ["Al\u00b7lein", "ihr", "Hei\u00b7lig\u00b7thum", "will", "kei\u00b7nen", "Sp\u00f6t\u00b7ter", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VMFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}