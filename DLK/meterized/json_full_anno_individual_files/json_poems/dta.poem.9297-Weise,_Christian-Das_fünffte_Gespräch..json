{"dta.poem.9297": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Das f\u00fcnffte Gespr\u00e4ch.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.85", "af:0.14"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Mein allerliebstes kind/ wofern sie ein belieben", "tokens": ["Mein", "al\u00b7ler\u00b7liebs\u00b7tes", "kind", "/", "wo\u00b7fern", "sie", "ein", "be\u00b7lie\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "KOUS", "PPER", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An meinen schrifften hat/ so nehme sie den gru\u00df", "tokens": ["An", "mei\u00b7nen", "schriff\u00b7ten", "hat", "/", "so", "neh\u00b7me", "sie", "den", "gru\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "VAFIN", "$(", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den meine feder bringt/ und lese di\u00df geschrieben/", "tokens": ["Den", "mei\u00b7ne", "fe\u00b7der", "bringt", "/", "und", "le\u00b7se", "di\u00df", "ge\u00b7schrie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$(", "KON", "VVFIN", "PDS", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was ich au\u00df bl\u00f6digkeit annoch verschweigen mu\u00df.", "tokens": ["Was", "ich", "au\u00df", "bl\u00f6\u00b7dig\u00b7keit", "an\u00b7noch", "ver\u00b7schwei\u00b7gen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wir menschen haben doch kein fenster an dem hertzen/", "tokens": ["Wir", "men\u00b7schen", "ha\u00b7ben", "doch", "kein", "fens\u00b7ter", "an", "dem", "hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VAFIN", "ADV", "PIAT", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die lust/ die uns beschwert/ mu\u00df durch die worte gehn", "tokens": ["Die", "lust", "/", "die", "uns", "be\u00b7schwert", "/", "mu\u00df", "durch", "die", "wor\u00b7te", "gehn"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "VVPP", "$(", "VMFIN", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wer keine reden braucht der leidet ewig schmertzen:", "tokens": ["Wer", "kei\u00b7ne", "re\u00b7den", "braucht", "der", "lei\u00b7det", "e\u00b7wig", "schmert\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVINF", "VVFIN", "ART", "VVFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Denn wer uns helffen sol mu\u00df uns zuvor verstehn.", "tokens": ["Denn", "wer", "uns", "helf\u00b7fen", "sol", "mu\u00df", "uns", "zu\u00b7vor", "ver\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVINF", "VMFIN", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ja wol/ ich sagt es gern/ es m\u00f6chte nicht verfangen/", "tokens": ["Ja", "wol", "/", "ich", "sagt", "es", "gern", "/", "es", "m\u00f6ch\u00b7te", "nicht", "ver\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "$(", "PPER", "VVFIN", "PPER", "ADV", "$(", "PPER", "VMFIN", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Sie hat das urthel schon von meiner treu gef\u00e4llt/", "tokens": ["Sie", "hat", "das", "ur\u00b7thel", "schon", "von", "mei\u00b7ner", "treu", "ge\u00b7f\u00e4llt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und meynt es sey gar recht wenn jemand mein verlangen", "tokens": ["Und", "meynt", "es", "sey", "gar", "recht", "wenn", "je\u00b7mand", "mein", "ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "VAFIN", "ADV", "ADV", "KOUS", "PIS", "PPOSAT", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Vor einen \u00fcberflu\u00df der schlimsten falschheit h\u00e4lt.", "tokens": ["Vor", "ei\u00b7nen", "\u00fc\u00b7berf\u00b7lu\u00df", "der", "schlims\u00b7ten", "falschheit", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.13": {"text": "Ich bin das b\u00f6se kind: was ich von hertzen rede/", "tokens": ["Ich", "bin", "das", "b\u00f6\u00b7se", "kind", ":", "was", "ich", "von", "hert\u00b7zen", "re\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$.", "PWS", "PPER", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Das heist ein blosser schertz: bin ich ein bi\u00dfgen froh/", "tokens": ["Das", "heist", "ein", "blos\u00b7ser", "schertz", ":", "bin", "ich", "ein", "bi\u00df\u00b7gen", "froh", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$.", "VAFIN", "PPER", "ART", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So heists ich spotte sie: und bin ich wieder bl\u00f6de/", "tokens": ["So", "heists", "ich", "spot\u00b7te", "sie", ":", "und", "bin", "ich", "wie\u00b7der", "bl\u00f6\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "PPER", "$.", "KON", "VAFIN", "PPER", "ADV", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So heists ich stelle mich zum blossen possen so.", "tokens": ["So", "heists", "ich", "stel\u00b7le", "mich", "zum", "blos\u00b7sen", "pos\u00b7sen", "so", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ach was hab ich gethan? womit bin ich so k\u00fchne?", "tokens": ["Ach", "was", "hab", "ich", "ge\u00b7than", "?", "wo\u00b7mit", "bin", "ich", "so", "k\u00fch\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "VAFIN", "PPER", "VVPP", "$.", "PWAV", "VAFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Was steht mir \u00fcbel an/ und wo thu ich zu viel?", "tokens": ["Was", "steht", "mir", "\u00fc\u00b7bel", "an", "/", "und", "wo", "thu", "ich", "zu", "viel", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADJD", "PTKVZ", "$(", "KON", "PWAV", "VVFIN", "PPER", "PTKA", "PIS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Da\u00df ich verd\u00e4chtig bin und lauter ha\u00df verdiene/", "tokens": ["Da\u00df", "ich", "ver\u00b7d\u00e4ch\u00b7tig", "bin", "und", "lau\u00b7ter", "ha\u00df", "ver\u00b7die\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "KON", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wenn ich das s\u00fcsse band der freundschafft suchen wil!", "tokens": ["Wenn", "ich", "das", "s\u00fcs\u00b7se", "band", "der", "freund\u00b7schafft", "su\u00b7chen", "wil", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "VVFIN", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Mein kind sie trifft es nicht: ich schwere bey dem gl\u00fccke", "tokens": ["Mein", "kind", "sie", "trifft", "es", "nicht", ":", "ich", "schwe\u00b7re", "bey", "dem", "gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "PPER", "PTKNEG", "$.", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Das mich in dieser welt einmahl vergn\u00fcgen sol:", "tokens": ["Das", "mich", "in", "die\u00b7ser", "welt", "ein\u00b7mahl", "ver\u00b7gn\u00fc\u00b7gen", "sol", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPR", "PDAT", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Schertz ich mit ihrer gunst durch falsche liebes-blicke/", "tokens": ["Schertz", "ich", "mit", "ih\u00b7rer", "gunst", "durch", "fal\u00b7sche", "lie\u00b7bes\u00b7bli\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Wolan so geh es mir in keinem stande wohl.", "tokens": ["Wo\u00b7lan", "so", "geh", "es", "mir", "in", "kei\u00b7nem", "stan\u00b7de", "wohl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PRF", "APPR", "PIAT", "ADJA", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Sie ist mein steter trost der antrib meiner sorgen/", "tokens": ["Sie", "ist", "mein", "ste\u00b7ter", "trost", "der", "an\u00b7trib", "mei\u00b7ner", "sor\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Der nordpol meiner lust/ der anla\u00df meiner ruh:", "tokens": ["Der", "nord\u00b7pol", "mei\u00b7ner", "lust", "/", "der", "an\u00b7la\u00df", "mei\u00b7ner", "ruh", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$(", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Sie ist mein guter tag: sie ist mein guter morgen:", "tokens": ["Sie", "ist", "mein", "gu\u00b7ter", "tag", ":", "sie", "ist", "mein", "gu\u00b7ter", "mor\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "$.", "PPER", "VAFIN", "PPOSAT", "ADJA", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Sie ist die gute nacht und alles was ich thu.", "tokens": ["Sie", "ist", "die", "gu\u00b7te", "nacht", "und", "al\u00b7les", "was", "ich", "thu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "KON", "PIS", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ich lebe/ doch nicht ich: sie lebt in meiner seele/", "tokens": ["Ich", "le\u00b7be", "/", "doch", "nicht", "ich", ":", "sie", "lebt", "in", "mei\u00b7ner", "see\u00b7le", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "ADV", "PTKNEG", "PPER", "$.", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Sonst hat der blasse tod mich schon in seiner macht.", "tokens": ["Sonst", "hat", "der", "blas\u00b7se", "tod", "mich", "schon", "in", "sei\u00b7ner", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "PPER", "ADV", "APPR", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Drum wenn ich meinen lauff und meine stunden zehle/", "tokens": ["Drum", "wenn", "ich", "mei\u00b7nen", "lauff", "und", "mei\u00b7ne", "stun\u00b7den", "zeh\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "KOUS", "PPER", "PPOSAT", "NN", "KON", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "So nenn ich nur die zeit/ wenn ich an sie gedacht.", "tokens": ["So", "nenn", "ich", "nur", "die", "zeit", "/", "wenn", "ich", "an", "sie", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$(", "KOUS", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-++--+", "measure": "iambic.hexa.chol"}, "line.33": {"text": "Sie dencke selbsten nach: hab ich ihr nicht erwiesen", "tokens": ["Sie", "den\u00b7cke", "selbs\u00b7ten", "nach", ":", "hab", "ich", "ihr", "nicht", "er\u00b7wie\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "$.", "VAFIN", "PPER", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Was ein verbundnes hertz der liebste leisten kan?", "tokens": ["Was", "ein", "ver\u00b7bund\u00b7nes", "hertz", "der", "liebs\u00b7te", "leis\u00b7ten", "kan", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "ART", "ADJA", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Ich habe sie bedient/ ich habe sie gepriesen/", "tokens": ["Ich", "ha\u00b7be", "sie", "be\u00b7dient", "/", "ich", "ha\u00b7be", "sie", "ge\u00b7prie\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$(", "PPER", "VAFIN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und h\u00e4tt ich mehr vermocht so h\u00e4tt ich mehr gethan.", "tokens": ["Und", "h\u00e4tt", "ich", "mehr", "ver\u00b7mocht", "so", "h\u00e4tt", "ich", "mehr", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "VVPP", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}