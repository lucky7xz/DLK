{"dta.poem.1562": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Klage \u00fcber den t\u00f6dtlichen Hintritt Herrn George  \n Friderichs von Abschatz/ K\u00f6nigl. Mann-Recht-  \n Sitzers und Landes-Eltisten des F\u00fcrsten-  \n thums Bre\u00dflau.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Ich/ zweyer V\u00e4ter Sohn/ ein Wahlkind fremder G\u00fctte/", "tokens": ["Ich", "/", "zwey\u00b7er", "V\u00e4\u00b7ter", "Sohn", "/", "ein", "Wahl\u00b7kind", "frem\u00b7der", "G\u00fct\u00b7te", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "ADJA", "NN", "NN", "$(", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mu\u00df izt zum andern mahl verlassne W\u00e4yse seyn.", "tokens": ["Mu\u00df", "izt", "zum", "an\u00b7dern", "mahl", "ver\u00b7lass\u00b7ne", "W\u00e4y\u00b7se", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPRART", "ADJA", "ADV", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich war ins f\u00fcnffte Jahr nicht l\u00e4ngst getreten ein/", "tokens": ["Ich", "war", "ins", "f\u00fcnff\u00b7te", "Jahr", "nicht", "l\u00e4ngst", "ge\u00b7tre\u00b7ten", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NN", "PTKNEG", "ADV", "VVPP", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als mir entzogen ward mein Vater von Gebl\u00fctte/", "tokens": ["Als", "mir", "ent\u00b7zo\u00b7gen", "ward", "mein", "Va\u00b7ter", "von", "Ge\u00b7bl\u00fct\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "PPOSAT", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sein Bruder zeigte sich als Vater von Gem\u00fctte/", "tokens": ["Sein", "Bru\u00b7der", "zeig\u00b7te", "sich", "als", "Va\u00b7ter", "von", "Ge\u00b7m\u00fct\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "KOUS", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vorsorge/ Zucht/ Verlag und eigner Tugend Schein", "tokens": ["Vor\u00b7sor\u00b7ge", "/", "Zucht", "/", "Ver\u00b7lag", "und", "eig\u00b7ner", "Tu\u00b7gend", "Schein"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$(", "NN", "$(", "NN", "KON", "ADJA", "NN", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.7": {"text": "War Fackel/ Stab und Bahn zu meinem Wohlgedeyn.", "tokens": ["War", "Fa\u00b7ckel", "/", "Stab", "und", "Bahn", "zu", "mei\u00b7nem", "Wohl\u00b7ge\u00b7deyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$(", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Nun seh ich wie der Tod mein Gl\u00fcck auffs neu zerr\u00fctte.", "tokens": ["Nun", "seh", "ich", "wie", "der", "Tod", "mein", "Gl\u00fcck", "auffs", "neu", "zer\u00b7r\u00fct\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOKOM", "ART", "NN", "PPOSAT", "NN", "APPRART", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Gedult! der H\u00f6chste wird noch ferner Vater bleiben.", "tokens": ["Ge\u00b7dult", "!", "der", "H\u00f6chs\u00b7te", "wird", "noch", "fer\u00b7ner", "Va\u00b7ter", "blei\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "VAFIN", "ADV", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dein Nachruhm/ edler Freund/ schwebt um des Him\u0304els Pol.", "tokens": ["Dein", "Nach\u00b7ruhm", "/", "ed\u00b7ler", "Freund", "/", "schwebt", "um", "des", "Him\u0304els", "Pol", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "ADJA", "NN", "$(", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Der reinen Seelen gehts in GOttes H\u00e4nden wohl:", "tokens": ["Der", "rei\u00b7nen", "See\u00b7len", "gehts", "in", "Got\u00b7tes", "H\u00e4n\u00b7den", "wohl", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ich will zur Danckbarkeit auff deine Grabst\u00e4tt schreiben:", "tokens": ["Ich", "will", "zur", "Dan\u00b7ck\u00b7bar\u00b7keit", "auff", "dei\u00b7ne", "Grab\u00b7st\u00e4tt", "schrei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Durch Tugend und Verstand nach wahrer Ehre streben", "tokens": ["Durch", "Tu\u00b7gend", "und", "Ver\u00b7stand", "nach", "wah\u00b7rer", "Eh\u00b7re", "stre\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Heist seinen Ahnen gleich/ wie der von ABSCHATZ/ leben.", "tokens": ["Heist", "sei\u00b7nen", "Ah\u00b7nen", "gleich", "/", "wie", "der", "von", "Ab\u00b7SCHATZ", "/", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "$(", "KOKOM", "ART", "APPR", "NE", "$(", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}}}}