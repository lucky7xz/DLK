{"dta.poem.9773": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf die hochzeit Hn. Friedrich Perlitzes mit  \n Jungfer Anna Dorothea Sommers.  \n C. S. L.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jtzt da die schwalben gleich beginnen abzuziehen/", "tokens": ["Jtzt", "da", "die", "schwal\u00b7ben", "gleich", "be\u00b7gin\u00b7nen", "ab\u00b7zu\u00b7zie\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "VVFIN", "ADV", "VVINF", "VVIZU", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dieser lande sich verliehren allgemach/", "tokens": ["Und", "die\u00b7ser", "lan\u00b7de", "sich", "ver\u00b7lieh\u00b7ren", "all\u00b7ge\u00b7mach", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "PRF", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So sucht herr Perlitz auch dem winter zu entfliehen/", "tokens": ["So", "sucht", "herr", "Per\u00b7litz", "auch", "dem", "win\u00b7ter", "zu", "ent\u00b7flie\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "NE", "ADV", "ART", "ADJA", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und zeucht mit dieser schaar dem warmen sommer nach.", "tokens": ["Und", "zeucht", "mit", "die\u00b7ser", "schaar", "dem", "war\u00b7men", "som\u00b7mer", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PDAT", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mich d\u00fcnckt ich werde nicht was ungereimtes setzen/", "tokens": ["Mich", "d\u00fcnckt", "ich", "wer\u00b7de", "nicht", "was", "un\u00b7ge\u00b7reim\u00b7tes", "set\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VAFIN", "PTKNEG", "PWS", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn meine feder ihn den schwalben \u00e4hnlich macht.", "tokens": ["Wenn", "mei\u00b7ne", "fe\u00b7der", "ihn", "den", "schwal\u00b7ben", "\u00e4hn\u00b7lich", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ART", "ADJA", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die frechen adler sind den r\u00e4ubern gleich zu sch\u00e4tzen/", "tokens": ["Die", "fre\u00b7chen", "ad\u00b7ler", "sind", "den", "r\u00e4u\u00b7bern", "gleich", "zu", "sch\u00e4t\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und tauben machet fast die einfalt gar veracht.", "tokens": ["Und", "tau\u00b7ben", "ma\u00b7chet", "fast", "die", "ein\u00b7falt", "gar", "ver\u00b7acht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "VVFIN", "ADV", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Bey schwalben aber ist mit unschuld witz verbunden/", "tokens": ["Bey", "schwal\u00b7ben", "a\u00b7ber", "ist", "mit", "un\u00b7schuld", "witz", "ver\u00b7bun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ADV", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wie uns ihr kl\u00fcglich thun und frommer wandel zeigt:", "tokens": ["Wie", "uns", "ihr", "kl\u00fcg\u00b7lich", "thun", "und", "from\u00b7mer", "wan\u00b7del", "zeigt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADJD", "VVINF", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und beydes hat bey ihm/ herr br\u00e4utigam/ gefunden/", "tokens": ["Und", "bey\u00b7des", "hat", "bey", "ihm", "/", "herr", "br\u00e4u\u00b7ti\u00b7gam", "/", "ge\u00b7fun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "APPR", "PPER", "$(", "NN", "NE", "$(", "VVPP", "$("], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Wer sein gem\u00fcth erforscht und ihm nicht abgeneigt.", "tokens": ["Wer", "sein", "ge\u00b7m\u00fcth", "er\u00b7forscht", "und", "ihm", "nicht", "ab\u00b7ge\u00b7neigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "KON", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die schwalben sind mit schwartz bekleidet auf dem r\u00fccken;", "tokens": ["Die", "schwal\u00b7ben", "sind", "mit", "schwartz", "be\u00b7klei\u00b7det", "auf", "dem", "r\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "APPR", "ADJD", "VVFIN", "APPR", "ART", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und diese farbe schl\u00e4gt auch seinem stande bey:", "tokens": ["Und", "die\u00b7se", "far\u00b7be", "schl\u00e4gt", "auch", "sei\u00b7nem", "stan\u00b7de", "bey", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVFIN", "ADV", "PPOSAT", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wie jener reine brust beschneete liljen schm\u00fccken;", "tokens": ["Wie", "je\u00b7ner", "rei\u00b7ne", "brust", "be\u00b7schnee\u00b7te", "lil\u00b7jen", "schm\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "ADJA", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So ist sein hertz geziert mit ungef\u00e4rbter treu.", "tokens": ["So", "ist", "sein", "hertz", "ge\u00b7ziert", "mit", "un\u00b7ge\u00b7f\u00e4rb\u00b7ter", "treu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "APPR", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wo Gottes-h\u00e4user sind/ da nisten an den w\u00e4nden", "tokens": ["Wo", "Got\u00b7tes\u00b7h\u00e4u\u00b7ser", "sind", "/", "da", "nis\u00b7ten", "an", "den", "w\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "VAFIN", "$(", "ADV", "VVFIN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Nicht ungern/ wie man sieht/ sich diese v\u00f6gel ein:", "tokens": ["Nicht", "un\u00b7gern", "/", "wie", "man", "sieht", "/", "sich", "die\u00b7se", "v\u00f6\u00b7gel", "ein", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$(", "PWAV", "PIS", "VVFIN", "$(", "PRF", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So hat ihn GOttes schlu\u00df in tempel wollen senden/", "tokens": ["So", "hat", "ihn", "Got\u00b7tes", "schlu\u00df", "in", "tem\u00b7pel", "wol\u00b7len", "sen\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "NN", "APPR", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und seine wohnstatt wird nah an der kirchen seyn.", "tokens": ["Und", "sei\u00b7ne", "wohn\u00b7statt", "wird", "nah", "an", "der", "kir\u00b7chen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJD", "VAFIN", "ADJD", "APPR", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ists also/ wie gar viel von den gelehrten schreiben/", "tokens": ["Ists", "al\u00b7so", "/", "wie", "gar", "viel", "von", "den", "ge\u00b7lehr\u00b7ten", "schrei\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$(", "KOKOM", "ADV", "ADV", "APPR", "ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Da\u00df sich die schwalbe nur allein im fluge nehrt;", "tokens": ["Da\u00df", "sich", "die", "schwal\u00b7be", "nur", "al\u00b7lein", "im", "flu\u00b7ge", "nehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "ADJA", "ADV", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "So kan auch diesesfals das gleichni\u00df f\u00fcglich bleiben/", "tokens": ["So", "kan", "auch", "die\u00b7ses\u00b7fals", "das", "gleich\u00b7ni\u00df", "f\u00fcg\u00b7lich", "blei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "ADV", "ART", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Dieweil ihm GOtt sein theil bey steter m\u00fch beschert.", "tokens": ["Die\u00b7weil", "ihm", "Gott", "sein", "theil", "bey", "ste\u00b7ter", "m\u00fch", "be\u00b7schert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "PPOSAT", "NN", "APPR", "ADJA", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Die schwalbe bringet uns die freuden-post getragen/", "tokens": ["Die", "schwal\u00b7be", "brin\u00b7get", "uns", "die", "freu\u00b7den\u00b7post", "ge\u00b7tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Wenn der begr\u00fcnte lentz nunmehro vor der th\u00fcr:", "tokens": ["Wenn", "der", "be\u00b7gr\u00fcn\u00b7te", "lentz", "nun\u00b7meh\u00b7ro", "vor", "der", "th\u00fcr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und Evangelium heist gute botschafft sagen/", "tokens": ["Und", "E\u00b7van\u00b7ge\u00b7li\u00b7um", "heist", "gu\u00b7te", "bot\u00b7schafft", "sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Das tr\u00e4gt er \u00f6ffentlich dem volcke GOttes f\u00fcr.", "tokens": ["Das", "tr\u00e4gt", "er", "\u00f6f\u00b7fent\u00b7lich", "dem", "vol\u00b7cke", "Got\u00b7tes", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Die schwalben-nester sind mit federn auffgebettet/", "tokens": ["Die", "schwal\u00b7ben\u00b7nes\u00b7ter", "sind", "mit", "fe\u00b7dern", "auff\u00b7ge\u00b7bet\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "Vielleicht ist eben so sein lager zugericht.", "tokens": ["Viel\u00b7leicht", "ist", "e\u00b7ben", "so", "sein", "la\u00b7ger", "zu\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wie jene von gewalt der aberglaub errettet:", "tokens": ["Wie", "je\u00b7ne", "von", "ge\u00b7walt", "der", "a\u00b7berg\u00b7laub", "er\u00b7ret\u00b7tet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "APPR", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "So wei\u00df er/ da\u00df sein amt ihm sicherheit verspricht.", "tokens": ["So", "wei\u00df", "er", "/", "da\u00df", "sein", "amt", "ihm", "si\u00b7cher\u00b7heit", "ver\u00b7spricht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "KOUS", "PPOSAT", "NN", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Und also kan er sich den schwalben zugesellen/", "tokens": ["Und", "al\u00b7so", "kan", "er", "sich", "den", "schwal\u00b7ben", "zu\u00b7ge\u00b7sel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "PRF", "ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Weil gleich und gleiche doch sich wohl zusammen schickt;", "tokens": ["Weil", "gleich", "und", "glei\u00b7che", "doch", "sich", "wohl", "zu\u00b7sam\u00b7men", "schickt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "KON", "ADJA", "ADV", "PRF", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Er darff nur den compas nach seiner liebsten stellen/", "tokens": ["Er", "darff", "nur", "den", "com\u00b7pas", "nach", "sei\u00b7ner", "liebs\u00b7ten", "stel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NE", "APPR", "PPOSAT", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "So hat sein zug alsbald ins sommer-land ger\u00fcckt/", "tokens": ["So", "hat", "sein", "zug", "als\u00b7bald", "ins", "som\u00b7mer\u00b7land", "ge\u00b7r\u00fcckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Er wird in ihrem scho\u00df den sch\u00f6nsten sommer finden/", "tokens": ["Er", "wird", "in", "ih\u00b7rem", "scho\u00df", "den", "sch\u00f6ns\u00b7ten", "som\u00b7mer", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Als die ohn ursach nicht sich von dem sommer nennt.", "tokens": ["Als", "die", "ohn", "ur\u00b7sach", "nicht", "sich", "von", "dem", "som\u00b7mer", "nennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "APPR", "NE", "PTKNEG", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Sie wird durch freundlich thun ihm hertz und geist entz\u00fcnden/", "tokens": ["Sie", "wird", "durch", "freund\u00b7lich", "thun", "ihm", "hertz", "und", "geist", "ent\u00b7z\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJD", "VVFIN", "PPER", "NN", "KON", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Mit feuer das zwar w\u00e4rmt/ und dennoch nicht verbrennt.", "tokens": ["Mit", "feu\u00b7er", "das", "zwar", "w\u00e4rmt", "/", "und", "den\u00b7noch", "nicht", "ver\u00b7brennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADV", "VAFIN", "$(", "KON", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Er wird ohn unterla\u00df gut wetter bey ihr haben/", "tokens": ["Er", "wird", "ohn", "un\u00b7ter\u00b7la\u00df", "gut", "wet\u00b7ter", "bey", "ihr", "ha\u