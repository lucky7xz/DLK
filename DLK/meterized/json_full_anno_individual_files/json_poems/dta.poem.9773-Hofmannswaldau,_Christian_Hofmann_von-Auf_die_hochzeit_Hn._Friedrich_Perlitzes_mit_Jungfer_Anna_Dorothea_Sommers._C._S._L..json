, "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Er wird zu seiner zeit auch reiffe fr\u00fcchte schau\u2019n/", "tokens": ["Er", "wird", "zu", "sei\u00b7ner", "zeit", "auch", "reif\u00b7fe", "fr\u00fcch\u00b7te", "schau'n", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "ADV", "VVFIN", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Er wird mit tausend lust die garben h\u00f6ren schreyen/", "tokens": ["Er", "wird", "mit", "tau\u00b7send", "lust", "die", "gar\u00b7ben", "h\u00f6\u00b7ren", "schre\u00b7yen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "CARD", "VVFIN", "ART", "NN", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.48": {"text": "Und eine scheune denn ins wochen-bette bau\u2019n.", "tokens": ["Und", "ei\u00b7ne", "scheu\u00b7ne", "denn", "ins", "wo\u00b7chen\u00b7bet\u00b7te", "bau'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "ART", "ADJA", "KON", "APPRART", "ADJA", "NN", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Der himmel wolle gl\u00fcck zu diesem zuge geben/", "tokens": ["Der", "him\u00b7mel", "wol\u00b7le", "gl\u00fcck", "zu", "die\u00b7sem", "zu\u00b7ge", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "NN", "APPR", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Den mit den schwalben er nach seinem sommer thut/", "tokens": ["Den", "mit", "den", "schwal\u00b7ben", "er", "nach", "sei\u00b7nem", "som\u00b7mer", "thut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Er lasse sein genist in lauter perlen schweben/", "tokens": ["Er", "las\u00b7se", "sein", "ge\u00b7nist", "in", "lau\u00b7ter", "per\u00b7len", "schwe\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Beschirmet vor gewalt/ gesichert vor der glut.", "tokens": ["Be\u00b7schir\u00b7met", "vor", "ge\u00b7walt", "/", "ge\u00b7si\u00b7chert", "vor", "der", "glut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$(", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Und wie man insgemein das sprichwort angenommen/", "tokens": ["Und", "wie", "man", "ins\u00b7ge\u00b7mein", "das", "sprich\u00b7wort", "an\u00b7ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIS", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Da\u00df eine schwalbe nicht den bunten fr\u00fchling macht:", "tokens": ["Da\u00df", "ei\u00b7ne", "schwal\u00b7be", "nicht", "den", "bun\u00b7ten", "fr\u00fch\u00b7ling", "macht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "PTKNEG", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "So soll er nicht zu uns den sommer wieder kommen/", "tokens": ["So", "soll", "er", "nicht", "zu", "uns", "den", "som\u00b7mer", "wie\u00b7der", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "APPR", "PPER", "ART", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Er habe denn mit sich ein schw\u00e4lbchen noch gebracht.", "tokens": ["Er", "ha\u00b7be", "denn", "mit", "sich", "ein", "schw\u00e4lb\u00b7chen", "noch", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PRF", "ART", "ADJA", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}