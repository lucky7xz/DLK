{"textgrid.poem.46063": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Alles lieben oder Eins lieben", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Still folgt die Liebe deinen Schritten,", "tokens": ["Still", "folgt", "die", "Lie\u00b7be", "dei\u00b7nen", "Schrit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Denn alle Lust und alle Pracht,", "tokens": ["Denn", "al\u00b7le", "Lust", "und", "al\u00b7le", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die dich erg\u00f6tzt in Kunst und Sitten,", "tokens": ["Die", "dich", "er\u00b7g\u00f6tzt", "in", "Kunst", "und", "Sit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hat sie ja selbst f\u00fcr dich erdacht.", "tokens": ["Hat", "sie", "ja", "selbst", "f\u00fcr", "dich", "er\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich darf nicht rings umher mehr blicken", "tokens": ["Ich", "darf", "nicht", "rings", "um\u00b7her", "mehr", "bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "PTKVZ", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Farben Glut, der Formen Zier", "tokens": ["Der", "Far\u00b7ben", "Glut", ",", "der", "For\u00b7men", "Zier"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der L\u00fcfte Wehn, der Blumen Nicken", "tokens": ["Der", "L\u00fcf\u00b7te", "Wehn", ",", "der", "Blu\u00b7men", "Ni\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist all f\u00fcr dich, k\u00f6mmt all von mir.", "tokens": ["Ist", "all", "f\u00fcr", "dich", ",", "k\u00f6mmt", "all", "von", "mir", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "APPR", "PPER", "$,", "VVFIN", "PIAT", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es wird kein stolzes Schlo\u00df gebauet,", "tokens": ["Es", "wird", "kein", "stol\u00b7zes", "Schlo\u00df", "ge\u00b7bau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es wird kein edles Bild geschnitzt,", "tokens": ["Es", "wird", "kein", "ed\u00b7les", "Bild", "ge\u00b7schnitzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Liebe hat es durchgeschauet,", "tokens": ["Die", "Lie\u00b7be", "hat", "es", "durch\u00b7ge\u00b7schau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Liebe hat hindurchgeblitzt.", "tokens": ["Die", "Lie\u00b7be", "hat", "hin\u00b7durch\u00b7ge\u00b7blitzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Weil du in Vielem liebst zu leben", "tokens": ["Weil", "du", "in", "Vie\u00b7lem", "liebst", "zu", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PIS", "VVFIN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hab Vieles ich dir herbestellt,", "tokens": ["Hab", "Vie\u00b7les", "ich", "dir", "her\u00b7be\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als Gott der Liebe sich ergeben,", "tokens": ["Als", "Gott", "der", "Lie\u00b7be", "sich", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da kamst du selbst mitsamt der Welt.", "tokens": ["Da", "kamst", "du", "selbst", "mit\u00b7samt", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Da kam auch ich mit meiner Liebe", "tokens": ["Da", "kam", "auch", "ich", "mit", "mei\u00b7ner", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und alle Kunst und aller Sinn,", "tokens": ["Und", "al\u00b7le", "Kunst", "und", "al\u00b7ler", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und da\u00df ich w\u00fc\u00dft', wo Alles bliebe", "tokens": ["Und", "da\u00df", "ich", "w\u00fc\u00dft'", ",", "wo", "Al\u00b7les", "blie\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "PWAV", "PIS", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Trug ich es zu der Einen hin.", "tokens": ["Trug", "ich", "es", "zu", "der", "Ei\u00b7nen", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Du gehst ganz lustig durch spazieren", "tokens": ["Du", "gehst", "ganz", "lus\u00b7tig", "durch", "spa\u00b7zie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPR", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und drehst das H\u00e4lschen in die Rund,", "tokens": ["Und", "drehst", "das", "H\u00e4l\u00b7schen", "in", "die", "Rund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich habe Eins nur zu verlieren,", "tokens": ["Ich", "ha\u00b7be", "Eins", "nur", "zu", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit dir geht Alles mir zu Grund.", "tokens": ["Mit", "dir", "geht", "Al\u00b7les", "mir", "zu", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PIS", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Du suchest das in allen Dingen", "tokens": ["Du", "su\u00b7chest", "das", "in", "al\u00b7len", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was ich in dir gefunden hab',", "tokens": ["Was", "ich", "in", "dir", "ge\u00b7fun\u00b7den", "hab'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du m\u00f6chtest allen Liebe bringen,", "tokens": ["Du", "m\u00f6ch\u00b7test", "al\u00b7len", "Lie\u00b7be", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich tret' der Lieben Alles ab.", "tokens": ["Ich", "tret'", "der", "Lie\u00b7ben", "Al\u00b7les", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Du suchst die Liebe rings entfaltet", "tokens": ["Du", "suchst", "die", "Lie\u00b7be", "rings", "ent\u00b7fal\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich sehe sie in dich verh\u00fcllt,", "tokens": ["Ich", "se\u00b7he", "sie", "in", "dich", "ver\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nichts hast du, was sich dir gestaltet,", "tokens": ["Nichts", "hast", "du", ",", "was", "sich", "dir", "ge\u00b7stal\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "$,", "PRELS", "PRF", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich hab' dich nicht du s\u00fc\u00dfes Bild.", "tokens": ["Ich", "hab'", "dich", "nicht", "du", "s\u00fc\u00b7\u00dfes", "Bild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Was du inmitten von vier Winden", "tokens": ["Was", "du", "in\u00b7mit\u00b7ten", "von", "vier", "Win\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADV", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu suchen hin und her dich drehst", "tokens": ["Zu", "su\u00b7chen", "hin", "und", "her", "dich", "drehst"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "PTKVZ", "KON", "ADV", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kann mir in vier Fischbeinchen schwinden,", "tokens": ["Kann", "mir", "in", "vier", "Fischbein\u00b7chen", "schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "CARD", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn du ein bi\u00dfchen schneller gehst.", "tokens": ["Wenn", "du", "ein", "bi\u00df\u00b7chen", "schnel\u00b7ler", "gehst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Du m\u00f6chtest in der Liebe w\u00e4hlen", "tokens": ["Du", "m\u00f6ch\u00b7test", "in", "der", "Lie\u00b7be", "w\u00e4h\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich folge Arm Lind dir, weil ich mu\u00df.", "tokens": ["Ich", "fol\u00b7ge", "Arm", "Lind", "dir", ",", "weil", "ich", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "NN", "PPER", "$,", "KOUS", "PPER", "VMFIN", "$."], "meter": "-+-++-+-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Du m\u00f6chtest die Gestirne z\u00e4hlen", "tokens": ["Du", "m\u00f6ch\u00b7test", "die", "Ge\u00b7stir\u00b7ne", "z\u00e4h\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich fand die Welt in einer Nu\u00df.", "tokens": ["Ich", "fand", "die", "Welt", "in", "ei\u00b7ner", "Nu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "S\u00fc\u00df Lieb, das ich mu\u00df hei\u00df verlangen,", "tokens": ["S\u00fc\u00df", "Lieb", ",", "das", "ich", "mu\u00df", "hei\u00df", "ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PRELS", "PPER", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Arm Lind, all, was du krank vermiss'st,", "tokens": ["Arm", "Lind", ",", "all", ",", "was", "du", "krank", "ver\u00b7miss'st", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PIAT", "$,", "PWS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir Werdens einst in Uns umfangen", "tokens": ["Wir", "Wer\u00b7dens", "einst", "in", "Uns", "um\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "ADV", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In dem, der Eins und Alles ist.", "tokens": ["In", "dem", ",", "der", "Eins", "und", "Al\u00b7les", "ist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "ART", "NN", "KON", "PIS", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Mir brennet in dem kranken Herzen", "tokens": ["Mir", "bren\u00b7net", "in", "dem", "kran\u00b7ken", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In einem Flammenblumenstrau\u00df.", "tokens": ["In", "ei\u00b7nem", "Flam\u00b7men\u00b7blu\u00b7men\u00b7strau\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von unerme\u00dfner Art der Schmerzen", "tokens": ["Von", "un\u00b7er\u00b7me\u00df\u00b7ner", "Art", "der", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die tiefgebeugte Seele aus.", "tokens": ["Die", "tief\u00b7ge\u00b7beug\u00b7te", "See\u00b7le", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Und du, durch die der Strau\u00df erbl\u00fchet,", "tokens": ["Und", "du", ",", "durch", "die", "der", "Strau\u00df", "er\u00b7bl\u00fc\u00b7het", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "APPR", "PRELS", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Streckst wohl zu ihm die feine Hand", "tokens": ["Streckst", "wohl", "zu", "ihm", "die", "fei\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Scheust nicht die Glut, aus dir ergl\u00fchet,", "tokens": ["Scheust", "nicht", "die", "Glut", ",", "aus", "dir", "er\u00b7gl\u00fc\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "$,", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Scheust nicht dies Herz von dir entbrannt,", "tokens": ["Scheust", "nicht", "dies", "Herz", "von", "dir", "ent\u00b7brannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "PDS", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Und wenn die Feuerblumen blitzen", "tokens": ["Und", "wenn", "die", "Feu\u00b7er\u00b7blu\u00b7men", "blit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von meiner Tr\u00e4nen hei\u00dfem Tau", "tokens": ["Von", "mei\u00b7ner", "Tr\u00e4\u00b7nen", "hei\u00b7\u00dfem", "Tau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Z\u00e4hlst du mit k\u00fchlen Fingerspitzen", "tokens": ["Z\u00e4hlst", "du", "mit", "k\u00fch\u00b7len", "Fin\u00b7ger\u00b7spit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Bl\u00fcmchen auf des Traumes Au.", "tokens": ["Die", "Bl\u00fcm\u00b7chen", "auf", "des", "Trau\u00b7mes", "Au", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Ich hab' den Schmerzenstrau\u00df gedichtet,", "tokens": ["Ich", "hab'", "den", "Schmer\u00b7zen\u00b7strau\u00df", "ge\u00b7dich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der flammend mir im Herzen rast,", "tokens": ["Der", "flam\u00b7mend", "mir", "im", "Her\u00b7zen", "rast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und hab' in Flammen es vernichtet", "tokens": ["Und", "hab'", "in", "Flam\u00b7men", "es", "ver\u00b7nich\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "NN", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df nicht die Glut dein Herz verglast.", "tokens