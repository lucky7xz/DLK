, "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der flammend mir im Herzen rast,", "tokens": ["Der", "flam\u00b7mend", "mir", "im", "Her\u00b7zen", "rast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und hab' in Flammen es vernichtet", "tokens": ["Und", "hab'", "in", "Flam\u00b7men", "es", "ver\u00b7nich\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "NN", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df nicht die Glut dein Herz verglast.", "tokens": ["Da\u00df", "nicht", "die", "Glut", "dein", "Herz", "ver\u00b7glast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.33": {"line.1": {"text": "Ich habe viel zu dir gesprochen", "tokens": ["Ich", "ha\u00b7be", "viel", "zu", "dir", "ge\u00b7spro\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auch letzte Worte bis zum Tod,", "tokens": ["Auch", "letz\u00b7te", "Wor\u00b7te", "bis", "zum", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und hab' mein Herz vor dir gebrochen,", "tokens": ["Und", "hab'", "mein", "Herz", "vor", "dir", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "ia