{"dta.poem.11925": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "86.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Licht! du bist nicht warm, die Sonne steht zu schief;", "tokens": ["Mein", "Licht", "!", "du", "bist", "nicht", "warm", ",", "die", "Son\u00b7ne", "steht", "zu", "schief", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PPER", "VAFIN", "PTKNEG", "ADJD", "$,", "ART", "NN", "VVFIN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du streifest nur mein Aug' und dringst ins Herz nicht tief.", "tokens": ["Du", "strei\u00b7fest", "nur", "mein", "Aug'", "und", "dringst", "ins", "Herz", "nicht", "tief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "KON", "ADV", "APPRART", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Blume will nicht bl\u00fchn, der Anger wird nicht gr\u00fcn;", "tokens": ["Die", "Blu\u00b7me", "will", "nicht", "bl\u00fchn", ",", "der", "An\u00b7ger", "wird", "nicht", "gr\u00fcn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "VVINF", "$,", "ART", "NN", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weltsehnsucht ist umsonst, umsonst dein Liebesm\u00fchn.", "tokens": ["Welt\u00b7sehn\u00b7sucht", "ist", "um\u00b7sonst", ",", "um\u00b7sonst", "dein", "Lie\u00b7bes\u00b7m\u00fchn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "$,", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "O hebe dich, mein Licht, aus winterlicher Schiefe", "tokens": ["O", "he\u00b7be", "dich", ",", "mein", "Licht", ",", "aus", "win\u00b7ter\u00b7li\u00b7cher", "Schie\u00b7fe"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zur Sommerh\u00f6h', und geu\u00df Erregung in die Tiefe!", "tokens": ["Zur", "Som\u00b7mer\u00b7h\u00f6h'", ",", "und", "geu\u00df", "Er\u00b7re\u00b7gung", "in", "die", "Tie\u00b7fe", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KON", "VVIMP", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}