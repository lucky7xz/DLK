{"textgrid.poem.33062": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich fleh' zu dir, o, lausche meinen T\u00f6nen,", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich fleh' zu dir, o, lausche meinen T\u00f6nen,", "tokens": ["Ich", "fleh'", "zu", "dir", ",", "o", ",", "lau\u00b7sche", "mei\u00b7nen", "T\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "FM", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die sanfte Luft zu deinem Ohre tr\u00e4gt,", "tokens": ["Die", "sanf\u00b7te", "Luft", "zu", "dei\u00b7nem", "Oh\u00b7re", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Lass' sagen meines Liedes hei\u00dfes Sehnen,", "tokens": ["Lass'", "sa\u00b7gen", "mei\u00b7nes", "Lie\u00b7des", "hei\u00b7\u00dfes", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was lange schon mein volles Herz bewegt.", "tokens": ["Was", "lan\u00b7ge", "schon", "mein", "vol\u00b7les", "Herz", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Du lauschst ja auch der Aeolsharfe Klingen,", "tokens": ["Du", "lauschst", "ja", "auch", "der", "A\u00b7e\u00b7ols\u00b7har\u00b7fe", "Klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Wenn sanfter Wind durch ihre Saiten zieht,", "tokens": ["Wenn", "sanf\u00b7ter", "Wind", "durch", "ih\u00b7re", "Sai\u00b7ten", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und l\u00e4chelst fr\u00f6hlich bei der Lerche Singen \u2013", "tokens": ["Und", "l\u00e4\u00b7chelst", "fr\u00f6h\u00b7lich", "bei", "der", "Ler\u00b7che", "Sin\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "So l\u00e4chle jetzt auch freundlich meinem Lied.", "tokens": ["So", "l\u00e4ch\u00b7le", "jetzt", "auch", "freund\u00b7lich", "mei\u00b7nem", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Denn, um das Herz dir schmeichelnd zu erschlie\u00dfen,", "tokens": ["Denn", ",", "um", "das", "Herz", "dir", "schmei\u00b7chelnd", "zu", "er\u00b7schlie\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUI", "ART", "NN", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Hab' ich manch' s\u00fc\u00dfen Ton hineingebannt,", "tokens": ["Hab'", "ich", "man\u00b7ch'", "s\u00fc\u00b7\u00dfen", "Ton", "hin\u00b7ein\u00b7ge\u00b7bannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PIAT", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.11": {"text": "Und, die vom Himmel sich zur Erd' ergie\u00dfen,", "tokens": ["Und", ",", "die", "vom", "Him\u00b7mel", "sich", "zur", "Erd'", "er\u00b7gie\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PRELS", "APPRART", "NN", "PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Die Melodieen der Natur entwandt.", "tokens": ["Die", "Me\u00b7lo\u00b7di\u00b7e\u00b7en", "der", "Na\u00b7tur", "ent\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+--+---+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Der Nachtigall lauscht' ich im dunklen Hain,", "tokens": ["Der", "Nach\u00b7ti\u00b7gall", "lauscht'", "ich", "im", "dunk\u00b7len", "Hain", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Sog ihren vollsten Ton in's Herz hinein,", "tokens": ["Sog", "ih\u00b7ren", "volls\u00b7ten", "Ton", "in's", "Herz", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Ich h\u00f6rte, was bei'm sanften Sternenlicht", "tokens": ["Ich", "h\u00f6r\u00b7te", ",", "was", "bei'm", "sanf\u00b7ten", "Ster\u00b7nen\u00b7licht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Geheim die Lilie zu der Rose spricht.", "tokens": ["Ge\u00b7heim", "die", "Li\u00b7lie", "zu", "der", "Ro\u00b7se", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Ich lag im Wald am mos'gen Felsenhang,", "tokens": ["Ich", "lag", "im", "Wald", "am", "mos'\u00b7gen", "Fel\u00b7sen\u00b7hang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Aus dessen Brust ein B\u00e4chlein murmelnd sprang,", "tokens": ["Aus", "des\u00b7sen", "Brust", "ein", "B\u00e4ch\u00b7lein", "mur\u00b7melnd", "sprang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Des Rieselns Sinn hab' ich ihm abgelauscht,", "tokens": ["Des", "Rie\u00b7selns", "Sinn", "hab'", "ich", "ihm", "ab\u00b7ge\u00b7lauscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Und wie's ihm Antwort durch die Zweige rauscht. \u2013", "tokens": ["Und", "wie's", "ihm", "Ant\u00b7wort", "durch", "die", "Zwei\u00b7ge", "rauscht", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "APPR", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Sein Nachtgebet das letzte V\u00f6glein sang,", "tokens": ["Sein", "Nacht\u00b7ge\u00b7bet", "das", "letz\u00b7te", "V\u00f6\u00b7glein", "sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Zur Ruhe mahnt der Abendglocke Klang,", "tokens": ["Zur", "Ru\u00b7he", "mahnt", "der", "A\u00b7bend\u00b7glo\u00b7cke", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Nur leise summt noch die Cikade dort,", "tokens": ["Nur", "lei\u00b7se", "summt", "noch", "die", "Ci\u00b7ka\u00b7de", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ADV", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Die Glocke schweigt in zitterndem Accord,", "tokens": ["Die", "Glo\u00b7cke", "schweigt", "in", "zit\u00b7tern\u00b7dem", "Ac\u00b7cord", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Ein Seufzer noch \u2013 dann h\u00f6rt mein Ohr mit Beben", "tokens": ["Ein", "Seuf\u00b7zer", "noch", "\u2013", "dann", "h\u00f6rt", "mein", "Ohr", "mit", "Be\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$(", "ADV", "VVFIN", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Des Tages letzten Laut in Nacht verschweben.", "tokens": ["Des", "Ta\u00b7ges", "letz\u00b7ten", "Laut", "in", "Nacht", "ver\u00b7schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "APPR", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Auf ging der Mond, und neue Melodie'n", "tokens": ["Auf", "ging", "der", "Mond", ",", "und", "neu\u00b7e", "Me\u00b7lo\u00b7die'n"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "VVFIN", "ART", "NN", "$,", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Begannen durch die stille Nacht zu zieh'n;", "tokens": ["Be\u00b7gan\u00b7nen", "durch", "die", "stil\u00b7le", "Nacht", "zu", "zieh'n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Der Erd' entstr\u00f6mten s\u00fc\u00dfe Liebesklagen,", "tokens": ["Der", "Erd'", "ent\u00b7str\u00f6m\u00b7ten", "s\u00fc\u00b7\u00dfe", "Lie\u00b7bes\u00b7kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Die milde L\u00fcfte hoch gen Himmel tragen,", "tokens": ["Die", "mil\u00b7de", "L\u00fcf\u00b7te", "hoch", "gen", "Him\u00b7mel", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "D'raus leise tr\u00f6stend T\u00f6ne niederwallen,", "tokens": ["D'raus", "lei\u00b7se", "tr\u00f6s\u00b7tend", "T\u00f6\u00b7ne", "nie\u00b7der\u00b7wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Wie droben sie von Engelsharfen schallen.", "tokens": ["Wie", "dro\u00b7ben", "sie", "von", "En\u00b7gels\u00b7har\u00b7fen", "schal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Der Erde Leid, des Himmels sel'ge Lust \u2013", "tokens": ["Der", "Er\u00b7de", "Leid", ",", "des", "Him\u00b7mels", "sel'\u00b7ge", "Lust", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und Blumensprach' und Nachtigallensang", "tokens": ["Und", "Blu\u00b7men\u00b7sprach'", "und", "Nach\u00b7ti\u00b7gal\u00b7len\u00b7sang"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und Bachesmurmeln, Abendglockenklang,", "tokens": ["Und", "Ba\u00b7ches\u00b7mur\u00b7meln", ",", "A\u00b7bend\u00b7glo\u00b7cken\u00b7klang", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dies Alles ist in meinem Lied erklungen,", "tokens": ["Dies", "Al\u00b7les", "ist", "in", "mei\u00b7nem", "Lied", "er\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VAFIN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich hab' dir's zitternd, bebend vorgesungen.", "tokens": ["Ich", "hab'", "dir's", "zit\u00b7ternd", ",", "be\u00b7bend", "vor\u00b7ge\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "VVPP", "$,", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dein dunkles Auge eine Thr\u00e4ne f\u00fcllt,", "tokens": ["Dein", "dunk\u00b7les", "Au\u00b7ge", "ei\u00b7ne", "Thr\u00e4\u00b7ne", "f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Seufzer deinen Lippen sanft entquillt,", "tokens": ["Ein", "Seuf\u00b7zer", "dei\u00b7nen", "Lip\u00b7pen", "sanft", "ent\u00b7quillt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mein flehend Lied, es hat dein Herz erweicht,", "tokens": ["Mein", "fle\u00b7hend", "Lied", ",", "es", "hat", "dein", "Herz", "er\u00b7weicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVPP", "NN", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Des Lebens h\u00f6chstes Ziel, es ist erreicht!", "tokens": ["Des", "Le\u00b7bens", "h\u00f6chs\u00b7tes", "Ziel", ",", "es", "ist", "er\u00b7reicht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da wollt' ich jubeln wie der Wasserfall,", "tokens": ["Da", "wollt'", "ich", "ju\u00b7beln", "wie", "der", "Was\u00b7ser\u00b7fall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "So sollte donnern meiner T\u00f6ne Schall,", "tokens": ["So", "soll\u00b7te", "don\u00b7nern", "mei\u00b7ner", "T\u00f6\u00b7ne", "Schall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da wollt' ich jauchzen, wie die junge Welt,", "tokens": ["Da", "wollt'", "ich", "jauch\u00b7zen", ",", "wie", "die", "jun\u00b7ge", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wenn Sonnenku\u00df nach langer Nacht sie hellt.", "tokens": ["Wenn", "Son\u00b7nen\u00b7ku\u00df", "nach", "lan\u00b7ger", "Nacht", "sie", "hellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Hin ist die Kraft \u2013 mir blieb ein einz'ger Ton,", "tokens": ["Hin", "ist", "die", "Kraft", "\u2013", "mir", "blieb", "ein", "einz'\u00b7ger", "Ton", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$(", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Wie betend Engelslippen er entfloh'n!", "tokens": ["Wie", "be\u00b7tend", "En\u00b7gels\u00b7lip\u00b7pen", "er", "ent\u00b7floh'n", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ich fleh' zu dir, o, lausche meinen T\u00f6nen,", "tokens": ["Ich", "fleh'", "zu", "dir", ",", "o", ",", "lau\u00b7sche", "mei\u00b7nen", "T\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "FM", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die sanfte Luft zu deinem Ohre tr\u00e4gt,", "tokens": ["Die", "sanf\u00b7te", "Luft", "zu", "dei\u00b7nem", "Oh\u00b7re", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Lass' sagen meines Liedes hei\u00dfes Sehnen,", "tokens": ["Lass'", "sa\u00b7gen", "mei\u00b7nes", "Lie\u00b7des", "hei\u00b7\u00dfes", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was lange schon mein volles Herz bewegt.", "tokens": ["Was", "lan\u00b7ge", "schon", "mein", "vol\u00b7les", "Herz", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Du lauschst ja auch der Aeolsharfe Klingen,", "tokens": ["Du", "lauschst", "ja", "auch", "der", "A\u00b7e\u00b7ols\u00b7har\u00b7fe", "Klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Wenn sanfter Wind durch ihre Saiten zieht,", "tokens": ["Wenn", "sanf\u00b7ter", "Wind", "durch", "ih\u00b7re", "Sai\u00b7ten", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "