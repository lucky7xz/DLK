{"dta.poem.20106": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Manch Jahr, in immer schnellrer Flucht,", "tokens": ["Manch", "Jahr", ",", "in", "im\u00b7mer", "schnell\u00b7rer", "Flucht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist hin in\u2019s Land gegangen,", "tokens": ["Ist", "hin", "in's", "Land", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "L\u00e4ngst h\u00e4lt der Graf, in Sitt\u2019 und Zucht,", "tokens": ["L\u00e4ngst", "h\u00e4lt", "der", "Graf", ",", "in", "Sitt'", "und", "Zucht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein jung Gemahl umfangen;", "tokens": ["Ein", "jung", "Ge\u00b7mahl", "um\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In ihrem Aug\u2019 ist andres nicht", "tokens": ["In", "ih\u00b7rem", "Aug'", "ist", "and\u00b7res", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PIS", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie Lieb und Treu zu schauen,", "tokens": ["Wie", "Lieb", "und", "Treu", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Doch keinem Engelsangesicht", "tokens": ["Doch", "kei\u00b7nem", "En\u00b7gel\u00b7san\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word"], "pos": ["KON", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Verm\u00f6cht er zu vertrauen.", "tokens": ["Ver\u00b7m\u00f6cht", "er", "zu", "ver\u00b7trau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er schl\u00e4ft: \u2014 auff\u00e4hrt er aus dem Traum,", "tokens": ["Er", "schl\u00e4ft", ":", "auf\u00b7f\u00e4hrt", "er", "aus", "dem", "Traum", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er bebt an Seel\u2019 und Leibe,", "tokens": ["Er", "bebt", "an", "Seel'", "und", "Lei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Todbla\u00df, die F\u00fc\u00dfe wollen kaum,", "tokens": ["Tod\u00b7bla\u00df", ",", "die", "F\u00fc\u00b7\u00dfe", "wol\u00b7len", "kaum", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "VMFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schleicht er zu seinem Weibe;", "tokens": ["Schleicht", "er", "zu", "sei\u00b7nem", "Wei\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er lauscht, und als er vor ihr steht,", "tokens": ["Er", "lauscht", ",", "und", "als", "er", "vor", "ihr", "steht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was h\u00f6rt er? ", "tokens": ["Was", "h\u00f6rt", "er", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Ihr Tr\u00e4umen war ein fromm Gebet,", "tokens": ["Ihr", "Tr\u00e4u\u00b7men", "war", "ein", "fromm", "Ge\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Vernehmlich sprach sie: Amen!", "tokens": ["Ver\u00b7nehm\u00b7lich", "sprach", "sie", ":", "A\u00b7men", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$.", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er reitet einsam in den Wald,", "tokens": ["Er", "rei\u00b7tet", "ein\u00b7sam", "in", "den", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sinnt, und \u2014 mu\u00df erbleichen:", "tokens": ["Und", "sinnt", ",", "und", "mu\u00df", "er\u00b7blei\u00b7chen", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KON", "$(", "VMFIN", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Er dr\u00fcckt dem Renner allsobald", "tokens": ["Er", "dr\u00fcckt", "dem", "Ren\u00b7ner", "all\u00b7so\u00b7bald"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Sporen in die Weichen,", "tokens": ["Die", "Spo\u00b7ren", "in", "die", "Wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er fliegt ", "tokens": ["Er", "fliegt"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Im Wettlauf mit dem Winde, \u2014", "tokens": ["Im", "Wett\u00b7lauf", "mit", "dem", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und findet \u2014 spielend vor dem Schlo\u00df,", "tokens": ["Und", "fin\u00b7det", "spie\u00b7lend", "vor", "dem", "Schlo\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sein Weib mit seinem Kinde.", "tokens": ["Sein", "Weib", "mit", "sei\u00b7nem", "Kin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Oft l\u00e4\u00dft er selbst, auf seinen Knien,", "tokens": ["Oft", "l\u00e4\u00dft", "er", "selbst", ",", "auf", "sei\u00b7nen", "Kni\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den h\u00fcbschen Blondkopf schaukeln,", "tokens": ["Den", "h\u00fcb\u00b7schen", "Blond\u00b7kopf", "schau\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bis pl\u00f6tzlich tolle Bilder ihn,", "tokens": ["Bis", "pl\u00f6tz\u00b7lich", "tol\u00b7le", "Bil\u00b7der", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie hergeweht, umgaukeln:", "tokens": ["Wie", "her\u00b7ge\u00b7weht", ",", "um\u00b7gau\u00b7keln", ":"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Des Kindes Augen sind so blau,", "tokens": ["Des", "Kin\u00b7des", "Au\u00b7gen", "sind", "so", "blau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und schwarz sind doch die seinen, \u2014", "tokens": ["Und", "schwarz", "sind", "doch", "die", "sei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ADV", "ART", "PPOSAT", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Er st\u00f6\u00dft es fort, und murmelt rauh:", "tokens": ["Er", "st\u00f6\u00dft", "es", "fort", ",", "und", "mur\u00b7melt", "rauh", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "\u201ewas k\u00fcmmert mich sein Weinen?\u201c", "tokens": ["\u201e", "was", "k\u00fcm\u00b7mert", "mich", "sein", "Wei\u00b7nen", "?", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Einst als sein Ro\u00df, im Walde draus", "tokens": ["Einst", "als", "sein", "Ro\u00df", ",", "im", "Wal\u00b7de", "draus"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "KOKOM", "PPOSAT", "NN", "$,", "APPRART", "NN", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gar alten Weg genommen,", "tokens": ["Gar", "al\u00b7ten", "Weg", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ist an des F\u00f6rsters stillem Haus", "tokens": ["Ist", "an", "des", "F\u00f6rs\u00b7ters", "stil\u00b7lem", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Graf vorbeigekommen;", "tokens": ["Der", "Graf", "vor\u00b7bei\u00b7ge\u00b7kom\u00b7men", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er sprach: \u201edie Treu ist keine M\u00e4hr\u2019; \u2014", "tokens": ["Er", "sprach", ":", "\u201e", "die", "Treu", "ist", "kei\u00b7ne", "M\u00e4hr'", ";"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "ART", "NN", "VAFIN", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich hab ihr Band zerrissen,", "tokens": ["Ich", "hab", "ihr", "Band", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Nun treibt mich ruhelos umher", "tokens": ["Nun", "treibt", "mich", "ru\u00b7he\u00b7los", "um\u00b7her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein strafendes Gewissen.\u201c", "tokens": ["Ein", "stra\u00b7fen\u00b7des", "Ge\u00b7wis\u00b7sen", ".", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}