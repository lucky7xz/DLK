{"dta.poem.785": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XxI.  \n Auff H. Baums vnd Jungfr.  Ann\u00e6 Mari\u00e6  \n Gryphi\u00e6  Hochzeit.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Jn dieser Krieges Erndt vnd vberheissen zeit ", "tokens": ["In", "die\u00b7ser", "Krie\u00b7ges", "Erndt", "vnd", "vber\u00b7heis\u00b7sen", "zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "NE", "KON", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In der man was man schawt/ vor Hitze schawt ver-", "tokens": ["In", "der", "man", "was", "man", "schawt", "/", "vor", "Hit\u00b7ze", "schawt", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "PIS", "PWS", "PIS", "VVFIN", "$(", "APPR", "NN", "VVFIN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "schmachten/", "tokens": ["schmach\u00b7ten", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Seid Jhr G Jungfraw Brautt gl\u00fcckselig wol zu achten", "tokens": ["Seid", "Ihr", "G", "Jung\u00b7fraw", "Brautt", "gl\u00fcck\u00b7se\u00b7lig", "wol", "zu", "ach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAIMP", "PPER", "NE", "NE", "NE", "ADJD", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Weil euch der Liebe glutt/ selbst schatten hat bereit/", "tokens": ["Weil", "euch", "der", "Lie\u00b7be", "glutt", "/", "selbst", "schat\u00b7ten", "hat", "be\u00b7reit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "$(", "ADV", "VVFIN", "VAFIN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Jhr sitzt bey ewrem Baum vnd trotzt das grimme leid", "tokens": ["Ihr", "sitzt", "bey", "ew\u00b7rem", "Baum", "vnd", "trotzt", "das", "grim\u00b7me", "leid"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das vnser Vaterland eh\u2019 al\u00df wir noch gedachten", "tokens": ["Das", "vn\u00b7ser", "Va\u00b7ter\u00b7land", "eh'", "al\u00df", "wir", "noch", "ge\u00b7dach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NE", "KOUS", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Versenget vnd verbrand/ das. was die felder brachten/", "tokens": ["Ver\u00b7sen\u00b7get", "vnd", "ver\u00b7brand", "/", "das", ".", "was", "die", "fel\u00b7der", "brach\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "$(", "PDS", "$.", "PWS", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was in den G\u00e4rten bl\u00fcht/ mit Schwerdt vnd flammen", "tokens": ["Was", "in", "den", "G\u00e4r\u00b7ten", "bl\u00fcht", "/", "mit", "Schwerdt", "vnd", "flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "$(", "APPR", "NN", "KON", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "may\u2019t.", "tokens": ["may'", "t."], "token_info": ["word", "abbreviation"], "pos": ["XY", "XY"], "meter": "-", "measure": "single.down"}}, "stanza.3": {"line.1": {"text": "Der Baum den jhr erkie\u00dft wird jederzeit euch decken:", "tokens": ["Der", "Baum", "den", "jhr", "er\u00b7kie\u00dft", "wird", "je\u00b7der\u00b7zeit", "euch", "de\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "VVFIN", "VAFIN", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Euch wird kein tr\u00fcber sturm/ kein vngewitter schrecken/", "tokens": ["Euch", "wird", "kein", "tr\u00fc\u00b7ber", "sturm", "/", "kein", "vn\u00b7ge\u00b7wit\u00b7ter", "schre\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "ADJA", "NN", "$(", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Zweige werden st\u00e4ts voll sch\u00f6ner bl\u00e4tter stehn/", "tokens": ["Die", "Zwei\u00b7ge", "wer\u00b7den", "st\u00e4ts", "voll", "sch\u00f6\u00b7ner", "bl\u00e4t\u00b7ter", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die \u00e4ste/ die ihr schon seht voll von bl\u00fctten schweben/", "tokens": ["Die", "\u00e4s\u00b7te", "/", "die", "ihr", "schon", "seht", "voll", "von", "bl\u00fct\u00b7ten", "schwe\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$(", "PRELS", "PPER", "ADV", "VVFIN", "ADJD", "APPR", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die werden k\u00fcnfftig euch gew\u00fcndsche Fr\u00fcchte geben/", "tokens": ["Die", "wer\u00b7den", "k\u00fcnff\u00b7tig", "euch", "ge\u00b7w\u00fcnd\u00b7sche", "Fr\u00fcch\u00b7te", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "PPER", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Di\u00df Laub wird/ glaubt gewi\u00df/ von keinem frost vergehn.", "tokens": ["Di\u00df", "Laub", "wird", "/", "glaubt", "ge\u00b7wi\u00df", "/", "von", "kei\u00b7nem", "frost", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VAFIN", "$(", "VVFIN", "ADV", "$(", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}