{"dta.poem.17761": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "Sechstes Buch.  \n Geistreicher Sinn- unnd  \n Schlu\u00df reimen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Beyneben h\u00f6rt man nichts als Donnern/ Hageln/", "tokens": ["Bey\u00b7ne\u00b7ben", "h\u00f6rt", "man", "nichts", "als", "Don\u00b7nern", "/", "Ha\u00b7geln", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "PIS", "KOKOM", "NN", "$(", "NN", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Krachen.", "tokens": ["Kra\u00b7chen", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Man sieht den Basilischk mit Kr\u00f6ten/ Schlangen/", "tokens": ["Man", "sieht", "den", "Ba\u00b7si\u00b7lischk", "mit", "Kr\u00f6\u00b7ten", "/", "Schlan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "APPR", "NN", "$(", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Drachen/", "tokens": ["Dra\u00b7chen", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Und tausend ungeheur: Man ist f\u00fcr K\u00e4lt\u2019 erfrohrn/", "tokens": ["Und", "tau\u00b7send", "un\u00b7ge\u00b7heur", ":", "Man", "ist", "f\u00fcr", "K\u00e4lt'", "er\u00b7frohrn", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "$.", "PIS", "VAFIN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und schmeltzt f\u00fcr grosser Glutt: man schilt sich Narrn", "tokens": ["Und", "schmeltzt", "f\u00fcr", "gros\u00b7ser", "Glutt", ":", "man", "schilt", "sich", "Narrn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$.", "PIS", "VVFIN", "PRF", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "und Thorn.", "tokens": ["und", "Thorn", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Und kombt doch nimmermehr au\u00df diesem Teufels", "tokens": ["Und", "kombt", "doch", "nim\u00b7mer\u00b7mehr", "au\u00df", "die\u00b7sem", "Teu\u00b7fels"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.9": {"text": "rachen.", "tokens": ["ra\u00b7chen", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.10": {"text": "Man stirbt/ und stirbt doch nie/ man ligt im ewgen Tod/", "tokens": ["Man", "stirbt", "/", "und", "stirbt", "doch", "nie", "/", "man", "ligt", "im", "ew\u00b7gen", "Tod", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$(", "KON", "VVFIN", "ADV", "ADV", "$(", "PIS", "VVFIN", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Man w\u00fcttet tobt und z\u00f6rnt/ man flucht und l\u00e4stert", "tokens": ["Man", "w\u00fct\u00b7tet", "tobt", "und", "z\u00f6rnt", "/", "man", "flucht", "und", "l\u00e4s\u00b7tert"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "VVFIN", "KON", "VVFIN", "$(", "PIS", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Gott.", "tokens": ["Gott", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.13": {"text": "Man beist und Hadert sich/ man lebt wie Hund\u2019 unnd", "tokens": ["Man", "beist", "und", "Ha\u00b7dert", "sich", "/", "man", "lebt", "wie", "Hund'", "unnd"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "KON", "NE", "PRF", "$(", "PIS", "VVFIN", "KOKOM", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Katzen:", "tokens": ["Kat\u00b7zen", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "Man mu\u00df sich ewiglich mit allen Teuffeln kratzen.", "tokens": ["Man", "mu\u00df", "sich", "e\u00b7wig\u00b7lich", "mit", "al\u00b7len", "Teuf\u00b7feln", "krat\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PRF", "ADV", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Man frisset H\u00fcttenrauch/ Pech/ Schweffel/ Teuffels-", "tokens": ["Man", "fris\u00b7set", "H\u00fct\u00b7ten\u00b7rauch", "/", "Pech", "/", "Schwef\u00b7fel", "/", "Teuf\u00b7fels"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["PIS", "VVFIN", "NN", "$(", "NE", "$(", "NN", "$(", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "mist:", "tokens": ["mist", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.18": {"text": "Ach S\u00fcnder thu doch Buss\u2019/ eh du darinnen bist!", "tokens": ["Ach", "S\u00fcn\u00b7der", "thu", "doch", "Buss'", "/", "eh", "du", "da\u00b7rin\u00b7nen", "bist", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "VVFIN", "ADV", "NN", "$(", "KOUS", "PPER", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}