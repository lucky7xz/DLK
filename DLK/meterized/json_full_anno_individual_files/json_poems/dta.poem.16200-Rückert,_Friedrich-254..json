{"dta.poem.16200": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "254.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Den innern Widerspruch im Menschen zu erkl\u00e4ren,", "tokens": ["Den", "in\u00b7nern", "Wi\u00b7der\u00b7spruch", "im", "Men\u00b7schen", "zu", "er\u00b7kl\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verneinten manche, da\u00df in ihm zwo Seelen w\u00e4ren,", "tokens": ["Ver\u00b7nein\u00b7ten", "man\u00b7che", ",", "da\u00df", "in", "ihm", "zwo", "See\u00b7len", "w\u00e4\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "KOUS", "APPR", "PPER", "CARD", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und ihn zum Guten die, zum B\u00f6sen jene triebe,", "tokens": ["Und", "ihn", "zum", "Gu\u00b7ten", "die", ",", "zum", "B\u00f6\u00b7sen", "je\u00b7ne", "trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "ART", "$,", "APPRART", "NN", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er aber unterthan bald der bald jener bliebe.", "tokens": ["Er", "a\u00b7ber", "un\u00b7ter\u00b7than", "bald", "der", "bald", "je\u00b7ner", "blie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "ADV", "ART", "ADV", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und andre nahmen an, da\u00df ihn zu beiden Seiten", "tokens": ["Und", "and\u00b7re", "nah\u00b7men", "an", ",", "da\u00df", "ihn", "zu", "bei\u00b7den", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "PTKVZ", "$,", "KOUS", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zween Engel, einer b\u00f6s' und einer gut, begleiten,", "tokens": ["Zween", "En\u00b7gel", ",", "ei\u00b7ner", "b\u00f6s'", "und", "ei\u00b7ner", "gut", ",", "be\u00b7glei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "NE", "$,", "ART", "ADJD", "KON", "ART", "ADJD", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die hier ins rechte Ohr ihm fl\u00fcstern, dort ins linke,", "tokens": ["Die", "hier", "ins", "rech\u00b7te", "Ohr", "ihm", "fl\u00fcs\u00b7tern", ",", "dort", "ins", "lin\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "ADJA", "NN", "PPER", "VVINF", "$,", "ADV", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hier, da\u00df er sich erheb', und dort, da\u00df er versinke.", "tokens": ["Hier", ",", "da\u00df", "er", "sich", "er\u00b7heb'", ",", "und", "dort", ",", "da\u00df", "er", "ver\u00b7sin\u00b7ke", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,", "KON", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Zwo Seelen sollst du nicht, noch auch zween Engel glauben;", "tokens": ["Zwo", "See\u00b7len", "sollst", "du", "nicht", ",", "noch", "auch", "zween", "En\u00b7gel", "glau\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VMFIN", "PPER", "PTKNEG", "$,", "ADV", "ADV", "VVFIN", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Freiheit w\u00fcrdest du, die eigne Kraft dir rauben.", "tokens": ["Die", "Frei\u00b7heit", "w\u00fcr\u00b7dest", "du", ",", "die", "eig\u00b7ne", "Kraft", "dir", "rau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "$,", "ART", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Der Widerspruch ist da, woher ist er gekommen?", "tokens": ["Der", "Wi\u00b7der\u00b7spruch", "ist", "da", ",", "wo\u00b7her", "ist", "er", "ge\u00b7kom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "PWAV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus dem Verschiedenen, woraus dich Gott genommen.", "tokens": ["Aus", "dem", "Ver\u00b7schie\u00b7de\u00b7nen", ",", "wo\u00b7raus", "dich", "Gott", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Genommen hat er, da\u00df du beider Einheit seist,", "tokens": ["Ge\u00b7nom\u00b7men", "hat", "er", ",", "da\u00df", "du", "bei\u00b7der", "Ein\u00b7heit", "seist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "KOUS", "PPER", "PIAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von Erde deinen Leib, von Himmel deinen Geist.", "tokens": ["Von", "Er\u00b7de", "dei\u00b7nen", "Leib", ",", "von", "Him\u00b7mel", "dei\u00b7nen", "Geist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$,", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Der Leib von Erde kann nur Irdisches begehren,", "tokens": ["Der", "Leib", "von", "Er\u00b7de", "kann", "nur", "Ir\u00b7di\u00b7sches", "be\u00b7geh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VMFIN", "ADV", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Geist vom Himmel nur zum Himmlischen sich kehren.", "tokens": ["Der", "Geist", "vom", "Him\u00b7mel", "nur", "zum", "Himm\u00b7li\u00b7schen", "sich", "keh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ADV", "APPRART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "So hat er dich gemischt, da\u00df du dich selbst bekriegest,", "tokens": ["So", "hat", "er", "dich", "ge\u00b7mischt", ",", "da\u00df", "du", "dich", "selbst", "be\u00b7krie\u00b7gest", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "VVPP", "$,", "KOUS", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit deinem H\u00f6heren dein Niederes besiegest,", "tokens": ["Mit", "dei\u00b7nem", "H\u00f6\u00b7he\u00b7ren", "dein", "Nie\u00b7de\u00b7res", "be\u00b7sie\u00b7gest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Ein Bild der Sch\u00f6pfung selbst, die er nur dazu schuf,", "tokens": ["Ein", "Bild", "der", "Sch\u00f6p\u00b7fung", "selbst", ",", "die", "er", "nur", "da\u00b7zu", "schuf", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "$,", "PRELS", "PPER", "ADV", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df dienstbar Leibliches sei geistigem Beruf.", "tokens": ["Da\u00df", "dienst\u00b7bar", "Leib\u00b7li\u00b7ches", "sei", "geis\u00b7ti\u00b7gem", "Be\u00b7ruf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}