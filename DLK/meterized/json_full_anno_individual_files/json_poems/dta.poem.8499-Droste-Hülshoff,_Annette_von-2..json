{"dta.poem.8499": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Und w&#228;r' es wahr auch, da\u00df der Jahre Hand", "tokens": ["Und", "w", "&#228;", "r'", "es", "wahr", "auch", ",", "da\u00df", "der", "Jah\u00b7re", "Hand"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NE", "$(", "VVFIN", "PPER", "ADJD", "ADV", "$,", "KOUS", "ART", "NN", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dir Furchen in die reine Stirn geschrieben,", "tokens": ["Dir", "Fur\u00b7chen", "in", "die", "rei\u00b7ne", "Stirn", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht so elastisch deiner Z\u00fcge Band", "tokens": ["Nicht", "so", "e\u00b7las\u00b7tisch", "dei\u00b7ner", "Z\u00fc\u00b7ge", "Band"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "ADJD", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bezeichne mehr dein Z\u00fcrnen und dein Lieben,", "tokens": ["Be\u00b7zeich\u00b7ne", "mehr", "dein", "Z\u00fcr\u00b7nen", "und", "dein", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "KON", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wenn minder klar die H\u00fclle dich umschlingt,", "tokens": ["Wenn", "min\u00b7der", "klar", "die", "H\u00fcl\u00b7le", "dich", "um\u00b7schlingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Durch die der Strahl, der gottbeseelte, dringt,", "tokens": ["Durch", "die", "der", "Strahl", ",", "der", "gott\u00b7be\u00b7seel\u00b7te", ",", "dringt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Mir bist die Gleiche immer du geblieben.", "tokens": ["Mir", "bist", "die", "Glei\u00b7che", "im\u00b7mer", "du", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wenn minder stolz und edel die Gestalt,", "tokens": ["Wenn", "min\u00b7der", "stolz", "und", "e\u00b7del", "die", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "KON", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich kenne sie, die ungebeugte Seele;", "tokens": ["Ich", "ken\u00b7ne", "sie", ",", "die", "un\u00b7ge\u00b7beug\u00b7te", "See\u00b7le", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn es wie Nebel deine Stirn umwallt,", "tokens": ["Wenn", "es", "wie", "Ne\u00b7bel", "dei\u00b7ne", "Stirn", "um\u00b7wallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOKOM", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich wei\u00df es, da\u00df die Wolke Strahlen hehle;", "tokens": ["Ich", "wei\u00df", "es", ",", "da\u00df", "die", "Wol\u00b7ke", "Strah\u00b7len", "heh\u00b7le", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und deiner reichen Stimme tiefer Klang", "tokens": ["Und", "dei\u00b7ner", "rei\u00b7chen", "Stim\u00b7me", "tie\u00b7fer", "Klang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Verhallend geisterhaft, wie Wellensang,", "tokens": ["Ver\u00b7hal\u00b7lend", "geis\u00b7ter\u00b7haft", ",", "wie", "Wel\u00b7len\u00b7sang", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "PWAV", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich f\u00fchl\u2019 es, da\u00df kein Liebeshauch ihm fehle.", "tokens": ["Ich", "f\u00fchl'", "es", ",", "da\u00df", "kein", "Lie\u00b7bes\u00b7hauch", "ihm", "feh\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PIAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O Fluch des Alters, wenn das Lebensheil", "tokens": ["O", "Fluch", "des", "Al\u00b7ters", ",", "wenn", "das", "Le\u00b7bens\u00b7heil"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "ART", "NN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit ihm, dem Gottesbilde m\u00fc\u00dfte weichen!", "tokens": ["Mit", "ihm", ",", "dem", "Got\u00b7tes\u00b7bil\u00b7de", "m\u00fc\u00df\u00b7te", "wei\u00b7chen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn minder liebewarm ein L\u00e4cheln, weil", "tokens": ["Wenn", "min\u00b7der", "lie\u00b7be\u00b7warm", "ein", "L\u00e4\u00b7cheln", ",", "weil"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "ADV", "ADJD", "ART", "NN", "$,", "KOUS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ihm Kummer eingegraben seine Zeichen!", "tokens": ["Ihm", "Kum\u00b7mer", "ein\u00b7ge\u00b7gra\u00b7ben", "sei\u00b7ne", "Zei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVPP", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein Auge g\u00fctig nur, so lange leicht", "tokens": ["Ein", "Au\u00b7ge", "g\u00fc\u00b7tig", "nur", ",", "so", "lan\u00b7ge", "leicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "ADV", "$,", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und silbern sich die Thr\u00e4ne ihm entschleicht,", "tokens": ["Und", "sil\u00b7bern", "sich", "die", "Thr\u00e4\u00b7ne", "ihm", "ent\u00b7schleicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und ros\u2019ge Wangen z\u00fccht\u2019ger als die bleichen.", "tokens": ["Und", "ros'\u00b7ge", "Wan\u00b7gen", "z\u00fccht'\u00b7ger", "als", "die", "blei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJD", "KOKOM", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und dennoch h\u00e4lt sie Alle uns beth\u00f6rt,", "tokens": ["Und", "den\u00b7noch", "h\u00e4lt", "sie", "Al\u00b7le", "uns", "be\u00b7th\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PIS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die staubgeborne Form, die wandelbare,", "tokens": ["Die", "staub\u00b7ge\u00b7bor\u00b7ne", "Form", ",", "die", "wan\u00b7del\u00b7ba\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Scheint willig uns ein Ohr, das leise h\u00f6rt,", "tokens": ["Scheint", "wil\u00b7lig", "uns", "ein", "Ohr", ",", "das", "lei\u00b7se", "h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "K\u00fchn einer frischen Kehle Lustfanfare;", "tokens": ["K\u00fchn", "ei\u00b7ner", "fri\u00b7schen", "Keh\u00b7le", "Lust\u00b7fan\u00b7fa\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wir Alle sehen nur des Pharus Licht,", "tokens": ["Wir", "Al\u00b7le", "se\u00b7hen", "nur", "des", "Pha\u00b7rus", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die Glut im Erdenschoo\u00dfe seh\u2019n wir nicht,", "tokens": ["Die", "Glut", "im", "Er\u00b7den\u00b7schoo\u00b7\u00dfe", "seh'n", "wir", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und Keiner denkt der Lampe am Altare.", "tokens": ["Und", "Kei\u00b7ner", "denkt", "der", "Lam\u00b7pe", "am", "Al\u00b7ta\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}