{"dta.poem.16139": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "193.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Armen Anblick ist ein stummer Vorwurf dir,", "tokens": ["Der", "Ar\u00b7men", "An\u00b7blick", "ist", "ein", "stum\u00b7mer", "Vor\u00b7wurf", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ART", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O Reicher, frage dich: Wer gab den Vorzug mir?", "tokens": ["O", "Rei\u00b7cher", ",", "fra\u00b7ge", "dich", ":", "Wer", "gab", "den", "Vor\u00b7zug", "mir", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "VVFIN", "PPER", "$.", "PWS", "VVFIN", "ART", "NN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der dir den Vorzug hat gegeben vor den Armen,", "tokens": ["Der", "dir", "den", "Vor\u00b7zug", "hat", "ge\u00b7ge\u00b7ben", "vor", "den", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VAFIN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gab er nicht auch f\u00fcr sie dir in die Seel' Erbarmen?", "tokens": ["Gab", "er", "nicht", "auch", "f\u00fcr", "sie", "dir", "in", "die", "Seel'", "Er\u00b7bar\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "APPR", "PPER", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}}, "stanza.3": {"line.1": {"text": "Und sind sie dankbarer f\u00fcr ihre Bl\u00f6\u00dfe gar,", "tokens": ["Und", "sind", "sie", "dank\u00b7ba\u00b7rer", "f\u00fcr", "ih\u00b7re", "Bl\u00f6\u00b7\u00dfe", "gar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als du f\u00fcr deine Pracht, wie bist du undankbar!", "tokens": ["Als", "du", "f\u00fcr", "dei\u00b7ne", "Pracht", ",", "wie", "bist", "du", "un\u00b7dank\u00b7bar", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "$,", "PWAV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und wenn an freudigem Vertraun sie dich besch\u00e4men,", "tokens": ["Und", "wenn", "an", "freu\u00b7di\u00b7gem", "Ver\u00b7traun", "sie", "dich", "be\u00b7sch\u00e4\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "ADJA", "NN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So braucht zur Strafe dir Gott nicht den Schatz zu nehmen.", "tokens": ["So", "braucht", "zur", "Stra\u00b7fe", "dir", "Gott", "nicht", "den", "Schatz", "zu", "neh\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "PPER", "NN", "PTKNEG", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Er lasse dir den Schatz, damit du wie die Schlange,", "tokens": ["Er", "las\u00b7se", "dir", "den", "Schatz", ",", "da\u00b7mit", "du", "wie", "die", "Schlan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,", "KOUS", "PPER", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die sch\u00e4tzeh\u00fctende, dich k\u00fcmmerst zag und bange,", "tokens": ["Die", "sch\u00e4t\u00b7ze\u00b7h\u00fc\u00b7ten\u00b7de", ",", "dich", "k\u00fcm\u00b7merst", "zag", "und", "ban\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PPER", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Da\u00df es die Armuth seh' und nicht solch Gl\u00fcck verlange.", "tokens": ["Da\u00df", "es", "die", "Ar\u00b7muth", "seh'", "und", "nicht", "solch", "Gl\u00fcck", "ver\u00b7lan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "KON", "PTKNEG", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}