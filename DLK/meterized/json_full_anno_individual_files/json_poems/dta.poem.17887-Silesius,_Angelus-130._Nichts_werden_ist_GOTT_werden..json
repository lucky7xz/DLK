{"dta.poem.17887": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "130.  Nichts werden ist GOTT werden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Nichts wird was zuvor ist: wirstu nicht vor zu nicht/", "tokens": ["Nichts", "wird", "was", "zu\u00b7vor", "ist", ":", "wirs\u00b7tu", "nicht", "vor", "zu", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PIS", "ADV", "VAFIN", "$.", "VAFIN", "PTKNEG", "APPR", "APPR", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wirstu nimmermehr gebohrn vom ewgen Licht.", "tokens": ["So", "wirs\u00b7tu", "nim\u00b7mer\u00b7mehr", "ge\u00b7bohrn", "vom", "ew\u00b7gen", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "VVPP", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}