{"dta.poem.13609": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Spruch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Bau nur auf Weltgunst recht", "tokens": ["Bau", "nur", "auf", "Welt\u00b7gunst", "recht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "NN", "ADJD"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Und pa\u00df' auf jeden Wink und Gru\u00df,", "tokens": ["Und", "pa\u00df'", "auf", "je\u00b7den", "Wink", "und", "Gru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wirst dabei nimmer fr\u00f6hlich werden!", "tokens": ["Wirst", "da\u00b7bei", "nim\u00b7mer", "fr\u00f6h\u00b7lich", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PAV", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es hat's kein Hund so schlecht,", "tokens": ["Es", "hat's", "kein", "Hund", "so", "schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der hinter seinem Herren mu\u00df,", "tokens": ["Der", "hin\u00b7ter", "sei\u00b7nem", "Her\u00b7ren", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht frei spazieren kann auf Erden.", "tokens": ["Nicht", "frei", "spa\u00b7zie\u00b7ren", "kann", "auf", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVINF", "VMFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}