{"textgrid.poem.57308": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Grieche sang in lyrischem Ton B\u00fcrgergesetz.", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Grieche sang in lyrischem Ton B\u00fcrgergesetz.", "tokens": ["Der", "Grie\u00b7che", "sang", "in", "ly\u00b7ri\u00b7schem", "Ton", "B\u00fcr\u00b7ger\u00b7ge\u00b7setz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+--++--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Verwandter sind die Gesetze der Kunst dem lyrischen Ton;", "tokens": ["Ver\u00b7wand\u00b7ter", "sind", "die", "Ge\u00b7set\u00b7ze", "der", "Kunst", "dem", "ly\u00b7ri\u00b7schen", "Ton", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "So d\u00fcrfen wir ja auch wohl ein ernsteres Wort", "tokens": ["So", "d\u00fcr\u00b7fen", "wir", "ja", "auch", "wohl", "ein", "erns\u00b7te\u00b7res", "Wort"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "In die Tafel graben. Wir d\u00fcrfen nicht; aber wir thun's.", "tokens": ["In", "die", "Ta\u00b7fel", "gra\u00b7ben", ".", "Wir", "d\u00fcr\u00b7fen", "nicht", ";", "a\u00b7ber", "wir", "thun'", "s."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "abbreviation"], "pos": ["APPR", "ART", "NN", "VVINF", "$.", "PPER", "VMFIN", "PTKNEG", "$.", "KON", "PPER", "VVFIN", "NE"], "meter": "--+-+--+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Dichter, dem es noch nicht da sich entschleyerte,", "tokens": ["Der", "Dich\u00b7ter", ",", "dem", "es", "noch", "nicht", "da", "sich", "ent\u00b7schley\u00b7er\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "PTKNEG", "ADV", "PRF", "VVFIN", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Dass die Freude der edlen \u00f6fter schweigt,", "tokens": ["Dass", "die", "Freu\u00b7de", "der", "ed\u00b7len", "\u00f6f\u00b7ter", "schweigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Als selbst ihr m\u00e4chtigster Schmerz,", "tokens": ["Als", "selbst", "ihr", "m\u00e4ch\u00b7tigs\u00b7ter", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Der wanket schon an der Schwelle des Heiligthums.", "tokens": ["Der", "wan\u00b7ket", "schon", "an", "der", "Schwel\u00b7le", "des", "Hei\u00b7lig\u00b7thums", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Aber der unanstossendes Schrittes", "tokens": ["A\u00b7ber", "der", "un\u00b7ans\u00b7tos\u00b7sen\u00b7des", "Schrit\u00b7tes"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "In den Tempel trat der Kunst, diesem muss,", "tokens": ["In", "den", "Tem\u00b7pel", "trat", "der", "Kunst", ",", "die\u00b7sem", "muss", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,", "PDS", "VMFIN", "$,"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "F\u00fcr jede Kentniss, die dort zeiget, oder warnt,", "tokens": ["F\u00fcr", "je\u00b7de", "Kent\u00b7niss", ",", "die", "dort", "zei\u00b7get", ",", "o\u00b7der", "warnt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dennoch den Blick sch\u00e4rfen der Genius,", "tokens": ["Den\u00b7noch", "den", "Blick", "sch\u00e4r\u00b7fen", "der", "Ge\u00b7nius", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Bevor er lernt, was die edlen dann,", "tokens": ["Be\u00b7vor", "er", "lernt", ",", "was", "die", "ed\u00b7len", "dann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "PRELS", "ART", "ADJA", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wenn in Stimme sich nun ihr Verstummen wandelt,", "tokens": ["Wenn", "in", "Stim\u00b7me", "sich", "nun", "ihr", "Ver\u00b7stum\u00b7men", "wan\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "PRF", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Dann sagen, und welche Worte der Wahl sie w\u00fcrdigen,", "tokens": ["Dann", "sa\u00b7gen", ",", "und", "wel\u00b7che", "Wor\u00b7te", "der", "Wahl", "sie", "w\u00fcr\u00b7di\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "KON", "PWAT", "NN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+--+-+--+-+--", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wenn sich nun ihr Verstummen wandelt!", "tokens": ["Wenn", "sich", "nun", "ihr", "Ver\u00b7stum\u00b7men", "wan\u00b7delt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Bevor er geweiht, und, an der Hand", "tokens": ["Be\u00b7vor", "er", "ge\u00b7weiht", ",", "und", ",", "an", "der", "Hand"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVPP", "$,", "KON", "$,", "APPR", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Entdeckung, so tiefer Erfinder wird,", "tokens": ["Der", "Ent\u00b7de\u00b7ckung", ",", "so", "tie\u00b7fer", "Er\u00b7fin\u00b7der", "wird", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADJA", "NN", "VAFIN", "$,"], "meter": "-----+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dass zu seiner Saite Klang mit der vollen", "tokens": ["Dass", "zu", "sei\u00b7ner", "Sai\u00b7te", "Klang", "mit", "der", "vol\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "NN", "APPR", "ART", "ADJA"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Harmonie das Herz der H\u00f6renden klingt!", "tokens": ["Har\u00b7mo\u00b7nie", "das", "Herz", "der", "H\u00f6\u00b7ren\u00b7den", "klingt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.6": {"line.1": {"text": "Wenn je die Stirn der Kunst mit Ernste gebot,", "tokens": ["Wenn", "je", "die", "Stirn", "der", "Kunst", "mit", "Erns\u00b7te", "ge\u00b7bot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "So war es hier; sie gebot: Wie Raphael bildete, Gluck", "tokens": ["So", "war", "es", "hier", ";", "sie", "ge\u00b7bot", ":", "Wie", "Ra\u00b7phael", "bil\u00b7de\u00b7te", ",", "Gluck"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$.", "PPER", "VVFIN", "$.", "PWAV", "NE", "VVFIN", "$,", "NN"], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Mit dem Tone vereinte den Ton, so vollende der Dichter,", "tokens": ["Mit", "dem", "To\u00b7ne", "ver\u00b7ein\u00b7te", "den", "Ton", ",", "so", "voll\u00b7en\u00b7de", "der", "Dich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,", "ADV", "ADJA", "ART", "NN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Mehr noch, treffender noch, wenn es Freude gilt!", "tokens": ["Mehr", "noch", ",", "tref\u00b7fen\u00b7der", "noch", ",", "wenn", "es", "Freu\u00b7de", "gilt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADJA", "ADV", "$,", "KOUS", "PPER", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.7": {"line.1": {"text": "Freude, Freude, du Himmelskind!", "tokens": ["Freu\u00b7de", ",", "Freu\u00b7de", ",", "du", "Him\u00b7mel\u00b7skind", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PPER", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Danksagend k\u00fcsst er den Zauberstab,", "tokens": ["Dank\u00b7sa\u00b7gend", "k\u00fcsst", "er", "den", "Zau\u00b7ber\u00b7stab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Von dem, als du damit ihn ber\u00fchrtest,", "tokens": ["Von", "dem", ",", "als", "du", "da\u00b7mit", "ihn", "be\u00b7r\u00fchr\u00b7test", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "KOUS", "PPER", "PAV", "PPER", "VVFIN", "$,"], "meter": "--+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ein heiliger Funken ihm in die Seele sprang.", "tokens": ["Ein", "hei\u00b7li\u00b7ger", "Fun\u00b7ken", "ihm", "in", "die", "See\u00b7le", "sprang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.8": {"line.1": {"text": "Der Grieche sang in lyrischem Ton B\u00fcrgergesetz.", "tokens": ["Der", "Grie\u00b7che", "sang", "in", "ly\u00b7ri\u00b7schem", "Ton", "B\u00fcr\u00b7ger\u00b7ge\u00b7setz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+--++--+", "measure": "iambic.he