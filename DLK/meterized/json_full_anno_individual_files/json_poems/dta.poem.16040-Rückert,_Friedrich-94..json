{"dta.poem.16040": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "94.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mich freuts am Abend nicht, da\u00df mir manch Lied entsprungen;", "tokens": ["Mich", "freuts", "am", "A\u00b7bend", "nicht", ",", "da\u00df", "mir", "manch", "Lied", "ent\u00b7sprun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PTKNEG", "$,", "KOUS", "PPER", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mich freuts nur, wenn ich wei\u00df, da\u00df keines mir mislungen.", "tokens": ["Mich", "freuts", "nur", ",", "wenn", "ich", "wei\u00df", ",", "da\u00df", "kei\u00b7nes", "mir", "mis\u00b7lun\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,", "KOUS", "PIS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was thuts, wenn keins entsprang? doch wenn nur eins mislang,", "tokens": ["Was", "thuts", ",", "wenn", "keins", "ent\u00b7sprang", "?", "doch", "wenn", "nur", "eins", "mis\u00b7lang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "PIS", "VVFIN", "$.", "ADV", "KOUS", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit diesem mu\u00df ich dann mich plagen tagelang.", "tokens": ["Mit", "die\u00b7sem", "mu\u00df", "ich", "dann", "mich", "pla\u00b7gen", "ta\u00b7ge\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VMFIN", "PPER", "ADV", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich kann ihm nicht entziehn das Leben, ihm verliehn;", "tokens": ["Ich", "kann", "ihm", "nicht", "ent\u00b7ziehn", "das", "Le\u00b7ben", ",", "ihm", "ver\u00b7liehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "ART", "NN", "$,", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das misgeborne Kind, ich mu\u00df es doch erziehn.", "tokens": ["Das", "mis\u00b7ge\u00b7bor\u00b7ne", "Kind", ",", "ich", "mu\u00df", "es", "doch", "er\u00b7ziehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}