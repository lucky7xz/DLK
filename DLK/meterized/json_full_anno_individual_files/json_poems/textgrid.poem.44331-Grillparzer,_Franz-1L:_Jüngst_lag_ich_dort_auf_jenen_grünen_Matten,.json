{"textgrid.poem.44331": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: J\u00fcngst lag ich dort auf jenen gr\u00fcnen Matten,", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "J\u00fcngst lag ich dort auf jenen gr\u00fcnen Matten,", "tokens": ["J\u00fcngst", "lag", "ich", "dort", "auf", "je\u00b7nen", "gr\u00fc\u00b7nen", "Mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "die um der Donau Beet sich lachend ziehn,", "tokens": ["die", "um", "der", "Do\u00b7nau", "Beet", "sich", "la\u00b7chend", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NE", "NE", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "in einer Linde gastlich k\u00fchlem Schatten", "tokens": ["in", "ei\u00b7ner", "Lin\u00b7de", "gast\u00b7lich", "k\u00fch\u00b7lem", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NE", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und starrte in die weite Sch\u00f6pfung hin.", "tokens": ["und", "starr\u00b7te", "in", "die", "wei\u00b7te", "Sch\u00f6p\u00b7fung", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da warf ich auf zum Himmel meine H\u00e4nde,", "tokens": ["Da", "warf", "ich", "auf", "zum", "Him\u00b7mel", "mei\u00b7ne", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "ob sich denn nimmermehr mein Unstern wende,", "tokens": ["ob", "sich", "denn", "nim\u00b7mer\u00b7mehr", "mein", "Uns\u00b7tern", "wen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADV", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "und fluchte dem Geschick und seufzte tief,", "tokens": ["und", "fluch\u00b7te", "dem", "Ge\u00b7schick", "und", "seufz\u00b7te", "tief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "bis matt mein Aug sich schlo\u00df, und ich entschlief.", "tokens": ["bis", "matt", "mein", "Aug", "sich", "schlo\u00df", ",", "und", "ich", "ent\u00b7schlief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "PRF", "VVFIN", "$,", "KON", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da fand ich mich in eine \u00f6de W\u00fcste", "tokens": ["Da", "fand", "ich", "mich", "in", "ei\u00b7ne", "\u00f6\u00b7de", "W\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "hinabgeschleudert im verworrnen Traum,", "tokens": ["hin\u00b7ab\u00b7ge\u00b7schleu\u00b7dert", "im", "ver\u00b7worr\u00b7nen", "Traum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVF