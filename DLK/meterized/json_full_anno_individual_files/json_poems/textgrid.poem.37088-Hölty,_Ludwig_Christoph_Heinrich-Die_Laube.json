{"textgrid.poem.37088": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Laube", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nimmer, nimmer werd ich dein verge\u00dfen,", "tokens": ["Nim\u00b7mer", ",", "nim\u00b7mer", "werd", "ich", "dein", "ver\u00b7ge\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VAFIN", "PPER", "PPOSAT", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Laub', in deren Einsamkeit", "tokens": ["Laub'", ",", "in", "de\u00b7ren", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "APPR", "PRELAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine Laura, weiland, oft gese\u00dfen,", "tokens": ["Mei\u00b7ne", "Lau\u00b7ra", ",", "wei\u00b7land", ",", "oft", "ge\u00b7se\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "$,", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und des Fr\u00fchlings sich gefreut.", "tokens": ["Und", "des", "Fr\u00fch\u00b7lings", "sich", "ge\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PRF", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Schauer wird durch meine Nerven beben,", "tokens": ["Schau\u00b7er", "wird", "durch", "mei\u00b7ne", "Ner\u00b7ven", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Werd ich deine Bl\u00fcthen sehn,", "tokens": ["Werd", "ich", "dei\u00b7ne", "Bl\u00fc\u00b7then", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ihr Bildni\u00df mir entgegenschweben,", "tokens": ["Und", "ihr", "Bild\u00b7ni\u00df", "mir", "ent\u00b7ge\u00b7gen\u00b7schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ihre Gottheit mich umwehn.", "tokens": ["Ih\u00b7re", "Got\u00b7theit", "mich", "um\u00b7wehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Thr\u00e4nenvoll, werd ich, beym Mondenlichte,", "tokens": ["Thr\u00e4\u00b7nen\u00b7voll", ",", "werd", "ich", ",", "beym", "Mon\u00b7den\u00b7lich\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "VAFIN", "PPER", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In der Geisterstunde Graun,", "tokens": ["In", "der", "Geis\u00b7ter\u00b7stun\u00b7de", "Graun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dir entgegenzittern, und Gesichte", "tokens": ["Dir", "ent\u00b7ge\u00b7gen\u00b7zit\u00b7tern", ",", "und", "Ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVINF", "$,", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Auf Gesichte werd ich schaun.", "tokens": ["Auf", "Ge\u00b7sich\u00b7te", "werd", "ich", "schaun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Mich in manchen G\u00f6ttertraum verirren,", "tokens": ["Mich", "in", "man\u00b7chen", "G\u00f6t\u00b7ter\u00b7traum", "ver\u00b7ir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Bis Entz\u00fcckung mich durchbebt,", "tokens": ["Bis", "Ent\u00b7z\u00fc\u00b7ckung", "mich", "durch\u00b7bebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und nach meinem s\u00fc\u00dfen T\u00e4ubchen girren,", "tokens": ["Und", "nach", "mei\u00b7nem", "s\u00fc\u00b7\u00dfen", "T\u00e4ub\u00b7chen", "gir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "De\u00dfen Abbild vor mir schwebt.", "tokens": ["De\u00b7\u00dfen", "Ab\u00b7bild", "vor", "mir", "schwebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wenn ich auf der Bahn der Tugend wanke,", "tokens": ["Wenn", "ich", "auf", "der", "Bahn", "der", "Tu\u00b7gend", "wan\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Erdvergn\u00fcgen mich bestrickt;", "tokens": ["Erd\u00b7ver\u00b7