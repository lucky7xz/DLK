{"dta.poem.20047": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Andreas an Anna .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Liebes M\u00e4dchen! sahst du nicht wie gestern", "tokens": ["Lie\u00b7bes", "M\u00e4d\u00b7chen", "!", "sahst", "du", "nicht", "wie", "ge\u00b7stern"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "VVFIN", "PPER", "PTKNEG", "KOKOM", "ADV"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Ich auf hohem Berge lang gelegen,", "tokens": ["Ich", "auf", "ho\u00b7hem", "Ber\u00b7ge", "lang", "ge\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Blickend auf das wei\u00dfe Kreutz im Thale,", "tokens": ["Bli\u00b7ckend", "auf", "das", "wei\u00b7\u00dfe", "Kreutz", "im", "Tha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Das die Fl\u00fcgel deines Fensters bilden?", "tokens": ["Das", "die", "Fl\u00fc\u00b7gel", "dei\u00b7nes", "Fens\u00b7ters", "bil\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Glaubt' ich schon, du k\u00e4mst durch's Thal gewandelt,", "tokens": ["Glaubt'", "ich", "schon", ",", "du", "k\u00e4mst", "durch's", "Thal", "ge\u00b7wan\u00b7delt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PPER", "VVFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sprang ich auf, da war's ein wei\u00dfes Bl\u00fcmlein,", "tokens": ["Sprang", "ich", "auf", ",", "da", "wa\u00b7r's", "ein", "wei\u00b7\u00dfes", "Bl\u00fcm\u00b7lein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Das sich t\u00e4uschend mir vor's Auge stellte.", "tokens": ["Das", "sich", "t\u00e4u\u00b7schend", "mir", "vor's", "Au\u00b7ge", "stell\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "ADJD", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Lange harrt' ich, aber endlich breiten", "tokens": ["Lan\u00b7ge", "harrt'", "ich", ",", "a\u00b7ber", "end\u00b7lich", "brei\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "ADV", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Auseinander sich des Fensters Fl\u00fcgel,", "tokens": ["Aus\u00b7ein\u00b7an\u00b7der", "sich", "des", "Fens\u00b7ters", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und an seinem wei\u00dfen Kreutze stehst du,", "tokens": ["Und", "an", "sei\u00b7nem", "wei\u00b7\u00dfen", "Kreut\u00b7ze", "stehst", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Berg und Thal ein stiller Friedensengel.", "tokens": ["Berg", "und", "Thal", "ein", "stil\u00b7ler", "Frie\u00b7den\u00b7sen\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "V\u00f6glein ziehen nah' an dir vor\u00fcber,", "tokens": ["V\u00f6\u00b7glein", "zie\u00b7hen", "nah'", "an", "dir", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "T\u00e4ublein sitzen auf dem nahen Dache,", "tokens": ["T\u00e4ub\u00b7lein", "sit\u00b7zen", "auf", "dem", "na\u00b7hen", "Da\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Kommt der Mond, und kommen alle Sterne,", "tokens": ["Kommt", "der", "Mond", ",", "und", "kom\u00b7men", "al\u00b7le", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Blicken all' dir keck in's blaue Auge.", "tokens": ["Bli\u00b7cken", "all'", "dir", "keck", "in's", "blau\u00b7e", "Au\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "PPER", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Steh' ich einsam, einsam in der Ferne,", "tokens": ["Steh'", "ich", "ein\u00b7sam", ",", "ein\u00b7sam", "in", "der", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Habe keine Fl\u00fcgel hinzufliegen,", "tokens": ["Ha\u00b7be", "kei\u00b7ne", "Fl\u00fc\u00b7gel", "hin\u00b7zu\u00b7flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Habe keine Strahlen hinzusenden,", "tokens": ["Ha\u00b7be", "kei\u00b7ne", "Strah\u00b7len", "hin\u00b7zu\u00b7sen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Steh' ich einsam, einsam in der Ferne!", "tokens": ["Steh'", "ich", "ein\u00b7sam", ",", "ein\u00b7sam", "in", "der", "Fer\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Gehst du, sprech' ich mit verhaltnen Thr\u00e4nen:", "tokens": ["Gehst", "du", ",", "sprech'", "ich", "mit", "ver\u00b7halt\u00b7nen", "Thr\u00e4\u00b7nen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ruhet s\u00fc\u00df, ihr lieben, lieben Augen!", "tokens": ["Ru\u00b7het", "s\u00fc\u00df", ",", "ihr", "lie\u00b7ben", ",", "lie\u00b7ben", "Au\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "PPER", "VVINF", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ruhet s\u00fc\u00df, ihr wei\u00dfen, wei\u00dfen Lilgen!", "tokens": ["Ru\u00b7het", "s\u00fc\u00df", ",", "ihr", "wei\u00b7\u00dfen", ",", "wei\u00b7\u00dfen", "Lil\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ruhet s\u00fc\u00df, ihr lieben, lieben H\u00e4nde!", "tokens": ["Ru\u00b7het", "s\u00fc\u00df", ",", "ihr", "lie\u00b7ben", ",", "lie\u00b7ben", "H\u00e4n\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "PPER", "VVINF", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Sprachen's nach die Sterne an dem Himmel,", "tokens": ["Spra\u00b7chen's", "nach", "die", "Ster\u00b7ne", "an", "dem", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sprachen's nach die Blumen in dem Thale.", "tokens": ["Spra\u00b7chen's", "nach", "die", "Blu\u00b7men", "in", "dem", "Tha\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Weh! o weh! du hast es nicht vernommen!", "tokens": ["Weh", "!", "o", "weh", "!", "du", "hast", "es", "nicht", "ver\u00b7nom\u00b7men", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "FM", "PTKVZ", "$.", "PPER", "VAFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}