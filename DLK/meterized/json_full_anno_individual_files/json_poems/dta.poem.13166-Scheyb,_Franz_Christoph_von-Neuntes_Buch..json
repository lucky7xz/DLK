{"dta.poem.13166": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Neuntes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In dem Gedr\u00e4ng, Gemurr, Ger\u00e4usch, Gezisch, Gesumm", "tokens": ["In", "dem", "Ge\u00b7dr\u00e4ng", ",", "Ge\u00b7murr", ",", "Ge\u00b7r\u00e4usch", ",", "Ge\u00b7zisch", ",", "Ge\u00b7summ"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Lief jemand in dem Saal besch\u00e4fftiget herum,", "tokens": ["Lief", "je\u00b7mand", "in", "dem", "Saal", "be\u00b7sch\u00e4ff\u00b7ti\u00b7get", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "315Und zog die Vorh\u00e4ng' auf; er\u00f6ffnete dem Morgen,", "tokens": ["zog", "die", "Vor\u00b7h\u00e4ng'", "auf", ";", "er\u00b7\u00f6ff\u00b7ne\u00b7te", "dem", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$.", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Den die Beleuchtungen in ihrem Schein verborgen,", "tokens": ["Den", "die", "Be\u00b7leuch\u00b7tun\u00b7gen", "in", "ih\u00b7rem", "Schein", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.5": {"text": "Den Eingang in den Saal. Welch angenchmer Gru\u00df!", "tokens": ["Den", "Ein\u00b7gang", "in", "den", "Saal", ".", "Welch", "an\u00b7gench\u00b7mer", "Gru\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$.", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "So folgte zwar der Nacht, doch nicht der Freuden Schlu\u00df.", "tokens": ["So", "folg\u00b7te", "zwar", "der", "Nacht", ",", "doch", "nicht", "der", "Freu\u00b7den", "Schlu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$,", "ADV", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Man f\u00fchlte sich erquickt. Was erst in Lichtern prangte,", "tokens": ["Man", "f\u00fchl\u00b7te", "sich", "er\u00b7quickt", ".", "Was", "erst", "in", "Lich\u00b7tern", "prang\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "VVPP", "$.", "PWS", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "320War, was nun gr\u00f6\u00dfre Pracht vom Glanz des Tags erlangte.", "tokens": [",", "was", "nun", "gr\u00f6\u00df\u00b7re", "Pracht", "vom", "Glanz", "des", "Tags", "er\u00b7lang\u00b7te", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$,", "PRELS", "ADV", "ADJA", "NN", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.9": {"text": "Mein Aug entz\u00fcckte sich, da sich der Saal erhellt,", "tokens": ["Mein", "Aug", "ent\u00b7z\u00fcck\u00b7te", "sich", ",", "da", "sich", "der", "Saal", "er\u00b7hellt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "$,", "KOUS", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ich wei\u00df nicht, was er mir weit Edlers vorgestellt.", "tokens": ["Ich", "wei\u00df", "nicht", ",", "was", "er", "mir", "weit", "Ed\u00b7lers", "vor\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "PPER", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die Sonne brach herein; so schien es an den W\u00e4nden,", "tokens": ["Die", "Son\u00b7ne", "brach", "her\u00b7ein", ";", "so", "schien", "es", "an", "den", "W\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$.", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wie wann sie neu vergoldt, bepurpurt sich bef\u00e4nden.", "tokens": ["Wie", "wann", "sie", "neu", "ver\u00b7goldt", ",", "be\u00b7pur\u00b7purt", "sich", "be\u00b7f\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PWAV", "PPER", "ADJD", "VVPP", "$,", "VVFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}