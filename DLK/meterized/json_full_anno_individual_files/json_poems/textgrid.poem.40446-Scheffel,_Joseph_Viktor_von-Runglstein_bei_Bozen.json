{"textgrid.poem.40446": {"metadata": {"author": {"name": "Scheffel, Joseph Viktor von", "birth": "N.A.", "death": "N.A."}, "title": "Runglstein bei Bozen", "genre": "verse", "period": "N.A.", "pub_year": 1856, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch heute freut's mich, o Runglstein,", "tokens": ["Noch", "heu\u00b7te", "freut's", "mich", ",", "o", "Rungl\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$,", "FM", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da\u00df einstmals zu guter Stunden", "tokens": ["Da\u00df", "einst\u00b7mals", "zu", "gu\u00b7ter", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "In der Talfer felsenges Tal hinein", "tokens": ["In", "der", "Tal\u00b7fer", "fel\u00b7sen\u00b7ges", "Tal", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "APZR"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zu dir den Weg ich gefunden.", "tokens": ["Zu", "dir", "den", "Weg", "ich", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Melodisch scholl aus der Tiefe empor", "tokens": ["Me\u00b7lo\u00b7disch", "scholl", "aus", "der", "Tie\u00b7fe", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "APPR", "ART", "NN", "PTKVZ"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Des Wildbachs entstr\u00f6mendes Tosen,", "tokens": ["Des", "Wild\u00b7bachs", "ent\u00b7str\u00f6\u00b7men\u00b7des", "To\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Am Burgpfad erbl\u00fchten in lustigem Chor", "tokens": ["Am", "Burg\u00b7pfad", "er\u00b7bl\u00fch\u00b7ten", "in", "lus\u00b7ti\u00b7gem", "Chor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Glutnelken und wilde Rosen.", "tokens": ["Glut\u00b7nel\u00b7ken", "und", "wil\u00b7de", "Ro\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Des Runglsteins verfallen Geb\u00e4u", "tokens": ["Des", "Rungl\u00b7steins", "ver\u00b7fal\u00b7len", "Ge\u00b7b\u00e4u"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Wei\u00df nichts von Gr\u00e4men und Trauern,", "tokens": ["Wei\u00df", "nichts", "von", "Gr\u00e4\u00b7men", "und", "Trau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der Geist der Dichtung, fr\u00f6hlich und frei,", "tokens": ["Der", "Geist", "der", "Dich\u00b7tung", ",", "fr\u00f6h\u00b7lich", "und", "frei", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Nistet in seinen Mauern.", "tokens": ["Nis\u00b7tet", "in", "sei\u00b7nen", "Mau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Herr ", "tokens": ["Herr"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Des Kurzweil war allerwegen", "tokens": ["Des", "Kurz\u00b7weil", "war", "al\u00b7ler\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Beim Klang der Laute und Stengelglas", "tokens": ["Beim", "Klang", "der", "Lau\u00b7te", "und", "Sten\u00b7gel\u00b7glas"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "KON", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der freien K\u00fcnste zu pflegen.", "tokens": ["Der", "frei\u00b7en", "K\u00fcns\u00b7te", "zu", "pfle\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "L\u00e4ngst war des Minnelieds Glanz vorbei", "tokens": ["L\u00e4ngst", "war", "des", "Min\u00b7ne\u00b7lieds", "Glanz", "vor\u00b7bei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "PTKVZ"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und anderes wollt' sich gestalten,", "tokens": ["Und", "an\u00b7de\u00b7res", "wollt'", "sich", "ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "PRF", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Drum dacht' er, ein k\u00fcnstlerisch Konterfei", "tokens": ["Drum", "dacht'", "er", ",", "ein", "k\u00fcnst\u00b7le\u00b7risch", "Kon\u00b7ter\u00b7fei"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "$,", "ART", "ADJD", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Entschwundener Pracht zu behalten.", "tokens": ["Ent\u00b7schwun\u00b7de\u00b7ner", "Pracht", "zu", "be\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.6": {"line.1": {"text": "Viel sinnige M\u00e4nner malten ihm gern", "tokens": ["Viel", "sin\u00b7ni\u00b7ge", "M\u00e4n\u00b7ner", "mal\u00b7ten", "ihm", "gern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Helden der altdeutschen Lieder;", "tokens": ["Die", "Hel\u00b7den", "der", "alt\u00b7deut\u00b7schen", "Lie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Noch schauen Herr Hagen und Dietrich von Bern", "tokens": ["Noch", "schau\u00b7en", "Herr", "Ha\u00b7gen", "und", "Diet\u00b7rich", "von", "Bern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "NE", "KON", "NE", "APPR", "NE"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Vom S\u00f6ller zum Burghof hernieder.", "tokens": ["Vom", "S\u00f6l\u00b7ler", "zum", "Burg\u00b7hof", "her\u00b7nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.7": {"line.1": {"text": "Und Grau in Grau \u2013 dort den Saal entlang,", "tokens": ["Und", "Grau", "in", "Grau", "\u2013", "dort", "den", "Saal", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "$(", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wer deutet die Gruppen, die holden?", "tokens": ["Wer", "deu\u00b7tet", "die", "Grup\u00b7pen", ",", "die", "hol\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$,", "PRELS", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "'s ist Gottfrieds von Stra\u00dfburg minniger Sang", "tokens": ["'s", "ist", "Gott\u00b7frieds", "von", "Stra\u00df\u00b7burg", "min\u00b7ni\u00b7ger", "Sang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NE", "APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Von Tristan und Isolden.", "tokens": ["Von", "Tris\u00b7tan", "und", "I\u00b7sol\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Tristan und Isolde auf weitem Meer \u2013", "tokens": ["Tris\u00b7tan", "und", "I\u00b7sol\u00b7de", "auf", "wei\u00b7tem", "Meer", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "APPR", "ADJA", "NN", "$("], "meter": "+--+---+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Isolde und Tristan im Walde \u2013", "tokens": ["I\u00b7sol\u00b7de", "und", "Tris\u00b7tan", "im", "Wal\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "APPRART", "NN", "$("], "meter": "++--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Brang\u00e4ne l\u00e4chelt \u2013 betr\u00fcblich sehr", "tokens": ["Bran\u00b7g\u00e4\u00b7ne", "l\u00e4\u00b7chelt", "\u2013", "be\u00b7tr\u00fcb\u00b7lich", "sehr"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "$(", "ADJD", "ADV"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Steht K\u00f6nig Marke der Alte ...", "tokens": ["Steht", "K\u00f6\u00b7nig", "Mar\u00b7ke", "der", "Al\u00b7te", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "NE", "ART", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Noch heute freut's mich, o Runglstein,", "tokens": ["Noch", "heu\u00b7te", "freut's", "mich", ",", "o", "Rungl\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$,", "FM", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da\u00df einstmals zu guter Stunden", "tokens": ["Da\u00df", "einst\u00b7mals", "zu", "gu\u00b7ter", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "In der Talfer felsenges Tal hinein", "tokens": ["In", "der", "Tal\u00b7fer", "fel\u00b7sen\u00b7ges", "Tal", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "APZR"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zu dir den Weg ich gefunden.", "tokens": ["Zu", "dir", "den", "Weg", "ich", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "Durch der Fenster farbige Scheiben entsandt'", "tokens": ["Durch", "der", "Fens\u00b7ter", "far\u00b7bi\u00b7ge", "Schei\u00b7ben", "ent\u00b7sandt'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Sonne ihr Gold vor dem Scheiden;", "tokens": ["Die", "Son\u00b7ne", "ihr", "Gold", "vor", "dem", "Schei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Es umflammte die Schildereien der Wand", "tokens": ["Es", "um\u00b7flamm\u00b7te", "die", "Schil\u00b7de\u00b7rei\u00b7en", "der", "Wand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Wie ein Gru\u00df vergehender Zeiten.", "tokens": ["Wie", "ein", "Gru\u00df", "ver\u00b7ge\u00b7hen\u00b7der", "Zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.11": {"line.1": {"text": "Im Rittersaale am hohen Kamin", "tokens": ["Im", "Rit\u00b7ter\u00b7saa\u00b7le", "am", "ho\u00b7hen", "Ka\u00b7min"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sa\u00df lang' ich, in Sinnen versunken,", "tokens": ["Sa\u00df", "lang'", "ich", ",", "in", "Sin\u00b7nen", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "$,", "APPR", "NN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und ha