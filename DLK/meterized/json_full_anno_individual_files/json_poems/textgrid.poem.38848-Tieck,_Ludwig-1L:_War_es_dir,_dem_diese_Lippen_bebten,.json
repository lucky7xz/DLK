{"textgrid.poem.38848": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: War es dir, dem diese Lippen bebten,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "War es dir, dem diese Lippen bebten,", "tokens": ["War", "es", "dir", ",", "dem", "die\u00b7se", "Lip\u00b7pen", "beb\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "$,", "PRELS", "PDAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dir der dargebotne s\u00fc\u00dfe Ku\u00df?", "tokens": ["Dir", "der", "dar\u00b7ge\u00b7bot\u00b7ne", "s\u00fc\u00b7\u00dfe", "Ku\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Giebt ein irdisch Leben so Genu\u00df?", "tokens": ["Giebt", "ein", "ir\u00b7disch", "Le\u00b7ben", "so", "Ge\u00b7nu\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJD", "NN", "ADV", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ha! wie Licht und Glanz vor meinen Augen schwebten,", "tokens": ["Ha", "!", "wie", "Licht", "und", "Glanz", "vor", "mei\u00b7nen", "Au\u00b7gen", "schweb\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Alle Sinne nach den Lippen strebten!", "tokens": ["Al\u00b7le", "Sin\u00b7ne", "nach", "den", "Lip\u00b7pen", "streb\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "