{"textgrid.poem.39636": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die lange Schriftsteller-Laufbahn", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Arndts Paradiesg\u00e4rtlein, voll geistlicher Beschauung,", "tokens": ["Arndts", "Pa\u00b7ra\u00b7dies\u00b7g\u00e4rt\u00b7lein", ",", "voll", "geist\u00b7li\u00b7cher", "Be\u00b7schau\u00b7ung", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dient ein Jahrhundert schon den Frommen zur Erbauung.", "tokens": ["Dient", "ein", "Jahr\u00b7hun\u00b7dert", "schon", "den", "From\u00b7men", "zur", "Er\u00b7bau\u00b7ung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-++-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.3": {"text": "Der Autor mu\u00df nun wohl bei hohen Jahren sein,", "tokens": ["Der", "Au\u00b7tor", "mu\u00df", "nun", "wohl", "bei", "ho\u00b7hen", "Jah\u00b7ren", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ADV", "APPR", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und dennoch stellt er nicht das B\u00fccherschreiben ein:", "tokens": ["Und", "den\u00b7noch", "stellt", "er", "nicht", "das", "B\u00fc\u00b7cher\u00b7schrei\u00b7ben", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jetzt schreibt er, nicht, wie sonst, vom wahren Christenthume,", "tokens": ["Jetzt", "schreibt", "er", ",", "nicht", ",", "wie", "sonst", ",", "vom", "wah\u00b7ren", "Chris\u00b7ten\u00b7thu\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PTKNEG", "$,", "PWAV", "ADV", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch breit, postillenhaft, vom wahren Deutschenthume.", "tokens": ["Doch", "breit", ",", "pos\u00b7til\u00b7len\u00b7haft", ",", "vom", "wah\u00b7ren", "Deut\u00b7schen\u00b7thu\u00b7me", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADJD", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Arndts Paradiesg\u00e4rtlein, voll geistlicher Beschauung,", "tokens": ["Arndts", "Pa\u00b7ra\u00b7dies\u00b7g\u00e4rt\u00b7lein", ",", "voll", "geist\u00b7li\u00b7cher", "Be\u00b7schau\u00b7ung", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dient ein Jahrhundert schon den Frommen zur Erbauung.", "tokens": ["Dient", "ein", "Jahr\u00b7hun\u00b7dert", "schon", "den", "From\u00b7men", "zur", "Er\u00b7bau\u00b7ung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-++-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.3": {"text": "Der Autor mu\u00df nun wohl bei hohen Jahren sein,", "tokens": ["Der", "Au\u00b7tor", "mu\u00df", "nun", "wohl", "bei", "ho\u00b7hen", "Jah