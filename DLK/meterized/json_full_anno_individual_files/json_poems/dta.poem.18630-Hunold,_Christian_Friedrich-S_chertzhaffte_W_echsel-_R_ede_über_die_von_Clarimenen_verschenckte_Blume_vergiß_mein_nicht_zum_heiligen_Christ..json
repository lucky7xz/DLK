{"dta.poem.18630": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "S chertzhaffte  W echsel- R ede  \n \u00fcber die von Clarimenen verschenckte  \n Blume vergi\u00df mein nicht zum  \n heiligen Christ.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Vergi\u00df mein nicht/", "tokens": ["Ver\u00b7gi\u00df", "mein", "nicht", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "PTKNEG", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Das lasse dir/ mein Leben/", "tokens": ["Das", "las\u00b7se", "dir", "/", "mein", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$(", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Stets in Gedancken schweben/", "tokens": ["Stets", "in", "Ge\u00b7dan\u00b7cken", "schwe\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es ist ja deine Pflicht:", "tokens": ["Es", "ist", "ja", "dei\u00b7ne", "Pflicht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Blumen hast du sonst mit gr\u00f6ster Lust genesen/", "tokens": ["Die", "Blu\u00b7men", "hast", "du", "sonst", "mit", "gr\u00f6s\u00b7ter", "Lust", "ge\u00b7ne\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Drum la\u00df auch diese nicht in deiner Brust verwesen/", "tokens": ["Drum", "la\u00df", "auch", "die\u00b7se", "nicht", "in", "dei\u00b7ner", "Brust", "ver\u00b7we\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "PDS", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Vergi\u00df mein nicht.", "tokens": ["Ver\u00b7gi\u00df", "mein", "nicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Mein wehrtes Licht!", "tokens": ["Mein", "wehr\u00b7tes", "Licht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Ich habe mich vermessen/", "tokens": ["Ich", "ha\u00b7be", "mich", "ver\u00b7mes\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dich niemahls zu vergessen/", "tokens": ["Dich", "nie\u00b7mahls", "zu", "ver\u00b7ges\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bi\u00df mir das Leben bricht:", "tokens": ["Bi\u00df", "mir", "das", "Le\u00b7ben", "bricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Blume bleibet wohl in meiner Brust vergraben/", "tokens": ["Die", "Blu\u00b7me", "blei\u00b7bet", "wohl", "in", "mei\u00b7ner", "Brust", "ver\u00b7gra\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und du wirst allezeit den liebsten Nahmen haben", "tokens": ["Und", "du", "wirst", "al\u00b7le\u00b7zeit", "den", "liebs\u00b7ten", "Nah\u00b7men", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ART", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mein wehrtes Licht.", "tokens": ["Mein", "wehr\u00b7tes", "Licht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Vergi\u00df mein nicht", "tokens": ["Ver\u00b7gi\u00df", "mein", "nicht"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "PTKNEG"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Soll mich zwar auch vergn\u00fcgen/", "tokens": ["Soll", "mich", "zwar", "auch", "ver\u00b7gn\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn sie/ dich zu besiegen/", "tokens": ["Wenn", "sie", "/", "dich", "zu", "be\u00b7sie\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$(", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Si ch um dein Hertze flicht.", "tokens": ["Si", "ch", "um", "dein", "Hert\u00b7ze", "flicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "PPOSAT", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Allein die Flora will durch diese Kostbarkeiten/", "tokens": ["Al\u00b7lein", "die", "Flo\u00b7ra", "will", "durch", "die\u00b7se", "Kost\u00b7bar\u00b7kei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VMFIN", "APPR", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mein Kind/ errath es doch/ was sonderlichs bedeuten/", "tokens": ["Mein", "Kind", "/", "er\u00b7rath", "es", "doch", "/", "was", "son\u00b7der\u00b7lichs", "be\u00b7deu\u00b7ten", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "VVFIN", "PPER", "ADV", "$(", "PWS", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Vergi\u00df mein nicht.", "tokens": ["Ver\u00b7gi\u00df", "mein", "nicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Mein werthes Licht!", "tokens": ["Mein", "wert\u00b7hes", "Licht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Was sollen diese Worte", "tokens": ["Was", "sol\u00b7len", "die\u00b7se", "Wor\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Aus deiner Zucker Pforte?", "tokens": ["Aus", "dei\u00b7ner", "Zu\u00b7cker", "Pfor\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er\u00f6ffne dein Gesicht/", "tokens": ["Er\u00b7\u00f6ff\u00b7ne", "dein", "Ge\u00b7sicht", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ach nimm die Larve weg von deinen Purpur Wangen/", "tokens": ["Ach", "nimm", "die", "Lar\u00b7ve", "weg", "von", "dei\u00b7nen", "Pur\u00b7pur", "Wan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVIMP", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vertraue mir doch nur dein eintziges Verlangen/", "tokens": ["Ver\u00b7trau\u00b7e", "mir", "doch", "nur", "dein", "eint\u00b7zi\u00b7ges", "Ver\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mein wehrtes Licht.", "tokens": ["Mein", "wehr\u00b7tes", "Licht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Vergi\u00df mein nicht", "tokens": ["Ver\u00b7gi\u00df", "mein", "nicht"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "PTKNEG"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Das pfleget man in Lieben", "tokens": ["Das", "pfle\u00b7get", "man", "in", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PIS", "APPR", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Fast jeden Tag zu \u00fcben/", "tokens": ["Fast", "je\u00b7den", "Tag", "zu", "\u00fc\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie da\u00df es dir gebricht?", "tokens": ["Wie", "da\u00df", "es", "dir", "ge\u00b7bricht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Des Frauenzimmers Gunst wird sonsten bald erkalten/", "tokens": ["Des", "Frau\u00b7en\u00b7zim\u00b7mers", "Gunst", "wird", "sons\u00b7ten", "bald", "er\u00b7kal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn sich der Courtisan die Thaler l\u00e4st veralten.", "tokens": ["Wenn", "sich", "der", "Cour\u00b7ti\u00b7san", "die", "Tha\u00b7ler", "l\u00e4st", "ver\u00b7al\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "ART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Vergi\u00df mein nicht.", "tokens": ["Ver\u00b7gi\u00df", "mein", "nicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Mein werthes Licht!", "tokens": ["Mein", "wert\u00b7hes", "Licht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Was sol ich doch ersinnen/", "tokens": ["Was", "sol", "ich", "doch", "er\u00b7sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dich v\u00f6llig zu gewinnen/", "tokens": ["Dich", "v\u00f6l\u00b7lig", "zu", "ge\u00b7win\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erkl\u00e4re die Geschicht.", "tokens": ["Er\u00b7kl\u00e4\u00b7re", "die", "Ge\u00b7schicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wenn deine Gunst erstirbt/ so wird mein Geist erblassen/", "tokens": ["Wenn", "dei\u00b7ne", "Gunst", "er\u00b7stirbt", "/", "so", "wird", "mein", "Geist", "er\u00b7blas\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$(", "ADV", "VAFIN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das Grab der Liebe wil schon meinen Leib \u00fcmfassen/", "tokens": ["Das", "Grab", "der", "Lie\u00b7be", "wil", "schon", "mei\u00b7nen", "Leib", "\u00fcm\u00b7fas\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VMFIN", "ADV", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mein werthes Licht!", "tokens": ["Mein", "wert\u00b7hes", "Licht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Vergi\u00df mein nicht", "tokens": ["Ver\u00b7gi\u00df", "mein", "nicht"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "PTKNEG"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Kennt keine Schmeicheleyen/", "tokens": ["Kennt", "kei\u00b7ne", "Schmei\u00b7che\u00b7le\u00b7yen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Und mich kan nichts erfreuen/", "tokens": ["Und", "mich", "kan", "nichts", "er\u00b7freu\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PIS", "VVINF", "$("], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Als was die That verspricht.", "tokens": ["Als", "was", "die", "That", "ver\u00b7spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Grillen helffen nichts/ lauff hin zu einer Buden/", "tokens": ["Die", "Gril\u00b7len", "helf\u00b7fen", "nichts", "/", "lauff", "hin", "zu", "ei\u00b7ner", "Bu\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "$(", "VVIMP", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und kauffe nur allein bey einen reichen Juden", "tokens": ["Und", "kauf\u00b7fe", "nur", "al\u00b7lein", "bey", "ei\u00b7nen", "rei\u00b7chen", "Ju\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Vergi\u00df mein nicht.", "tokens": ["Ver\u00b7gi\u00df", "mein", "nicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Mein werthes Licht!", "tokens": ["Mein", "wert\u00b7hes", "Licht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Ich kan bey diesen Lauffen", "tokens": ["Ich", "kan", "bey", "die\u00b7sen", "Lauf\u0