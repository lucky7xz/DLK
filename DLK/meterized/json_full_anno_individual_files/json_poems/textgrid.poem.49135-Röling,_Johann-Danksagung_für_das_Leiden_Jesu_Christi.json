{"textgrid.poem.49135": {"metadata": {"author": {"name": "R\u00f6ling, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Danksagung f\u00fcr das Leiden Jesu Christi", "genre": "verse", "period": "N.A.", "pub_year": 1656, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was soll ich, liebster Jesu, dir", "tokens": ["Was", "soll", "ich", ",", "liebs\u00b7ter", "Je\u00b7su", ",", "dir"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PWS", "VMFIN", "PPER", "$,", "ADJA", "NE", "$,", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Heyl und Trost, mein H\u00fclff' und Zier,", "tokens": ["Mein", "Heyl", "und", "Trost", ",", "mein", "H\u00fclff'", "und", "Zier", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$,", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr dein beschwerlichs Leben,", "tokens": ["F\u00fcr", "dein", "be\u00b7schwer\u00b7lichs", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr all dein Leiden, M\u00fch' und Noth,", "tokens": ["F\u00fcr", "all", "dein", "Lei\u00b7den", ",", "M\u00fch'", "und", "Noth", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "F\u00fcr deine Wunden, Blut und Todt", "tokens": ["F\u00fcr", "dei\u00b7ne", "Wun\u00b7den", ",", "Blut", "und", "Todt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "F\u00fcr Dank und Ehre geben?", "tokens": ["F\u00fcr", "Dank", "und", "Eh\u00b7re", "ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Geb' ich hief\u00fcr dir all das Mein',", "tokens": ["Geb'", "ich", "hie\u00b7f\u00fcr", "dir", "all", "das", "Mein'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PAV", "PPER", "PIAT", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Was kan das f\u00fcr Vergeltung seyn?", "tokens": ["Was", "kan", "das", "f\u00fcr", "Ver\u00b7gel\u00b7tung", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PDS", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du hast mich, was ich bin, gemacht,", "tokens": ["Du", "hast", "mich", ",", "was", "ich", "bin", ",", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "PWS", "PPER", "VAFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du hast mich selbst zu recht gebracht,", "tokens": ["Du", "hast", "mich", "selbst", "zu", "recht", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PTKA", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie ich dir abgestorben,", "tokens": ["Wie", "ich", "dir", "ab\u00b7ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du hast all meine Schuld geb\u00fcst", "tokens": ["Du", "hast", "all", "mei\u00b7ne", "Schuld", "ge\u00b7b\u00fcst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und da ich ewig leiden m\u00fcst,", "tokens": ["Und", "da", "ich", "e\u00b7wig", "lei\u00b7den", "m\u00fcst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Himmel mir erworben.", "tokens": ["Den", "Him\u00b7mel", "mir", "er\u00b7wor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Geb' ich hief\u00fcr dir all das Mein',", "tokens": ["Geb'", "ich", "hie\u00b7f\u00fcr", "dir", "all", "das", "Mein'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PAV", "PPER", "PIAT", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Was kan das f\u00fcr Vergeltung seyn?", "tokens": ["Was", "kan", "das", "f\u00fcr", "Ver\u00b7gel\u00b7tung", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PDS", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich baue noch die Nichtigkeit", "tokens": ["Ich", "bau\u00b7e", "noch", "die", "Nich\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und bin von deinem Throne weit,", "tokens": ["Und", "bin", "von", "dei\u00b7nem", "Thro\u00b7ne", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auff den du bist gestiegen;", "tokens": ["Auff", "den", "du", "bist", "ge\u00b7stie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPER", "VAFIN", "VVPP", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Du bist nun wieder Herr der Welt,", "tokens": ["Du", "bist", "nun", "wie\u00b7der", "Herr", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die mir so schwer und \u00e4ngstlich f\u00e4llt,", "tokens": ["Die", "mir", "so", "schwer", "und", "\u00e4ngst\u00b7lich", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df ich fast mu\u00df erliegen.", "tokens": ["Da\u00df", "ich", "fast", "mu\u00df", "er\u00b7lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Geb' ich hiebey dir all das Mein,", "tokens": ["Geb'", "ich", "hie\u00b7bey", "dir", "all", "das", "Mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PPER", "PIAT", "PDS", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Was kan das f\u00fcr Vergeltung seyn?", "tokens": ["Was", "kan", "das", "f\u00fcr", "Ver\u00b7gel\u00b7tung", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PDS", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Nimm doch es, o mein Heyland, an,", "tokens": ["Nimm", "doch", "es", ",", "o", "mein", "Hey\u00b7land", ",", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "KON", "PPER", "$,", "FM", "PPOSAT", "NN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weil ich nichts Be\u00dfres geben kan,", "tokens": ["Weil", "ich", "nichts", "Be\u00df\u00b7res", "ge\u00b7ben", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Bi\u00df da\u00df ich frey der Erden.", "tokens": ["Bi\u00df", "da\u00df", "ich", "frey", "der", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ach, sch\u00f6ne Lust, ach, s\u00fc\u00dfe Freud,", "tokens": ["Ach", ",", "sch\u00f6\u00b7ne", "Lust", ",", "ach", ",", "s\u00fc\u00b7\u00dfe", "Freud", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJA", "NN", "$,", "ITJ", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da du und ich, wir, Jesu, beyd", "tokens": ["Da", "du", "und", "ich", ",", "wir", ",", "Je\u00b7su", ",", "beyd"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["KOUS", "PPER", "KON", "PPER", "$,", "PPER", "$,", "NE", "$,", "XY"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zusammen kommen werden;", "tokens": ["Zu\u00b7sam\u00b7men", "kom\u00b7men", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Denn werd' ich, Herr, und all das Mein'", "tokens": ["Denn", "werd'", "ich", ",", "Herr", ",", "und", "all", "das", "Mein'"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "$,", "NN", "$,", "KON", "PIAT", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Geschickter zur Vergeltung seyn.", "tokens": ["Ge\u00b7schick\u00b7ter", "zur", "Ver\u00b7gel\u00b7tung", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "F\u00fchr' aber du mich, o mein Hort,", "tokens": ["F\u00fchr'", "a\u00b7ber", "du", "mich", ",", "o", "mein", "Hort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PPER", "PRF", "$,", "FM", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hieher in deiner Unschuld fort", "tokens": ["Hie\u00b7her", "in", "dei\u00b7ner", "Un\u00b7schuld", "fort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und la\u00df mich unterde\u00dfen", "tokens": ["Und", "la\u00df", "mich", "un\u00b7ter\u00b7de\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nie deines Leidens, deiner Pein", "tokens": ["Nie", "dei\u00b7nes", "Lei\u00b7dens", ",", "dei\u00b7ner", "Pein"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und was wir hief\u00fcr schuldig seyn,", "tokens": ["Und", "was", "wir", "hie\u00b7f\u00fcr", "schul\u00b7dig", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PAV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nie, Jesu, nie verge\u00dfen,", "tokens": ["Nie", ",", "Je\u00b7su", ",", "nie", "ver\u00b7ge\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "NE", "$,", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "So werd' ich dir und all das Mein'", "tokens": ["So", "werd'", "ich", "dir", "und", "all", "das", "Mein'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "KON", "PIAT", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Gef\u00e4llig zur Vergeltung seyn.", "tokens": ["Ge\u00b7f\u00e4l\u00b7lig", "zur", "Ver\u00b7gel\u00b7tung", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Gef\u00e4llt es dir denn auch, mein Heyl,", "tokens": ["Ge\u00b7f\u00e4llt", "es", "dir", "denn", "auch", ",", "mein", "Heyl", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ich von deinem Kreutz ein Theil", "tokens": ["Da\u00df", "ich", "von", "dei\u00b7nem", "Kreutz", "ein", "Theil"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "ART", "NN"], "meter":