{"dta.poem.9819": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Gespr\u00e4ch  \n Der sterbenden Belise/ und ihres sie  \n beklagende Lysis.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Belise starb/ und sprach im scheiden:", "tokens": ["Be\u00b7li\u00b7se", "starb", "/", "und", "sprach", "im", "schei\u00b7den", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$(", "KON", "VVFIN", "APPRART", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nun/ Lysis/ nun verla\u00df ich dich!", "tokens": ["Nun", "/", "Ly\u00b7sis", "/", "nun", "ver\u00b7la\u00df", "ich", "dich", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "NE", "$(", "ADV", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich st\u00fcrbe willig und mit freuden/", "tokens": ["Ich", "st\u00fcr\u00b7be", "wil\u00b7lig", "und", "mit", "freu\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "APPR", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Liebt eine dich so sehr als ich.", "tokens": ["Liebt", "ei\u00b7ne", "dich", "so", "sehr", "als", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "PPER", "ADV", "ADV", "KOUS", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach! sprach er/ mag dich das betr\u00fcben/", "tokens": ["Ach", "!", "sprach", "er", "/", "mag", "dich", "das", "be\u00b7tr\u00fc\u00b7ben", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "$(", "VMFIN", "PRF", "PDS", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Belise? nur dein tod ist schwer!", "tokens": ["Be\u00b7li\u00b7se", "?", "nur", "dein", "tod", "ist", "schwer", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Kanst du mich selbst nicht l\u00e4nger lieben/", "tokens": ["Kanst", "du", "mich", "selbst", "nicht", "l\u00e4n\u00b7ger", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "PTKNEG", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Bedarff ich keiner liebe mehr.", "tokens": ["Be\u00b7darff", "ich", "kei\u00b7ner", "lie\u00b7be", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PIS", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}