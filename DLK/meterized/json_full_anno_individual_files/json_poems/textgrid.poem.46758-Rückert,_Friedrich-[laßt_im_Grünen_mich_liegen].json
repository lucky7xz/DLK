rd", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "PPER", "VVFIN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Gleich den L\u00e4mmern und Ziegen,", "tokens": ["Gleich", "den", "L\u00e4m\u00b7mern", "und", "Zie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Unter Blumen und Klee!", "tokens": ["Un\u00b7ter", "Blu\u00b7men", "und", "Klee", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Unterm Klee und den Blumen,", "tokens": ["Un\u00b7term", "Klee", "und", "den", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Wo erm\u00fcdet zu fr\u00fch", "tokens": ["Wo", "er\u00b7m\u00fc\u00b7det", "zu", "fr\u00fch"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PTKA", "ADJD"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ihr ins Bettchen gestiegen,", "tokens": ["Ihr", "ins", "Bett\u00b7chen", "ge\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Unter Blumen und Klee!", "tokens": ["Un\u00b7ter", "Blu\u00b7men", "und", "Klee", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.6": {"line.1": {"text": "H\u00f6r' ich drinnen das Athmen", "tokens": ["H\u00f6r'", "ich", "drin\u00b7nen", "das", "Ath\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Ihres Schlummers? o Luft,", "tokens": ["Ih\u00b7res", "Schlum\u00b7mers", "?", "o", "Luft", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "FM", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Will dein Hauch mich betr\u00fcgen", "tokens": ["Will", "dein", "Hauch", "mich", "be\u00b7tr\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "PPER", "VVFIN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Unter Blumen und Klee?", "tokens": ["Un\u00b7ter", "Blu\u00b7men", "und", "Klee", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "Quillst du, F\u00fclle des Fr\u00fchlings,", "tokens": ["Quillst", "du", ",", "F\u00fcl\u00b7le", "des", "Fr\u00fch\u00b7lings", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Oder bl\u00e4hen sich so", "tokens": ["O\u00b7der", "bl\u00e4\u00b7hen", "sich", "so"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "ADV"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ihre schwellenden Wiegen", "tokens": ["Ih\u00b7re", "schwel\u00b7len\u00b7den", "Wie\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Unter Blumen und Klee?", "tokens": ["Un\u00b7ter", "Blu\u00b7men", "und", "Klee", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.8": {"line.1": {"text": "Ja, ich f\u00fchle das Pochen", "tokens": ["Ja", ",", "ich", "f\u00fch\u00b7le", "das", "Po\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Eurer Herzen hindurch,", "tokens": ["Eu\u00b7rer", "Her\u00b7zen", "hin\u00b7durch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PAV", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wo ihr lauschet verschwiegen", "tokens": ["Wo", "ihr", "lau\u00b7schet", "ver\u00b7schwie\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "VVINF"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Unter Blumen und Klee!", "tokens": ["Un\u00b7ter", "Blu\u00b7men", "und", "Klee", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.9": {"line.1": {"text": "Ihr wollt schlafen, so schlafet!", "tokens": ["Ihr", "wollt", "schla\u00b7fen", ",", "so", "schla\u00b7fet", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "ADV", "VVFIN", "$."], "meter": "-+---+-", "measure": "dactylic.init"}, "line.2": {"text": "Wie ich immer euch gern", "tokens": ["Wie", "ich", "im\u00b7mer", "euch", "gern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "PPER", "ADV"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wiegte, will ich euch wiegen", "tokens": ["Wieg\u00b7te", ",", "will", "ich", "euch", "wie\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VMFIN", "PPER", "PPER", "VVFIN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Unter Blumen und Klee!", "tokens": ["Un\u00b7ter", "Blu\u00b7men", "und", "Klee", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.10": {"line.1": {"text": "Schlaft! es m\u00fcsse vom Seufzer", "tokens": ["Schlaft", "!", "es", "m\u00fcs\u00b7se", "vom", "Seuf\u00b7zer"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "PPER", "VMFIN", "APPRART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Kein zu st\u00fcrmischer Hauch", "tokens": ["Kein", "zu", "st\u00fcr\u00b7mi\u00b7scher", "Hauch"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "APPR", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Euern Frieden bekriegen", "tokens": ["Eu\u00b7ern", "Frie\u00b7den", "be\u00b7krie\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVINF"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Unter Blumen und Klee!", "tokens": ["Un\u00b7ter", "Blu\u00b7men", "und", "Klee", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.11": {"line.1": {"text": "Und die leisere Thr\u00e4ne", "tokens": ["Und", "die", "lei\u00b7se\u00b7re", "Thr\u00e4\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Rinn' am Halmen hinab,", "tokens": ["Rinn'", "am", "Hal\u00b7men", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ohn' ein H\u00e4lmchen zu biegen", "tokens": ["Ohn'", "ein", "H\u00e4lm\u00b7chen", "zu", "bie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Unter Blumen und Klee!", "tokens": ["Un\u00b7ter", "Blu\u00b7men", "und", "Klee", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.12": {"line.1": {"text": "Gl\u00fccklich nenn' ich den Seufzer", "tokens": ["Gl\u00fcck\u00b7lich", "nenn'", "ich", "den", "Seuf\u00b7zer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Und die Thr\u00e4ne, da\u00df sie", "tokens": ["Und", "die", "Thr\u00e4\u00b7ne", ",", "da\u00df", "sie"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "KOUS", "PPER"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "So vergehn und versiegen", "tokens": ["So", "ver\u00b7gehn", "und", "ver\u00b7sie\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVINF", "KON", "VVINF"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Unter Blumen und Klee!", "tokens": ["Un\u00b7ter", "Blu\u00b7men", "und", "Klee", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.13": {"line.1": {"text": "La\u00dft im Gr\u00fcnen mich liegen", "tokens": ["La\u00dft", "im", "Gr\u00fc\u00b7nen", "mich", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "APPRART", "NN", "PPER", "VVFIN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Unter Blumen und Klee,", "tokens": ["Un\u00b7ter", "Blu\u00b7men", "und", "Klee", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "--+--+", "measu