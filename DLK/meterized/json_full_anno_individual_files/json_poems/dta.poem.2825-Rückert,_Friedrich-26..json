{"dta.poem.2825": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "26.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.85", "en:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Angenehme thut, wenns keine Frucht auch trug,", "tokens": ["Das", "An\u00b7ge\u00b7neh\u00b7me", "thut", ",", "wenns", "kei\u00b7ne", "Frucht", "auch", "trug", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "PIAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch augenblicklichen Genu\u00df uns schon genug.", "tokens": ["Durch", "au\u00b7gen\u00b7blick\u00b7li\u00b7chen", "Ge\u00b7nu\u00df", "uns", "schon", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Unangenehmem, dem wir k\u00f6nnen nicht entrinnen,", "tokens": ["Un\u00b7an\u00b7ge\u00b7neh\u00b7mem", ",", "dem", "wir", "k\u00f6n\u00b7nen", "nicht", "ent\u00b7rin\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "VMFIN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wollen wir wenigstens Belehrung abgewinnen.", "tokens": ["Wol\u00b7len", "wir", "we\u00b7nigs\u00b7tens", "Be\u00b7leh\u00b7rung", "ab\u00b7ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}}}}