{"textgrid.poem.26114": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Liebste ging mit zum Maiengarten,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Liebste ging mit zum Maiengarten,", "tokens": ["Die", "Liebs\u00b7te", "ging", "mit", "zum", "Mai\u00b7en\u00b7gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wo Dompfaff und Fink in dem Rasen aufwarten,", "tokens": ["Wo", "Dom\u00b7pfaff", "und", "Fink", "in", "dem", "Ra\u00b7sen", "auf\u00b7war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "KON", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Drin wilde Hyazinthen aufs Bienenvolk harrten;", "tokens": ["Drin", "wil\u00b7de", "Hy\u00b7a\u00b7zin\u00b7then", "aufs", "Bie\u00b7nen\u00b7volk", "harr\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wo jetzt die B\u00fcsche mit goldgr\u00fcnen Maschen sich bauen,", "tokens": ["Wo", "jetzt", "die", "B\u00fc\u00b7sche", "mit", "gold\u00b7gr\u00fc\u00b7nen", "Ma\u00b7schen", "sich", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+--++-+--+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Und durchsichtige Lauben, denen noch nicht zu trauen.", "tokens":