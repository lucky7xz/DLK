{"dta.poem.13203": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z ehndes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201edir ist mein Sinn bewu\u00dft, du kennest mein Gem\u00fcth", "tokens": ["\u201e", "dir", "ist", "mein", "Sinn", "be\u00b7wu\u00dft", ",", "du", "ken\u00b7nest", "mein", "Ge\u00b7m\u00fcth"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "PPOSAT", "NN", "ADJD", "$,", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201eo GOtt! vor dem die Welt in Furcht und Demuth kniet.", "tokens": ["\u201e", "o", "Gott", "!", "vor", "dem", "die", "Welt", "in", "Furcht", "und", "De\u00b7muth", "kniet", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "$.", "APPR", "ART", "ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "255\u201dDu zeigst es offenbar, da\u00df du mein Haus besch\u00fcztest;", "tokens": ["\"", "Du", "zeigst", "es", "of\u00b7fen\u00b7bar", ",", "da\u00df", "du", "mein", "Haus", "be\u00b7sch\u00fcz\u00b7test", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ADJD", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eden so gest\u00fcrmten Thron durch Wunderwercke st\u00fcztest.", "tokens": ["\u201e", "den", "so", "ge\u00b7st\u00fcrm\u00b7ten", "Thron", "durch", "Wun\u00b7der\u00b7wer\u00b7cke", "st\u00fcz\u00b7test", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADV", "ADJA", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201edu wei\u00dft es, wie beherzt wir nur auf dich gebaut,", "tokens": ["\u201e", "du", "wei\u00dft", "es", ",", "wie", "be\u00b7herzt", "wir", "nur", "auf", "dich", "ge\u00b7baut", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "$,", "PWAV", "ADJD", "PPER", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-++--+", "measure": "iambic.hexa.chol"}, "line.6": {"text": "\u201ewie zuversichtlich wir uns deinem Arm vertraut.", "tokens": ["\u201e", "wie", "zu\u00b7ver\u00b7sicht\u00b7lich", "wir", "uns", "dei\u00b7nem", "Arm", "ver\u00b7traut", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "ADJD", "PPER", "PRF", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201ehat es an Menschen Hand, Verstand und Wiz gebrochen,", "tokens": ["\u201e", "hat", "es", "an", "Men\u00b7schen", "Hand", ",", "Ver\u00b7stand", "und", "Wiz", "ge\u00b7bro\u00b7chen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "APPR", "NN", "NN", "$,", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.8": {"text": "260\u201dSo ward ich an dem Stolz des Feinds durch dich gerochen.", "tokens": ["\"", "So", "ward", "ich", "an", "dem", "Stolz", "des", "Feinds", "durch", "dich", "ge\u00b7ro\u00b7chen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201eich dancke dir darum, und zeig\u2019 es offentlich", "tokens": ["\u201e", "ich", "dan\u00b7cke", "dir", "da\u00b7rum", ",", "und", "zeig'", "es", "of\u00b7fent\u00b7lich"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "PPER", "PAV", "$,", "KON", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201eso sehr, als deine Werck\u2019 an mir verwunderlich.", "tokens": ["\u201e", "so", "sehr", ",", "als", "dei\u00b7ne", "Werck", "an", "mir", "ver\u00b7wun\u00b7der\u00b7lich", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "$,", "KOUS", "PPOSAT", "NN", "APPR", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201eich will auch nimmermehr von deinem Bund mich trennen,", "tokens": ["\u201e", "ich", "will", "auch", "nim\u00b7mer\u00b7mehr", "von", "dei\u00b7nem", "Bund", "mich", "tren\u00b7nen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201emach und verschaff, da\u00df ich mich kann so standhaft nennen.", "tokens": ["\u201e", "mach", "und", "ver\u00b7schaff", ",", "da\u00df", "ich", "mich", "kann", "so", "stand\u00b7haft", "nen\u00b7nen", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "KON", "VVFIN", "$,", "KOUS", "PPER", "PPER", "VMFIN", "ADV", "ADJD", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}}}}