{"dta.poem.11162": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Abigail.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich habe Davids zorn mit freundlichkeit vers\u00f6hnt,", "tokens": ["Ich", "ha\u00b7be", "Da\u00b7vids", "zorn", "mit", "freund\u00b7lich\u00b7keit", "ver\u00b7s\u00f6hnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df er mein wittwen-lied mit seiner liebe cr\u00f6nt.", "tokens": ["Da\u00df", "er", "mein", "witt\u00b7wen\u00b7lied", "mit", "sei\u00b7ner", "lie\u00b7be", "cr\u00f6nt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jhr weiber, dencket nach! War ich nicht klug und schlau?", "tokens": ["Ihr", "wei\u00b7ber", ",", "den\u00b7cket", "nach", "!", "War", "ich", "nicht", "klug", "und", "schlau", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PTKVZ", "$.", "VAFIN", "PPER", "PTKNEG", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ich gab ihm einen korb, so ward ich seine frau.", "tokens": ["Ich", "gab", "ihm", "ei\u00b7nen", "korb", ",", "so", "ward", "ich", "sei\u00b7ne", "frau", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,", "ADV", "VAFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}