{"dta.poem.23708": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Diener der Liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-200905197682", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Alles, Liebe, mu\u00df dir dienen,", "tokens": ["Al\u00b7les", ",", "Lie\u00b7be", ",", "mu\u00df", "dir", "die\u00b7nen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "NN", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alles dienet deinen Kindern.", "tokens": ["Al\u00b7les", "die\u00b7net", "dei\u00b7nen", "Kin\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sonnen scheinen, sie zu w\u00e4rmen,", "tokens": ["Son\u00b7nen", "schei\u00b7nen", ",", "sie", "zu", "w\u00e4r\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schatten schweben, sie zu k\u00fchlen,", "tokens": ["Schat\u00b7ten", "schwe\u00b7ben", ",", "sie", "zu", "k\u00fch\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "V\u00f6gel singen, sie zu lokken,", "tokens": ["V\u00f6\u00b7gel", "sin\u00b7gen", ",", "sie", "zu", "lok\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Tauben girren, sie zu reitzen,", "tokens": ["Tau\u00b7ben", "gir\u00b7ren", ",", "sie", "zu", "reit\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Rosen bl\u00fchen, sie zu schm\u00fckken,", "tokens": ["Ro\u00b7sen", "bl\u00fc\u00b7hen", ",", "sie", "zu", "schm\u00fck\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sterne funkeln, sie zu f\u00fchren,", "tokens": ["Ster\u00b7ne", "fun\u00b7keln", ",", "sie", "zu", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Monde leuchten, sie zu zeigen,", "tokens": ["Mon\u00b7de", "leuch\u00b7ten", ",", "sie", "zu", "zei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und die N\u00e4chte tragen Wolken,", "tokens": ["Und", "die", "N\u00e4ch\u00b7te", "tra\u00b7gen", "Wol\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Deine Kinder zu verbergen;", "tokens": ["Dei\u00b7ne", "Kin\u00b7der", "zu", "ver\u00b7ber\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Liebe, la\u00df doch, wenn ich liebe,", "tokens": ["Lie\u00b7be", ",", "la\u00df", "doch", ",", "wenn", "ich", "lie\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Schatten, Rosen, V\u00f6gel, Sonnen,", "tokens": ["Schat\u00b7ten", ",", "Ro\u00b7sen", ",", "V\u00f6\u00b7gel", ",", "Son\u00b7nen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Sterne, Mond und N\u00e4chte dienen.", "tokens": ["Ster\u00b7ne", ",", "Mond", "und", "N\u00e4ch\u00b7te", "die\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}