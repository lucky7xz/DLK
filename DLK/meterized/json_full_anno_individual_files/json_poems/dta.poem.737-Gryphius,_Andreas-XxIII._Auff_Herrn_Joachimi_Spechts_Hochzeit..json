, "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADJD", "PTKVZ", "$(", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Drumb schawet vnser Specht weil alle B\u00e4um erbleichen/ ", "tokens": ["Drumb", "scha\u00b7wet", "vn\u00b7ser", "Specht", "weil", "al\u00b7le", "B\u00e4um", "er\u00b7blei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "KOUS", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf die der ", "tokens": ["Auf", "die", "der"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PRELS", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "An welchem orth er doch der ", "tokens": ["An", "wel\u00b7chem", "orth", "er", "doch", "der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "PPER", "ADV", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Entgeh\u2019/ vnd ob f\u00fcr jhn ein Nest sey zu erreichen.", "tokens": ["Ent\u00b7geh'", "/", "vnd", "ob", "f\u00fcr", "jhn", "ein", "Nest", "sey", "zu", "er\u00b7rei\u00b7chen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "KON", "KOUS", "APPR", "PPER", "ART", "NN", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}