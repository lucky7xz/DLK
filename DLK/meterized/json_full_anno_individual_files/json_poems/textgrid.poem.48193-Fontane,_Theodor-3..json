{"textgrid.poem.48193": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1858, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und wieder hier drau\u00dfen ein neues Jahr \u2013", "tokens": ["Und", "wie\u00b7der", "hier", "drau\u00b7\u00dfen", "ein", "neu\u00b7es", "Jahr", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Was werden die Tage bringen?!", "tokens": ["Was", "wer\u00b7den", "die", "Ta\u00b7ge", "brin\u00b7gen", "?!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wird's werden, wie es immer war,", "tokens": ["Wird's", "wer\u00b7den", ",", "wie", "es", "im\u00b7mer", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAINF", "$,", "PWAV", "PPER", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Halb scheitern, halb gelingen?", "tokens": ["Halb", "schei\u00b7tern", ",", "halb", "ge\u00b7lin\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wird's f\u00f6rdern das, worauf ich gebaut,", "tokens": ["Wird's", "f\u00f6r\u00b7dern", "das", ",", "wo\u00b7rauf", "ich", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PDS", "$,", "PWAV", "PPER", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Oder vollends es verderben?", "tokens": ["O\u00b7der", "vol\u00b7lends", "es", "ver\u00b7der\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gleichviel, was es im Kessel braut,", "tokens": ["Gleich\u00b7viel", ",", "was", "es", "im", "Kes\u00b7sel", "braut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur w\u00fcnsch' ich nicht zu sterben.", "tokens": ["Nur", "w\u00fcn\u00b7sch'", "ich", "nicht", "zu", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich m\u00f6chte noch wieder im Vaterland", "tokens": ["Ich", "m\u00f6ch\u00b7te", "noch", "wie\u00b7der", "im", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "APPRART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die Gl\u00e4ser klingen lassen", "tokens": ["Die", "Gl\u00e4\u00b7ser", "klin\u00b7gen", "las\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wieder noch des Freundes Hand", "tokens": ["Und", "wie\u00b7der", "noch", "des", "Freun\u00b7des", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Einverst\u00e4ndnis fassen.", "tokens": ["Im", "Ein\u00b7ver\u00b7st\u00e4nd\u00b7nis", "fas\u00b7sen", "."], "token_info