{"dta.poem.20060": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Im Ilsethal .  \n 1841.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Hier m\u00f6cht\u2019 ich wo h\u00fcpfend die Wellen", "tokens": ["Hier", "m\u00f6cht'", "ich", "wo", "h\u00fcp\u00b7fend", "die", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PWAV", "ADJD", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Sich st\u00fcrzen vom Felsgestein,", "tokens": ["Sich", "st\u00fcr\u00b7zen", "vom", "Fels\u00b7ge\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Hier unter dem blauenden Himmel", "tokens": ["Hier", "un\u00b7ter", "dem", "blau\u00b7en\u00b7den", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Im Fr\u00fchling geboren sein.", "tokens": ["Im", "Fr\u00fch\u00b7ling", "ge\u00b7bo\u00b7ren", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "VAINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Dann h\u00e4tte sich, statt eines Priesters,", "tokens": ["Dann", "h\u00e4t\u00b7te", "sich", ",", "statt", "ei\u00b7nes", "Pries\u00b7ters", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PRF", "$,", "KOUI", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sobald ich die Sonne erblickt,", "tokens": ["So\u00b7bald", "ich", "die", "Son\u00b7ne", "er\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Die hehre, g\u00f6ttliche Sch\u00f6pfung", "tokens": ["Die", "heh\u00b7re", ",", "g\u00f6tt\u00b7li\u00b7che", "Sch\u00f6p\u00b7fung"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Zu meiner Taufe beschickt.", "tokens": ["Zu", "mei\u00b7ner", "Tau\u00b7fe", "be\u00b7schickt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Es h\u00e4tte sich \u00fcber dem T\u00e4ufling", "tokens": ["Es", "h\u00e4t\u00b7te", "sich", "\u00fc\u00b7ber", "dem", "T\u00e4uf\u00b7ling"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Gew\u00f6lbt des Himmels Dom,", "tokens": ["Ge\u00b7w\u00f6lbt", "des", "Him\u00b7mels", "Dom", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die B\u00e4ume h\u00e4tten gerauschet", "tokens": ["Die", "B\u00e4u\u00b7me", "h\u00e4t\u00b7ten", "ge\u00b7rau\u00b7schet"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wie leiser Orgelstrom.", "tokens": ["Wie", "lei\u00b7ser", "Or\u00b7gel\u00b7strom", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Es w\u00e4re darinnen erklungen", "tokens": ["Es", "w\u00e4\u00b7re", "da\u00b7rin\u00b7nen", "er\u00b7klun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Der V\u00f6gel Melodei;", "tokens": ["Der", "V\u00f6\u00b7gel", "Me\u00b7lo\u00b7dei", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Felsen h\u00e4tten gestanden", "tokens": ["Die", "Fel\u00b7sen", "h\u00e4t\u00b7ten", "ge\u00b7stan\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Als ernste Zeugen dabei.", "tokens": ["Als", "erns\u00b7te", "Zeu\u00b7gen", "da\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PAV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Ein Felsblock h\u00e4tte mich sicher", "tokens": ["Ein", "Fels\u00b7block", "h\u00e4t\u00b7te", "mich", "si\u00b7cher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "PRF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "In seinem weiten Schoo\u00df", "tokens": ["In", "sei\u00b7nem", "wei\u00b7ten", "Schoo\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wohl \u00fcber die Taufe gehalten,", "tokens": ["Wohl", "\u00fc\u00b7ber", "die", "Tau\u00b7fe", "ge\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Umh\u00fcllt von duftigem Moos.", "tokens": ["Um\u00b7h\u00fcllt", "von", "duf\u00b7ti\u00b7gem", "Moos", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Es h\u00e4tte der Ku\u00df der Sonne", "tokens": ["Es", "h\u00e4t\u00b7te", "der", "Ku\u00df", "der", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Stirne mir gesengt,", "tokens": ["Die", "Stir\u00b7ne", "mir", "ge\u00b7sengt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und mit dem Wasser der Taufe", "tokens": ["Und", "mit", "dem", "Was\u00b7ser", "der", "Tau\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Die Ilse mich besprengt.", "tokens": ["Die", "Il\u00b7se", "mich", "be\u00b7sprengt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Die Felsen h\u00e4tten geschworen", "tokens": ["Die", "Fel\u00b7sen", "h\u00e4t\u00b7ten", "ge\u00b7schwo\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Den felsenfesten Schwur:", "tokens": ["Den", "fel\u00b7sen\u00b7fes\u00b7ten", "Schwur", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Glauben mich gro\u00df zu ziehen", "tokens": ["Im", "Glau\u00b7ben", "mich", "gro\u00df", "zu", "zie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPER", "ADJD", "PTKZU", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "An Gott in der Natur.", "tokens": ["An", "Gott", "in", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}