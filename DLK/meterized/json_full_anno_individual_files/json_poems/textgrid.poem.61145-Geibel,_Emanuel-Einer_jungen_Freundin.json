{"textgrid.poem.61145": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Einer jungen Freundin", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Meer ist oben glatt und spiegeleben,", "tokens": ["Das", "Meer", "ist", "o\u00b7ben", "glatt", "und", "spie\u00b7ge\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch bunte G\u00e4rten tr\u00e4gt's auf seinem Grunde;", "tokens": ["Doch", "bun\u00b7te", "G\u00e4r\u00b7ten", "tr\u00e4gt's", "auf", "sei\u00b7nem", "Grun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Goldw\u00e4lder, Purpurstauden stehn im Sunde,", "tokens": ["Gold\u00b7w\u00e4l\u00b7der", ",", "Pur\u00b7pur\u00b7stau\u00b7den", "stehn", "im", "Sun\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.4": {"text": "Darinnen Perlen statt des Taues beben.", "tokens": ["Da\u00b7rin\u00b7nen", "Per\u00b7len", "statt", "des", "Tau\u00b7es", "be\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das ist ein heimlich Gl\u00fchn, ein farbig Leben,", "tokens": ["Das", "ist", "ein", "heim\u00b7lich", "Gl\u00fchn", ",", "ein", "far\u00b7big", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJD", "NN", "$,", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch selten wird dem Schiffenden die Kunde;", "tokens": ["Doch", "sel\u00b7ten", "wird", "dem", "Schif\u00b7fen\u00b7den", "die", "Kun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Sonntagskind nur sieht in guter Stunde", "tokens": ["Ein", "Sonn\u00b7tags\u00b7kind", "nur", "sieht", "in", "gu\u00b7ter", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Wipfel d\u00e4mmernd aus der Tiefe streben.", "tokens": ["Die", "Wip\u00b7fel", "d\u00e4m\u00b7mernd", "aus", "der", "Tie\u00b7fe", "stre\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So bl\u00fcht auch dir ein Garten im Gem\u00fcte;", "tokens": ["So", "bl\u00fcht", "auch", "dir", "ein", "Gar\u00b7ten", "im", "Ge\u00b7m\u00fc\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Allein die Welt, get\u00e4uscht von deinen Scherzen,", "tokens": ["Al\u00b7lein", "die", "Welt", ",", "ge\u00b7t\u00e4uscht", "von", "dei\u00b7nen", "Scher\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ist blind f\u00fcr seine wundersame Bl\u00fcte.", "tokens": ["Ist", "blind", "f\u00fcr", "sei\u00b7ne", "wun\u00b7der\u00b7sa\u00b7me", "Bl\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der Dichter nur, vertraut mit Lust und Schmerzen,", "tokens": ["Der", "Dich\u00b7ter", "nur", ",", "ver\u00b7traut", "mit", "Lust", "und", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Las, was im Dunkel deines Auges gl\u00fchte,", "tokens": ["Las", ",", "was", "im", "Dun\u00b7kel", "dei\u00b7nes", "Au\u00b7ges", "gl\u00fch\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "APPRART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und ahnt die Zauberwelt in deinem Herzen.", "tokens": ["Und", "ahnt", "die", "Zau\u00b7ber\u00b7welt", "in", "dei\u00b7nem", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Das Meer ist oben glatt und spiegeleben,", "tokens": ["Das", "Meer", "ist", "o\u00b7ben", "glatt", "und", "spie\u00b7ge\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "KON