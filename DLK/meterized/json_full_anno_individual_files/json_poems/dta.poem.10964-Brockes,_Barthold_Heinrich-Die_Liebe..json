{"dta.poem.10964": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dein bitteres Honigseim, schmeichlende Liebe,", "tokens": ["Dein", "bit\u00b7te\u00b7res", "Ho\u00b7ni\u00b7gseim", ",", "schmeich\u00b7len\u00b7de", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wirkt liebliche Plagen, wirkt kr\u00e4nkenden Scherz,", "tokens": ["Wirkt", "lieb\u00b7li\u00b7che", "Pla\u00b7gen", ",", "wirkt", "kr\u00e4n\u00b7ken\u00b7den", "Scherz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$,", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ergetzenden Kummer, erquickenden Schmerz,", "tokens": ["Er\u00b7get\u00b7zen\u00b7den", "Kum\u00b7mer", ",", "er\u00b7qui\u00b7cken\u00b7den", "Schmerz", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und f\u00fcllet mit Nectar und Wermuth ein Herz,", "tokens": ["Und", "f\u00fcl\u00b7let", "mit", "Nec\u00b7tar", "und", "Wer\u00b7muth", "ein", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Das deine vergn\u00fcglich-versehrende Triebe,", "tokens": ["Das", "dei\u00b7ne", "ver\u00b7gn\u00fcg\u00b7lich\u00b7ver\u00b7seh\u00b7ren\u00b7de", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Mit Unmuth und Anmuth gemischet, genossen.", "tokens": ["Mit", "Un\u00b7muth", "und", "An\u00b7muth", "ge\u00b7mi\u00b7schet", ",", "ge\u00b7nos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,", "VVPP", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Dein loderndes Frieren, dein k\u00fchlender Brand,", "tokens": ["Dein", "lo\u00b7dern\u00b7des", "Frie\u00b7ren", ",", "dein", "k\u00fch\u00b7len\u00b7der", "Brand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Aus welchem der Sterblichen Wesen entsprossen,", "tokens": ["Aus", "wel\u00b7chem", "der", "Sterb\u00b7li\u00b7chen", "We\u00b7sen", "ent\u00b7spros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.9": {"text": "Macht unsre Beschaffenheit, Wesen und Stand,", "tokens": ["Macht", "uns\u00b7re", "Be\u00b7schaf\u00b7fen\u00b7heit", ",", "We\u00b7sen", "und", "Stand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+---+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "Voll stetigen Widerspruch, deutlich bekannt.", "tokens": ["Voll", "ste\u00b7ti\u00b7gen", "Wi\u00b7der\u00b7spruch", ",", "deut\u00b7lich", "be\u00b7kannt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,", "ADJD", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}