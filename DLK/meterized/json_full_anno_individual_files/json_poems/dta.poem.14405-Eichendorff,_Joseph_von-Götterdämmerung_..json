{"dta.poem.14405": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "G\u00f6tterd\u00e4mmerung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wo sind nun die Gespielen?", "tokens": ["Wo", "sind", "nun", "die", "Ge\u00b7spie\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Diana schl\u00e4ft im Wald,", "tokens": ["Di\u00b7a\u00b7na", "schl\u00e4ft", "im", "Wald", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Neptunus ruht im k\u00fchlen", "tokens": ["Nep\u00b7tu\u00b7nus", "ruht", "im", "k\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPRART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Meerschlo\u00df, das einsam hallt.", "tokens": ["Meer\u00b7schlo\u00df", ",", "das", "ein\u00b7sam", "hallt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}