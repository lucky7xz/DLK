{"textgrid.poem.36838": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Syrakus", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja ganz, Marcell, hast du die Gewaltige", "tokens": ["Ja", "ganz", ",", "Mar\u00b7cell", ",", "hast", "du", "die", "Ge\u00b7wal\u00b7ti\u00b7ge"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "ADV", "$,", "NE", "$,", "VAFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Zermalmet, oder glaub' ich der Thr\u00e4ne, die", "tokens": ["Zer\u00b7mal\u00b7met", ",", "o\u00b7der", "glaub'", "ich", "der", "Thr\u00e4\u00b7ne", ",", "die"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "$,", "KON", "VVFIN", "PPER", "ART", "NN", "$,", "PRELS"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Du ihr geweint, war's nicht dein Adler", "tokens": ["Du", "ihr", "ge\u00b7weint", ",", "wa\u00b7r's", "nicht", "dein", "Ad\u00b7ler"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "PPER", "VVPP", "$,", "VAFIN", "PTKNEG", "PPOSAT", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Doch, dein gef\u00fcrchteter Bote, Vater,", "tokens": ["Doch", ",", "dein", "ge\u00b7f\u00fcrch\u00b7te\u00b7ter", "Bo\u00b7te", ",", "Va\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "$,", "PPOSAT", "ADJA", "NN", "$,", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Blitze schleudert und Schicksalsrath vollbringt,", "tokens": ["Der", "Blit\u00b7ze", "schleu\u00b7dert", "und", "Schick\u00b7sals\u00b7rath", "voll\u00b7bringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Trinakriens vierst\u00e4dtiges Rom hat er's", "tokens": ["Tri\u00b7na\u00b7kri\u00b7ens", "vier\u00b7st\u00e4d\u00b7ti\u00b7ges", "Rom", "hat", "er's"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NE", "VAFIN", "NE"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Zermalmt und weggetilgt vom Boden,", "tokens": ["Zer\u00b7malmt", "und", "weg\u00b7ge\u00b7tilgt", "vom", "Bo\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Tempel zertr\u00fcmmert und Burg und Mauer?", "tokens": ["Tem\u00b7pel", "zer\u00b7tr\u00fcm\u00b7mert", "und", "Burg", "und", "Mau\u00b7er", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "KON", "NN", "KON", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Des Denkers selbst, des v\u00f6lkerzerst\u00f6renden,", "tokens": ["Des", "Den\u00b7kers", "selbst", ",", "des", "v\u00f6l\u00b7ker\u00b7zer\u00b7st\u00f6\u00b7ren\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Geschonet nicht, und schlangenbekr\u00e4nzt von Mund", "tokens": ["Ge\u00b7scho\u00b7net", "nicht", ",", "und", "schlan\u00b7gen\u00b7be\u00b7kr\u00e4nzt", "von", "Mund"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "$,", "KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Zu Mund gereicht des Wahnsinns Becher,", "tokens": ["Zu", "Mund", "ge\u00b7reicht", "des", "Wahn\u00b7sinns", "Be\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den mit der Flamme der Mordwuth Eris", "tokens": ["Den", "mit", "der", "Flam\u00b7me", "der", "Mord\u00b7wuth", "E\u00b7ris"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "NE"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "Mit Blut, die streitbegeisterte, bis zum Rand", "tokens": ["Mit", "Blut", ",", "die", "streit\u00b7be\u00b7geis\u00b7ter\u00b7te", ",", "bis", "zum", "Rand"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "VVFIN", "$,", "KOUS", "APPRART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Gef\u00fcllt, vom zarten Weibe, vom S\u00e4ugling Blut", "tokens": ["Ge\u00b7f\u00fcllt", ",", "vom", "zar\u00b7ten", "Wei\u00b7be", ",", "vom", "S\u00e4ug\u00b7ling", "Blut"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "$,", "APPRART", "ADJA", "NN", "$,", "APPRART", "NN", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Gleich fordernd im bachant'schen Taumel", "tokens": ["Gleich", "for\u00b7dernd", "im", "bachant'\u00b7schen", "Tau\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "APPRART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wie von den M\u00f6rdern des K\u00f6nigshauses.", "tokens": ["Wie", "von", "den", "M\u00f6r\u00b7dern", "des", "K\u00f6\u00b7nigs\u00b7hau\u00b7ses", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "So jemand niederschaute vom grauen Fels,", "tokens": ["So", "je\u00b7mand", "nie\u00b7der\u00b7schau\u00b7te", "vom", "grau\u00b7en", "Fels", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der einst umstarrt' die Mauer des Dionys,", "tokens": ["Der", "einst", "um\u00b7starrt'", "die", "Mau\u00b7er", "des", "Di\u00b7o\u00b7nys", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "ART", "NN", "ART", "NE", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "So er des Berges Schutt und Tr\u00fcmmer,", "tokens": ["So", "er", "des", "Ber\u00b7ges", "Schutt", "und", "Tr\u00fcm\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hafen und Fels und Dianens Insel", "tokens": ["Ha\u00b7fen", "und", "Fels", "und", "Di\u00b7a\u00b7nens", "In\u00b7sel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "NN", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.6": {"line.1": {"text": "Anblickte fragend: Wo denn ersp\u00e4h ich sie", "tokens": ["An\u00b7blick\u00b7te", "fra\u00b7gend", ":", "Wo", "denn", "er\u00b7sp\u00e4h", "ich", "sie"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "$.", "PWAV", "ADV", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Karthagos stolze Siegerin und Athens?", "tokens": ["Kar\u00b7tha\u00b7gos", "stol\u00b7ze", "Sie\u00b7ge\u00b7rin", "und", "A\u00b7thens", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "KON", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich frage Meer und Land: die goldne,", "tokens": ["Ich", "fra\u00b7ge", "Meer", "und", "Land", ":", "die", "gold\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$.", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Herrlichste Tochter Korinths, wo ist sie?", "tokens": ["Herr\u00b7lichs\u00b7te", "Toch\u00b7ter", "Ko\u00b7rinths", ",", "wo", "ist", "sie", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,", "PWAV", "VAFIN", "PPER", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.7": {"line.1": {"text": "Der Ceres Frucht wohl seh ich in jenem Thal,", "tokens": ["Der", "Ce\u00b7res", "Frucht", "wohl", "seh", "ich", "in", "je\u00b7nem", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ganz andre Garben aber hat einst sie hier", "tokens": ["Ganz", "and\u00b7re", "Gar\u00b7ben", "a\u00b7ber", "hat", "einst", "sie", "hier"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "ADV", "VAFIN", "ADV", "PPER", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Aehrenleserin, die gro\u00dfe,", "tokens": ["Die", "A\u00b7eh\u00b7ren\u00b7le\u00b7se\u00b7rin", ",", "die", "gro\u00b7\u00dfe", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Hat die Geschichte sich hier gewunden,", "tokens": ["Hat", "die", "Ge\u00b7schich\u00b7te", "sich", "hier", "ge\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PRF", "ADV", "VVPP", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Die strenge Thatensammlerin, giftig Kraut", "tokens": ["Die", "stren\u00b7ge", "Tha\u00b7ten\u00b7samm\u00b7le\u00b7rin", ",", "gif\u00b7tig", "Kraut"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Absondernd von s\u00fc\u00dfn\u00e4hrender Frucht, den Sohn", "tokens": ["Ab\u00b7son\u00b7dernd", "von", "s\u00fc\u00df\u00b7n\u00e4h\u00b7ren\u00b7der", "Frucht", ",", "den", "Sohn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,", "ART", "NN"], "meter": "-+--++--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Doris und den Sohn des T\u00f6pfers", "tokens": ["Der", "Do\u00b7ris", "und", "den", "Sohn", "des", "T\u00f6p\u00b7fers"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "KON", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Scheidend von Hierons bessrem Gl\u00fccke.", "tokens": ["Schei\u00b7dend", "von", "Hie\u00b7rons", "bess\u00b7rem", "Gl\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NE", "ADJA", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.9": {"line.1": {"text": "O weintest du, Zerst\u00f6rer, was bliebe noch", "tokens": ["O", "wein\u00b7test", "du", ",", "Zer\u00b7st\u00f6\u00b7rer", ",", "was", "blie\u00b7be", "noch"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$,", "NN", "$,", "PWS", "VVFIN", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Nachwelt? Schutt und Grausen von Labdals Burg", "tokens": ["Der", "Nach\u00b7welt", "?", "Schutt", "und", "Grau\u00b7sen", "von", "Lab\u00b7dals", "Burg"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "NN", "KON", "NN", "APPR", "NN", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Von meerumrauschter Akradina", "tokens": ["Von", "mee\u00b7rum\u00b7rauschter", "Ak\u00b7ra\u00b7di\u00b7na"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bis zu Kronions verwaisten S\u00e4ulen!", "tokens": ["Bis", "zu", "Kro\u00b7nions", "ver\u00b7wais\u00b7ten", "S\u00e4u\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "NE", "ADJA", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.10": {"line.1": {"text": "Nur Steine, wo einst Thaten und Tugenden;", "tokens": ["Nur", "Stei\u00b7ne", ",", "wo", "einst", "Tha\u00b7ten", "und", "Tu\u00b7gen\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "PWAV", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der fromme Stier, wo einst der Tyrann sein Volk", "tokens": ["Der", "from\u00b7me", "Stier", ",", "wo", "einst", "der", "Ty\u00b7rann", "sein", "Volk"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "wor