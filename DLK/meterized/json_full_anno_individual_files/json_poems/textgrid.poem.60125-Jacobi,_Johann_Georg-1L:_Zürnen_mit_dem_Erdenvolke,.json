{"textgrid.poem.60125": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Z\u00fcrnen mit dem Erdenvolke,", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Z\u00fcrnen mit dem Erdenvolke,", "tokens": ["Z\u00fcr\u00b7nen", "mit", "dem", "Er\u00b7den\u00b7vol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Z\u00fcrnen will der Himmel nicht,", "tokens": ["Z\u00fcr\u00b7nen", "will", "der", "Him\u00b7mel", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ART", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn aus Wetterschwangrer Wolke", "tokens": ["Wenn", "aus", "Wet\u00b7ter\u00b7schwang\u00b7rer", "Wol\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gott mit seinen Kindern spricht.", "tokens": ["Gott", "mit", "sei\u00b7nen", "Kin\u00b7dern", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bey der Sonne sanftem Licht", "tokens": ["Bey", "der", "Son\u00b7ne", "sanf\u00b7tem", "Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Schweigen seine Donner wieder,", "tokens": ["Schwei\u00b7gen", "sei\u00b7ne", "Don\u00b7ner", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADV", "$,"], "meter": "+-+-+-+