{"dta.poem.12950": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als er sie gek\u00fcsset hatte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die schlau' und eifers\u00fcchtge liebe", "tokens": ["Die", "schlau'", "und", "ei\u00b7fer\u00b7s\u00fccht\u00b7ge", "lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "KON", "ADJA", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bewachte Florabellens mund.", "tokens": ["Be\u00b7wach\u00b7te", "Flo\u00b7ra\u00b7bel\u00b7lens", "mund", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Indem ich nun aus vorwitz-vollem triebe", "tokens": ["In\u00b7dem", "ich", "nun", "aus", "vor\u00b7witz\u00b7vol\u00b7lem", "trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ADJA", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nach diesen frischen rosen stund,", "tokens": ["Nach", "die\u00b7sen", "fri\u00b7schen", "ro\u00b7sen", "stund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "(worunter Amor sich versteckte)", "tokens": ["(", "wo\u00b7run\u00b7ter", "A\u00b7mor", "sich", "ver\u00b7steck\u00b7te", ")"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "NE", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und endlich auch den safft der s\u00fc\u00dfen blumen schmeckte,", "tokens": ["Und", "end\u00b7lich", "auch", "den", "safft", "der", "s\u00fc\u00b7\u00dfen", "blu\u00b7men", "schmeck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ART", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So stach die liebe zu, wie eine biene thut;", "tokens": ["So", "stach", "die", "lie\u00b7be", "zu", ",", "wie", "ei\u00b7ne", "bie\u00b7ne", "thut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "VVFIN", "PTKVZ", "$,", "PWAV", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der stich durchlief das heisse blut,", "tokens": ["Der", "stich", "durch\u00b7lief", "das", "heis\u00b7se", "blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und drang, iedoch nicht ohne s\u00fc\u00dfe schmertzen,", "tokens": ["Und", "drang", ",", "ie\u00b7doch", "nicht", "oh\u00b7ne", "s\u00fc\u00b7\u00dfe", "schmert\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADV", "PTKNEG", "APPR", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Mir von dem munde zu dem hertzen.", "tokens": ["Mir", "von", "dem", "mun\u00b7de", "zu", "dem", "hert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}}}}