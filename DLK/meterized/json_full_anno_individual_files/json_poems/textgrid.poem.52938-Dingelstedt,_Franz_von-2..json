{"textgrid.poem.52938": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir sa\u00dfen im Wagen zu Drei oder Vier,", "tokens": ["Wir", "sa\u00b7\u00dfen", "im", "Wa\u00b7gen", "zu", "Drei", "o\u00b7der", "Vier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR", "CARD", "KON", "CARD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ein verschleiertes Weib gegen\u00fcber mir.", "tokens": ["Ein", "ver\u00b7schlei\u00b7er\u00b7tes", "Weib", "ge\u00b7gen\u00b7\u00fc\u00b7ber", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Mond schien hell zum Fenster herein", "tokens": ["Der", "Mond", "schien", "hell", "zum", "Fens\u00b7ter", "her\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPRART", "NN", "PTKVZ"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und flo\u00df um ihr Haupt wie Heiligenschein.", "tokens": ["Und", "flo\u00df", "um", "ihr", "Haupt", "wie", "Hei\u00b7li\u00b7gen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "KOKOM", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Es war so heimlich drinnen, so traut,", "tokens": ["Es", "war", "so", "heim\u00b7lich", "drin\u00b7nen", ",", "so", "traut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "ADV", "$,", "ADV", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ringsum in der Nacht kein Licht, kein Laut.", "tokens": ["Ring\u00b7sum", "in", "der", "Nacht", "kein", "Licht", ",", "kein", "Laut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PIAT", "NN", "$,", "PIAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Nur die R\u00e4der knarrten in sandigem Gleis,", "tokens": ["Nur", "die", "R\u00e4\u00b7der", "knarr\u00b7ten", "in", "san\u00b7di\u00b7gem", "Gleis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Und die ledernen Polster seufzten leis.", "tokens": ["Und", "die", "le\u00b7der\u00b7nen", "Pols\u00b7ter", "seufz\u00b7ten", "leis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wer bist du, fremdes, liebes Gesicht", "tokens": ["Wer", "bist", "du", ",", "frem\u00b7des", ",", "lie\u00b7bes", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "$,", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Mit dn dunkelen Augen im Mondenlicht?", "tokens": ["Mit", "dn", "dun\u00b7ke\u00b7len", "Au\u00b7gen", "im", "Mon\u00b7den\u00b7licht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "O halte die Blicke nicht abgewandt,", "tokens": ["O", "hal\u00b7te", "die", "Bli\u00b7cke", "nicht", "ab\u00b7ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Du bist einsam wie ich, komm, reich mir die Hand!", "tokens": ["Du", "bist", "ein\u00b7sam", "wie", "ich", ",", "komm", ",", "reich", "mir", "die", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KOKOM", "PPER", "$,", "VVFIN", "$,", "ADJD", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.7": {"line.1": {"text": "Und lehn an meine Schulter Dich an,", "tokens": ["Und", "lehn", "an", "mei\u00b7ne", "Schul\u00b7ter", "Dich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Wenn die m\u00fcde Stirn nicht mehr wachen kann!", "tokens": ["Wenn", "die", "m\u00fc\u00b7de", "Stirn", "nicht", "mehr", "wa\u00b7chen", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PTKNEG", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "Ich h\u00f6rte sie atmen, ruhig-tief,", "tokens": ["Ich", "h\u00f6r\u00b7te", "sie", "at\u00b7men", ",", "ru\u00b7hig\u00b7tief", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVINF", "$,", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Busen wogte, \u2013 das M\u00e4dchen schlief ...", "tokens": ["Der", "Bu\u00b7sen", "wog\u00b7te", ",", "\u2013", "das", "M\u00e4d\u00b7chen", "schlief", "..."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "$(", "ART", "NN", "VVFIN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Ein Stunde, so hielt der Wagen an,", "tokens": ["Ein", "Stun\u00b7de", ",", "so", "hielt", "der", "Wa\u00b7gen", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Am Schlage harrte ein gro\u00dfer Mann.", "tokens": ["Am", "Schla\u00b7ge", "harr\u00b7te", "ein", "gro\u00b7\u00dfer", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "Das Posthorn klang, das M\u00e4dchen erwacht, \u2013", "tokens": ["Das", "Post\u00b7horn", "klang", ",", "das", "M\u00e4d\u00b7chen", "er\u00b7wacht", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVPP", "$,", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ein Gr\u00fc\u00dfen, ein K\u00fcssen scholl durch die Nacht.", "tokens": ["Ein", "Gr\u00fc\u00b7\u00dfen", ",", "ein", "K\u00fcs\u00b7sen", "scholl", "durch", "die", "Nacht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.11": {"line.1": {"text": "Sie hatten sich wieder, ein liebend Paar,", "tokens": ["Sie", "hat\u00b7ten", "sich", "wie\u00b7der", ",", "ein", "lie\u00b7bend", "Paar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "ADV", "$,", "ART", "ADJD", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Sie herzten sich, da\u00df ein Freude war.", "tokens": ["Sie", "herz\u00b7ten", "sich", ",", "da\u00df", "ein", "Freu\u00b7de", "war", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "KOUS", "ART", "NN", "VAFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.12": {"line.1": {"text": "Der Schleier entfiel, das Mondenlicht", "tokens": ["Der", "Schlei\u00b7er", "ent\u00b7fiel", ",", "das", "Mon\u00b7den\u00b7licht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Beleuchtete hell ein Engels-Gesicht.", "tokens": ["Be\u00b7leuch\u00b7te\u00b7te", "hell", "ein", "En\u00b7gels\u00b7Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.13": {"line.1": {"text": "Ich sah es von fern, mein Herz war voll,", "tokens": ["Ich", "sah", "es", "von", "fern", ",", "mein", "Herz", "war", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJD", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Eine Tr\u00e4ne hei\u00df aus der Wimper quoll.", "tokens": ["Ei\u00b7ne", "Tr\u00e4\u00b7ne", "hei\u00df", "aus", "der", "Wim\u00b7per", "quoll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.14": {"line.1": {"text": "Und als der Wagen von dannen flog,", "tokens": ["Und", "als", "der", "Wa\u00b7gen", "von", "dan\u00b7nen", "flog", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da stunden umschlungen die Beiden noch.", "tokens": ["Da", "stun\u00b7den", "um\u00b7schlun\u00b7gen", "die", "Bei\u00b7den", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.15": {"line.1": {"text": "Ich fuhr allein hinaus in die Nacht, \u2013 \u2013", "tokens": ["Ich", "fuhr", "al\u00b7lein", "hin\u00b7aus", "in", "die", "Nacht", ",", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APZR", "APPR", "ART", "NN", "$,", "$(", "$("], "meter": "-+---+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Ach Gott! w\u00e4r sie nimmer, nimmer erwacht!", "tokens": ["Ach", "Gott", "!", "w\u00e4r", "sie", "nim\u00b7mer", ",", "nim\u00b7mer", "er\u00b7wacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "VAFIN", "PPER", "ADV", "$,", "ADV", "VVPP", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.16": {"line.1": {"text": "Wir sa\u00dfen im Wagen zu Drei oder Vier,", "tokens": ["Wir", "sa\u00b7\u00dfen", "im", "Wa\u00b7gen", "zu", "Drei", "o\u00b7der", "Vier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "w