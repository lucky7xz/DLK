{"textgrid.poem.60214": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vergieb, o K\u00f6nigin! Mein Herz entschliesset sich,", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vergieb, o K\u00f6nigin! Mein Herz entschliesset sich,", "tokens": ["Ver\u00b7gieb", ",", "o", "K\u00f6\u00b7ni\u00b7gin", "!", "Mein", "Herz", "ent\u00b7schlies\u00b7set", "sich", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "FM", "NN", "$.", "PPOSAT", "NN", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kommt vor Dein Angesicht getreten,", "tokens": ["Kommt", "vor", "Dein", "An\u00b7ge\u00b7sicht", "ge\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Verschlinget Deinen Blick, wird k\u00fchn und fraget Dich:", "tokens": ["Ver\u00b7schlin\u00b7get", "Dei\u00b7nen", "Blick", ",", "wird", "k\u00fchn", "und", "fra\u00b7get", "Dich", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "VAFIN", "ADJD", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hat schon Dein grosses Herz f\u00fcr mich,", "tokens": ["Hat", "schon", "Dein", "gros\u00b7ses", "Herz", "f\u00fcr", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "ADJA", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den Bruder und den Held gebethen?", "tokens": ["Den", "Bru\u00b7der", "und", "den", "Held", "ge\u00b7be\u00b7then", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hast Du mein Lied an Ferdinand", "tokens": ["Hast", "Du", "mein", "Lied", "an", "Fer\u00b7di\u00b7nand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In Deinen Brief geh\u00fcllt, und also fortgesandt?", "tokens": ["In", "Dei\u00b7nen", "Brief", "ge\u00b7h\u00fcllt", ",", "und", "al\u00b7so", "fort\u00b7ge\u00b7sandt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,", "KON", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Darf ich der armen Bergstadt sagen,", "tokens": ["Darf", "ich", "der", "ar\u00b7men", "Berg\u00b7stadt", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Da\u00df Deine Seele, K\u00f6nigin!", "tokens": ["Da\u00df", "Dei\u00b7ne", "See\u00b7le", ",", "K\u00f6\u00b7ni\u00b7gin", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ger\u00fchret sey von ihren Klagen?", "tokens": ["Ge\u00b7r\u00fch\u00b7ret", "sey", "von", "ih\u00b7ren", "Kla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Verzeyhst Du mir, da\u00df ich verwegen bin,", "tokens": ["Ver\u00b7zeyhst", "Du", "mir", ",", "da\u00df", "ich", "ver\u00b7we\u00b7gen", "bin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Halb ungeduldig Dich zu fragen?", "tokens": ["Halb", "un\u00b7ge\u00b7dul\u00b7dig", "Dich", "zu", "fra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach siehe doch, der Herbst entflieht mit k\u00fcrzern Tagen", "tokens": ["Ach", "sie\u00b7he", "doch", ",", "der", "Herbst", "ent\u00b7flieht", "mit", "k\u00fcr\u00b7zern", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VVIMP", "ADV", "$,", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Allzugeschwind, und bald verh\u00fcllt", "tokens": ["All\u00b7zu\u00b7ge\u00b7schwind", ",", "und", "bald", "ver\u00b7h\u00fcllt"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "KON", "ADV", "VVPP"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Die Sonne sich, mit kalter Wolke!", "tokens": ["Die", "Son\u00b7ne", "sich", ",", "mit", "kal\u00b7ter", "Wol\u00b7ke", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn denn der rauhe Nord vom Harzgeb\u00fcrge br\u00fcllt;", "tokens": ["Wenn", "denn", "der", "rau\u00b7he", "Nord", "vom", "Harz\u00b7ge\u00b7b\u00fcr\u00b7ge", "br\u00fcllt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dann machet er dem armen Volke", "tokens": ["Dann", "ma\u00b7chet", "er", "dem", "ar\u00b7men", "Vol\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das in dem Bretterhause sitzt,", "tokens": ["Das", "in", "dem", "Bret\u00b7ter\u00b7hau\u00b7se", "sitzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Glieder kalt und starr, da\u00df frommer Andacht Feuer", "tokens": ["Die", "Glie\u00b7der", "kalt", "und", "starr", ",", "da\u00df", "from\u00b7mer", "An\u00b7dacht", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$,", "KOUS", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Kaum noch des H\u00f6rers Herz erhitzt.", "tokens": ["Kaum", "noch", "des", "H\u00f6\u00b7rers", "Herz", "er\u00b7hitzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Frau, ich beschw\u00f6re Dich bey allem, was Dir theuer", "tokens": ["Frau", ",", "ich", "be\u00b7schw\u00f6\u00b7re", "Dich", "bey", "al\u00b7lem", ",", "was", "Dir", "theu\u00b7er"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPER", "APPR", "PIS", "$,", "PRELS", "PPER", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In Deines Herzens Augen jemals war!", "tokens": ["In", "Dei\u00b7nes", "Her\u00b7zens", "Au\u00b7gen", "je\u00b7mals", "war", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bey Friedrichs Leben! bey dem Leben", "tokens": ["Bey", "Fried\u00b7richs", "Le\u00b7ben", "!", "bey", "dem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "$.", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Prinzen, der nach Ruhm zu streben,", "tokens": ["Des", "Prin\u00b7zen", ",", "der", "nach", "Ruhm", "zu", "stre\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nicht achtet Kugeln und Gefahr!", "tokens": ["Nicht", "ach\u00b7tet", "Ku\u00b7geln", "und", "Ge\u00b7fahr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und bey dem Frieden, den das Jahr", "tokens": ["Und", "bey", "dem", "Frie\u00b7den", ",", "den", "das", "Jahr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das wir erwarten mit sich bringet!", "tokens": ["Das", "wir", "er\u00b7war\u00b7ten", "mit", "sich", "brin\u00b7get", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Bey jeder Heldenthat, die Deinem Ferdinand", "tokens": ["Bey", "je\u00b7der", "Hel\u00b7den\u00b7that", ",", "die", "Dei\u00b7nem", "Fer\u00b7di\u00b7nand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wenn er sie unternimmt, gelinget!", "tokens": ["Wenn", "er", "sie", "un\u00b7ter\u00b7nimmt", ",", "ge\u00b7lin\u00b7get", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Verschaffe, da\u00df von seiner Hand", "tokens": ["Ver\u00b7schaf\u00b7fe", ",", "da\u00df", "von", "sei\u00b7ner", "Hand"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Das Haus gebauet wird zu unsers Gottes Preise!", "tokens": ["Das", "Haus", "ge\u00b7bau\u00b7et", "wird", "zu", "un\u00b7sers", "Got\u00b7tes", "Prei\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Es bringt dem Helden gr\u00f6ssern Ruhm,", "tokens": ["Es", "bringt", "dem", "Hel\u00b7den", "gr\u00f6s\u00b7sern", "Ruhm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Als wenn ein Sieg ihn schm\u00fcckt mit frischem Lorbeerreise.", "tokens": ["Als", "wenn", "ein", "Sieg", "ihn", "schm\u00fcckt", "mit", "fri\u00b7schem", "Lor\u00b7beer\u00b7rei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Dein Bildni\u00df soll das Heiligthum", "tokens": ["Dein", "Bild\u00b7ni\u00df", "soll", "das", "Hei\u00b7lig\u00b7thum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "An einem hohen Pfeiler schm\u00fccken;", "tokens": ["An", "ei\u00b7nem", "ho\u00b7hen", "Pfei\u00b7ler", "schm\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Und wenn die Kinder einst neugierig es beblicken,", "tokens": ["Und", "wenn", "die", "Kin\u00b7der", "einst", "neu\u00b7gie\u00b7rig", "es", "be\u00b7bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Denn lobt der Mutter Mund noch Ferdinandens That,", "tokens": ["Denn", "lobt", "der", "Mut\u00b7ter", "Mund", "noch", "Fer\u00b7di\u00b7nan\u00b7dens", "That", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "ADV", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und r\u00fchmt die K\u00f6nigin, die ihren Bruder bath.", "tokens": ["Und", "r\u00fchmt", "die", "K\u00f6\u00b7ni\u00b7gin", ",", "die", "ih\u00b7ren", "Bru\u00b7der", "ba\u00b7th", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"