{"textgrid.poem.33693": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "1L: W\u00e4r's nicht genug, im Herzen fortzudauern?", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4r's nicht genug, im Herzen fortzudauern?", "tokens": ["W\u00e4r's", "nicht", "ge\u00b7nug", ",", "im", "Her\u00b7zen", "fort\u00b7zu\u00b7dau\u00b7ern", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "$,", "APPRART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bed\u00fcrft' er einer andern Huldigung,", "tokens": ["Be\u00b7d\u00fcrft'", "er", "ei\u00b7ner", "an\u00b7dern", "Hul\u00b7di\u00b7gung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der Genius, als da\u00df die Besten trauern,", "tokens": ["Der", "Ge\u00b7nius", ",", "als", "da\u00df", "die", "Bes\u00b7ten", "trau\u00b7ern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOKOM", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wenn seine Harfe ri\u00df in j\u00e4hem Sprung?", "tokens": ["Wenn", "sei\u00b7ne", "Har\u00b7fe", "ri\u00df", "in", "j\u00e4\u00b7hem", "Sprung", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und doch! Wenn ihm die Welt in frommen Schauern", "tokens": ["Und", "doch", "!", "Wenn", "ihm", "die", "Welt", "in", "from\u00b7men", "Schau\u00b7ern"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$.", "KOUS", "PPER", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Den Dank beut eherner Verg\u00f6tterung,", "tokens": ["Den", "Dank", "beut", "e\u00b7her\u00b7ner", "Ver\u00b7g\u00f6t\u00b7te\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Ein Bild des Ruhms f\u00fcr tausend M\u00fch'n und Z\u00e4hren:", "tokens": ["Ein", "Bild", "des", "Ruhms", "f\u00fcr", "tau\u00b7send", "M\u00fch'n", "und", "Z\u00e4h\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "CARD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Auch das ist sch\u00f6n und gut; la\u00dft sie gew\u00e4hren!", "tokens": ["Auch", "das", "ist", "sch\u00f6n", "und", "gut", ";", "la\u00dft", "sie", "ge\u00b7w\u00e4h\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VAFIN", "ADJD", "KON", "ADJD", "$.", "VVIMP", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Asche weht im grauen Sturm durch Kl\u00fcfte", "tokens": ["Die", "A\u00b7sche", "weht", "im", "grau\u00b7en", "Sturm", "durch", "Kl\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von \u00c4tna's \u00fcberschneiten H\u00f6h'n herab,", "tokens": ["Von", "\u00c4t\u00b7na's", "\u00fc\u00b7bersc\u00b7hnei\u00b7ten", "H\u00f6h'n", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der \u00d6lbaum kr\u00e4nzt den Schutt verfallner Gr\u00fcfte", "tokens": ["Der", "\u00d6l\u00b7baum", "kr\u00e4nzt", "den", "Schutt", "ver\u00b7fall\u00b7ner", "Gr\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Am Golf, dem Syrakus den Namen gab.", "tokens": ["Am", "Golf", ",", "dem", "Sy\u00b7ra\u00b7kus", "den", "Na\u00b7men", "gab", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ART", "NE", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dort schatten Lorbeern, wehen Bl\u00fctend\u00fcfte", "tokens": ["Dort", "schat\u00b7ten", "Lor\u00b7beern", ",", "we\u00b7hen", "Bl\u00fc\u00b7ten\u00b7d\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJA", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Um eines deutschen Dichters einsam Grab,", "tokens": ["Um", "ei\u00b7nes", "deut\u00b7schen", "Dich\u00b7ters", "ein\u00b7sam", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der hier verblutend an langsamen Wunden", "tokens": ["Der", "hier", "ver\u00b7blu\u00b7tend", "an", "lang\u00b7sa\u00b7men", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "VVPP", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Im fremden Land die letzte Rast gefunden.", "tokens": ["Im", "frem\u00b7den", "Land", "die", "letz\u00b7te", "Rast", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nicht hat sein Volk den schwer von Gram Gekr\u00e4nkten", "tokens": ["Nicht", "hat", "sein", "Volk", "den", "schwer", "von", "Gram", "Ge\u00b7kr\u00e4nk\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VAFIN", "PPOSAT", "NN", "ART", "ADJD", "APPR", "NE", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vergessen, den von innrer Glut zernagt", "tokens": ["Ver\u00b7ges\u00b7sen", ",", "den", "von", "inn\u00b7rer", "Glut", "zer\u00b7nagt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "PRELS", "APPR", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zum S\u00fcden Stolz und Durst nach Sch\u00f6nheit lenkten,", "tokens": ["Zum", "S\u00fc\u00b7den", "Stolz", "und", "Durst", "nach", "Sch\u00f6n\u00b7heit", "lenk\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "KON", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da ihm die Heimat, was er bat, versagt.", "tokens": ["Da", "ihm", "die", "Hei\u00b7mat", ",", "was", "er", "bat", ",", "ver\u00b7sagt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "PWS", "PPER", "VVFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch die ihm dort den freien Blick beschr\u00e4nkten,", "tokens": ["Doch", "die", "ihm", "dort", "den", "frei\u00b7en", "Blick", "be\u00b7schr\u00e4nk\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die, selbst als er den h\u00f6chsten Flug gewagt,", "tokens": ["Die", ",", "selbst", "als", "er", "den", "h\u00f6chs\u00b7ten", "Flug", "ge\u00b7wagt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "KOUS", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Den Alpenr\u00fcckweg dunkelnd ihm verschlossen,", "tokens": ["Den", "Al\u00b7pen\u00b7r\u00fcck\u00b7weg", "dun\u00b7kelnd", "ihm", "ver\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die Wolken jener Zeit, sie sind zerflossen.", "tokens": ["Die", "Wol\u00b7ken", "je\u00b7ner", "Zeit", ",", "sie", "sind", "zer\u00b7flos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nicht sch\u00fcchtern mehr, wie zage J\u00fcnger, dr\u00e4ngen", "tokens": ["Nicht", "sch\u00fcch\u00b7tern", "mehr", ",", "wie", "za\u00b7ge", "J\u00fcn\u00b7ger", ",", "dr\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PTKNEG", "ADV", "ADV", "$,", "PWAV", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Um ihren S\u00e4nger sich, die ihn gekannt.", "tokens": ["Um", "ih\u00b7ren", "S\u00e4n\u00b7ger", "sich", ",", "die", "ihn", "ge\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "PRF", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das ganze Volk schon horchet den Ges\u00e4ngen,", "tokens": ["Das", "gan\u00b7ze", "Volk", "schon", "hor\u00b7chet", "den", "Ge\u00b7s\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er kam nach Hause, der sich selbst verbannt.", "tokens": ["Er", "kam", "nach", "Hau\u00b7se", ",", "der", "sich", "selbst", "ver\u00b7bannt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "PRELS", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Kein Fremdling ist sein Geist mehr; deinen Kl\u00e4ngen,", "tokens": ["Kein", "Fremd\u00b7ling", "ist", "sein", "Geist", "mehr", ";", "dei\u00b7nen", "Kl\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPOSAT", "NN", "ADV", "$.", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ghasel, hat unser Ohr sich zugewandt;", "tokens": ["Gha\u00b7sel", ",", "hat", "un\u00b7ser", "Ohr", "sich", "zu\u00b7ge\u00b7wandt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VAFIN", "PPOSAT", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wir tranken all' am Melodienborne,", "tokens": ["Wir", "tran\u00b7ken", "all'", "am", "Me\u00b7lo\u00b7dien\u00b7bor\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Der Platen's Brust entstr\u00f6mt' in Lieb' und Zorne.", "tokens": ["Der", "Pla\u00b7ten's", "Brust", "ent\u00b7str\u00f6mt'", "in", "Lieb'", "und", "Zor\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Die Lose rollten vor den ernsten Richtern,", "tokens": ["Die", "Lo\u00b7se", "roll\u00b7ten", "vor", "den", "erns\u00b7ten", "Rich\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Jahren, dunkel erst und endlich hell.", "tokens": ["Den", "Jah\u00b7ren", ",", "dun\u00b7kel", "erst", "und", "end\u00b7lich", "hell", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "ADV", "KON", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wir nennen ihn mit unsern besten Dichtern:", "tokens": ["Wir", "nen\u00b7nen", "ihn", "mit", "un\u00b7sern", "bes\u00b7ten", "Dich\u00b7tern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Ostens zarte Glut, der goldne Quell", "tokens": ["Des", "Os\u00b7tens", "zar\u00b7te", "Glut", ",", "der", "gold\u00b7ne", "Quell"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Des S\u00fcdens reiften ihn zu immer lichtern", "tokens": ["Des", "S\u00fc\u00b7dens", "reif\u00b7ten", "ihn", "zu", "im\u00b7mer", "lich\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Entfaltungen. Die Kr\u00e4fte wuchsen schnell", "tokens": ["Ent\u00b7fal\u00b7tun\u00b7gen", ".", "Die", "Kr\u00e4f\u00b7te", "wuch\u00b7sen", "schnell"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "ART", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dem f\u00fcr sein Vaterland, des Ruhmes Wiege,", "tokens": ["Dem", "f\u00fcr", "sein", "Va\u00b7ter\u00b7land", ",", "des", "Ruh\u00b7mes", "Wie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Entflammten Krieger im Befreiungskriege.", "tokens": ["Ent\u00b7flamm\u00b7ten", "Krie\u00b7ger", "im", "Be\u00b7frei\u00b7ungs\u00b7krie\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Dem Norden ist die Sehnsucht eingeboren,", "tokens": ["Dem", "Nor\u00b7den", "ist", "die", "Sehn\u00b7sucht", "ein\u00b7ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es kennt die Sehnsucht, wer Italien kennt.", "tokens": ["Es", "kennt", "die", "Sehn\u00b7sucht", ",", "wer", "I\u00b7ta\u00b7li\u00b7en", "kennt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PWS", "NE", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Venedigs alter Glanz, heraufbeschworen,", "tokens": ["Ve\u00b7ne\u00b7digs", "al\u00b7ter", "Glanz", ",", "her\u00b7auf\u00b7be\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "VVIZU", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Erf\u00fcllt' ihn dort und Meer und Firmament.", "tokens": ["Er\u00b7f\u00fcllt'", "ihn", "dort", "und", "Meer", "und", "Fir\u00b7ma\u00b7ment", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dort zeichnet' er leicht wie den Tanz der Horen", "tokens": ["Dort", "zeich\u00b7net'", "er", "leicht", "wie", "den", "Tanz", "der", "Ho\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KOKOM", "ART", "NN", "ART", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Die Bilder aus Neapel und Sorrent", "tokens": ["Die", "Bil\u00b7der", "aus", "Nea\u00b7pel", "und", "Sor\u00b7rent"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NE", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Und gr\u00fc\u00dft' im w\u00fcrdevollen Schritt der Alten", "tokens": ["Und", "gr\u00fc\u00dft'", "im", "w\u00fcr\u00b7de\u00b7vol\u00b7len", "Schritt", "der", "Al\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VV