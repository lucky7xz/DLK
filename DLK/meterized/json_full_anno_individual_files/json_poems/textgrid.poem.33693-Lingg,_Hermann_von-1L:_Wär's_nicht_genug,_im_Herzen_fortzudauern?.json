 "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "VVPP", "PPER", "$,", "PRELS", "ADJD", "APPR", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als Ringer stand, ein Mal im Strom der Zeit.", "tokens": ["Als", "Rin\u00b7ger", "stand", ",", "ein", "Mal", "im", "Strom", "der", "Zeit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "$,", "ART", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wenn nun bald, indes die Schleier fallen,", "tokens": ["Und", "wenn", "nun", "bald", ",", "in\u00b7des", "die", "Schlei\u00b7er", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ADV", "$,", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Fr\u00fchrots Strahl das Erz verkl\u00e4rend weiht,", "tokens": ["Des", "Fr\u00fch\u00b7rots", "Strahl", "das", "Erz", "ver\u00b7kl\u00e4\u00b7rend", "weiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ART", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dann wird ein Klang memnonisch ihm entschallen,", "tokens": ["Dann", "wird", "ein", "Klang", "mem\u00b7no\u00b7nisch", "ihm", "ent\u00b7schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein hoher Festgru\u00df der Unsterblichkeit,", "tokens": ["Ein", "ho\u00b7her", "Fest\u00b7gru\u00df", "der", "U\u00b7nsterb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und heilig Weh'n wird rauschen in den Zweigen", "tokens": ["Und", "hei\u00b7lig", "Weh'n", "wird", "rau\u00b7schen", "in", "den", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "NN", "VAFIN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Lorbeern, die auf Platen's Gruft sich neigen.", "tokens": ["Der", "Lor\u00b7beern", ",", "die", "auf", "Pla\u00b7ten's", "Gruft", "sich", "nei\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NE", "VVFIN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}