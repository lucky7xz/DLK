{"dta.poem.639": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "M. Crassi.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Ich ehrte nur das Gold/ und funde mein Erg\u00f6tzen", "tokens": ["Ich", "ehr\u00b7te", "nur", "das", "Gold", "/", "und", "fun\u00b7de", "mein", "Er\u00b7g\u00f6t\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$(", "KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jm Reichthum und in Gut/ doch weil ich allzu sehr", "tokens": ["Jm", "Reicht\u00b7hum", "und", "in", "Gut", "/", "doch", "weil", "ich", "all\u00b7zu", "sehr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "APPR", "NN", "$(", "ADV", "KOUS", "PPER", "PTKA", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nach Geld und Gold getracht/ und weidlich kunte sch\u00e4tzen/", "tokens": ["Nach", "Geld", "und", "Gold", "ge\u00b7tracht", "/", "und", "weid\u00b7lich", "kun\u00b7te", "sch\u00e4t\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$(", "KON", "ADJD", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So blieb im Tod\u2019 auch nicht mein Mund von Golde leer.", "tokens": ["So", "blieb", "im", "Tod'", "auch", "nicht", "mein", "Mund", "von", "Gol\u00b7de", "leer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ADV", "PTKNEG", "PPOSAT", "NN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}