 "word", "word", "word"], "pos": ["KOUS", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jetzt dein Haupt beginnt zu regen,", "tokens": ["Jetzt", "dein", "Haupt", "be\u00b7ginnt", "zu", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sich die Locken mitbewegen,", "tokens": ["Sich", "die", "Lo\u00b7cken", "mit\u00b7be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dein Bejahen hold versch\u00f6nen \u2013", "tokens": ["Dein", "Be\u00b7ja\u00b7hen", "hold", "ver\u00b7sch\u00f6\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Nein! den s\u00fc\u00dfen Haargeflechten", "tokens": ["Nein", "!", "den", "s\u00fc\u00b7\u00dfen", "Haar\u00b7ge\u00b7flech\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$.", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Soll kein Tadel widerfahren!", "tokens": ["Soll", "kein", "Ta\u00b7del", "wi\u00b7der\u00b7fah\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit so sch\u00f6nen blonden Haaren", "tokens": ["Mit", "so", "sch\u00f6\u00b7nen", "blon\u00b7den", "Haa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wahrlich! w\u00e4re schwer zu rechten!", "tokens": ["Wahr\u00b7lich", "!", "w\u00e4\u00b7re", "schwer", "zu", "rech\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "VAFIN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}