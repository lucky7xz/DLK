{"dta.poem.14658": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Brautfahrt .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.57", "af:0.42"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wie die Sterne matter blinken", "tokens": ["Wie", "die", "Ster\u00b7ne", "mat\u00b7ter", "blin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In die morgenrothe Fluth,", "tokens": ["In", "die", "mor\u00b7gen\u00b7ro\u00b7the", "Fluth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieht sie fern die Berge sinken,", "tokens": ["Sieht", "sie", "fern", "die", "Ber\u00b7ge", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Flammend steigt die hehre Gluth,", "tokens": ["Flam\u00b7mend", "steigt", "die", "heh\u00b7re", "Gluth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ueber'm Spiegel trunkner Wellen", "tokens": ["Ue\u00b7ber'm", "Spie\u00b7gel", "trunk\u00b7ner", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Rauschender die Seegel schwellen.", "tokens": ["Rau\u00b7schen\u00b7der", "die", "See\u00b7gel", "schwel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}