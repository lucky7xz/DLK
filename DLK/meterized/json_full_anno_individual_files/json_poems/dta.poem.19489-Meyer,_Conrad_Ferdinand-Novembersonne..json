{"dta.poem.19489": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Novembersonne.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "In den \u00e4chzenden Gewinden", "tokens": ["In", "den", "\u00e4ch\u00b7zen\u00b7den", "Ge\u00b7win\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat die Kelter sich gedreht,", "tokens": ["Hat", "die", "Kel\u00b7ter", "sich", "ge\u00b7dreht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unter meinen alten Linden", "tokens": ["Un\u00b7ter", "mei\u00b7nen", "al\u00b7ten", "Lin\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Liegt das Laub hoch aufgeweht.", "tokens": ["Liegt", "das", "Laub", "hoch", "auf\u00b7ge\u00b7weht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dieser Erde Werke rasten,", "tokens": ["Die\u00b7ser", "Er\u00b7de", "Wer\u00b7ke", "ras\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schon beginnt die Winterruh \u2014", "tokens": ["Schon", "be\u00b7ginnt", "die", "Win\u00b7ter\u00b7ruh"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sonne, noch mit unverbla\u00dften,", "tokens": ["Son\u00b7ne", ",", "noch", "mit", "un\u00b7ver\u00b7bla\u00df\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "APPR", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Warmen Strahlen wanderst du!", "tokens": ["War\u00b7men", "Strah\u00b7len", "wan\u00b7derst", "du", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ehe sich das Jahr entlaubte,", "tokens": ["E\u00b7he", "sich", "das", "Jahr", "ent\u00b7laub\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gingen, traun, sie m\u00fc\u00dfig nie,", "tokens": ["Gin\u00b7gen", ",", "traun", ",", "sie", "m\u00fc\u00b7\u00dfig", "nie", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVINF", "$,", "PPER", "ADJD", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nun an deinem lichten Haupte", "tokens": ["Nun", "an", "dei\u00b7nem", "lich\u00b7ten", "Haup\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Flammen unbesch\u00e4ftigt sie.", "tokens": ["Flam\u00b7men", "un\u00b7be\u00b7sch\u00e4f\u00b7tigt", "sie", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Erst ein Ackerknecht und Schnitter,", "tokens": ["Erst", "ein", "A\u00b7cker\u00b7knecht", "und", "Schnit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch ein Traubenkoch zuletzt,", "tokens": ["Noch", "ein", "Trau\u00b7ben\u00b7koch", "zu\u00b7letzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bist du jetzt der freie Ritter,", "tokens": ["Bist", "du", "jetzt", "der", "frei\u00b7e", "Rit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der sich auf der Fahrt ergetzt.", "tokens": ["Der", "sich", "auf", "der", "Fahrt", "er\u00b7getzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und die Sch\u00fcler, zu den B\u00e4nken", "tokens": ["Und", "die", "Sch\u00fc\u00b7ler", ",", "zu", "den", "B\u00e4n\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kehrend, gr\u00fc\u00dfen jubelvoll", "tokens": ["Keh\u00b7rend", ",", "gr\u00fc\u00b7\u00dfen", "ju\u00b7bel\u00b7voll"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADJD", "$,", "VVFIN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hingelagert vor den Schenken", "tokens": ["Hin\u00b7ge\u00b7la\u00b7gert", "vor", "den", "Schen\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dich als Musengott Apoll.", "tokens": ["Dich", "als", "Mu\u00b7sen\u00b7gott", "A\u00b7poll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "KOUS", "NN", "NE", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}}}}