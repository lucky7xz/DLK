{"dta.poem.12758": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Recept wider die liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr liebsten jungfern, ihr, so euch das liebes-sieber", "tokens": ["Ihr", "liebs\u00b7ten", "jung\u00b7fern", ",", "ihr", ",", "so", "euch", "das", "lie\u00b7bes\u00b7sie\u00b7ber"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "PPER", "$,", "ADV", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verzehrt den zarten leib, ersch\u00fcttert alle glieder,", "tokens": ["Ver\u00b7zehrt", "den", "zar\u00b7ten", "leib", ",", "er\u00b7sch\u00fct\u00b7tert", "al\u00b7le", "glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein! nehmt doch di\u00df recept, das ich aus schuldigkeit,", "tokens": ["Mein", "!", "nehmt", "doch", "di\u00df", "re\u00b7cept", ",", "das", "ich", "aus", "schul\u00b7dig\u00b7keit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "$.", "VVFIN", "ADV", "PDS", "VVFIN", "$,", "PRELS", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als ein bew\u00e4hrter artzt euch habe zubereit.", "tokens": ["Als", "ein", "be\u00b7w\u00e4hr\u00b7ter", "artzt", "euch", "ha\u00b7be", "zu\u00b7be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "VVFIN", "PPER", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein stirnblat wird f\u00fcrwahr den schmertzen wenig stillen,", "tokens": ["Ein", "stirn\u00b7blat", "wird", "f\u00fcr\u00b7wahr", "den", "schmert\u00b7zen", "we\u00b7nig", "stil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "VVFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Kein s\u00e4lbgen auf den schlaf wird euren wunsch erf\u00fcllen:", "tokens": ["Kein", "s\u00e4lb\u00b7gen", "auf", "den", "schlaf", "wird", "eu\u00b7ren", "wunsch", "er\u00b7f\u00fcl\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "VAFIN", "PPOSAT", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Nehmt eine mandel halb, und zwar das f\u00f6rdertheil,", "tokens": ["Nehmt", "ei\u00b7ne", "man\u00b7del", "halb", ",", "und", "zwar", "das", "f\u00f6r\u00b7der\u00b7theil", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "$,", "KON", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Das ist ein gut recept, und macht die wunden heil.", "tokens": ["Das", "ist", "ein", "gut", "re\u00b7cept", ",", "und", "macht", "die", "wun\u00b7den", "heil", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJD", "VVFIN", "$,", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So welche unter euch die cur nicht lassen gelten,", "tokens": ["So", "wel\u00b7che", "un\u00b7ter", "euch", "die", "cur", "nicht", "las\u00b7sen", "gel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRELS", "APPR", "PPER", "ART", "NN", "PTKNEG", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und die probate kunst vor eitle possen schelten,", "tokens": ["Und", "die", "pro\u00b7ba\u00b7te", "kunst", "vor", "eit\u00b7le", "pos\u00b7sen", "schel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So wird sich bald die braut selbst meiner nehmen an,", "tokens": ["So", "wird", "sich", "bald", "die", "braut", "selbst", "mei\u00b7ner", "neh\u00b7men", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PRF", "ADV", "ART", "VVFIN", "ADV", "PPOSAT", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Fragt sie nur morgen fr\u00fch, was es bey ihr gethan?", "tokens": ["Fragt", "sie", "nur", "mor\u00b7gen", "fr\u00fch", ",", "was", "es", "bey", "ihr", "ge\u00b7than", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADJD", "$,", "PWS", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "---+-+--+--+", "measure": "iambic.tetra.relaxed"}}}}}