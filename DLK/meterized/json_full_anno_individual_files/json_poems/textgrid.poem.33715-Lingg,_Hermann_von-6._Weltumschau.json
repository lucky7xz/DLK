{"textgrid.poem.33715": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "6. Weltumschau", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dort m\u00f6cht' ich weilen an des Ganges Bronnen", "tokens": ["Dort", "m\u00f6cht'", "ich", "wei\u00b7len", "an", "des", "Gan\u00b7ges", "Bron\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf Himalaja's H\u00f6h'n und ungeblendet", "tokens": ["Auf", "Hi\u00b7ma\u00b7la\u00b7ja's", "H\u00f6h'n", "und", "un\u00b7ge\u00b7blen\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "KON", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Schaun, wie den Bergalt\u00e4ren Feuer spendet", "tokens": ["Schaun", ",", "wie", "den", "Ber\u00b7gal\u00b7t\u00e4\u00b7ren", "Feu\u00b7er", "spen\u00b7det"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "NN", "VVFIN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Das ewig neugeborne Licht der Sonnen.", "tokens": ["Das", "e\u00b7wig", "neu\u00b7ge\u00b7bor\u00b7ne", "Licht", "der", "Son\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich schaute, wie zum Tal von Nacht umronnen", "tokens": ["Ich", "schau\u00b7te", ",", "wie", "zum", "Tal", "von", "Nacht", "um\u00b7ron\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN"