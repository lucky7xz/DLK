{"textgrid.poem.51288": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "20.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Indem der wilde Krieg kommt alle zu verjagen,", "tokens": ["In\u00b7dem", "der", "wil\u00b7de", "Krieg", "kommt", "al\u00b7le", "zu", "ver\u00b7ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was ist doch wol zu thun? h\u00f6r, ich wil es dir sagen:", "tokens": ["Was", "ist", "doch", "wol", "zu", "thun", "?", "h\u00f6r", ",", "ich", "wil", "es", "dir", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADV", "PTKZU", "VVINF", "$.", "VVFIN", "$,", "PPER", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ist dir dein Gut verterbt? Begieb dich in Verstand.", "tokens": ["Ist", "dir", "dein", "Gut", "ver\u00b7terbt", "?", "Be\u00b7gieb", "dich", "in", "Ver\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "VVFIN", "$.", "NN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Must du ins Elend gehn? Die Welt ist Vaterland.", "tokens": ["Must", "du", "ins", "E\u00b7lend", "gehn", "?", "Die", "Welt", "ist", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "VVINF", "$.", "ART", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Hast du kein Kleid? Du kommst, und zeuchst auch nackt von hinnen,", "tokens": ["Hast", "du", "kein", "Kleid", "?", "Du", "kommst", ",", "und", "zeuchst", "auch", "nackt", "von", "hin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "NN", "$.", "PPER", "VVFIN", "$,", "KON", "VVFIN", "ADV", "ADJD", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Kein Brod? Gott kan ohn Brod, o Mensch, dein Heil beginnen.", "tokens": ["Kein", "Brod", "?", "Gott", "kan", "ohn", "Brod", ",", "o", "Mensch", ",", "dein", "Heil", "be\u00b7gin\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$.", "NN", "VMFIN", "APPR", "NN", "$,", "FM", "NN", "$,", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Kein Geld? Wie viel hat Geld in Noth und Tod gebracht.", "tokens": ["Kein", "Geld", "?", "Wie", "viel", "hat", "Geld", "in", "Noth", "und", "Tod", "ge\u00b7bracht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$.", "PWAV", "ADV", "VAFIN", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Bist du allein? Gott nihmt auch dich allein in acht.", "tokens": ["Bist", "du", "al\u00b7lein", "?", "Gott", "nihmt", "auch", "dich", "al\u00b7lein", "in", "acht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "$.", "NN", "VVFIN", "ADV", "PPER", "ADV", "APPR", "CARD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Verst\u00f6\u00dfet dich die Welt? Dir bleibt der Himmel offen,", "tokens": ["Ver\u00b7st\u00f6\u00b7\u00dfet", "dich", "die", "Welt", "?", "Dir", "bleibt", "der", "Him\u00b7mel", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Den weder Krieg noch Feind noch Elend ie betroffen.", "tokens": ["Den", "we\u00b7der", "Krieg", "noch", "Feind", "noch", "E\u00b7lend", "ie", "be\u00b7trof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KON", "NN", "ADV", "NN", "ADV", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Auff da\u00df dich aber gar kein Ungl\u00fcck f\u00e4llen kan,", "tokens": ["Auff", "da\u00df", "dich", "a\u00b7ber", "gar", "kein", "Un\u00b7gl\u00fcck", "f\u00e4l\u00b7len", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "ADV", "ADV", "PIAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ob es gleich Tag vor Tag dich spornstreichs drabet an,", "tokens": ["Ob", "es", "gleich", "Tag", "vor", "Tag", "dich", "sporn\u00b7streichs", "dra\u00b7bet", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "APPR", "NN", "PPER", "VVFIN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text":