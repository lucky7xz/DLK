{"dta.poem.11083": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf ihr hertze.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein kind! was ist dein hertz? Ein ort, da reine tugend", "tokens": ["Mein", "kind", "!", "was", "ist", "dein", "hertz", "?", "Ein", "ort", ",", "da", "rei\u00b7ne", "tu\u00b7gend"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "PWS", "VAFIN", "PPOSAT", "NN", "$.", "ART", "NN", "$,", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Beliebte fr\u00f6mmigkeit, und keusche flammen hegt;", "tokens": ["Be\u00b7lieb\u00b7te", "fr\u00f6m\u00b7mig\u00b7keit", ",", "und", "keu\u00b7sche", "flam\u00b7men", "hegt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "ADJA", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es ist ein fester schild, der bey beliebter jugend", "tokens": ["Es", "ist", "ein", "fes\u00b7ter", "schild", ",", "der", "bey", "be\u00b7lieb\u00b7ter", "ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das wort best\u00e4ndigkeit statt seiner losung tr\u00e4gt;", "tokens": ["Das", "wort", "be\u00b7st\u00e4n\u00b7dig\u00b7keit", "statt", "sei\u00b7ner", "lo\u00b7sung", "tr\u00e4gt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es ist die sichre burg, die noch kein feind bestiegen.", "tokens": ["Es", "ist", "die", "sich\u00b7re", "burg", ",", "die", "noch", "kein", "feind", "be\u00b7stie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Solt\u2019 ich darinnen doch nur zur besatzung liegen!", "tokens": ["Solt'", "ich", "da\u00b7rin\u00b7nen", "doch", "nur", "zur", "be\u00b7sat\u00b7zung", "lie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}