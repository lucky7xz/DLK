{"dta.poem.21372": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XVI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Guillotiniren \u2013 erkl\u00e4rte ich ihm \u2013", "tokens": ["Das", "Guil\u00b7lo\u00b7ti\u00b7ni\u00b7ren", "\u2013", "er\u00b7kl\u00e4r\u00b7te", "ich", "ihm", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "VVFIN", "PPER", "PPER", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ist eine neue Methode,", "tokens": ["Ist", "ei\u00b7ne", "neu\u00b7e", "Me\u00b7tho\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Womit man die Leute jeglichen Stands", "tokens": ["Wo\u00b7mit", "man", "die", "Leu\u00b7te", "jeg\u00b7li\u00b7chen", "Stands"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "ART", "NN", "PIAT", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Vom Leben bringt zu Tode.", "tokens": ["Vom", "Le\u00b7ben", "bringt", "zu", "To\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Bey dieser Methode bedient man sich", "tokens": ["Bey", "die\u00b7ser", "Me\u00b7tho\u00b7de", "be\u00b7dient", "man", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PIS", "PRF"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Auch einer neuen Maschine,", "tokens": ["Auch", "ei\u00b7ner", "neu\u00b7en", "Ma\u00b7schi\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die hat erfunden Herr Guillotin,", "tokens": ["Die", "hat", "er\u00b7fun\u00b7den", "Herr", "Guil\u00b7lo\u00b7tin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVPP", "NN", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Drum nennt man sie Guillotine.", "tokens": ["Drum", "nennt", "man", "sie", "Guil\u00b7lo\u00b7ti\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIS", "PPER", "NE", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Du wirst hier an ein Brett geschnallt; \u2013", "tokens": ["Du", "wirst", "hier", "an", "ein", "Brett", "ge\u00b7schnallt", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das senkt sich; \u2013 du wirst geschoben", "tokens": ["Das", "senkt", "sich", ";", "\u2013", "du", "wirst", "ge\u00b7scho\u00b7ben"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PRF", "$.", "$(", "PPER", "VAFIN", "VVPP"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Geschwinde zwischen zwey Pfosten; \u2013 es h\u00e4ngt", "tokens": ["Ge\u00b7schwin\u00b7de", "zwi\u00b7schen", "zwey", "Pfos\u00b7ten", ";", "\u2013", "es", "h\u00e4ngt"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["NN", "APPR", "CARD", "NN", "$.", "$(", "PPER", "VVFIN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ein dreyeckig Beil ganz oben; \u2013", "tokens": ["Ein", "drey\u00b7ec\u00b7kig", "Beil", "ganz", "o\u00b7ben", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "CARD", "NN", "ADV", "ADV", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Man zieht eine Schnur, dann schie\u00dft herab", "tokens": ["Man", "zieht", "ei\u00b7ne", "Schnur", ",", "dann", "schie\u00dft", "her\u00b7ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das Beil, ganz lustig und munter; \u2013", "tokens": ["Das", "Beil", ",", "ganz", "lus\u00b7tig", "und", "mun\u00b7ter", ";", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Bey dieser Gelegenheit f\u00e4llt dein Kopf", "tokens": ["Bey", "die\u00b7ser", "Ge\u00b7le\u00b7gen\u00b7heit", "f\u00e4llt", "dein", "Kopf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "In einen Sack hinunter.", "tokens": ["In", "ei\u00b7nen", "Sack", "hin\u00b7un\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}