{"dta.poem.21456": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "O lass\u2019 mich k\u00fcssen dein Gewand \u2026", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "O lass\u2019 mich k\u00fcssen dein Gewand,", "tokens": ["O", "lass'", "mich", "k\u00fcs\u00b7sen", "dein", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du Einzige-Holde, Heilig-Reine,", "tokens": ["Du", "Ein\u00b7zi\u00b7ge\u00b7Hol\u00b7de", ",", "Hei\u00b7lig\u00b7Rei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "NN", "$,", "NE", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich wei\u00df: da\u00df ich in dir nur fand", "tokens": ["Ich", "wei\u00df", ":", "da\u00df", "ich", "in", "dir", "nur", "fand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "KOUS", "PPER", "APPR", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Liebe Gl\u00fcck, nach dem ich weine.", "tokens": ["Der", "Lie\u00b7be", "Gl\u00fcck", ",", "nach", "dem", "ich", "wei\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ents\u00fchne mich vom irdischen Tand,", "tokens": ["Ent\u00b7s\u00fch\u00b7ne", "mich", "vom", "ir\u00b7di\u00b7schen", "Tand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Ganz sei dein F\u00fchlen auch das meine,", "tokens": ["Ganz", "sei", "dein", "F\u00fch\u00b7len", "auch", "das", "mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Lass\u2019 in der Liebe Wunderland", "tokens": ["Lass'", "in", "der", "Lie\u00b7be", "Wun\u00b7der\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Uns tr\u00e4umen s\u00fc\u00df das Ewig-Eine.", "tokens": ["Uns", "tr\u00e4u\u00b7men", "s\u00fc\u00df", "das", "E\u00b7wig\u00b7Ei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Du giebst mir deine Zauberhand,", "tokens": ["Du", "giebst", "mir", "dei\u00b7ne", "Zau\u00b7ber\u00b7hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Tief unter uns stirbt das Gemeine \u2014", "tokens": ["Tief", "un\u00b7ter", "uns", "stirbt", "das", "Ge\u00b7mei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "VVFIN", "ART", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.11": {"text": "Du f\u00fchr\u2019st mich an des Abgrunds Rand", "tokens": ["Du", "f\u00fchr'st", "mich", "an", "des", "Ab\u00b7grunds", "Rand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Vorbei zum ewigen Sonnenscheine.", "tokens": ["Vor\u00b7bei", "zum", "e\u00b7wi\u00b7gen", "Son\u00b7nen\u00b7schei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}