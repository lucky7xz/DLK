{"dta.poem.23523": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "St\u00f6pselzieher.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn es keine Flaschen g\u00e4be,", "tokens": ["Wenn", "es", "kei\u00b7ne", "Fla\u00b7schen", "g\u00e4\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00fcrden keine St\u00f6psel sein,", "tokens": ["W\u00fcr\u00b7den", "kei\u00b7ne", "St\u00f6p\u00b7sel", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wie einst dem Zeus die Hebe", "tokens": ["Und", "wie", "einst", "dem", "Zeus", "die", "He\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADV", "ART", "NE", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "So kredenzt' ich dir den Wein.", "tokens": ["So", "kre\u00b7denzt'", "ich", "dir", "den", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Aber leider wird gezogen", "tokens": ["A\u00b7ber", "lei\u00b7der", "wird", "ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jetzt auf Flaschen nur der Wein", "tokens": ["Jetzt", "auf", "Fla\u00b7schen", "nur", "der", "Wein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wie einen Demagogen", "tokens": ["Und", "wie", "ei\u00b7nen", "De\u00b7ma\u00b7go\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sperret man den Edlen ein.", "tokens": ["Sper\u00b7ret", "man", "den", "Ed\u00b7len", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und ein St\u00f6psel h\u00e4lt die Wache", "tokens": ["Und", "ein", "St\u00f6p\u00b7sel", "h\u00e4lt", "die", "Wa\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Wie ein Scherge Tag und Nacht,", "tokens": ["Wie", "ein", "Scher\u00b7ge", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und er sitzt ihm auf dem Dache,", "tokens": ["Und", "er", "sitzt", "ihm", "auf", "dem", "Da\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df er sich nicht mausig macht.", "tokens": ["Da\u00df", "er", "sich", "nicht", "mau\u00b7sig", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch dein R\u00e4cher ist vorhanden,", "tokens": ["Doch", "dein", "R\u00e4\u00b7cher", "ist", "vor\u00b7han\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur Geduld, du edler Wein!", "tokens": ["Nur", "Ge\u00b7duld", ",", "du", "ed\u00b7ler", "Wein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und er wird aus deinen Banden", "tokens": ["Und", "er", "wird", "aus", "dei\u00b7nen", "Ban\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dich zu rechter Zeit befrein.", "tokens": ["Dich", "zu", "rech\u00b7ter", "Zeit", "be\u00b7fr\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und wie hei\u00dft der brave R\u00e4cher,", "tokens": ["Und", "wie", "hei\u00dft", "der", "bra\u00b7ve", "R\u00e4\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der den Wein befreien kann?", "tokens": ["Der", "den", "Wein", "be\u00b7frei\u00b7en", "kann", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "O ihr wisst es, frohe Zecher,", "tokens": ["O", "ihr", "wisst", "es", ",", "fro\u00b7he", "Ze\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPER", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "St\u00f6pselzieher hei\u00dft der Mann.", "tokens": ["St\u00f6p\u00b7sel\u00b7zie\u00b7her", "hei\u00dft", "der", "Mann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "St\u00f6pselzieher! hoch erheben", "tokens": ["St\u00f6p\u00b7sel\u00b7zie\u00b7her", "!", "hoch", "er\u00b7he\u00b7ben"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$.", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lasst uns ihn bei Sang und Wein:", "tokens": ["Lasst", "uns", "ihn", "bei", "Sang", "und", "Wein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle, alle sollen leben,", "tokens": ["Al\u00b7le", ",", "al\u00b7le", "sol\u00b7len", "le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PIS", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "St\u00f6pselzieher gro\u00df und klein!", "tokens": ["St\u00f6p\u00b7sel\u00b7zie\u00b7her", "gro\u00df", "und", "klein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und ein jeder Hauswirth denke", "tokens": ["Und", "ein", "je\u00b7der", "Haus\u00b7wirth", "den\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "PIAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heuer und zu jeder Frist,", "tokens": ["Heu\u00b7er", "und", "zu", "je\u00b7der", "Frist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df kein ordentlich Getr\u00e4nke", "tokens": ["Da\u00df", "kein", "or\u00b7dent\u00b7lich", "Ge\u00b7tr\u00e4n\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "ADJD", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ohne St\u00f6pselzieher ist.", "tokens": ["Oh\u00b7ne", "St\u00f6p\u00b7sel\u00b7zie\u00b7her", "ist", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}