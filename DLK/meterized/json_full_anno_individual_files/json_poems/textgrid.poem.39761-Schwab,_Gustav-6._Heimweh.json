{"textgrid.poem.39761": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "6. Heimweh", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es wecken mich Gedanken auf;", "tokens": ["Es", "we\u00b7cken", "mich", "Ge\u00b7dan\u00b7ken", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch schl\u00e4ft ringsum die Nacht.", "tokens": ["Noch", "schl\u00e4ft", "ring\u00b7sum", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und schon beginn' ich meinen Lauf,", "tokens": ["Und", "schon", "be\u00b7ginn'", "ich", "mei\u00b7nen", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Mond schleicht vor mir sacht.", "tokens": ["Der", "Mond", "schleicht", "vor", "mir", "sacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie \u00e4ngstet mich sein blaues Licht,", "tokens": ["Wie", "\u00e4ngs\u00b7tet", "mich", "sein", "blau\u00b7es", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie schweigt der lange Wald!", "tokens": ["Wie", "schweigt", "der", "lan\u00b7ge", "Wald", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kein L\u00fcftchen, keine Quelle spricht,", "tokens": ["Kein", "L\u00fcft\u00b7chen", ",", "kei\u00b7ne", "Quel\u00b7le", "spricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Welt starrt leichenkalt.", "tokens": ["Die", "Welt", "starrt", "lei\u00b7chen\u00b7kalt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und ein Gef\u00fchl von schlimmer Art", "tokens": ["Und", "ein", "Ge\u00b7f\u00fchl", "von", "schlim\u00b7mer", "Art"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schn\u00fcrt mir die Seele zu:", "tokens": ["Schn\u00fcrt", "mir", "die", "See\u00b7le", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Fehlst, Sch\u00f6pfer, de\u00df Allgegenwart", "tokens": ["Fehlst", ",", "Sch\u00f6p\u00b7fer", ",", "de\u00df", "All\u00b7ge\u00b7gen\u00b7wart"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Natur sonst f\u00fchlt, auch du? \u2013", "tokens": ["Na\u00b7tur", "sonst", "f\u00fchlt", ",", "auch", "du", "?", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "ADV", "VVFIN", "$,", "ADV", "PPER", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "W\u00e4r' ich zu Haus mit meinem Schmerz", "tokens": ["W\u00e4r'", "ich", "zu", "Haus", "mit", "mei\u00b7nem", "Schmerz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bei meiner Jugend Weib,", "tokens": ["Bei", "mei\u00b7ner", "Ju\u00b7gend", "Weib", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und legt' ihr an das treue Herz", "tokens": ["Und", "legt'", "ihr", "an", "das", "treu\u00b7e", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den zagen Geist und Leib!", "tokens": ["Den", "za\u00b7gen", "Geist", "und", "Leib", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ob sie wohl jetzt in Frieden ruht,", "tokens": ["Ob", "sie", "wohl", "jetzt", "in", "Frie\u00b7den", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Kinder um sie her?", "tokens": ["Die", "Kin\u00b7der", "um", "sie", "her", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kreist ihr und ihnen leicht das Blut,", "tokens": ["Kreist", "ihr", "und", "ih\u00b7nen", "leicht", "das", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "PPER", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und athmet keines schwer?", "tokens": ["Und", "ath\u00b7met", "kei\u00b7nes", "schwer", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wei\u00df ich, ob eines wimmernd nicht", "tokens": ["Wei\u00df", "ich", ",", "ob", "ei\u00b7nes", "wim\u00b7mernd", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "ART", "ADJD", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Mutter pl\u00f6tzlich weckt,", "tokens": ["Die", "Mut\u00b7ter", "pl\u00f6tz\u00b7lich", "weckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ob nicht sein gl\u00fchend Angesicht", "tokens": ["Ob", "nicht", "sein", "gl\u00fc\u00b7hend", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Fiebers Scharlach deckt?", "tokens": ["Des", "Fie\u00b7bers", "Schar\u00b7lach", "deckt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wald, la\u00df mich los, du bist ein Grab!", "tokens": ["Wald", ",", "la\u00df", "mich", "los", ",", "du", "bist", "ein", "Grab", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "PTKVZ", "$,", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Mond, scheine nicht so bleich!", "tokens": ["Mond", ",", "schei\u00b7ne", "nicht", "so", "bleich", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PTKNEG", "ADV", "ADJD", "$."], "meter": "++-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O werd' ein Fl\u00fcgel, Wanderstab!", "tokens": ["O", "werd'", "ein", "Fl\u00fc\u00b7gel", ",", "Wan\u00b7der\u00b7stab", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "PWAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wildfremder Boden, weich!", "tokens": ["Wild\u00b7frem\u00b7der", "Bo\u00b7den", ",", "weich", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.8": {"line.1": {"text": "Und jetzt umhaucht es kr\u00e4ftig mich;", "tokens": ["Und", "jetzt", "um\u00b7haucht", "es", "kr\u00e4f\u00b7tig", "mich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Fr\u00fche, bist es du?", "tokens": ["O", "Fr\u00fc\u00b7he", ",", "bist", "es", "du", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VAFIN", "PPER", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der gr\u00fcne Kerker \u00f6ffnet sich!", "tokens": ["Der", "gr\u00fc\u00b7ne", "Ker\u00b7ker", "\u00f6ff\u00b7net", "sich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur zu, nur immer zu!", "tokens": ["Nur", "zu", ",", "nur", "im\u00b7mer", "zu", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Schon liegt die Welt vor mir in Duft,", "tokens": ["Schon", "liegt", "die", "Welt", "vor", "mir", "in", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schon perlet auf der Au'", "tokens": ["Schon", "per\u00b7let", "auf", "der", "Au'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Kind des Mondes und der Luft,", "tokens": ["Das", "Kind", "des", "Mon\u00b7des", "und", "der", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der morgenhelle Thau.", "tokens": ["Der", "mor\u00b7gen\u00b7hel\u00b7le", "Thau", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Dort steigt der Sonne goldnes Rund,", "tokens": ["Dort", "steigt", "der", "Son\u00b7ne", "gold\u00b7nes", "Rund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Gott ist wieder da.", "tokens": ["Und", "Gott", "ist", "wie\u00b7der", "da", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich frage bang: sind sie gesund?", "tokens": ["Ich", "fra\u00b7ge", "bang", ":", "sind", "sie", "ge\u00b7sund", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$.", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Licht sagt l\u00e4chelnd: Ja!", "tokens": ["Das", "Licht", "sagt", "l\u00e4\u00b7chelnd", ":", "Ja", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$.", "PTKANT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Es wecken mich Gedanken auf;", "tokens": ["Es", "we\u00b7cken", "mich", "Ge\u00b7dan\u00b7ken", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch schl\u00e4ft ringsum die Nacht.", "tokens": ["Noch", "schl\u00e4ft", "ring\u00b7sum", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und schon beginn' ich meinen Lauf,", "tokens": ["Und", "schon", "be\u00b7ginn'", "ich", "mei\u00b7nen", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Mond schleicht vor mir sacht.", "tokens": ["Der", "Mond", "schleicht", "vor", "mir", "sacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Wie \u00e4ngstet mich sein blaues Licht,", "tokens": ["Wie", "\u00e4ngs\u00b7tet", "mich", "sein", "blau\u00b7es", "Licht", ","], "to