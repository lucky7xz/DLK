{"textgrid.poem.63791": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "2. Rat der G\u00f6tter", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aphrodite in eigner Person und Eros und Peitho", "tokens": ["A\u00b7phro\u00b7di\u00b7te", "in", "eig\u00b7ner", "Per\u00b7son", "und", "E\u00b7ros", "und", "Pei\u00b7tho"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ADJA", "NN", "KON", "NN", "KON", "NE"], "meter": "-----+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Um die beiden bem\u00fcht, die sich zu gut nur verstehen?", "tokens": ["Um", "die", "bei\u00b7den", "be\u00b7m\u00fcht", ",", "die", "sich", "zu", "gut", "nur", "ver\u00b7ste\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "PIAT", "VVFIN", "$,", "PRELS", "PRF", "PTKA", "ADJD", "ADV", "VVINF", "$."], "meter": "--+--+---+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Helena senkt schamgl\u00fchend das Kinn, der kecke Verf\u00fchrer", "tokens": ["He\u00b7le\u00b7na", "senkt", "scham\u00b7gl\u00fc\u00b7hend", "das", "Kinn", ",", "der", "ke\u00b7cke", "Ver\u00b7f\u00fch\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADJD", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Scheint zu erw\u00e4gen, ob auch ehrbar und sittlich der Raub.", "tokens": ["Scheint", "zu", "er\u00b7w\u00e4\u00b7gen", ",", "ob", "auch", "ehr\u00b7bar", "und", "sitt\u00b7lich", "der", "Raub", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKZU", "VVINF", "$,", "KOUS", "ADV", "ADJD", "KON", "ADJD", "ART", "NN", "$."], "meter": "---+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "O die Losen! Sie spielen die Sch\u00fcchternen, m\u00f6chten den Schein sich", "tokens": ["O", "die", "Lo\u00b7sen", "!", "Sie", "spie\u00b7len", "die", "Sch\u00fcch\u00b7ter\u00b7nen", ",", "m\u00f6ch\u00b7ten", "den", "Schein", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "$,", "VMFIN", "ART", "NN", "PRF"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Geben, als folgten sie nur z\u00f6gernd der Himmlischen Rat.", "tokens": ["Ge\u00b7ben", ",", "als", "folg\u00b7ten", "sie", "nur", "z\u00f6\u00b7gernd", "der", "Himm\u00b7li\u00b7schen", "Rat", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOKOM", "VVFIN", "PPER", "ADV", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "---+--++--+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "La\u00dft sie nur zwei Minuten allein, und Helena liegt in", "tokens": ["La\u00dft", "sie", "nur", "zwei", "Mi\u00b7nu\u00b7ten", "al\u00b7lein", ",", "und", "He\u00b7le\u00b7na", "liegt", "in"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "CARD", "NN", "ADV", "$,", "KON", "NE", "VVFIN", "APPR"], "meter": "-+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "Paris' Armen; es kann Peitho noch lernen von ihm.", "tokens": ["Pa\u00b7ris'", "Ar\u00b7men", ";", "es", "kann", "Pei\u00b7tho", "noch", "ler\u00b7nen", "von", "ihm", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PPER", "VMFIN", "NE", "ADV", "VVINF", "APPR", "PPER", "$.