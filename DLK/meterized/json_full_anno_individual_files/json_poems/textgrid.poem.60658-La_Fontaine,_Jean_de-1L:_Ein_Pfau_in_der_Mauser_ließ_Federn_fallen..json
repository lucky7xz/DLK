{"textgrid.poem.60658": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Pfau in der Mauser lie\u00df Federn fallen.", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Pfau in der Mauser lie\u00df Federn fallen.", "tokens": ["Ein", "Pfau", "in", "der", "Mau\u00b7ser", "lie\u00df", "Fe\u00b7dern", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "NN", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Eine Elster nahm sie und legte sie an,", "tokens": ["Ei\u00b7ne", "Els\u00b7ter", "nahm", "sie", "und", "leg\u00b7te", "sie", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Stolzierte unter andre Pfauen dann", "tokens": ["Stol\u00b7zier\u00b7te", "un\u00b7ter", "and\u00b7re", "Pfau\u00b7en", "dann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und meinte, sie m\u00fcsse allen", "tokens": ["Und", "mein\u00b7te", ",", "sie", "m\u00fcs\u00b7se", "al\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PPER", "VMFIN", "PIAT"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Als Sch\u00f6nste wohlgefallen.", "tokens": ["Als", "Sch\u00f6ns\u00b7te", "wohl\u00b7ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Doch irgendeiner erkannte sie.", "tokens": ["Doch", "ir\u00b7gend\u00b7ei\u00b7ner", "er\u00b7kann\u00b7te", "sie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Man beschimpfte, verlachte, verspottete sie,", "tokens": ["Man", "be\u00b7schimpf\u00b7te", ",", "ver\u00b7lach\u00b7te", ",", "ver\u00b7spot\u00b7te\u00b7te", "sie", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "PPER", "$,"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Man hielt sie zum besten, man pfiff sie aus", "tokens": ["Man", "hielt", "sie", "zum", "bes\u00b7ten", ",", "man", "pfiff", "sie", "aus"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "APPRART", "ADJA", "$,", "PIS", "VVFIN", "PPER", "PTKVZ"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "Und ri\u00df ihr die fremden Federn heraus.", "tokens": ["Und", "ri\u00df", "ihr", "die", "frem\u00b7den", "Fe\u00b7dern", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Man hat sie zu ihrer Sippe gehetzt,", "tokens": ["Man", "hat", "sie", "zu", "ih\u00b7rer", "Sip\u00b7pe", "ge\u00b7hetzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Dort wurde sie auch vor die T\u00fcr gesetzt.", "tokens": ["Dort", "wur\u00b7de", "sie", "auch", "vor", "die", "T\u00fcr", "ge\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "'s gibt viele Elstern wie die unsrer Fabel,", "tokens": ["'s", "gibt", "vie\u00b7le", "Els\u00b7tern", "wie", "die", "uns\u00b7rer", "Fa\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Doch mit einer Nase statt einem Schnabel,", "tokens": ["Doch", "mit", "ei\u00b7ner", "Na\u00b7se", "statt", "ei\u00b7nem", "Schna\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Die schm\u00fccken sich unentwegt", "tokens": ["Die", "schm\u00fc\u00b7cken", "sich", "un\u00b7ent\u00b7wegt"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PRF", "ADJD"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mit Sachen, die andere abgelegt.", "tokens": ["Mit", "Sa\u00b7chen", ",", "die", "an\u00b7de\u00b7re", "ab\u00b7ge\u00b7legt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PIS", "VVPP", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Plagiatoren pflegt man sie zu nennen.", "tokens": ["Pla\u00b7gi\u00b7a\u00b7to\u00b7ren", "pflegt", "man", "sie", "zu", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "PPER", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6