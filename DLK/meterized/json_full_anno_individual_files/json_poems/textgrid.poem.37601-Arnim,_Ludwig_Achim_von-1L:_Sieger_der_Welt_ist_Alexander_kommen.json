{"textgrid.poem.37601": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sieger der Welt ist Alexander kommen", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieger der Welt ist Alexander kommen", "tokens": ["Sie\u00b7ger", "der", "Welt", "ist", "A\u00b7lex\u00b7an\u00b7der", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "VAFIN", "NE", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum Abendstrand der Welt,", "tokens": ["Zum", "A\u00b7bend\u00b7strand", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zum Greis hinaufgeklommen,", "tokens": ["Zum", "Greis", "hin\u00b7auf\u00b7ge\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der ihm sein Zeichen stellt.", "tokens": ["Der", "ihm", "sein", "Zei\u00b7chen", "stellt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sterne des Tages sich ihm zuerst da zeigen,", "tokens": ["Ster\u00b7ne", "des", "Ta\u00b7ges", "sich", "ihm", "zu\u00b7erst", "da", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PRF", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Nun sieht er erst das Meer,", "tokens": ["Nun", "sieht", "er", "erst", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sieht Well' auf Welle steigen,", "tokens": ["Sieht", "Well'", "auf", "Wel\u00b7le", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und alles Treiben leer.", "tokens": ["Und", "al\u00b7les", "Trei\u00b7ben", "leer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Kreise im Kreis der Alte ruhig drehet,", "tokens": ["Krei\u00b7se", "im", "Kreis", "der", "Al\u00b7te", "ru\u00b7hig", "dre\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Und er versteht sie nicht,", "tokens": ["Und", "er", "ver\u00b7steht", "sie", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sein Blick sich da ergehet,", "tokens": ["Sein", "Blick", "sich", "da", "er\u00b7ge\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo Meer zum Himmel dicht.", "tokens": ["Wo", "Meer", "zum", "Him\u00b7mel", "dicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Schwindelnd er sieht vom steilen Felsen nieder", "tokens": ["Schwin\u00b7delnd", "er", "sieht", "vom", "stei\u00b7len", "Fel\u00b7sen", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "PTKVZ"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Die aufgethane Lust,", "tokens": ["Die", "auf\u00b7ge\u00b7tha\u00b7ne", "Lust", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie alles schwingt die Glieder", "tokens": ["Wie", "al\u00b7les", "schwingt", "die", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und alles drehen mu\u00dft'.", "tokens": ["Und", "al\u00b7les", "dre\u00b7hen", "mu\u00dft'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u00bbk\u00fcnde mir, Greis, ist nichts mehr zu vollbringen,", "tokens": ["\u00bb", "k\u00fcn\u00b7de", "mir", ",", "Greis", ",", "ist", "nichts", "mehr", "zu", "voll\u00b7brin\u00b7gen", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "$,", "NN", "$,", "VAFIN", "PIS", "PIS", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ist hier begr\u00e4nzt die Welt?", "tokens": ["Ist", "hier", "be\u00b7gr\u00e4nzt", "die", "Welt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kann ich zum Thron nicht dringen,", "tokens": ["Kann", "ich", "zum", "Thron", "nicht", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der leuchtend hingestellt?\u00ab", "tokens": ["Der", "leuch\u00b7tend", "hin\u00b7ge\u00b7stellt", "?", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJD", "VVPP", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Halte den Muth und f\u00fcrchte nur dich selber,", "tokens": ["Hal\u00b7te", "den", "Muth", "und", "f\u00fcrch\u00b7te", "nur", "dich", "sel\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "KON", "VVFIN", "ADV", "PPER", "ADV", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Du K\u00f6nig aller Welt,", "tokens": ["Du", "K\u00f6\u00b7nig", "al\u00b7ler", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Eiche st\u00fcrzet W\u00e4lder,", "tokens": ["Die", "Ei\u00b7che", "st\u00fcr\u00b7zet", "W\u00e4l\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn sie herniederf\u00e4llt.", "tokens": ["Wenn", "sie", "her\u00b7nie\u00b7der\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Eines du thust, und willst es doch gern meiden,", "tokens": ["Ei\u00b7nes", "du", "thust", ",", "und", "willst", "es", "doch", "gern", "mei\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PPER", "VVFIN", "$,", "KON", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Den Vater t\u00f6dtest du,", "tokens": ["Den", "Va\u00b7ter", "t\u00f6d\u00b7test", "du", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Welt soll dir's verleiden,", "tokens": ["Die", "Welt", "soll", "dir's", "ver\u00b7lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Menschen z\u00e4hlen zu.", "tokens": ["Den", "Men\u00b7schen", "z\u00e4h\u00b7len", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Nieder hinab, du Sch\u00e4nder meines Glaubens,", "tokens": ["Nie\u00b7der", "hin\u00b7ab", ",", "du", "Sch\u00e4n\u00b7der", "mei\u00b7nes", "Glau\u00b7bens", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "PPER", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom Gott ich rein entspro\u00df,", "tokens": ["Vom", "Gott", "ich", "rein", "ent\u00b7spro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sink' mit den wilden Tauben", "tokens": ["Sink'", "mit", "den", "wil\u00b7den", "Tau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und l\u00fcg' im Meeresschoo\u00df.", "tokens": ["Und", "l\u00fcg'", "im", "Mee\u00b7res\u00b7schoo\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "\u00bbdank sei dir, Sohn, du bist's, ich sterb' dein Vater,", "tokens": ["\u00bb", "dank", "sei", "dir", ",", "Sohn", ",", "du", "bist's", ",", "ich", "sterb'", "dein", "Va\u00b7ter", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "VAFIN", "PPER", "$,", "NN", "$,", "PPER", "NE", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vers\u00f6hn' den G\u00f6ttern dich,", "tokens": ["Ver\u00b7s\u00f6hn'", "den", "G\u00f6t\u00b7tern", "dich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die deines Gl\u00fcckes, deiner Thaten", "tokens": ["Die", "dei\u00b7nes", "Gl\u00fc\u00b7ckes", ",", "dei\u00b7ner", "Tha\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Voll Neid erz\u00fcrnten sich.\u00ab", "tokens": ["Voll", "Neid", "er\u00b7z\u00fcrn\u00b7ten", "sich", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "NN", "VVFIN", "PRF", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Wahrlich der Baum erstirbt in hohem Gl\u00fccke,", "tokens": ["Wahr\u00b7lich", "der", "Baum", "er\u00b7stirbt", "in", "ho\u00b7hem", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Den Sch\u00f6\u00dflingsschatten stickt,", "tokens": ["Den", "Sch\u00f6\u00df\u00b7lings\u00b7schat\u00b7ten", "stickt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So gl\u00e4nzt im h\u00f6chsten Blicke,", "tokens": ["So", "gl\u00e4nzt", "im", "h\u00f6chs\u00b7ten", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Sohnes Licht erdr\u00fcckt.", "tokens": ["Den", "Soh\u00b7nes", "Licht", "er\u00b7dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Wissen st\u00fcrz Wissen war des Alten Wille,", "tokens": ["Wis\u00b7sen", "st\u00fcrz", "Wis\u00b7sen", "war", "des", "Al\u00b7ten", "Wil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Das Wissen wird gebeugt", "tokens": ["Das", "Wis\u00b7sen", "wird", "ge\u00b7beugt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In tiefe Meeresstille,", "tokens": ["In", "tie\u00b7fe", "Mee\u00b7res\u00b7stil\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn es das Thun gezeugt.", "tokens": ["Wenn", "es", "das", "Thun", "ge\u00b7zeugt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Also gelagert scheinen alle Fl\u00f6tze", "tokens": ["Al\u00b7so", "ge\u00b7la\u00b7gert", "schei\u00b7nen", "al\u00b7le", "Fl\u00f6t\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "VVFIN", "PIAT", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Des Weltbaus Schicht auf Schicht,", "tokens": ["Des", "Welt\u00b7baus", "Schicht", "auf", "Schicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er folget dem Gesetze:", "tokens": ["Er", "fol\u00b7get", "dem", "Ge\u00b7set\u00b7ze", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u00bbdas H\u00f6chste komm zum Licht.\u00ab", "tokens": ["\u00bb", "das", "H\u00f6chs\u00b7te", "komm", "zum", "Licht", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "ADJA", "VVFIN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Sieger der Welt ist Alexander kommen", "tokens": ["Sie\u00b7ger", "der", "Welt", "ist", "A\u00b7lex\u00b7an\u00b7der", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "VAFIN", "NE", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum Abendstrand der Welt,", "tokens": ["Zum", "A\u00b7bend\u00b7strand", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zum Greis hinaufgeklommen,", "tokens": ["Zum", "Greis", "hin\u00b7auf\u00b7ge\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der ihm sein Zeichen stellt.", "tokens": ["Der", "ihm", "sein", "Zei\u00b7chen", "stellt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Sterne des Tages sich ihm zuerst da zeigen,", "tokens": ["Ster\u00b7ne", "des", "Ta\u00b7ges", "sich", "ihm", "zu\u00b7erst", "da", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PRF", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Nun sieht er erst das Meer,", "tokens": ["Nun", "sieht", "er", "erst", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sieht Well' auf Welle steigen,", "tokens": ["Sieht", "Well'", "auf", "Wel\u00b7le", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und alles Treiben leer.", "tokens": ["Und", "al\u00b7les", "Trei\u00b7ben", "leer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Kreise im Kreis der Alte ruhig drehet,", "tokens": ["Krei\u00b7se", "im", "Kreis", "der", "Al\u00b7te", "ru\u00b7hig", "dre\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "ADJD", "VV