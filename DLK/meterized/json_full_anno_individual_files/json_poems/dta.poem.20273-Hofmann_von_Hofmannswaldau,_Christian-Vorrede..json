{"dta.poem.20273": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Vorrede.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was kan ein solcher herr f\u00fcr kluge sinnen haben/", "tokens": ["Was", "kan", "ein", "sol\u00b7cher", "herr", "f\u00fcr", "klu\u00b7ge", "sin\u00b7nen", "ha\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "PIAT", "NN", "APPR", "ADJA", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem allzeit die vernunfft im becher liegt begraben/", "tokens": ["Dem", "all\u00b7zeit", "die", "ver\u00b7nunfft", "im", "be\u00b7cher", "liegt", "be\u00b7gra\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "APPRART", "ADJA", "VVFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und auff dem glase schwimmt? Wer nichts f\u00fcr leut und land", "tokens": ["Und", "auff", "dem", "gla\u00b7se", "schwimmt", "?", "Wer", "nichts", "f\u00fcr", "leut", "und", "land"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "VVFIN", "$.", "PWS", "PIS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als wein vergossen hat/ der macht sich zwar bekandt/", "tokens": ["Als", "wein", "ver\u00b7gos\u00b7sen", "hat", "/", "der", "macht", "sich", "zwar", "be\u00b7kandt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVPP", "VAFIN", "$(", "ART", "VVFIN", "PRF", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch nicht durch tapfferkeit: mu\u00df b\u00f6sen menschen trauen/", "tokens": ["Doch", "nicht", "durch", "tapf\u00b7fer\u00b7keit", ":", "mu\u00df", "b\u00f6\u00b7sen", "men\u00b7schen", "trau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "NN", "$.", "VMFIN", "VVINF", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die ihn/ und sich/ und mich offt zuverkauffen schauen;", "tokens": ["Die", "ihn", "/", "und", "sich", "/", "und", "mich", "offt", "zu\u00b7ver\u00b7kauf\u00b7fen", "schau\u00b7en", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$(", "KON", "PRF", "$(", "KON", "PPER", "ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Ist seiner diener knecht/ und trincket durch den wein", "tokens": ["Ist", "sei\u00b7ner", "die\u00b7ner", "knecht", "/", "und", "trin\u00b7cket", "durch", "den", "wein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "$(", "KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "(wie theuer wasser doch!) viel tausend thr\u00e4nen ein.", "tokens": ["(", "wie", "theu\u00b7er", "was\u00b7ser", "doch", "!", ")", "viel", "tau\u00b7send", "thr\u00e4\u00b7nen", "ein", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "ADJD", "ADJD", "ADV", "$.", "$(", "ADV", "CARD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}