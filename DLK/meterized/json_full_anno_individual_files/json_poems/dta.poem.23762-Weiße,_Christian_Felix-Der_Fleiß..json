{"dta.poem.23762": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "Der Flei\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20643-5", "language": ["de:0.99"], "booktitle": "Wei\u00dfe, Christian Felix: Lieder f\u00fcr Kinder. Leipzig, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "S\u00fc\u00dfer, angenehmer Flei\u00df!", "tokens": ["S\u00fc\u00b7\u00dfer", ",", "an\u00b7ge\u00b7neh\u00b7mer", "Flei\u00df", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "O wie herrlich ist der Prei\u00df,", "tokens": ["O", "wie", "herr\u00b7lich", "ist", "der", "Prei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Den er jedem J\u00fcngling beut,", "tokens": ["Den", "er", "je\u00b7dem", "J\u00fcng\u00b7ling", "beut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der ihm seine Kr\u00e4fte weiht.", "tokens": ["Der", "ihm", "sei\u00b7ne", "Kr\u00e4f\u00b7te", "weiht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn die Langenweile g\u00e4hnt", "tokens": ["Wenn", "die", "Lan\u00b7gen\u00b7wei\u00b7le", "g\u00e4hnt"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und sich krank nach Possen sehnt,", "tokens": ["Und", "sich", "krank", "nach", "Pos\u00b7sen", "sehnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00fcpft in froher Th\u00e4tigkeit", "tokens": ["H\u00fcpft", "in", "fro\u00b7her", "Th\u00e4\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die ihm nie zu lange Zeit.", "tokens": ["Die", "ihm", "nie", "zu", "lan\u00b7ge", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ja, auf seidnen Schwingen fliehn", "tokens": ["Ja", ",", "auf", "seid\u00b7nen", "Schwin\u00b7gen", "fliehn"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Seine Stunden vor ihm hin:", "tokens": ["Sei\u00b7ne", "Stun\u00b7den", "vor", "ihm", "hin", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Den verlohrnen Augenblick,", "tokens": ["Den", "ver\u00b7lohr\u00b7nen", "Au\u00b7gen\u00b7blick", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nichts sonst, w\u00fcnscht er sich zur\u00fcck.", "tokens": ["Nichts", "sonst", ",", "w\u00fcnscht", "er", "sich", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "$,", "VVFIN", "PPER", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Er ist stark, gesund und frisch,", "tokens": ["Er", "ist", "stark", ",", "ge\u00b7sund", "und", "frisch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Arbeit w\u00fcrzet ihm den Tisch,", "tokens": ["Ar\u00b7beit", "w\u00fcr\u00b7zet", "ihm", "den", "Tisch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und kein kranker Ekel schleicht", "tokens": ["Und", "kein", "kran\u00b7ker", "E\u00b7kel", "schleicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich zu seiner Mahlzeit leicht.", "tokens": ["Sich", "zu", "sei\u00b7ner", "Mahl\u00b7zeit", "leicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wenn er winkt, dr\u00fcckt ihm die Ruh", "tokens": ["Wenn", "er", "winkt", ",", "dr\u00fcckt", "ihm", "die", "Ruh"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ART", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Seine Augen willig zu:", "tokens": ["Sei\u00b7ne", "Au\u00b7gen", "wil\u00b7lig", "zu", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nie hat ihn ein Traum geweckt,", "tokens": ["Nie", "hat", "ihn", "ein", "Traum", "ge\u00b7weckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.4": {"text": "Der im Schlummer ihn geschreckt.", "tokens": ["Der", "im", "Schlum\u00b7mer", "ihn", "ge\u00b7schreckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Er begegnet allemal", "tokens": ["Er", "be\u00b7geg\u00b7net", "al\u00b7le\u00b7mal"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Fr\u00fch dem ersten Sonnenstral,", "tokens": ["Fr\u00fch", "dem", "ers\u00b7ten", "Son\u00b7nen\u00b7stral", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn er, munterm Flei\u00df geneigt,", "tokens": ["Wenn", "er", ",", "mun\u00b7term", "Flei\u00df", "ge\u00b7neigt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Von den Bergen nieder steigt.", "tokens": ["Von", "den", "Ber\u00b7gen", "nie\u00b7der", "steigt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "In der Jahre reifern Lauf", "tokens": ["In", "der", "Jah\u00b7re", "rei\u00b7fern", "Lauf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Suchen Ehr und W\u00fcrd ihn auf:", "tokens": ["Su\u00b7chen", "Ehr", "und", "W\u00fcrd", "ihn", "auf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gl\u00fcck und Seegen warten sein,", "tokens": ["Gl\u00fcck", "und", "See\u00b7gen", "war\u00b7ten", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Um sein Alter zu erfreun.", "tokens": ["Um", "sein", "Al\u00b7ter", "zu", "er\u00b7freun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Aller Orten trifft er dann", "tokens": ["Al\u00b7ler", "Or\u00b7ten", "trifft", "er", "dann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Fr\u00fcchte seiner Arbeit an,", "tokens": ["Fr\u00fcch\u00b7te", "sei\u00b7ner", "Ar\u00b7beit", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Keinen Augenblick der Zeit,", "tokens": ["Kei\u00b7nen", "Au\u00b7gen\u00b7blick", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die er nun umsonst bereut.", "tokens": ["Die", "er", "nun", "um\u00b7sonst", "be\u00b7reut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Auch im Alter, auch als Grei\u00df", "tokens": ["Auch", "im", "Al\u00b7ter", ",", "auch", "als", "Grei\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "$,", "ADV", "KOUS", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist er munter und voll Flei\u00df,", "tokens": ["Ist", "er", "mun\u00b7ter", "und", "voll", "Flei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "KON", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ihn tr\u00e4gt kein falscher Stab", "tokens": ["Und", "ihn", "tr\u00e4gt", "kein", "fal\u00b7scher", "Stab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "An sein ruhigs, sp\u00e4tes Grab.", "tokens": ["An", "sein", "ru\u00b7higs", ",", "sp\u00e4\u00b7tes", "Grab", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}