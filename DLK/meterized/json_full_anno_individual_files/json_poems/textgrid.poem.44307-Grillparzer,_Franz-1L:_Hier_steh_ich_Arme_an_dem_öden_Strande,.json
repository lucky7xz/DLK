{"textgrid.poem.44307": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hier steh ich Arme an dem \u00f6den Strande,", "genre": "verse", "period": "N.A.", "pub_year": 1807, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier steh ich Arme an dem \u00f6den Strande,", "tokens": ["Hier", "steh", "ich", "Ar\u00b7me", "an", "dem", "\u00f6\u00b7den", "Stran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo jeder mich als seine Feindin ha\u00dft,", "tokens": ["Wo", "je\u00b7der", "mich", "als", "sei\u00b7ne", "Fein\u00b7din", "ha\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "An meinen H\u00e4nden klirren ehrne Bande,", "tokens": ["An", "mei\u00b7nen", "H\u00e4n\u00b7den", "klir\u00b7ren", "ehr\u00b7ne", "Ban\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als Sklavin blick ich nach dem fernem Lande,", "tokens": ["Als", "Skla\u00b7vin", "blick", "ich", "nach", "dem", "fer\u00b7nem", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Das alles, was mir teuer war, umfa\u00dft,", "tokens": ["Das", "al\u00b7les", ",", "was", "mir", "teu\u00b7er", "war", ",", "um\u00b7fa\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "PIS", "$,", "PWS", "PPER", "ADJD", "VAFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Nach den Ruinen, die des teuern Gatten", "tokens": ["Nach", "den", "Ru\u00b7i\u00b7nen", ",", "die", "des", "teu\u00b7ern", "Gat\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.7": {"text": "Und meiner Heldens\u00f6hne Grab umschatten!", "tokens": ["Und", "mei\u00b7ner", "Hel\u00b7den\u00b7s\u00f6h\u00b7ne", "Grab", "um\u00b7schat\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ihr Lieben meiner Seele seid gefallen,", "tokens": ["Ihr", "Lie\u00b7ben", "mei\u00b7ner", "See\u00b7le", "seid", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie Eichen, die der Wettersturm zerknickt,", "tokens": ["Wie", "Ei\u00b7chen", ",", "die", "der", "Wet\u00b7ter\u00b7sturm", "zer\u00b7knickt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und meiner Trauer herbe Seufzer schallen", "tokens": ["Und", "mei\u00b7ner", "Trau\u00b7er", "her\u00b7be", "Seuf\u00b7zer", "schal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In blutbegieriger Hellenen Hallen,", "tokens": ["In", "blut\u00b7be\u00b7gie\u00b7ri\u00b7ger", "Hel\u00b7le\u00b7nen", "Hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die sie mit euern Waffen ausgeschm\u00fcckt,", "tokens": ["Die", "sie", "mit", "eu\u00b7ern", "Waf\u00b7fen", "aus\u00b7ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und niedrig schm\u00e4hend trinken rohe Zecher,", "tokens": ["Und", "nied\u00b7rig", "schm\u00e4\u00b7hend", "trin\u00b7ken", "ro\u00b7he", "Ze\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "An feiler Dirnen Brust, aus Priams Becher,", "tokens": ["An", "fei\u00b7ler", "Dir\u00b7nen", "Brust", ",", "aus", "Pri\u00b7ams", "Be\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,", "APPR", "NE", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Besudeln frech des grauen Helden Ehre,", "tokens": ["Be\u00b7su\u00b7deln", "frech", "des", "grau\u00b7en", "Hel\u00b7den", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die auch bei seinem Fall nicht unterging,", "tokens": ["Die", "auch", "bei", "sei\u00b7nem", "Fall", "nicht", "un\u00b7ter\u00b7ging", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als er am Fu\u00df der heimischen Alt\u00e4re", "tokens": ["Als", "er", "am", "Fu\u00df", "der", "hei\u00b7mi\u00b7schen", "Al\u00b7t\u00e4\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von Neoptolemos grausamen Speere", "tokens": ["Von", "Neop\u00b7to\u00b7le\u00b7mos", "grau\u00b7sa\u00b7men", "Spee\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "An meiner Brust den Todesstreich empfing.", "tokens": ["An", "mei\u00b7ner", "Brust", "den", "To\u00b7des\u00b7streich", "emp\u00b7fing", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und ich darf nicht zu murren mich erk\u00fchnen,", "tokens": ["Und", "ich", "darf", "nicht", "zu", "mur\u00b7ren", "mich", "er\u00b7k\u00fch\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PTKNEG", "PTKZU", "VVINF", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Als niedre Magd mu\u00df ich den M\u00f6rdern dienen!", "tokens": ["Als", "nied\u00b7re", "Magd", "mu\u00df", "ich", "den", "M\u00f6r\u00b7dern", "die\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich, einst die Mutter von so vielen S\u00f6hnen,", "tokens": ["Ich", ",", "einst", "die", "Mut\u00b7ter", "von", "so", "vie\u00b7len", "S\u00f6h\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADV", "ART", "NN", "APPR", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich, die man ehmals hochbegl\u00fccket pries,", "tokens": ["Ich", ",", "die", "man", "eh\u00b7mals", "hoch\u00b7be\u00b7gl\u00fc\u00b7cket", "pries", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PIS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Verklage nun vorm Throne Zeus mit Tr\u00e4nen", "tokens": ["Ver\u00b7kla\u00b7ge", "nun", "vorm", "Thro\u00b7ne", "Zeus", "mit", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "NE", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Grausamkeit barbarischer Hellenen,", "tokens": ["Die", "Grau\u00b7sam\u00b7keit", "bar\u00b7ba\u00b7ri\u00b7scher", "Hel\u00b7le\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die meine wackern Kinder mir entri\u00df;", "tokens": ["Die", "mei\u00b7ne", "wa\u00b7ckern", "Kin\u00b7der", "mir", "ent\u00b7ri\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Gemordet von der Griechen blutgem Schwerte,", "tokens": ["Ge\u00b7mor\u00b7det", "von", "der", "Grie\u00b7chen", "blut\u00b7gem", "Schwer\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Deckt k\u00fchlend sie die m\u00fctterliche Erde.", "tokens": ["Deckt", "k\u00fch\u00b7lend", "sie", "die", "m\u00fct\u00b7ter\u00b7li\u00b7che", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Doch auch den schwachen Trost sollt ich nicht haben,", "tokens": ["Doch", "auch", "den", "schwa\u00b7chen", "Trost", "sollt", "ich", "nicht", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "VMFIN", "PPER", "PTKNEG", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu sehn, da\u00df sie ein niedrer H\u00fcgel deckt,", "tokens": ["Zu", "sehn", ",", "da\u00df", "sie", "ein", "nie\u00b7drer", "H\u00fc\u00b7gel", "deckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Brich Mutterherz, sie liegen unbegraben", "tokens": ["Brich", "Mut\u00b7ter\u00b7herz", ",", "sie", "lie\u00b7gen", "un\u00b7be\u00b7gra\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PPER", "VVFIN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und nackt, ein Raub der Geier und der Raben,", "tokens": ["Und", "nackt", ",", "ein", "Raub", "der", "Gei\u00b7er", "und", "der", "Ra\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ART", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Auf Trojas gl\u00fchnde Tr\u00fcmmer hingestreckt.", "tokens": ["Auf", "Tro\u00b7jas", "gl\u00fchn\u00b7de", "Tr\u00fcm\u00b7mer", "hin\u00b7ge\u00b7streckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und ich von all den Meinen losgerissen,", "tokens": ["Und", "ich", "von", "all", "den", "Mei\u00b7nen", "los\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PIAT", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Kann ihnen nicht einmal die Augen schlie\u00dfen.", "tokens": ["Kann", "ih\u00b7nen", "nicht", "ein\u00b7mal", "die", "Au\u00b7gen", "schlie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "O Hektor! \u2013 Ha, im Mutterbusen nagen", "tokens": ["O", "Hek\u00b7tor", "!", "\u2013", "Ha", ",", "im", "Mut\u00b7ter\u00b7bu\u00b7sen", "na\u00b7gen"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$.", "$(", "ITJ", "$,", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir bei dem Namen Schmerz und Wut,", "tokens": ["Mir", "bei", "dem", "Na\u00b7men", "Schmerz", "und", "Wut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Halt ein, Barbar! Achilleus Pferde jagen", "tokens": ["Halt", "ein", ",", "Bar\u00b7bar", "!", "A\u00b7chil\u00b7leus", "Pfer\u00b7de", "ja\u00b7gen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PTKVZ", "$,", "NN", "$.", "NE", "NN", "VVINF"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Dahin und Hektorn schleift der goldne Wagen,", "tokens": ["Da\u00b7hin", "und", "Hek\u00b7torn", "schleift", "der", "gold\u00b7ne", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "KON", "NE", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Mauern Ilions netzt Hektors Blut,", "tokens": ["Die", "Mau\u00b7ern", "I\u00b7li\u00b7ons", "netzt", "Hek\u00b7tors", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "NE", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und seiner Leiche spotten feige Scharen,", "tokens": ["Und", "sei\u00b7ner", "Lei\u00b7che", "spot\u00b7ten", "fei\u00b7ge", "Scha\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die sonst vor seinem Arm geflohen waren.", "tokens": ["Die", "sonst", "vor", "sei\u00b7nem", "Arm", "ge\u00b7flo\u00b7hen", "wa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Doch wohl euch allen, ihr habt ausgelitten,", "tokens": ["Doch", "wohl", "euch", "al\u00b7len", ",", "ihr", "habt", "aus\u00b7ge\u00b7lit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "PIS", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fcr eures Vaters Wohl, f\u00fcr Ilion", "tokens": ["F\u00fcr", "eu\u00b7res", "Va\u00b7ters", "Wohl", ",", "f\u00fcr", "I\u00b7li\u00b7on"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Habt ihr als wackre M\u00e4nner brav gestritten,", "tokens": ["Habt", "ihr", "als", "wack\u00b7re", "M\u00e4n\u00b7ner", "brav", "ge\u00b7strit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "KOUS", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Lebens Dornenbahn ist nun durchschritten,", "tokens": ["Des", "Le\u00b7bens", "Dor\u00b7nen\u00b7bahn", "ist", "nun", "durch\u00b7schrit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Jenseits des Lethe harret euer Lohn.", "tokens": ["Jen\u00b7seits", "des", "Le\u00b7the", "har\u00b7ret", "eu\u00b7er", "Lohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wohl allen, die bei Trojas Fall erbla\u00dften,", "tokens": ["Wohl", "al\u00b7len", ",", "die", "bei", "Tro\u00b7jas", "Fall", "er\u00b7bla\u00df\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "$,", "PRELS", "APPR", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.pent