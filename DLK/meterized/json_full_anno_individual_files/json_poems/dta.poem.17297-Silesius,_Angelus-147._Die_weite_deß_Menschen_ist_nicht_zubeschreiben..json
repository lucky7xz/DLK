{"dta.poem.17297": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "147.  Die weite de\u00df Menschen ist nicht  \n zubeschreiben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Wer ist der mir wie weit und breit ich bin zeigt an?", "tokens": ["Wer", "ist", "der", "mir", "wie", "weit", "und", "breit", "ich", "bin", "zeigt", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "PPER", "KOKOM", "ADJD", "KON", "ADJD", "PPER", "VAFIN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil der Unendliche (GOtt) in mir wandeln kan.", "tokens": ["Weil", "der", "Un\u00b7end\u00b7li\u00b7che", "(", "Gott", ")", "in", "mir", "wan\u00b7deln", "kan", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$(", "NN", "$(", "APPR", "PPER", "VVFIN", "VMFIN", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "2. ", "tokens": [], "token_info": [], "pos": []}}}}}