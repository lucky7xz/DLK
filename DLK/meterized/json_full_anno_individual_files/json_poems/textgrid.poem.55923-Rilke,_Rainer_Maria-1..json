{"textgrid.poem.55923": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da blieben sie, als w\u00e4re jene Flut", "tokens": ["Da", "blie\u00b7ben", "sie", ",", "als", "w\u00e4\u00b7re", "je\u00b7ne", "Flut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOKOM", "VAFIN", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "zur\u00fcckgetreten, deren gro\u00dfes Branden", "tokens": ["zu\u00b7r\u00fcck\u00b7ge\u00b7tre\u00b7ten", ",", "de\u00b7ren", "gro\u00b7\u00dfes", "Bran\u00b7den"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "an diesen Steinen wusch, bis sie entstanden;", "tokens": ["an", "die\u00b7sen", "Stei\u00b7nen", "wusch", ",", "bis", "sie", "ent\u00b7stan\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJD", "$,", "KOUS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "sie nahm im Fallen manches Attribut", "tokens": ["sie", "nahm", "im", "Fal\u00b7len", "man\u00b7ches", "At\u00b7tri\u00b7but"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "aus ihren H\u00e4nden, welche viel zu gut", "tokens": ["aus", "ih\u00b7ren", "H\u00e4n\u00b7den", ",", "wel\u00b7che", "viel", "zu", "gut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "ADV", "PTKA", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und gebend sind, um etwas festzuhalten.", "tokens": ["und", "ge\u00b7bend", "sind", ",", "um", "et\u00b7was", "fest\u00b7zu\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "$,", "KOUI", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie blieben, von den Formen in Basalten", "tokens": ["Sie", "blie\u00b7ben", ",", "von", "den", "For\u00b7men", "in", "Ba\u00b7sal\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "durch einen Nimbus, einen Bischofshut,", "tokens": ["durch", "ei\u00b7nen", "Nim\u00b7bus", ",", "ei\u00b7nen", "Bi\u00b7schof\u00b7shut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "bisweilen durch ein L\u00e4cheln unterschieden,", "tokens": ["bis\u00b7wei\u00b7len", "durch", "ein", "L\u00e4\u00b7cheln", "un\u00b7ter\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "f\u00fcr das ein Antlitz seiner Stunden Frieden", "tokens": ["f\u00fcr", "das", "ein", "Ant\u00b7litz", "sei\u00b7ner", "Stun\u00b7den", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "ART", "NN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "bewahrt hat als ein stilles Zifferblatt;", "tokens": ["be\u00b7wahrt", "hat", "als", "ein", "st