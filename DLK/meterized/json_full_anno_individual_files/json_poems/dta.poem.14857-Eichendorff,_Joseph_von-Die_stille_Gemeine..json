{"dta.poem.14857": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die stille Gemeine.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Er sprach verwirrt: \u201eDa dr\u00fcben stand", "tokens": ["Er", "sprach", "ver\u00b7wirrt", ":", "\u201e", "Da", "dr\u00fc\u00b7ben", "stand"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$.", "$(", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Vaters Schlo\u00df am Weiher,", "tokens": ["Des", "Va\u00b7ters", "Schlo\u00df", "am", "Wei\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich selbst steckt's an; das war ein Brand,", "tokens": ["Ich", "selbst", "steckt's", "an", ";", "das", "war", "ein", "Brand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PTKVZ", "$.", "PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Freiheit Freudenfeuer!\u201c", "tokens": ["Der", "Frei\u00b7heit", "Freu\u00b7den\u00b7feu\u00b7er", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}