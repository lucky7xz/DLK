{"textgrid.poem.61958": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "Der Polterabend", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herab von seiner stolzen Veste", "tokens": ["Her\u00b7ab", "von", "sei\u00b7ner", "stol\u00b7zen", "Ves\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Lehnt sich ein Rittersmann,", "tokens": ["Lehnt", "sich", "ein", "Rit\u00b7ters\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Tief unten aus dem Felsengrunde", "tokens": ["Tief", "un\u00b7ten", "aus", "dem", "Fel\u00b7sen\u00b7grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schwingt's lautlos sich hinan.", "tokens": ["Schwingt's", "laut\u00b7los", "sich", "hi\u00b7nan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PRF", "PTKVZ", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Schwarzbraune Locken auf dem Nacken,", "tokens": ["Schwarz\u00b7brau\u00b7ne", "Lo\u00b7cken", "auf", "dem", "Na\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Rotsamtnes Prachtgewand,", "tokens": ["Rot\u00b7samt\u00b7nes", "Pracht\u00b7ge\u00b7wand", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Den erznen Panzer um die H\u00fcfte,", "tokens": ["Den", "erz\u00b7nen", "Pan\u00b7zer", "um", "die", "H\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Visier in der Hand.", "tokens": ["Das", "Vi\u00b7sier", "in", "der", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "So lehnt er an dem Erkerfenster", "tokens": ["So", "lehnt", "er", "an", "dem", "Er\u00b7ker\u00b7fens\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im hochzeitlichen Schmuck,", "tokens": ["Im", "hoch\u00b7zeit\u00b7li\u00b7chen", "Schmuck", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+---+", "measure": "dactylic.init"}, "line.3": {"text": "Was stierst Du, Ritter, in die Tiefe,", "tokens": ["Was", "stierst", "Du", ",", "Rit\u00b7ter", ",", "in", "die", "Tie\u00b7fe", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Irrlicht zeigt nur Trug!", "tokens": ["Das", "Irr\u00b7licht", "zeigt", "nur", "Trug", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ruht Laura nicht im stillen Grabe?", "tokens": ["Ruht", "Lau\u00b7ra", "nicht", "im", "stil\u00b7len", "Gra\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "PTKNEG", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Schatten kehrt zur\u00fcck,", "tokens": ["Kein", "Schat\u00b7ten", "kehrt", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vergi\u00df die Schuld, zum Hochzeitsmahle", "tokens": ["Ver\u00b7gi\u00df", "die", "Schuld", ",", "zum", "Hoch\u00b7zeits\u00b7mah\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "ART", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ruft heut' Dein froh' Geschick!", "tokens": ["Ruft", "heut'", "Dein", "froh'", "Ge\u00b7schick", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ha, immer stiert er noch herunter", "tokens": ["Ha", ",", "im\u00b7mer", "stiert", "er", "noch", "her\u00b7un\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPER", "ADV", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den scharfen Blick hinab.", "tokens": ["Den", "schar\u00b7fen", "Blick", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Irrlicht steht an jener Stelle,", "tokens": ["Das", "Irr\u00b7licht", "steht", "an", "je\u00b7ner", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo sie den Tod sich gab.", "tokens": ["Wo", "sie", "den", "Tod", "sich", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Sein Grund ist leer, o weh, der Schrecken!", "tokens": ["Sein", "Grund", "ist", "leer", ",", "o", "weh", ",", "der", "Schre\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "FM", "PTKVZ", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was singt dort am Gestein?", "tokens": ["Was", "singt", "dort", "am", "Ge\u00b7stein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was schwingt sich hoch von Fels zu Felsen,", "tokens": ["Was", "schwingt", "sich", "hoch", "von", "Fels", "zu", "Fel\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "ADJD", "APPR", "NE", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im wei\u00dfen Heil'genschein?", "tokens": ["Im", "wei\u00b7\u00dfen", "Heil'\u00b7gen\u00b7schein", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "\u00bbnoch grauet nicht Dein Hochzeitsmorgen,", "tokens": ["\u00bb", "noch", "grau\u00b7et", "nicht", "Dein", "Hoch\u00b7zeits\u00b7mor\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Noch schaust Du nicht Dein Gl\u00fcck,", "tokens": ["Noch", "schaust", "Du", "nicht", "Dein", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O, harter Ritter, schau' lieb' Laura,", "tokens": ["O", ",", "har\u00b7ter", "Rit\u00b7ter", ",", "schau'", "lieb'", "Lau\u00b7ra", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,", "ADJD", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Schatten kehrt zur\u00fcck!\u00ab", "tokens": ["Ihr", "Schat\u00b7ten", "kehrt", "zu\u00b7r\u00fcck", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Den stolzen Ritter fa\u00dft ein Grausen,", "tokens": ["Den", "stol\u00b7zen", "Rit\u00b7ter", "fa\u00dft", "ein", "Grau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als er das Lied geh\u00f6rt,", "tokens": ["Als", "er", "das", "Lied", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von Geisterarmen fortgerissen", "tokens": ["Von", "Geis\u00b7ter\u00b7ar\u00b7men", "fort\u00b7ge\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er in den Abgrund f\u00e4hrt.", "tokens": ["Er", "in", "den", "Ab\u00b7grund", "f\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Horch da, ein namenloses Poltern", "tokens": ["Horch", "da", ",", "ein", "na\u00b7men\u00b7lo\u00b7ses", "Pol\u00b7tern"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADV", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im felsigten Gestein,", "tokens": ["Im", "fel\u00b7sig\u00b7ten", "Ge\u00b7stein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als wenn auf einmal tausend Donner", "tokens": ["Als", "wenn", "auf", "ein\u00b7mal", "tau\u00b7send", "Don\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "APPR", "ADV", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In's Burgtor schl\u00fcgen ein.", "tokens": ["In's", "Burg\u00b7tor", "schl\u00fc\u00b7gen", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "D'rum soll am Abend vor der Hochzeit", "tokens": ["D'\u00b7rum", "soll", "am", "A\u00b7bend", "vor", "der", "Hoch\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "APPRART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ein Polterabend sein,", "tokens": ["Ein", "Pol\u00b7te\u00b7ra\u00b7bend", "sein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denn \u2013 hei\u00dft es \u2013 wo viel Licht und Freude", "tokens": ["Denn", "\u2013",