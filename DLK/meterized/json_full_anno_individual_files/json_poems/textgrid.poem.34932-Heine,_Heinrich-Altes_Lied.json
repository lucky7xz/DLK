{"textgrid.poem.34932": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Altes Lied", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist gestorben und wei\u00dft es nicht,", "tokens": ["Du", "bist", "ge\u00b7stor\u00b7ben", "und", "wei\u00dft", "es", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "KON", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Erloschen ist dein Augenlicht,", "tokens": ["Er\u00b7lo\u00b7schen", "ist", "dein", "Au\u00b7gen\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erblichen ist dein rotes M\u00fcndchen,", "tokens": ["Er\u00b7bli\u00b7chen", "ist", "dein", "ro\u00b7tes", "M\u00fcnd\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und du bist tot, mein totes Kindchen.", "tokens": ["Und", "du", "bist", "tot", ",", "mein", "to\u00b7tes", "Kind\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "In einer schaurigen Sommernacht", "tokens": ["In", "ei\u00b7ner", "schau\u00b7ri\u00b7gen", "Som\u00b7mer\u00b7nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hab ich dich selber zu Grabe gebracht;", "to