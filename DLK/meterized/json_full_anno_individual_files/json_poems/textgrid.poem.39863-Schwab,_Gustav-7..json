{"textgrid.poem.39863": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mondschein, Traum, Dichtung, Liebe war vergessen,", "tokens": ["Mond\u00b7schein", ",", "Traum", ",", "Dich\u00b7tung", ",", "Lie\u00b7be", "war", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "VAFIN", "VVPP", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Und in der Stimmung sp\u00e4ter Lebenstage,", "tokens": ["Und", "in", "der", "Stim\u00b7mung", "sp\u00e4\u00b7ter", "Le\u00b7bens\u00b7ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zufrieden-ledig von der Alltagsplage,", "tokens": ["Zu\u00b7frie\u00b7den\u00b7le\u00b7dig", "von", "der", "All\u00b7tags\u00b7pla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schlich ich durch H\u00e4ngeweiden und Cypressen.", "tokens": ["Schlich", "ich", "durch", "H\u00e4n\u00b7ge\u00b7wei\u00b7den", "und", "Cyp\u00b7res\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "++-+-+--+--", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Die krummen G\u00e4nge hatt' ich all' durchmessen,", "tokens": ["Die", "krum\u00b7men", "G\u00e4n\u00b7ge", "hatt'", "ich", "all'", "durch\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die G\u00e4ste waren von gemeinem Schlage,", "tokens": ["Die", "G\u00e4s\u00b7te", "wa\u00b7ren", "von", "ge\u00b7mei\u00b7nem", "Schla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu gr\u00fc\u00dfen und zu danken war mir Plage,", "tokens": ["Zu", "gr\u00fc\u00b7\u00dfen", "und", "zu", "dan\u00b7ken", "war", "mir", "Pla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "VAFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bis ich die fernste Bank im Hain besessen.", "tokens": ["Bis", "ich", "die", "ferns\u00b7te", "Bank", "im", "Hain", "be\u00b7ses\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da h\u00f6rt' ich's knistern hinter mir im Moose;", "tokens": ["Da", "h\u00f6rt'", "ich's", "knis\u00b7tern", "hin\u00b7ter", "mir", "im", "Moo\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "VVFIN", "APPR", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gewendet sah ich eine Mutter pflegen", "tokens": ["Ge\u00b7wen\u00b7det", "sah", "ich", "ei\u00b7ne", "Mut\u00b7ter", "pfle\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein spielend Kind, hell wie die rote Rose.", "tokens": ["Ein", "spie\u00b7lend", "Kind", ",", "hell", "wie", "die", "ro\u00b7te", "Ro\u00b7se", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "ADJD", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und wie das Frauenantlitz aufw\u00e4rts schauet,", "tokens": ["Und", "wie", "das", "Frau\u00b7en\u00b7ant\u00b7litz", "auf\u00b7w\u00e4rts", "schau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Blickt eine welke Lilie mir entgegen,", "tokens": ["Blickt", "ei\u00b7ne", "wel\u00b7ke", "Li\u00b7lie", "mir", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit blauen Augen himmlisch noch bethauet.", "tokens": ["Mit", "blau\u00b7en", "Au\u00b7gen", "himm\u00b7lisch", "noch", "be\u00b7thau\u00b7et", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Mondschein, Traum, Dichtung, Liebe war vergessen,", "tokens": ["Mond\u00b7schein", ",", "Traum", ",", "Dich\u00b7tung", ",", "Lie\u00b7be", "war", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "VAFIN", "VVPP", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "U