{"textgrid.poem.42639": {"metadata": {"author": {"name": "Ratschky, Joseph Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr G\u00f6tter, helft! ein Waldgott, d\u00fcnket mich,", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr G\u00f6tter, helft! ein Waldgott, d\u00fcnket mich,", "tokens": ["Ihr", "G\u00f6t\u00b7ter", ",", "helft", "!", "ein", "Wald\u00b7gott", ",", "d\u00fcn\u00b7ket", "mich", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "$.", "ART", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und Don Quixot' aus Mancha raufen sich.", "tokens": ["Und", "Don", "Qui\u00b7xot'", "aus", "Man\u00b7cha", "rau\u00b7fen", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "APPR", "NE", "VVFIN", "PRF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Welch eine Scene! Lasst uns n\u00e4her gehn! ...", "tokens": ["Welch", "ei\u00b7ne", "Sce\u00b7ne", "!", "Lasst", "uns", "n\u00e4\u00b7her", "gehn", "!", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "ART", "NN", "$.", "VVFIN", "PPER", "ADJD", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ey! hab' ich je was Tolleres gesehn,", "tokens": ["Ey", "!", "hab'", "ich", "je", "was", "Tol\u00b7le\u00b7res", "ge\u00b7sehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VAFIN", "PPER", "ADV", "PWS", "ADJA", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So strafe Venus mich mit einem Kuss", "tokens": ["So", "stra\u00b7fe", "Ve\u00b7nus", "mich", "mit", "ei\u00b7nem", "Kuss"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Von Chloens welken Lippen! Bergelfuss,", "tokens": ["Von", "Chloens", "wel\u00b7ken", "Lip\u00b7pen", "!", "Ber\u00b7gel\u00b7fuss", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$.", "NE", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Der Barde, balgt mit Niethard Effterkrum,", "tokens": ["Der", "Bar\u00b7de", ",", "balgt", "mit", "Niet\u00b7hard", "Eff\u00b7ter\u00b7krum", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "APPR", "NE", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Dem Minnes\u00e4nger, sich auf's Blut herum.", "tokens": ["Dem", "Min\u00b7ne\u00b7s\u00e4n\u00b7ger", ",", "sich", "auf's", "Blut", "he\u00b7rum", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRF", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Ein alter Kranz von Eichenbl\u00e4ttern laubt", "tokens": ["Ein", "al\u00b7ter", "Kranz", "von", "Ei\u00b7chen\u00b7bl\u00e4t\u00b7tern", "laubt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Sich bardenhaft um Bergelfussens Haupt:", "tokens": ["Sich", "bar\u00b7den\u00b7haft", "um", "Ber\u00b7gel\u00b7fus\u00b7sens", "Haupt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Sein schnurrend Instrument, die Harfe, h\u00e4ngt", "tokens": ["Sein", "schnur\u00b7rend", "Inst\u00b7ru\u00b7ment", ",", "die", "Har\u00b7fe", ",", "h\u00e4ngt"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPOSAT", "ADJD", "NN", "$,", "ART", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Ihm auf dem R\u00fccken: seinen Leib umf\u00e4ngt", "tokens": ["Ihm", "auf", "dem", "R\u00fc\u00b7cken", ":", "sei\u00b7nen", "Leib", "um\u00b7f\u00e4ngt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "$.", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Ein B\u00e4renfell. Den s\u00fcssen Niethard schm\u00fcckt", "tokens": ["Ein", "B\u00e4\u00b7ren\u00b7fell", ".", "Den", "s\u00fcs\u00b7sen", "Niet\u00b7hard", "schm\u00fcckt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "ART", "ADJA", "NE", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Ein Panzer, dessen Glanz das Aug' entz\u00fcckt:", "tokens": ["Ein", "Pan\u00b7zer", ",", "des\u00b7sen", "Glanz", "das", "Aug'", "ent\u00b7z\u00fcckt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Der bunte Schild, den seine Linke f\u00fchrt,", "tokens": ["Der", "bun\u00b7te", "Schild", ",", "den", "sei\u00b7ne", "Lin\u00b7ke", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Ist minniglich mit Hulda's Bild geziert,", "tokens": ["Ist", "min\u00b7nig\u00b7lich", "mit", "Huld\u00b7a's", "Bild", "ge\u00b7ziert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NE", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "F\u00fcr die er lebt und webt. \u00bbO edles Paar!", "tokens": ["F\u00fcr", "die", "er", "lebt", "und", "webt", ".", "\u00bb", "O", "ed\u00b7les", "Paar", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "KON", "VVFIN", "$.", "$(", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "\u00bbwas soll der Zwist? verschonet euer Haar", "tokens": ["\u00bb", "was", "soll", "der", "Zwist", "?", "ver\u00b7scho\u00b7net", "eu\u00b7er", "Haar"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PWS", "VMFIN", "ART", "NN", "$.", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Und eure F\u00e4uste!\u00ab \u00bbK\u00fchner Fremdling! Wir", "tokens": ["Und", "eu\u00b7re", "F\u00e4us\u00b7te", "!", "\u00ab", "\u00bb", "K\u00fch\u00b7ner", "Fremd\u00b7ling", "!", "Wir"], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "word", "punct", "word"], "pos": ["KON", "PPOSAT", "NN", "$.", "$(", "$(", "ADJA", "NN", "$.", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Entscheiden nach den Dichterrechten hier", "tokens": ["Ent\u00b7schei\u00b7den", "nach", "den", "Dich\u00b7ter\u00b7rech\u00b7ten", "hier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Den Werth und Vorrang unsrer Lieder. Doch", "tokens": ["Den", "Werth", "und", "Vor\u00b7rang", "uns\u00b7rer", "Lie\u00b7der", ".", "Doch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "KON", "NN", "PPOSAT", "NN", "$.", "KON"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Du k\u00f6mmst uns, wie gerufen: weile noch!", "tokens": ["Du", "k\u00f6mmst", "uns", ",", "wie", "ge\u00b7ru\u00b7fen", ":", "wei\u00b7le", "noch", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "VVPP", "$.", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Du sollst der Schiedsmann seyn.\u00ab Sie setzten ganz", "tokens": ["Du", "sollst", "der", "Schieds\u00b7mann", "seyn", ".", "\u00ab", "Sie", "setz\u00b7ten", "ganz"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "VAINF", "$.", "$(", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Vertraulich nun, der Barde seinen Kranz,", "tokens": ["Ver\u00b7trau\u00b7lich", "nun", ",", "der", "Bar\u00b7de", "sei\u00b7nen", "Kranz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "$,", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Und seinen Schild der werthe Rittersmann,", "tokens": ["Und", "sei\u00b7nen", "Schild", "der", "wert\u00b7he", "Rit\u00b7ters\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Zum Wettpreis auf, und Bergelfuss begann:", "tokens": ["Zum", "Wett\u00b7preis", "auf", ",", "und", "Ber\u00b7gel\u00b7fuss", "be\u00b7gann", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,", "KON", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Auf! reichet mir die Leichenruthe", "tokens": ["Auf", "!", "rei\u00b7chet", "mir", "die", "Lei\u00b7chen\u00b7ru\u00b7the"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "$.", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Odins Schlachthemd von der Wand!", "tokens": ["Und", "O\u00b7dins", "Schlacht\u00b7hemd", "von", "der", "Wand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mich l\u00fcstet's, ha! nach Armyrs Blute.", "tokens": ["Mich", "l\u00fcs\u00b7tet's", ",", "ha", "!", "nach", "Ar\u00b7myrs", "Blu\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ITJ", "$.", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Tyr den Schiffweg hergesandt.", "tokens": ["Den", "Tyr", "den", "Schiff\u00b7weg", "her\u00b7ge\u00b7sandt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Hulda! dir nur bin ich pflichtig,", "tokens": ["Hul\u00b7da", "!", "dir", "nur", "bin", "ich", "pflich\u00b7tig", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Keinem Fr\u00e4ulein sonder dir;", "tokens": ["Kei\u00b7nem", "Fr\u00e4u\u00b7lein", "son\u00b7der", "dir", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wank und Trug verschw\u00f6r' ich: z\u00fcchtig", "tokens": ["Wank", "und", "Trug", "ver\u00b7schw\u00f6r'", "ich", ":", "z\u00fcch\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "PPER", "$.", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Traun! ist meine Kussbegier.", "tokens": ["Traun", "!", "ist", "mei\u00b7ne", "Kuss\u00b7be\u00b7gier", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Kommt, Klingenr\u00f6ther, Flammenschwinger,", "tokens": ["Kommt", ",", "Klin\u00b7gen\u00b7r\u00f6\u00b7ther", ",", "Flam\u00b7men\u00b7schwin\u00b7ger", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu Gonduls Hagel lad' ich euch:", "tokens": ["Zu", "Gon\u00b7duls", "Ha\u00b7gel", "lad'", "ich", "euch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kommt, schickt den feigen Methverschlinger", "tokens": ["Kommt", ",", "schickt", "den", "fei\u00b7gen", "Meth\u00b7ver\u00b7schlin\u00b7ger"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hinab nach Hela's Schlangenreich!", "tokens": ["Hin\u00b7ab", "nach", "He\u00b7la's", "Schlan\u00b7gen\u00b7reich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Deine preislichzarten H\u00e4nde", "tokens": ["Dei\u00b7ne", "preis\u00b7lich\u00b7zar\u00b7ten", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und dein M\u00fcndlein sind fast sch\u00f6n:", "tokens": ["Und", "dein", "M\u00fcnd\u00b7lein", "sind", "fast", "sch\u00f6n", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wonnespenderinn! ohn' Ende", "tokens": ["Won\u00b7nes\u00b7pen\u00b7de\u00b7rinn", "!", "ohn'", "En\u00b7de"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$.", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wollt' ich dir in's \u00c4uglein sehn.", "tokens": ["Wollt'", "ich", "dir", "in's", "\u00c4ug\u00b7lein", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Da soll in Naftronds M\u00f6rdertiefen,", "tokens": ["Da", "soll", "in", "Naf\u00b7tronds", "M\u00f6r\u00b7der\u00b7tie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo Lok, der G\u00f6ttert\u00e4uscher, heult,", "tokens": ["Wo", "Lok", ",", "der", "G\u00f6t\u00b7ter\u00b7t\u00e4u\u00b7scher", ",", "heult", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "NE", "$,", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihm Drachengift in's Antlitz triefen,", "tokens": ["Ihm", "Dra\u00b7chen\u00b7gift", "in's", "Ant\u00b7litz", "trie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bis Skoll einst Imers Licht ereilt.", "tokens": ["Bis", "Skoll", "einst", "I\u00b7mers", "Licht", "er\u00b7eilt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Hey! wie w\u00e4r's mir so beh\u00e4glich,", "tokens": ["Hey", "!", "wie", "w\u00e4r's", "mir", "so", "be\u00b7h\u00e4g\u00b7lich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "B\u00f6tst du mir den Minnekuss!", "tokens": ["B\u00f6tst", "du", "mir", "den", "Min\u00b7ne\u00b7kuss", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wie \u00e4chzt mein Sang so kl\u00e4glich,", "tokens": ["Und", "wie", "\u00e4chzt", "mein", "Sang", "so", "kl\u00e4g\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Weil ich soldlos minnen muss!", "tokens": ["Weil", "ich", "sold\u00b7los", "min\u00b7nen", "muss", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "\u00bbgenug, beym Herkules! g