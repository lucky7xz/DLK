{"dta.poem.5376": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Erinnerung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Geliebteste Belisa, sprich,", "tokens": ["Ge\u00b7lieb\u00b7tes\u00b7te", "Be\u00b7li\u00b7sa", ",", "sprich", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da du in unserm sch\u00f6nen Garten", "tokens": ["Da", "du", "in", "un\u00b7serm", "sch\u00f6\u00b7nen", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So manche Sch\u00f6nheit siehst, und nicht recht sonderlich", "tokens": ["So", "man\u00b7che", "Sch\u00f6n\u00b7heit", "siehst", ",", "und", "nicht", "recht", "son\u00b7der\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "$,", "KON", "PTKNEG", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dar\u00fcber fr\u00f6lich bist; sprich, worauf wilst du warten?", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "fr\u00f6\u00b7lich", "bist", ";", "sprich", ",", "wo\u00b7rauf", "wilst", "du", "war\u00b7ten", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "VAFIN", "$.", "ADJD", "$,", "PWAV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Auf welche Zeit verschiebst du deine Lust?", "tokens": ["Auf", "wel\u00b7che", "Zeit", "ver\u00b7schiebst", "du", "dei\u00b7ne", "Lust", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Was auf der Welt vergn\u00fcgliches zu hoffen,", "tokens": ["Was", "auf", "der", "Welt", "ver\u00b7gn\u00fcg\u00b7li\u00b7ches", "zu", "hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "ADJA", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wie dir so wol, als mir bewust,", "tokens": ["Wie", "dir", "so", "wol", ",", "als", "mir", "be\u00b7wust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ist ja, GOtt Lob! bey uns schon eingetroffen.", "tokens": ["Ist", "ja", ",", "Gott", "Lob", "!", "bey", "uns", "schon", "ein\u00b7ge\u00b7trof\u00b7fen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "NN", "NN", "$.", "APPR", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Du kanst und wirst nichts bessers hier verlangen.", "tokens": ["Du", "kanst", "und", "wirst", "nichts", "bes\u00b7sers", "hier", "ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KON", "VAFIN", "PIS", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Ach, so verz\u00f6gre doch nicht l\u00e4nger, anzufangen,", "tokens": ["Ach", ",", "so", "ver\u00b7z\u00f6g\u00b7re", "doch", "nicht", "l\u00e4n\u00b7ger", ",", "an\u00b7zu\u00b7fan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "ADV", "PTKNEG", "ADJD", "$,", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Dich an den gegenw\u00e4rt\u2019gen Sch\u00e4tzen", "tokens": ["Dich", "an", "den", "ge\u00b7gen\u00b7w\u00e4rt'\u00b7gen", "Sch\u00e4t\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Auch gegenw\u00e4rtig zu ergetzen,", "tokens": ["Auch", "ge\u00b7gen\u00b7w\u00e4r\u00b7tig", "zu", "er\u00b7get\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Und dem, der es uns giebt, zu Ehren, froh zu seyn!", "tokens": ["Und", "dem", ",", "der", "es", "uns", "giebt", ",", "zu", "Eh\u00b7ren", ",", "froh", "zu", "seyn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,", "APPR", "NN", "$,", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Denn, nach der Ordnung der Natur,", "tokens": ["Denn", ",", "nach", "der", "Ord\u00b7nung", "der", "Na\u00b7tur", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.15": {"text": "Bemerckt man \u00fcberall die Spur,", "tokens": ["Be\u00b7merckt", "man", "\u00fc\u00b7be\u00b7rall", "die", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Da\u00df ungepr\u00fcfte Lust und nicht gef\u00fchlt Vergn\u00fcgen,", "tokens": ["Da\u00df", "un\u00b7ge\u00b7pr\u00fcf\u00b7te", "Lust", "und", "nicht", "ge\u00b7f\u00fchlt", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "KON", "PTKNEG", "VVPP", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Mit der best\u00e4ndig-regen Zeit,", "tokens": ["Mit", "der", "be\u00b7st\u00e4n\u00b7dig\u00b7re\u00b7gen", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "So wol als die, so man gef\u00fchlet und genossen,", "tokens": ["So", "wol", "als", "die", ",", "so", "man", "ge\u00b7f\u00fch\u00b7let", "und", "ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "ART", "$,", "ADV", "PIS", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Mit ungehemmter Schnelligkeit", "tokens": ["Mit", "un\u00b7ge\u00b7hemm\u00b7ter", "Schnel\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Unwiederbringlich von uns fliegen", "tokens": ["Un\u00b7wie\u00b7der\u00b7bring\u00b7lich", "von", "uns", "flie\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPER", "VVINF"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.21": {"text": "Und, eh man sichs versieht, bereits davon geflossen.", "tokens": ["Und", ",", "eh", "man", "sichs", "ver\u00b7sieht", ",", "be\u00b7reits", "da\u00b7von", "ge\u00b7flos\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PIS", "PIS", "VVFIN", "$,", "ADV", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die Augenblick\u2019 und Zeit, worin uns Anmuth fehlt,", "tokens": ["Die", "Au\u00b7gen\u00b7blick'", "und", "Zeit", ",", "wo\u00b7rin", "uns", "An\u00b7muth", "fehlt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,", "PWAV", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die wir uns selber k\u00f6nnen geben,", "tokens": ["Die", "wir", "uns", "sel\u00b7ber", "k\u00f6n\u00b7nen", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Sind uns nicht minder zugezehlt,", "tokens": ["Sind", "uns", "nicht", "min\u00b7der", "zu\u00b7ge\u00b7zehlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Als die Vergn\u00fcglichsten von unserm Leben.", "tokens": ["Als", "die", "Ver\u00b7gn\u00fcg\u00b7lichs\u00b7ten", "von", "un\u00b7serm", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}}}}}