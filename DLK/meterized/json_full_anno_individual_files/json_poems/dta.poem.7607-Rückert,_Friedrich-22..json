{"dta.poem.7607": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "22.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.85", "lv:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zu ", "tokens": ["Zu"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Der alle Herrlichkeit der Welt zur Wahl ihm bot.", "tokens": ["Der", "al\u00b7le", "Herr\u00b7lich\u00b7keit", "der", "Welt", "zur", "Wahl", "ihm", "bot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ART", "NN", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Von Rossen, feurigen, gezogen war sein Wagen,", "tokens": ["Von", "Ros\u00b7sen", ",", "feu\u00b7ri\u00b7gen", ",", "ge\u00b7zo\u00b7gen", "war", "sein", "Wa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "ADJA", "$,", "VVPP", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Worauf Erobrerschwert' und K\u00f6nigskronen lagen.", "tokens": ["Wo\u00b7rauf", "Er\u00b7ob\u00b7rer\u00b7schwert'", "und", "K\u00f6\u00b7nigs\u00b7kro\u00b7nen", "la\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Mehr als von Sonnenschein und Mondglanz stralt der Wagen,", "tokens": ["Mehr", "als", "von", "Son\u00b7nen\u00b7schein", "und", "Mond\u00b7glanz", "stralt", "der", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "APPR", "NN", "KON", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit allem Prachtgestein und Gold der Welt beschlagen.", "tokens": ["Mit", "al\u00b7lem", "Pracht\u00b7ge\u00b7stein", "und", "Gold", "der", "Welt", "be\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "KON", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die Freuden und die K\u00fcnst' im Fr\u00fchlingsblumenkranz", "tokens": ["Die", "Freu\u00b7den", "und", "die", "K\u00fcnst'", "im", "Fr\u00fch\u00b7lings\u00b7blu\u00b7men\u00b7kranz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Begleiteten die Fahrt mit Sang und Klang und Tanz.", "tokens": ["Be\u00b7glei\u00b7te\u00b7ten", "die", "Fahrt", "mit", "Sang", "und", "Klang", "und", "Tanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und aus dem bunten Chor aufrichtete der Tod", "tokens": ["Und", "aus", "dem", "bun\u00b7ten", "Chor", "auf\u00b7rich\u00b7te\u00b7te", "der", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sich hoch, als er die Wahl dem ", "tokens": ["Sich", "hoch", ",", "als", "er", "die", "Wahl", "dem"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PRF", "ADJD", "$,", "KOUS", "PPER", "ART", "NN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Erw\u00e4hle, was du willst, von diesen G\u00fctern allen;", "tokens": ["Er\u00b7w\u00e4h\u00b7le", ",", "was", "du", "willst", ",", "von", "die\u00b7sen", "G\u00fc\u00b7tern", "al\u00b7len", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "PPER", "VMFIN", "$,", "APPR", "PDAT", "NN", "PIAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn deine Weisheit hat erregt mein Wohlgefallen.", "tokens": ["Denn", "dei\u00b7ne", "Weis\u00b7heit", "hat", "er\u00b7regt", "mein", "Wohl\u00b7ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "VVPP", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Doch ", "tokens": ["Doch"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Wie d\u00fcrft' ich denn von dir begehren ein Gabe?", "tokens": ["Wie", "d\u00fcrft'", "ich", "denn", "von", "dir", "be\u00b7geh\u00b7ren", "ein", "Ga\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "APPR", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "Satt wird das Menschenherz von allen Sch\u00e4tzen nicht;", "tokens": ["Satt", "wird", "das", "Men\u00b7schen\u00b7herz", "von", "al\u00b7len", "Sch\u00e4t\u00b7zen", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "APPR", "PIAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wer begehrte sie, der sah dein Angesicht?", "tokens": ["Und", "wer", "be\u00b7gehr\u00b7te", "sie", ",", "der", "sah", "dein", "An\u00b7ge\u00b7sicht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPER", "$,", "PRELS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Das Leben, was es hat und ist, was ists? ein Hauch:", "tokens": ["Das", "Le\u00b7ben", ",", "was", "es", "hat", "und", "ist", ",", "was", "ists", "?", "ein", "Hauch", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWS", "PPER", "VAFIN", "KON", "VAFIN", "$,", "PWS", "VAFIN", "$.", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Hauch vergeht durch dich, und du vergehest auch.", "tokens": ["Der", "Hauch", "ver\u00b7geht", "durch", "dich", ",", "und", "du", "ver\u00b7ge\u00b7hest", "auch", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "$,", "KON", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "La\u00df diesen Lebenshauch mich hauchen denn in Frieden,", "tokens": ["La\u00df", "die\u00b7sen", "Le\u00b7bens\u00b7hauch", "mich", "hau\u00b7chen", "denn", "in", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PDAT", "NN", "PPER", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Solang es dir beliebt, und es mir ist beschieden.", "tokens": ["So\u00b7lang", "es", "dir", "be\u00b7liebt", ",", "und", "es", "mir", "ist", "be\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "$,", "KON", "PPER", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Er sprachs, da war der Tod mit Wagen und mit Rossen,", "tokens": ["Er", "sprachs", ",", "da", "war", "der", "Tod", "mit", "Wa\u00b7gen", "und", "mit", "Ros\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "VAFIN", "ART", "NN", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Schlacht und Macht und Pracht, in Nacht und Nichts zerflossen.", "tokens": ["Mit", "Schlacht", "und", "Macht", "und", "Pracht", ",", "in", "Nacht", "und", "Nichts", "zer\u00b7flos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$,", "APPR", "NN", "KON", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}