{"textgrid.poem.60234": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie Alexanders Zeitgenossen", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie Alexanders Zeitgenossen", "tokens": ["Wie", "A\u00b7lex\u00b7an\u00b7ders", "Zeit\u00b7ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "NE", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nach Delphos giengen, und des Gottes Gegenwart,", "tokens": ["Nach", "Del\u00b7phos", "gien\u00b7gen", ",", "und", "des", "Got\u00b7tes", "Ge\u00b7gen\u00b7wart", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "$,", "KON", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Von heilgem Schauer \u00fcbergossen,", "tokens": ["Von", "heil\u00b7gem", "Schau\u00b7er", "\u00fc\u00b7ber\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit schneller wundersamer Art", "tokens": ["Mit", "schnel\u00b7ler", "wun\u00b7der\u00b7sa\u00b7mer", "Art"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Empfanden auf des Tempels Stufen;", "tokens": ["Emp\u00b7fan\u00b7den", "auf", "des", "Tem\u00b7pels", "Stu\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So gieng der Schweizer, Zimmermann,", "tokens": ["So", "gieng", "der", "Schwei\u00b7zer", ",", "Zim\u00b7mer\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "J\u00fcngsthin nach Sanssouci, vom K\u00f6nige berufen,", "tokens": ["J\u00fcng\u00b7sthin", "nach", "Sans\u00b7sou\u00b7ci", ",", "vom", "K\u00f6\u00b7ni\u00b7ge", "be\u00b7ru\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.8": {"text": "Und staunte da den Marmor an,", "tokens": ["Und", "staun\u00b7te", "da", "den", "Mar\u00b7mor", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und f\u00fchlete die Gegenwart der Musen", "tokens": ["Und", "f\u00fch\u00b7le\u00b7te", "die", "Ge\u00b7gen\u00b7wart", "der", "Mu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Die vor und um den K\u00f6nig gehn.", "tokens": ["Die", "vor", "und", "um", "den", "K\u00f6\u00b7nig", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKVZ", "KON", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Sein schwellend Herz erhub sich klopfend in dem Busen", "tokens": ["Sein", "schwel\u00b7lend", "Herz", "er\u00b7hub", "sich", "klop\u00b7fend", "in", "dem", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "NN", "VVFIN", "PRF", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und blieb an seinem Munde stehn.", "tokens": ["Und", "blieb", "an", "sei\u00b7nem", "Mun\u00b7de", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Itzt kam der K\u00f6nig \u2013 itzt ergriff ihn Frost und Hitze,", "tokens": ["Itzt", "kam", "der", "K\u00f6\u00b7nig", "\u2013", "itzt", "er\u00b7griff", "ihn", "Frost", "und", "Hit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$(", "ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ihm war zu Muth' als k\u00e4me, mit dem Blitze", "tokens": ["Ihm", "war", "zu", "Muth'", "als", "k\u00e4\u00b7me", ",", "mit", "dem", "Blit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KOKOM", "VVFIN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "In aufgehobner Hand, von des Olympus Spitze", "tokens": ["In", "auf\u00b7ge\u00b7hob\u00b7ner", "Hand", ",", "von", "des", "O\u00b7lym\u00b7pus", "Spit\u00b7ze"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ART", "NE", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Der G\u00f6ttergott herab.", "tokens": ["Der", "G\u00f6t\u00b7ter\u00b7gott", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Jedoch, so bald der K\u00f6nig mit dem Geiste", "tokens": ["Je\u00b7doch", ",", "so", "bald", "der", "K\u00f6\u00b7nig", "mit", "dem", "Geis\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "ADV", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Leutselger Sanftmuth ihm zween holde Blicke gab;", "tokens": ["Leut\u00b7sel\u00b7ger", "Sanft\u00b7muth", "ihm", "zween", "hol\u00b7de", "Bli\u00b7cke", "gab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "VVFIN", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+---+-+-+", "measure": "dactylic.di.plus"}, "line.19": {"text": "Und einmal l\u00e4chelte \u2013 da ward der Schweizer dreiste,", "tokens": ["Und", "ein\u00b7mal", "l\u00e4\u00b7chel\u00b7te", "\u2013", "da", "ward", "der", "Schwei\u00b7zer", "dreis\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$(", "ADV", "VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und seine Rede flo\u00df den Honigworten nach,", "tokens": ["Und", "sei\u00b7ne", "Re\u00b7de", "flo\u00df", "den", "Ho\u00b7nig\u00b7wor\u00b7ten", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die Friedrich eine ganze Stunde", "tokens": ["Die", "Fried\u00b7rich", "ei\u00b7ne", "gan\u00b7ze", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NE", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Hindurch aus vollem Herzen sprach.", "tokens": ["Hin\u00b7durch", "aus", "vol\u00b7lem", "Her\u00b7zen", "sprach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Auf Cicerons und C\u00e4sars Munde", "tokens": ["Auf", "Ci\u00b7ce\u00b7rons", "und", "C\u00e4\u00b7sars", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NE", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Hat nie die Suada so geschwebt:", "tokens": ["Hat", "nie", "die", "Sua\u00b7da", "so", "ge\u00b7schwebt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Nie sprach im edlen Markaurele", "tokens": ["Nie", "sprach", "im", "ed\u00b7len", "Mar\u00b7kau\u00b7re\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Mehr eine k\u00f6nigliche Seele,", "tokens": ["Mehr", "ei\u00b7ne", "k\u00f6\u00b7nig\u00b7li\u00b7che", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Die G\u00f6ttern nachzuahmen strebt,", "tokens": ["Die", "G\u00f6t\u00b7tern", "nach\u00b7zu\u00b7ah\u00b7men", "strebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Mit einem R\u00f6mer oder Griechen.", "tokens": ["Mit", "ei\u00b7nem", "R\u00f6\u00b7mer", "o\u00b7der", "Grie\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Nach dieser theuren Stunde pries", "tokens": ["Nach", "die\u00b7ser", "theu\u00b7ren", "Stun\u00b7de", "pries"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Der Schweizer Gott durch Thr\u00e4nen hei\u00df und s\u00fc\u00df,", "tokens": ["Der", "Schwei\u00b7zer", "Gott", "durch", "Thr\u00e4\u00b7nen", "hei\u00df", "und", "s\u00fc\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Da\u00df nicht sein Geist von ihm gewichen,", "tokens": ["Da\u00df", "nicht", "sein", "Geist", "von", "ihm", "ge\u00b7wi\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "PPOSAT", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Da\u00df er ihn auf der Erde lie\u00df,", "tokens": ["Da\u00df", "er", "ihn", "auf", "der", "Er\u00b7de", "lie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Um diesen K\u00f6nig anzuschauen.", "tokens": ["Um", "die\u00b7sen", "K\u00f6\u00b7nig", "an\u00b7zu\u00b7schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PDAT", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Ihr V\u00f6lker, die ihr euch bem\u00fcht,", "tokens": ["Ihr", "V\u00f6l\u00b7ker", ",", "die", "ihr", "euch", "be\u00b7m\u00fcht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Ihm Ehrens\u00e4ulen aufzubauen;", "tokens": ["Ihm", "Eh\u00b7ren\u00b7s\u00e4u\u00b7len", "auf\u00b7zu\u00b7bau\u00b7en", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Ihr Dichter, die ihr ihm ein Lied", "tokens": ["Ihr", "Dich\u00b7ter", ",", "die", "ihr", "ihm", "ein", "Lied"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Wollt singen, wie Virgil gesungen,", "tokens": ["Wollt", "sin\u00b7gen", ",", "wie", "Vir\u00b7gil", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$,", "PWAV", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Seyd noch so stark von seinem Ruhm durchdrungen;", "tokens": ["Seyd", "noch", "so", "stark", "von", "sei\u00b7nem", "Ruhm", "durch\u00b7drun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADV", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Wer nicht sein strahlend Auge sieht,", "tokens": ["Wer", "nicht", "sein", "strah\u00b7lend", "Au\u00b7ge", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "PPOSAT", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Wer nicht in ihm die Menschenliebe h\u00f6ret,", "tokens": ["Wer", "nicht", "in", "ihm", "die", "Men\u00b7schen\u00b7lie\u00b7be", "h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "APPR", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Mit aller ihrer G\u00f6ttlichkeit;", "tokens": ["Mit", "al\u00b7ler", "ih\u00b7rer", "G\u00f6tt\u00b7lich\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Der wei\u00df noch nicht genug, das er die Nachwelt lehret,", "tokens": ["Der", "wei\u00df", "noch", "nicht", "ge\u00b7nug", ",", "das", "er", "die", "Nach\u00b7welt", "leh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PTKNEG", "ADV", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Was Friedrich war zu unsrer Zeit.", "tokens": ["Was", "Fried\u00b7rich", "war", "zu", "uns\u00b7rer", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "VAFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie Alexanders Zeitgenossen", "tokens": ["Wie", "A\u00b7lex\u00b7an\u00b7ders", "Zeit\u00b7ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "NE", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nach Delphos giengen, und des Gottes Gegenwart,", "tokens": ["Nach", "Del\u00b7phos", "gien\u00b7gen", ",", "und", "des", "Got\u00b7tes", "Ge\u00b7gen\u00b7wart", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "$,", "KON", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Von heilgem Schauer \u00fcbergossen,", "tokens": ["Von", "heil\u00b7gem", "Schau\u00b7er", "\u00fc\u00b7ber\u00b7gos\u00b7s