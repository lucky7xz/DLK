{"textgrid.poem.56842": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wei\u00dft, was ich m\u00f6chte, M\u00e4dchen?", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wei\u00dft, was ich m\u00f6chte, M\u00e4dchen?", "tokens": ["Wei\u00dft", ",", "was", "ich", "m\u00f6ch\u00b7te", ",", "M\u00e4d\u00b7chen", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "PPER", "VMFIN", "$,", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Ich wollt, ich w\u00e4r ein Maurer", "tokens": ["Ich", "wollt", ",", "ich", "w\u00e4r", "ein", "Mau\u00b7rer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "und st\u00fcrzte vom Ger\u00fcst,", "tokens": ["und", "st\u00fcrz\u00b7te", "vom", "Ge\u00b7r\u00fcst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "und kurze Frist nur g\u00e4be", "tokens": ["und", "kur\u00b7ze", "Frist", "nur", "g\u00e4\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "man meinem Leben noch ...", "tokens": ["man", "mei\u00b7nem", "Le\u00b7ben", "noch", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "PPOSAT", "NN", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Sie tr\u00fcgen in dein Haus mich,", "tokens": ["Sie", "tr\u00fc\u00b7gen", "in", "dein", "Haus", "mich", ","], "token_info": ["word", "word", "word", "word