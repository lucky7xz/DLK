{"textgrid.poem.49399": {"metadata": {"author": {"name": "Verlaine, Paul-Marie", "birth": "N.A.", "death": "N.A."}, "title": "Initium", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sah sie auf dem Ball im Wirbeltanz der Paare,", "tokens": ["Ich", "sah", "sie", "auf", "dem", "Ball", "im", "Wir\u00b7bel\u00b7tanz", "der", "Paa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Geigen Lachen einte sich dem Fl\u00f6tenklang,", "tokens": ["Der", "Gei\u00b7gen", "La\u00b7chen", "ein\u00b7te", "sich", "dem", "Fl\u00f6\u00b7ten\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hold spielten um ihr Ohr die feinen, blonden Haare,", "tokens": ["Hold", "spiel\u00b7ten", "um", "ihr", "Ohr", "die", "fei\u00b7nen", ",", "blon\u00b7den", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ihr Ohr, zu dem mein Wunsch gleich einem Kuss sich schwang,", "tokens": ["Ihr", "Ohr", ",", "zu", "dem", "mein", "Wunsch", "gleich", "ei\u00b7nem", "Kuss", "sich", "schwang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "ART", "PPOSAT", "NN", "ADV", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Als spr\u00e4ch' er gern und w\u00e4re doch zu reden bang.", "tokens": ["Als", "spr\u00e4ch'", "er", "gern", "und", "w\u00e4\u00b7re", "doch", "zu", "re\u00b7den", "bang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ADV", "KON", "VAFIN", "ADV", "PTKZU", "VVINF", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und die Mazurka trug in schwebend-stillem Tanze", "tokens": ["Und", "die", "Ma\u00b7zur\u00b7ka", "trug", "in", "schwe\u00b7ben\u00b7dstil\u00b7lem", "Tan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NE", "VVFIN", "APPR", "ADJA", "NN"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Mild t\u00f6nend wie ein Lied sie weiter durch die Reih'n,", "tokens": ["Mild", "t\u00f6\u00b7nend", "wie", "ein", "Lied", "sie", "wei\u00b7ter", "durch", "die", "Reih'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KOKOM", "ART", "NN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein Reim von s\u00fcssem Klang, ein Bild von lichtem Glanze,", "tokens": ["Ein", "Reim", "von", "s\u00fcs\u00b7sem", "Klang", ",", "ein", "Bild", "von", "lich\u00b7tem", "Glan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und ihre Kinderseele strahlte hell und rein", "tokens": ["Und", "ih\u00b7re", "Kin\u00b7der\u00b7see\u00b7le", "strahl\u00b7te", "hell", "und", "rein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Durch ihrer grauen Augen sinnlich weichen Schein.", "tokens": ["Durch", "ih\u00b7rer", "grau\u00b7en", "Au\u00b7gen", "sinn\u00b7lich", "wei\u00b7chen", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und unbewegt seit diesem Augenblicke bete", "tokens": ["Und", "un\u00b7be\u00b7wegt", "seit", "die\u00b7sem", "Au\u00b7gen\u00b7bli\u00b7cke", "be\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "PDAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich ihre Sch\u00f6nheit an, der sich mein Herz geweiht,", "tokens": ["Ich", "ih\u00b7re", "Sch\u00f6n\u00b7heit", "an", ",", "der", "sich", "mein", "Herz", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "PTKVZ", "$,", "PRELS", "PRF", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So schreitet bang, als ob in Tempels Nacht sie tr\u00e4te,", "tokens": ["So", "schrei\u00b7tet", "bang", ",", "als", "ob", "in", "Tem\u00b7pels", "Nacht", "sie", "tr\u00e4\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "$,", "KOKOM", "KOUS", "APPR", "NE", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In die E