{"dta.poem.12655": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf ihre Spatzierfahrt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Chlorinde, wenn du dich hinaus aufs feld bem\u00fchst,", "tokens": ["Chlo\u00b7rin\u00b7de", ",", "wenn", "du", "dich", "hin\u00b7aus", "aufs", "feld", "be\u00b7m\u00fchst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "PRF", "APZR", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und da vergn\u00fcgungen aus allen blumen ziehst,", "tokens": ["Und", "da", "ver\u00b7gn\u00fc\u00b7gun\u00b7gen", "aus", "al\u00b7len", "blu\u00b7men", "ziehst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So lasse doch dein hertz ans lieben auch gedencken;", "tokens": ["So", "las\u00b7se", "doch", "dein", "hertz", "ans", "lie\u00b7ben", "auch", "ge\u00b7den\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "APPRART", "ADJA", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Fragstu: wie schicket sich das lieben auf das feld,", "tokens": ["Frags\u00b7tu", ":", "wie", "schi\u00b7cket", "sich", "das", "lie\u00b7ben", "auf", "das", "feld", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "VVFIN", "PRF", "ART", "ADJA", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das blumen bey sich f\u00fchrt und anmuth in sich h\u00e4lt,", "tokens": ["Das", "blu\u00b7men", "bey", "sich", "f\u00fchrt", "und", "an\u00b7muth", "in", "sich", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PRF", "VVFIN", "KON", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie kan denn dieses mich zur liebes-anmuth lencken?", "tokens": ["Wie", "kan", "denn", "die\u00b7ses", "mich", "zur", "lie\u00b7bes\u00b7an\u00b7muth", "len\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ADV", "PDAT", "PRF", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So sag ich: ja gar recht; es paart ja jedes sich,", "tokens": ["So", "sag", "ich", ":", "ja", "gar", "recht", ";", "es", "paart", "ja", "je\u00b7des", "sich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "ADV", "ADV", "ADJD", "$.", "PPER", "VVFIN", "ADV", "PIAT", "PRF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und di\u00df bedencke du, und denck an mich und dich.", "tokens": ["Und", "di\u00df", "be\u00b7den\u00b7cke", "du", ",", "und", "denck", "an", "mich", "und", "dich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPER", "$,", "KON", "VVIMP", "APPR", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}