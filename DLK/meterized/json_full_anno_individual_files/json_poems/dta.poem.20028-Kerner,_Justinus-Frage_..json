{"dta.poem.20028": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Frage .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4rst du nicht, heil'ger Abendschein!", "tokens": ["W\u00e4rst", "du", "nicht", ",", "heil'\u00b7ger", "A\u00b7bend\u00b7schein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "W\u00e4rst du nicht, sternerhellte Nacht!", "tokens": ["W\u00e4rst", "du", "nicht", ",", "ster\u00b7ner\u00b7hell\u00b7te", "Nacht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "$,", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du Bl\u00fcthenschmuk! du \u00fcpp'ger Hain!", "tokens": ["Du", "Bl\u00fc\u00b7then\u00b7schmuk", "!", "du", "\u00fcpp'\u00b7ger", "Hain", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$.", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und du Gebirg voll ernster Pracht!", "tokens": ["Und", "du", "Ge\u00b7birg", "voll", "erns\u00b7ter", "Pracht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du Vogelsang aus Himmeln hoch!", "tokens": ["Du", "Vo\u00b7gel\u00b7sang", "aus", "Him\u00b7meln", "hoch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du Lied aus voller Menschenbrust!", "tokens": ["Du", "Lied", "aus", "vol\u00b7ler", "Men\u00b7schen\u00b7brust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "W\u00e4rst du nicht \u2014 ach! was f\u00fcllte noch", "tokens": ["W\u00e4rst", "du", "nicht", "ach", "!", "was", "f\u00fcll\u00b7te", "noch"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PTKNEG", "$(", "ITJ", "$.", "PWS", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "In arger Zeit ein Herz mit Lust? \u2014", "tokens": ["In", "ar\u00b7ger", "Zeit", "ein", "Herz", "mit", "Lust", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}