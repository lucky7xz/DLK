{"dta.poem.20368": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet an Mirabellen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es wundert mich nicht sehr/ da\u00df Golgerus Melinden", "tokens": ["Es", "wun\u00b7dert", "mich", "nicht", "sehr", "/", "da\u00df", "Gol\u00b7ge\u00b7rus", "Me\u00b7lin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "$(", "KOUS", "NE", "NE"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bald Mirabelle t\u00e4ufft/ bald ein verg\u00f6ttert kind;", "tokens": ["Bald", "Mi\u00b7ra\u00b7bel\u00b7le", "t\u00e4ufft", "/", "bald", "ein", "ver\u00b7g\u00f6t\u00b7tert", "kind", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "$(", "ADV", "ART", "VVPP", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil ihre zierden ja fast mehr als g\u00f6ttlich sind/", "tokens": ["Weil", "ih\u00b7re", "zier\u00b7den", "ja", "fast", "mehr", "als", "g\u00f6tt\u00b7lich", "sind", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADV", "PIAT", "KOKOM", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die auch schon schimmernd stroh sind m\u00e4chtig zu entz\u00fcnden.", "tokens": ["Die", "auch", "schon", "schim\u00b7mernd", "stroh", "sind", "m\u00e4ch\u00b7tig", "zu", "ent\u00b7z\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJD", "ADJD", "VAFIN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der glieder haut gleicht sich der weichsten bircken-rinden/", "tokens": ["Der", "glie\u00b7der", "haut", "gleicht", "sich", "der", "weichs\u00b7ten", "bir\u00b7cken\u00b7rin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "PRF", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der augen gold/ das fast den agtstein \u00fcberwindt/", "tokens": ["Der", "au\u00b7gen", "gold", "/", "das", "fast", "den", "agt\u00b7stein", "\u00fc\u00b7berw\u00b7indt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "PDS", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Aus denen thr\u00e4nen-saltz/ wie fette milch abrinnt/", "tokens": ["Aus", "de\u00b7nen", "thr\u00e4\u00b7nen\u00b7saltz", "/", "wie", "fet\u00b7te", "milch", "ab\u00b7rinnt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "NE", "$(", "KOKOM", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ist gut/ da\u00df Venus ihr daraus l\u00e4\u00dft fackeln winden.", "tokens": ["Ist", "gut", "/", "da\u00df", "Ve\u00b7nus", "ihr", "da\u00b7raus", "l\u00e4\u00dft", "fa\u00b7ckeln", "win\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$(", "KOUS", "NE", "PPER", "PAV", "VVFIN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ihr haar/ der liebes-strick/ ist weisser als der schnee/", "tokens": ["Ihr", "haar", "/", "der", "lie\u00b7bes\u00b7strick", "/", "ist", "weis\u00b7ser", "als", "der", "schnee", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "ART", "NN", "$(", "VAFIN", "ADJD", "KOKOM", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die lippen d\u00f6rffen nicht den blausten veilgen weichen/", "tokens": ["Die", "lip\u00b7pen", "d\u00f6rf\u00b7fen", "nicht", "den", "blaus\u00b7ten", "veil\u00b7gen", "wei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Kein mahler kan so gut das wang\u2019 als sie bestreichen;", "tokens": ["Kein", "mah\u00b7ler", "kan", "so", "gut", "das", "wang'", "als", "sie", "be\u00b7strei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "ADV", "ADJD", "ART", "NN", "KOUS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Den br\u00fcsten mangelt nichts als eine runde h\u00f6h/", "tokens": ["Den", "br\u00fcs\u00b7ten", "man\u00b7gelt", "nichts", "als", "ei\u00b7ne", "run\u00b7de", "h\u00f6h", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PIS", "KOKOM", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Das rothe feuer strahlt ihr sichtbar aus den augen.", "tokens": ["Das", "ro\u00b7the", "feu\u00b7er", "strahlt", "ihr", "sicht\u00b7bar", "aus", "den", "au\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Warum denn solte sie dem Golgerus nicht taugen?", "tokens": ["Wa\u00b7rum", "denn", "sol\u00b7te", "sie", "dem", "Gol\u00b7ge\u00b7rus", "nicht", "tau\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}