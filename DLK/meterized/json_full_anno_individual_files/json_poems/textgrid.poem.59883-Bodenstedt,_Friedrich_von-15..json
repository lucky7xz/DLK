{"textgrid.poem.59883": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "15.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist ein Witz zur rechten Stunde gekommen,", "tokens": ["Ist", "ein", "Witz", "zur", "rech\u00b7ten", "Stun\u00b7de", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "So antwortet jeder, den du nie gefragt hast:", "tokens": ["So", "ant\u00b7wor\u00b7tet", "je\u00b7der", ",", "den", "du", "nie", "ge\u00b7fragt", "hast", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "PRELS", "PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Du hast mir das Wort aus dem Munde genommen,", "tokens": ["Du", "hast", "mir", "das", "Wort", "aus", "dem", "Mun\u00b7de", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Oft hab' ich gedacht, was du mir gesagt hast!", "tokens": ["Oft", "hab'", "ich", "ge\u00b7dacht", ",", "was", "du", "mir", "ge\u00b7sagt", "hast", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$,", "PWS", "PPER", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Mirza-Schaffy, das ist dein Gesc