{"dta.poem.10361": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "I.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin's gewohnt den Kopf recht hoch zu tragen,", "tokens": ["Ich", "bin's", "ge\u00b7wohnt", "den", "Kopf", "recht", "hoch", "zu", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "ART", "NN", "ADJD", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein Sinn ist auch ein bischen starr und z\u00e4he;", "tokens": ["Mein", "Sinn", "ist", "auch", "ein", "bi\u00b7schen", "starr", "und", "z\u00e4\u00b7he", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ART", "ADV", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn selbst der K\u00f6nig mir in's Antlitz s\u00e4he,", "tokens": ["Wenn", "selbst", "der", "K\u00f6\u00b7nig", "mir", "in's", "Ant\u00b7litz", "s\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich w\u00fcrde nicht die Augen niederschlagen.", "tokens": ["Ich", "w\u00fcr\u00b7de", "nicht", "die", "Au\u00b7gen", "nie\u00b7der\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch, liebe Mutter, offen will ich's sagen:", "tokens": ["Doch", ",", "lie\u00b7be", "Mut\u00b7ter", ",", "of\u00b7fen", "will", "ich's", "sa\u00b7gen", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJA", "NN", "$,", "ADJD", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wie m\u00e4chtig auch mein stolzer Muth sich bl\u00e4he,", "tokens": ["Wie", "m\u00e4ch\u00b7tig", "auch", "mein", "stol\u00b7zer", "Muth", "sich", "bl\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "PPOSAT", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "In deiner selig s\u00fc\u00dfen, trauten N\u00e4he", "tokens": ["In", "dei\u00b7ner", "se\u00b7lig", "s\u00fc\u00b7\u00dfen", ",", "trau\u00b7ten", "N\u00e4\u00b7he"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJD", "VVINF", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ergreift mich oft ein demuthvolles Zagen.", "tokens": ["Er\u00b7greift", "mich", "oft", "ein", "de\u00b7muth\u00b7vol\u00b7les", "Za\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Ist es dein Geist, der heimlich mich bezwinget,", "tokens": ["Ist", "es", "dein", "Geist", ",", "der", "heim\u00b7lich", "mich", "be\u00b7zwin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "$,", "PRELS", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Dein hoher Geist, der Alles k\u00fchn durchdringet,", "tokens": ["Dein", "ho\u00b7her", "Geist", ",", "der", "Al\u00b7les", "k\u00fchn", "durch\u00b7drin\u00b7get", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PRELS", "PIS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und blitzend sich zum Himmelslichte schwinget?", "tokens": ["Und", "blit\u00b7zend", "sich", "zum", "Him\u00b7mels\u00b7lich\u00b7te", "schwin\u00b7get", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Qu\u00e4lt mich Erinnerung, da\u00df ich ver\u00fcbet", "tokens": ["Qu\u00e4lt", "mich", "E\u00b7rin\u00b7ne\u00b7rung", ",", "da\u00df", "ich", "ver\u00b7\u00fc\u00b7bet"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "$,", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "So manche That, die dir das Herz betr\u00fcbet,", "tokens": ["So", "man\u00b7che", "That", ",", "die", "dir", "das", "Herz", "be\u00b7tr\u00fc\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Das sch\u00f6ne Herz, das mich so sehr geliebet?", "tokens": ["Das", "sch\u00f6\u00b7ne", "Herz", ",", "das", "mich", "so", "sehr", "ge\u00b7lie\u00b7bet", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}