{"dta.poem.21132": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "An die Sonne .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die du in dunkeln Wolken", "tokens": ["Die", "du", "in", "dun\u00b7keln", "Wol\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dein stralend Haupt versteckest;", "tokens": ["Dein", "stra\u00b7lend", "Haupt", "ver\u00b7ste\u00b7ckest", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O liebe liebe Sonne,", "tokens": ["O", "lie\u00b7be", "lie\u00b7be", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Willst du mir ietzt nicht scheinen?", "tokens": ["Willst", "du", "mir", "ietzt", "nicht", "schei\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Du scheinst dem leeren Thoren,", "tokens": ["Du", "scheinst", "dem", "lee\u00b7ren", "Tho\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wenn sein gestickter Aufschlag", "tokens": ["Wenn", "sein", "ge\u00b7stick\u00b7ter", "Auf\u00b7schlag"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "In deinen Stralen funkelt.", "tokens": ["In", "dei\u00b7nen", "Stra\u00b7len", "fun\u00b7kelt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Du scheinst der eiteln Dame,", "tokens": ["Du", "scheinst", "der", "ei\u00b7teln", "Da\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Wenn ihre Zitternadel", "tokens": ["Wenn", "ih\u00b7re", "Zit\u00b7ter\u00b7na\u00b7del"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Des B\u00fcrgers Auge blendet.", "tokens": ["Des", "B\u00fcr\u00b7gers", "Au\u00b7ge", "blen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Du wirst ja mir auch scheinen,", "tokens": ["Du", "wirst", "ja", "mir", "auch", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Damit mein wei\u00dfes M\u00e4dchen", "tokens": ["Da\u00b7mit", "mein", "wei\u00b7\u00dfes", "M\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Mich nicht umsonst erwartet.", "tokens": ["Mich", "nicht", "um\u00b7sonst", "er\u00b7war\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}