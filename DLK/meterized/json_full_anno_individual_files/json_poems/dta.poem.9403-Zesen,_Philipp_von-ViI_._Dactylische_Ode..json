{"dta.poem.9403": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "ViI .  \n  Dactylische Ode.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Lustig erzeiget Euch meine Gesellen/", "tokens": ["Lus\u00b7tig", "er\u00b7zei\u00b7get", "Euch", "mei\u00b7ne", "Ge\u00b7sel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "W\u00fcndschet da\u00df dieses Fest offte begeh", "tokens": ["W\u00fcnd\u00b7schet", "da\u00df", "die\u00b7ses", "Fest", "off\u00b7te", "be\u00b7geh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOUS", "PDAT", "NN", "ADJA", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Gueintzius ohne Leid/ Trauren und Weh.", "tokens": ["Gu\u00b7e\u00b7int\u00b7zius", "oh\u00b7ne", "Leid", "/", "Trau\u00b7ren", "und", "Weh", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "$(", "NN", "KON", "NN", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "La\u00dfet uns Spieler und Geiger bestellen/", "tokens": ["La\u00b7\u00dfet", "uns", "Spie\u00b7ler", "und", "Gei\u00b7ger", "be\u00b7stel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "VVINF", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "La\u00dfet mit Freude vertreiben das Leid:", "tokens": ["La\u00b7\u00dfet", "mit", "Freu\u00b7de", "ver\u00b7trei\u00b7ben", "das", "Leid", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.6": {"text": "unser Herr Rector erlebe die zeiten/", "tokens": ["un\u00b7ser", "Herr", "Rec\u00b7tor", "er\u00b7le\u00b7be", "die", "zei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "ART", "NN", "$("], "meter": "+-+---+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Welche der Nestor und andre beschreiten/", "tokens": ["Wel\u00b7che", "der", "Nes\u00b7tor", "und", "and\u00b7re", "be\u00b7schrei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "KON", "PIS", "VVFIN", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.8": {"text": "W\u00fcndschet ein jeder zu jetziger zeit!", "tokens": ["W\u00fcnd\u00b7schet", "ein", "je\u00b7der", "zu", "jet\u00b7zi\u00b7ger", "zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "PIS", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}}}}