{"textgrid.poem.57220": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sie kommen, Cramern gehet in Rythmustanz,", "genre": "verse", "period": "N.A.", "pub_year": 1763, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie kommen, Cramern gehet in Rythmustanz,", "tokens": ["Sie", "kom\u00b7men", ",", "Cra\u00b7mern", "ge\u00b7het", "in", "Ry\u00b7th\u00b7mu\u00b7stanz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "VVFIN", "APPR", "NE", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mit hochgehobner Leyer Iduna vor!", "tokens": ["Mit", "hoch\u00b7ge\u00b7hob\u00b7ner", "Le\u00b7yer", "I\u00b7du\u00b7na", "vor", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Sie geht, und sieht auf ihn zur\u00fccke,", "tokens": ["Sie", "geht", ",", "und", "sieht", "auf", "ihn", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie auf die Wipfel des Hains der Tag sieht.", "tokens": ["Wie", "auf", "die", "Wip\u00b7fel", "des", "Hains", "der", "Tag", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Sing noch Beredtsamkelten! die erste weckt", "tokens": ["Sing", "noch", "Be\u00b7redt\u00b7sam\u00b7kel\u00b7ten", "!", "die", "ers\u00b7te", "weckt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "NN", "$.", "ART", "ADJA", "VVFIN"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Den Schwan in Glasor schon zur Entz\u00fcckung auf!", "tokens": ["Den", "Schwan", "in", "Gla\u00b7sor", "schon", "zur", "Ent\u00b7z\u00fc\u00b7ckung", "auf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sein Fittig steigt, und sanft gebogen", "tokens": ["Sein", "Fit\u00b7tig", "steigt", ",", "und", "sanft", "ge\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "KON", "ADJD", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schwebet sein Hals mit des Liedes T\u00f6nen!", "tokens": ["Schwe\u00b7bet", "sein", "Hals", "mit", "des", "Lie\u00b7des", "T\u00f6\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Die deutsche Nachwelt singet der Barden Lied,", "tokens": ["Die", "deut\u00b7sche", "Nach\u00b7welt", "sin\u00b7get", "der", "Bar\u00b7den", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "(wir sind ihr Barden!) einst bey der Lanze Klang!", "tokens": ["(", "wir", "sind", "ihr", "Bar\u00b7den", "!", ")", "einst", "bey", "der", "Lan\u00b7ze", "Klang", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PPOSAT", "NN", "$.", "$(", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Sie wird von dir auch Lieder singen,", "tokens": ["Sie", "wird", "von", "dir", "auch", "Lie\u00b7der", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn sie daher zu der k\u00fchnen Schlacht zeucht.", "tokens": ["Wenn", "sie", "da\u00b7her", "zu", "der", "k\u00fch\u00b7nen", "Schlacht", "zeucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Schon hat den Geist der Donnerer ausgehaucht,", "tokens": ["Schon", "hat", "den", "Geist", "der", "Don\u00b7ne\u00b7rer", "aus\u00b7ge\u00b7haucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Schon w\u00e4lzt sein Leib sich blutig im Rheine fort,", "tokens": ["Schon", "w\u00e4lzt", "sein", "Leib", "sich", "blu\u00b7tig", "im", "Rhei\u00b7ne", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PRF", "ADJD", "APPRART", "NE", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Doch bleibt am leichenvollen Ufer", "tokens": ["Doch", "bleibt", "am", "lei\u00b7chen\u00b7vol\u00b7len", "U\u00b7fer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Horchend der eilende Geist noch schweben.", "tokens": ["Hor\u00b7chend", "der", "ei\u00b7len\u00b7de", "Geist", "noch", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Du schweigest, Freund, und siehest mich weinend an.", "tokens": ["Du", "schwei\u00b7gest", ",", "Freund", ",", "und", "sie\u00b7hest", "mich", "wei\u00b7nend", "an", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "$,", "KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ach warum starb die liebende Radikin?", "tokens": ["Ach", "wa\u00b7rum", "starb", "die", "lie\u00b7ben\u00b7de", "Ra\u00b7di\u00b7kin", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWAV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sch\u00f6n wie die junge Morgenr\u00f6the,", "tokens": ["Sch\u00f6n", "wie", "die", "jun\u00b7ge", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Heiter und sanft, wie die Sommermondnacht.", "tokens": ["Hei\u00b7ter", "und", "sanft", ",", "wie", "die", "Som\u00b7mer\u00b7mond\u00b7nacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$,", "PWAV", "ART", "NN", "$."], "meter": "+--+--+-++", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Nim diese Rosen, Gieseke; Velleda", "tokens": ["Nim", "die\u00b7se", "Ro\u00b7sen", ",", "Gie\u00b7se\u00b7ke", ";", "Vel\u00b7le\u00b7da"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["NE", "PDAT", "NN", "$,", "NN", "$.", "NE"], "meter": "---+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hat sie mit Z\u00e4hren heute noch sanft gen\u00e4sst,", "tokens": ["Hat", "sie", "mit", "Z\u00e4h\u00b7ren", "heu\u00b7te", "noch", "sanft", "ge\u00b7n\u00e4sst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Als sie dein Lied mir von den Schmerzen", "tokens": ["Als", "sie", "dein", "Lied", "mir", "von", "den", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Deiner Gespielin der Liebe vorsang.", "tokens": ["Dei\u00b7ner", "Ge\u00b7spie\u00b7lin", "der", "Lie\u00b7be", "vor\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.7": {"line.1": {"text": "Du l\u00e4chelst! Ja, dein Auge voll Z\u00e4rtlichkeit", "tokens": ["Du", "l\u00e4\u00b7chelst", "!", "Ja", ",", "dein", "Au\u00b7ge", "voll", "Z\u00e4rt\u00b7lich\u00b7keit"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PTKANT", "$,", "PPOSAT", "NN", "ADJD", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Hat dir mein Herz schon dazumal zugewandt,", "tokens": ["Hat", "dir", "mein", "Herz", "schon", "da\u00b7zu\u00b7mal", "zu\u00b7ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Als ich zum erstenmal dich sahe,", "tokens": ["Als", "ich", "zum", "ers\u00b7ten\u00b7mal", "dich", "sa\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als ich dich sah, und du mich nicht kantest.", "tokens": ["Als", "ich", "dich", "sah", ",", "und", "du", "mich", "nicht", "kan\u00b7test", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$,", "KON", "PPER", "PRF", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-++-+-", "measure": "unknown.measure.penta"}}, "stanza.8": {"line.1": {"text": "Wenn einst ich todt bin, Freund, so besinge mich!", "tokens": ["Wenn", "einst", "ich", "todt", "bin", ",", "Freund", ",", "so", "be\u00b7sin\u00b7ge", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "ADJD", "VAFIN", "$,", "NN", "$,", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dein Lied voll Thr\u00e4nen wird den entfliehenden", "tokens": ["Dein", "Lied", "voll", "Thr\u00e4\u00b7nen", "wird", "den", "ent\u00b7flie\u00b7hen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJD", "NN", "VAFIN", "ART", "ADJA"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dir treuen Geist noch um dein Auge,", "tokens": ["Dir", "treu\u00b7en", "Geist", "noch", "um", "dein", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das mich beweint, zu verweilen zwingen.", "tokens": ["Das", "mich", "be\u00b7weint", ",", "zu", "ver\u00b7wei\u00b7len", "zwin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVPP", "$,", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Dann soll mein Schutzgeist, schweigend und unbemerkt,", "tokens": ["Dann", "soll", "mein", "Schutz\u00b7geist", ",", "schwei\u00b7gend", "und", "un\u00b7be\u00b7merkt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dich dreymal segnen! dreymal dein sinkend Haupt", "tokens": ["Dich", "drey\u00b7mal", "seg\u00b7nen", "!", "drey\u00b7mal", "dein", "sin\u00b7kend", "Haupt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVINF", "$.", "ADV", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Umfliegen, und nach mir, der scheidet,", "tokens": ["Um\u00b7flie\u00b7gen", ",", "und", "nach", "mir", ",", "der", "schei\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "APPR", "PPER", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dreymal noch sehn, und dein Schutzgeist werden.", "tokens": ["Drey\u00b7mal", "noch", "sehn", ",", "und", "dein", "Schutz\u00b7geist", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "$,", "KON", "PPOSAT", "NN", "VAINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.10": {"line.1": {"text": "Der Thorheit Hasser, aber auch Menschenfreund,", "tokens": ["Der", "Thor\u00b7heit", "Has\u00b7ser", ",", "a\u00b7ber", "auch", "Men\u00b7schen\u00b7freund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "ADV", "ADV", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Allzeit gerechter Rabner, dein heller Blick,", "tokens": ["All\u00b7zeit", "ge\u00b7rech\u00b7ter", "Rab\u00b7ner", ",", "dein", "hel\u00b7ler", "Blick", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dein froh und herzenvoll Gesicht ist", "tokens": ["Dein", "froh", "und", "her\u00b7zen\u00b7voll", "Ge\u00b7sicht", "ist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "KON", "ADJD", "NN", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Freunden der Tugend, und deinen Freunden", "tokens": ["Freun\u00b7den", "der", "Tu\u00b7gend", ",", "und", "dei\u00b7nen", "Freun\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "KON", "PPOSAT", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.11": {"line.1": {"text": "Nur liebensw\u00fcrdig; aber den Thoren bist", "tokens": ["Nur", "lie\u00b7bens\u00b7w\u00fcr\u00b7dig", ";", "a\u00b7ber", "den", "Tho\u00b7ren", "bist"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$.", "ADV", "ART", "NN", "VAFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Du furchtbar! Scheuche, wenn du noch schweigst, sie schon", "tokens": ["Du", "furcht\u00b7bar", "!", "Scheu\u00b7che", ",", "wenn", "du", "noch", "schweigst", ",", "sie", "schon"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADJD", "$.", "NN", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,", "PPER", "ADV"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Zur\u00fcck! Lass selbst ihr kriechend L\u00e4cheln", "tokens": ["Zu\u00b7r\u00fcck", "!", "Lass", "selbst", "ihr", "krie\u00b7chend", "L\u00e4\u00b7cheln"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKVZ", "$.", "NE", "ADV", "PPER", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dich in dem r\u00fcgenden Zorn nicht irren.", "tokens": ["Dich", "in", "dem", "r\u00fc\u00b7gen\u00b7den", "Zorn", "nicht", "ir\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "PTKNEG", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.12": {"line.1": {"text": "Stolz, und voll Demuth, arten sie niemals aus!", "tokens": ["Stolz", ",", "und", "voll", "De\u00b7muth", ",", "ar\u00b7ten", "sie", "nie\u00b7mals", "aus", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "ADJD", "NN", "$,", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Sey unbek\u00fcmmert, wenn auch ihr zahllos Heer", "tokens": ["Sey", "un\u00b7be\u00b7k\u00fcm\u00b7mert", ",", "wenn", "auch", "ihr", "zahl\u00b7los", "Heer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAIMP", "ADJD", "$,", "KOUS", "ADV", "PPER", "ADJD", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Stets w\u00fcchs', und wenn in V\u00f6lkerschaften", "tokens": ["Stets", "w\u00fcchs'", ",", "und", "wenn", "in", "V\u00f6l\u00b7ker\u00b7schaf\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "KON", "KOUS", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auch Philosophen die Welt umschw\u00e4rmten!", "tokens": ["Auch", "Phi\u00b7lo\u00b7so\u00b7phen", "die", "Welt", "um\u00b7schw\u00e4rm\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.13": {"line.1": {"text": "Wenn du nur Einen jedes Jahrhundert nimst,", "tokens": ["Wenn", "du", "nur", "Ei\u00b7nen", "je\u00b7des", "Jahr\u00b7hun\u00b7dert", "nimst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und ihn der Weisheit Lehrlingen zugesellst;", "tokens": ["Und", "ihn", "der", "Weis\u00b7heit", "Lehr\u00b7lin\u00b7gen", "zu\u00b7ge\u00b7sellst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wohl dir! Wir wollen deine Siege", "tokens": ["Wohl", "dir", "!", "Wir", "wol\u00b7len", "dei\u00b7ne", "Sie\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "$.", "PPER", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Singen, die dich in der Fern erwarten.", "tokens": ["Sin\u00b7gen", ",", "die", "dich", "in", "der", "Fern", "er\u00b7war\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-++--+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.14": {"line.1": {"text": "Dem Enkel winkend stell' ich dein heilig Bild", "tokens": ["Dem", "En\u00b7kel", "win\u00b7kend", "stell'", "ich", "dein", "hei\u00b7lig", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVFIN", "PPER", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zu Tiburs Lacher, und zu der Houyhmess Freund;", "tokens": ["Zu", "Ti\u00b7burs", "La\u00b7cher", ",", "und", "zu", "der", "Hou\u00b7yh\u00b7mess", "Freund", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "$,", "KON", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da sollst du einst den Namen (wenig", "tokens": ["Da", "sollst", "du", "einst", "den", "Na\u00b7men", "(", "we\u00b7nig"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "NN", "$(", "PIS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fchreten ihn) des Gerechten f\u00fchren!", "tokens": ["F\u00fch\u00b7re\u00b7ten", "ihn", ")", "des", "Ge\u00b7rech\u00b7ten", "f\u00fch\u00b7ren", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "ART", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.15": {"line.1": {"text": "Sie kommen, Cramern gehet in Rythmustanz,", "tokens": ["Sie", "kom\u00b7men", ",", "Cra\u00b7mern", "ge\u00b7het", "in", "Ry\u00b7th\u00b7mu\u00b7stanz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "VVFIN", "APPR", "NE", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mit hochgehobner Leyer Iduna vor!", "tokens": ["Mit", "hoch\u00b7ge\u00b7hob\u00b7ner", "Le\u00b7yer", "I\u00b7du\u00b7na", "vor", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Sie geht, und sieht auf ihn zur\u00fccke,", "tokens": ["Sie", "geht", ",", "und", "sieht", "auf", "ihn", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie auf die Wipfel des Hains der Tag sieht.", "tokens": ["Wie", "auf", "die", "Wip\u00b7fel", "des", "Hains", "der", "Tag", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.16": {"line.1": {"text": "Sing noch Beredtsamkelten! die erste weckt", "tokens": ["Sing", "noch", "Be\u00b7redt\u00b7sam\u00b7kel\u00b7ten", "!", "die", "ers\u00b7te", "weckt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "NN", "$.", "ART", "ADJA", "VVFIN"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Den Schwan in Glasor schon zur Entz\u00fcckung auf!", "tokens": ["Den", "Schwan", "in", "Gla\u00b7sor", "schon", "zur", "Ent\u00b7z\u00fc\u00b7ckung", "auf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sein Fittig steigt, und sanft gebogen", "tokens": ["Sein", "Fit\u00b7tig", "steigt", ",", "und", "sanft", "ge\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "KON", "ADJD", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schwebet sein Hals mit des Liedes T\u00f6nen!", "tokens": ["Schwe\u00b7bet", "sein", "Hals", "mit", "des", "Lie\u00b7des", "T\u00f6\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.17": {"line.1": {"text": "Die deutsche Nachwelt singet der Barden Lied,", "tokens": ["Die", "deut\u00b7sche", "Nach\u00b7welt", "sin\u00b7get", "der", "Bar\u00b7den", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "(wir sind ihr Barden!) einst bey der Lanze Klang!", "tokens": ["(", "wir", "sind", "ihr", "Bar\u00b7den", "!", ")", "einst", "bey", "der", "Lan\u00b7ze", "Klang", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PPOSAT", "NN", "$.", "$(", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Sie wird von dir auch Lieder singen,", "tokens": ["Sie", "wird", "von", "dir", "auch", "Lie\u00b7der", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn sie daher zu der k\u00fchnen Schlacht zeucht.", "tokens": ["Wenn", "sie", "da\u00b7her", "zu", "der", "k\u00fch\u00b7nen", "Schlacht", "zeucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.18": {"line.1": {"text": "Schon hat den Geist der Donnerer ausgehaucht,", "tokens": ["Schon", "hat", "den", "Geist", "der", "Don\u00b7ne\u00b7rer", "aus\u00b7ge\u00b7haucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Schon w\u00e4lzt sein Leib sich blutig im Rheine fort,", "tokens": ["Schon", "w\u00e4lzt", "sein", "Leib", "sich", "blu\u00b7tig", "im", "Rhei\u00b7ne", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PRF", "ADJD", "APPRART", "NE", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Doch bleibt am leichenvollen Ufer", "tokens": ["Doch", "bleibt", "am", "lei\u00b7chen\u00b7vol\u00b7len", "U\u00b7fer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Horchend der eilende Geist noch schweben.", "tokens": ["Hor\u00b7chend", "der", "ei\u00b7len\u00b7de", "Geist", "noch", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.19": {"line.1": {"text": "Du schweigest, Freund, und siehest mich weinend an.", "tokens": ["Du", "schwei\u00b7gest", ",", "Freund", ",", "und", "sie\u00b7hest", "mich", "wei\u00b7nend", "an", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "$,", "KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ach warum starb die liebende Radikin?", "tokens": ["Ach", "wa\u00b7rum", "starb", "die", "lie\u00b7ben\u00b7de", "Ra\u00b7di\u00b7kin", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWAV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sch\u00f6n wie die junge Morgenr\u00f6the,", "tokens": ["Sch\u00f6n", "wie", "die", "jun\u00b7ge", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Heiter und sanft, wie die Sommermondnacht.", "tokens": ["Hei\u00b7ter", "und", "sanft", ",", "wie", "die", "Som\u00b7mer\u00b7mond\u00b7nacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$,", "PWAV", "ART", "NN", "$."], "meter": "+--+--+-++", "measure": "dactylic.di.plus"}}, "stanza.20": {"line.1": {"text": "Nim diese Rosen, Gieseke; Velleda", "tokens": ["Nim", "die\u00b7se", "Ro\u00b7sen", ",", "Gie\u00b7se\u00b7ke", ";", "Vel\u00b7le\u00b7da"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["NE", "PDAT", "NN", "$,", "NN", "$.", "NE"], "meter": "---+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hat sie mit Z\u00e4hren heute noch sanft gen\u00e4sst,", "tokens": ["Hat", "sie", "mit", "Z\u00e4h\u00b7ren", "heu\u00b7te", "noch", "sanft", "ge\u00b7n\u00e4sst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Als sie dein Lied mir von den Schmerzen", "tokens": ["Als", "sie", "dein", "Lied", "mir", "von", "den", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Deiner Gespielin der Liebe vorsang.", "tokens": ["Dei\u00b7ner", "Ge\u00b7spie\u00b7lin", "der", "Lie\u00b7be", "vor\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.21": {"line.1": {"text": "Du l\u00e4chelst! Ja, dein Auge voll Z\u00e4rtlichkeit", "tokens": ["Du", "l\u00e4\u00b7chelst", "!", "Ja", ",", "dein", "Au\u00b7ge", "voll", "Z\u00e4rt\u00b7lich\u00b7keit"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PTKANT", "$,", "PPOSAT", "NN", "ADJD", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Hat dir mein Herz schon dazumal zugewandt,", "tokens": ["Hat", "dir", "mein", "Herz", "schon", "da\u00b7zu\u00b7mal", "zu\u00b7ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Als ich zum erstenmal dich sahe,", "tokens": ["Als", "ich", "zum", "ers\u00b7ten\u00b7mal", "dich", "sa\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als ich dich sah, und du mich nicht kantest.", "tokens": ["Als", "ich", "dich", "sah", ",", "und", "du", "mich", "nicht", "kan\u00b7test", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$,", "KON", "PPER", "PRF", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-++-+-", "measure": "unknown.measure.penta"}}, "stanza.22": {"line.1": {"text": "Wenn einst ich todt bin, Freund, so besinge mich!", "tokens": ["Wenn", "einst", "ich", "todt", "bin", ",", "Freund", ",", "so", "be\u00b7sin\u00b7ge", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "ADJD", "VAFIN", "$,", "NN", "$,", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dein Lied voll Thr\u00e4nen wird den entfliehenden", "tokens": ["Dein", "Lied", "voll", "Thr\u00e4\u00b7nen", "wird", "den", "ent\u00b7flie\u00b7hen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJD", "NN", "VAFIN", "ART", "ADJA"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dir treuen Geist noch um dein Auge,", "tokens": ["Dir", "treu\u00b7en", "Geist", "noch", "um", "dein", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das mich beweint, zu verweilen zwingen.", "tokens": ["Das", "mich", "be\u00b7weint", ",", "zu", "ver\u00b7wei\u00b7len", "zwin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVPP", "$,", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.23": {"line.1": {"text": "Dann soll mein Schutzgeist, schweigend und unbemerkt,", "tokens": ["Dann", "soll", "mein", "Schutz\u00b7geist", ",", "schwei\u00b7gend", "und", "un\u00b7be\u00b7merkt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dich dreymal segnen! dreymal dein sinkend Haupt", "tokens": ["Dich", "drey\u00b7mal", "seg\u00b7nen", "!", "drey\u00b7mal", "dein", "sin\u00b7kend", "Haupt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVINF", "$.", "ADV", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Umfliegen, und nach mir, der scheidet,", "tokens": ["Um\u00b7flie\u00b7gen", ",", "und", "nach", "mir", ",", "der", "schei\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "APPR", "PPER", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dreymal noch sehn, und dein Schutzgeist werden.", "tokens": ["Drey\u00b7mal", "noch", "sehn", ",", "und", "dein", "Schutz\u00b7geist", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "$,", "KON", "PPOSAT", "NN", "VAINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.24": {"line.1": {"text": "Der Thorheit Hasser, aber auch Menschenfreund,", "tokens": ["Der", "Thor\u00b7heit", "Has\u00b7ser", ",", "a\u00b7ber", "auch", "Men\u00b7schen\u00b7freund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "ADV", "ADV", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Allzeit gerechter Rabner, dein heller Blick,", "tokens": ["All\u00b7zeit", "ge\u00b7rech\u00b7ter", "Rab\u00b7ner", ",", "dein", "hel\u00b7ler", "Blick", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dein froh und herzenvoll Gesicht ist", "tokens": ["Dein", "froh", "und", "her\u00b7zen\u00b7voll", "Ge\u00b7sicht", "ist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "KON", "ADJD", "NN", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Freunden der Tugend, und deinen Freunden", "tokens": ["Freun\u00b7den", "der", "Tu\u00b7gend", ",", "und", "dei\u00b7nen", "Freun\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "KON", "PPOSAT", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.25": {"line.1": {"text": "Nur liebensw\u00fcrdig; aber den Thoren bist", "tokens": ["Nur", "lie\u00b7bens\u00b7w\u00fcr\u00b7dig", ";", "a\u00b7ber", "den", "Tho\u00b7ren", "bist"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$.", "ADV", "ART", "NN", "VAFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Du furchtbar! Scheuche, wenn du noch schweigst, sie schon", "tokens": ["Du", "furcht\u00b7bar", "!", "Scheu\u00b7che", ",", "wenn", "du", "noch", "schweigst", ",", "sie", "schon"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADJD", "$.", "NN", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,", "PPER", "ADV"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Zur\u00fcck! Lass selbst ihr kriechend L\u00e4cheln", "tokens": ["Zu\u00b7r\u00fcck", "!", "Lass", "selbst", "ihr", "krie\u00b7chend", "L\u00e4\u00b7cheln"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKVZ", "$.", "NE", "ADV", "PPER", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dich in dem r\u00fcgenden Zorn nicht irren.", "tokens": ["Dich", "in", "dem", "r\u00fc\u00b7gen\u00b7den", "Zorn", "nicht", "ir\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "PTKNEG", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.26": {"line.1": {"text": "Stolz, und voll Demuth, arten sie niemals aus!", "tokens": ["Stolz", ",", "und", "voll", "De\u00b7muth", ",", "ar\u00b7ten", "sie", "nie\u00b7mals", "aus", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "ADJD", "NN", "$,", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Sey unbek\u00fcmmert, wenn auch ihr zahllos Heer", "tokens": ["Sey", "un\u00b7be\u00b7k\u00fcm\u00b7mert", ",", "wenn", "auch", "ihr", "zahl\u00b7los", "Heer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAIMP", "ADJD", "$,", "KOUS", "ADV", "PPER", "ADJD", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Stets w\u00fcchs', und wenn in V\u00f6lkerschaften", "tokens": ["Stets", "w\u00fcchs'", ",", "und", "wenn", "in", "V\u00f6l\u00b7ker\u00b7schaf\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "KON", "KOUS", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auch Philosophen die Welt umschw\u00e4rmten!", "tokens": ["Auch", "Phi\u00b7lo\u00b7so\u00b7phen", "die", "Welt", "um\u00b7schw\u00e4rm\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.27": {"line.1": {"text": "Wenn du nur Einen jedes Jahrhundert nimst,", "tokens": ["Wenn", "du", "nur", "Ei\u00b7nen", "je\u00b7des", "Jahr\u00b7hun\u00b7dert", "nimst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und ihn der Weisheit Lehrlingen zugesellst;", "tokens": ["Und", "ihn", "der", "Weis\u00b7heit", "Lehr\u00b7lin\u00b7gen", "zu\u00b7ge\u00b7sellst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wohl dir! Wir wollen deine Siege", "tokens": ["Wohl", "dir", "!", "Wir", "wol\u00b7len", "dei\u00b7ne", "Sie\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "$.", "PPER", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Singen, die dich in der Fern erwarten.", "tokens": ["Sin\u00b7gen", ",", "die", "dich", "in", "der", "Fern", "er\u00b7war\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-++--+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.28": {"line.1": {"text": "Dem Enkel winkend stell' ich dein heilig Bild", "tokens": ["Dem", "En\u00b7kel", "win\u00b7kend", "stell'", "ich", "dein", "hei\u00b7lig", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVFIN", "PPER", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zu Tiburs Lacher, und zu der Houyhmess Freund;", "tokens": ["Zu", "Ti\u00b7burs", "La\u00b7cher", ",", "und", "zu", "der", "Hou\u00b7yh\u00b7mess", "Freund", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "$,", "KON", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da sollst du einst den Namen (wenig", "tokens": ["Da", "sollst", "du", "einst", "den", "Na\u00b7men", "(", "we\u00b7nig"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "NN", "$(", "PIS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fchreten ihn) des Gerechten f\u00fchren!", "tokens": ["F\u00fch\u00b7re\u00b7ten", "ihn", ")", "des", "Ge\u00b7rech\u00b7ten", "f\u00fch\u00b7ren", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "ART", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}