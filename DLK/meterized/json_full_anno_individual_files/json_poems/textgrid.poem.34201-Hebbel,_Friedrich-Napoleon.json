{"textgrid.poem.34201": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Napoleon", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nennt doch den Korsen nicht gro\u00df! Er wu\u00dfte die Menschen zu brauchen,", "tokens": ["Nennt", "doch", "den", "Kor\u00b7sen", "nicht", "gro\u00df", "!", "Er", "wu\u00df\u00b7te", "die", "Men\u00b7schen", "zu", "brau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKNEG", "ADJD", "$.", "PPER", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Wies Jedwedem den Platz, welcher ihm eignete, an,", "tokens": ["Wies", "Jed\u00b7we\u00b7dem", "den", "Platz", ",", "wel\u00b7cher", "ihm", "eig\u00b7ne\u00b7te", ",", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "PTKVZ", "$,"], "meter": "-----+---+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Kn\u00fcpfte, was rings geschah, mit klugem Geiste zusammen,", "tokens": ["Kn\u00fcpf\u00b7te", ",", "was", "rings", "ge\u00b7schah", ",", "mit", "klu\u00b7gem", "Geis\u00b7te", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "VVFIN", "$,", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "N\u00fctzte es listig und hieb endlich darein mit dem Schwert.", "tokens": ["N\u00fctz\u00b7te", "es", "lis\u00b7tig", "und", "hieb", "end\u00b7lich", "da\u00b7rein", "mit", "dem", "Schwert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "VVFIN", "ADV", "PAV", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Freilich, was r\u00fchmt man den Shakespeare! Er reihte Buchstab' an Buchstab',", "tokens": ["Frei\u00b7lich", ",", "was", "r\u00fchmt", "man", "den", "Sha\u00b7ke\u00b7spe\u00b7a\u00b7re", "!", "Er", "reih\u00b7te", "Buch\u00b7stab'", "an", "Buch\u00b7stab'", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWS", "VVFIN", "PIS", "ART", "NE", "$.", "PPER", "VVFIN", "NN", "APPR", "NN", "$,"], "meter": "+--+--+--+--+-+--+-", "measure": "dactylic.tetra.plus"}, "line.6": {"text": "Setzte am richtigen Ort Komma und Kolon und Punct,", "tokens": ["Setz\u00b7te", "am", "rich\u00b7ti\u00b7gen", "Ort", "Kom\u00b7ma", "und", "Ko\u00b7lon", "und", "Punct", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "NE", "KON", "NN", "KON", "NN", "$,"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Mischte das Alphabet, wie And're, nur etwas geschickter,", "tokens": ["Mischte", "das", "Al\u00b7pha\u00b7bet", ",", "wie", "An\u00b7d'\u00b7re", ",", "nur", "et\u00b7was", "ge\u00b7schick\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PWAV", "PIS", "$,", "ADV", "ADV", "ADJD", "$,"], "meter": "+--+---+--+--+-", "measure": "dactylic.di.plus"}, "line.8": {"text": "Bis ein Macbeth, ein Lear oder ein Hamlet entstand.", "tokens": ["Bis", "ein", "Mac\u00b7be\u00b7th", ",", "ein", "Le\u00b7ar", "o\u00b7der", "ein", "Ham\u00b7let", "ent\u00b7stand", "."], "token_info": ["word", "word", "wor