{"textgrid.poem.34751": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An dem stillen Meeresstrande", "tokens": ["An", "dem", "stil\u00b7len", "Mee\u00b7res\u00b7stran\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist die Nacht heraufgezogen,", "tokens": ["Ist", "die", "Nacht", "her\u00b7auf\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Mond bricht aus den Wolken,", "tokens": ["Und", "der", "Mond", "bricht", "aus", "den", "Wol\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und es fl\u00fcstert aus den Wogen:", "tokens": ["Und", "es", "fl\u00fcs\u00b7tert", "aus", "den", "Wo\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbjener Mensch dort, ist er n\u00e4rrisch,", "tokens": ["\u00bb", "je\u00b7ner", "Mensch", "dort", ",", "ist", "er", "n\u00e4r\u00b7risch", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PDAT", "NN", "ADV", "$,", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Oder ist er gar verliebet,", "tokens": ["O\u00b7der", "ist", "er", "gar", "ver\u00b7lie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn er schaut so tr\u00fcb und heiter,", "tokens": ["Denn", "er", "schaut", "so", "tr\u00fcb", "und", "hei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Heiter und zugleich betr\u00fcbet?\u00ab", "tokens": ["Hei\u00b7ter", "und", "zu\u00b7gleich", "be\u00b7tr\u00fc\u00b7bet", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "KON", "ADV", "VVFIN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch der Mond, der lacht herunter,", "tokens": ["Doch", "der", "Mond", ",", "der", "lacht", "her\u00b7un\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mit heller Stimme spricht er:", "tokens": ["Und", "mit", "hel\u00b7ler", "Stim\u00b7me", "spricht", "er", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbjener ist verliebt und n\u00e4rrisch,", "tokens": ["\u00bb", "je\u00b7ner", "ist", "ver\u00b7liebt", "und", "n\u00e4r\u00b7risch", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "VVPP", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und noch obendrein ein Dichter.\u00ab", "tokens": ["Und", "noch", "o\u00b7ben\u00b7drein", "ein", "D