{"textgrid.poem.52704": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zwielicht f\u00fcllt die Nebelforste,", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwielicht f\u00fcllt die Nebelforste,", "tokens": ["Zwie\u00b7licht", "f\u00fcllt", "die", "Ne\u00b7bel\u00b7fors\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schimmert an der Felsenwand,", "tokens": ["Schim\u00b7mert", "an", "der", "Fel\u00b7sen\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo sich Falken hoch am Horste", "tokens": ["Wo", "sich", "Fal\u00b7ken", "hoch", "am", "Hors\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "NN", "ADJD", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weithin schwingen \u00fcbers Land,", "tokens": ["Weit\u00b7hin", "schwin\u00b7gen", "\u00fc\u00b7bers", "Land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Langsam hellen", "tokens": ["Lang\u00b7sam", "hel\u00b7len"], "token_info": ["word", "word"], "pos": ["ADJD", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Sich die Wellen,", "tokens": ["Sich", "die", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Und der See blitzt auf am Rand.", "tokens": ["Und", "der", "See", "blitzt", "auf", "am", "Rand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "APPRART", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Gott zum Gru\u00df! die Eidgenossen", "tokens": ["Gott", "zum", "Gru\u00df", "!", "die", "Eid\u00b7ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPRART", "NN", "$.", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "R\u00fccken an im hellen Hauf.", "tokens": ["R\u00fc\u00b7cken", "an", "im", "hel\u00b7len", "Hauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Heut mit t\u00fcckischen Geschossen", "tokens": ["Heut", "mit", "t\u00fc\u00b7cki\u00b7schen", "Ge\u00b7schos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Stellen sie sich lauernd auf,", "tokens": ["Stel\u00b7len", "sie", "sich", "lau\u00b7ernd", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Zu erneuern", "tokens": ["Zu", "er\u00b7neu\u00b7ern"], "token_info": ["word", "word"], "pos": ["PTKZU", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Ihrer theuern", "tokens": ["Ih\u00b7rer", "theu\u00b7ern"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Freiheit blutigen Erkauf.", "tokens": ["Frei\u00b7heit", "blu\u00b7ti\u00b7gen", "Er\u00b7kauf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "H\u00f6rt ihr schon das dumpfe Tosen?", "tokens": ["H\u00f6rt", "ihr", "schon", "das", "dump\u00b7fe", "To\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ferner Waffen Widerhall!", "tokens": ["Fer\u00b7ner", "Waf\u00b7fen", "Wi\u00b7der\u00b7hall", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie sich in der Bergschlucht Schoo\u00dfen", "tokens": ["Wie", "sich", "in", "der", "Berg\u00b7schlucht", "Schoo\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "APPR", "ART", "NN", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "F\u00e4ngt der Schlachtenh\u00f6rner Schall;", "tokens": ["F\u00e4ngt", "der", "Schlach\u00b7ten\u00b7h\u00f6r\u00b7ner", "Schall", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Immer j\u00e4her", "tokens": ["Im\u00b7mer", "j\u00e4\u00b7her"], "token_info": ["word", "word"], "pos": ["ADV", "ADJD"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "W\u00e4lzt sich n\u00e4her", "tokens": ["W\u00e4lzt", "sich", "n\u00e4\u00b7her"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PRF", "ADJD"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Ihrer stolzen Feinde Fall.", "tokens": ["Ih\u00b7rer", "stol\u00b7zen", "Fein\u00b7de", "Fall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Gleichend einem Feuermeere,", "tokens": ["Glei\u00b7chend", "ei\u00b7nem", "Feu\u00b7er\u00b7mee\u00b7re", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn es durch die Saaten quillt,", "tokens": ["Wenn", "es", "durch", "die", "Saa\u00b7ten", "quillt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gl\u00fchet weit im Adelsheere", "tokens": ["Gl\u00fc\u00b7het", "weit", "im", "A\u00b7dels\u00b7hee\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwert und Speer und Helm und Schild.", "tokens": ["Schwert", "und", "Speer", "und", "Helm", "und", "Schild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Solchen Schimmer", "tokens": ["Sol\u00b7chen", "Schim\u00b7mer"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Sah man nimmer,", "tokens": ["Sah", "man", "nim\u00b7mer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Solch ein prunkend Schreckenbild!", "tokens": ["Solch", "ein", "prun\u00b7kend", "Schre\u00b7cken\u00b7bild", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Schon verwirret sich die Masse", "tokens": ["Schon", "ver\u00b7wir\u00b7ret", "sich", "die", "Mas\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vorn im unvorsichtgen Trab,", "tokens": ["Vorn", "im", "un\u00b7vor\u00b7sicht\u00b7gen", "Trab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist ihr doch die hohle Gasse", "tokens": ["Ist", "ihr", "doch", "die", "hoh\u00b7le", "Gas\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vorbestimmt zum sichern Grab!", "tokens": ["Vor\u00b7be\u00b7stimmt", "zum", "si\u00b7chern", "Grab", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Das Gedr\u00e4nge", "tokens": ["Das", "Ge\u00b7dr\u00e4n\u00b7ge"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "In der Enge", "tokens": ["In", "der", "En\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Woget l\u00e4rmend auf und ab.", "tokens": ["Wo\u00b7get", "l\u00e4r\u00b7mend", "auf", "und", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Gr\u00e4\u00dflich in dem Augenblicke", "tokens": ["Gr\u00e4\u00df\u00b7lich", "in", "dem", "Au\u00b7gen\u00b7bli\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Donnerts in den Rittertro\u00df,", "tokens": ["Don\u00b7nerts", "in", "den", "Rit\u00b7ter\u00b7tro\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ungeheure Felsenst\u00fccke", "tokens": ["Un\u00b7ge\u00b7heu\u00b7re", "Fel\u00b7sen\u00b7st\u00fc\u00b7cke"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schmettern nieder Mann und Ro\u00df;", "tokens": ["Schmet\u00b7tern", "nie\u00b7der", "Mann", "und", "Ro\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "In den scheuen", "tokens": ["In", "den", "scheu\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Reiterreihen", "tokens": ["Rei\u00b7ter\u00b7rei\u00b7hen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "W\u00fcthet schrecklich das Gescho\u00df.", "tokens": ["W\u00fct\u00b7het", "schreck\u00b7lich", "das", "Ge\u00b7scho\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Abgewaldet stehn die K\u00e4mme", "tokens": ["Ab\u00b7ge\u00b7wal\u00b7det", "stehn", "die", "K\u00e4m\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Des Gebirgs, o tolle Schlacht!", "tokens": ["Des", "Ge\u00b7birgs", ",", "o", "tol\u00b7le", "Schlacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "FM", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wuchtiger Tannen Riesenst\u00e4mme", "tokens": ["Wuch\u00b7ti\u00b7ger", "Tan\u00b7nen", "Rie\u00b7sen\u00b7st\u00e4m\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Sausen nieder in den Schacht.", "tokens": ["Sau\u00b7sen", "nie\u00b7der", "in", "den", "Schacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Staubumqualmet", "tokens": ["Stau\u00b7bum\u00b7qual\u00b7met"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Liegt zermalmet", "tokens": ["Liegt", "zer\u00b7mal\u00b7met"], "token_info": ["word", "word"], "pos": ["ADJD", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Ryburg in der Todesnacht.", "tokens": ["Ry\u00b7burg", "in", "der", "To\u00b7des\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Seht sie jetzt heruntereilen", "tokens": ["Seht", "sie", "jetzt", "her\u00b7un\u00b7ter\u00b7ei\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unter jubelndem Geschrei,", "tokens": ["Un\u00b7ter", "ju\u00b7beln\u00b7dem", "Ge\u00b7schrei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie sie schlagen mit den Keulen", "tokens": ["Wie", "sie", "schla\u00b7gen", "mit", "den", "Keu\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aller Richtung Trotz entzwei!", "tokens": ["Al\u00b7ler", "Rich\u00b7tung", "Trotz", "ent\u00b7zwei", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja, da sanken", "tokens": ["Ja", ",", "da", "san\u00b7ken"], "token_info": ["word", "punct", "word", "word"], "pos": ["PTKANT", "$,", "ADV", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Auf den Flanken", "tokens": ["Auf", "den", "Flan\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Viele Herren stolz und frei.", "tokens": ["Vie\u00b7le", "Her\u00b7ren", "stolz", "und", "frei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Diese Bauern zu zertreten", "tokens": ["Die\u00b7se", "Bau\u00b7ern", "zu", "zer\u00b7tre\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit dem siegesharten Fu\u00df,", "tokens": ["Mit", "dem", "sie\u00b7ges\u00b7har\u00b7ten", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "War den prahlenden Trompeten", "tokens": ["War", "den", "prah\u00b7len\u00b7den", "Trom\u00b7pe\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hohngetr\u00e4nkter Schlachtengru\u00df,", "tokens": ["Hohn\u00b7ge\u00b7tr\u00e4nk\u00b7ter", "Schlach\u00b7ten\u00b7gru\u00df", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "War in schn\u00f6den", "tokens": ["War", "in", "schn\u00f6\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "APPR", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Ingrimms Reden", "tokens": ["In\u00b7grimms", "Re\u00b7den"], "token_info": ["word", "word"], "pos": ["NE", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Eures Uebermuths Ergu\u00df!", "tokens": ["Eu\u00b7res", "Ue\u00b7ber\u00b7muths", "Er\u00b7gu\u00df", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Jetzt vergeblich m\u00f6cht ich schauen,", "tokens": ["Jetzt", "ver\u00b7geb\u00b7lich", "m\u00f6cht", "ich", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jetzt bewundern jenen Zug,", "tokens": ["Jetzt", "be\u00b7wun\u00b7dern", "je\u00b7nen", "Zug", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Den so stattlich durch die Gauen", "tokens": ["Den", "so", "statt\u00b7lich", "durch", "die", "Gau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einst der Schwarm der Rosse trug.", "tokens": ["Einst", "der", "Schwarm", "der", "Ros\u00b7se", "trug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weh