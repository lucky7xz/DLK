{"textgrid.poem.65814": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "88", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn dir gefallen wird, mich zu verschm\u00e4hn,", "tokens": ["Wenn", "dir", "ge\u00b7fal\u00b7len", "wird", ",", "mich", "zu", "ver\u00b7schm\u00e4hn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dem Hohn und Unglimpf mein Verdienst zu weihn,", "tokens": ["Dem", "Hohn", "und", "Un\u00b7glimpf", "mein", "Ver\u00b7dienst", "zu", "weihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Will ich, mich selbst befehdend, zu dir stehn,", "tokens": ["Will", "ich", ",", "mich", "selbst", "be\u00b7feh\u00b7dend", ",", "zu", "dir", "stehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "PPER", "ADV", "VVPP", "$,", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Trotz deines Meineids dein Verteid'ger sein.", "tokens": ["Trotz", "dei\u00b7nes", "Mein\u00b7eids", "dein", "Ver\u00b7tei\u00b7d'\u00b7ger", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Am besten kundig meiner eignen Schw\u00e4chen,", "tokens": ["Am", "bes\u00b7ten", "kun\u00b7dig", "mei\u00b7ner", "eig\u00b7nen", "Schw\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "ADJD", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Kann ich von stillen Fehlern, die mich beugen,", "tokens": ["Kann", "ich", "von", "stil\u00b7len", "Feh\u00b7lern", ",", "die", "mich", "beu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "NN", "$,", "PRELS", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Zu deinen Gunsten dir Bericht versprechen;", "tokens": ["Zu", "dei\u00b7nen", "Guns\u00b7ten", "dir", "Be\u00b7richt", "ver\u00b7spre\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df dir mein Abschied soll zum Ruhm gereichen.", "tokens": ["Da\u00df", "dir", "mein", "Ab\u00b7schied", "soll", "zum", "Ruhm", "ge\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VMFIN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und mir auch wird's Gewinn sein, der ich dir", "tokens": ["Und", "mir", "auch", "wird's", "Ge\u00b7winn", "sein", ",", "der", "ich", "dir"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "VAFIN", "NN", "VAINF", "$,", "PRELS", "PPER", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Mich liebend ganz gewidmet; denn es frommt", "tokens": ["Mich", "lie\u00b7bend", "ganz", "ge\u00b7wid\u00b7met", ";", "denn", "es", "frommt"], "token_info": ["word", "word", "w