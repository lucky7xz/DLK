{"textgrid.poem.59875": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gelb rollt mir zu F\u00fc\u00dfen der brausende Kur", "tokens": ["Gelb", "rollt", "mir", "zu", "F\u00fc\u00b7\u00dfen", "der", "brau\u00b7sen\u00b7de", "Kur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "NN", "ART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Im tanzenden Wellengetriebe;", "tokens": ["Im", "tan\u00b7zen\u00b7den", "Wel\u00b7len\u00b7ge\u00b7trie\u00b7be", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Hell l\u00e4chelt die Sonne, mein Herz und die Flur \u2013", "tokens": ["Hell", "l\u00e4\u00b7chelt", "die", "Son\u00b7ne", ",", "mein", "Herz", "und", "die", "Flur", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,", "PPOSAT", "NN", "KON", "ART", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Oh, wenn es doch immer so bliebe!", "tokens": ["Oh", ",", "wenn", "es", "doch", "im\u00b7mer", "so", "blie\u00b7be", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPER", "ADV", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Rot funkelt im Glas der kachetische Wein,", "tokens": ["Rot", "fun\u00b7kelt", "im", "Glas", "der", "ka\u00b7che\u00b7ti\u00b7sche", "Wein", ","], "token_info": ["word", "word", "word", "word", "word",