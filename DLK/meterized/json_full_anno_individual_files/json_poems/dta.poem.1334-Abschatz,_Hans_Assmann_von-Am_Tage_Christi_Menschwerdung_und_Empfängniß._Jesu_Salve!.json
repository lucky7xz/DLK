{"dta.poem.1334": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Am Tage Christi Menschwerdung  \n und Empf\u00e4ngni\u00df.  \n  Jesu Salve!", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Jauchze Pal\u00e4stins Gefilde/", "tokens": ["Jauch\u00b7ze", "Pa\u00b7l\u00e4s\u00b7tins", "Ge\u00b7fil\u00b7de", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NE", "NN", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Freu dich kleines Ephrata!", "tokens": ["Freu", "dich", "klei\u00b7nes", "E\u00b7phra\u00b7ta", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was dir l\u00e4ngst gezeigt im Bilde", "tokens": ["Was", "dir", "l\u00e4ngst", "ge\u00b7zeigt", "im", "Bil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADV", "VVPP", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00f6mmt nun deinen Gr\u00e4ntzen nah.", "tokens": ["K\u00f6mmt", "nun", "dei\u00b7nen", "Gr\u00e4nt\u00b7zen", "nah", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Neues Wunder l\u00e4st sich schauen", "tokens": ["Neu\u00b7es", "Wun\u00b7der", "l\u00e4st", "sich", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auff den Nazarether Auen.", "tokens": ["Auff", "den", "Na\u00b7za\u00b7re\u00b7ther", "Au\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Eine Jungfrau ohne Flecken", "tokens": ["Ei\u00b7ne", "Jung\u00b7frau", "oh\u00b7ne", "Fle\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zeuget einen Wunder-Sohn/", "tokens": ["Zeu\u00b7get", "ei\u00b7nen", "Wun\u00b7der\u00b7Sohn", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "(klare Schrifften/ tunckle Decken", "tokens": ["(", "kla\u00b7re", "Schriff\u00b7ten", "/", "tunck\u00b7le", "De\u00b7cken"], "token_info": ["punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "ADJA", "NN", "$(", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Haben l\u00e4ngst gezeugt darvon:)", "tokens": ["Ha\u00b7ben", "l\u00e4ngst", "ge\u00b7zeugt", "dar\u00b7von", ":)"], "token_info": ["word", "word", "word", "word", "emoticon"], "pos": ["VAFIN", "ADV", "VVPP", "PAV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gott mit uns und Heyl der Erden", "tokens": ["Gott", "mit", "uns", "und", "Heyl", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "KON", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Soll das Kind genennet werden.", "tokens": ["Soll", "das", "Kind", "ge\u00b7nen\u00b7net", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VVPP", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "S\u00fcnde bracht uns Tod und Sterben/", "tokens": ["S\u00fcn\u00b7de", "bracht", "uns", "Tod", "und", "Ster\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "NN", "KON", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "B\u00f6ser Mutter herbe Zucht/", "tokens": ["B\u00f6\u00b7ser", "Mut\u00b7ter", "her\u00b7be", "Zucht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sodoms Aepffel zum Verderben/", "tokens": ["So\u00b7doms", "A\u00b7e\u00b7pf\u00b7fel", "zum", "Ver\u00b7der\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPRART", "NN", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Argen Stammes bittre Frucht:", "tokens": ["Ar\u00b7gen", "Stam\u00b7mes", "bitt\u00b7re", "Frucht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jesse Wurtzel will nun gr\u00fcnen", "tokens": ["Jes\u00b7se", "Wurt\u00b7zel", "will", "nun", "gr\u00fc\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "VMFIN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und zum Lebens-Balsam dienen.", "tokens": ["Und", "zum", "Le\u00b7bens\u00b7Bal\u00b7sam", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Unter tr\u00fcben Finsternissen", "tokens": ["Un\u00b7ter", "tr\u00fc\u00b7ben", "Fins\u00b7ter\u00b7nis\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lag die Welt/ und seuffzet ihr:", "tokens": ["Lag", "die", "Welt", "/", "und", "seuff\u00b7zet", "ihr", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$(", "KON", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df die Wolcken doch zerrissen/", "tokens": ["Da\u00df", "die", "Wol\u00b7cken", "doch", "zer\u00b7ris\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Sonne br\u00e4ch herf\u00fcr!", "tokens": ["Und", "die", "Son\u00b7ne", "br\u00e4ch", "her\u00b7f\u00fcr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schaut ihr nicht ihr goldnes Blincken", "tokens": ["Schaut", "ihr", "nicht", "ihr", "gold\u00b7nes", "Blin\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schon der frischen Erde wincken?", "tokens": ["Schon", "der", "fri\u00b7schen", "Er\u00b7de", "win\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sey willkommen/ Licht der Heyden/", "tokens": ["Sey", "will\u00b7kom\u00b7men", "/", "Licht", "der", "Hey\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$(", "NN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auff der Hebroniter Feld!", "tokens": ["Auff", "der", "Heb\u00b7ro\u00b7ni\u00b7ter", "Feld", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wilt du dich ins Fleisch verkleiden/", "tokens": ["Wilt", "du", "dich", "ins", "Fleisch", "ver\u00b7klei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "APPRART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gott von Art und Mensch ein Held?", "tokens": ["Gott", "von", "Art", "und", "Mensch", "ein", "Held", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sohn Mariens/ Gast auff Erden/", "tokens": ["Sohn", "Ma\u00b7ri\u00b7ens", "/", "Gast", "auff", "Er\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$(", "NN", "APPR", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mein Gefert und Bruder werden?", "tokens": ["Mein", "Ge\u00b7fert", "und", "Bru\u00b7der", "wer\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ach! wie kan ich dich umfassen!", "tokens": ["Ach", "!", "wie", "kan", "ich", "dich", "um\u00b7fas\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach! wie mag ich danckbar seyn!", "tokens": ["Ach", "!", "wie", "mag", "ich", "dan\u00b7ck\u00b7bar", "seyn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "VMFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Deinen Himmel zu verlassen/", "tokens": ["Dei\u00b7nen", "Him\u00b7mel", "zu", "ver\u00b7las\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In die Welt zu treten ein/", "tokens": ["In", "die", "Welt", "zu", "tre\u00b7ten", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "ART", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Reich und Herrschafft/ Thron und Leben", "tokens": ["Reich", "und", "Herr\u00b7schafft", "/", "Thron", "und", "Le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$(", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "F\u00fcr den Feind in Raub zu geben!", "tokens": ["F\u00fcr", "den", "Feind", "in", "Raub", "zu", "ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Liebe/ du thust solche Wunder!", "tokens": ["Lie\u00b7be", "/", "du", "thust", "sol\u00b7che", "Wun\u00b7der", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebe/ deren keine gleich!", "tokens": ["Lie\u00b7be", "/", "de\u00b7ren", "kei\u00b7ne", "gleich", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PDS", "PIAT", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach! da\u00df solcher Liebe Zunder", "tokens": ["Ach", "!", "da\u00df", "sol\u00b7cher", "Lie\u00b7be", "Zun\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "KOUS", "PIAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mein gefrornes Hertz erweich!", "tokens": ["Mein", "ge\u00b7fror\u00b7nes", "Hertz", "er\u00b7weich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach! da\u00df sie mich auch entz\u00fcnde", "tokens": ["Ach", "!", "da\u00df", "sie", "mich", "auch", "ent\u00b7z\u00fcn\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "KOUS", "PPER", "PRF", "ADV", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und zur Gegen-Brunst verbinde!", "tokens": ["Und", "zur", "Ge\u00b7gen\u00b7Brunst", "ver\u00b7bin\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Veyeln/ Rosen last uns pfl\u00fccken/", "tokens": ["Vey\u00b7eln", "/", "Ro\u00b7sen", "last", "uns", "pfl\u00fc\u00b7cken", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "VVFIN", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So viel ihr zu finden seyn/", "tokens": ["So", "viel", "ihr", "zu", "fin\u00b7den", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PTKZU", "VVINF", "VAINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dem liebsten Kinde schicken!", "tokens": ["Und", "dem", "liebs\u00b7ten", "Kin\u00b7de", "schi\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Last uns Palmen sammlen ein/", "tokens": ["Last", "uns", "Pal\u00b7men", "samm\u00b7len", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "VVFIN", "ART", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und auff Salems frischen H\u00f6hen", "tokens": ["Und", "auff", "Sa\u00b7lems", "fri\u00b7schen", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NE", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihm damit entgegen gehen!", "tokens": ["Ihm", "da\u00b7mit", "ent\u00b7ge\u00b7gen", "ge\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PAV", "PTKVZ", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Eitle Blumen m\u00fcssen bleichen", "tokens": ["Eit\u00b7le", "Blu\u00b7men", "m\u00fcs\u00b7sen", "blei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gegen dieser Ro\u00df im Thal/", "tokens": ["Ge\u00b7gen", "die\u00b7ser", "Ro\u00df", "im", "Thal", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "APPRART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was die Erde kan erreichen", "tokens": ["Was", "die", "Er\u00b7de", "kan", "er\u00b7rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gleichet nicht des Himmels Saal.", "tokens": ["Glei\u00b7chet", "nicht", "des", "Him\u00b7mels", "Saal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Drum ich Hertz-Kraut/ Sonnenwende/", "tokens": ["Drum", "ich", "Hertz\u00b7Kraut", "/", "Son\u00b7nen\u00b7wen\u00b7de", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "PPER", "NN", "$(", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Jesu/ dir zum Opffer sende!", "tokens": ["Je\u00b7su", "/", "dir", "zum", "Opf\u00b7fer", "sen\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}