{"textgrid.poem.40781": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Seht, sie haben an das Rathaus aufgeklebt ein neu Edikt,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seht, sie haben an das Rathaus aufgeklebt ein neu Edikt,", "tokens": ["Seht", ",", "sie", "ha\u00b7ben", "an", "das", "Rat\u00b7haus", "auf\u00b7ge\u00b7klebt", "ein", "neu", "E\u00b7dikt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VAFIN", "APPR", "ART", "NN", "VVFIN", "ART", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Drauf aus den geschlungenen Lettern noch manch andre Schlinge blickt;", "tokens": ["Drauf", "aus", "den", "ge\u00b7schlun\u00b7ge\u00b7nen", "Let\u00b7tern", "noch", "manch", "and\u00b7re", "Schlin\u00b7ge", "blickt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "ART", "ADJA", "NN", "ADV", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+--+-+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Ein possirlich kleines M\u00e4nnlein liest's und h\u00e4lt sich still und stumm,", "tokens": ["Ein", "pos\u00b7sir\u00b7lich", "klei\u00b7nes", "M\u00e4nn\u00b7lein", "liest's", "und", "h\u00e4lt", "sich", "still", "und", "stumm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "PRF", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Unterf\u00e4ngt sich nicht zu murren, leise fr\u00e4gt es nur: Warum?", "tokens": ["Un\u00b7ter\u00b7f\u00e4ngt", "sich", "nicht", "zu", "mur\u00b7ren", ",", "lei\u00b7se", "fr\u00e4gt", "es", "nur", ":", "Wa\u00b7rum", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PRF", "PTKNEG", "PTKZU", "VVINF", "$,", "ADJD", "VVFIN", "PPER", "ADV", "$.", "PWAV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Auf der Kanzel st\u00f6hnt, wie Eulen, wimmernd gegen Sonnenlicht,", "tokens": ["Auf", "der", "Kan\u00b7zel", "st\u00f6hnt", ",", "wie", "Eu\u00b7len", ",", "wim\u00b7mernd", "ge\u00b7gen", "Son\u00b7nen\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,", "PWAV", "NN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Hier ein M\u00f6nch, an dem die Kutte wohl das einz'ge Dunkle nicht,", "tokens": ["Hier", "ein", "M\u00f6nch", ",", "an", "dem", "die", "Kut\u00b7te", "wohl", "das", "einz'\u00b7ge", "Dunk\u00b7le", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "APPR", "PRELS", "ART", "NN", "ADV", "ART", "ADJA", "NN", "PTKNEG", "$,"], "meter": "--+-+-+-+-+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Dort ein Abt, an dem der Krummstab wohl nicht Alles ist, was krumm;", "tokens": ["Dort", "ein", "Abt", ",", "an", "dem", "der", "Krumm\u00b7stab", "wohl", "nicht", "Al\u00b7les", "ist", ",", "was", "krumm", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "APPR", "PRELS", "ART", "NN", "ADV", "PTKNEG", "PIS", "VAFIN", "$,", "PRELS", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Stets gelassen h\u00f6rt's der Kleine, lispelnd leise nur: Warum?", "tokens": ["Stets", "ge\u00b7las\u00b7sen", "h\u00f6rt's", "der", "Klei\u00b7ne", ",", "lis\u00b7pelnd", "lei\u00b7se", "nur", ":", "Wa\u00b7rum", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVPP", "VVFIN", "ART", "ADJA", "$,", "ADJD", "ADJD", "ADV", "$.", "PWAV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.3": {"line.1": {"text": "Wenn mit Hellebard' und Spie\u00dfen sie auf Spatzen r\u00fccken aus,", "tokens": ["Wenn", "mit", "Hel\u00b7le\u00b7bard'", "und", "Spie\u00b7\u00dfen", "sie", "auf", "Spat\u00b7zen", "r\u00fc\u00b7cken", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NE", "KON", "NN", "PPER", "APPR", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wenn sie lichtscheu ohne Fenster aufgebaut ihr neues Haus,", "tokens": ["Wenn", "sie", "licht\u00b7scheu", "oh\u00b7ne", "Fens\u00b7ter", "auf\u00b7ge\u00b7baut", "ihr", "neu\u00b7es", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Wenn das Schwert, das sie befreite, sie zu Fesseln schmieden um,", "tokens": ["Wenn", "das", "Schwert", ",", "das", "sie", "be\u00b7frei\u00b7te", ",", "sie", "zu", "Fes\u00b7seln", "schmie\u00b7den", "um", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "PPER", "APPR", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Sieht er's ruhig und gelassen, fragt nur still vor sich: Warum?", "tokens": ["Sieht", "er's", "ru\u00b7hig", "und", "ge\u00b7las\u00b7sen", ",", "fragt", "nur", "still", "vor", "sich", ":", "Wa\u00b7rum", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "KON", "VVPP", "$,", "VVFIN", "ADV", "ADJD", "APPR", "PRF", "$.", "PWAV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.4": {"line.1": {"text": "Wenn sie mit Kanonen schie\u00dfen auf die Lerche, leichtbeschwingt,", "tokens": ["Wenn", "sie", "mit", "Ka\u00b7no\u00b7nen", "schie\u00b7\u00dfen", "auf", "die", "Ler\u00b7che", ",", "leicht\u00b7be\u00b7schwingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "APPR", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Die, wie ein Gebet der Freiheit, singend durch die Wolken dringt,", "tokens": ["Die", ",", "wie", "ein", "Ge\u00b7bet", "der", "Frei\u00b7heit", ",", "sin\u00b7gend", "durch", "die", "Wol\u00b7ken", "dringt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "ART", "NN", "ART", "NN", "$,", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+----+-+-+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Wenn den Dichtergaul am Markte sie beim Schwanze z\u00e4umen um,", "tokens": ["Wenn", "den", "Dich\u00b7ter\u00b7gaul", "am", "Mark\u00b7te", "sie", "beim", "Schwan\u00b7ze", "z\u00e4u\u00b7men", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN", "PPER", "APPRART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Will er drob sogar nicht lachen, sondern seufzet nur: Warum?", "tokens": ["Will", "er", "drob", "so\u00b7gar", "nicht", "la\u00b7chen", ",", "son\u00b7dern", "seuf\u00b7zet", "nur", ":", "Wa\u00b7rum", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "PTKNEG", "VVINF", "$,", "KON", "VVFIN", "ADV", "$.", "PWAV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.5": {"line.1": {"text": "Auf der Sprache garbenreichem unerme\u00dfnem Erntefeld", "tokens": ["Auf", "der", "Spra\u00b7che", "gar\u00b7ben\u00b7rei\u00b7chem", "un\u00b7er\u00b7me\u00df\u00b7nem", "Ern\u00b7te\u00b7feld"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Hat ein einz'ges goldnes K\u00f6rnlein er sich liebend auserw\u00e4hlt;", "tokens": ["Hat", "ein", "einz'\u00b7ges", "gold\u00b7nes", "K\u00f6rn\u00b7lein", "er", "sich", "lie\u00b7bend", "au\u00b7ser\u00b7w\u00e4hlt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "ADJA", "NN", "PPER", "PRF", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Und aus ihrem reichen Meere, rauschend laut um ihn herum,", "tokens": ["Und", "aus", "ih\u00b7rem", "rei\u00b7chen", "Mee\u00b7re", ",", "rau\u00b7schend", "laut", "um", "ihn", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN", "$,", "ADJD", "ADJD", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Fischt' er eine einz'ge Perle, nur das M\u00e4nnerwort: Warum?", "tokens": ["Fischt'", "er", "ei\u00b7ne", "einz'\u00b7ge", "Per\u00b7le", ",", "nur", "das", "M\u00e4n\u00b7ner\u00b7wort", ":", "Wa\u00b7rum", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "ADV", "ART", "NN", "$.", "PWAV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.6": {"line.1": {"text": "Doch der weise Rath bescheidet streng vor sich den Mann und spricht:", "tokens": ["Doch", "der", "wei\u00b7se", "Rath", "be\u00b7schei\u00b7det", "streng", "vor", "sich", "den", "Mann", "und", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "ADJD", "APPR", "PRF", "ART", "NN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "\u00bbeurer frevelhaften Frage ziemt, f\u00fcrwahr, die Antwort nicht!", "tokens": ["\u00bb", "eu\u00b7rer", "fre\u00b7vel\u00b7haf\u00b7ten", "Fra\u00b7ge", "ziemt", ",", "f\u00fcr\u00b7wahr", ",", "die", "Ant\u00b7wort", "nicht", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "VVFIN", "$,", "ADV", "$,", "ART", "NN", "PTKNEG", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Unser Thun, es sei dem Volke ein verschlo\u00dfnes Heiligthum!\u00ab", "tokens": ["Un\u00b7ser", "Thun", ",", "es", "sei", "dem", "Vol\u00b7ke", "ein", "ver\u00b7schlo\u00df\u00b7nes", "Hei\u00b7lig\u00b7thum", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Ruhig h\u00f6rt den Spruch das M\u00e4nnlein, nur bescheiden fragt's: Warum?", "tokens": ["Ru\u00b7hig", "h\u00f6rt", "den", "Spruch", "das", "M\u00e4nn\u00b7lein", ",", "nur", "be\u00b7schei\u00b7den", "fragt's", ":", "Wa\u00b7rum", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "ART", "NN", "$,", "ADV", "ADJD", "VVFIN", "$.", "PWAV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.7": {"line.1": {"text": "W\u00fcthend springen All' vom Sessel, da\u00df der Rathstisch taumelt drein!", "tokens": ["W\u00fct\u00b7hend", "sprin\u00b7gen", "All'", "vom", "Ses\u00b7sel", ",", "da\u00df", "der", "Rath\u00b7stisch", "tau\u00b7melt", "drein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PIS", "APPRART", "NN", "$,", "KOUS", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "In Arrest bei Brod und Wasser zieh'n sie den Rebellen ein,", "tokens": ["In", "Ar\u00b7rest", "bei", "Brod", "und", "Was\u00b7ser", "zieh'n", "sie", "den", "Re\u00b7bel\u00b7len", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Lassen in den Bock ihn spannen, und in Eisen schlie\u00dfen krumm:", "tokens": ["Las\u00b7sen", "in", "den", "Bock", "ihn", "span\u00b7nen", ",", "und", "in", "Ei\u00b7sen", "schlie\u00b7\u00dfen", "krumm", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NE", "PPER", "VVINF", "$,", "KON", "APPR", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Doch er duldet's still gelassen, spricht kein W\u00f6rtlein, als: Warum?", "tokens": ["Doch", "er", "dul\u00b7det's", "still", "ge\u00b7las\u00b7sen", ",", "spricht", "kein", "W\u00f6rt\u00b7lein", ",", "als", ":", "Wa\u00b7rum", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "VVPP", "$,", "VVFIN", "PIAT", "NN", "$,", "KOUS", "$.", "PWAV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.8": {"line.1": {"text": "Morgens mu\u00df er geh'n zur Beichte, dann aufs Feld im Karren fort!", "tokens": ["Mor\u00b7gens", "mu\u00df", "er", "geh'n", "zur", "Beich\u00b7te", ",", "dann", "aufs", "Feld", "im", "Kar\u00b7ren", "fort", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "APPRART", "NN", "$,", "ADV", "APPRART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Sch\u00fctzen steh'n in Reih' und Gliede, laden stumm die Flinten dort;", "tokens": ["Sch\u00fct\u00b7zen", "steh'n", "in", "Reih'", "und", "Glie\u00b7de", ",", "la\u00b7den", "stumm", "die", "Flin\u00b7ten", "dort", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,", "VVFIN", "ADJD", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Feuer! ruft's, die R\u00f6hre krachen! Blutig sinkt der Frevler um,", "tokens": ["Feu\u00b7er", "!", "ruft's", ",", "die", "R\u00f6h\u00b7re", "kra\u00b7chen", "!", "Blu\u00b7tig", "sinkt", "der", "Frev\u00b7ler", "um", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "$,", "ART", "NN", "VVINF", "$.", "ADJD", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Doch von bleichen Lippen schaurig st\u00f6hnt es r\u00f6chelnd noch: Warum?", "tokens": ["Doch", "von", "blei\u00b7chen", "Lip\u00b7pen", "schau\u00b7rig", "st\u00f6hnt", "es", "r\u00f6\u00b7chelnd", "noch", ":", "Wa\u00b7rum", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "ADJD", "VVFIN", "PPER", "ADJD", "ADV", "$.", "PWAV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.9": {"line.1": {"text": "Ueber seine Leichengrube w\u00e4lzen sie noch einen Stein,", "tokens": ["Ue\u00b7ber", "sei\u00b7ne", "Lei\u00b7chen\u00b7gru\u00b7be", "w\u00e4l\u00b7zen", "sie", "noch", "ei\u00b7nen", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Dann zum feierlichen Hochamt eilen sie zum Dom hinein,", "tokens": ["Dann", "zum", "fei\u00b7er\u00b7li\u00b7chen", "Hoc\u00b7hamt", "ei\u00b7len", "sie", "zum", "Dom", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Br\u00fcnstig danken sie dem Himmel, da\u00df der Schreier endlich stumm,", "tokens": ["Br\u