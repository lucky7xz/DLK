{"dta.poem.8779": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Zweytes Buch .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.57", "af:0.42"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Im Wasser wogt die Lilie, die blanke, hin und her,", "tokens": ["Im", "Was\u00b7ser", "wogt", "die", "Li\u00b7lie", ",", "die", "blan\u00b7ke", ",", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "$,", "ADV", "KON", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Doch irrst du, Freund, so bald du sagst, sie schwanke hin", "tokens": ["Doch", "irrst", "du", ",", "Freund", ",", "so", "bald", "du", "sagst", ",", "sie", "schwan\u00b7ke", "hin"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "NN", "$,", "ADV", "ADV", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es wurzelt ja so fest ihr Fu\u00df im tiefen Meeresgrund,", "tokens": ["Es", "wur\u00b7zelt", "ja", "so", "fest", "ihr", "Fu\u00df", "im", "tie\u00b7fen", "Mee\u00b7res\u00b7grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADJD", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Ihr Haupt nur wiegt ein lieblicher Gedanke hin und her!", "tokens": ["Ihr", "Haupt", "nur", "wiegt", "ein", "lieb\u00b7li\u00b7cher", "Ge\u00b7dan\u00b7ke", "hin", "und", "her", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}