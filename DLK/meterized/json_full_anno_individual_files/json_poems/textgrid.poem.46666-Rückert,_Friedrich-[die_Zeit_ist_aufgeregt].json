{"textgrid.poem.46666": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[die Zeit ist aufgeregt]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Zeit ist aufgeregt;", "tokens": ["Die", "Zeit", "ist", "auf\u00b7ge\u00b7regt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Gott wei\u00df, was sie bewegt;", "tokens": ["Gott", "wei\u00df", ",", "was", "sie", "be\u00b7wegt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf welches hohe Ziel", "tokens": ["Auf", "wel\u00b7ches", "ho\u00b7he", "Ziel"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PWAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ihr Kampf ist angelegt!", "tokens": ["Ihr", "Kampf", "ist", "an\u00b7ge\u00b7legt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich wei\u00df, da\u00df sich der Sturm", "tokens": ["Ich", "wei\u00df", ",", "da\u00df", "sich", "der", "Sturm"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PRF", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Vor meinem Hauch nicht legt;", "tokens": ["Vor", "mei\u00b7nem", "Hauch", "nicht", "legt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Sturm, der Wellen peitscht,", "tokens": ["Der", "Sturm", ",", "der", "Wel\u00b7len", "peitscht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und Weltbautr\u00fcmmer fegt.", "tokens": ["Und", "Welt\u00b7bau\u00b7tr\u00fcm\u00b7mer", "fegt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Doch einen sucht das Leid,", "tokens": ["Doch", "ei\u00b7nen", "sucht", "das", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Der es im Stillen pflegt,", "tokens": ["Der", "es", "im", "Stil\u00b7len", "pflegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Zu retten durch die Zeit,", "tokens": ["Zu", "ret\u00b7ten", "durch", "die", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Was keine Zeit umhegt.", "tokens": ["Was", "kei\u00b7ne", "Zeit", "um\u00b7hegt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Wie durchs emp\u00f6rte Meer,", "tokens": ["Wie", "durchs", "em\u00b7p\u00f6r\u00b7te", "Meer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Das gegen Himmel schl\u00e4gt,", "tokens": ["Das", "ge\u00b7gen", "Him\u00b7mel", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Das ihr vertraute Pfand", "tokens": ["Das", "ihr", "ver\u00b7trau\u00b7te", "Pfand"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Die Perlenmuschel tr\u00e4gt.", "tokens": ["Die", "Per\u00b7len\u00b7mu\u00b7schel", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Zeit ist aufgeregt;", "tokens": ["Die", "Zeit", "ist", "auf\u00b7ge\u00b7regt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Gott wei\u00df, was sie bewegt;", "tokens": ["Gott", "wei\u00df", ",", "was", "sie", "be\u00b7wegt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf welches hohe Ziel", "tokens": ["Auf", "wel\u00b7ches", "ho\u00b7he", "Ziel"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PWAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ihr Kampf ist angelegt!", "tokens": ["Ihr", "Kampf", "ist", "an\u00b7ge\u00b7legt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich wei\u00df, da\u00df sich der Sturm", "tokens": ["Ich", "wei\u00df", ",", "da\u00df", "sich", "der", "Sturm"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PRF", "ART", "NN"], "meter": "-+