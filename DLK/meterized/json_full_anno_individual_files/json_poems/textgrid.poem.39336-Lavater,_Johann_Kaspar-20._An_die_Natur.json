{"textgrid.poem.39336": {"metadata": {"author": {"name": "Lavater, Johann Kaspar", "birth": "N.A.", "death": "N.A."}, "title": "20. An die Natur", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erscheine mir im Taggewand,", "tokens": ["Er\u00b7schei\u00b7ne", "mir", "im", "Tag\u00b7ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Nachtgewand, Natur!", "tokens": ["Im", "Nacht\u00b7ge\u00b7wand", ",", "Na\u00b7tur", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$,", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Reich deinem Sohn die Mutterhand,", "tokens": ["Reich", "dei\u00b7nem", "Sohn", "die", "Mut\u00b7ter\u00b7hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihm einen Finger nur!", "tokens": ["Ihm", "ei\u00b7nen", "Fin\u00b7ger", "nur", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dich zu erkennen, welch ein Gl\u00fcck,", "tokens": ["Dich", "zu", "er\u00b7ken\u00b7nen", ",", "welch", "ein", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "PWAT", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu f\u00fchlen, welche Lust!", "tokens": ["Zu", "f\u00fch\u00b7len", ",", "wel\u00b7che", "Lust", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PWAT", "NN",