{"textgrid.poem.58409": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "447. Immer daheim", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott ist mir immer nah, daheim und in der Ferne,", "tokens": ["Gott", "ist", "mir", "im\u00b7mer", "nah", ",", "da\u00b7heim", "und", "in", "der", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "ADJD", "$,", "ADV", "KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er denket stets an mich, er spricht mit mir so gerne;", "tokens": ["Er", "den\u00b7ket", "stets", "an", "mich", ",", "er", "spricht", "mit", "mir", "so", "ger\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "$,", "PPER", "VVFIN", "APPR", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kein Ort ihn schlie\u00dfet ein, kein Ort ihn schlie\u00dfet aus,", "tokens": ["Kein", "Ort", "ihn", "schlie\u00b7\u00dfet", "ein", ",", "kein", "Ort", "ihn", "schlie\u00b7\u00dfet", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "VVFIN", "PTKVZ", "$,", "PIAT", "NN", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wer in Gott wohnen kann, ist \u00fcberall zu Haus.", "tokens": ["Wer", "in", "Gott", "woh\u00b7nen", "kann", ",", "ist", "\u00fc\u00b7be\u00b7rall", "zu", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "wor