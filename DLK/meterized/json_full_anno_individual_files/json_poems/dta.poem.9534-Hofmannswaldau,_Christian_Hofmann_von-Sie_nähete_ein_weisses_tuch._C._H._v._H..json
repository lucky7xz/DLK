{"dta.poem.9534": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Sie n\u00e4hete ein weisses tuch.  \n C. H. v. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es f\u00fchrte Lesbia in ihrer weissen hand", "tokens": ["Es", "f\u00fchr\u00b7te", "Les\u00b7bia", "in", "ih\u00b7rer", "weis\u00b7sen", "hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NE", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ein wundersch\u00f6nes tuch/ dem kreide nicht zu gleichen/", "tokens": ["Ein", "wun\u00b7der\u00b7sch\u00f6\u00b7nes", "tuch", "/", "dem", "krei\u00b7de", "nicht", "zu", "glei\u00b7chen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "NN", "PTKNEG", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So nur alleine will dem schnee der h\u00e4nde weichen/", "tokens": ["So", "nur", "al\u00b7lei\u00b7ne", "will", "dem", "schnee", "der", "h\u00e4n\u00b7de", "wei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VMFIN", "ART", "NN", "ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil dieser es besch\u00fctzt vor ihrer augen brand.", "tokens": ["Weil", "die\u00b7ser", "es", "be\u00b7sch\u00fctzt", "vor", "ih\u00b7rer", "au\u00b7gen", "brand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch scheint es/ wie sich selbst das k\u00f6stliche gewand", "tokens": ["Doch", "scheint", "es", "/", "wie", "sich", "selbst", "das", "k\u00f6st\u00b7li\u00b7che", "ge\u00b7wand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$(", "PWAV", "PRF", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Blo\u00df und alleine will von dieser sonne bleichen/", "tokens": ["Blo\u00df", "und", "al\u00b7lei\u00b7ne", "will", "von", "die\u00b7ser", "son\u00b7ne", "blei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "VMFIN", "APPR", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und mu\u00df die nadel gleich durch seine faden streichen/", "tokens": ["Und", "mu\u00df", "die", "na\u00b7del", "gleich", "durch", "sei\u00b7ne", "fa\u00b7den", "strei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So wird es doch durch di\u00df ie mehr und mehr bekandt.", "tokens": ["So", "wird", "es", "doch", "durch", "di\u00df", "ie", "mehr", "und", "mehr", "be\u00b7kandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PDS", "ADV", "ADV", "KON", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "O wundersch\u00f6nes tuch! dir bl\u00fchet das gel\u00fccke/", "tokens": ["O", "wun\u00b7der\u00b7sch\u00f6\u00b7nes", "tuch", "!", "dir", "bl\u00fc\u00b7het", "das", "ge\u00b7l\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Jhr auge zieret dich/ mich t\u00f6dten dessen blicke/", "tokens": ["Ihr", "au\u00b7ge", "zie\u00b7ret", "dich", "/", "mich", "t\u00f6d\u00b7ten", "des\u00b7sen", "bli\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$(", "PPER", "VVFIN", "PDS", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Dich macht es lieb und werth/ mir hat es ha\u00df gebracht/", "tokens": ["Dich", "macht", "es", "lieb", "und", "werth", "/", "mir", "hat", "es", "ha\u00df", "ge\u00b7bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$(", "PPER", "VAFIN", "PPER", "VVFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Dein faden f\u00fchlt die stich/ ich f\u00fchle sie im hertzen/", "tokens": ["Dein", "fa\u00b7den", "f\u00fchlt", "die", "stich", "/", "ich", "f\u00fch\u00b7le", "sie", "im", "hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVFIN", "ART", "ADJD", "$(", "PPER", "VVFIN", "PPER", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Dir bringt er ehr un\u0303 ruhm/ mir macht er noth un\u0303 schmerzen/", "tokens": ["Dir", "bringt", "er", "ehr", "u\u00f1", "ruhm", "/", "mir", "macht", "er", "noth", "u\u00f1", "schmer\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "KON", "NN", "$(", "PPER", "VVFIN", "PPER", "NN", "KON", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Dich setzt er an den tag/ mich in die todes-nacht.", "tokens": ["Dich", "setzt", "er", "an", "den", "tag", "/", "mich", "in", "die", "to\u00b7des\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$(", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}