{"dta.poem.18348": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Tres faciunt Collegium.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Drin machen sich ekelerregend breit", "tokens": ["Drin", "ma\u00b7chen", "sich", "e\u00b7ke\u00b7ler\u00b7re\u00b7gend", "breit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ADJD", "ADJD"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Kr\u00f6te, Basilisk und Unke;", "tokens": ["Kr\u00f6\u00b7te", ",", "Ba\u00b7si\u00b7lisk", "und", "Un\u00b7ke", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und w\u00f6chentlich schon \u2014 juchheideldidum \u2014", "tokens": ["Und", "w\u00f6\u00b7chent\u00b7lich", "schon", "juch\u00b7hei\u00b7del\u00b7di\u00b7dum"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "$(", "FM.la", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Predgen vor ihrem Publikum", "tokens": ["Pred\u00b7gen", "vor", "ih\u00b7rem", "Pub\u00b7li\u00b7kum"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Herr Most, Herr St\u00f6cker und Herr Majunke!", "tokens": ["Herr", "Most", ",", "Herr", "St\u00f6\u00b7cker", "und", "Herr", "Ma\u00b7jun\u00b7ke", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "NN", "NN", "KON", "NN", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}}}}