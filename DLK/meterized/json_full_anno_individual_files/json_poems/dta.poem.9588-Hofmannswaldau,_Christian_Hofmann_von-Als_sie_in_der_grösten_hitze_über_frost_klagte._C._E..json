{"dta.poem.9588": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als sie in der gr\u00f6sten hitze \u00fcber frost klagte.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aurora/ die den blitz in beyden augen f\u00fchrt/", "tokens": ["Au\u00b7ro\u00b7ra", "/", "die", "den", "blitz", "in", "bey\u00b7den", "au\u00b7gen", "f\u00fchrt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ART", "ART", "NN", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auff deren wangen sich die rothen flammen f\u00e4rben/", "tokens": ["Auff", "de\u00b7ren", "wan\u00b7gen", "sich", "die", "ro\u00b7then", "flam\u00b7men", "f\u00e4r\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "VVFIN", "PRF", "ART", "ADJA", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Will/ da des himmels glut den gantzen erd-krey\u00df r\u00fchrt/", "tokens": ["Will", "/", "da", "des", "him\u00b7mels", "glut", "den", "gant\u00b7zen", "erd\u00b7krey\u00df", "r\u00fchrt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$(", "KOUS", "ART", "NN", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Jm sommer/ nur allein/ vor frost und k\u00e4lte sterben.", "tokens": ["Jm", "som\u00b7mer", "/", "nur", "al\u00b7lein", "/", "vor", "frost", "und", "k\u00e4l\u00b7te", "ster\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$(", "ADV", "ADV", "$(", "APPR", "NN", "KON", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jhr mund/ der feur und brand auff seinen lippen tr\u00e4gt/", "tokens": ["Ihr", "mund", "/", "der", "feur", "und", "brand", "auff", "sei\u00b7nen", "lip\u00b7pen", "tr\u00e4gt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "ART", "NN", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "F\u00fchrt \u00fcber schnee und ei\u00df sehr ungegr\u00fcndte klagen/", "tokens": ["F\u00fchrt", "\u00fc\u00b7ber", "schnee", "und", "ei\u00df", "sehr", "un\u00b7ge\u00b7gr\u00fcnd\u00b7te", "kla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "ADJD", "ADV", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und will/ ob schon die glut uns blut und adern regt/", "tokens": ["Und", "will", "/", "ob", "schon", "die", "glut", "uns", "blut", "und", "a\u00b7dern", "regt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "$(", "KOUS", "ADV", "ART", "NN", "PPER", "NN", "KON", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Doch nur von rauher lufft und kaltem wetter sagen.", "tokens": ["Doch", "nur", "von", "rau\u00b7her", "lufft", "und", "kal\u00b7tem", "wet\u00b7ter", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Jhr gantzer leib erstarrt/ die stirne selbst erbleicht/", "tokens": ["Ihr", "gant\u00b7zer", "leib", "er\u00b7starrt", "/", "die", "stir\u00b7ne", "selbst", "er\u00b7bleicht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$(", "ART", "ADJA", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und ihre liljen-brust/ die erst dem helffen-beine/", "tokens": ["Und", "ih\u00b7re", "lil\u00b7jen\u00b7brust", "/", "die", "erst", "dem", "helf\u00b7fen\u00b7bei\u00b7ne", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$(", "ART", "ADV", "ART", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Jtzt aber halb erbla\u00dft dem todten winter gleicht/", "tokens": ["Jtzt", "a\u00b7ber", "halb", "er\u00b7bla\u00dft", "dem", "tod\u00b7ten", "win\u00b7ter", "gleicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Erstirbt/ verwandelt sich/ und wird wie fast zum steine:", "tokens": ["Er\u00b7stirbt", "/", "ver\u00b7wan\u00b7delt", "sich", "/", "und", "wird", "wie", "fast", "zum", "stei\u00b7ne", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "VVFIN", "PRF", "$(", "KON", "VAFIN", "KOKOM", "ADV", "APPRART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der fall/ der sie betrifft/ ist sattsam uns bekandt.", "tokens": ["Der", "fall", "/", "der", "sie", "be\u00b7tr\u00b7ifft", "/", "ist", "satt\u00b7sam", "uns", "be\u00b7kandt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "VVFIN", "$(", "VAFIN", "ADJD", "PPER", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Was aber hier vor rath? wird man sie sterben lassen?", "tokens": ["Was", "a\u00b7ber", "hier", "vor", "rath", "?", "wird", "man", "sie", "ster\u00b7ben", "las\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "APPR", "NN", "$.", "VAFIN", "PIS", "PPER", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ist gar kein retten mehr/ kein mittel nicht zur hand?", "tokens": ["Ist", "gar", "kein", "ret\u00b7ten", "mehr", "/", "kein", "mit\u00b7tel", "nicht", "zur", "hand", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "ADV", "$(", "PIAT", "NN", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Auff Amor! la\u00df sie doch nicht vor der zeit erblassen.", "tokens": ["Auff", "A\u00b7mor", "!", "la\u00df", "sie", "doch", "nicht", "vor", "der", "zeit", "er\u00b7blas\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$.", "VVIMP", "PPER", "ADV", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Hilff/ denn du kanst es thun durch deinen heissen strahl/", "tokens": ["Hilff", "/", "denn", "du", "kanst", "es", "thun", "durch", "dei\u00b7nen", "heis\u00b7sen", "strahl", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KON", "PPER", "VMFIN", "PPER", "VVINF", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Z\u00fcnd ihre geister an/ la\u00df sie in flammen schwitzen/", "tokens": ["Z\u00fcnd", "ih\u00b7re", "geis\u00b7ter", "an", "/", "la\u00df", "sie", "in", "flam\u00b7men", "schwit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "PTKVZ", "$(", "VVIMP", "PPER", "APPR", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Zerbrich ihr kaltes hertz/ das h\u00e4rter noch denn stahl/", "tokens": ["Zer\u00b7brich", "ihr", "kal\u00b7tes", "hertz", "/", "das", "h\u00e4r\u00b7ter", "noch", "denn", "stahl", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "ADJA", "NN", "$(", "ART", "ADJA", "ADV", "ADV", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "F\u00fcll es bi\u00df oben an mit deinen reichen blitzen/", "tokens": ["F\u00fcll", "es", "bi\u00df", "o\u00b7ben", "an", "mit", "dei\u00b7nen", "rei\u00b7chen", "blit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "APPR", "APPR", "PPOSAT", "ADJA", "VVFIN", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.21": {"text": "Zerschmeltze diesen schnee/ der ihre brust bedeckt/", "tokens": ["Zer\u00b7schmelt\u00b7ze", "die\u00b7sen", "schnee", "/", "der", "ih\u00b7re", "brust", "be\u00b7deckt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "$(", "ART", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und la\u00df sie brand und glut in allen adern finden;", "tokens": ["Und", "la\u00df", "sie", "brand", "und", "glut", "in", "al\u00b7len", "a\u00b7dern", "fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "VVFIN", "KON", "NN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Doch nim ihr feur in acht/ das in den augen steckt/", "tokens": ["Doch", "nim", "ihr", "feur", "in", "acht", "/", "das", "in", "den", "au\u00b7gen", "steckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPOSAT", "NN", "APPR", "CARD", "$(", "ART", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Sonst m\u00f6cht es beydes dich und alle welt entz\u00fcnden.", "tokens": ["Sonst", "m\u00f6cht", "es", "bey\u00b7des", "dich", "und", "al\u00b7le", "welt", "ent\u00b7z\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIS", "PPER", "KON", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}