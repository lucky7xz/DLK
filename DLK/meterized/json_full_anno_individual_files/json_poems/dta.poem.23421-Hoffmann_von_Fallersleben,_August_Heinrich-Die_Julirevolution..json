{"dta.poem.23421": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Julirevolution.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Unsre lieben H\u00fchnerchen", "tokens": ["Uns\u00b7re", "lie\u00b7ben", "H\u00fch\u00b7ner\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Verloren ihren Hahn,", "tokens": ["Ver\u00b7lo\u00b7ren", "ih\u00b7ren", "Hahn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hatten ihm zu Leide", "tokens": ["Hat\u00b7ten", "ihm", "zu", "Lei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Zwar auch mal was gethan.", "tokens": ["Zwar", "auch", "mal", "was", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PIS", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie trauerten die H\u00fchnerchen,", "tokens": ["Wie", "trau\u00b7er\u00b7ten", "die", "H\u00fch\u00b7ner\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$,"], "meter": "-+---+--", "measure": "dactylic.init"}, "line.6": {"text": "Da\u00df sie ihn nicht mehr sahn,", "tokens": ["Da\u00df", "sie", "ihn", "nicht", "mehr", "sahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Den lieben guten Hahn!", "tokens": ["Den", "lie\u00b7ben", "gu\u00b7ten", "Hahn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Unsre lieben H\u00fchnerchen", "tokens": ["Uns\u00b7re", "lie\u00b7ben", "H\u00fch\u00b7ner\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Sahn einen andern Hahn,", "tokens": ["Sahn", "ei\u00b7nen", "an\u00b7dern", "Hahn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der da ging spatzieren", "tokens": ["Der", "da", "ging", "spat\u00b7zie\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "VVFIN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Auf einem gr\u00fcnen Plan.", "tokens": ["Auf", "ei\u00b7nem", "gr\u00fc\u00b7nen", "Plan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie freuten sich die H\u00fchnerchen:", "tokens": ["Wie", "freu\u00b7ten", "sich", "die", "H\u00fch\u00b7ner\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.6": {"text": "Komm, la\u00df dich froh empfahn", "tokens": ["Komm", ",", "la\u00df", "dich", "froh", "em\u00b7pfahn"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVIMP", "PPER", "ADJD", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und sei du unser Hahn!", "tokens": ["Und", "sei", "du", "un\u00b7ser", "Hahn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u201emeine lieben H\u00fchnerchen,", "tokens": ["\u201e", "mei\u00b7ne", "lie\u00b7ben", "H\u00fch\u00b7ner\u00b7chen", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Gern bin ich euer Hahn:", "tokens": ["Gern", "bin", "ich", "eu\u00b7er", "Hahn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wahrheit wird die Charte!", "tokens": ["Wahr\u00b7heit", "wird", "die", "Char\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ihr k\u00f6nnt mich froh empfahn \u2014\u201c", "tokens": ["Ihr", "k\u00f6nnt", "mich", "froh", "em\u00b7pfahn", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "VVPP", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie jubelten die H\u00fchnerchen:", "tokens": ["Wie", "ju\u00b7bel\u00b7ten", "die", "H\u00fch\u00b7ner\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$."], "meter": "-+---+--", "measure": "dactylic.init"}, "line.6": {"text": "Wir sind dir unterthan,", "tokens": ["Wir", "sind", "dir", "un\u00b7ter\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Du bist der beste Hahn!", "tokens": ["Du", "bist", "der", "bes\u00b7te", "Hahn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Unsre lieben H\u00fchnerchen", "tokens": ["Uns\u00b7re", "lie\u00b7ben", "H\u00fch\u00b7ner\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Die f\u00fchrten ihren Hahn", "tokens": ["Die", "f\u00fchr\u00b7ten", "ih\u00b7ren", "Hahn"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Voller Freud und Jubel", "tokens": ["Vol\u00b7ler", "Freud", "und", "Ju\u00b7bel"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Hoch auf den Schlo\u00dfaltan.", "tokens": ["Hoch", "auf", "den", "Schlo\u00df\u00b7al\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie war'n entz\u00fcckt die H\u00fchnerchen,", "tokens": ["Wie", "wa\u00b7r'n", "ent\u00b7z\u00fcckt", "die", "H\u00fch\u00b7ner\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "VVPP", "ART", "NN", "$,"], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Als da zu kr\u00e4hn begann", "tokens": ["Als", "da", "zu", "kr\u00e4hn", "be\u00b7gann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PTKZU", "VVINF", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der neue Hahnemann!", "tokens": ["Der", "neu\u00b7e", "Hah\u00b7ne\u00b7mann", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}