{"dta.poem.5742": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Eigenschafften der  \n Materie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Allein,", "tokens": ["Al\u00b7lein", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Wenn man aus der Materie,", "tokens": ["Wenn", "man", "aus", "der", "Ma\u00b7te\u00b7rie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die fruchtbar ist von so viel Dingen,", "tokens": ["Die", "frucht\u00b7bar", "ist", "von", "so", "viel", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "APPR", "ADV", "PIAT", "NN", "$,"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dem C\u00f6rper, der ein Grund, so allgemein", "tokens": ["Dem", "C\u00f6r\u00b7per", ",", "der", "ein", "Grund", ",", "so", "all\u00b7ge\u00b7mein"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "$,", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Von allen C\u00f6rpern ist, die Welt hervor wil bringen.", "tokens": ["Von", "al\u00b7len", "C\u00f6r\u00b7pern", "ist", ",", "die", "Welt", "her\u00b7vor", "wil", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "$,", "ART", "NN", "PTKVZ", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Erfodert es die Schuldigkeit,", "tokens": ["Er\u00b7fo\u00b7dert", "es", "die", "Schul\u00b7dig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df ihre Eigenschafft erkl\u00e4ret werde.", "tokens": ["Da\u00df", "ih\u00b7re", "Ei\u00b7gen\u00b7schafft", "er\u00b7kl\u00e4\u00b7ret", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Sie ist ", "tokens": ["Sie", "ist"], "token_info": ["word", "word"], "pos": ["PPER", "VAFIN"], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Sie ist ein jedes Ding insonderheit.", "tokens": ["Sie", "ist", "ein", "je\u00b7des", "Ding", "in\u00b7son\u00b7der\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Doch alles dieses zeigt noch nicht,", "tokens": ["Doch", "al\u00b7les", "die\u00b7ses", "zeigt", "noch", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PDAT", "VVFIN", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Was sie recht eigentlich, und wie sie zugericht.", "tokens": ["Was", "sie", "recht", "ei\u00b7gent\u00b7lich", ",", "und", "wie", "sie", "zu\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "$,", "KON", "PWAV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}