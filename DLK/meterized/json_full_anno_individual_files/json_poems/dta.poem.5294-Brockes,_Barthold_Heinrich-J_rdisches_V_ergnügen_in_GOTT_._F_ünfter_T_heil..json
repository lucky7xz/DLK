{"dta.poem.5294": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zuweilen sich zusammen setzen, den gantzen Kreis der L\u00fcf-", "tokens": ["Zu\u00b7wei\u00b7len", "sich", "zu\u00b7sam\u00b7men", "set\u00b7zen", ",", "den", "gant\u00b7zen", "Kreis", "der", "L\u00fcf"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PRF", "ADV", "VVINF", "$,", "ART", "ADJA", "NN", "ART", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "te f\u00fcllen", "tokens": ["te", "f\u00fcl\u00b7len"], "token_info": ["word", "word"], "pos": ["NE", "VVINF"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Und oft der Sonnen glantz dadurch verdecken, und oft", "tokens": ["Und", "oft", "der", "Son\u00b7nen", "glantz", "da\u00b7durch", "ver\u00b7de\u00b7cken", ",", "und", "oft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "NN", "PAV", "VVINF", "$,", "KON", "ADV"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.4": {"text": "gantz verh\u00fcllen.", "tokens": ["gantz", "ver\u00b7h\u00fcl\u00b7len", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Doch ist der Duft nicht dicht und schwartz, wie er vorhin", "tokens": ["Doch", "ist", "der", "Duft", "nicht", "dicht", "und", "schwartz", ",", "wie", "er", "vor\u00b7hin"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NN", "PTKNEG", "ADJD", "KON", "ADJD", "$,", "PWAV", "PPER", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "im Winter war,", "tokens": ["im", "Win\u00b7ter", "war", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Er ist, mit Glantz und Licht vermischt, zwar tr\u00fcb\u2019 und falb\u2019", "tokens": ["Er", "ist", ",", "mit", "Glantz", "und", "Licht", "ver\u00b7mischt", ",", "zwar", "tr\u00fcb'", "und", "falb'"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "APPR", "NN", "KON", "NN", "VVPP", "$,", "ADV", "ADJD", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "und dennoch klar,", "tokens": ["und", "den\u00b7noch", "klar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Es scheint die\u00df d\u00e4mmricht-sanfte Wesen von Fruchtbarkeit,", "tokens": ["Es", "scheint", "die\u00df", "d\u00e4mm\u00b7richt\u00b7sanf\u00b7te", "We\u00b7sen", "von", "Frucht\u00b7bar\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "von Licht und Schein,", "tokens": ["von", "Licht", "und", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Von Hofnung und von Seegen schwanger, und schwer vom", "tokens": ["Von", "Hof\u00b7nung", "und", "von", "See\u00b7gen", "schwan\u00b7ger", ",", "und", "schwer", "vom"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "ADJD", "$,", "KON", "ADJD", "APPRART"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Ueber-Flu\u00df zu seyn.", "tokens": ["Ue\u00b7ber\u00b7Flu\u00df", "zu", "seyn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VAINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.13": {"text": "Man sieht, nicht sonder sanfte Freude, sie sanft bald hie, bald", "tokens": ["Man", "sieht", ",", "nicht", "son\u00b7der", "sanf\u00b7te", "Freu\u00b7de", ",", "sie", "sanft", "bald", "hie", ",", "bald"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PIS", "VVFIN", "$,", "PTKNEG", "ADV", "ADJA", "NN", "$,", "PPER", "ADJD", "ADV", "ADV", "$,", "ADV"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "dort hin, ziehn,", "tokens": ["dort", "hin", ",", "ziehn", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "VVINF", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.15": {"text": "Und einen kleinen lauen Regen bald hie, bald da, bald", "tokens": ["Und", "ei\u00b7nen", "klei\u00b7nen", "lau\u00b7en", "Re\u00b7gen", "bald", "hie", ",", "bald", "da", ",", "bald"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN", "ADV", "ADV", "$,", "ADV", "ADV", "$,", "ADV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.16": {"text": "dorten, spr\u00fch'n.", "tokens": ["dor\u00b7ten", ",", "spr\u00fch'", "n."], "token_info": ["word", "punct", "word", "abbreviation"], "pos": ["ADV", "$,", "VVFIN", "NE"], "meter": "+-+", "measure": "trochaic.di"}, "line.17": {"text": "In solcher tr\u00fcben Fr\u00fchlings-Zeit empfindet man, wie eine", "tokens": ["In", "sol\u00b7cher", "tr\u00fc\u00b7ben", "Fr\u00fch\u00b7lings\u00b7Zeit", "emp\u00b7fin\u00b7det", "man", ",", "wie", "ei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN", "VVFIN", "PIS", "$,", "PWAV", "ART"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.18": {"text": "Stille,", "tokens": ["Stil\u00b7le", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.19": {"text": "So wie sie dort das Firmament, auch unsre Seele lieb-", "tokens": ["So", "wie", "sie", "dort", "das", "Fir\u00b7ma\u00b7ment", ",", "auch", "uns\u00b7re", "See\u00b7le", "lie\u00b7b"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "PPER", "ADV", "ART", "NN", "$,", "ADV", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.20": {"text": "lich f\u00fclle.", "tokens": ["lich", "f\u00fcl\u00b7le", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.21": {"text": "Ein angenehm ", "tokens": ["Ein", "an\u00b7ge\u00b7nehm"], "token_info": ["word", "word"], "pos": ["ART", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.22": {"text": "Nerven r\u00fchrt,", "tokens": ["Ner\u00b7ven", "r\u00fchrt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.23": {"text": "Wird, so wie \u00fcberall empfunden, auch \u00fcberall von uns", "tokens": ["Wird", ",", "so", "wie", "\u00fc\u00b7be\u00b7rall", "emp\u00b7fun\u00b7den", ",", "auch", "\u00fc\u00b7be\u00b7rall", "von", "uns"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "$,", "ADV", "KOKOM", "ADV", "VVPP", "$,", "ADV", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.24": {"text": "versp\u00fchrt.", "tokens": ["ver\u00b7sp\u00fchrt", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.25": {"text": "Ach m\u00f6gten wir die sanfte Sch\u00f6nheit der lauen Luft im fro-", "tokens": ["Ach", "m\u00f6g\u00b7ten", "wir", "die", "sanf\u00b7te", "Sch\u00f6n\u00b7heit", "der", "lau\u00b7en", "Luft", "im", "fro"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "APPRART", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.26": {"text": "hen Lentzen,", "tokens": ["hen", "Lent\u00b7zen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.27": {"text": "Worinn voll Anmuth, W\u00e4rm und Seegen, der Sonnen", "tokens": ["Wo\u00b7rinn", "voll", "An\u00b7muth", ",", "W\u00e4rm", "und", "See\u00b7gen", ",", "der", "Son\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADJD", "NN", "$,", "NN", "KON", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.28": {"text": "helle Strahlen gl\u00e4ntzen,", "tokens": ["hel\u00b7le", "Strah\u00b7len", "gl\u00e4nt\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.29": {"text": "Der Sonnen Sonn\u2019 und HErrn zu Ehren, mit innigli-", "tokens": ["Der", "Son\u00b7nen", "Sonn'", "und", "Herrn", "zu", "Eh\u00b7ren", ",", "mit", "in\u00b7nig\u00b7li"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "NN", "APPR", "NN", "$,", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "cher Anmuth sehn,", "tokens": ["cher", "An\u00b7muth", "sehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.31": {"text": "Und seine Weisheit, Lieb und Allmacht, in unsrer frohen", "tokens": ["Und", "sei\u00b7ne", "Weis\u00b7heit", ",", "Lieb", "und", "All\u00b7macht", ",", "in", "uns\u00b7rer", "fro\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "$,", "NN", "KON", "NN", "$,", "APPR", "PPOSAT", "ADJA"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.32": {"text": "Seel' erh\u00f6hn!", "tokens": ["Seel'", "er\u00b7h\u00f6hn", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}