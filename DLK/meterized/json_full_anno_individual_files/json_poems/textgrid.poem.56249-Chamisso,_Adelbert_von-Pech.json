kens": ["Doch", "mein", "Va\u00b7ter", "war", "ein", "B\u00fcr\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und das ist genug gesagt.", "tokens": ["Und", "das", "ist", "ge\u00b7nug", "ge\u00b7sagt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Wahrlich aus mir h\u00e4tte vieles", "tokens": ["Wahr\u00b7lich", "aus", "mir", "h\u00e4t\u00b7te", "vie\u00b7les"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPER", "VAFIN", "PIS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Werden k\u00f6nnen in der Welt,", "tokens": ["Wer\u00b7den", "k\u00f6n\u00b7nen", "in", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VMFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00e4tte t\u00fcckisch nicht mein Schicksal", "tokens": ["H\u00e4t\u00b7te", "t\u00fc\u00b7ckisch", "nicht", "mein", "Schick\u00b7sal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PTKNEG", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich mir in den Weg gestellt.", "tokens": ["Sich", "mir", "in", "den", "Weg", "ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}