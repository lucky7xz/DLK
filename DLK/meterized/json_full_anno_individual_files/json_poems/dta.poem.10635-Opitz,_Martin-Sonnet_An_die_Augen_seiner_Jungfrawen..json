{"dta.poem.10635": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet  \n  An die Augen seiner Jungfrawen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Leit Sternen meines Haupts vnd meiner jungen Zeit/", "tokens": ["Leit", "Ster\u00b7nen", "mei\u00b7nes", "Haupts", "vnd", "mei\u00b7ner", "jun\u00b7gen", "Zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PPOSAT", "NN", "KON", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die als Planeten sein gesetzet meinem Leben/", "tokens": ["Die", "als", "Pla\u00b7ne\u00b7ten", "sein", "ge\u00b7set\u00b7zet", "mei\u00b7nem", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOUS", "NN", "VAINF", "VVFIN", "PPOSAT", "NN", "$("], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Ihr Augen/ wann ich euch so freundlich sehe schweben/", "tokens": ["Ihr", "Au\u00b7gen", "/", "wann", "ich", "euch", "so", "freund\u00b7lich", "se\u00b7he", "schwe\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PWAV", "PPER", "PPER", "ADV", "ADJD", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bin ich gleich als entz\u00fcckt/ f\u00fcr vnerh\u00f6rter Frewd/", "tokens": ["Bin", "ich", "gleich", "als", "ent\u00b7z\u00fcckt", "/", "f\u00fcr", "vner\u00b7h\u00f6r\u00b7ter", "Frewd", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "KOKOM", "VVPP", "$(", "APPR", "ADJA", "NN", "$("], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Dann jhr beschliest in euch ein hohe Liebligkeit/", "tokens": ["Dann", "jhr", "be\u00b7schliest", "in", "euch", "ein", "ho\u00b7he", "Lieb\u00b7lig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vnd lieblich hoheit: Ihr/ jhr k\u00f6nnt alleine geben", "tokens": ["Vnd", "lieb\u00b7lich", "ho\u00b7heit", ":", "Ihr", "/", "jhr", "k\u00f6nnt", "al\u00b7lei\u00b7ne", "ge\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "NN", "$.", "PPER", "$(", "PPER", "VVFIN", "ADV", "VVINF"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Gen\u00fcge/ rechte Lust/ vnd nach dem alle streben/", "tokens": ["Ge\u00b7n\u00fc\u00b7ge", "/", "rech\u00b7te", "Lust", "/", "vnd", "nach", "dem", "al\u00b7le", "stre\u00b7ben", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ADJA", "NN", "$(", "KON", "APPR", "ART", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ist v\u00f6lliglich bey euch/ O mein Gestirn/ bereit.", "tokens": ["Ist", "v\u00f6l\u00b7lig\u00b7lich", "bey", "euch", "/", "O", "mein", "Ge\u00b7stirn", "/", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "PPER", "$(", "NE", "PPOSAT", "NN", "$(", "ADJD", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Natura selber ligt in Finsternu\u00df begraben/", "tokens": ["Na\u00b7tu\u00b7ra", "sel\u00b7ber", "ligt", "in", "Fins\u00b7ter\u00b7nu\u00df", "be\u00b7gra\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Vnd mangelt jhres Liechts/ von wegen jhrer Gaben/", "tokens": ["Vnd", "man\u00b7gelt", "jhres", "Liechts", "/", "von", "we\u00b7gen", "jhrer", "Ga\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$(", "APPR", "APPR", "PPOSAT", "NN", "$("], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Die gantz beschlossen sindt in solcher engen statt/", "tokens": ["Die", "gantz", "be\u00b7schlos\u00b7sen", "sindt", "in", "sol\u00b7cher", "en\u00b7gen", "statt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "VAFIN", "APPR", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Doch ist sie enge nicht/ vnd thut sich weit ergiessen/", "tokens": ["Doch", "ist", "sie", "en\u00b7ge", "nicht", "/", "vnd", "thut", "sich", "weit", "er\u00b7gies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVFIN", "PTKNEG", "$(", "KON", "VVFIN", "PRF", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ja w\u00fcst\u2019 vnd gro\u00df genug fast alles einzuschliessen/", "tokens": ["Ja", "w\u00fcst'", "vnd", "gro\u00df", "ge\u00b7nug", "fast", "al\u00b7les", "ein\u00b7zu\u00b7schlies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "KON", "ADJD", "ADV", "ADV", "PIS", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Weil sich mein arme Seel in jhr verjrret hatt.", "tokens": ["Weil", "sich", "mein", "ar\u00b7me", "Seel", "in", "jhr", "ver\u00b7jr\u00b7ret", "hatt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "ADJA", "NN", "APPR", "PPER", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}