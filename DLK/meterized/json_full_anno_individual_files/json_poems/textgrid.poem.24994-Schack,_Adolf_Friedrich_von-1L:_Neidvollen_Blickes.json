{"textgrid.poem.24994": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Neidvollen Blickes", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Neidvollen Blickes", "tokens": ["Neid\u00b7vol\u00b7len", "Bli\u00b7ckes"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Empor zu euch schau' ich,", "tokens": ["Em\u00b7por", "zu", "euch", "schau'", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr hohen Unsterblichen,", "tokens": ["Ihr", "ho\u00b7hen", "U\u00b7nsterb\u00b7li\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.4": {"text": "Die ihr auf Himmelsgipfeln,", "tokens": ["Die", "ihr", "auf", "Him\u00b7mels\u00b7gip\u00b7feln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Einsiedler des Ruhms,", "tokens": ["Ein\u00b7sied\u00b7ler", "des", "Ruhms", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Im ewigen Lichte wohnt,", "tokens": ["Im", "e\u00b7wi\u00b7gen", "Lich\u00b7te", "wohnt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und von den strahlenden Scheiteln", "tokens": ["Und", "von", "den", "strah\u00b7len\u00b7den", "Schei\u00b7teln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "Geschlechter auf Geschlechter der Menschen", "tokens": ["Ge\u00b7schlech\u00b7ter", "auf", "Ge\u00b7schlech\u00b7ter", "der", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "ART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Mit eurer Werke Glanz erleuchtet!", "tokens": ["Mit", "eu\u00b7rer", "Wer\u00b7ke", "Glanz", "er\u00b7leuch\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Weh dem Armen hier unten,", "tokens": ["Weh", "dem", "Ar\u00b7men", "hier", "un\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "ADV", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Dem, gleich euch zu den heiligen H\u00f6hen zu klimmen,", "tokens": ["Dem", ",", "gleich", "euch", "zu", "den", "hei\u00b7li\u00b7gen", "H\u00f6\u00b7hen", "zu", "klim\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "ADV", "PPER", "APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "In die Seele der Trieb gepflanzt ist,", "tokens": ["In", "die", "See\u00b7le", "der", "Trieb", "ge\u00b7pflanzt", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Aber zu schwach die Kraft!", "tokens": ["A\u00b7ber", "zu", "schwach", "die", "Kraft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKA", "ADJD", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Ewig ihm vor dem Geiste schwebt", "tokens": ["E\u00b7wig", "ihm", "vor", "dem", "Geis\u00b7te", "schwebt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "APPR", "ART", "NN", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Die himmlische Sch\u00f6nheit,", "tokens": ["Die", "himm\u00b7li\u00b7sche", "Sch\u00f6n\u00b7heit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Die er in Formen bannen m\u00f6chte!", "tokens": ["Die", "er", "in", "For\u00b7men", "ban\u00b7nen", "m\u00f6ch\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Doch nicht der Prometheusfunke", "tokens": ["Doch", "nicht", "der", "Pro\u00b7me\u00b7theus\u00b7fun\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Glimmt in der Brust ihm,", "tokens": ["Glimmt", "in", "der", "Brust", "ihm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPER", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Da\u00df er das marmorentstiegene Bild", "tokens": ["Da\u00df", "er", "das", "mar\u00b7mo\u00b7rent\u00b7stie\u00b7ge\u00b7ne", "Bild"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}, "line.11": {"text": "Mit Sch\u00f6pferglut beseele.", "tokens": ["Mit", "Sch\u00f6p\u00b7fer\u00b7glut", "be\u00b7see\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "In jeder Fr\u00fche", "tokens": ["In", "je\u00b7der", "Fr\u00fc\u00b7he"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Schwanken Schrittes eilt er zur Werkstatt,", "tokens": ["Schwan\u00b7ken", "Schrit\u00b7tes", "eilt", "er", "zur", "Werk\u00b7statt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.14": {"text": "Und im Hoffen und Zweifeln und Zagen", "tokens": ["Und", "im", "Hof\u00b7fen", "und", "Zwei\u00b7feln", "und", "Za\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "KON", "NN", "KON", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.15": {"text": "Zittert sein Herz,", "tokens": ["Zit\u00b7tert", "sein", "Herz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.16": {"text": "W\u00e4hrend die Hand den Mei\u00dfel f\u00fchrt;", "tokens": ["W\u00e4h\u00b7rend", "die", "Hand", "den", "Mei\u00b7\u00dfel", "f\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.17": {"text": "Aber starr bleibt der Stein;", "tokens": ["A\u00b7ber", "starr", "bleibt", "der", "Stein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.18": {"text": "Statt da\u00df er des G\u00f6ttervaters Antlitz", "tokens": ["Statt", "da\u00df", "er", "des", "G\u00f6t\u00b7ter\u00b7va\u00b7ters", "Ant\u00b7litz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KOUS", "PPER", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "In olympischer Hoheit", "tokens": ["In", "o\u00b7lym\u00b7pi\u00b7scher", "Ho\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.20": {"text": "Ihm entsteigen s\u00e4he,", "tokens": ["Ihm", "ent\u00b7stei\u00b7gen", "s\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.21": {"text": "Blicken verzerrte Z\u00fcge", "tokens": ["Bli\u00b7cken", "ver\u00b7zerr\u00b7te", "Z\u00fc\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.22": {"text": "Wie zum Hohn ihm entgegen.", "tokens": ["Wie", "zum", "Hohn", "ihm", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "PPER", "PTKVZ", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.23": {"text": "Da sinkt ihm ermattet die Hand;", "tokens": ["Da", "sinkt", "ihm", "er\u00b7mat\u00b7tet", "die", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.24": {"text": "Und seufzend all derer gedenkt er,", "tokens": ["Und", "seuf\u00b7zend", "all", "de\u00b7rer", "ge\u00b7denkt", "er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PIAT", "PDS", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Die, wie er, gestrebt und gerungen \u2013", "tokens": ["Die", ",", "wie", "er", ",", "ge\u00b7strebt", "und", "ge\u00b7run\u00b7gen", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "PPER", "$,", "VVPP", "KON", "VVPP", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.26": {"text": "Und ruhmlos ins Grab gesunken.", "tokens": ["Und", "ruh\u00b7mlos", "ins", "Grab", "ge\u00b7sun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "VVPP", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.27": {"text": "Ueber sich hin die Scharen", "tokens": ["Ue\u00b7ber", "sich", "hin", "die", "Scha\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRF", "ADV", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.28": {"text": "Der Erlesenen sieht er ziehen,", "tokens": ["Der", "Er\u00b7le\u00b7se\u00b7nen", "sieht", "er", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Der G\u00f6tters\u00f6hne,", "tokens": ["Der", "G\u00f6t\u00b7ter\u00b7s\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.30": {"text": "Die, von des Genius Fl\u00fcgeln getragen,", "tokens": ["Die", ",", "von", "des", "Ge\u00b7nius", "Fl\u00fc\u00b7geln", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+---+--+-", "measure": "iambic.tri.relaxed"}, "line.31": {"text": "Zu den sonnigen Gipfeln eilen;", "tokens": ["Zu", "den", "son\u00b7ni\u00b7gen", "Gip\u00b7feln", "ei\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.32": {"text": "Aber um ihn hoch und h\u00f6her", "tokens": ["A\u00b7ber", "um", "ihn", "hoch", "und", "h\u00f6\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Schwillt der Strom", "tokens": ["Schwillt", "der", "Strom"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.34": {"text": "Des niederen Erdentreibens", "tokens": ["Des", "nie\u00b7de\u00b7ren", "Er\u00b7den\u00b7trei\u00b7bens"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.35": {"text": "Und will hinweg ihn rei\u00dfen von dem Altar,", "tokens": ["Und", "will", "hin\u00b7weg", "ihn", "rei\u00b7\u00dfen", "von", "dem", "Al\u00b7tar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.36": {"text": "An dem er fruchtlos geopfert.", "tokens": ["An", "dem", "er", "frucht\u00b7los", "ge\u00b7o\u00b7pfert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O blickt mild auf ihn herab, ihr Unsterblichen!", "tokens": ["O", "blickt", "mild", "auf", "ihn", "her\u00b7ab", ",", "ihr", "U\u00b7nsterb\u00b7li\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "APPR", "PPER", "ADV", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+---", "measure": "unknown.measure.penta"}, "line.2": {"text": "Gie\u00dft Mut und Kraft ihm ins Herz,", "tokens": ["Gie\u00dft", "Mut", "und", "Kraft", "ihm", "ins", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Da\u00df er ausharre im heiligen Amte!", "tokens": ["Da\u00df", "er", "aus\u00b7har\u00b7re", "im", "hei\u00b7li\u00b7gen", "Am\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "---+--+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Einen Strahl eures Geistes", "tokens": ["Ei\u00b7nen", "Strahl", "eu\u00b7res", "Geis\u00b7tes"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Sendet hernieder zu ihm", "tokens": ["Sen\u00b7det", "her\u00b7nie\u00b7der", "zu", "ihm"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PPER"], "meter": "+--+-++", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Und la\u00dft, ob auch sp\u00e4t,", "tokens": ["Und", "la\u00dft", ",", "ob", "auch", "sp\u00e4t", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ADV", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Ein Werk, nur ", "tokens": ["Ein", "Werk", ",", "nur"], "token_info": ["word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Das ein Denkmal auf Erden ihm sei,", "tokens": ["Das", "ein", "Denk\u00b7mal", "auf", "Er\u00b7den", "ihm", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "APPR", "NN", "PPER", "VAFIN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.9": {"text": "Auf da\u00df er nicht gleich den andern", "tokens": ["Auf", "da\u00df", "er", "nicht", "gleich", "den", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "PTKNEG", "ADV", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Kindern des Staubes", "tokens": ["Kin\u00b7dern", "des", "Stau\u00b7bes"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "In den Wirbeln des Lebens", "tokens": ["In", "den", "Wir\u00b7beln", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.12": {"text": "Spurlos verschwinde,", "tokens": ["Spur\u00b7los", "ver\u00b7schwin\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADJA", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Und dessen, was er war, nicht alles", "tokens": ["Und", "des\u00b7sen", ",", "was", "er", "war", ",", "nicht", "al\u00b7les"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PDS", "$,", "PWS", "PPER", "VAFIN", "$,", "PTKNEG", "PIS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Das gierige Grab verschlinge!", "tokens": ["Das", "gie\u00b7ri\u00b7ge", "Grab", "ver\u00b7schlin\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Neidvollen Blickes", "tokens": ["Neid\u00b7vol\u00b7len", "Bli\u00b7ckes"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.rel