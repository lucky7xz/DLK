7bes"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "In den Wirbeln des Lebens", "tokens": ["In", "den", "Wir\u00b7beln", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.12": {"text": "Spurlos verschwinde,", "tokens": ["Spur\u00b7los", "ver\u00b7schwin\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADJA", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Und dessen, was er war, nicht alles", "tokens": ["Und", "des\u00b7sen", ",", "was", "er", "war", ",", "nicht", "al\u00b7les"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PDS", "$,", "PWS", "PPER", "VAFIN", "$,", "PTKNEG", "PIS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Das gierige Grab verschlinge!", "tokens": ["Das", "gie\u00b7ri\u00b7ge", "Grab", "ver\u00b7schlin\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}