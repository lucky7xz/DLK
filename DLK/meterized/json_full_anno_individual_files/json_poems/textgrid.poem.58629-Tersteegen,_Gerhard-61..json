{"textgrid.poem.58629": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "61.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es preist der H\u00f6chste selbst allhier die Treu der ersten Liebe,", "tokens": ["Es", "preist", "der", "H\u00f6chs\u00b7te", "selbst", "all\u00b7hier", "die", "Treu", "der", "ers\u00b7ten", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADV", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Da sich die Seel' auf ewig ihm zu seiner Braut verschriebe;", "tokens": ["Da", "sich", "die", "Seel'", "auf", "e\u00b7wig", "ihm", "zu", "sei\u00b7ner", "Braut", "ver\u00b7schrie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "APPR", "ADJD", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Wie war das Herz so keusch und zart, wie willig war der Sinn!", "tokens": ["Wie", "war", "das", "Herz", "so", "keusch", "und", "zart", ",", "wie", "wil\u00b7lig", "war", "der", "Sinn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$,", "PWAV", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Sie lie\u00df die Welt und folgte Gott getrost zur W\u00fcste hin,", "tokens": ["Sie", "lie\u00df", "die", "Welt", "und", "folg\u00b7te", "Gott", "ge\u00b7trost", "zur", "W\u00fcs\u00b7te", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "NN", "VVPP", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Zur W\u00fcste, da sie Gott zulieb die Kreatur wollt' missen,", "tokens": ["Zur", "W\u00fcs\u00b7te", ",", "da", "sie", "Gott", "zu\u00b7lieb", "die", "Kre\u00b7a\u00b7tur", "wollt'", "mis\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "PPER", "NN", "VVFIN", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Zur W\u00fcste, da sie sonst nichts wollt', als Jesus Christus wissen;", "tokens": ["Zur", "W\u00fcs\u00b7te", ",", "da", "sie", "sonst", "nichts", "wollt'", ",", "als", "Je\u00b7sus", "Chris\u00b7tus", "wis\u00b7sen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "PPER", "ADV", "PIS", "VMFIN", "$,", "KOUS", "NE", "NE", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.7": {"text": "Da war die Seel' so sch\u00f6n und fromm,", "tokens": ["Da", "war", "die", "Seel'", "so", "sch\u00f6n", "und", "fromm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da war sie Gottes Heiligtum.", "tokens": ["Da", "war", "sie", "Got\u00b7tes", "Hei\u00b7lig\u00b7tum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wem Gott ge