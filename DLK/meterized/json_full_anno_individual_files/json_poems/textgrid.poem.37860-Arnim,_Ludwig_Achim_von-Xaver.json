{"textgrid.poem.37860": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Xaver", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als nach Japon weit entlegen,", "tokens": ["Als", "nach", "Ja\u00b7pon", "weit", "ent\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NE", "ADJD", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Xaver dachte, Gottes Mann,", "tokens": ["Xa\u00b7ver", "dach\u00b7te", ",", "Got\u00b7tes", "Mann", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle waren ihm entgegen,", "tokens": ["Al\u00b7le", "wa\u00b7ren", "ihm", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fielen ihn mit Worten an,", "tokens": ["Fie\u00b7len", "ihn", "mit", "Wor\u00b7ten", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wind und Wetter, Meer und Wellen,", "tokens": ["Wind", "und", "Wet\u00b7ter", ",", "Meer", "und", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mahlten seinen Augen dar,", "tokens": ["Mahl\u00b7ten", "sei\u00b7nen", "Au\u00b7gen", "dar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Redten viel von Ungef\u00e4llen,", "tokens": ["Red\u00b7ten", "viel", "von", "Un\u00b7ge\u00b7f\u00e4l\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Von Gewitter und Gefahr.", "tokens": ["Von", "Ge\u00b7wit\u00b7ter", "und", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbschweiget, schweiget von Gewitter,", "tokens": ["\u00bb", "schwei\u00b7get", ",", "schwei\u00b7get", "von", "Ge\u00b7wit\u00b7ter", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach, von Winden schweiget still:", "tokens": ["Ach", ",", "von", "Win\u00b7den", "schwei\u00b7get", "still", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "APPR", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nie, noch wahrer Held, noch Ritter", "tokens": ["Nie", ",", "noch", "wah\u00b7rer", "Held", ",", "noch", "Rit\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "ADV", "ADJA", "NN", "$,", "ADV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Achtet solcher Kinderspiel:", "tokens": ["Ach\u00b7tet", "sol\u00b7cher", "Kin\u00b7der\u00b7spiel", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lasset Wind und Wetter blasen,", "tokens": ["Las\u00b7set", "Wind", "und", "Wet\u00b7ter", "bla\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Flamm der Lieb, vom Blasen w\u00e4chst,", "tokens": ["Flamm", "der", "Lieb", ",", "vom", "Bla\u00b7sen", "w\u00e4chst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Lasset Meer und Wellen rasen,", "tokens": ["Las\u00b7set", "Meer", "und", "Wel\u00b7len", "ra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wellen gehn zum Himmel n\u00e4chst.", "tokens": ["Wel\u00b7len", "gehn", "zum", "Him\u00b7mel", "n\u00e4chst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ey doch lasset ab von Scherzen,", "tokens": ["Ey", "doch", "las\u00b7set", "ab", "von", "Scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PTKVZ", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schrecket mich mit keiner Noth,", "tokens": ["Schre\u00b7cket", "mich", "mit", "kei\u00b7ner", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Noch Soldat, noch Krieges-Herzen,", "tokens": ["Noch", "Sol\u00b7dat", ",", "noch", "Krie\u00b7ge\u00b7sHer\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcrchten nimmer Kraut und Loth;", "tokens": ["F\u00fcrch\u00b7ten", "nim\u00b7mer", "Kraut", "und", "Loth", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Spie\u00df und Pfeil, und blo\u00dfe Degen,", "tokens": ["Spie\u00df", "und", "Pfeil", ",", "und", "blo\u00b7\u00dfe", "De\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Rohr, Pistol und B\u00fcchsen-Spei\u00df,", "tokens": ["Rohr", ",", "Pis\u00b7tol", "und", "B\u00fcch\u00b7sen\u00b7S\u00b7pei\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Macht Soldaten mehr verwegen", "tokens": ["Macht", "Sol\u00b7da\u00b7ten", "mehr", "ver\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und sie lockt zum Ehren-Prei\u00df.", "tokens": ["Und", "sie", "lockt", "zum", "Eh\u00b7ren\u00b7Prei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Lasset ihre H\u00f6rner wetzen,", "tokens": ["Las\u00b7set", "ih\u00b7re", "H\u00f6r\u00b7ner", "wet\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wind, und Wetter ungest\u00fcm,", "tokens": ["Wind", ",", "und", "Wet\u00b7ter", "un\u00b7ge\u00b7st\u00fcm", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00dft die Wellen brummend schwetzen", "tokens": ["La\u00dft", "die", "Wel\u00b7len", "brum\u00b7mend", "schwet\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Trommeln schlagen um,", "tokens": ["Und", "die", "Trom\u00b7meln", "schla\u00b7gen", "um", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nord und S\u00fcden, Ost und Westen,", "tokens": ["Nord", "und", "S\u00fc\u00b7den", ",", "Ost", "und", "Wes\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "K\u00e4mpfen la\u00dft auf salzgem Feld;", "tokens": ["K\u00e4mp\u00b7fen", "la\u00dft", "auf", "salz\u00b7gem", "Feld", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nie wirds dem an Ruh gebrechen,", "tokens": ["Nie", "wirds", "dem", "an", "Ruh", "ge\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "APPR", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Der nur Fried im Herzen h\u00e4lt.", "tokens": ["Der", "nur", "Fried", "im", "Her\u00b7zen", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wer wills \u00fcber Meer nicht wagen,", "tokens": ["Wer", "wills", "\u00fc\u00b7ber", "Meer", "nicht", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "APPR", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ueber tausend Wasser wild,", "tokens": ["Ue\u00b7ber", "tau\u00b7send", "Was\u00b7ser", "wild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dem es mit dem Pfeil und Bogen,", "tokens": ["Dem", "es", "mit", "dem", "Pfeil", "und", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "ART", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch viel tausend Seelen gilt?", "tokens": ["Noch", "viel", "tau\u00b7send", "See\u00b7len", "gilt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "CARD", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wem will grausen vor den Winden,", "tokens": ["Wem", "will", "grau\u00b7sen", "vor", "den", "Win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "F\u00fcrchten ihre Fl\u00fcgel na\u00df?", "tokens": ["F\u00fcrch\u00b7ten", "ih\u00b7re", "Fl\u00fc\u00b7gel", "na\u00df", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Der nur Seelen denkt zu finden,", "tokens": ["Der", "nur", "See\u00b7len", "denkt", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Seelen sch\u00f6n, ohn alle Maa\u00df.", "tokens": ["See\u00b7len", "sch\u00f6n", ",", "ohn", "al\u00b7le", "Maa\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "KOUI", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Eya stark und freche Wellen,", "tokens": ["Ey\u00b7a", "stark", "und", "fre\u00b7che", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eya stark und stolze Wind',", "tokens": ["Ey\u00b7a", "stark", "und", "stol\u00b7ze", "Wind'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihr mich nimmer sollet f\u00e4llen,", "tokens": ["Ihr", "mich", "nim\u00b7mer", "sol\u00b7let", "f\u00e4l\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ADV", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Euch zu stehn, ich bin gesinnt,", "tokens": ["Euch", "zu", "stehn", ",", "ich", "bin", "ge\u00b7sinnt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Seelen, Seelen mu\u00df ich haben,", "tokens": ["See\u00b7len", ",", "See\u00b7len", "mu\u00df", "ich", "ha\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VMFIN", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sattle dich nur h\u00f6lzern Ro\u00df,", "tokens": ["Satt\u00b7le", "dich", "nur", "h\u00f6l\u00b7zern", "Ro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Du must \u00fcber Wellen traben,", "tokens": ["Du", "must", "\u00fc\u00b7ber", "Wel\u00b7len", "tra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Auf ihr Segel, Anker los!\u00ab", "tokens": ["Auf", "ihr", "Se\u00b7gel", ",", "An\u00b7ker", "los", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "NN", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Als nach Japon weit entlegen,", "tokens": ["Als", "nach", "Ja\u00b7pon", "weit", "ent\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NE", "ADJD", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Xaver dachte, Gottes Mann,", "tokens": ["Xa\u00b7ver", "dach\u00b7te", ",", "Got\u00b7tes", "Mann", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle waren ihm entgegen,", "tokens": ["Al\u00b7le", "wa\u00b7ren", "ihm", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "PTKVZ", "$,"], "