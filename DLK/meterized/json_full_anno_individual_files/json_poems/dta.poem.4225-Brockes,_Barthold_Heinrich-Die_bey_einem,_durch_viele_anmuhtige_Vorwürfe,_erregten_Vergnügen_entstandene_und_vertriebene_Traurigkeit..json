{"dta.poem.4225": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die  \n bey einem,  \n  durch viele anmuhtige Vorw\u00fcrfe, erregten  \n Vergn\u00fcgen  \n entstandene und vertriebene  \n Traurigkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nachdem aus Hamburg meine Kinder, mich zu besu-", "tokens": ["Nach\u00b7dem", "aus", "Ham\u00b7burg", "mei\u00b7ne", "Kin\u00b7der", ",", "mich", "zu", "be\u00b7su"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "APPR", "NE", "PPOSAT", "NN", "$,", "PRF", "APPR", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "chen, kommen waren,", "tokens": ["chen", ",", "kom\u00b7men", "wa\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "VAFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und sie ein kleines Enkelchen von noch nicht v\u00f6lligen zwo", "tokens": ["Und", "sie", "ein", "klei\u00b7nes", "En\u00b7kel\u00b7chen", "von", "noch", "nicht", "v\u00f6l\u00b7li\u00b7gen", "zwo"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ART", "ADJA", "NN", "APPR", "ADV", "PTKNEG", "ADJD", "CARD"], "meter": "-+-+-+--+--+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Jahren,", "tokens": ["Jah\u00b7ren", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "So ich noch nie gesehn, mir zeigten, an dessen lieblichen", "tokens": ["So", "ich", "noch", "nie", "ge\u00b7sehn", ",", "mir", "zeig\u00b7ten", ",", "an", "des\u00b7sen", "lieb\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPER", "ADV", "ADV", "VVPP", "$,", "PPER", "VVFIN", "$,", "APPR", "PRELAT", "ADJA"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "Gestalt", "tokens": ["Ge\u00b7stalt"], "token_info": ["word"], "pos": ["NN"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Und munterm Geist ich mich ergetzte;", "tokens": ["Und", "mun\u00b7term", "Geist", "ich", "mich", "er\u00b7getz\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Indem er jeden alsobald,", "tokens": ["In\u00b7dem", "er", "je\u00b7den", "al\u00b7so\u00b7bald", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der ihn sah\u2019, in Verwundrung setzte.", "tokens": ["Der", "ihn", "sah'", ",", "in", "Ver\u00b7wund\u00b7rung", "setz\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "APPR", "NN", "VVFIN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Ein nie annoch gef\u00fchlt Vergn\u00fcgen fing mein Gebl\u00fct an", "tokens": ["Ein", "nie", "an\u00b7noch", "ge\u00b7f\u00fchlt", "Ver\u00b7gn\u00fc\u00b7gen", "fing", "mein", "Ge\u00b7bl\u00fct", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "VVPP", "NN", "VVFIN", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "zu durchdringen.", "tokens": ["zu", "durch\u00b7drin\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.12": {"text": "Ich setzte mich, sein kindisch Gaukeln, sein schnell Ge-", "tokens": ["Ich", "setz\u00b7te", "mich", ",", "sein", "kin\u00b7disch", "Gau\u00b7keln", ",", "sein", "schnell", "Ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPOSAT", "ADJD", "NN", "$,", "PPOSAT", "ADJD", "TRUNC"], "meter": "-+-+-+-+-++-", "measure": "unknown.measure.hexa"}, "line.13": {"text": "h\u00fcpf, sein stolpernd Gehn,", "tokens": ["h\u00fcpf", ",", "sein", "stol\u00b7pernd", "Gehn", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PPOSAT", "ADJD", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.14": {"text": "Mit frohen Blicken, anzusehn,", "tokens": ["Mit", "fro\u00b7hen", "Bli\u00b7cken", ",", "an\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVIZU", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Und sein mit L\u00e4cheln stets begleitet, schon klug, noch stamm-", "tokens": ["Und", "sein", "mit", "L\u00e4\u00b7cheln", "stets", "be\u00b7glei\u00b7tet", ",", "schon", "klug", ",", "noch", "stam\u00b7m"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPOSAT", "APPR", "NN", "ADV", "VVPP", "$,", "ADV", "ADJD", "$,", "ADV", "TRUNC"], "meter": "-+-+-+-+--+---", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "lendes Get\u00f6n", "tokens": ["len\u00b7des", "Ge\u00b7t\u00f6n"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.17": {"text": "Und halbe W\u00f6rter anzuh\u00f6ren. Inzwischen fing, mit hel-", "tokens": ["Und", "hal\u00b7be", "W\u00f6r\u00b7ter", "an\u00b7zu\u00b7h\u00f6\u00b7ren", ".", "I\u00b7nzwi\u00b7schen", "fing", ",", "mit", "hel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVIZU", "$.", "NN", "VVFIN", "$,", "APPR", "TRUNC"], "meter": "-+-+-+-+----+-+", "measure": "unknown.measure.hexa"}, "line.18": {"text": "lem Singen,", "tokens": ["lem", "Sin\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.19": {"text": "Ein Paar von meinen Kindern an, beym lieblich klingen-", "tokens": ["Ein", "Paar", "von", "mei\u00b7nen", "Kin\u00b7dern", "an", ",", "beym", "lieb\u00b7lich", "klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,", "APPRART", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "den Clavier,", "tokens": ["den", "Cla\u00b7vier", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "--+", "measure": "anapaest.init"}, "line.21": {"text": "Mit einer Fl\u00f6ten Ton begleitet, in einem sch\u00f6nen ", "tokens": ["Mit", "ei\u00b7ner", "Fl\u00f6\u00b7ten", "Ton", "be\u00b7glei\u00b7tet", ",", "in", "ei\u00b7nem", "sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "mir,", "tokens": ["mir", ","], "token_info": ["word", "punct"], "pos": ["PPER", "$,"], "meter": "-", "measure": "single.down"}}, "stanza.2": {"line.1": {"text": "Aus unsers grossen Hendels St\u00fccken, ein liebliches Con-", "tokens": ["Aus", "un\u00b7sers", "gros\u00b7sen", "Hen\u00b7dels", "St\u00fc\u00b7cken", ",", "ein", "lieb\u00b7li\u00b7ches", "Con"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN", "$,", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "cert zu bringen.", "tokens": ["cert", "zu", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Noch mehr, mir ward, von bunten Bluhmen, ein\u2019 ausser-", "tokens": ["Noch", "mehr", ",", "mir", "ward", ",", "von", "bun\u00b7ten", "Bluh\u00b7men", ",", "ein'", "aus\u00b7ser"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "$,", "PPER", "VAFIN", "$,", "APPR", "ADJA", "NN", "$,", "ART", "TRUNC"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "ordentliche Pracht,", "tokens": ["or\u00b7dent\u00b7li\u00b7che", "Pracht", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Die eben erst aus Hamburg kommen, in einer Sch\u00fcssel", "tokens": ["Die", "e\u00b7ben", "erst", "aus", "Ham\u00b7burg", "kom\u00b7men", ",", "in", "ei\u00b7ner", "Sch\u00fcs\u00b7sel"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "APPR", "NE", "VVINF", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "hergebracht,", "tokens": ["her\u00b7ge\u00b7bracht", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Wovon der bunte Blitz nicht minder, als wie der liebliche", "tokens": ["Wo\u00b7von", "der", "bun\u00b7te", "Blitz", "nicht", "min\u00b7der", ",", "als", "wie", "der", "lieb\u00b7li\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "PTKNEG", "ADV", "$,", "KOUS", "KOKOM", "ART", "ADJA"], "meter": "-+-+-+-+--+-+--", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Gesang", "tokens": ["Ge\u00b7sang"], "token_info": ["word"], "pos": ["NN"], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Durchs Ohr, mir durch die frohen Augen, in mein ge-", "tokens": ["Durchs", "Ohr", ",", "mir", "durch", "die", "fro\u00b7hen", "Au\u00b7gen", ",", "in", "mein", "ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PPER", "APPR", "ART", "ADJA", "NN", "$,", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "r\u00fchrtes Herze drang.", "tokens": ["r\u00fchr\u00b7tes", "Her\u00b7ze", "drang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Der Geist war, durch den frommen Inhalt der W\u00f6rter,", "tokens": ["Der", "Geist", "war", ",", "durch", "den", "from\u00b7men", "In\u00b7halt", "der", "W\u00f6r\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "die ich beygef\u00fcget,", "tokens": ["die", "ich", "bey\u00b7ge\u00b7f\u00fc\u00b7get", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.13": {"text": "Dem Sch\u00f6pfer der Natur zu Ehren, dabey nicht weniger", "tokens": ["Dem", "Sch\u00f6p\u00b7fer", "der", "Na\u00b7tur", "zu", "Eh\u00b7ren", ",", "da\u00b7bey", "nicht", "we\u00b7ni\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "APPR", "NN", "$,", "PAV", "PTKNEG", "ADV"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.14": {"text": "vergn\u00fcget.", "tokens": ["ver\u00b7gn\u00fc\u00b7get", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "Indem man nun zu gleicher Zeit, als ich die Anmuht", "tokens": ["In\u00b7dem", "man", "nun", "zu", "glei\u00b7cher", "Zeit", ",", "als", "ich", "die", "An\u00b7muht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "APPR", "ADJA", "NN", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "\u00fcberdachte,", "tokens": ["\u00fc\u00b7berd\u00b7ach\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.17": {"text": "Ein volles angez\u00fcndet Pfeifchen von rauchendem Toback", "tokens": ["Ein", "vol\u00b7les", "an\u00b7ge\u00b7z\u00fcn\u00b7det", "Pfei\u00b7fchen", "von", "rau\u00b7chen\u00b7dem", "To\u00b7back"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VVPP", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "mir brachte,", "tokens": ["mir", "brach\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.19": {"text": "Vermehrt\u2019 es annoch mein Vergn\u00fcgen. Ich sah\u2019, beym", "tokens": ["Ver\u00b7mehrt'", "es", "an\u00b7noch", "mein", "Ver\u00b7gn\u00fc\u00b7gen", ".", "Ich", "sah'", ",", "beym"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "$,", "APPRART"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "schmatzenden Gebrauch,", "tokens": ["schmat\u00b7zen\u00b7den", "Ge\u00b7brauch", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.21": {"text": "Den stets in regen Cirkelchen sich wirbelnden und dreh\u2019n-", "tokens": ["Den", "stets", "in", "re\u00b7gen", "Cir\u00b7kel\u00b7chen", "sich", "wir\u00b7beln\u00b7den", "und", "dreh'\u00b7n"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN", "PRF", "VVINF", "KON", "TRUNC"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "den Rauch", "tokens": ["den", "Rauch"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.23": {"text": "Vergn\u00fcglich an. Allein die\u00df rege, ver\u00e4nderlich- und", "tokens": ["Ver\u00b7gn\u00fcg\u00b7lich", "an", ".", "Al\u00b7lein", "die\u00df", "re\u00b7ge", ",", "ver\u00b7\u00e4n\u00b7der\u00b7lich", "und"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "PTKVZ", "$.", "ADV", "PDS", "ADJA", "$,", "TRUNC", "KON"], "meter": "-+-+----+-+-+-", "measure": "unknown.measure.penta"}, "line.24": {"text": "fl\u00fccht'ge Schweben", "tokens": ["fl\u00fccht'\u00b7ge", "Schwe\u00b7ben"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.25": {"text": "Fing an, ganz andere Gedanken, bey meinen Freuden,", "tokens": ["Fing", "an", ",", "ganz", "an\u00b7de\u00b7re", "Ge\u00b7dan\u00b7ken", ",", "bey", "mei\u00b7nen", "Freu\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "ADV", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "mir zu geben.", "tokens": ["mir", "zu", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.27": {"text": "Ich dachte: Dieser rege Rauch, ein Sinnbild unsrer Le-", "tokens": ["Ich", "dach\u00b7te", ":", "Die\u00b7ser", "re\u00b7ge", "Rauch", ",", "ein", "Sinn\u00b7bild", "uns\u00b7rer", "Le"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PDAT", "ADJA", "NN", "$,", "ART", "NN", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.28": {"text": "bens-Zeit,", "tokens": ["bens\u00b7Zeit", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.29": {"text": "Beweis\u2019t mir, so wie aller Dinge, auch meiner Anmuht", "tokens": ["Be\u00b7weis't", "mir", ",", "so", "wie", "al\u00b7ler", "Din\u00b7ge", ",", "auch", "mei\u00b7ner", "An\u00b7muht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "ADV", "KOKOM", "PIAT", "NN", "$,", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.30": {"text": "Fl\u00fcchtigkeit:", "tokens": ["Fl\u00fcch\u00b7tig\u00b7keit", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.31": {"text": "Wie bald verwelkt der Bluhmen Pracht!", "tokens": ["Wie", "bald", "ver\u00b7welkt", "der", "Bluh\u00b7men", "Pracht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Wie schnell vergehen Ton und Schall!", "tokens": ["Wie", "schnell", "ver\u00b7ge\u00b7hen", "Ton", "und", "Schall", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Leicht wird, durch einen Trauer-Fall,", "tokens": ["Leicht", "wird", ",", "durch", "ei\u00b7nen", "Trau\u00b7e\u00b7rFall", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das, was uns lieb, zur Gruft gebracht.", "tokens": ["Das", ",", "was", "uns", "lieb", ",", "zur", "Gruft", "ge\u00b7bracht", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PPER", "ADJD", "$,", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jtzt sitz ich mitten im Vergn\u00fcgen; allein wie schnell kann", "tokens": ["Jtzt", "sitz", "ich", "mit\u00b7ten", "im", "Ver\u00b7gn\u00fc\u00b7gen", ";", "al\u00b7lein", "wie", "schnell", "kann"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "APPRART", "NN", "$.", "ADV", "KOKOM", "ADJD", "VMFIN"], "meter": "-+-+-+-+-+++-+", "measure": "unknown.measure.octa.plus"}, "line.4": {"text": "es geschehen,", "tokens": ["es", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVPP", "$,"], "meter": "--+-", "measure": "anapaest.init"}, "line.5": {"text": "Da\u00df von der Anmuht, die mich r\u00fchret, wir Wechsel und", "tokens": ["Da\u00df", "von", "der", "An\u00b7muht", ",", "die", "mich", "r\u00fch\u00b7ret", ",", "wir", "Wech\u00b7sel", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "PPER", "NN", "KON"], "meter": "+--+-+-+--+-+", "measure": "iambic.hexa.invert"}, "line.6": {"text": "Ver\u00e4ndrung sehen!", "tokens": ["Ver\u00b7\u00e4n\u00b7drung", "se\u00b7hen", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Ja wenn ich auch so gl\u00fccklich w\u00e4r\u2019, sie Jahren-lang noch", "tokens": ["Ja", "wenn", "ich", "auch", "so", "gl\u00fcck\u00b7lich", "w\u00e4r'", ",", "sie", "Jah\u00b7ren\u00b7lang", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "KOUS", "PPER", "ADV", "ADV", "ADJD", "VAFIN", "$,", "PPER", "NN", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "zu behalten;", "tokens": ["zu", "be\u00b7hal\u00b7ten", ";"], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "So w\u00fcrden wir, und ich zumahl, in nicht gar langer", "tokens": ["So", "w\u00fcr\u00b7den", "wir", ",", "und", "ich", "zu\u00b7mahl", ",", "in", "nicht", "gar", "lan\u00b7ger"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "$,", "KON", "PPER", "ADV", "$,", "APPR", "PTKNEG", "ADV", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Zeit veralten,", "tokens": ["Zeit", "ver\u00b7al\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Und, wo sie mich nicht, ich doch sie, verlieren m\u00fcssen.", "tokens": ["Und", ",", "wo", "sie", "mich", "nicht", ",", "ich", "doch", "sie", ",", "ver\u00b7lie\u00b7ren", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PPER", "PRF", "PTKNEG", "$,", "PPER", "ADV", "PPER", "$,", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Dieses Denken.", "tokens": ["Die\u00b7ses", "Den\u00b7ken", "."], "token_info": ["word", "word", "punct"], "pos": ["PDAT", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.13": {"text": "Fing an, recht mitten in der Lust, in Traurigkeit mich zu", "tokens": ["Fing", "an", ",", "recht", "mit\u00b7ten", "in", "der", "Lust", ",", "in", "Trau\u00b7rig\u00b7keit", "mich", "zu"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$,", "ADV", "ADV", "APPR", "ART", "NN", "$,", "APPR", "NN", "PPER", "PTKZU"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.14": {"text": "versenken,", "tokens": ["ver\u00b7sen\u00b7ken", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "So, da\u00df sogar die helle Stimme, das angenehmste Sai-", "tokens": ["So", ",", "da\u00df", "so\u00b7gar", "die", "hel\u00b7le", "Stim\u00b7me", ",", "das", "an\u00b7ge\u00b7nehms\u00b7te", "Sai"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "ADV", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.16": {"text": "ten-Spiel,", "tokens": ["ten\u00b7Spiel", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.17": {"text": "Vom ersten Reiz sehr viel verlohr, und mir viel weniger", "tokens": ["Vom", "ers\u00b7ten", "Reiz", "sehr", "viel", "ver\u00b7lohr", ",", "und", "mir", "viel", "we\u00b7ni\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ADV", "ADV", "VVFIN", "$,", "KON", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.18": {"text": "gefiel.", "tokens": ["ge\u00b7fiel", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.4": {"line.1": {"text": "Allein der schwarze Gram verschwand, so bald ich mich", "tokens": ["Al\u00b7lein", "der", "schwar\u00b7ze", "Gram", "ver\u00b7schwand", ",", "so", "bald", "ich", "mich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$,", "ADV", "ADV", "PPER", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "nur recht besann;", "tokens": ["nur", "recht", "be\u00b7sann", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Es stimmten meine Kinder eben von ungefehr die\u00df Lied-", "tokens": ["Es", "stimm\u00b7ten", "mei\u00b7ne", "Kin\u00b7der", "e\u00b7ben", "von", "un\u00b7ge\u00b7fehr", "die\u00df", "Lie\u00b7d"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "APPR", "ADJD", "PDS", "TRUNC"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "chen an:", "tokens": ["chen", "an", ":"], "token_info": ["word", "word", "punct"], "pos": ["NE", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.5": {"line.1": {"text": "Es ist nur die\u00df ein wahres Leben,", "tokens": ["Es", "ist", "nur", "die\u00df", "ein", "wah\u00b7res", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PDS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf alle Sch\u00f6nheit Achtung geben", "tokens": ["Auf", "al\u00b7le", "Sch\u00f6n\u00b7heit", "Ach\u00b7tung", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In der durch GOtt geschm\u00fcckten Welt.", "tokens": ["In", "der", "durch", "Gott", "ge\u00b7schm\u00fcck\u00b7ten", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was Seine Lieb\u2019 uns wollen zeigen,", "tokens": ["Was", "Sei\u00b7ne", "Lieb'", "uns", "wol\u00b7len", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wird uns, nur blo\u00df im Denken, eigen,", "tokens": ["Wird", "uns", ",", "nur", "blo\u00df", "im", "Den\u00b7ken", ",", "ei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "ADV", "ADV", "APPRART", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und Jhm gef\u00e4llt, wenns uns gef\u00e4llt.", "tokens": ["Und", "Jhm", "ge\u00b7f\u00e4llt", ",", "wenns", "uns", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVPP", "$,", "KOUS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "La\u00dft uns, dem Sch\u00f6pfer denn zu Ehren,", "tokens": ["La\u00dft", "uns", ",", "dem", "Sch\u00f6p\u00b7fer", "denn", "zu", "Eh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "ART", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Anmuht h\u00f6ren, was wir h\u00f6ren,", "tokens": ["Mit", "An\u00b7muht", "h\u00f6\u00b7ren", ",", "was", "wir", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Freuden sehen, was wir sehn!", "tokens": ["Mit", "Freu\u00b7den", "se\u00b7hen", ",", "was", "wir", "sehn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir wollen Seiner Liebe Willen", "tokens": ["Wir", "wol\u00b7len", "Sei\u00b7ner", "Lie\u00b7be", "Wil\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "In unsrer Einsamkeit erf\u00fcllen,", "tokens": ["In", "uns\u00b7rer", "Ein\u00b7sam\u00b7keit", "er\u00b7f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und Jhn, in unsrer Lust, erh\u00f6hn.", "tokens": ["Und", "Jhn", ",", "in", "uns\u00b7rer", "Lust", ",", "er\u00b7h\u00f6hn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Das Denken: Da\u00df wir alle Gaben", "tokens": ["Das", "Den\u00b7ken", ":", "Da\u00df", "wir", "al\u00b7le", "Ga\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "KOUS", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von einer nie ver\u00e4nderten, unwandelbaren Gottheit", "tokens": ["Von", "ei\u00b7ner", "nie", "ver\u00b7\u00e4n\u00b7der\u00b7ten", ",", "un\u00b7wan\u00b7del\u00b7ba\u00b7ren", "Got\u00b7theit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADV", "VVFIN", "$,", "ADJA", "NN"], "meter": "-+-+-+---+-+--+", "measure": "iambic.hexa.chol"}, "line.3": {"text": "haben,", "tokens": ["ha\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["VAFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Vereinet gleichsam meine Seele mit Seiner Unverg\u00e4ng-", "tokens": ["Ver\u00b7ei\u00b7net", "gleich\u00b7sam", "mei\u00b7ne", "See\u00b7le", "mit", "Sei\u00b7ner", "Un\u00b7ver\u00b7g\u00e4ng"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.5": {"text": "lichkeit", "tokens": ["lich\u00b7keit"], "token_info": ["word"], "pos": ["NN"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Und f\u00fcgt sie in gewisser Maasse, trotz der Ver\u00e4ndrung", "tokens": ["Und", "f\u00fcgt", "sie", "in", "ge\u00b7wis\u00b7ser", "Maas\u00b7se", ",", "trotz", "der", "Ver\u00b7\u00e4n\u00b7drung"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.7": {"text": "dieser Zeit,", "tokens": ["die\u00b7ser", "Zeit", ","], "token_info": ["word", "word", "punct"], "pos": ["PDAT", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "An Seine Dau\u2019r, als ihren Ursprung. Die nicht v