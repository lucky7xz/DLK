{"textgrid.poem.35366": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "21.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Meer, du bist das ewig zaubervolle,", "tokens": ["O", "Meer", ",", "du", "bist", "das", "e\u00b7wig", "zau\u00b7ber\u00b7vol\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VAFIN", "ART", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das ewig sch\u00f6ne und das ewig wahre,", "tokens": ["Das", "e\u00b7wig", "sch\u00f6\u00b7ne", "und", "das", "e\u00b7wig", "wah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ADJA", "KON", "ART", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die gro\u00dfe Wiege und die Totenbahre.", "tokens": ["Die", "gro\u00b7\u00dfe", "Wie\u00b7ge", "und", "die", "To\u00b7ten\u00b7bah\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vor deiner Milde wie vor deinem Grolle,", "tokens": ["Vor", "dei\u00b7ner", "Mil\u00b7de", "wie", "vor", "dei\u00b7nem", "Grol\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KOKOM", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Vor deinem Hauch verstummt des S\u00e4ngers Leier.", "tokens": ["Vor", "dei\u00b7nem", "Hauch", "ver\u00b7stummt", "des", "S\u00e4n\u00b7gers", "Lei\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Du bist der Anfang und das letzte Wort,", "tokens": ["Du", "bist", "der", "An\u00b7fang", "und", "das", "letz\u00b7te", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der Menschheit Schrecken und ihr bester Hort,", "tokens": ["Der", "Menschheit", "Schre\u00b7cken", "und", "ihr", "bes\u00b7ter", "Hort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Ihr Tr\u00f6ster, ihr Ern\u00e4hrer, ihr Befreier.", "tokens": ["Ihr", "Tr\u00f6s\u00b7ter", ",", "ihr", "Er\u00b7n\u00e4h\u00b7rer", ",", "ihr", "Be\u00b7frei\u00b7er", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Entz\u00fcckend ist dein L\u00e4cheln und gewaltig", "tokens": ["Ent\u00b7z\u00fc\u00b7ckend", "ist", "dein", "L\u00e4\u00b7cheln", "und", "ge\u00b7wal\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "KON", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Dein tiefer Atemzug. Mit Salzkrystallen", "tokens": ["Dein", "tie\u00b7fer", "A\u00b7tem\u00b7zug", ".", "Mit", "Salz\u00b7krys\u00b7tal\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Hinsch\u00e4umend \u00fcber zackige Korallen", "tokens": ["Hin\u00b7sch\u00e4u\u00b7mend", "\u00fc\u00b7ber", "za\u00b7cki\u00b7ge", "Ko\u00b7ral\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und immer Leben spr\u00fchend, tausendfaltig;", "tokens": ["Und", "im\u00b7mer", "Le\u00b7ben", "spr\u00fc\u00b7hend", ",", "tau\u00b7send\u00b7fal\u00b7tig", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "NN", "VVPP", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Eisberge rollend, Lotusinseln pflegend,", "tokens": ["Eis\u00b7ber\u00b7ge", "rol\u00b7lend", ",", "Lo\u00b7tus\u00b7in\u00b7seln", "pfle\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "NN", "VVPP", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.14": {"text": "Stolze Fregatten, Handelsflotten schaukelnd,", "tokens": ["Stol\u00b7ze", "Fre\u00b7gat\u00b7ten", ",", "Han\u00b7dels\u00b7flot\u00b7ten", "schau\u00b7kelnd", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "VVPP", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.15": {"text": "Bald Falten werfend, bald im Lichte gaukelnd", "tokens": ["Bald", "Fal\u00b7ten", "wer\u00b7fend", ",", "bald", "im", "Lich\u00b7te", "gau\u00b7kelnd"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VVPP", "$,", "ADV", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Und eine Welt von Kreaturen hegend", "tokens": ["Und", "ei\u00b7ne", "Welt", "von", "Kre\u00b7a\u00b7tu\u00b7ren", "he\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "In deinem Scho\u00dfe; Nordlands kahle D\u00fcnen", "tokens": ["In", "dei\u00b7nem", "Scho\u00b7\u00dfe", ";", "Nord\u00b7lands", "kah\u00b7le", "D\u00fc\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$.", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Besp\u00fclend, Fichtenw\u00e4lder, schneebekr\u00e4nzt,", "tokens": ["Be\u00b7sp\u00fc\u00b7lend", ",", "Fich\u00b7ten\u00b7w\u00e4l\u00b7der", ",", "schnee\u00b7be\u00b7kr\u00e4nzt", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Und dr\u00fcben, wo die Tropensonne gl\u00e4nzt,", "tokens": ["Und", "dr\u00fc\u00b7ben", ",", "wo", "die", "Tro\u00b7pen\u00b7son\u00b7ne", "gl\u00e4nzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Die Palmen, die geliebten, immergr\u00fcnen,", "tokens": ["Die", "Pal\u00b7men", ",", "die", "ge\u00b7lieb\u00b7ten", ",", "im\u00b7mer\u00b7gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Die schlanken Palmen k\u00fcssend, ihre Kronen", "tokens": ["Die", "schlan\u00b7ken", "Pal\u00b7men", "k\u00fcs\u00b7send", ",", "ih\u00b7re", "Kro\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Ber\u00fchrend und ihr Fl\u00fcstern weitertragend \u2013", "tokens": ["Be\u00b7r\u00fch\u00b7rend", "und", "ihr", "Fl\u00fcs\u00b7tern", "wei\u00b7ter\u00b7tra\u00b7gend", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Glorreiches Meer! befruchtend, jauchzend, klagend,", "tokens": ["Glor\u00b7rei\u00b7ches", "Meer", "!", "be\u00b7fruch\u00b7tend", ",", "jauch\u00b7zend", ",", "kla\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$.", "VVPP", "$,", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "So flutest du dahin durch alle Zonen,", "tokens": ["So", "flu\u00b7test", "du", "da\u00b7hin", "durch", "al\u00b7le", "Zo\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PAV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Unendlich, unersch\u00f6pflich, unbezwungen,", "tokens": ["Un\u00b7end\u00b7lich", ",", "un\u00b7er\u00b7sch\u00f6pf\u00b7lich", ",", "un\u00b7be\u00b7zwun\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Entfesselt, ohne Ruhe, ewig dr\u00e4ngend,", "tokens": ["Ent\u00b7fes\u00b7selt", ",", "oh\u00b7ne", "Ru\u00b7he", ",", "e\u00b7wig", "dr\u00e4n\u00b7gend", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUI", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Und doch, wie eine Thr\u00e4ne, lichtdurchdrungen", "tokens": ["Und", "doch", ",", "wie", "ei\u00b7ne", "Thr\u00e4\u00b7ne", ",", "licht\u00b7durch\u00b7drun\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADV", "$,", "PWAV", "ART", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Dich an den dunkeln Saum der Wolken h\u00e4ngend,", "tokens": ["Dich", "an", "den", "dun\u00b7keln", "Saum", "der", "Wol\u00b7ken", "h\u00e4n\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Oft freudestrahlend, oft in stiller Trauer \u2013", "tokens": ["Oft", "freu\u00b7des\u00b7trah\u00b7lend", ",", "oft", "in", "stil\u00b7ler", "Trau\u00b7er", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Du hast der Menschen Heimat eng umschlossen.", "tokens": ["Du", "hast", "der", "Men\u00b7schen", "Hei\u00b7mat", "eng", "um\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Du hast in unsrer Mutter Brust gegossen", "tokens": ["Du", "hast", "in", "uns\u00b7rer", "Mut\u00b7ter", "Brust", "ge\u00b7gos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Des Lebens Odem, der Vernichtung Schauer;", "tokens": ["Des", "Le\u00b7bens", "O\u00b7dem", ",", "der", "Ver\u00b7nich\u00b7tung", "Schau\u00b7er", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Mich aber hast du \u00fcber Raum und Zeit", "tokens": ["Mich", "a\u00b7ber", "hast", "du", "\u00fc\u00b7ber", "Raum", "und", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Erhoben und mein Herz zu tausendmalen", "tokens": ["Er\u00b7ho\u00b7ben", "und", "mein", "Herz", "zu", "tau\u00b7send\u00b7ma\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "KON", "PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Befreit von Zweifeln und von Todesqualen,", "tokens": ["Be\u00b7freit", "von", "Zwei\u00b7feln", "und", "von", "To\u00b7des\u00b7qua\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Befreit von Tr\u00fcbsal und Zerrissenheit.", "tokens": ["Be\u00b7freit", "von", "Tr\u00fcb\u00b7sal", "und", "Zer\u00b7ris\u00b7sen\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Ich danke dir \u2013 dir, dem ich lebenskrank", "tokens": ["Ich", "dan\u00b7ke", "dir", "\u2013", "dir", ",", "dem", "ich", "le\u00b7bens\u00b7krank"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$(", "PPER", "$,", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Von meinem Leid erz\u00e4hlt, dem winzig kleinen.", "tokens": ["Von", "mei\u00b7n