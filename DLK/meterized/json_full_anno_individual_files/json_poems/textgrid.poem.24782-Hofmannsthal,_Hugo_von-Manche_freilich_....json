{"textgrid.poem.24782": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "Manche freilich ...", "genre": "verse", "period": "N.A.", "pub_year": 1901, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Manche freilich m\u00fcssen drunten sterben,", "tokens": ["Man\u00b7che", "frei\u00b7lich", "m\u00fcs\u00b7sen", "drun\u00b7ten", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VMFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo die schweren Ruder der Schiffe streifen,", "tokens": ["Wo", "die", "schwe\u00b7ren", "Ru\u00b7der", "der", "Schif\u00b7fe", "strei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Andre wohnen bei dem Steuer droben,", "tokens": ["And\u00b7re", "woh\u00b7nen", "bei", "dem", "Steu\u00b7er", "dro\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Kennen Vogelflug und die L\u00e4nder der Sterne.", "tokens": ["Ken\u00b7nen", "Vo\u00b7gel\u00b7flug", "und", "die", "L\u00e4n\u00b7der", "der", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Manche liegen immer mit schweren Gliedern", "tokens": ["Man\u00b7che", "lie\u00b7gen", "im\u00b7mer", "mit", "schwe\u00b7ren", "Glie\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "