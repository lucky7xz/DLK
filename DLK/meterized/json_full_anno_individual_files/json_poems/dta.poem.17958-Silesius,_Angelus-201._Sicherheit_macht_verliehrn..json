{"dta.poem.17958": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "201.  Sicherheit macht verliehrn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Steh wache fast\u2019 und beth; in einer Sicherheit/", "tokens": ["Steh", "wa\u00b7che", "fast'", "und", "be\u00b7th", ";", "in", "ei\u00b7ner", "Si\u00b7cher\u00b7heit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "VVFIN", "KON", "VVFIN", "$.", "APPR", "ART", "NN", "$("], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Hat mancher gahr verlohrn das Schlo\u00df der Ewigkeit.", "tokens": ["Hat", "man\u00b7cher", "gahr", "ver\u00b7lohrn", "das", "Schlo\u00df", "der", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}