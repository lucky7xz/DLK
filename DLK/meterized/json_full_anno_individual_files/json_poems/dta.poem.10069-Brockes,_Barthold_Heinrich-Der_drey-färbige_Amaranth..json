{"dta.poem.10069": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der drey-f\u00e4rbige Amaranth.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was l\u00e4sset sich doch meinen Blicken", "tokens": ["Was", "l\u00e4s\u00b7set", "sich", "doch", "mei\u00b7nen", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PRF", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hier f\u00fcr ein neues Wunder sehn!", "tokens": ["Hier", "f\u00fcr", "ein", "neu\u00b7es", "Wun\u00b7der", "sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hier will ein Kraut sich ja so sch\u00f6n,", "tokens": ["Hier", "will", "ein", "Kraut", "sich", "ja", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PRF", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als wie die sch\u00f6nsten Bluhmen, schm\u00fccken.", "tokens": ["Als", "wie", "die", "sch\u00f6ns\u00b7ten", "Bluh\u00b7men", ",", "schm\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nicht nur ein fast Smaragdnes gr\u00fcn,", "tokens": ["Nicht", "nur", "ein", "fast", "Sma\u00b7ragd\u00b7nes", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "ADV", "NN", "ADJD", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Nicht nur ein gleichsam g\u00fcldner Schein,", "tokens": ["Nicht", "nur", "ein", "gleich\u00b7sam", "g\u00fcld\u00b7ner", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Glantz vom flammenden Rubin,", "tokens": ["Der", "Glantz", "vom", "flam\u00b7men\u00b7den", "Ru\u00b7bin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Nimmt einen Platz auf seinen Bl\u00e4ttern ein.", "tokens": ["Nimmt", "ei\u00b7nen", "Platz", "auf", "sei\u00b7nen", "Bl\u00e4t\u00b7tern", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es ziert ein gantzes Bluhmen-Bette,", "tokens": ["Es", "ziert", "ein", "gant\u00b7zes", "Bluh\u00b7men\u00b7Bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und pranget in dem Bluhmen-Reich", "tokens": ["Und", "pran\u00b7get", "in", "dem", "Bluh\u00b7men\u00b7Reich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit allen Bluhmen um die Wette,", "tokens": ["Mit", "al\u00b7len", "Bluh\u00b7men", "um", "die", "Wet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von tausend ist ihm keine gleich.", "tokens": ["Von", "tau\u00b7send", "ist", "ihm", "kei\u00b7ne", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "VAFIN", "PPER", "PIAT", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es brauchet nichts von fremdem Ruhme,", "tokens": ["Es", "brau\u00b7chet", "nichts", "von", "frem\u00b7dem", "Ruh\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Indem es keine Bluhme tr\u00e4gt.", "tokens": ["In\u00b7dem", "es", "kei\u00b7ne", "Bluh\u00b7me", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die gantze Staud\u2019 ist eine Bluhme", "tokens": ["Die", "gant\u00b7ze", "Staud'", "ist", "ei\u00b7ne", "Bluh\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mit buntem Schmeltz-Werck eingelegt.", "tokens": ["Mit", "bun\u00b7tem", "Schmeltz\u00b7\u00b7Werck", "ein\u00b7ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ist die\u00df nicht eine neue Probe", "tokens": ["Ist", "die\u00df", "nicht", "ei\u00b7ne", "neu\u00b7e", "Pro\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PDS", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von einer Unersch\u00f6pflichkeit?", "tokens": ["Von", "ei\u00b7ner", "Un\u00b7er\u00b7sch\u00f6pf\u00b7lich\u00b7keit", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auch dieses Krauts Vollkommenheit", "tokens": ["Auch", "die\u00b7ses", "Krauts", "Voll\u00b7kom\u00b7men\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PDAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gereicht nur Dir, o HERR, zum Lobe.", "tokens": ["Ge\u00b7reicht", "nur", "Dir", ",", "o", "HeRR", ",", "zum", "Lo\u00b7be", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "$,", "FM", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "O Sch\u00f6pfer, Dessen ich mich freue,", "tokens": ["O", "Sch\u00f6p\u00b7fer", ",", "Des\u00b7sen", "ich", "mich", "freu\u00b7e", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mein GOTT, da nichts auf dieser Erden,", "tokens": ["Mein", "GoTT", ",", "da", "nichts", "auf", "die\u00b7ser", "Er\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "PIS", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Das von sich selbst sich bilden kann;", "tokens": ["Das", "von", "sich", "selbst", "sich", "bil\u00b7den", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PRF", "ADV", "PRF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So zeigt uns dieses Kraut aufs neue", "tokens": ["So", "zeigt", "uns", "die\u00b7ses", "Kraut", "aufs", "neu\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PDAT", "NN", "APPRART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Dein\u2019 Allmacht, Lieb\u2019 und Weisheit an.", "tokens": ["Dein'", "All\u00b7macht", ",", "Lieb'", "und", "Weis\u00b7heit", "an", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Es zeiget mir, zu Deiner Ehr,", "tokens": ["Es", "zei\u00b7get", "mir", ",", "zu", "Dei\u00b7ner", "Ehr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Indem ich es genau betrachte,", "tokens": ["In\u00b7dem", "ich", "es", "ge\u00b7nau", "be\u00b7trach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und auf die Farb und Bildung achte,", "tokens": ["Und", "auf", "die", "Farb", "und", "Bil\u00b7dung", "ach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "KON", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die\u00df bunte Bluhmen-Kraut noch mehr.", "tokens": ["Die\u00df", "bun\u00b7te", "Bluh\u00b7men\u00b7Kraut", "noch", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wie ich ein Blat davon gepfl\u00fccket,", "tokens": ["Wie", "ich", "ein", "Blat", "da\u00b7von", "ge\u00b7pfl\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Um in der N\u00e4h\u2019 es anzusehn;", "tokens": ["Um", "in", "der", "N\u00e4h'", "es", "an\u00b7zu\u00b7sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "ART", "NN", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Fand ich in selben Wunder-sch\u00f6n", "tokens": ["Fand", "ich", "in", "sel\u00b7ben", "Wun\u00b7der\u00b7sch\u00f6n"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch Form und Farben ausgedr\u00fccket", "tokens": ["Durch", "Form", "und", "Far\u00b7ben", "aus\u00b7ge\u00b7dr\u00fc\u00b7cket"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ein rothes Hertz, draus gelbe Flammen,", "tokens": ["Ein", "ro\u00b7thes", "Hertz", ",", "draus", "gel\u00b7be", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PAV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit einem gr\u00fcnen Rauch \u00fcmringt,", "tokens": ["Mit", "ei\u00b7nem", "gr\u00fc\u00b7nen", "Rauch", "\u00fcm\u00b7ringt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Als wie aus einer Quelle, stammen.", "tokens": ["Als", "wie", "aus", "ei\u00b7ner", "Quel\u00b7le", ",", "stam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "KOKOM", "APPR", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Es scheint, da\u00df beides aufw\u00e4rts dringt.", "tokens": ["Es", "scheint", ",", "da\u00df", "bei\u00b7des", "auf\u00b7w\u00e4rts", "dringt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Bey dieser sch\u00f6nen Farben Schein", "tokens": ["Bey", "die\u00b7ser", "sch\u00f6\u00b7nen", "Far\u00b7ben", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Bildung, fiel mir dieses ein:", "tokens": ["Und", "Bil\u00b7dung", ",", "fiel", "mir", "die\u00b7ses", "ein", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "VVFIN", "PPER", "PDS", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201each w\u00fcrde dieses sch\u00f6ne Kraut", "tokens": ["\u201e", "ach", "w\u00fcr\u00b7de", "die\u00b7ses", "sch\u00f6\u00b7ne", "Kraut"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VAFIN", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201evon uns, zum Unterricht, doch \u00f6ffters angeschaut!", "tokens": ["\u201e", "von", "uns", ",", "zum", "Un\u00b7ter\u00b7richt", ",", "doch", "\u00f6ff\u00b7ters", "an\u00b7ge\u00b7schaut", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPER", "$,", "APPRART", "NN", "$,", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201each m\u00f6gt\u2019 es uns zum Lehr-Bild dienen!", "tokens": ["\u201e", "ach", "m\u00f6gt'", "es", "uns", "zum", "Lehr\u00b7Bild", "die\u00b7nen", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VMFIN", "PPER", "PRF", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "\u201each triebe doch auch unser Hertz,", "tokens": ["\u201e", "ach", "trie\u00b7be", "doch", "auch", "un\u00b7ser", "Hertz", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "VVFIN", "ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "\u201ein Andachts-Flammen, Himmel-w\u00e4rts,", "tokens": ["\u201e", "in", "An\u00b7dachts\u00b7Flam\u00b7men", ",", "Him\u00b7mel\u00b7w\u00e4rts", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "APPR", "NN", "$,", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.8": {"text": "\u201edie Lust von dem empfundnen gr\u00fcnen!", "tokens": ["\u201e", "die", "Lust", "von", "dem", "emp\u00b7fund\u00b7nen", "gr\u00fc\u00b7nen", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "APPR", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "\u201each m\u00f6gte doch die Pracht der sch\u00f6nen Erden,", "tokens": ["\u201e", "ach", "m\u00f6g\u00b7te", "doch", "die", "Pracht", "der", "sch\u00f6\u00b7nen", "Er\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VMFIN", "ADV", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "\u201eso bald ihr gr\u00fcner Schmuck mein Hertz entz\u00fcndet,", "tokens": ["\u201e", "so", "bald", "ihr", "gr\u00fc\u00b7ner", "Schmuck", "mein", "Hertz", "ent\u00b7z\u00fcn\u00b7det", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "\u201ein Andachts-Flammen aufw\u00e4rts steigen,", "tokens": ["\u201e", "in", "An\u00b7dachts\u00b7Flam\u00b7men", "auf\u00b7w\u00e4rts", "stei\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "\u201edem N\u00e4chsten mein Vergn\u00fcgen zeigen,", "tokens": ["\u201e", "dem", "N\u00e4chs\u00b7ten", "mein", "Ver\u00b7gn\u00fc\u00b7gen", "zei\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "\u201eund GOTT, der Lust am Menschen findet,", "tokens": ["\u201e", "und", "GoTT", ",", "der", "Lust", "am", "Men\u00b7schen", "fin\u00b7det", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "NE", "$,", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "\u201eein wolgef\u00e4llig Opfer werden!", "tokens": ["\u201e", "ein", "wol\u00b7ge\u00b7f\u00e4l\u00b7lig", "Op\u00b7fer", "wer\u00b7den", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJD", "NN", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}