{"dta.poem.6594": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den durchsichtigen und nicht durch-  \n sichtigen Dingen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So kan auch nicht durch dichte Bogen G\u00e4nge", "tokens": ["So", "kan", "auch", "nicht", "durch", "dich\u00b7te", "Bo\u00b7gen", "G\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "PTKNEG", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des schw\u00fclen Sommers Hitze gehn.", "tokens": ["Des", "schw\u00fc\u00b7len", "Som\u00b7mers", "Hit\u00b7ze", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn gleich der Sonnen Licht", "tokens": ["Wenn", "gleich", "der", "Son\u00b7nen", "Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Durch einige von ihren Bl\u00e4ttern bricht;", "tokens": ["Durch", "ei\u00b7ni\u00b7ge", "von", "ih\u00b7ren", "Bl\u00e4t\u00b7tern", "bricht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So hemmet sie jedoch der Zweig\u2019 und Bl\u00e4tter Menge.", "tokens": ["So", "hem\u00b7met", "sie", "je\u00b7doch", "der", "Zweig'", "und", "Bl\u00e4t\u00b7ter", "Men\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "KON", "NN", "NN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Es kan sein scharffer Pfeil sie alle nicht durchstechen,", "tokens": ["Es", "kan", "sein", "scharf\u00b7fer", "Pfeil", "sie", "al\u00b7le", "nicht", "durch\u00b7ste\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "ADJA", "NN", "PPER", "PIS", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Noch durch die Dunckelheit der Schatten brechen.", "tokens": ["Noch", "durch", "die", "Dun\u00b7ckel\u00b7heit", "der", "Schat\u00b7ten", "bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}