{"dta.poem.13889": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "IiI .  \n  Zum Abschied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Sturm w\u00fchlt, und die Zeiten b\u00e4umen", "tokens": ["Sturm", "w\u00fchlt", ",", "und", "die", "Zei\u00b7ten", "b\u00e4u\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "KON", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sehns\u00fcchtig sich himmelan,", "tokens": ["Sehn\u00b7s\u00fcch\u00b7tig", "sich", "him\u00b7me\u00b7lan", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "ADV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Hoch in solcher Wellen Sch\u00e4umen", "tokens": ["Hoch", "in", "sol\u00b7cher", "Wel\u00b7len", "Sch\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PIAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Segle, k\u00fchner Steuermann!", "tokens": ["Seg\u00b7le", ",", "k\u00fch\u00b7ner", "Steu\u00b7er\u00b7mann", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}