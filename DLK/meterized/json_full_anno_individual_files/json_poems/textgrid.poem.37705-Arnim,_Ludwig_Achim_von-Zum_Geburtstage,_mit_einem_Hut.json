{"textgrid.poem.37705": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Zum Geburtstage, mit einem Hut", "genre": "verse", "period": "N.A.", "pub_year": 1807, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Antoinette! Antoinette!", "tokens": ["An\u00b7to\u00b7i\u00b7net\u00b7te", "!", "An\u00b7to\u00b7i\u00b7net\u00b7te", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$.", "NE", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Aus dem Bette, aus dem Bette!", "tokens": ["Aus", "dem", "Bet\u00b7te", ",", "aus", "dem", "Bet\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "H\u00f6r' mein Singen, h\u00f6r' mein Pfeifen,", "tokens": ["H\u00f6r'", "mein", "Sin\u00b7gen", ",", "h\u00f6r'", "mein", "Pfei\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sieh die hellen lichten Streifen!", "tokens": ["Sieh", "die", "hel\u00b7len", "lich\u00b7ten", "Strei\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Auf, erwache! denn im Nacken sitzet", "tokens": ["Auf", ",", "er\u00b7wa\u00b7che", "!", "denn", "im", "Na\u00b7cken", "sit\u00b7zet"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "$,", "VVFIN", "$.", "ADV", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Dir ein Schelm, der Pfeile spitzet,", "tokens": ["Dir", "ein", "Schelm", ",", "der", "Pfei\u00b7le", "spit\u00b7zet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und es ruft zu aller Ohren,", "tokens": ["Und", "es", "ruft", "zu", "al\u00b7ler", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df du heute wardst geboren.", "tokens": ["Da\u00df", "du", "heu\u00b7te", "wardst", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Antoinette! Antoinette!", "tokens": ["An\u00b7to\u00b7i\u00b7net\u00b7te", "!", "An\u00b7to\u00b7i\u00b7net\u00b7te", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$.", "NE", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wenn ich h\u00e4tte eine Kette,", "tokens": ["Wenn", "ich", "h\u00e4t\u00b7te", "ei\u00b7ne", "Ket\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00fcrde ich dein Z\u00fcnglein legen", "tokens": ["W\u00fcr\u00b7de", "ich", "dein", "Z\u00fcn\u00b7glein", "le\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In die Kette, denn verwegen", "tokens": ["In", "die", "Ket\u00b7te", ",", "denn", "ver\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bist du in dem Wahrheitsagen!", "tokens": ["Bist", "du", "in", "dem", "Wahr\u00b7heit\u00b7sa\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Heut will ich dir Wahrheit sagen!", "tokens": ["Heut", "will", "ich", "dir", "Wahr\u00b7heit", "sa\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Klingen dir nicht schon die Ohren", "tokens": ["Klin\u00b7gen", "dir", "nicht", "schon", "die", "Oh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PTKNEG", "ADV", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "Links und rechts? Es geh' dir nichts verloren!", "tokens": ["Links", "und", "rechts", "?", "Es", "geh'", "dir", "nichts", "ver\u00b7lo\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADV", "$.", "PPER", "VVFIN", "PPER", "PIS", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Antoinette! Antoinette!", "tokens": ["An\u00b7to\u00b7i\u00b7net\u00b7te", "!", "An\u00b7to\u00b7i\u00b7net\u00b7te", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$.", "NE", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Ach ich wette, ach ich wette,", "tokens": ["Ach", "ich", "wet\u00b7te", ",", "ach", "ich", "wet\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "VVFIN", "$,", "XY", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df du wirst den Trotz bereuen!", "tokens": ["Da\u00df", "du", "wirst", "den", "Trotz", "be\u00b7reu\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn der Schelm in deinem Nacken", "tokens": ["Denn", "der", "Schelm", "in", "dei\u00b7nem", "Na\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wird dich selber endlich zwacken,", "tokens": ["Wird", "dich", "sel\u00b7ber", "end\u00b7lich", "zwa\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und du wirst vergebens dr\u00e4uen,", "tokens": ["Und", "du", "wirst", "ver\u00b7ge\u00b7bens", "dr\u00e4u\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Diesen Tag hat er sich recht erkoren,", "tokens": ["Die\u00b7sen", "Tag", "hat", "er", "sich", "recht", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "PPER", "PRF", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Weil du heute wardst geboren.", "tokens": ["Weil", "du", "heu\u00b7te", "wardst", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Antoinette! Antoinette!", "tokens": ["An\u00b7to\u00b7i\u00b7net\u00b7te", "!", "An\u00b7to\u00b7i\u00b7net\u00b7te", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$.", "NE", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Sieh, ich rette, sieh, ich rette", "tokens": ["Sieh", ",", "ich", "ret\u00b7te", ",", "sieh", ",", "ich", "ret\u00b7te"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "$,", "PPER", "VVFIN", "$,", "VVFIN", "$,", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dich durch diesen Hut von Fortunaten,", "tokens": ["Dich", "durch", "die\u00b7sen", "Hut", "von", "For\u00b7tu\u00b7na\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PDAT", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Der dich unsichtbar kann machen.", "tokens": ["Der", "dich", "un\u00b7sicht\u00b7bar", "kann", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sei auf deiner Hut nicht stets zu lachen,", "tokens": ["Sei", "auf", "dei\u00b7ner", "Hut", "nicht", "stets", "zu", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "PTKNEG", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "H\u00f6r' was Andre sagen, mu\u00df ich rathen!", "tokens": ["H\u00f6r'", "was", "And\u00b7re", "sa\u00b7gen", ",", "mu\u00df", "ich", "ra\u00b7then", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PWS", "PIS", "VVINF", "$,", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Manches kleidet dich vor Thoren,", "tokens": ["Man\u00b7ches", "klei\u00b7det", "dich", "vor", "Tho\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Denke, da\u00df kein Jahr f\u00fcr dich verloren.", "tokens": ["Den\u00b7ke", ",", "da\u00df", "kein", "Jahr", "f\u00fcr", "dich", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "PIAT", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Antoinette! Antoinette!", "tokens": ["An\u00b7to\u00b7i\u00b7net\u00b7te", "!", "An\u00b7to\u00b7i\u00b7net\u00b7te", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$.", "NE", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wie so nette, wie so nette", "tokens": ["Wie", "so", "net\u00b7te", ",", "wie", "so", "net\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADJA", "$,", "PWAV", "ADV", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00f6nnt' ich dir was Sch\u00f6nes sagen!", "tokens": ["K\u00f6nnt'", "ich", "dir", "was", "Sch\u00f6\u00b7nes", "sa\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PIS", "ADJA", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch ich will dich mir verstecken", "tokens": ["Doch", "ich", "will", "dich", "mir", "ver\u00b7ste\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "PPER", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In dem Hut um dich zu necken,", "tokens": ["In", "dem", "Hut", "um", "dich", "zu", "ne\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn sonst w\u00fcrde ich's nicht wagen;", "tokens": ["Denn", "sonst", "w\u00fcr\u00b7de", "ich's", "nicht", "wa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PIS", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Furcht vor Augen oder Ohren, \u2013", "tokens": ["Furcht", "vor", "Au\u00b7gen", "o\u00b7der", "Oh\u00b7ren", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Schone meiner, weil du heut geboren!", "tokens": ["Scho\u00b7ne", "mei\u00b7ner", ",", "weil", "du", "heut", "ge\u00b7bo\u00b7ren", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "$,", "KOUS", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Antoinette! Antoinette!", "tokens": ["An\u00b7to\u00b7i\u00b7net\u00b7te", "!", "An\u00b7to\u00b7i\u00b7net\u00b7te", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$.", "NE", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Aus dem Bette, aus dem Bette!", "tokens": ["Aus", "dem", "Bet\u00b7te", ",", "aus", "dem", "Bet\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "H\u00f6r' mein Singen, h\u00f6r' mein Pfeifen,", "tokens": ["H\u00f6r'", "mein", "Sin\u00b7gen", ",", "h\u00f6r'", "mein", "Pfei\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sieh die hellen lichten Streifen!", "tokens": ["Sieh", "die", "hel\u00b7len", "lich\u00b7ten", "Strei\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Auf, erwache! denn im Nacken sitzet", "tokens": ["Auf", ",", "er\u00b7wa\u00b7che", "!", "denn", "im", "Na\u00b7cken", "sit\u00b7zet"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "$,", "VVFIN", "$.", "ADV", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Dir ein Schelm, der Pfeile spitzet,", "tokens": ["Dir", "ein", "Schelm", ",", "der", "Pfei\u00b7le", "spit\u00b7zet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und es ruft zu aller Ohren,", "tokens": ["Und", "es", "ruft", "zu", "al\u00b7ler", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df du heute wardst geboren.", "tokens": ["Da\u00df", "du", "heu\u00b7te", "wardst", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochai