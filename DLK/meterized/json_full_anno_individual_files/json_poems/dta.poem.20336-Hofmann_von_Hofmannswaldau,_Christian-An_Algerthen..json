{"dta.poem.20336": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "An Algerthen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach! k\u00f6nte doch mein geist durch meine feder fliessen/", "tokens": ["Ach", "!", "k\u00f6n\u00b7te", "doch", "mein", "geist", "durch", "mei\u00b7ne", "fe\u00b7der", "flies\u00b7sen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VMFIN", "ADV", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie gerne schl\u00fc\u00df er sich in diese reimen ein/", "tokens": ["Wie", "ger\u00b7ne", "schl\u00fc\u00df", "er", "sich", "in", "die\u00b7se", "rei\u00b7men", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "PRF", "APPR", "PDS", "VVFIN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie emsig w\u00fcrd\u2019 er dir die s\u00fcssen lippen k\u00fcssen/", "tokens": ["Wie", "em\u00b7sig", "w\u00fcrd'", "er", "dir", "die", "s\u00fcs\u00b7sen", "lip\u00b7pen", "k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "PPER", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und einer biene gleich auff deinen rosen seyn.", "tokens": ["Und", "ei\u00b7ner", "bie\u00b7ne", "gleich", "auff", "dei\u00b7nen", "ro\u00b7sen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "ADV", "APPR", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er w\u00fcrde z\u00e4rtlich sich auff ihre bl\u00e4tter legen/", "tokens": ["Er", "w\u00fcr\u00b7de", "z\u00e4rt\u00b7lich", "sich", "auff", "ih\u00b7re", "bl\u00e4t\u00b7ter", "le\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und durch den honig-thau bald truncken seyn gemacht.", "tokens": ["Und", "durch", "den", "ho\u00b7nig\u00b7\u00b7thau", "bald", "trun\u00b7cken", "seyn", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "ADJD", "VAINF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dein purpur w\u00fcrd\u2019 in ihm dergleichen trieb erregen/", "tokens": ["Dein", "pur\u00b7pur", "w\u00fcrd'", "in", "ihm", "derg\u00b7lei\u00b7chen", "trieb", "er\u00b7re\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "VAFIN", "APPR", "PPER", "PIS", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So nur das paradi\u00df zu erst hat angelacht.", "tokens": ["So", "nur", "das", "pa\u00b7ra\u00b7di\u00df", "zu", "erst", "hat", "an\u00b7ge\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "APPR", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Auff deinen bergen w\u00fcrd\u2019 er rothe beeren suchen/", "tokens": ["Auff", "dei\u00b7nen", "ber\u00b7gen", "w\u00fcrd'", "er", "ro\u00b7the", "bee\u00b7ren", "su\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "VVINF", "VAFIN", "PPER", "ADJA", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wohin dringt endlich doch lieb und auch f\u00fcrwitz nicht?", "tokens": ["Wo\u00b7hin", "dringt", "end\u00b7lich", "doch", "lieb", "und", "auch", "f\u00fcr\u00b7witz", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ADV", "ADJD", "KON", "ADV", "NE", "PTKNEG", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "(ich mu\u00df aus ungedult auff das verh\u00e4ngni\u00df fluchen/", "tokens": ["(", "ich", "mu\u00df", "aus", "un\u00b7ge\u00b7dult", "auff", "das", "ver\u00b7h\u00e4ng\u00b7ni\u00df", "flu\u00b7chen", "/"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "APPR", "NN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So unlust st\u00e4hlern macht/ und lust wie gla\u00df zerbricht.)", "tokens": ["So", "un\u00b7lust", "st\u00e4h\u00b7lern", "macht", "/", "und", "lust", "wie", "gla\u00df", "zer\u00b7bricht", ".", ")"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "VVINF", "VVFIN", "$(", "KON", "VVFIN", "KOKOM", "ADJD", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So bleibt mein schwacher geist in seinen liebes-schrancken/", "tokens": ["So", "bleibt", "mein", "schwa\u00b7cher", "geist", "in", "sei\u00b7nen", "lie\u00b7bes\u00b7schran\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und kommt/ wie dieser brieff/ Algertha/ nicht zu dir/", "tokens": ["Und", "kommt", "/", "wie", "die\u00b7ser", "brieff", "/", "Al\u00b7ger\u00b7tha", "/", "nicht", "zu", "dir", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KOKOM", "PDS", "VVFIN", "$(", "NE", "$(", "PTKNEG", "APPR", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Verschluckt die hoffnungs-kost/ und tr\u00e4nckt sich in gedancken/", "tokens": ["Ver\u00b7schluckt", "die", "hoff\u00b7nungs\u00b7kost", "/", "und", "tr\u00e4nckt", "sich", "in", "ge\u00b7dan\u00b7cken", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$(", "KON", "VVFIN", "PRF", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Was ich nicht melden kan/ verdolmerscht das pappier:", "tokens": ["Was", "ich", "nicht", "mel\u00b7den", "kan", "/", "ver\u00b7dol\u00b7merscht", "das", "pap\u00b7pier", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$(", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wer offtmahls wenig sagt/ thut allzu viel zu wissen/", "tokens": ["Wer", "offt\u00b7mahls", "we\u00b7nig", "sagt", "/", "thut", "all\u00b7zu", "viel", "zu", "wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PIS", "VVFIN", "$(", "VVFIN", "PTKA", "PIS", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Nimm meiner liebe pfand/ die schlechte reimen/ an:", "tokens": ["Nimm", "mei\u00b7ner", "lie\u00b7be", "pfand", "/", "die", "schlech\u00b7te", "rei\u00b7men", "/", "an", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "$(", "ART", "ADJA", "VVINF", "$(", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Du wirst das leben mir ie mehr und mehr vers\u00fcssen/", "tokens": ["Du", "wirst", "das", "le\u00b7ben", "mir", "ie", "mehr", "und", "mehr", "ver\u00b7s\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PDS", "VVFIN", "PPER", "ADV", "ADV", "KON", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wenn ich in deiner gunst seyn und auch sterben kan.", "tokens": ["Wenn", "ich", "in", "dei\u00b7ner", "gunst", "seyn", "und", "auch", "ster\u00b7ben", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VAINF", "KON", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Du schickst mir einen brieff/ geziert mit weisser seide/", "tokens": ["Du", "schickst", "mir", "ei\u00b7nen", "brieff", "/", "ge\u00b7ziert", "mit", "weis\u00b7ser", "sei\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$(", "VVPP", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Mit gold der zierlichkeit und perlen ausgeschm\u00fcckt.", "tokens": ["Mit", "gold", "der", "zier\u00b7lich\u00b7keit", "und", "per\u00b7len", "aus\u00b7ge\u00b7schm\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "KON", "VVINF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und meiner der ist schwartz und geht wie ich im leide/", "tokens": ["Und", "mei\u00b7ner", "der", "ist", "schwartz", "und", "geht", "wie", "ich", "im", "lei\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ART", "VAFIN", "ADJD", "KON", "VVFIN", "KOKOM", "PPER", "APPRART", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Daraus die traurigkeit an allen orten blickt.", "tokens": ["Da\u00b7raus", "die", "trau\u00b7rig\u00b7keit", "an", "al\u00b7len", "or\u00b7ten", "blickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Algerthe/ wo soll ich doch endlich worte finden?", "tokens": ["Al\u00b7ger\u00b7the", "/", "wo", "soll", "ich", "doch", "end\u00b7lich", "wor\u00b7te", "fin\u00b7den", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PWAV", "VMFIN", "PPER", "ADV", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ach! was gew\u00e4hr ich dir f\u00fcr deine freundlichkeit.", "tokens": ["Ach", "!", "was", "ge\u00b7w\u00e4hr", "ich", "dir", "f\u00fcr", "dei\u00b7ne", "freund\u00b7lich\u00b7keit", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWS", "VVFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Es will mich deine faust mit solchen seilen binden/", "tokens": ["Es", "will", "mich", "dei\u00b7ne", "faust", "mit", "sol\u00b7chen", "sei\u00b7len", "bin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Die nicht zernagen kan der scharffe zahn der zeit.", "tokens": ["Die", "nicht", "zer\u00b7na\u00b7gen", "kan", "der", "scharf\u00b7fe", "zahn", "der", "zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "VVINF", "VMFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Du weist der seelen selbst die f\u00e4ssel anzulegen:", "tokens": ["Du", "weist", "der", "see\u00b7len", "selbst", "die", "f\u00e4s\u00b7sel", "an\u00b7zu\u00b7le\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "O s\u00fcsse dienstbarkeit/ so nach der freyheit schmeckt!", "tokens": ["O", "s\u00fcs\u00b7se", "dienst\u00b7bar\u00b7keit", "/", "so", "nach", "der", "frey\u00b7heit", "schmeckt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$(", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Du kanst mehr lieblichkeit durch deine hand erregen/", "tokens": ["Du", "kanst", "mehr", "lieb\u00b7lich\u00b7keit", "durch", "dei\u00b7ne", "hand", "er\u00b7re\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Als nicht in Indien das zucker-rohr verdeckt.", "tokens": ["Als", "nicht", "in", "In\u00b7di\u00b7en", "das", "zu\u00b7cke\u00b7rrohr", "ver\u00b7deckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "APPR", "NE", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Wie ist dein sch\u00f6ner brieff doch mit zibet bestrichen/", "tokens": ["Wie", "ist", "dein", "sch\u00f6\u00b7ner", "brieff", "doch", "mit", "zi\u00b7bet", "be\u00b7stri\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "ADJA", "NN", "ADV", "APPR", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und wie verschwenderisch ist deiner worte pracht:", "tokens": ["Und", "wie", "ver\u00b7schwen\u00b7de\u00b7risch", "ist", "dei\u00b7ner", "wor\u00b7te", "pracht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Ein iede sylbe will nach mo\u00dfk und ambra riechen/", "tokens": ["Ein", "ie\u00b7de", "syl\u00b7be", "will", "nach", "mo\u00dfk", "und", "am\u00b7bra", "rie\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "VMFIN", "APPR", "NE", "KON", "NE", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "So dich zur herrscherin und mich zum sclaven macht.", "tokens": ["So", "dich", "zur", "herr\u00b7sche\u00b7rin", "und", "mich", "zum", "scla\u00b7ven", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPRART", "NN", "KON", "PPER", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Doch zeucht vor andern mich dein redliches gem\u00fcthe/", "tokens": ["Doch", "zeucht", "vor", "an\u00b7dern", "mich", "dein", "red\u00b7li\u00b7ches", "ge\u00b7m\u00fc\u00b7the", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIS", "PRF", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "So wie ein heller stern aus deinem brieffe dringt.", "tokens": ["So", "wie", "ein", "hel\u00b7ler", "stern", "aus", "dei\u00b7nem", "brief\u00b7fe", "dringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Es r\u00fchret meinen geist und reget mein gebl\u00fcthe/", "tokens": ["Es", "r\u00fch\u00b7ret", "mei\u00b7nen", "geist", "und", "re\u00b7get", "mein", "ge\u00b7bl\u00fc\u00b7the", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "PPOSAT", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Ich f\u00fchle wie sein strahl die seele mir bezwingt.", "tokens": ["Ich", "f\u00fch\u00b7le", "wie", "sein", "strahl", "die", "see\u00b7le", "mir", "be\u00b7zwingt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "PPOSAT", "NN", "ART", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Du zeigst mir unverstellt die reinen liebes-flammen/", "tokens": ["Du", "zeigst", "mir", "un\u00b7ver\u00b7stellt", "die", "rei\u00b7nen", "lie\u00b7bes\u00b7flam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Das feuer/ das durch dich auch mich zugleiche brennt.", "tokens": ["Das", "feu\u00b7er", "/", "das", "durch", "dich", "auch", "mich", "zu\u00b7glei\u00b7che", "brennt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "APPR", "PPER", "ADV", "PPER", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Es reimt sich in der welt doch nichts so wohl zusammen/", "tokens": ["Es", "reimt", "sich", "in", "der", "welt", "doch", "nichts", "so", "wohl", "zu\u00b7sam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "ADV", "PIS", "ADV", "ADV", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Als wenn sich eine brunst der andern freundin nennt.", "tokens": ["Als", "wenn", "sich", "ei\u00b7ne", "brunst", "der", "an\u00b7dern", "freun\u00b7din", "nennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PRF", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "In dieser wollen wir als Salamander leben/", "tokens": ["In", "die\u00b7ser", "wol\u00b7len", "wir", "als", "Sa\u00b7la\u00b7man\u00b7der", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VMFIN", "PPER", "KOUS", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Die tugend tr\u00e4gt uns stets ihr reines \u00f6le zu:", "tokens": ["Die", "tu\u00b7gend", "tr\u00e4gt", "uns", "stets", "ihr", "rei\u00b7nes", "\u00f6\u00b7le", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Es wird uns noch die welt das gute zeugni\u00df geben/", "tokens": ["Es", "wird", "uns", "noch", "die", "welt", "das", "gu\u00b7te", "zeug\u00b7ni\u00df", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Es liebe keiner nicht so rein als ich und du.", "tokens": ["Es", "lie\u00b7be", "kei\u00b7ner", "nicht", "so", "rein", "als", "ich", "und", "du", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PTKNEG", "ADV", "ADJD", "KOKOM", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Und k\u00f6nnen wir nicht stets der s\u00fcssen frucht geniessen/", "tokens": ["Und", "k\u00f6n\u00b7nen", "wir", "nicht", "stets", "der", "s\u00fcs\u00b7sen", "frucht", "ge\u00b7nies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "ADV", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "So schmeckt doch nichts so gut/ als wann mans selten schmeckt.", "tokens": ["So", "schmeckt", "doch", "nichts", "so", "gut", "/", "als", "wann", "mans", "sel\u00b7ten", "schmeckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PIS", "ADV", "ADJD", "$(", "KON", "PWAV", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Der wein/ der m\u00e4\u00dfig mu\u00df in unsre kehle fliessen/", "tokens": ["Der", "wein", "/", "der", "m\u00e4\u00b7\u00dfig", "mu\u00df", "in", "uns\u00b7re", "keh\u00b7le", "flies\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADJD", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Hat in dem magen offt die gr\u00f6ste lust erweckt.", "tokens": ["Hat", "in", "dem", "ma\u00b7gen", "offt", "die", "gr\u00f6s\u00b7te", "lust", "er\u00b7weckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Gefahr/ verbot und zwang brennt zunder zu der liebe/", "tokens": ["Ge\u00b7fahr", "/", "ver\u00b7bot", "und", "zwang", "brennt", "zun\u00b7der", "zu", "der", "lie\u00b7be", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "VVFIN", "KON", "VVFIN", "VVFIN", "ADV", "APPR", "ART", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Verschlo\u00dfne th\u00fcrme sind die sparren unsrer lust/", "tokens": ["Ver\u00b7schlo\u00df\u00b7ne", "th\u00fcr\u00b7me", "sind", "die", "spar\u00b7ren", "uns\u00b7rer", "lust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ART", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Erz\u00fcrnte blicke seyn die sch\u00e4rffsten buhlschaffts-triebe.", "tokens": ["Er\u00b7z\u00fcrn\u00b7te", "bli\u00b7cke", "seyn", "die", "sch\u00e4rffs\u00b7ten", "buhl\u00b7schaffts\u00b7trie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Und die bestraffung selbst erhitzt uns geist und brust.", "tokens": ["Und", "die", "be\u00b7straf\u00b7fung", "selbst", "er\u00b7hitzt", "uns", "geist", "und", "brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVFIN", "PPER", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Der lange winter giebt dem lentz die beste zierde/", "tokens": ["Der", "lan\u00b7ge", "win\u00b7ter", "giebt", "dem", "lentz", "die", "bes\u00b7te", "zier\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Der sch\u00f6nste sonnenschein kommt aus der schwartzen nacht:", "tokens": ["Der", "sch\u00f6ns\u00b7te", "son\u00b7nen\u00b7schein", "kommt", "aus", "der", "schwart\u00b7zen", "nacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Verbotne frucht vermehrt dem menschen die begierde/", "tokens": ["Ver\u00b7bot\u00b7ne", "frucht", "ver\u00b7mehrt", "dem", "men\u00b7schen", "die", "be\u00b7gier\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Und folgen haben offt gla\u00df zu rubin gemacht.", "tokens": ["Und", "fol\u00b7gen", "ha\u00b7ben", "offt", "gla\u00df", "zu", "ru\u00b7bin", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VAFIN", "ADV", "VVFIN", "APPR", "NE", "VVPP", "$."], "meter": "-+-+-+--++-+", "measure": "iambic.hexa.relaxed"}, "line.61": {"text": "Wer ungest\u00f6hret liebt/ ist mehr als halb gestorben/", "tokens": ["Wer", "un\u00b7ge\u00b7st\u00f6h\u00b7ret", "liebt", "/", "ist", "mehr", "als", "halb", "ge\u00b7stor\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "$(", "VAFIN", "PIS", "KOKOM", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "Wer t\u00e4glich zucker k\u00e4ut/ sp\u00fcrt keine lieblichkeit.", "tokens": ["Wer", "t\u00e4g\u00b7lich", "zu\u00b7cker", "k\u00e4ut", "/", "sp\u00fcrt", "kei\u00b7ne", "lieb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "ADJD", "VVFIN", "$(", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Die speisen haben selbst den besten ruhm erworben/", "token