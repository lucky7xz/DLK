{"dta.poem.9747": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Jtem.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer nicht gedultig ist/ der komm zu meinem grabe/", "tokens": ["Wer", "nicht", "ge\u00b7dul\u00b7tig", "ist", "/", "der", "komm", "zu", "mei\u00b7nem", "gra\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "ADJD", "VAFIN", "$(", "ART", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und lerne: da\u00df gedult die allergr\u00f6ste gabe.", "tokens": ["Und", "ler\u00b7ne", ":", "da\u00df", "ge\u00b7dult", "die", "al\u00b7ler\u00b7gr\u00f6s\u00b7te", "ga\u00b7be", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "KOUS", "NN", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Fast alles \u00fcbel kam auf meinen kopff und mich;", "tokens": ["Fast", "al\u00b7les", "\u00fc\u00b7bel", "kam", "auf", "mei\u00b7nen", "kopff", "und", "mich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "KON", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch lidt ichs/ weil mein weib trug schwerer noch als ich.", "tokens": ["Doch", "lidt", "ichs", "/", "weil", "mein", "weib", "trug", "schwe\u00b7rer", "noch", "als", "ich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "$(", "