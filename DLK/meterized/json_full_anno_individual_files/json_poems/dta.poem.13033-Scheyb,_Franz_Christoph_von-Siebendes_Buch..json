{"dta.poem.13033": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Siebendes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich hatte nach und nach Zufriedenheit gesp\u00fchrt,", "tokens": ["Ich", "hat\u00b7te", "nach", "und", "nach", "Zu\u00b7frie\u00b7den\u00b7heit", "ge\u00b7sp\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "KON", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit welcher dieser Grei\u00df den ganzen Saal ger\u00fchrt.", "tokens": ["Mit", "wel\u00b7cher", "die\u00b7ser", "Grei\u00df", "den", "gan\u00b7zen", "Saal", "ge\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PDAT", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auf einmahl ward es still; indem er angefangen", "tokens": ["Auf", "ein\u00b7mahl", "ward", "es", "still", ";", "in\u00b7dem", "er", "an\u00b7ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADV", "VAFIN", "PPER", "PTKVZ", "$.", "KOUS", "PPER", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den Krei\u00df in Freundlichkeit und Z\u00e4rte zu belangen:", "tokens": ["Den", "Krei\u00df", "in", "Freund\u00b7lich\u00b7keit", "und", "Z\u00e4r\u00b7te", "zu", "be\u00b7lan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "225Warum er hergerufft, und hergekommen sey;", "tokens": ["er", "her\u00b7ge\u00b7rufft", ",", "und", "her\u00b7ge\u00b7kom\u00b7men", "sey", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Er habe nichts gewu\u00dft; es schein ihm alles neu:", "tokens": ["Er", "ha\u00b7be", "nichts", "ge\u00b7wu\u00dft", ";", "es", "schein", "ihm", "al\u00b7les", "neu", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "VVPP", "$.", "PPER", "VVFIN", "PPER", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Von der Versammlung aus hab man um ihn geschicket;", "tokens": ["Von", "der", "Ver\u00b7samm\u00b7lung", "aus", "hab", "man", "um", "ihn", "ge\u00b7schi\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "VAFIN", "PIS", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dahero sch\u00e4z\u2019 er sich verpflichtet und begl\u00fccket,", "tokens": ["Da\u00b7he\u00b7ro", "sch\u00e4\u00b7z'", "er", "sich", "ver\u00b7pflich\u00b7tet", "und", "be\u00b7gl\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "VVPP", "KON", "VVFIN", "$,"], "meter": "--+-+-+-+-+-+-", "measure": "anapaest.init"}, "line.9": {"text": "Und nehme den Befehl zu seines Alters Ehr,", "tokens": ["Und", "neh\u00b7me", "den", "Be\u00b7fehl", "zu", "sei\u00b7nes", "Al\u00b7ters", "Ehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "230Da\u00df man dasselbige zu diesem Rath begehr.", "tokens": ["man", "das\u00b7sel\u00b7bi\u00b7ge", "zu", "die\u00b7sem", "Rath", "be\u00b7gehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PDAT", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}