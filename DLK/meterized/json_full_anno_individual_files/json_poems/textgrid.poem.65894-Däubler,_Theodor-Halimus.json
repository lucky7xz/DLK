{"textgrid.poem.65894": {"metadata": {"author": {"name": "D\u00e4ubler, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Halimus", "genre": "verse", "period": "N.A.", "pub_year": 1905, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Heimat des Thukydides, im Grollen", "tokens": ["Die", "Hei\u00b7mat", "des", "Thu\u00b7ky\u00b7di\u00b7des", ",", "im", "Grol\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "APPRART", "NN"], "meter": "-+--+-++-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der See von Salamis, hat mich empfangen:", "tokens": ["Der", "See", "von", "Sa\u00b7la\u00b7mis", ",", "hat", "mich", "emp\u00b7fan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$,", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein k\u00fchnes St\u00fcrmen purpurt unsre Wangen,", "tokens": ["Ein", "k\u00fch\u00b7nes", "St\u00fcr\u00b7men", "pur\u00b7purt", "uns\u00b7re", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem Augenblick entschleiern sich die Schollen:", "tokens": ["Dem", "Au\u00b7gen\u00b7blick", "ent\u00b7schlei\u00b7ern", "sich", "die", "Schol\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Heroen modern in verborgnen Stollen;", "tokens": ["He\u00b7roen", "mo\u00b7dern", "in", "ver\u00b7borg\u00b7nen", "Stol\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Begnadete, die ihren Ruhm besangen,", "tokens": ["Be\u00b7gna\u00b7de\u00b7te", ",", "die", "ih\u00b7ren", "Ruhm", "be\u00b7san\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erscheinen noch im Mond, und zaudernd bangen", "tokens": ["Er\u00b7schei\u00b7nen", "noch", "im", "Mond", ",", "und", "zau\u00b7dernd", "ban\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADV", "APPRART", "NN", "$,", "KON", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kytharen dann nach Hellas' andachtsvollen", "tokens": ["Ky\u00b7tha\u00b7ren", "dann", "nach", "Hel\u00b7las'", "an\u00b7dachts\u00b7vol\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "NE", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Geschlechtern vor Poseidons Zornesworten:", "tokens": ["Ge\u00b7schlech\u00b7tern", "vor", "Po\u00b7sei\u00b7dons", "Zor\u00b7nes\u00b7wor\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wer nie auf ausgestreckten Klippen kniete,", "tokens": ["Wer", "nie", "auf", "aus\u00b7ge\u00b7streck\u00b7ten", "Klip\u00b7pen", "knie\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Blieb Fremdling dem Geschick, an Drohungsorten", "tokens": ["Blieb", "Fremd\u00b7ling", "dem", "Ge\u00b7schick", ",", "an", "Dro\u