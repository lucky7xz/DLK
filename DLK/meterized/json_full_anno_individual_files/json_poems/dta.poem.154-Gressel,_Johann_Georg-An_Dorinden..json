{"dta.poem.154": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An  Dorinden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.71", "fr:0.28"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Dorinde lebst du noch/ und denckst du noch an mich?", "tokens": ["Do\u00b7rin\u00b7de", "lebst", "du", "noch", "/", "und", "denckst", "du", "noch", "an", "mich", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "$(", "KON", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich hoffe deine Lieb und Treu wird nicht erkalten/", "tokens": ["Ich", "hof\u00b7fe", "dei\u00b7ne", "Lieb", "und", "Treu", "wird", "nicht", "er\u00b7kal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "NN", "VAFIN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ob mich das Schicksahl gleich drey Wochen wunderlich", "tokens": ["Ob", "mich", "das", "Schick\u00b7sahl", "gleich", "drey", "Wo\u00b7chen", "wun\u00b7der\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "CARD", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von deinem Umgang hat verdrie\u00dflich abgehalten.", "tokens": ["Von", "dei\u00b7nem", "Um\u00b7gang", "hat", "ver\u00b7drie\u00df\u00b7lich", "ab\u00b7ge\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Sonne z\u00fcndet auch von fernen H\u00e4user an:", "tokens": ["Die", "Son\u00b7ne", "z\u00fcn\u00b7det", "auch", "von", "fer\u00b7nen", "H\u00e4u\u00b7ser", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein Basiliske kan durch blosses Sehn vergifften/", "tokens": ["Ein", "Ba\u00b7si\u00b7lis\u00b7ke", "kan", "durch", "blos\u00b7ses", "Sehn", "ver\u00b7giff\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Drum ists kein Wunder/ da\u00df das Feuer brennen kan/", "tokens": ["Drum", "ists", "kein", "Wun\u00b7der", "/", "da\u00df", "das", "Feu\u00b7er", "bren\u00b7nen", "kan", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PIAT", "NN", "$(", "KOUS", "ART", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So du abwesend kanst in meiner Seelen stifften.", "tokens": ["So", "du", "ab\u00b7we\u00b7send", "kanst", "in", "mei\u00b7ner", "See\u00b7len", "stiff\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADJD", "VMFIN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So bald dein Saamen-Licht mich einmahl angeblitzt/", "tokens": ["So", "bald", "dein", "Saa\u00b7men\u00b7Licht", "mich", "ein\u00b7mahl", "an\u00b7ge\u00b7blitzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So wurde Seel und Leib in voller Flamm entz\u00fcndet", "tokens": ["So", "wur\u00b7de", "Seel", "und", "Leib", "in", "vol\u00b7ler", "Flamm", "ent\u00b7z\u00fcn\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "NN", "KON", "NN", "APPR", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Dein s\u00fcsses Gifft zugleich im Marck und Bein gespr\u00fctzt/", "tokens": ["Dein", "s\u00fcs\u00b7ses", "Gifft", "zu\u00b7gleich", "im", "Marck", "und", "Bein", "ge\u00b7spr\u00fctzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "APPRART", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da\u00df in mir Lebenden das Leben fast verschwindet.", "tokens": ["Da\u00df", "in", "mir", "Le\u00b7ben\u00b7den", "das", "Le\u00b7ben", "fast", "ver\u00b7schwin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "NN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wilt du nun nicht mein Artzt und mein Erretter seyn;", "tokens": ["Wilt", "du", "nun", "nicht", "mein", "Artzt", "und", "mein", "Er\u00b7ret\u00b7ter", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PTKNEG", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Werd ich voll Flamm und Gifft vor deinen Augen sterben/", "tokens": ["Werd", "ich", "voll", "Flamm", "und", "Gifft", "vor", "dei\u00b7nen", "Au\u00b7gen", "ster\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Weil der/ in den/ sich schenckt der Augen-Strahl hinein/", "tokens": ["Weil", "der", "/", "in", "den", "/", "sich", "schenckt", "der", "Au\u00b7gen\u00b7Strahl", "hin\u00b7ein", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "$(", "APPR", "ART", "$(", "PRF", "VVFIN", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wenn du nicht l\u00f6schen wilt/ vor Gluhten mu\u00df verderben.", "tokens": ["Wenn", "du", "nicht", "l\u00f6\u00b7schen", "wilt", "/", "vor", "Gluh\u00b7ten", "mu\u00df", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$(", "APPR", "NN", "VMFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Dorinde, wenn du nun noch Gnade vor mir hast/", "tokens": ["Do\u00b7rin\u00b7de", ",", "wenn", "du", "nun", "noch", "Gna\u00b7de", "vor", "mir", "hast", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "ADV", "ADV", "NN", "APPR", "PPER", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "So sage/ ob ich darf noch heute zu dir kommen/", "tokens": ["So", "sa\u00b7ge", "/", "ob", "ich", "darf", "noch", "heu\u00b7te", "zu", "dir", "kom\u00b7men", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "KOUS", "PPER", "VMFIN", "ADV", "ADV", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Weil meine Seele sich nicht eh zu frieden fa\u00dft/", "tokens": ["Weil", "mei\u00b7ne", "See\u00b7le", "sich", "nicht", "eh", "zu", "frie\u00b7den", "fa\u00dft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "PTKNEG", "KOUS", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Bis deine Gottheit ihr hat ihre Traur benommen.", "tokens": ["Bis", "dei\u00b7ne", "Got\u00b7theit", "ihr", "hat", "ih\u00b7re", "Traur", "be\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}