{"textgrid.poem.46914": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[menschenkind, noch eh sie trocken]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Menschenkind, noch eh sie trocken", "tokens": ["Men\u00b7schen\u00b7kind", ",", "noch", "eh", "sie", "tro\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "KOUS", "PPER", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist von deinem Blute,", "tokens": ["Ist", "von", "dei\u00b7nem", "Blu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Willst du sie noch einmal locken", "tokens": ["Willst", "du", "sie", "noch", "ein\u00b7mal", "lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gegen dich, die Ruthe?", "tokens": ["Ge\u00b7gen", "dich", ",", "die", "Ru\u00b7the", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Solltest heilsam sein erschrocken", "tokens": ["Soll\u00b7test", "heil\u00b7sam", "sein", "er\u00b7schro\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAINF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit dem\u00fcth'gem Muthe;", "tokens": ["Mit", "de\u00b7m\u00fcth'\u00b7gem", "Mu\u00b7the", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tr