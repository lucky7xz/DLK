{"dta.poem.7144": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung des Schlafs,  \n als  \n eine G\u00f6ttliche Wohlthat,  \n bey dem 1728sten Jahres-Wechsel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dies kann uns nun zugleich entdecken,", "tokens": ["Dies", "kann", "uns", "nun", "zu\u00b7gleich", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie sehr die Seele auch in Traum, durch Gram und Pein,", "tokens": ["Wie", "sehr", "die", "See\u00b7le", "auch", "in", "Traum", ",", "durch", "Gram", "und", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "ADV", "APPR", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Offt sehr bewegt, offt sehr empfindlich seyn.", "tokens": ["Offt", "sehr", "be\u00b7wegt", ",", "offt", "sehr", "emp\u00b7find\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$,", "ADV", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Entsetzlich ist die Angst, unleidlich ist der Schmertz,", "tokens": ["Ent\u00b7setz\u00b7lich", "ist", "die", "Angst", ",", "un\u00b7leid\u00b7lich", "ist", "der", "Schmertz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So uns ein schwerer Traum erwecken,", "tokens": ["So", "uns", "ein", "schwe\u00b7rer", "Traum", "er\u00b7we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und in uns w\u00fcrcken kann. Wenn das beklemmte Hertz,", "tokens": ["Und", "in", "uns", "w\u00fcr\u00b7cken", "kann", ".", "Wenn", "das", "be\u00b7klemm\u00b7te", "Hertz", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVINF", "VMFIN", "$.", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Von banger Furcht erf\u00fcllt, blutgier\u2019ge R\u00e4uber schrecken,", "tokens": ["Von", "ban\u00b7ger", "Furcht", "er\u00b7f\u00fcllt", ",", "blut\u00b7gier'\u00b7ge", "R\u00e4u\u00b7ber", "schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die in geschw\u00e4rtzten M\u00f6rder-H\u00f6len,", "tokens": ["Die", "in", "ge\u00b7schw\u00e4rtz\u00b7ten", "M\u00f6r\u00b7der\u00b7H\u00f6len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Uns zu zersleischen, zu entseelen,", "tokens": ["Uns", "zu", "zer\u00b7slei\u00b7schen", ",", "zu", "ent\u00b7see\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Mit Stahl und Flammen drohn. Wenn aufgesperrte Ra-", "tokens": ["Mit", "Stahl", "und", "Flam\u00b7men", "drohn", ".", "Wenn", "auf\u00b7ge\u00b7sperr\u00b7te", "Ra"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$.", "KOUS", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "chen,", "tokens": ["chen", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "-", "measure": "single.down"}, "line.12": {"text": "Von Tigern, Basilisken, Drachen,", "tokens": ["Von", "Ti\u00b7gern", ",", "Ba\u00b7si\u00b7lis\u00b7ken", ",", "Dra\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Um uns zu w\u00fcrgen, zu zersticken,", "tokens": ["Um", "uns", "zu", "w\u00fcr\u00b7gen", ",", "zu", "zer\u00b7sti\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "PPER", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Den heissen Zahn, mit knirschendem Ger\u00e4usch,", "tokens": ["Den", "heis\u00b7sen", "Zahn", ",", "mit", "knir\u00b7schen\u00b7dem", "Ge\u00b7r\u00e4usch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "In das schon angefasste Fleisch,", "tokens": ["In", "das", "schon", "an\u00b7ge\u00b7fass\u00b7te", "Fleisch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Ergrimmt bereits zusammen dr\u00fccken.", "tokens": ["Er\u00b7grimmt", "be\u00b7reits", "zu\u00b7sam\u00b7men", "dr\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Wenn wir offt schwartz-und aufgesperrte Klauen", "tokens": ["Wenn", "wir", "offt", "schwartz\u00b7\u00b7und", "auf\u00b7ge\u00b7sperr\u00b7te", "Klau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Verlarffter Schreck-Gespenster schauen,", "tokens": ["Ver\u00b7larff\u00b7ter", "Schreck\u00b7Ge\u00b7spens\u00b7ter", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Die sie bereits zum Greiffen ausgestreckt,", "tokens": ["Die", "sie", "be\u00b7reits", "zum", "Greif\u00b7fen", "aus\u00b7ge\u00b7streckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Und man von Schrecken starr, sodann,", "tokens": ["Und", "man", "von", "Schre\u00b7cken", "starr", ",", "so\u00b7dann", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PIS", "APPR", "NN", "ADJD", "$,", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Um ihnen zu entfliehn, kein Glied nicht regen kann.", "tokens": ["Um", "ih\u00b7nen", "zu", "ent\u00b7fliehn", ",", "kein", "Glied", "nicht", "re\u00b7gen", "kann", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "PTKZU", "VVINF", "$,", "PIAT", "NN", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}