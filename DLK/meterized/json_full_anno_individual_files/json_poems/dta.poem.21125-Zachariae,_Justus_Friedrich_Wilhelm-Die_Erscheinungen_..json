{"dta.poem.21125": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Erscheinungen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Senkt euch herab, mittern\u00e4chtliche Schauer,", "tokens": ["Senkt", "euch", "her\u00b7ab", ",", "mit\u00b7ter\u00b7n\u00e4cht\u00b7li\u00b7che", "Schau\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Von des Olymps dunkeln Wolkengeb\u00fcrgen;", "tokens": ["Von", "des", "O\u00b7lymps", "dun\u00b7keln", "Wol\u00b7ken\u00b7ge\u00b7b\u00fcr\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "F\u00fcllt dies Gemach, von der sterbenden Lampe", "tokens": ["F\u00fcllt", "dies", "Ge\u00b7mach", ",", "von", "der", "ster\u00b7ben\u00b7den", "Lam\u00b7pe"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PDS", "NN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Furchtsam erhellt.", "tokens": ["Furcht\u00b7sam", "er\u00b7hellt", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Jetzt, da das Herz aller Furchtsamen klopfet,", "tokens": ["Jetzt", ",", "da", "das", "Herz", "al\u00b7ler", "Furcht\u00b7sa\u00b7men", "klop\u00b7fet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und sich mit Angst vor Erscheinungen f\u00fcrchtet,", "tokens": ["Und", "sich", "mit", "Angst", "vor", "Er\u00b7schei\u00b7nun\u00b7gen", "f\u00fcrch\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "W\u00fcnsch ich, vertieft in den schrecklichen Stunden,", "tokens": ["W\u00fcnsch", "ich", ",", "ver\u00b7tieft", "in", "den", "schreck\u00b7li\u00b7chen", "Stun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Geister um mich.", "tokens": ["Geis\u00b7ter", "um", "mich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Ach! bist du todt, oder lebst, wie die Todten,", "tokens": ["Ach", "!", "bist", "du", "todt", ",", "o\u00b7der", "lebst", ",", "wie", "die", "Tod\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VAFIN", "PPER", "ADJD", "$,", "KON", "VVFIN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die mich geliebt, ungl\u00fcckselige Sch\u00f6ne!", "tokens": ["Die", "mich", "ge\u00b7liebt", ",", "un\u00b7gl\u00fcck\u00b7se\u00b7li\u00b7ge", "Sch\u00f6\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O so komm tetzt! W\u00e4r es auch nur ein Schatten", "tokens": ["O", "so", "komm", "tetzt", "!", "W\u00e4r", "es", "auch", "nur", "ein", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VVFIN", "ADJD", "$.", "VAFIN", "PPER", "ADV", "ADV", "ART", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Trauriger Furcht.", "tokens": ["Trau\u00b7ri\u00b7ger", "Furcht", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}