{"dta.poem.5893": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Elementen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hingegen Heraclit, und zwar", "tokens": ["Hin\u00b7ge\u00b7gen", "Her\u00b7a\u00b7clit", ",", "und", "zwar"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "NE", "$,", "KON", "ADV"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Nicht unwahrscheinlich, hat gelehrt:", "tokens": ["Nicht", "un\u00b7wahr\u00b7schein\u00b7lich", ",", "hat", "ge\u00b7lehrt", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es w\u00fcrde von der Elementen-Schaar", "tokens": ["Es", "w\u00fcr\u00b7de", "von", "der", "E\u00b7le\u00b7men\u00b7ten\u00b7Schaar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das eine stets ins andere verkehret.", "tokens": ["Das", "ei\u00b7ne", "stets", "ins", "an\u00b7de\u00b7re", "ver\u00b7keh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADV", "APPRART", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}