{"textgrid.poem.48715": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "2. Bei einem Angebinde", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was hier mein liebster Freund und ich euch \u00fcbersenden,", "tokens": ["Was", "hier", "mein", "liebs\u00b7ter", "Freund", "und", "ich", "euch", "\u00fc\u00b7ber\u00b7sen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPOSAT", "ADJA", "NN", "KON", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ihr, Sohn des Helikons und seiner G\u00f6tter Zier,", "tokens": ["ihr", ",", "Sohn", "des", "He\u00b7li\u00b7kons", "und", "sei\u00b7ner", "G\u00f6t\u00b7ter", "Zier", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "ART", "NN", "KON", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "das komt von gutem Wundsch und herzlicher Begier,", "tokens": ["das", "komt", "von", "gu\u00b7tem", "Wund\u00b7sch", "und", "herz\u00b7li\u00b7cher", "Be\u00b7gier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "mit der ich euch zu uns und wir zu euch uns wenden.", "tokens": ["mit", "der", "ich", "euch", "zu", "uns", "und", "wir", "zu", "euch", "uns", "wen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "APPR", "PPER", "KON", "PPER", "APPR", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nehmt hin, nehmt, nehmt es hin mit freudenvollen H\u00e4nden", "tokens": ["Nehmt", "hin", ",", "nehmt", ",", "nehmt", "es", "hin", "mit", "freu\u00b7den\u00b7vol\u00b7len", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$,", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "di\u00df zwar geringe Tun; doch ist nichts Schlechtes hier.", "tokens": ["di\u00df", "zwar", "ge\u00b7rin\u00b7ge", "Tun", ";", "doch", "ist", "nichts", "Schlech\u00b7tes", "hier", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJA", "NN", "$.", "ADV", "VAFIN", "PIS", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hier ist der gro\u00dfe Held, den alle Welt und wir", "tokens": ["Hier", "ist", "der", "gro\u00b7\u00dfe", "Held", ",", "den", "al\u00b7le", "Welt", "und", "wir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,", "ART", "PIAT", "NN", "KON", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "f\u00fcr einen andern Gott beloben aller Enden.", "tokens": ["f\u00fcr", "ei\u00b7nen", "an\u00b7dern", "Gott", "be\u00b7lo\u00b7ben", "al\u00b7ler", "En\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Gott hat uns diesen Gott zu einem Sch\u00fctzer geben.", "tokens": ["Gott", "hat", "uns", "die\u00b7sen", "Gott", "zu", "ei\u00b7nem", "Sch\u00fct\u00b7zer", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PDAT", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und hier ist noch ein Gott, der uns kan recht beleben", "tokens": ["Und", "hier", "ist", "noch", "ein", "Gott", ",", "der", "uns", "kan", "recht", "be\u00b7le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "VMFIN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "mit seiner S\u00fc\u00dfigkeit Was kan man mehrers wol", "tokens": ["mit", "sei\u00b7ner", "S\u00fc\u00b7\u00dfig\u00b7keit", "Was", "kan", "man", "meh\u00b7rers", "wol"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PWS", "VMFIN", "PIS", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "an eurem Namenstag' euch, lieber G\u00f6nner, g\u00f6nnen?", "tokens": ["an", "eu\u00b7rem", "Na\u00b7mens\u00b7tag'", "euch", ",", "lie\u00b7ber", "G\u00f6n\u00b7ner", ",", "g\u00f6n\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "$,", "ADV", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wir schicken G\u00f6tter euch und w\u00fcndschen, was wir k\u00f6nnen,", "tokens": ["Wir", "schi\u00b7cken", "G\u00f6t\u00b7ter", "euch", "und", "w\u00fcnd\u00b7schen", ",", "was", "wir", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"