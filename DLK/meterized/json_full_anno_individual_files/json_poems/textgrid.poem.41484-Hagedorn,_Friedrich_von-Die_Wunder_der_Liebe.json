{"textgrid.poem.41484": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Die Wunder der Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Liebe Macht ist allgemein,", "tokens": ["Der", "Lie\u00b7be", "Macht", "ist", "all\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr dient ein jeder Stand auf Erden.", "tokens": ["Ihr", "dient", "ein", "je\u00b7der", "Stand", "auf", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "PIAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es kann durch sie ein K\u00f6nig klein,", "tokens": ["Es", "kann", "durch", "sie", "ein", "K\u00f6\u00b7nig", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Sch\u00e4fer gro\u00df und edel werden.", "tokens": ["Ein", "Sch\u00e4\u00b7fer", "gro\u00df", "und", "e\u00b7del", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Tyrannen raubt sie Stolz und Wuth,", "tokens": ["Ty\u00b7ran\u00b7nen", "raubt", "sie", "Stolz", "und", "Wuth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Helden Lust und Kraft zum Streiten;", "tokens": ["Den", "Hel\u00b7den", "Lust", "und", "Kraft", "zum", "Strei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der Feigheit gibt sie starken Muth,", "tokens": ["Der", "Feig\u00b7heit", "gibt", "sie", "star\u00b7ken", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Falschheit wahre Z\u00e4rtlichkeiten.", "tokens": ["Der", "Falschheit", "wah\u00b7re", "Z\u00e4rt\u00b7lich\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Einfalt schenkt sie den Verstand,", "tokens": ["Der", "Ein\u00b7falt", "schenkt", "sie", "den", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den sie der Klugheit oft entwendet.", "tokens": ["Den", "sie", "der", "Klug\u00b7heit", "oft", "ent\u00b7wen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Grillenf\u00e4nger wird galant,", "tokens": ["Ein", "Gril\u00b7len\u00b7f\u00e4n\u00b7ger", "wird", "ga\u00b7lant", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn sie an ihm den Sieg vollendet.", "tokens": ["Wenn", "sie", "an", "ihm", "den", "Sieg", "voll\u00b7en\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ART", "NN", "VVPP", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Des strengen Alters Eigensinn", "tokens": ["Des", "stren\u00b7gen", "Al\u00b7ters", "Ei\u00b7gen\u00b7sinn"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Verwandelt sie in Scherz und Lachen,", "tokens": ["Ver\u00b7wan\u00b7delt", "sie", "in", "Scherz", "und", "La\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und diese holde Lehrerin", "tokens": ["Und", "die\u00b7se", "hol\u00b7de", "Leh\u00b7re\u00b7rin"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Kann auch die Jugend altklug machen.", "tokens": ["Kann", "auch", "die", "Ju\u00b7gend", "alt\u00b7klug", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein Spanier vergi\u00dft den Rang,", "tokens": ["Ein", "Spa\u00b7nier", "ver\u00b7gi\u00dft", "den", "Rang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Unedlen Sch\u00f6nen liebzukosen:", "tokens": ["U\u00b7ned\u00b7len", "Sch\u00f6\u00b7nen", "lieb\u00b7zu\u00b7ko\u00b7sen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Ein junger Franzmann den Gesang,", "tokens": ["Ein", "jun\u00b7ger", "Franz\u00b7mann", "den", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Wahn, das Selbstlob der Franzosen.", "tokens": ["Den", "Wahn", ",", "das", "Selbst\u00b7lob", "der", "Fran\u00b7zo\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn jenen Reiz und Sch\u00f6nheit k\u00f6rnt,", "tokens": ["Wenn", "je\u00b7nen", "Reiz", "und", "Sch\u00f6n\u00b7heit", "k\u00f6rnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Entsaget er dem Hochmuthstriebe:", "tokens": ["Ent\u00b7sa\u00b7get", "er", "dem", "Hoch\u00b7muth\u00b7strie\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und dieser seufzet und erlernt,", "tokens": ["Und", "die\u00b7ser", "seuf\u00b7zet", "und", "er\u00b7lernt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "KON", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Freiheit prahle, nicht die Liebe.", "tokens": ["Die", "Frei\u00b7heit", "prah\u00b7le", ",", "nicht", "die", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie gibt der deutschen M\u00e4nnlichkeit", "tokens": ["Sie", "gibt", "der", "deut\u00b7schen", "M\u00e4nn\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die sanfte Schmeichelei beim K\u00fcssen,", "tokens": ["Die", "sanf\u00b7te", "Schmei\u00b7che\u00b7lei", "beim", "K\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den Heiligen die L\u00fcsternheit,", "tokens": ["Den", "Hei\u00b7li\u00b7gen", "die", "L\u00fcs\u00b7tern\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und auch den Juden ein Gewissen.", "tokens": ["Und", "auch", "den", "Ju\u00b7den", "ein", "Ge\u00b7wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie fand, so oft sie sich nur wies,", "tokens": ["Sie", "fand", ",", "so", "oft", "sie", "sich", "nur", "wies", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "ADV", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Verehrer in den besten Kennern.", "tokens": ["Ver\u00b7eh\u00b7rer", "in", "den", "bes\u00b7ten", "Ken\u00b7nern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Nur sie entwarf ein Paradies", "tokens": ["Nur", "sie", "ent\u00b7warf", "ein", "Pa\u00b7ra\u00b7dies"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den ihr geweihten Muselm\u00e4nnern.", "tokens": ["Den", "ihr", "ge\u00b7weih\u00b7ten", "Mu\u00b7sel\u00b7m\u00e4n\u00b7nern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ja! deine siegende Gewalt,", "tokens": ["Ja", "!", "dei\u00b7ne", "sie\u00b7gen\u00b7de", "Ge\u00b7walt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "O Liebe! wird umsonst bestritten.", "tokens": ["O", "Lie\u00b7be", "!", "wird", "um\u00b7sonst", "be\u00b7strit\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VAFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dir unterwirft sich Jung und Alt", "tokens": ["Dir", "un\u00b7ter\u00b7wirft", "sich", "Jung", "und", "Alt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "NE", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An H\u00f6fen und in Sch\u00e4ferh\u00fctten.", "tokens": ["An", "H\u00f6\u00b7fen", "und", "in", "Sch\u00e4\u00b7fer\u00b7h\u00fct\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch meine Sch\u00f6ne hofft allein", "tokens": ["Doch", "mei\u00b7ne", "Sch\u00f6\u00b7ne", "hofft", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Reizungen zu widerstehen.", "tokens": ["Den", "Rei\u00b7zun\u00b7gen", "zu", "wi\u00b7der\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.7": {"text": "O la\u00df sie mir nur g\u00fcnstig sein!", "tokens": ["O", "la\u00df", "sie", "mir", "nur", "g\u00fcns\u00b7tig", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PPER", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wie wirst du dich ger\u00e4chet sehen!", "tokens": ["Wie", "wirst", "du", "dich", "ge\u00b7r\u00e4\u00b7chet", "se\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der Liebe Macht ist allgemein,", "tokens": ["Der", "Lie\u00b7be", "Macht", "ist", "all\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr dient ein jeder Stand auf Erden.", "tokens": ["Ihr", "dient", "ein", "je\u00b7der", "Stand", "auf", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "PIAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es kann durch sie ein K\u00f6nig klein,", "tokens": ["Es", "kann", "durch", "sie", "ein", "K\u00f6\u00b7nig", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "mea