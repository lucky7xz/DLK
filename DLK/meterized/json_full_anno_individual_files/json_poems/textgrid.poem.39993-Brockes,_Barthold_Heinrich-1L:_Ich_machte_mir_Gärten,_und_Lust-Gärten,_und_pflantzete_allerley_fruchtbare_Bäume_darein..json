{"textgrid.poem.39993": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich machte mir G\u00e4rten, und Lust-G\u00e4rten, und pflantzete allerley fruchtbare B\u00e4ume darein.", "genre": "verse", "period": "N.A.", "pub_year": 1713, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich machte mir G\u00e4rten, und Lust-G\u00e4rten, und pflantzete allerley fruchtbare B\u00e4ume darein.", "tokens": ["Ich", "mach\u00b7te", "mir", "G\u00e4r\u00b7ten", ",", "und", "Lust\u00b7G\u00e4r\u00b7ten", ",", "und", "pflant\u00b7ze\u00b7te", "al\u00b7ler\u00b7ley", "frucht\u00b7ba\u00b7re", "B\u00e4u\u00b7me", "da\u00b7rein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$,", "KON", "NN", "$,", "KON", "VVFIN", "PIAT", "ADJA", "NN", "PAV", "$."], "meter": "-+--+--++--+--+-+---+--+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Ihr durch die Leidenschaft verf\u00fchrte Seelen, h\u00f6ret,", "tokens": ["Ihr", "durch", "die", "Lei\u00b7den\u00b7schaft", "ver\u00b7f\u00fchr\u00b7te", "See\u00b7len", ",", "h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer Gott, durch Sein Gesch\u00f6pf ger\u00fchrt, mit Freuden ehret,", "tokens": ["Wer", "Gott", ",", "durch", "Sein", "Ge\u00b7sch\u00f6pf", "ge\u00b7r\u00fchrt", ",", "mit", "Freu\u00b7den", "eh\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und seine Lust mit Ernst in Gottes Wercken sucht;", "tokens": ["Und", "sei\u00b7ne", "Lust", "mit", "Ernst", "in", "Got\u00b7tes", "Wer\u00b7cken", "sucht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "NE", "APPR", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem tr\u00e4get jede Bluhm, die er betrachtend schaut,", "tokens": ["Dem", "tr\u00e4\u00b7get", "je\u00b7de", "Bluhm", ",", "die", "er", "be\u00b7trach\u00b7tend", "schaut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "NN", "$,", "PRELS", "PPER", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dem tr\u00e4get jeder Baum, dem tr\u00e4get jedes Kraut,", "tokens": ["Dem", "tr\u00e4\u00b7get", "je\u00b7der", "Baum", ",", "dem", "tr\u00e4\u00b7get", "je\u00b7des", "Kraut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "NN", "$,", "PRELS", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dem tr\u00e4get jedes Blatt der Freuden s\u00fcsse Frucht.", "tokens": ["Dem", "tr\u00e4\u00b7get", "je\u00b7des", "Blatt", "der", "Freu\u00b7den", "s\u00fcs\u00b7se", "Frucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich machte mir G\u00e4rten, und Lust-G\u00e4rten, und pflantzete allerley fruchtbare B\u00e4ume darein.", "tokens": ["Ich", "mach\u00b7te", "mir", "G\u00e4r