{"textgrid.poem.48678": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "41. An Anemonen, die Liebste", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "meines Herzen stete Zier,", "tokens": ["mei\u00b7nes", "Her\u00b7zen", "ste\u00b7te", "Zier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "meine Klarheit, meine Sonne,", "tokens": ["mei\u00b7ne", "Klar\u00b7heit", ",", "mei\u00b7ne", "Son\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "kanst du di\u00df denn gl\u00e4uben dir,", "tokens": ["kanst", "du", "di\u00df", "denn", "gl\u00e4u\u00b7ben", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PDS", "ADV", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "da\u00df, was dir mein Mund verspricht,", "tokens": ["da\u00df", ",", "was", "dir", "mein", "Mund", "ver\u00b7spricht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWS", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "meine mein Gem\u00fcte nicht?", "tokens": ["mei\u00b7ne", "mein", "Ge\u00b7m\u00fc\u00b7te", "nicht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nicht so, Liebste. La\u00df dir sagen,", "tokens": ["Nicht", "so", ",", "Liebs\u00b7te", ".", "La\u00df", "dir", "sa\u00b7ge