{"dta.poem.2889": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "90.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn dir ein Gl\u00fcck will nahn, o nenne nicht das Gl\u00fcck", "tokens": ["Wenn", "dir", "ein", "Gl\u00fcck", "will", "nahn", ",", "o", "nen\u00b7ne", "nicht", "das", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VMFIN", "ADJA", "$,", "FM", "VVFIN", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bei seinem Namen! scheu vor'm Namen weicht's zur\u00fcck.", "tokens": ["Bei", "sei\u00b7nem", "Na\u00b7men", "!", "scheu", "vor'm", "Na\u00b7men", "weicht's", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "ADJD", "APPRART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und droht ein Ungl\u00fcck dir, so nenn' es nicht beim Namen,", "tokens": ["Und", "droht", "ein", "Un\u00b7gl\u00fcck", "dir", ",", "so", "nenn'", "es", "nicht", "beim", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sonst siehst du zwei, die auf des Einen Namen kamen.", "tokens": ["Sonst", "siehst", "du", "zwei", ",", "die", "auf", "des", "Ei\u00b7nen", "Na\u00b7men", "ka\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "CARD", "$,", "PRELS", "APPR", "ART", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So \u00fcbel ists bestellt, mein Sohn, um diese Welt,", "tokens": ["So", "\u00fc\u00b7bel", "ists", "be\u00b7stellt", ",", "mein", "Sohn", ",", "um", "die\u00b7se", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "VVPP", "$,", "PPOSAT", "NN", "$,", "KOUI", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df B\u00f6ses bei dir zieht, was Gutes ab dir h\u00e4lt.", "tokens": ["Da\u00df", "B\u00f6\u00b7ses", "bei", "dir", "zieht", ",", "was", "Gu\u00b7tes", "ab", "dir", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PPER", "VVFIN", "$,", "PWS", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch dich nicht lehren will ich dieses, o mein Sohn;", "tokens": ["Doch", "dich", "nicht", "leh\u00b7ren", "will", "ich", "die\u00b7ses", ",", "o", "mein", "Sohn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKNEG", "VVINF", "VMFIN", "PPER", "PDS", "$,", "FM", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich selber lernt' es nur, du lerne nichts davon!", "tokens": ["Ich", "sel\u00b7ber", "lernt'", "es", "nur", ",", "du", "ler\u00b7ne", "nichts", "da\u00b7von", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "ADV", "$,", "PPER", "VVFIN", "PIS", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ich w\u00fcnsche, da\u00df du nie so eingesch\u00fcchtert werdest,", "tokens": ["Ich", "w\u00fcn\u00b7sche", ",", "da\u00df", "du", "nie", "so", "ein\u00b7ge\u00b7sch\u00fcch\u00b7tert", "wer\u00b7dest", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Frei immer, wie es dir ums Herz ist, dich geberdest.", "tokens": ["Frei", "im\u00b7mer", ",", "wie", "es", "dir", "ums", "Herz", "ist", ",", "dich", "ge\u00b7ber\u00b7dest", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PWAV", "PPER", "PPER", "APPRART", "NN", "VAFIN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}