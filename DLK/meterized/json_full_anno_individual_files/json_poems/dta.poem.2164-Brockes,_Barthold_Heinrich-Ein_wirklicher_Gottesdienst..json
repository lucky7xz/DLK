{"dta.poem.2164": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ein wirklicher Gottesdienst.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Einzig- Ewig- Selge Wesen, das Raum und", "tokens": ["Das", "Ein\u00b7zig", "E\u00b7wig", "Sel\u00b7ge", "We\u00b7sen", ",", "das", "Raum", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "TRUNC", "TRUNC", "NN", "NN", "$,", "ART", "NN", "KON"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ewigkeit erf\u00fcllet,", "tokens": ["E\u00b7wig\u00b7keit", "er\u00b7f\u00fcl\u00b7let", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Aus dem der Sonn- und Welten Heer, das nicht zu z\u00e4hlen,", "tokens": ["Aus", "dem", "der", "Sonn", "und", "Wel\u00b7ten", "Heer", ",", "das", "nicht", "zu", "z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "TRUNC", "KON", "NN", "NN", "$,", "PRELS", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "quoll und quillet,", "tokens": ["quoll", "und", "quil\u00b7let", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Der sein allgegenw\u00e4rtigs All in ihnen zeiget und verh\u00fcllet,", "tokens": ["Der", "sein", "all\u00b7ge\u00b7gen\u00b7w\u00e4r\u00b7tigs", "All", "in", "ih\u00b7nen", "zei\u00b7get", "und", "ver\u00b7h\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "APPR", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.6": {"text": "Ist unsrer Ehrfurcht, Andacht, Lieb, auch unsers Dien-", "tokens": ["Ist", "uns\u00b7rer", "Ehr\u00b7furcht", ",", "An\u00b7dacht", ",", "Lieb", ",", "auch", "un\u00b7sers", "Die\u00b7n"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "NN", "$,", "NN", "$,", "ADV", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "stes werth. Allein,", "tokens": ["stes", "werth", ".", "Al\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "ADJD", "$.", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Nun kann Er freylich angebetet, geliebet und gelobet seyn.", "tokens": ["Nun", "kann", "Er", "frey\u00b7lich", "an\u00b7ge\u00b7be\u00b7tet", ",", "ge\u00b7lie\u00b7bet", "und", "ge\u00b7lo\u00b7bet", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVPP", "$,", "VVPP", "KON", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.9": {"text": "Wie aber k\u00f6nnen wir ihm dienen? Die\u00df ist nicht m\u00f6g-", "tokens": ["Wie", "a\u00b7ber", "k\u00f6n\u00b7nen", "wir", "ihm", "die\u00b7nen", "?", "Die\u00df", "ist", "nicht", "m\u00f6g"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "PPER", "VVINF", "$.", "PDS", "VAFIN", "PTKNEG", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "lich. Gott gebraucht", "tokens": ["lich", ".", "Gott", "ge\u00b7braucht"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADJD", "$.", "NN", "VVPP"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Armselger Menschen Dienste nicht: und dennoch hat er uns,", "tokens": ["Arm\u00b7sel\u00b7ger", "Men\u00b7schen", "Diens\u00b7te", "nicht", ":", "und", "den\u00b7noch", "hat", "er", "uns", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "PTKNEG", "$.", "KON", "ADV", "VAFIN", "PPER", "PPER", "$,"], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.12": {"text": "zum Zeichen", "tokens": ["zum", "Zei\u00b7chen"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "Von seiner Huld, Gelegenheit in diesem Leben wollen reichen,", "tokens": ["Von", "sei\u00b7ner", "Huld", ",", "Ge\u00b7le\u00b7gen\u00b7heit", "in", "die\u00b7sem", "Le\u00b7ben", "wol\u00b7len", "rei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "NN", "APPR", "PDAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.14": {"text": "Wodurch wir ihm zu dienen f\u00e4hig. Die\u00df ist dein N\u00e4ch-", "tokens": ["Wo\u00b7durch", "wir", "ihm", "zu", "die\u00b7nen", "f\u00e4\u00b7hig", ".", "Die\u00df", "ist", "dein", "N\u00e4ch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPER", "PTKZU", "VVINF", "ADJD", "$.", "PDS", "VAFIN", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.15": {"text": "ster: liebt man ihn,", "tokens": ["ster", ":", "liebt", "man", "ihn", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "VVFIN", "PIS", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.16": {"text": "Nimmt er es, da auch der sein Werk, so, als ob man ihm", "tokens": ["Nimmt", "er", "es", ",", "da", "auch", "der", "sein", "Werk", ",", "so", ",", "als", "ob", "man", "ihm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "$,", "KOUS", "ADV", "ART", "PPOSAT", "NN", "$,", "ADV", "$,", "KOKOM", "KOUS", "PIS", "PPER"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.17": {"text": "selber dien'.", "tokens": ["sel\u00b7ber", "dien'", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.18": {"text": "A. Wofern uns etwas auf der Welt zur N\u00e4chstenliebe", "tokens": ["A.", "Wo\u00b7fern", "uns", "et\u00b7was", "auf", "der", "Welt", "zur", "N\u00e4chs\u00b7ten\u00b7lie\u00b7be"], "token_info": ["abbreviation", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPER", "ADV", "APPR", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "treiben sollte,", "tokens": ["trei\u00b7ben", "soll\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.20": {"text": "So w\u00e4r es ja wohl dieser Grund, so w\u00e4r es die\u00df Gesetz.", "tokens": ["So", "w\u00e4r", "es", "ja", "wohl", "die\u00b7ser", "Grund", ",", "so", "w\u00e4r", "es", "die\u00df", "Ge\u00b7setz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "PDAT", "NN", "$,", "ADV", "VAFIN", "PPER", "PDS", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.21": {"text": "Wer wollte", "tokens": ["Wer", "woll\u00b7te"], "token_info": ["word", "word"], "pos": ["PWS", "VMFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.22": {"text": "Dem Sch\u00f6pfer im Gesch\u00f6pf nicht dienen? Wenn die\u00df", "tokens": ["Dem", "Sch\u00f6p\u00b7fer", "im", "Ge\u00b7sch\u00f6pf", "nicht", "die\u00b7nen", "?", "Wenn", "die\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "PTKNEG", "VVINF", "$.", "KOUS", "PDS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "die Menschen doch nur w\u00fc\u00dften,", "tokens": ["die", "Men\u00b7schen", "doch", "nur", "w\u00fc\u00df\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Da\u00df sie, auf eine solche Weise, den Gottesdienst ver-", "tokens": ["Da\u00df", "sie", ",", "auf", "ei\u00b7ne", "sol\u00b7che", "Wei\u00b7se", ",", "den", "Got\u00b7tes\u00b7dienst", "ver"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "APPR", "ART", "PIAT", "NN", "$,", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.25": {"text": "richten m\u00fc\u00dften.", "tokens": ["rich\u00b7ten", "m\u00fc\u00df\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.26": {"text": "Warum lehrt aber, wenn die\u00df wahr, die Bibel dieses", "tokens": ["Wa\u00b7rum", "lehrt", "a\u00b7ber", ",", "wenn", "die\u00df", "wahr", ",", "die", "Bi\u00b7bel", "die\u00b7ses"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ADV", "$,", "KOUS", "PDS", "PTKVZ", "$,", "ART", "NN", "PDAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "nicht die Christen?", "tokens": ["nicht", "die", "Chris\u00b7ten", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.28": {"text": "B. Du irrest dich, die Bibel lehrt es, schau diesen hol-", "tokens": ["B.", "Du", "ir\u00b7rest", "dich", ",", "die", "Bi\u00b7bel", "lehrt", "es", ",", "schau", "die\u00b7sen", "hol"], "token_info": ["abbreviation", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PPER", "VVFIN", "PPER", "$,", "ART", "NN", "VVFIN", "PPER", "$,", "ADJD", "PDAT", "TRUNC"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.29": {"text": "den Spruch nur an:", "tokens": ["den", "Spruch", "nur", "an", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.30": {"text": "Was ihr von ihnen dem Geringsten gethan habt, habt", "tokens": ["Was", "ihr", "von", "ih\u00b7nen", "dem", "Ge\u00b7rings\u00b7ten", "ge\u00b7than", "habt", ",", "habt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWS", "PPER", "APPR", "PPER", "ART", "NN", "VVPP", "VAFIN", "$,", "VAFIN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.31": {"text": "ihr mir gethan.", "tokens": ["ihr", "mir", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PPER", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}