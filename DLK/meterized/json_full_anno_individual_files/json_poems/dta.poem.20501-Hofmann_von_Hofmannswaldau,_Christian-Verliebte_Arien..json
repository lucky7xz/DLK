{"dta.poem.20501": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Verliebte Arien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wo sind die stunden", "tokens": ["Wo", "sind", "die", "stun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Der s\u00fcssen zeit/", "tokens": ["Der", "s\u00fcs\u00b7sen", "zeit", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Da ich zuerst empfunden/", "tokens": ["Da", "ich", "zu\u00b7erst", "emp\u00b7fun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie deine lieblichkeit", "tokens": ["Wie", "dei\u00b7ne", "lieb\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mich dir verbunden?", "tokens": ["Mich", "dir", "ver\u00b7bun\u00b7den", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PPER", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Sie sind verrauscht/ es bleibet doch darbey/", "tokens": ["Sie", "sind", "ver\u00b7rauscht", "/", "es", "blei\u00b7bet", "doch", "dar\u00b7bey", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$(", "PPER", "VVFIN", "ADV", "PAV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da\u00df alle lust verg\u00e4nglich sey.", "tokens": ["Da\u00df", "al\u00b7le", "lust", "ver\u00b7g\u00e4ng\u00b7lich", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das reine schertzen/", "tokens": ["Das", "rei\u00b7ne", "schert\u00b7zen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVINF", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "So mich ergetzt/", "tokens": ["So", "mich", "er\u00b7getzt", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVPP", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und in dem tieffen hertzen", "tokens": ["Und", "in", "dem", "tief\u00b7fen", "hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sein merckmahl eingesetzt/", "tokens": ["Sein", "merck\u00b7mahl", "ein\u00b7ge\u00b7setzt", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "L\u00e4st mich in schmertzen/", "tokens": ["L\u00e4st", "mich", "in", "schmert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "VVINF", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Du hast mir mehr als deutlich kund gethan/", "tokens": ["Du", "hast", "mir", "mehr", "als", "deut\u00b7lich", "kund", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIAT", "KOKOM", "ADJD", "PTKVZ", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da\u00df freundlichkeit nicht anckern kan.", "tokens": ["Da\u00df", "freund\u00b7lich\u00b7keit", "nicht", "an\u00b7ckern", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Das angedencken", "tokens": ["Das", "an\u00b7ge\u00b7den\u00b7cken"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Der zucker-lust/", "tokens": ["Der", "zu\u00b7cke\u00b7rlust", "/"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Will mich in angst versencken.", "tokens": ["Will", "mich", "in", "angst", "ver\u00b7sen\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es will verdammte kost", "tokens": ["Es", "will", "ver\u00b7damm\u00b7te", "kost"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Uns zeitlich kr\u00e4ncken/", "tokens": ["Uns", "zeit\u00b7lich", "kr\u00e4n\u00b7cken", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Was man geschmeckt/ und nicht mehr schmecken soll/", "tokens": ["Was", "man", "ge\u00b7schmeckt", "/", "und", "nicht", "mehr", "schme\u00b7cken", "soll", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVPP", "$(", "KON", "PTKNEG", "ADV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ist freuden-leer und jammer-voll.", "tokens": ["Ist", "freu\u00b7den\u00b7leer", "und", "jam\u00b7mer\u00b7voll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Empfangne k\u00fcsse/", "tokens": ["Emp\u00b7fang\u00b7ne", "k\u00fcs\u00b7se", "/"], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Ambrirter safft", "tokens": ["Am\u00b7br\u00b7ir\u00b7ter", "safft"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "--+-+", "measure": "anapaest.init"}, "line.3": {"text": "Verbleibt nicht lange s\u00fcsse/", "tokens": ["Ver\u00b7bleibt", "nicht", "lan\u00b7ge", "s\u00fcs\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und kommt von aller krafft;", "tokens": ["Und", "kommt", "von", "al\u00b7ler", "krafft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Verrauschte fl\u00fcsse", "tokens": ["Ver\u00b7rauschte", "fl\u00fcs\u00b7se"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Erqvicken nicht was unsern geist erfreut/", "tokens": ["E\u00b7rqvi\u00b7cken", "nicht", "was", "un\u00b7sern", "geist", "er\u00b7freut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "PWS", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Entspringt aus gegenw\u00e4rtigkeit.", "tokens": ["Ent\u00b7springt", "aus", "ge\u00b7gen\u00b7w\u00e4r\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich schwamm in freude/", "tokens": ["Ich", "schwamm", "in", "freu\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Der liebe hand", "tokens": ["Der", "lie\u00b7be", "hand"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Spann mir ein kleid von seide/", "tokens": ["Spann", "mir", "ein", "kleid", "von", "sei\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "APPR", "NE", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Das blat hat sich gewand/", "tokens": ["Das", "blat", "hat", "sich", "ge\u00b7wand", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PRF", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich geh\u2019 im leide/", "tokens": ["Ich", "geh'", "im", "lei\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "VVFIN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Ich wein\u2019 itzund/ da\u00df lieb und sonnenschein", "tokens": ["Ich", "wein'", "it\u00b7zund", "/", "da\u00df", "lieb", "und", "son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$(", "KOUS", "ADJD", "KON", "XY"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Stets voller angst und wolcken seyn.", "tokens": ["Stets", "vol\u00b7ler", "angst", "und", "wol\u00b7cken", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}