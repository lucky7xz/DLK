{"textgrid.poem.48387": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1. Der 6. Psalm", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach schone, gro\u00dfer Herr, ach schone mich zu strafen,", "tokens": ["Ach", "scho\u00b7ne", ",", "gro\u00b7\u00dfer", "Herr", ",", "ach", "scho\u00b7ne", "mich", "zu", "stra\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "$,", "ADJA", "NN", "$,", "ITJ", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "wenn deine Huld und Gunst bei dir ist ganz entschlafen,", "tokens": ["wenn", "dei\u00b7ne", "Huld", "und", "Gunst", "bei", "dir", "ist", "ganz", "ent\u00b7schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "KON", "NN", "APPR", "PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und du f\u00fcr Zorne brennst! Herr, z\u00fcchtige mich nicht,", "tokens": ["und", "du", "f\u00fcr", "Zor\u00b7ne", "brennst", "!", "Herr", ",", "z\u00fcch\u00b7ti\u00b7ge", "mich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "VVFIN", "$.", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "wenn dir die Grimmesglut aus Mund und Augen bricht,", "tokens": ["wenn", "dir", "die", "Grim\u00b7mes\u00b7glut", "aus", "Mund", "und", "Au\u00b7gen", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "die niemand tragen kan! Umb so viel mehr la\u00df blicken", "tokens": ["die", "nie\u00b7mand", "tra\u00b7gen", "kan", "!", "Umb", "so", "viel", "mehr", "la\u00df", "bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVINF", "VMFIN", "$.", "KOUI", "ADV", "ADV", "PIAT", "NN", "VVINF"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "dein Gnadenangesicht, indem mich unterdr\u00fccken", "tokens": ["dein", "Gna\u00b7den\u00b7an\u00b7ge\u00b7sicht", ",", "in\u00b7dem", "mich", "un\u00b7ter\u00b7dr\u00fc\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "viel tausent Schmerz und Angst! Herr, heile, heile mich,", "tokens": ["viel", "tau\u00b7sent", "Schmerz", "und", "Angst", "!", "Herr", ",", "hei\u00b7le", ",", "hei\u00b7le", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "KON", "NN", "$.", "NN", "$,", "ADJA", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "weil ich voll Schwachheit bin! O Arzt, erweise dich!", "tokens": ["weil", "ich", "voll", "Schwach\u00b7heit", "bin", "!", "O", "Arzt", ",", "er\u00b7wei\u00b7se", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "NN", "VAFIN", "$.", "NE", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Seele zittert mir. Ach Herr, ach Herr, wie lange?", "tokens": ["Die", "See\u00b7le", "zit\u00b7tert", "mir", ".", "Ach", "Herr", ",", "ach", "Herr", ",", "wie", "lan\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$.", "ITJ", "NN", "$,", "XY", "NN", "$,", "PWAV", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Das Mark verschwindet aus, das Rei\u00dfen macht mir bange,", "tokens": ["Das", "Mark", "ver\u00b7schwin\u00b7det", "aus", ",", "das", "Rei\u00b7\u00dfen", "macht", "mir", "ban\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "das meine Beine kreischt. Herr, wende dich einmal,", "tokens": ["das", "mei\u00b7ne", "Bei\u00b7ne", "kreischt", ".", "Herr", ",", "wen\u00b7de", "dich", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "VVFIN", "$.", "NN", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "und hilf mir, so du wilst, aus dieser Seelenqual!", "tokens": ["und", "hilf", "mir", ",", "so", "du", "wilst", ",", "aus", "die\u00b7ser", "See\u00b7len\u00b7qual", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "$,", "ADV", "PPER", "VMFIN", "$,", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wer wird dir, wenn du mich nun wirst get\u00f6tet haben,", "tokens": ["Wer", "wird", "dir", ",", "wenn", "du", "mich", "nun", "wirst", "ge\u00b7t\u00f6\u00b7tet", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "KOUS", "PPER", "PRF", "ADV", "VAFIN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "f\u00fcr deine H\u00fclf' und Treu' erlegen solche Gaben,", "tokens": ["f\u00fcr", "dei\u00b7ne", "H\u00fcl\u00b7f'", "und", "Treu'", "er\u00b7le\u00b7gen", "sol\u00b7che", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "VVPP", "PIAT", "NN", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "wie ich bi\u00dfher getan? wer wil dir danken doch", "tokens": ["wie", "ich", "bi\u00df\u00b7her", "ge\u00b7tan", "?", "wer", "wil", "dir", "dan\u00b7ken", "doch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "VVPP", "$.", "PWS", "VMFIN", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "und denken deiner Ehr' in jenem finstern Loch,", "tokens": ["und", "den\u00b7ken", "dei\u00b7ner", "Ehr'", "in", "je\u00b7nem", "fins\u00b7tern", "Loch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "in welches du mich wirfst? Das herzenswehe Seufzen", "tokens": ["in", "wel\u00b7ches", "du", "mich", "wirfst", "?", "Das", "her\u00b7zens\u00b7we\u00b7he", "Seuf\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "PRF", "VVFIN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "macht mich so la\u00df und matt, da\u00df ich auch kaum kan geufzen.", "tokens": ["macht", "mich", "so", "la\u00df", "und", "matt", ",", "da\u00df", "ich", "auch", "kaum", "kan", "geuf\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "VVIMP", "KON", "ADJD", "$,", "KOUS", "PPER", "ADV", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Der Angstschwei\u00df schwemmet mir durch manche ganze Nacht", "tokens": ["Der", "A\u00b7ngstschwei\u00df", "schwem\u00b7met", "mir", "durch", "man\u00b7che", "gan\u00b7ze", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "mein m\u00fcdes Lager aus. Das Qual der Thr\u00e4nen macht", "tokens": ["mein", "m\u00fc\u00b7des", "La\u00b7ger", "aus", ".", "Das", "Qual", "der", "Thr\u00e4\u00b7nen", "macht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$.", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "mein Bett' als eine Bach. Wo ist mein' erste Bl\u00fcte,", "tokens": ["mein", "Bett'", "als", "ei\u00b7ne", "Bach", ".", "Wo", "ist", "mein'", "ers\u00b7te", "Bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KOKOM", "ART", "NN", "$.", "PWAV", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "da ich so sch\u00f6ne war, das freudige Gem\u00fcte?", "tokens": ["da", "ich", "so", "sch\u00f6\u00b7ne", "war", ",", "das", "freu\u00b7di\u00b7ge", "Ge\u00b7m\u00fc\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "VAFIN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die Augen dunkeln mich, die ausgefleischte Haut", "tokens": ["Die", "Au\u00b7gen", "dun\u00b7keln", "mich", ",", "die", "aus\u00b7ge\u00b7fleischte", "Haut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.24": {"text": "wird schlaff und runzelt sich, da\u00df mir selbst f\u00fcr mir graut.", "tokens": ["wird", "schlaff", "und", "run\u00b7zelt", "sich", ",", "da\u00df", "mir", "selbst", "f\u00fcr", "mir", "graut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "VVFIN", "PRF", "$,", "KOUS", "PPER", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "Ich bin bei Leben tot. Man dr\u00e4ngt mich vorn und hinden.", "tokens": ["Ich", "bin", "bei", "Le\u00b7ben", "tot", ".", "Man", "dr\u00e4ngt", "mich", "vorn", "und", "hin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ADJD", "$.", "PIS", "VVFIN", "PPER", "ADV", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Hier \u00e4dert mich dein Grimm, den ich durch meine S\u00fcnden", "tokens": ["Hier", "\u00e4\u00b7dert", "mich", "dein", "Grimm", ",", "den", "ich", "durch", "mei\u00b7ne", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NE", "$,", "PRELS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "geh\u00e4ufet hab' auf mich, dort \u00e4ngstet mich ein Man", "tokens": ["ge\u00b7h\u00e4u\u00b7fet", "hab'", "auf", "mich", ",", "dort", "\u00e4ngs\u00b7tet", "mich", "ein", "Man"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "APPR", "PPER", "$,", "ADV", "VVFIN", "PPER", "ART", "PIS"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.28": {"text": "(ach w\u00e4r' es Einer nur!), dem ich kein Leid getan.", "tokens": ["(", "ach", "w\u00e4r'", "es", "Ei\u00b7ner", "nur", "!", ")", ",", "dem", "ich", "kein", "Leid", "ge\u00b7tan", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "XY", "VAFIN", "PPER", "PIS", "ADV", "$.", "$(", "$,", "PRELS", "PPER", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Weg, ihr verruchtes Volk, ihr \u00dcbelt\u00e4ter, weichet!", "tokens": ["Weg", ",", "ihr", "ver\u00b7ruch\u00b7tes", "Volk", ",", "ihr", "\u00dc\u00b7belt\u00b7\u00e4\u00b7ter", ",", "wei\u00b7chet", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Mein Jammerseufzen hat die blaue Burg erreichet", "tokens": ["Mein", "Jam\u00b7mer\u00b7seuf\u00b7zen", "hat", "die", "blau\u00b7e", "Burg", "er\u00b7rei\u00b7chet"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "und ihren Prinz bewegt zu m\u00fcssen gn\u00e4dig sein.", "tokens": ["und", "ih\u00b7ren", "Prinz", "be\u00b7wegt", "zu", "m\u00fcs\u00b7sen", "gn\u00e4\u00b7dig", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "PTKZU", "VMINF", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Das Wetter ist vorbei, nun hab' ich Sonnenschein;", "tokens": ["Das", "Wet\u00b7ter", "ist", "vor\u00b7bei", ",", "nun", "hab'", "ich", "Son\u00b7nen\u00b7schein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "ADV", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "mein Flehen ist erh\u00f6rt, ich habe Gott zum Freunde.", "tokens": ["mein", "Fle\u00b7hen", "ist", "er\u00b7h\u00f6rt", ",", "ich", "ha\u00b7be", "Gott", "zum", "Freun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,", "PPER", "VAFIN", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Wie ist euch nun zu Mut, ihr schlangenarge Feinde?", "tokens": ["Wie", "ist", "euch", "nun", "zu", "Mut", ",", "ihr", "schlan\u00b7gen\u00b7ar\u00b7ge", "Fein\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "APPR", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Erschrecken m\u00fcsset ihr f\u00fcr meinem Gott und mir", "tokens": ["Er\u00b7schre\u00b7cken", "m\u00fcs\u00b7set", "ihr", "f\u00fcr", "mei\u00b7nem", "Gott", "und", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "APPR", "PPOSAT", "NN", "KON", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "und pl\u00f6tzlich kehren umb mit Schanden f\u00fcr und f\u00fcr.", "tokens": ["und", "pl\u00f6tz\u00b7lich", "keh\u00b7ren", "umb", "mit", "Schan\u00b7den", "f\u00fcr", "und", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "APPR", "NN", "APPR", "KON", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ach schone, gro\u00dfer Herr, ach schone mich zu strafen,", "tokens": ["Ach", "scho\u00b7ne", ",", "gro\u00b7\u00dfer", "Herr", ",", "ach", "scho\u00b7ne", "mich", "zu", "stra\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "$,", "ADJA", "NN", "$,", "ITJ", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "wenn deine Huld und Gunst bei dir ist ganz entschlafen,", "tokens": ["wenn", "dei\u00b7ne", "Huld", "und", "Gunst", "bei", "dir", "ist", "ganz", "ent\u00b7schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "KON", "NN", "APPR", "PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und du f\u00fcr Zorne brennst! Herr, z\u00fcchtige mich nicht,", "tokens": ["und", "du", "f\u00fcr", "Zor\u00b7ne", "brennst", "!", "Herr", ",", "z\u00fcch\u00b7ti\u00b7ge", "mich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "VVFIN", "$.", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "wenn dir die Grimmesglut aus Mund und Augen bricht,", "tokens": ["wenn", "dir", "die", "Grim\u00b7mes\u00b7glut", "aus", "Mund", "und", "Au\u00b7gen", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "die niemand tragen kan! Umb so viel mehr la\u00df blicken", "tokens": ["die", "nie\u00b7mand", "tra\u00b7gen", "kan", "!", "Umb", "so", "viel", "mehr", "la\u00df", "bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVINF", "VMFIN", "$.", "KOUI", "ADV", "ADV", "PIAT", "NN", "VVINF"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "dein Gnadenangesicht, indem mich unterdr\u00fccken", "tokens": ["dein", "Gna\u00b7den\u00b7an\u00b7ge\u00b7sicht", ",", "in\u00b7dem", "mich", "un\u00b7ter\u00b7dr\u00fc\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "viel tausent Schmerz und Angst! Herr, heile, heile mich,", "tokens": ["viel", "tau\u00b7sent", "Schmerz", "und", "Angst", "!", "Herr", ",", "hei\u00b7le", ",", "hei\u00b7le", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "KON", "NN", "$.", "NN", "$,", "ADJA", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "weil ich voll Schwachheit bin! O Arzt, erweise dich!", "tokens": ["weil", "ich", "voll", "Schwach\u00b7heit", "bin", "!", "O", "Arzt", ",", "er\u00b7wei\u00b7se", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "NN", "VAFIN", "$.", "NE", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Seele zittert mir. Ach Herr, ach Herr, wie lange?", "tokens": ["Die", "See\u00b7le", "zit\u00b7tert", "mir", ".", "Ach", "Herr", ",", "ach", "Herr", ",", "wie", "lan\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$.", "ITJ", "NN", "$,", "XY", "NN", "$,", "PWAV", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Das Mark verschwindet aus, das Rei\u00dfen macht mir bange,", "tokens": ["Das", "Mark", "ver\u00b7schwin\u00b7det", "aus", ",", "das", "Rei\u00b7\u00dfen", "macht", "mir", "ban\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "das meine Beine kreischt. Herr, wende dich einmal,", "tokens": ["das", "mei\u00b7ne", "Bei\u00b7ne", "kreischt", ".", "Herr", ",", "wen\u00b7de", "dich", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "VVFIN", "$.", "NN", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "und hilf mir, so du