{"textgrid.poem.66761": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Die Berufung", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es rollt das Rad der rastlosen D\u00e4monen", "tokens": ["Es", "rollt", "das", "Rad", "der", "rast\u00b7lo\u00b7sen", "D\u00e4\u00b7mo\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geleise hin, die Menschenhand ihm f\u00fcgt,", "tokens": ["Ge\u00b7lei\u00b7se", "hin", ",", "die", "Men\u00b7schen\u00b7hand", "ihm", "f\u00fcgt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und fl\u00fcgelsausend wagt zu Wolkenzonen", "tokens": ["Und", "fl\u00fc\u00b7gel\u00b7sau\u00b7send", "wagt", "zu", "Wol\u00b7ken\u00b7zo\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sich z\u00e4he Kraft, die niemals sich gen\u00fcgt.", "tokens": ["Sich", "z\u00e4\u00b7he", "Kraft", ",", "die", "nie\u00b7mals", "sich", "ge\u00b7n\u00fcgt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "NN", "$,", "PRELS", "ADV", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Erobrerwille wirbt um k\u00fchne Kronen,", "tokens": ["E\u00b7ro\u00b7brer\u00b7wil\u00b7le", "wirbt", "um", "k\u00fch\u00b7ne", "Kro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Triumph der Technik ist kein Traum, der tr\u00fcgt \u2013", "tokens": ["Tri\u00b7umph", "der", "Tech\u00b7nik", "ist", "kein", "Traum", ",", "der", "tr\u00fcgt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VAFIN", "PIAT", "NN", "$,", "PRELS", "VVFIN", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "So mu\u00df auch deine Sehnsucht sich belohnen,", "tokens": ["So", "mu\u00df", "auch", "dei\u00b7ne", "Sehn\u00b7sucht", "sich", "be\u00b7loh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PPOSAT", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du Mensch, der tief der Seele Gr\u00fcnde pfl\u00fcgt.", "tokens": ["Du", "Mensch", ",", "der", "tief", "der", "See\u00b7le", "Gr\u00fcn\u00b7de", "pfl\u00fcgt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PRELS", "ADJD", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Zieh deine Furchen, Dichter, unerschrocken", "tokens": ["Zieh", "dei\u00b7ne", "Fur\u00b7chen", ",", "Dich\u00b7ter", ",", "un\u00b7er\u00b7schro\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "NN", "$,", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durchs Land der innern Unerme\u00dflichkeit,", "tokens": ["Durchs", "Land", "der", "in\u00b7nern", "Un\u00b7er\u00b7me\u00df\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "