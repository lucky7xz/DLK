", ",", "den", "Rhein", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ART", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wolan, die gr\u00fcnen Wellen bringen", "tokens": ["Wo\u00b7lan", ",", "die", "gr\u00fc\u00b7nen", "Wel\u00b7len", "brin\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mir st\u00fcndlich holde Gr\u00fc\u00dfe zu.", "tokens": ["Mir", "st\u00fcnd\u00b7lich", "hol\u00b7de", "Gr\u00fc\u00b7\u00dfe", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und Wellenschlag und Lieder singen", "tokens": ["Und", "Wel\u00b7len\u00b7schlag", "und", "Lie\u00b7der", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mein Herz in die gew\u00fcnschte Ruh.", "tokens": ["Mein", "Herz", "in", "die", "ge\u00b7w\u00fcnschte", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.11": {"line.1": {"text": "Und wie dem Wandersmann im Dunkeln,", "tokens": ["Und", "wie", "dem", "Wan\u00b7ders\u00b7mann", "im", "Dun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In einer langen Winternacht,", "tokens": ["In", "ei\u00b7ner", "lan\u00b7gen", "Win\u00b7ter\u00b7nacht", ","], "token_info": ["w