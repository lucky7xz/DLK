{"textgrid.poem.62381": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "An Heinrich Jung, genannt Stilling, zu dessen sieben und siebzigsten Geburtstage", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Herbst hat seinen Thron genommen", "tokens": ["Der", "Herbst", "hat", "sei\u00b7nen", "Thron", "ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die liebe Blumenzeit verschwand,", "tokens": ["Die", "lie\u00b7be", "Blu\u00b7men\u00b7zeit", "ver\u00b7schwand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auch du bist wieder heimgekommen", "tokens": ["Auch", "du", "bist", "wie\u00b7der", "heim\u00b7ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VAFIN", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von Badens mildem Quellenrand.", "tokens": ["Von", "Ba\u00b7dens", "mil\u00b7dem", "Quel\u00b7len\u00b7rand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So kehret jedes von der Reise", "tokens": ["So", "keh\u00b7ret", "je\u00b7des", "von", "der", "Rei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und zieht in seine Heimat ein,", "tokens": ["Und", "zieht", "in", "sei\u00b7ne", "Hei\u00b7mat", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und richtet sich auf seine Weise", "tokens": ["Und", "rich\u00b7tet", "sich", "auf", "sei\u00b7ne", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zum langen Winter traulich ein.", "tokens": ["Zum", "lan\u00b7gen", "Win\u00b7ter", "trau\u00b7lich", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nur ich mu\u00df wieder dich ergreifen,", "tokens": ["Nur", "ich", "mu\u00df", "wie\u00b7der", "dich", "er\u00b7grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du vielgebrauchter Wanderstab,", "tokens": ["Du", "viel\u00b7ge\u00b7brauch\u00b7ter", "Wan\u00b7der\u00b7stab", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und mu\u00df mit meiner Liebe schweifen", "tokens": ["Und", "mu\u00df", "mit", "mei\u00b7ner", "Lie\u00b7be", "schwei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In fernes Land, den Rhein hinab.", "tokens": ["In", "fer\u00b7nes", "Land", ",", "den", "Rhein", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ART", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wolan, die gr\u00fcnen Wellen bringen", "tokens": ["Wo\u00b7lan", ",", "die", "gr\u00fc\u00b7nen", "Wel\u00b7len", "brin\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mir st\u00fcndlich holde Gr\u00fc\u00dfe zu.", "tokens": ["Mir", "st\u00fcnd\u00b7lich", "hol\u00b7de", "Gr\u00fc\u00b7\u00dfe", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und Wellenschlag und Lieder singen", "tokens": ["Und", "Wel\u00b7len\u00b7schlag", "und", "Lie\u00b7der", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mein Herz in die gew\u00fcnschte Ruh.", "tokens": ["Mein", "Herz", "in", "die", "ge\u00b7w\u00fcnschte", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Und wie dem Wandersmann im Dunkeln,", "tokens": ["Und", "wie", "dem", "Wan\u00b7ders\u00b7mann", "im", "Dun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In einer langen Winternacht,", "tokens": ["In", "ei\u00b7ner", "lan\u00b7gen", "Win\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Sterne Gottes tr\u00f6stlich funkeln", "tokens": ["Die", "Ster\u00b7ne", "Got\u00b7tes", "tr\u00f6st\u00b7lich", "fun\u00b7keln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In ihrer ew'gen Liebespracht,", "tokens": ["In", "ih\u00b7rer", "ew'\u00b7gen", "Lie\u00b7be\u00b7spracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gibt ", "tokens": ["Gibt"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Zu guter Letzt zum Abschied mit,", "tokens": ["Zu", "gu\u00b7ter", "Letzt", "zum", "Ab\u00b7schied", "mit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und leuchtet mir auf meinen Wegen,", "tokens": ["Und", "leuch\u00b7tet", "mir", "auf", "mei\u00b7nen", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Bei manchem schwanken Steg und Schritt.", "tokens": ["Bei", "man\u00b7chem", "schwan\u00b7ken", "Steg", "und", "Schritt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Fahr' wohl, o Haus der alten Treue,", "tokens": ["Fahr'", "wohl", ",", "o", "Haus", "der", "al\u00b7ten", "Treu\u00b7e", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "FM", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Fahr' wohl, du gastlich offnes Thor,", "tokens": ["Fahr'", "wohl", ",", "du", "gast\u00b7lich", "off\u00b7nes", "Thor", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "PPER", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr Lieben, t\u00e4glich schaut aufs Neue,", "tokens": ["Ihr", "Lie\u00b7ben", ",", "t\u00e4g\u00b7lich", "schaut", "aufs", "Neu\u00b7e", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJD", "ADJD", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu euern Bergen schaut empor!", "tokens": ["Zu", "eu\u00b7ern", "Ber\u00b7gen", "schaut", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Berge hab' ich oft durchzogen,", "tokens": ["Die", "Ber\u00b7ge", "hab'", "ich", "oft", "durch\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn ich zu sp\u00e4t am Abend kam,", "tokens": ["Wenn", "ich", "zu", "sp\u00e4t", "am", "A\u00b7bend", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKA", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dort ist so mancher Schmerz entflogen,", "tokens": ["Dort", "ist", "so", "man\u00b7cher", "Schmerz", "ent\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Geheilt so mancher bittre Gram.", "tokens": ["Ge\u00b7heilt", "so", "man\u00b7cher", "bitt\u00b7re", "Gram", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich kann es nimmermehr vergessen,", "tokens": ["Ich", "kann", "es", "nim\u00b7mer\u00b7mehr", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie alles hier so freundlich war,", "tokens": ["Wie", "al\u00b7les", "hier", "so", "freund\u00b7lich", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie ich an diesem Tisch gesessen", "tokens": ["Wie", "ich", "an", "die\u00b7sem", "Tisch", "ge\u00b7ses\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "PDAT", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So manchen Tag und manches Jahr,", "tokens": ["So", "man\u00b7chen", "Tag", "und", "man\u00b7ches", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie Vater ", "tokens": ["Wie", "Va\u00b7ter"], "token_info": ["word", "word"], "pos": ["PWAV", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Im fr\u00f6hlich christlichen Gespr\u00e4ch,", "tokens": ["Im", "fr\u00f6h\u00b7lich", "christ\u00b7li\u00b7chen", "Ge\u00b7spr\u00e4ch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und wie die T\u00f6chter uns kredenzten,", "tokens": ["Und", "wie", "die", "T\u00f6ch\u00b7ter", "uns", "kre\u00b7denz\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Als ob das Brod ", "tokens": ["Als", "ob", "das", "Brod"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "O du, von reinen Himmelsbl\u00fcten,", "tokens": ["O", "du", ",", "von", "rei\u00b7nen", "Him\u00b7mels\u00b7bl\u00fc\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von ew'gen Kr\u00e4nzen sch\u00f6n umlaubt,", "tokens": ["Von", "ew'\u00b7gen", "Kr\u00e4n\u00b7zen", "sch\u00f6n", "um\u00b7laubt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem sechs und siebzig Sonnen gl\u00fchten,", "tokens": ["Dem", "sechs", "und", "sieb\u00b7zig", "Son\u00b7nen", "gl\u00fch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "KON", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du theures, vielgepr\u00fcftes Haupt,", "tokens": ["Du", "theu\u00b7res", ",", "viel\u00b7ge\u00b7pr\u00fcf\u00b7tes", "Haupt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du darfst noch lange dich nicht neigen,", "tokens": ["Du", "darfst", "noch", "lan\u00b7ge", "dich", "nicht", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den Aehren gleich von Segen schwer,", "tokens": ["Den", "A\u00b7eh\u00b7ren", "gleich", "von", "Se\u00b7gen", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Mu\u00dft vielen noch die Wege zeigen", "tokens": ["Mu\u00dft", "vie\u00b7len", "noch", "die", "We\u00b7ge", "zei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zum Throne Gottes stark und hehr.", "tokens": ["Zum", "Thro\u00b7ne", "Got\u00b7tes", "stark", "und", "hehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "O schau' die j\u00fcngste Stillingsblume,", "tokens": ["O", "schau'", "die", "j\u00fcngs\u00b7te", "Stil\u00b7lings\u00b7blu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die deiner Tochter Kind gebar,", "tokens": ["Die", "dei\u00b7ner", "Toch\u00b7ter", "Kind", "ge\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schau' dr\u00fcben in dem Heiligthume", "tokens": ["Schau'", "dr\u00fc\u00b7ben", "in", "dem", "Hei\u00b7lig\u00b7thu\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die F\u00fchrerin der bl\u00fch'nden Schaar,", "tokens": ["Die", "F\u00fch\u00b7re\u00b7rin", "der", "bl\u00fch'n\u00b7den", "Schaar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sieh neben dir die Gattin weilen,", "tokens": ["Sieh", "ne\u00b7ben", "dir", "die", "Gat\u00b7tin", "wei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die dich umschlang dem Epheu gleich;", "tokens": ["Die", "dich", "um\u00b7schlang", "dem", "E\u00b7pheu", "gleich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So magst du deine Blicke theilen", "tokens": ["So", "magst", "du", "dei\u00b7ne", "Bli\u00b7cke", "thei\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mit jenem und mit diesem Reich.", "tokens": ["Mit", "je\u00b7nem", "und", "mit", "die\u00b7sem", "Reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "KON", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Fahr' wohl! zwar fernhin mu\u00df ich ziehen,", "tokens": ["Fahr'", "wohl", "!", "zwar", "fern\u00b7hin", "mu\u00df", "ich", "zie\u00b7hen", ","], "tok