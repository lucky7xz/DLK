{"textgrid.poem.51064": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Seht da den Vogel mit gerupften Schwingen!", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seht da den Vogel mit gerupften Schwingen!", "tokens": ["Seht", "da", "den", "Vo\u00b7gel", "mit", "ge\u00b7rupf\u00b7ten", "Schwin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Halb flattert er, halb l\u00e4uft er hin zum Neste,", "tokens": ["Halb", "flat\u00b7tert", "er", ",", "halb", "l\u00e4uft", "er", "hin", "zum", "Nes\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "ADJD", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sich einzubaun in weicher Arme Veste,", "tokens": ["Sich", "ein\u00b7zu\u00b7baun", "in", "wei\u00b7cher", "Ar\u00b7me", "Ves\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wohin kein rauhes L\u00fcftchen mehr soll dringen!", "tokens": ["Wo\u00b7hin", "kein", "rau\u00b7hes", "L\u00fcft\u00b7chen", "mehr", "soll", "drin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "ADJA", "NN", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch war er frech und mochte Ruhm erringen;", "tokens": ["Doch", "war", "er", "frech", "und", "moch\u00b7te", "Ruhm", "er\u00b7rin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "KON", "VVFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sein Reisig gr\u00fcnt\u00ab und bl\u00fchte schon aufs beste,", "tokens": ["Sein", "Rei\u00b7sig", "gr\u00fcnt", "\u00ab", "und", "bl\u00fch\u00b7te", "schon", "aufs", "bes\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$(", "KON", "VVFIN", "ADV", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In seinen Schatten lud er stolz die G\u00e4ste", "tokens": ["In", "sei\u00b7nen", "Schat\u00b7ten", "lud", "er", "stolz", "die", "G\u00e4s\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und war so recht ein Thema zum Besingen.", "tokens": ["Und", "war", "so", "recht", "ein", "The\u00b7ma", "zum", "Be\u00b7sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nur als den Zweig dem freien Feld er raubte,", "tokens": ["Nur", "als", "den", "Zweig", "dem", "frei\u00b7en", "Feld", "er", "raub\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus Luft und Licht, darin er aufgeschossen,", "tokens": ["Aus", "Luft", "und", "Licht", ",", "da\u00b7rin", "er", "auf\u00b7ge\u00b7schos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "PAV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und sachte mit sich zu salvieren glaubte:", "tokens": ["Und", "sach\u00b7te", "mit", "sich", "zu", "sal\u00b7vie\u00b7ren", "glaub\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PRF", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Da war der Traum bald wie ein Schaum zerflossen;", "tokens": ["Da", "war", "der", "Traum", "bald", "wie", "ein", "Schaum", "zer\u00b7flos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "KOKOM", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Reis verdorrt', das schon so nett belaubte \u2013", "tokens": ["Das", "Reis", "ver\u00b7dorrt'", ",", "das", "schon", "so", "nett", "be\u00b7laub\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "ADV", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nun zieht er ab, unfertig und verdrossen.", "tokens": ["Nun", "zieht", "er", "ab", ",", "un\u00b7fer\u00b7tig", "und", "ver\u00b7dros\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "ADJD", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Seht da den Vogel mit gerupften Schwingen!", "tokens": ["Seht", "da", "den", "Vo\u00b7gel", "mit"