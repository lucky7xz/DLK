{"dta.poem.20611": {"metadata": {"author": {"name": "Hebel, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Der Jenner .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1803", "urn": "urn:nbn:de:kobv:b4-200905192133", "language": ["de:0.99"], "booktitle": "[Hebel, Johann Peter]: Allemannische Gedichte. Karlsruhe, 1803."}, "poem": {"stanza.1": {"line.1": {"text": "Im Aetti sezt der Oehldampf zu.               ", "tokens": ["Im", "A\u00b7et\u00b7ti", "sezt", "der", "O\u00b7ehl\u00b7dampf", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Mer ch\u00f6nnte \u2019s Aempeli use thue,", "tokens": ["Mer", "ch\u00f6nn\u00b7te", "'s", "A\u00b7em\u00b7pe\u00b7li", "u\u00b7se", "thue", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "FM", "FM", "FM", "FM", "FM", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "und d\u2019L\u00e4den uf. Der Morge-Schi\u2019", "tokens": ["und", "d'\u00b7L\u00e4\u00b7den", "uf", ".", "Der", "Mor\u00b7ge\u00b7Schi'"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "NE", "PTKVZ", "$.", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "blickt scho zum runde Nastloch i. \u2014", "tokens": ["blickt", "scho", "zum", "run\u00b7de", "Nast\u00b7loch", "i."], "token_info": ["word", "word", "word", "word", "word", "abbreviation", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "ADJA", "NN", "NE", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "O lueget doch, wie chalt und roth", "tokens": ["O", "lue\u00b7get", "doch", ",", "wie", "chalt", "und", "roth"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "$,", "PWAV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "der Jenner uf de Berge stoht.", "tokens": ["der", "Jen\u00b7ner", "uf", "de", "Ber\u00b7ge", "stoht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er seit: ", "tokens": ["Er", "seit", ":"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "APPR", "$."], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "\u201eder Stern am Himmel lacht mi a!", "tokens": ["\u201e", "der", "Stern", "am", "Him\u00b7mel", "lacht", "mi", "a", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "APPRART", "NN", "VVFIN", "FM", "FM", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201eer glitzeret vor Lust und Freud,", "tokens": ["\u201e", "er", "glit\u00b7ze\u00b7ret", "vor", "Lust", "und", "Freud", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201eund mue\u00df er furt, sen ischs em Leid;", "tokens": ["\u201e", "und", "mu\u00b7e\u00df", "er", "furt", ",", "sen", "ischs", "em", "Leid", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVFIN", "PPER", "PTKVZ", "$,", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "\u201eer luegt mi a, und cha \u2019s nit lo,", "tokens": ["\u201e", "er", "luegt", "mi", "a", ",", "und", "cha", "'s", "nit", "lo", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "FM", "FM", "$,", "KON", "NE", "PPER", "PTKNEG", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "\u201eund w\u00fcrd byzite wieder cho.", "tokens": ["\u201e", "und", "w\u00fcrd", "by\u00b7zi\u00b7te", "wie\u00b7der", "cho", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VAFIN", "VVFIN", "ADV", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "\u201eund unter mer in Berg und Thal,", "tokens": ["\u201e", "und", "un\u00b7ter", "mer", "in", "Berg", "und", "Thal", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "APPR", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201ewie flimmerets nit \u00fcberal!", "tokens": ["\u201e", "wie", "flim\u00b7me\u00b7rets", "nit", "\u00fc\u00b7be\u00b7ral", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VVFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201ean allen Ende Schnee und Schnee;", "tokens": ["\u201e", "an", "al\u00b7len", "En\u00b7de", "Schnee", "und", "Schnee", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PIAT", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201e\u2019s isch alles mir zu Ehre gscheh,", "tokens": ["\u201e", "'s", "isch", "al\u00b7les", "mir", "zu", "Eh\u00b7re", "gs\u00b7cheh", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADJD", "PIS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "\u201eund woni gang im wite Feld,", "tokens": ["\u201e", "und", "wo\u00b7ni", "gang", "im", "wi\u00b7te", "Feld", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PWAV", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u201esin Stro\u00dfe bahnt, und Brucke gstellt.\u201c", "tokens": ["\u201e", "sin", "Stro\u00b7\u00dfe", "bahnt", ",", "und", "Bru\u00b7cke", "gs\u00b7tellt", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "VVFIN", "$,", "KON", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Er seit: ", "tokens": ["Er", "seit", ":"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "APPR", "$."], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "\u201ei ha ne luftig Tsch\u00f6pli a,", "tokens": ["\u201e", "i", "ha", "ne", "luf\u00b7tig", "Tsch\u00f6p\u00b7li", "a", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "FM", "FM", "FM", "FM", "FM", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201eund rothi Backe bis ans Ohr,", "tokens": ["\u201e", "und", "ro\u00b7thi", "Ba\u00b7cke", "bis", "ans", "Ohr", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "ADJA", "NN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201ee heiter Aug und Duft im Hoor,", "tokens": ["\u201e", "e", "hei\u00b7ter", "Aug", "und", "Duft", "im", "Hoor", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "ADJA", "NN", "KON", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u201eke Wintergfrist, ke Gliederweh,", "tokens": ["\u201e", "ke", "Win\u00b7terg\u00b7frist", ",", "ke", "Glie\u00b7der\u00b7weh", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u201eund woni gang, se chracht der Schnee.\u201c", "tokens": ["\u201e", "und", "wo\u00b7ni", "gang", ",", "se", "chracht", "der", "Schnee", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "PWAV", "ADJD", "$,", "FM", "FM", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Er seit: ", "tokens": ["Er", "seit", ":"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "APPR", "$."], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "\u201elueg, wieni \u00fcberzuckere cha!", "tokens": ["\u201e", "lu\u00b7eg", ",", "wie\u00b7ni", "\u00fc\u00b7ber\u00b7zu\u00b7cke\u00b7re", "cha", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "\u201ej chuuch, und an de H\u00fcrste hangts,", "tokens": ["\u201e", "j", "chuuch", ",", "und", "an", "de", "H\u00fcrs\u00b7te", "hangts", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM.la", "FM.la", "$,", "KON", "APPR", "NE", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201eund an de zarte Birche schwankts.", "tokens": ["\u201e", "und", "an", "de", "zar\u00b7te", "Bir\u00b7che", "schwankts", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "APPR", "NE", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u201eder Zuckerbeck mit gschickter Hand,", "tokens": ["\u201e", "der", "Zu\u00b7cker\u00b7beck", "mit", "gschick\u00b7ter", "Hand", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u201emit Geld und Gut w\u00e4r\u2019s nit im Stand.", "tokens": ["\u201e", "mit", "Geld", "und", "Gut", "w\u00e4r's", "nit", "im", "Stand", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "KON", "ADJD", "VAFIN", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "\u201ejez lueg au dini Schiben a,", "tokens": ["\u201e", "jez", "lu\u00b7eg", "au", "di\u00b7ni", "Schi\u00b7ben", "a", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "\u201eund wieni Helgli chritzle cha!", "tokens": ["\u201e", "und", "wie\u00b7ni", "Helg\u00b7li", "chritz\u00b7le", "cha", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "FM", "FM", "FM", "FM", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201edo hesch e Bl\u00fcemli, wenns der gfallt,", "tokens": ["\u201e", "do", "hesch", "e", "Bl\u00fcem\u00b7li", ",", "wenns", "der", "gfallt", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "NE", "NE", "$,", "KOUS", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201edo hesch e ganze Tannewald!", "tokens": ["\u201e", "do", "hesch", "e", "gan\u00b7ze", "Tan\u00b7ne\u00b7wald", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u201eder Fr\u00fcehlig ch\u00f6nnts nit halber so,", "tokens": ["\u201e", "der", "Fr\u00fceh\u00b7lig", "ch\u00f6nnts", "nit", "hal\u00b7ber", "so", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "NE", "PTKNEG", "APPO", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u201es isch mit der Farb nit alles tho.\u201c", "tokens": ["\u201e", "s", "isch", "mit", "der", "Farb", "nit", "al\u00b7les", "tho", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "ADJD", "APPR", "ART", "NN", "PTKNEG", "PIS", "NE", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Er seit: \u201eJ bi ne st ", "tokens": ["Er", "seit", ":", "\u201e", "J", "bi", "ne", "st"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "$.", "$(", "FM.fr", "FM.fr", "FM.fr", "FM.fr"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "\u201eund zwing mi n\u00e4umer, wenn er cha!", "tokens": ["\u201e", "und", "zwing", "mi", "n\u00e4u\u00b7mer", ",", "wenn", "er", "cha", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVFIN", "NE", "ADV", "$,", "KOUS", "PPER", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201eder Forster gstablet uf der Jacht,", "tokens": ["\u201e", "der", "Fors\u00b7ter", "gstab\u00b7let", "uf", "der", "Jacht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201eder Bru\u00f1trog springt, der Eichbaum chracht.", "tokens": ["\u201e", "der", "Bru\u00f1trog", "springt", ",", "der", "Eich\u00b7baum", "chracht", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "\u201ed\u2019 Frau Sunne mittem Gsichtli rund,", "tokens": ["\u201e", "d'", "Frau", "Sun\u00b7ne", "mit\u00b7tem", "Gsicht\u00b7li", "rund", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "NN", "NE", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "\u201ehet \u2019s Herz nit, a\u00df sie f\u00fcre chunnt.\u201c \u2014", "tokens": ["\u201e", "het", "'s", "Herz", "nit", ",", "a\u00df", "sie", "f\u00fc\u00b7re", "chunnt", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "VAFIN", "PPER", "NN", "PTKNEG", "$,", "VVFIN", "PPER", "VVFIN", "ADJD", "$.", "$(", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "\u2019s isch wohr, me wei\u00df nit, was sie tribt,", "tokens": ["'s", "isch", "wohr", ",", "me", "wei\u00df", "nit", ",", "was", "sie", "tribt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJD", "$,", "VVFIN", "VVFIN", "PTKNEG", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "und wo sie alli Morge blibt.", "tokens": ["und", "wo", "sie", "al\u00b7li", "Mor\u00b7ge", "blibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie l\u00e4nger Nacht, wie sp\u00f6ter Tag,", "tokens": ["Wie", "l\u00e4n\u00b7ger", "Nacht", ",", "wie", "sp\u00f6\u00b7ter", "Tag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "NN", "$,", "PWAV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "wie besser, a\u00df sie schlofe mag,", "tokens": ["wie", "bes\u00b7ser", ",", "a\u00df", "sie", "schlo\u00b7fe", "mag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "VVFIN", "PPER", "VVFIN", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "und blieb es bis um Zehni Nacht,", "tokens": ["und", "blieb", "es", "bis", "um", "Zeh\u00b7ni", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "se ch\u00e4m sie erst, wenns Oelfi schlacht.", "tokens": ["se", "ch\u00e4m", "sie", "erst", ",", "wenns", "O\u00b7el\u00b7fi", "schlacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM", "FM", "PPER", "ADV", "$,", "KOUS", "NE", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Nei het sie\u2019s gh\u00f6rt? D\u00f6rt chunnt sie io!", "tokens": ["Nei", "het", "sie's", "gh\u00f6rt", "?", "D\u00f6rt", "chunnt", "sie", "i\u00b7o", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIS", "VVFIN", "$.", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Me meint, \u2019s brenn alles liechterloh! \u2014", "tokens": ["Me", "meint", ",", "'s", "brenn", "al\u00b7les", "liech\u00b7ter\u00b7loh", "!"], "token_info": ["word", "word", "punct", "wo