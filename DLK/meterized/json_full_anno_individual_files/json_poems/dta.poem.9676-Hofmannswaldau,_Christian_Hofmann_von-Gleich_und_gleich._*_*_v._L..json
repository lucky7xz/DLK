{"dta.poem.9676": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Gleich und gleich.  \n * * v. L.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Am besten ists/ da\u00df gleiche gleich sich paaren/", "tokens": ["Am", "bes\u00b7ten", "ists", "/", "da\u00df", "glei\u00b7che", "gleich", "sich", "paa\u00b7ren", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VAFIN", "$(", "KOUS", "ADJA", "ADV", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die alten nicht mit den/ so jung an jahren;", "tokens": ["Die", "al\u00b7ten", "nicht", "mit", "den", "/", "so", "jung", "an", "jah\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PTKNEG", "APPR", "ART", "$(", "ADV", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die jungen nicht mit dem/ so grau von haaren;", "tokens": ["Die", "jun\u00b7gen", "nicht", "mit", "dem", "/", "so", "grau", "von", "haa\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PTKNEG", "APPR", "ART", "$(", "ADV", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die runtzlich sind/ mit sch\u00f6nen und mit klaren/", "tokens": ["Die", "runtz\u00b7lich", "sind", "/", "mit", "sch\u00f6\u00b7nen", "und", "mit", "kla\u00b7ren", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "$(", "APPR", "ADJA", "KON", "APPR", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die milden nicht mit denen/ so da sparen/", "tokens": ["Die", "mil\u00b7den", "nicht", "mit", "de\u00b7nen", "/", "so", "da", "spa\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PTKNEG", "APPR", "PRELS", "$(", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die arm/ mit dem so reich an geld und waaren.", "tokens": ["Die", "arm", "/", "mit", "dem", "so", "reich", "an", "geld", "und", "waa\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$(", "APPR", "ART", "ADV", "ADJD", "APPR", "NN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Denn thun sie es/ so m\u00fcssen sie erfahren/", "tokens": ["Denn", "thun", "sie", "es", "/", "so", "m\u00fcs\u00b7sen", "sie", "er\u00b7fah\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "$(", "ADV", "VMFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ein braut-bett werde leicht zu schwartzen bahren.", "tokens": ["Ein", "braut\u00b7bett", "wer\u00b7de", "leicht", "zu", "schwart\u00b7zen", "bah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "ADJD", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}