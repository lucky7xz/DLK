{"dta.poem.4399": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Tulpe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1727", "urn": "urn:nbn:de:kobv:b4-200905198599", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Qvell\u2019 aller Sch\u00f6nheit! ew\u2019ge Liebe,", "tokens": ["Qvell'", "al\u00b7ler", "Sch\u00f6n\u00b7heit", "!", "ew'\u00b7ge", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$.", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vermehr\u2019 in mir die F\u00e4higkeit,", "tokens": ["Ver\u00b7mehr'", "in", "mir", "die", "F\u00e4\u00b7hig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ich zur holden Fr\u00fchlings-Zeit", "tokens": ["Da\u00df", "ich", "zur", "hol\u00b7den", "Fr\u00fch\u00b7lings\u00b7Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit einem angeflamm\u2019ten Triebe", "tokens": ["Mit", "ei\u00b7nem", "an\u00b7ge\u00b7flam\u00b7m'\u00b7ten", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Der Tulpen Glanz, wie er so wundersch\u00f6n,", "tokens": ["Der", "Tul\u00b7pen", "Glanz", ",", "wie", "er", "so", "wun\u00b7der\u00b7sch\u00f6n", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PWAV", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Zu Deinen Ehren m\u00f6g\u2019 in tausend Freuden sehn!", "tokens": ["Zu", "Dei\u00b7nen", "Eh\u00b7ren", "m\u00f6g'", "in", "tau\u00b7send", "Freu\u00b7den", "sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "APPR", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}