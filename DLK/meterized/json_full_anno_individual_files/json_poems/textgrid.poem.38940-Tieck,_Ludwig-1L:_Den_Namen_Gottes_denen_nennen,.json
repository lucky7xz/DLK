{"textgrid.poem.38940": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Den Namen Gottes denen nennen,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den Namen Gottes denen nennen,", "tokens": ["Den", "Na\u00b7men", "Got\u00b7tes", "de\u00b7nen", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PDS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die ihn nicht mit dem Herzen kennen,", "tokens": ["Die", "ihn", "nicht", "mit", "dem", "Her\u00b7zen", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ist Missethat.", "tokens": ["Ist", "Mis\u00b7se\u00b7that", "."], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Es h\u00e4ngen um mich Geisterch\u00f6re,", "tokens": ["Es", "h\u00e4n\u00b7gen", "um", "mich", "Geis\u00b7ter\u00b7ch\u00f6\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und sprechen laut, da\u00df ich es h\u00f6re; \u2013", "tokens": ["Und", "spre\u00b7chen", "laut", ",", "da\u00df", "ich", "es", "h\u00f6\u00b7re", ";", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie halten Rath.", "tokens": ["Sie", "hal\u00b7ten", "Rath", "."], "token_in