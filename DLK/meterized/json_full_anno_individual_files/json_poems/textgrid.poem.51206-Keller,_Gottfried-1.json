, "Leich\u00b7tes\u00b7te", ",", "das", "Lied", ",", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ART", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Das, selbst sich bauend, aus der Kehle bricht;", "tokens": ["Das", ",", "selbst", "sich", "bau\u00b7end", ",", "aus", "der", "Keh\u00b7le", "bricht", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "ADV", "PRF", "VVPP", "$,", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Du aber legst dein unbeholfen Leben", "tokens": ["Du", "a\u00b7ber", "legst", "dein", "un\u00b7be\u00b7hol\u00b7fen", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wie einen Stein ihm auf den Weg zum Licht!\u00ab", "tokens": ["Wie", "ei\u00b7nen", "Stein", "ihm", "auf", "den", "Weg", "zum", "Licht", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "APPR", "ART", "NN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Sprach so der Wind? O nein, so sprach der Schmerz,", "tokens": ["Sprach", "so", "der", "Wind", "?", "O", "nein", ",", "so", "sprach", "der", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$.", "NE", "PTKANT", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der mir wie Ketten hing ums dunkle Herz!", "tokens": ["Der", "mir", "wie", "Ket\u00b7ten", "hing", "ums", "dunk\u00b7le", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "KOKOM", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein fremder K\u00f6rper ohne Form und Schall,", "tokens": ["Ein", "frem\u00b7der", "K\u00f6r\u00b7per", "oh\u00b7ne", "Form", "und", "Schall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So, deuchte mir, lag ich im regen All.", "tokens": ["So", ",", "deuch\u00b7te", "mir", ",", "lag", "ich", "im", "re\u00b7gen", "All", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Und Luft und Tannen, Berge, Moos und Sterne,", "tokens": ["Und", "Luft", "und", "Tan\u00b7nen", ",", "Ber\u00b7ge", ",", "Moos", "und", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sie schlangen l\u00e4chelnd ihren weiten Kranz;", "tokens": ["Sie", "schlan\u00b7gen", "l\u00e4\u00b7chelnd", "ih\u00b7ren", "wei\u00b7ten", "Kranz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wie an der Insel sich das Meer, das ferne,", "tokens": ["Wie", "an", "der", "In\u00b7sel", "sich", "das", "Meer", ",", "das", "fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "PRF", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Brach sich an mir ihr friedlich milder Glanz.", "tokens": ["Brach", "sich", "an", "mir", "ihr", "fried\u00b7lich", "mil\u00b7der", "Glanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPER", "PPER", "ADJD", "ADJA", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}}}}