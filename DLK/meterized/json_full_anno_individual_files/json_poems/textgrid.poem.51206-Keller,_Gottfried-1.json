{"textgrid.poem.51206": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vom Lager stand ich mit dem Fr\u00fchlicht auf", "tokens": ["Vom", "La\u00b7ger", "stand", "ich", "mit", "dem", "Fr\u00fch\u00b7licht", "auf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und nahm hinaus ins Freie meinen Lauf,", "tokens": ["Und", "nahm", "hin\u00b7aus", "ins", "Frei\u00b7e", "mei\u00b7nen", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo duftiggrau die Morgend\u00e4mmrung lag,", "tokens": ["Wo", "duf\u00b7tig\u00b7grau", "die", "Mor\u00b7gen\u00b7d\u00e4mm\u00b7rung", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Umflorend noch den rosenroten Tag;", "tokens": ["Um\u00b7flo\u00b7rend", "noch", "den", "ro\u00b7sen\u00b7ro\u00b7ten", "Tag", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mich einmal satt zu gehn in Busch und Feldern", "tokens": ["Mich", "ein\u00b7mal", "satt", "zu", "gehn", "in", "Busch", "und", "Fel\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADJD", "PTKZU", "VVINF", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Vom Morgen fr\u00fch bis in die sp\u00e4te Nacht,", "tokens": ["Vom", "Mor\u00b7gen", "fr\u00fch", "bis", "in", "die", "sp\u00e4\u00b7te", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und auch ein Lied zu holen in den W\u00e4ldern,", "tokens": ["Und", "auch", "ein", "Lied", "zu", "ho\u00b7len", "in", "den", "W\u00e4l\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PTKZU", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Hatt ich zum festen Vorsatz mir gemacht.", "tokens": ["Hatt", "ich", "zum", "fes\u00b7ten", "Vor\u00b7satz", "mir", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Rein war der Himmel, bald zum Tag erhellt,", "tokens": ["Rein", "war", "der", "Him\u00b7mel", ",", "bald", "zum", "Tag", "er\u00b7hellt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der volle Lebenspuls schlug durch die Welt;", "tokens": ["Der", "vol\u00b7le", "Le\u00b7bens\u00b7puls", "schlug", "durch", "die", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die L\u00fcfte wehten und der Vogel sang,", "tokens": ["Die", "L\u00fcf\u00b7te", "weh\u00b7ten", "und", "der", "Vo\u00b7gel", "sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Eichen wuchsen und die Quelle sprang.", "tokens": ["Die", "Ei\u00b7chen", "wuch\u00b7sen", "und", "die", "Quel\u00b7le", "sprang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Blumen bl\u00fchten und die Fr\u00fcchte reiften,", "tokens": ["Die", "Blu\u00b7men", "bl\u00fch\u00b7ten", "und", "die", "Fr\u00fcch\u00b7te", "reif\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein jeglich Gras tat seinen Atemzug;", "tokens": ["Ein", "jeg\u00b7lich", "Gras", "tat", "sei\u00b7nen", "A\u00b7tem\u00b7zug", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die Berge standen und die Wolken schweiften", "tokens": ["Die", "Ber\u00b7ge", "stan\u00b7den", "und", "die", "Wol\u00b7ken", "schweif\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "In gleicher Luft, die meinen Odem trug.", "tokens": ["In", "glei\u00b7cher", "Luft", ",", "die", "mei\u00b7nen", "O\u00b7dem", "trug", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich schlenderte den lieben Tag entlang,", "tokens": ["Ich", "schlen\u00b7der\u00b7te", "den", "lie\u00b7ben", "Tag", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Im Herzen regte sich der Hochgesang;", "tokens": ["Im", "Her\u00b7zen", "reg\u00b7te", "sich", "der", "Hoch\u00b7ge\u00b7sang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es brach sich Bahn der Wachtel heller Schlag,", "tokens": ["Es", "brach", "sich", "Bahn", "der", "Wach\u00b7tel", "hel\u00b7ler", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Jedoch mein Lied \u2013 es rang sich nicht zu Tag.", "tokens": ["Je\u00b7doch", "mein", "Lied", "\u2013", "es", "rang", "sich", "nicht", "zu", "Tag", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$(", "PPER", "VVFIN", "PRF", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Mittag kam, ich lag an Silberfl\u00fcssen,", "tokens": ["Der", "Mit\u00b7tag", "kam", ",", "ich", "lag", "an", "Sil\u00b7ber\u00b7fl\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Sonne sucht ich in der klaren Flut", "tokens": ["Die", "Son\u00b7ne", "sucht", "ich", "in", "der", "kla\u00b7ren", "Flut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und durfte nicht von Angesicht sie gr\u00fc\u00dfen,", "tokens": ["Und", "durf\u00b7te", "nicht", "von", "An\u00b7ge\u00b7sicht", "sie", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "APPR", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der ich allein in all dem Drang geruht.", "tokens": ["Der", "ich", "al\u00b7lein", "in", "all", "dem", "Drang", "ge\u00b7ruht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPR", "PIAT", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Sonne sank und lie\u00df die Welt der Ruh,", "tokens": ["Die", "Son\u00b7ne", "sank", "und", "lie\u00df", "die", "Welt", "der", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Abendnebel gingen ab und zu;", "tokens": ["Die", "A\u00b7bend\u00b7ne\u00b7bel", "gin\u00b7gen", "ab", "und", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Ich lag auf Bergesh\u00f6hen matt und m\u00fcd,", "tokens": ["Ich", "lag", "auf", "Ber\u00b7ges\u00b7h\u00f6\u00b7hen", "matt", "und", "m\u00fcd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PTKVZ", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Tief in der Brust das ungesungne Lied.", "tokens": ["Tief", "in", "der", "Brust", "das", "un\u00b7ge\u00b7sung\u00b7ne", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Da nickten, spottend mein, die schwanken Tannen,", "tokens": ["Da", "nick\u00b7ten", ",", "spot\u00b7tend", "mein", ",", "die", "schwan\u00b7ken", "Tan\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADJD", "PPOSAT", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Auch h\u00f6hnend sah das niedre Moos empor", "tokens": ["Auch", "h\u00f6h\u00b7nend", "sah", "das", "nied\u00b7re", "Moos", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Mit seinen W\u00fcrmern, die gesch\u00e4ftig spannen,", "tokens": ["Mit", "sei\u00b7nen", "W\u00fcr\u00b7mern", ",", "die", "ge\u00b7sch\u00e4f\u00b7tig", "span\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und lachend brach das Firmament hervor.", "tokens": ["Und", "la\u00b7chend", "brach", "das", "Fir\u00b7ma\u00b7ment", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Von Osten wehte frisch und voll der Wind:", "tokens": ["Von", "Os\u00b7ten", "weh\u00b7te", "frisch", "und", "voll", "der", "Wind", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADJD", "KON", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbwas suchst du hier, du m\u00fc\u00dfig Menschenkind,", "tokens": ["\u00bb", "was", "suchst", "du", "hier", ",", "du", "m\u00fc\u00b7\u00dfig", "Men\u00b7schen\u00b7kind", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "ADV", "$,", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du stumme Pfeife in dem Orgelchor,", "tokens": ["Du", "stum\u00b7me", "Pfei\u00b7fe", "in", "dem", "Or\u00b7gel\u00b7chor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Schlemihl, der tr\u00e4umend Raum und Zeit verlor?", "tokens": ["Schle\u00b7mihl", ",", "der", "tr\u00e4u\u00b7mend", "Raum", "und", "Zeit", "ver\u00b7lor", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADJD", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dir ward das Leichteste, das Lied, gegeben,", "tokens": ["Dir", "ward", "das", "Leich\u00b7tes\u00b7te", ",", "das", "Lied", ",", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ART", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Das, selbst sich bauend, aus der Kehle bricht;", "tokens": ["Das", ",", "selbst", "sich", "bau\u00b7end", ",", "aus", "der", "Keh\u00b7le", "bricht", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "ADV", "PRF", "VVPP", "$,", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Du aber legst dein unbeholfen Leben", "tokens": ["Du", "a\u00b7ber", "legst", "dein", "un\u00b7be\u00b7hol\u00b7fen", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wie einen Stein ihm auf den Weg zum Licht!\u00ab", "tokens": ["Wie", "ei\u00b7nen", "Stein", "ihm", "auf", "den", "Weg", "zum", "Licht", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "APPR", "ART", "NN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Sprach so der Wind? O nein, so sprach der Schmerz,", "tokens": ["Sprach", "so", "der", "Wind", "?", "O", "nein", ",", "so", "sprach", "der", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$.", "NE", "PTKANT", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der mir wie Ketten hing ums dunkle Herz!", "tokens": ["Der", "mir", "wie", "Ket\u00b7ten", "hing", "ums", "dunk\u00b7le", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "KOKOM", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein fremder K\u00f6rper ohne Form und Schall,", "tokens": ["Ein", "frem\u00b7der", "K\u00f6r\u00b7per", "oh\u00b7ne", "Form", "und", "Schall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So, deuchte mir, lag ich im regen All.", "tokens": ["So", ",", "deuch\u00b7te", "mir", ",", "lag", "ich", "im", "re\u00b7gen", "All", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Und Luft und Tannen, Berge, Moos und Sterne,", "tokens": ["Und", "Luft", "und", "Tan\u00b7nen", ",", "Ber\u00b7ge", ",", "Moos", "und", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sie schlangen l\u00e4chelnd ihren weiten Kranz;", "tokens": ["Sie", "schlan\u00b7gen", "l\u00e4\u00b7chelnd", "ih\u00b7ren", "wei\u00b7ten", "Kranz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wie an der Insel sich das Meer, das ferne,", "tokens": ["Wie", "an", "der", "In\u00b7sel", "sich", "das", "Meer", ",", "das", "fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "PRF", "ART", "NN", "$