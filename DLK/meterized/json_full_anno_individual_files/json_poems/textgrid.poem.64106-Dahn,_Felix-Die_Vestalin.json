{"textgrid.poem.64106": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Die Vestalin", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.71", "sv:0.28"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In den stillen Tempel l\u00e4rmend", "tokens": ["In", "den", "stil\u00b7len", "Tem\u00b7pel", "l\u00e4r\u00b7mend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bricht das Volk, emp\u00f6rt in Wut:", "tokens": ["Bricht", "das", "Volk", ",", "em\u00b7p\u00f6rt", "in", "Wut", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbauf und schleppt sie vor den Pr\u00e4tor,", "tokens": ["\u00bb", "auf", "und", "schleppt", "sie", "vor", "den", "Pr\u00e4\u00b7tor", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tilgt die Schuld in ihrem Blut,", "tokens": ["Tilgt", "die", "Schuld", "in", "ih\u00b7rem", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn kein Rauch steigt mehr zum Himmel,", "tokens": ["Denn", "kein", "Rauch", "steigt", "mehr", "zum", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und erloschen liegt die Glut.", "tokens": ["Und", "er\u00b7lo\u00b7schen", "liegt", "die", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Priesterin, wo war dein Eifer,", "tokens": ["Pries\u00b7te\u00b7rin", ",", "wo", "war", "dein", "Ei\u00b7fer", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Priesterin, wo war dein Herz?", "tokens": ["Pries\u00b7te\u00b7rin", ",", "wo", "war", "dein", "Herz", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tr\u00e4umtest du der Liebe Tr\u00e4ume,", "tokens": ["Tr\u00e4um\u00b7test", "du", "der", "Lie\u00b7be", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Pflogest du der Liebe Scherz?", "tokens": ["Pflo\u00b7gest", "du", "der", "Lie\u00b7be", "Scherz", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sucht den Buhlen und zerfleischt ihn", "tokens": ["Sucht", "den", "Buh\u00b7len", "und", "zer\u00b7fleischt", "ihn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Glied f\u00fcr Glied mit scharfem Erz.", "tokens": ["Glied", "f\u00fcr", "Glied", "mit", "schar\u00b7fem", "Erz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch sie selbst scharrt in die Erde", "tokens": ["Doch", "sie", "selbst", "scharrt", "in", "die", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Lebend ein mit ihrer Schmach.\u00ab", "tokens": ["Le\u00b7bend", "ein", "mit", "ih\u00b7rer", "Schmach", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "ART", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Also tobt die blinde Menge,", "tokens": ["Al\u00b7so", "tobt", "die", "blin\u00b7de", "Men\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von den S\u00e4ulen schallt es nach.", "tokens": ["Von", "den", "S\u00e4u\u00b7len", "schallt", "es", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch erwacht aus tiefem Schweigen", "tokens": ["Doch", "er\u00b7wacht", "aus", "tie\u00b7fem", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Trauervoll die Jungfrau sprach:", "tokens": ["Trau\u00b7er\u00b7voll", "die", "Jung\u00b7frau", "sprach", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbwehe, rohe M\u00e4nner, wehe,", "tokens": ["\u00bb", "we\u00b7he", ",", "ro\u00b7he", "M\u00e4n\u00b7ner", ",", "we\u00b7he", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ADJD", "$,", "ADJA", "NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die ihr scheulos, wild, im Streit,", "tokens": ["Die", "ihr", "scheu\u00b7los", ",", "wild", ",", "im", "Streit", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "$,", "ADJD", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf den Lippen Zorn und Fl\u00fcche,", "tokens": ["Auf", "den", "Lip\u00b7pen", "Zorn", "und", "Fl\u00fc\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In dies Haus getreten seid:", "tokens": ["In", "dies", "Haus", "ge\u00b7tre\u00b7ten", "seid", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "NN", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nicht die Priesterin, ihr selber", "tokens": ["Nicht", "die", "Pries\u00b7te\u00b7rin", ",", "ihr", "sel\u00b7ber"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "ART", "NN", "$,", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Habt das Heiligtum entweiht.\u00ab", "tokens": ["Habt", "das", "Hei\u00b7lig\u00b7tum", "ent\u00b7weiht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ART", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbheuchlerin, da sieh die Asche!", "tokens": ["\u00bb", "heuch\u00b7le\u00b7rin", ",", "da", "sieh", "die", "A\u00b7sche", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sprich, was l\u00f6schte diese Glut?\u00ab", "tokens": ["Sprich", ",", "was", "l\u00f6schte", "die\u00b7se", "Glut", "?", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "$,", "PWS", "VVFIN", "PDAT", "NN", "$.", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "\u00bbunausl\u00f6schlich lodert Vestas", "tokens": ["\u00bb", "un\u00b7aus\u00b7l\u00f6schlich", "lo\u00b7dert", "Ves\u00b7tas"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "ADJD", "VVFIN", "NE"], "meter": "+-+---+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Herd in meines Herzens Hut:", "tokens": ["Herd", "in", "mei\u00b7nes", "Her\u00b7zens", "Hut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und was diese Br\u00e4nde l\u00f6schte, \u2013", "tokens": ["Und", "was", "die\u00b7se", "Br\u00e4n\u00b7de", "l\u00f6schte", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PWS", "PDAT", "NN", "VVFIN", "$,", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Das war meiner Tr\u00e4nen Flut.\u00ab", "tokens": ["Das", "war", "mei\u00b7ner", "Tr\u00e4\u00b7nen", "Flut", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "NN", "$.", "$("], "meter": "----+-+", "measure": "unknown.measure.di"}}, "stanza.6": {"line.1": {"text": "\u00bbtr\u00e4nen? was hast du zu weinen,", "tokens": ["\u00bb", "tr\u00e4\u00b7nen", "?", "was", "hast", "du", "zu", "wei\u00b7nen", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVINF", "$.", "PWS", "VAFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du der G\u00f6ttin Dienerin?\u00ab", "tokens": ["Du", "der", "G\u00f6t\u00b7tin", "Die\u00b7ne\u00b7rin", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ART", "NN", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbvor drei Tagen sank bei Cann\u00e4", "tokens": ["\u00bb", "vor", "drei", "Ta\u00b7gen", "sank", "bei", "Can\u00b7n\u00e4"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "APPR", "CARD", "NN", "VVFIN", "APPR", "NE"], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Romas Ruhm und Macht dahin,", "tokens": ["Ro\u00b7mas", "Ruhm", "und", "Macht", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und als Priesterin ich worden,", "tokens": ["Und", "als", "Pries\u00b7te\u00b7rin", "ich", "wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NN", "PPER", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Blieb ich dennoch R\u00f6merin.\u00ab", "tokens": ["Blieb", "ich", "den\u00b7noch", "R\u00f6\u00b7me\u00b7rin", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "\u00bbnicht um Rom, um einen Buhlen,", "tokens": ["\u00bb", "nicht", "um", "Rom", ",", "um", "ei\u00b7nen", "Buh\u00b7len", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PTKNEG", "APPR", "NE", "$,", "KOUI", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der gefallen, weint sie wohl:", "tokens": ["Der", "ge\u00b7fal\u00b7len", ",", "weint", "sie", "wohl", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "VVPP", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf! ergreift sie, sie soll sterben,", "tokens": ["Auf", "!", "er\u00b7greift", "sie", ",", "sie", "soll", "ster\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "VVFIN", "PPER", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schleift sie fort aufs Kapitol.\u00ab", "tokens": ["Schleift", "sie", "fort", "aufs", "Ka\u00b7pi\u00b7tol", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch die Priesterin umklammert", "tokens": ["Doch", "die", "Pries\u00b7te\u00b7rin", "um\u00b7klam\u00b7mert"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Fest der G\u00f6ttin Steinsymbol:", "tokens": ["Fest", "der", "G\u00f6t\u00b7tin", "Stein\u00b7sym\u00b7bol", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "\u00bbh\u00f6re mich, du gro\u00dfe G\u00f6ttin,", "tokens": ["\u00bb", "h\u00f6\u00b7re", "mich", ",", "du", "gro\u00b7\u00dfe", "G\u00f6t\u00b7tin", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die du reiner dort nicht thronst", "tokens": ["Die", "du", "rei\u00b7ner", "dort", "nicht", "thronst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADJA", "ADV", "PTKNEG", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In den Hallen des Olympos,", "tokens": ["In", "den", "Hal\u00b7len", "des", "O\u00b7lym\u00b7pos", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als du mir im Herzen wohnst,", "tokens": ["Als", "du", "mir", "im", "Her\u00b7zen", "wohnst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die du schrecklich strafst den Frevel,", "tokens": ["Die", "du", "schreck\u00b7lich", "strafst", "den", "Fre\u00b7vel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wunderbar die Unschuld lohnst:", "tokens": ["Wun\u00b7der\u00b7bar", "die", "Un\u00b7schuld", "lohnst", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "H\u00f6re mich, die alle Feuer", "tokens": ["H\u00f6\u00b7re", "mich", ",", "die", "al\u00b7le", "Feu\u00b7er"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PRELS", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit dem heil'gen Atem sch\u00fcrt:", "tokens": ["Mit", "dem", "heil'\u00b7gen", "A\u00b7tem", "sch\u00fcrt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bin ich rein an Leib und Seele,", "tokens": ["Bin", "ich", "rein", "an", "Leib", "und", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie der Priesterin geb\u00fchrt, \u2013", "tokens": ["Wie", "der", "Pries\u00b7te\u00b7rin", "ge\u00b7b\u00fchrt", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "$,", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Auf, entz\u00fcnde diese Kohlen,", "tokens": ["Auf", ",", "ent\u00b7z\u00fcn\u00b7de", "die\u00b7se", "Koh\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "VVFIN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie sie meine Hand ber\u00fchrt.\u00ab", "tokens": ["Wie", "sie", "mei\u00b7ne", "Hand", "be\u00b7r\u00fchrt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVPP", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Spricht's, und auf die schwarzen Br\u00e4nde", "tokens": ["Spricht's", ",", "und", "auf", "die", "schwar\u00b7zen", "Br\u00e4n\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "KON", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Legt sie leis die wei\u00dfe