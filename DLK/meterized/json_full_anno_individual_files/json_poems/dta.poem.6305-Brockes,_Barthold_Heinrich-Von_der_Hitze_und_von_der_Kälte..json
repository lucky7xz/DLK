{"dta.poem.6305": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Hitze und von der K\u00e4lte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Empfindet man die Macht und Wuth", "tokens": ["Emp\u00b7fin\u00b7det", "man", "die", "Macht", "und", "Wuth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von angesch\u00fchrter Kohlen-Gluht;", "tokens": ["Von", "an\u00b7ge\u00b7sch\u00fchr\u00b7ter", "Koh\u00b7len\u00b7Gluht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was kan in uns das F\u00fchlen dieser Pein,", "tokens": ["Was", "kan", "in", "uns", "das", "F\u00fch\u00b7len", "die\u00b7ser", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "APPR", "PPER", "ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So uns vom Feur entspringt, doch seyn?", "tokens": ["So", "uns", "vom", "Feur", "ent\u00b7springt", ",", "doch", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPRART", "NN", "VVFIN", "$,", "ADV", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und was ist sonst im Feuer, wenns uns brennet,", "tokens": ["Und", "was", "ist", "sonst", "im", "Feu\u00b7er", ",", "wenns", "uns", "bren\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "ADV", "APPRART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Als eine Macht, die uns das Fleisch verr\u00fcckt und trennet?", "tokens": ["Als", "ei\u00b7ne", "Macht", ",", "die", "uns", "das", "Fleisch", "ver\u00b7r\u00fcckt", "und", "tren\u00b7net", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "PPER", "ART", "NN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wenn sich in uns des Feuers Theilchen pressen;", "tokens": ["Wenn", "sich", "in", "uns", "des", "Feu\u00b7ers", "Theil\u00b7chen", "pres\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "APPR", "PPER", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "So widme man darum dem Feur die Hitze nicht,", "tokens": ["So", "wid\u00b7me", "man", "da\u00b7rum", "dem", "Feur", "die", "Hit\u00b7ze", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PAV", "ART", "NN", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So wenig, als wenn uns der Stahl zerreist und sticht,", "tokens": ["So", "we\u00b7nig", ",", "als", "wenn", "uns", "der", "Stahl", "zer\u00b7reist", "und", "sticht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "$,", "KOKOM", "KOUS", "PPER", "ART", "NN", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Man unsern Schmertz mit Recht dem Stahl sucht beyzumessen.", "tokens": ["Man", "un\u00b7sern", "Schmertz", "mit", "Recht", "dem", "Stahl", "sucht", "bey\u00b7zu\u00b7mes\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PPOSAT", "NN", "APPR", "NN", "ART", "NN", "VVFIN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}