{"textgrid.poem.65984": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Er ward \u00fcber Nacht ein ber\u00fchmter Mann.", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er ward \u00fcber Nacht ein ber\u00fchmter Mann.", "tokens": ["Er", "ward", "\u00fc\u00b7ber", "Nacht", "ein", "be\u00b7r\u00fchm\u00b7ter", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die guten Leute starren ihn an,", "tokens": ["Die", "gu\u00b7ten", "Leu\u00b7te", "star\u00b7ren", "ihn", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Gr\u00fc\u00dfen tief und zeigen mit Fingern:", "tokens": ["Gr\u00fc\u00b7\u00dfen", "tief", "und", "zei\u00b7gen", "mit", "Fin\u00b7gern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Seht den, ist keiner von den Geringern.", "tokens": ["Seht", "den", ",", "ist", "kei\u00b7ner", "von", "den", "Ge\u00b7rin\u00b7gern", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "$,", "VAFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Ein Fremder fragte nach seinen Thaten,", "tokens": ["Ein", "Frem\u00b7der", "frag\u00b7te", "nach", "sei\u00b7nen", "Tha\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Da wussten sie sich nur halb zu raten.", "tokens": ["Da", "wuss\u00b7ten", "sie", "sich", "nur", "halb", "zu", "ra\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Sein Name w\u00e4r' M\u00fcller, gedichtet h\u00e4tt' er", "tokens": ["Sein", "Na\u00b7me", "w\u00e4r'", "M\u00fcl\u00b7ler", ",", "ge\u00b7dich\u00b7tet", "h\u00e4tt'", "er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "NE", "$,", "VVPP", "VAFIN", "PPER"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Geniales, so schrieben es alle Bl\u00e4tter.", "tokens": ["Ge\u00b7ni\u00b7a\u00b7les", ",", "so", "schrie\u00b7ben", "es", "al\u00b7le", "Bl\u00e4t\u00b7ter", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Zwar h\u00e4tten sie's selber noch nicht gelesen,", "tokens": ["Zwar", "h\u00e4t\u00b7ten", "sie's", "sel\u00b7ber", "noch", "nicht", "ge\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Doch w\u00e4r' es trotzdem sehr sch\u00f6n gewesen.", "tokens": ["Doch", "w\u00e4r'", "es", "trotz\u00b7dem", "sehr", "sch\u00f6n", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PA