{"textgrid.poem.40656": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbdeutschland ist frei!\u00ab Im Jubelsturm nur leise,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbdeutschland ist frei!\u00ab Im Jubelsturm nur leise,", "tokens": ["\u00bb", "deutschland", "ist", "frei", "!", "\u00ab", "Im", "Ju\u00b7bel\u00b7sturm", "nur", "lei\u00b7se", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "ADJD", "$.", "$(", "APPRART", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dich nicht zu schrecken, klang's aus Freundesmunde;", "tokens": ["Dich", "nicht", "zu", "schre\u00b7cken", ",", "klang's", "aus", "Freun\u00b7des\u00b7mun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "PTKZU", "VVINF", "$,", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Lenzstrahl doch, an dem dein Herz gesunde,", "tokens": ["Der", "Lenz\u00b7strahl", "doch", ",", "an", "dem", "dein", "Herz", "ge\u00b7sun\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "APPR", "ART", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ach, er durchdrang nicht deines Geistes Eise.", "tokens": ["Ach", ",", "er", "durch\u00b7drang", "nicht", "dei\u00b7nes", "Geis\u00b7tes", "Ei\u00b7se", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "PTKNEG", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "\u00bbdeutschland ist frei!\u00ab So scholl die stolze Weise,", "tokens": ["\u00bb", "deutschland", "ist", "frei", "!", "\u00ab", "So", "scholl", "die", "stol\u00b7ze", "Wei\u00b7se", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "ADJD", "$.", "$(", "ADV", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dich zu erwecken, donnernd in der Runde;", "tokens": ["Dich", "zu", "er\u00b7we\u00b7cken", ",", "don\u00b7nernd", "in", "der", "Run\u00b7de", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der beste Heilquell ist solch gro\u00dfe Stunde \u2013", "tokens": ["Der", "bes\u00b7te", "Heil\u00b7quell", "ist", "solch", "gro\u00b7\u00dfe", "Stun\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch sie zerbrach nicht deines Bannes Kreise.", "tokens": ["Doch", "sie", "zer\u00b7brach", "nicht", "dei\u00b7nes", "Ban\u00b7nes", "Krei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Des ehrnen Kaiserbilds will mich's gemahnen,", "tokens": ["Des", "ehr\u00b7nen", "Kai\u00b7ser\u00b7bilds", "will", "mich's", "ge\u00b7mah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem in die Hand sein Volk zur\u00fcckgegeben", "tokens": ["Dem", "in", "die", "Hand", "sein", "Volk", "zu\u00b7r\u00fcck\u00b7ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die heil'ge Fahne, der einst galt sein Ringen;", "tokens": ["Die", "heil'\u00b7ge", "Fah\u00b7ne", ",", "der", "einst", "galt", "sein", "Rin\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Hoch flattert sie, die Fahne aller Fahnen,", "tokens": ["Hoch", "flat\u00b7tert", "sie", ",", "die", "Fah\u00b7ne", "al\u00b7ler", "Fah\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ins starre Erzbild doch facht sie kein Leben", "tokens": ["Ins", "star\u00b7re", "Erz\u00b7bild", "doch", "facht", "sie", "kein", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ADV", "VVFIN", "PPER", "PIAT", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und jener Todte wird sie nimmer schwingen.", "tokens": ["Und", "je\u00b7ner", "Tod\u00b7te", "wird", "sie", "nim\u00b7mer", "schwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "\