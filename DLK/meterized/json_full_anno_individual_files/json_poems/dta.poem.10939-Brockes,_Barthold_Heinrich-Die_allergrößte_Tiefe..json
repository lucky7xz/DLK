{"dta.poem.10939": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die  \n allergr\u00f6\u00dfte Tiefe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Drey Tiefen sind auf dieser Welt. Die eine Tief erf\u00fcllt\ndie Fluth,", "tokens": ["Drey", "Tie\u00b7fen", "sind", "auf", "die\u00b7ser", "Welt", ".", "Die", "ei\u00b7ne", "Tief", "er\u00b7f\u00fcllt", "die", "Fluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "APPR", "PDAT", "NN", "$.", "ART", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Die andre Tiefe f\u00fcllt die ", "tokens": ["Die", "and\u00b7re", "Tie\u00b7fe", "f\u00fcllt", "die"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gluht.", "tokens": ["Gluht", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.4": {"text": "So wie die ", "tokens": ["So", "wie", "die"], "token_info": ["word", "word", "word"], "pos": ["ADV", "KOKOM", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Sich mit derselbigen vereint, sie auch umringet und um-", "tokens": ["Sich", "mit", "der\u00b7sel\u00b7bi\u00b7gen", "ver\u00b7eint", ",", "sie", "auch", "um\u00b7rin\u00b7get", "und", "um"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "PDS", "VVFIN", "$,", "PPER", "ADV", "VVFIN", "KON", "TRUNC"], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.6": {"text": "So wird die ", "tokens": ["So", "wird", "die"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VAFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Auch von der ", "tokens": ["Auch", "von", "der"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.8": {"text": "Durchdrungen, und erf\u00fcllt.", "tokens": ["Durch\u00b7drun\u00b7gen", ",", "und", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Von dieser scheint nun wieder \u00fcberzeuglich,", "tokens": ["Von", "die\u00b7ser", "scheint", "nun", "wie\u00b7der", "\u00fc\u00b7berz\u00b7eug\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Da\u00df sie, ", "tokens": ["Da\u00df", "sie", ","], "token_info": ["word", "word", "punct"], "pos": ["KOUS", "PPER", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Wodurch sie alle sich beleben.", "tokens": ["Wo\u00b7durch", "sie", "al\u00b7le", "sich", "be\u00b7le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIS", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Da diese nun so unergr\u00fcndlich ist, als unersteiglich,", "tokens": ["Da", "die\u00b7se", "nun", "so", "un\u00b7er\u00b7gr\u00fcnd\u00b7lich", "ist", ",", "als", "un\u00b7er\u00b7steig\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ADV", "ADV", "ADJD", "VAFIN", "$,", "KOUS", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.13": {"text": "Ist es vermuthlich die, von welcher Paulus riefe:", "tokens": ["Ist", "es", "ver\u00b7muth\u00b7lich", "die", ",", "von", "wel\u00b7cher", "Pau\u00b7lus", "rie\u00b7fe", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "$,", "APPR", "PRELS", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "O! welche Tiefe!", "tokens": ["O", "!", "wel\u00b7che", "Tie\u00b7fe", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "PWAT", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}