{"dta.poem.14175": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Verlorene Liebe .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Locken hatte sie wie deine,", "tokens": ["Lo\u00b7cken", "hat\u00b7te", "sie", "wie", "dei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "KOKOM", "PPOSAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bleiche Wangen, Lippen roth \u2014", "tokens": ["Blei\u00b7che", "Wan\u00b7gen", ",", "Lip\u00b7pen", "roth"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, du bist ja doch nicht meine,", "tokens": ["Ach", ",", "du", "bist", "ja", "doch", "nicht", "mei\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "ADV", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "H\u00e4ttest du nur nicht gesprochen", "tokens": ["H\u00e4t\u00b7test", "du", "nur", "nicht", "ge\u00b7spro\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und so frech geblickt nach mir,", "tokens": ["Und", "so", "frech", "ge\u00b7blickt", "nach", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVPP", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Das hat ganz den Traum zerbrochen", "tokens": ["Das", "hat", "ganz", "den", "Traum", "zer\u00b7bro\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "ART", "NN", "VVPP"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Und nun grauet mir vor dir.", "tokens": ["Und", "nun", "grau\u00b7et", "mir", "vor", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Da nimm Geld, kauf Putz und Flimmern,", "tokens": ["Da", "nimm", "Geld", ",", "kauf", "Putz", "und", "Flim\u00b7mern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "NN", "$,", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Fort und lache nicht so wild!", "tokens": ["Fort", "und", "la\u00b7che", "nicht", "so", "wild", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PTKNEG", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "O ich m\u00f6chte dich zertr\u00fcmmern,", "tokens": ["O", "ich", "m\u00f6ch\u00b7te", "dich", "zer\u00b7tr\u00fcm\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VMFIN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Sch\u00f6nes, l\u00fcgenhaftes Bild!", "tokens": ["Sch\u00f6\u00b7nes", ",", "l\u00fc\u00b7gen\u00b7haf\u00b7tes", "Bild", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}