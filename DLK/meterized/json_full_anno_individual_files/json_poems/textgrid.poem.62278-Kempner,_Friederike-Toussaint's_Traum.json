{"textgrid.poem.62278": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "Toussaint's Traum", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Urwald war's \u2013 Millionen B\u00e4ume,", "tokens": ["Ein", "Ur\u00b7wald", "wa\u00b7r's", "\u2013", "Mil\u00b7lion\u00b7en", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$(", "NN", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ach unabsehbar hoch und unerfa\u00dfbar tief,", "tokens": ["Ach", "un\u00b7ab\u00b7seh\u00b7bar", "hoch", "und", "un\u00b7er\u00b7fa\u00df\u00b7bar", "tief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJD", "ADJD", "KON", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und unter einem dieser B\u00e4ume", "tokens": ["Und", "un\u00b7ter", "ei\u00b7nem", "die\u00b7ser", "B\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da lag ein Mensch, ein Neger, und er schlief.", "tokens": ["Da", "lag", "ein", "Mensch", ",", "ein", "Ne\u00b7ger", ",", "und", "er", "schlief", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$,", "KON", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Er tr\u00e4umt von San Domingo's Krone", "tokens": ["Er", "tr\u00e4umt", "von", "San", "Do\u00b7min\u00b7go's", "Kro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": 