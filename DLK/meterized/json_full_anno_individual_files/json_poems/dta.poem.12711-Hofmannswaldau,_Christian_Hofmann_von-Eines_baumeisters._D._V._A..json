{"dta.poem.12711": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Eines baumeisters.  \n  D.  V. A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der bau zerf\u00e4llt, die h\u00fctte wird zur erden,", "tokens": ["Der", "bau", "zer\u00b7f\u00e4llt", ",", "die", "h\u00fct\u00b7te", "wird", "zur", "er\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PRELS", "ADV", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der seelen hau\u00df: GOtt reist es selber ein;", "tokens": ["Der", "see\u00b7len", "hau\u00df", ":", "Gott", "reist", "es", "sel\u00b7ber", "ein", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denn weil es dort soll unverwe\u00dflich seyn,", "tokens": ["Denn", "weil", "es", "dort", "soll", "un\u00b7ver\u00b7we\u00df\u00b7lich", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So mu\u00df es hier erst staub und asche werden.", "tokens": ["So", "mu\u00df", "es", "hier", "erst", "staub", "und", "asc\u00b7he", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "VVFIN", "KON", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}