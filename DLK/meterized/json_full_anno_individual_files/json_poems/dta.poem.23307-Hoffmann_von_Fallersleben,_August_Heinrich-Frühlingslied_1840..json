{"dta.poem.23307": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingslied 1840.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O des Maies sch\u00f6ne Tage!", "tokens": ["O", "des", "Mai\u00b7es", "sch\u00f6\u00b7ne", "Ta\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wann die Erd' ist wieder gr\u00fcn,", "tokens": ["Wann", "die", "Erd'", "ist", "wie\u00b7der", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wann im Felde, Wald und Hage", "tokens": ["Wann", "im", "Fel\u00b7de", ",", "Wald", "und", "Ha\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alle B\u00e4um' und Blumen bl\u00fchn \u2014", "tokens": ["Al\u00b7le", "B\u00e4um'", "und", "Blu\u00b7men", "bl\u00fchn"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "O des Maies sch\u00f6ne Tage!", "tokens": ["O", "des", "Mai\u00b7es", "sch\u00f6\u00b7ne", "Ta\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wann der Hoffnung volle Bl\u00fcthe", "tokens": ["Wann", "der", "Hoff\u00b7nung", "vol\u00b7le", "Bl\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Dann aus jeder Knospe bricht \u2014", "tokens": ["Dann", "aus", "je\u00b7der", "Knos\u00b7pe", "bricht"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Deutschland, da\u00df dich Gott beh\u00fcte!", "tokens": ["Deutschland", ",", "da\u00df", "dich", "Gott", "be\u00b7h\u00fc\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "NN", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.9": {"text": "Deine Hoffnung bl\u00fcht noch nicht.", "tokens": ["Dei\u00b7ne", "Hoff\u00b7nung", "bl\u00fcht", "noch", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Steht die Welt im Hoffnungskleide", "tokens": ["Steht", "die", "Welt", "im", "Hoff\u00b7nungs\u00b7klei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch schon f\u00fcnfundzwanzigmal,", "tokens": ["Doch", "schon", "f\u00fcn\u00b7fund\u00b7zwan\u00b7zig\u00b7mal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hoffnung springet auf der Heide,", "tokens": ["Hoff\u00b7nung", "sprin\u00b7get", "auf", "der", "Hei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wandelt \u00fcber Berg und Thal \u2014", "tokens": ["Wan\u00b7delt", "\u00fc\u00b7ber", "Berg", "und", "Thal"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "O die Welt im Hoffnungskleide!", "tokens": ["O", "die", "Welt", "im", "Hoff\u00b7nungs\u00b7klei\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird die Knospe nie erscheinen,", "tokens": ["Wird", "die", "Knos\u00b7pe", "nie", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Draus auch ", "tokens": ["Draus", "auch"], "token_info": ["word", "word"], "pos": ["PAV", "ADV"], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "La\u00dft mich schweigen, la\u00dft mich weinen!", "tokens": ["La\u00dft", "mich", "schwei\u00b7gen", ",", "la\u00dft", "mich", "wei\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "$,", "VVIMP", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Deine Hoffnung bl\u00fcht noch nicht.", "tokens": ["Dei\u00b7ne", "Hoff\u00b7nung", "bl\u00fcht", "noch", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}