{"textgrid.poem.25406": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "Hier ist Einkehr", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier ist Einkehr. Hier ist Stille, den Tagen und N\u00e4chten", "tokens": ["Hier", "ist", "Ein\u00b7kehr", ".", "Hier", "ist", "Stil\u00b7le", ",", "den", "Ta\u00b7gen", "und", "N\u00e4ch\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "$.", "ADV", "VAFIN", "NN", "$,", "ART", "NN", "KON", "NN"], "meter": "+---+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "zu lauschen, die aufstehen und versinken.", "tokens": ["zu", "lau\u00b7schen", ",", "die", "auf\u00b7ste\u00b7hen", "und", "ver\u00b7sin\u00b7ken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "ART", "ADJA", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hier beginnen die H\u00fcgel. Hier hebt sich,", "tokens": ["Hier", "be\u00b7gin\u00b7nen", "die", "H\u00fc\u00b7gel", ".", "Hier", "hebt", "sich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "ADV", "VVFIN", "PRF", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "tiefer landw\u00e4rts, Gebirge, Kiefernw\u00e4lder", "tokens": ["tie\u00b7fer", "land\u00b7w\u00e4rts", ",", "Ge\u00b7bir\u00b7ge", ",", "Kie\u00b7fern\u00b7w\u00e4l\u00b7der"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["ADJD", "ADV", "$,", "NN", "$,", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "und durchrauschte T\u00e4ler.", "tokens": ["und", "durc\u00b7hrauschte", "T\u00e4\u00b7ler", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Hier gie\u00dft sich Wiesengrund ins Freie.", "tokens": ["Hier", "gie\u00dft", "sich", "Wie\u00b7sen\u00b7grund", "ins", "Frei\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "B\u00e4che spiegeln ges\u00e4nftigt reine Wolken.", "tokens": ["B\u00e4\u00b7che", "spie\u00b7geln", "ge\u00b7s\u00e4nf\u00b7tigt", "rei\u00b7ne", "Wol\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VVPP", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Hier ist Ebene, breitschultrig, heftig bl\u00fchend,", "tokens": ["Hier", "ist", "E\u00b7be\u00b7ne", ",", "breit\u00b7schult\u00b7rig", ",", "hef\u00b7tig", "bl\u00fc\u00b7hend", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "$,", "ADJD", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.9": {"text": "\u00c4cker, streifenweis geordnet,", "tokens": ["\u00c4\u00b7cker", ",", "strei\u00b7fen\u00b7weis", "ge\u00b7ord\u00b7net", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Braunschollig, gr\u00fcn, goldgelb von Korn,", "tokens": ["Braun\u00b7schol\u00b7lig", ",", "gr\u00fcn", ",", "gold\u00b7gelb", "von", "Korn", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "das in der Julisonne reift.", "tokens": ["das", "in", "der", "Ju\u00b7li\u00b7son\u00b7ne", "reift", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Tag kommt mit aufgefrischtem Himmel,", "tokens": ["Tag", "kommt", "mit", "auf\u00b7ge\u00b7frischtem", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.13": {"text": "blitzend in den Halmen;", "tokens": ["blit\u00b7zend", "in", "den", "Hal\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.14": {"text": "Morgen mit den harten, k\u00fchlen Farben,", "tokens": ["Mor\u00b7gen", "mit", "den", "har\u00b7ten", ",", "k\u00fch\u00b7len", "Far\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "Die bet\u00e4ubt in einen brennendgelben Mittag sinken \u2013", "tokens": ["Die", "be\u00b7t\u00e4ubt", "in", "ei\u00b7nen", "bren\u00b7nend\u00b7gel\u00b7ben", "Mit\u00b7tag", "sin\u00b7ken", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.16": {"text": "grenzenlose Julisonne \u00fcber allen Feldern,", "tokens": ["gren\u00b7zen\u00b7lo\u00b7se", "Ju\u00b7li\u00b7son\u00b7ne", "\u00fc\u00b7ber", "al\u00b7len", "Fel\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.17": {"text": "In alle Krumen sickernd, schwer ins Mark versenkt,", "tokens": ["In", "al\u00b7le", "Kru\u00b7men", "si\u00b7ckernd", ",", "schwer", "ins", "Mark", "ver\u00b7senkt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$,", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "bewegungslos,", "tokens": ["be\u00b7we\u00b7gungs\u00b7los", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.19": {"text": "In langen Stunden weilend, nur von Schatten \u00fcberw\u00f6lbt,", "tokens": ["In", "lan\u00b7gen", "Stun\u00b7den", "wei\u00b7lend", ",", "nur", "von", "Schat\u00b7ten", "\u00fc\u00b7berw\u00b7\u00f6lbt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.20": {"text": "die langsam weiter laufen,", "tokens": ["die", "lang\u00b7sam", "wei\u00b7ter", "lau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Sich strecken und entz\u00fcndet in das violette Farbenspiel", "tokens": ["Sich", "stre\u00b7cken", "und", "ent\u00b7z\u00fcn\u00b7det", "in", "das", "vi\u00b7o\u00b7let\u00b7te", "Far\u00b7ben\u00b7spiel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PRF", "VVINF", "KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.22": {"text": "des Abends wachsen,", "tokens": ["des", "A\u00b7bends", "wach\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.23": {"text": "Das nicht mehr enden will.", "tokens": ["Das", "nicht", "mehr", "en\u00b7den", "will", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.24": {"text": "Schon ist es Nacht, doch tr\u00e4gt die Luft", "tokens": ["Schon", "ist", "es", "Nacht", ",", "doch", "tr\u00e4gt", "die", "Luft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Mit D\u00e4mmerung vollgesogen", "tokens": ["Mit", "D\u00e4m\u00b7me\u00b7rung", "voll\u00b7ge\u00b7so\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.26": {"text": "noch den lichten Schein,", "tokens": ["noch", "den", "lich\u00b7ten", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.27": {"text": "Der tiefer bl\u00fchend auf der Schwingung", "tokens": ["Der", "tie\u00b7fer", "bl\u00fc\u00b7hend", "auf", "der", "Schwin\u00b7gung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "der gewellten H\u00fcgelr\u00e4nder l\u00e4uft \u2013", "tokens": ["der", "ge\u00b7well\u00b7ten", "H\u00fc\u00b7gel\u00b7r\u00e4n\u00b7der", "l\u00e4uft", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.29": {"text": "Schon reicht unmerklich Fr\u00fche an die Nacht", "tokens": ["Schon", "reicht", "un\u00b7mer\u00b7klich", "Fr\u00fc\u00b7he", "an", "die", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "NN", "APPR", "ART", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.30": {"text": "der wei\u00dfen Sterne.", "tokens": ["der", "wei\u00b7\u00dfen", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.31": {"text": "Bald weht aus B\u00fcschen wieder", "tokens": ["Bald", "weht", "aus", "B\u00fc\u00b7schen", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.32": {"text": "aufgewirbelt junges Licht.", "tokens": ["auf\u00b7ge\u00b7wir\u00b7belt", "jun\u00b7ges", "Licht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und viele Tag und N\u00e4chte werden in der Bl\u00e4ue", "tokens": ["Und", "vie\u00b7le", "Tag", "und", "N\u00e4ch\u00b7te", "wer\u00b7den", "in", "der", "Bl\u00e4u\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "KON", "NN", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "auf- und niedersteigen,", "tokens": ["auf", "und", "nie\u00b7ders\u00b7tei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["TRUNC", "KON", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Eint\u00f6nig, tief ges\u00e4ttigt,", "tokens": ["Ein\u00b7t\u00f6\u00b7nig", ",", "tief", "ge\u00b7s\u00e4t\u00b7tigt", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "VVPP", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "wunschlos in der gro\u00dfen Sommerseligkeit \u2013", "tokens": ["wun\u00b7schlos", "in", "der", "gro\u00b7\u00dfen", "Som\u00b7mer\u00b7se\u00b7lig\u00b7keit", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "Sie tragen auf den schweren", "tokens": ["Sie", "tra\u00b7gen", "auf", "den", "schwe\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "sonngebr\u00e4unten Schultern S\u00e4nftigung und Gl\u00fcck.", "tokens": ["sonn\u00b7ge\u00b7br\u00e4un\u00b7ten", "Schul\u00b7tern", "S\u00e4nf\u00b7ti\u00b7gung", "und", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.3": {"line.1": {"text": "Hier ist Einkehr. Hier ist Stille, den Tagen und N\u00e4chten", "tokens": ["Hier", "ist", "Ein\u00b7kehr", ".", "Hier", "ist", "Stil\u00b7le", ",", "den", "Ta\u00b7gen", "und", "N\u00e4ch\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "$.", "ADV", "VAFIN", "NN", "$,", "ART", "NN", "KON", "NN"], "meter": "+---+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "zu lauschen, die aufstehen und versinken.", "tokens": ["zu", "lau\u00b7schen", ",", "die", "auf\u00b7ste\u00b7hen", "und", "ver\u00b7sin\u00b7ken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "ART", "ADJA", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hier beginnen die H\u00fcgel. Hier hebt sich,", "tokens": ["Hier", "be