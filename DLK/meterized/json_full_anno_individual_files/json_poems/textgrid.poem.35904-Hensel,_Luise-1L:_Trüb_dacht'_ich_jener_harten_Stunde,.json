{"textgrid.poem.35904": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Tr\u00fcb dacht' ich jener harten Stunde,", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tr\u00fcb dacht' ich jener harten Stunde,", "tokens": ["Tr\u00fcb", "dacht'", "ich", "je\u00b7ner", "har\u00b7ten", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da ein geliebtes Leben schied,", "tokens": ["Da", "ein", "ge\u00b7lieb\u00b7tes", "Le\u00b7ben", "schied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Um das des Schmerzes Doppelwunde", "tokens": ["Um", "das", "des", "Schmer\u00b7zes", "Dop\u00b7pel\u00b7wun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Stets frisch mir in dem Herzen gl\u00fcht,", "tokens": ["Stets", "frisch", "mir", "in", "dem", "Her\u00b7zen", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bis \u2013 bald vielleicht \u2013 auch meinem Munde", "tokens": ["Bis", "\u2013", "bald", "viel\u00b7leicht", "\u2013", "auch", "mei\u00b7nem", "Mun\u00b7de"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "$(", "ADV", "ADV", "$(", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der letzte Lebenshauch entflieht.", "tokens": ["Der", "letz\u00b7te", "Le\u00b7bens\u00b7hauch", "ent\u00b7flieht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich f\u00fchlte Dein verarmtes Leben", "tokens": ["Ich", "f\u00fchl\u00b7te", "Dein", "ver\u00b7arm\u00b7tes", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zugleich mit meinem tiefen Leid;", "tokens": ["Zu\u00b7gleich", "mit", "mei\u00b7nem", "tie\u00b7fen", "Leid", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So fr\u00fch geknickt Dein sch\u00f6nes Streben!", "tokens": ["So", "fr\u00fch", "ge\u00b7knickt", "Dein", "sch\u00f6\u00b7nes", "Stre\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Lauf gebrochen vor der Zeit!", "tokens": ["Dein", "Lauf", "ge\u00b7bro\u00b7chen", "vor", "der", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dein Geist \u2013 ach! kann sich nicht erheben", "tokens": ["Dein", "Geist", "\u2013", "ach", "!", "kann", "sich", "nicht", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "ITJ", "$.", "VMFIN", "PRF", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Aus seiner tiefen Traurigkeit.", "tokens": ["Aus", "sei\u00b7ner", "tie\u00b7fen", "Trau\u00b7rig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und immer b\u00e4nger ward mein Sinnen,", "tokens": ["Und", "im\u00b7mer", "b\u00e4n\u00b7ger", "ward", "mein", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und immer sch\u00e4rfer schnitt der Schmerz;", "tokens": ["Und", "im\u00b7mer", "sch\u00e4r\u00b7fer", "schnitt", "der", "Schmerz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich konnte Frieden nicht gewinnen,", "tokens": ["Ich", "konn\u00b7te", "Frie\u00b7den", "nicht", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Wehmuth spaltete mein Herz", "tokens": ["Die", "Weh\u00b7muth", "spal\u00b7te\u00b7te", "mein", "Herz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und nahm den Glaubensmuth von hinnen", "tokens": ["Und", "nahm", "den", "Glau\u00b7bens\u00b7muth", "von", "hin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und zog die Seele erdenw\u00e4rts.", "tokens": ["Und", "zog", "die", "See\u00b7le", "er\u00b7den\u00b7w\u00e4rts", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da scholl ein feierliches L\u00e4uten", "tokens": ["Da", "scholl", "ein", "fei\u00b7er\u00b7li\u00b7ches", "L\u00e4u\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu mir herab von Thurmes H\u00f6h';", "tokens": ["Zu", "mir", "her\u00b7ab", "von", "Thur\u00b7mes", "H\u00f6h'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es will uns heil'ge Feier deuten", "tokens": ["Es", "will", "uns", "heil'\u00b7ge", "Fei\u00b7er", "deu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Himmelstrost im Erdenweh \u2013", "tokens": ["Und", "Him\u00b7mel\u00b7strost", "im", "Er\u00b7den\u00b7weh", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O, la\u00df die Seele uns bereiten,", "tokens": ["O", ",", "la\u00df", "die", "See\u00b7le", "uns", "be\u00b7rei\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVIMP", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df sie dem Herrn entgegengeh'!", "tokens": ["Da\u00df", "sie", "dem", "Herrn", "ent\u00b7ge\u00b7gen\u00b7geh'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Auch uns're Sonne, sie wird sinken;", "tokens": ["Auch", "un\u00b7s'\u00b7re", "Son\u00b7ne", ",", "sie", "wird", "sin\u00b7ken", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wohl uns, wenn hell die Leuchte scheint!", "tokens": ["Wohl", "uns", ",", "wenn", "hell", "die", "Leuch\u00b7te", "scheint", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "KOUS", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann wird ", "tokens": ["Dann", "wird"], "token_info": ["word", "word"], "pos": ["ADV", "VAFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Um die jetzt unser Auge weint,", "tokens": ["Um", "die", "jetzt", "un\u00b7ser", "Au\u00b7ge", "weint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann seh'n wir ihre Krone blinken,", "tokens": ["Dann", "seh'n", "wir", "ih\u00b7re", "Kro\u00b7ne", "blin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In sel'ger Liebe ihr geeint.", "tokens": ["In", "sel'\u00b7ger", "Lie\u00b7be", "ihr", "ge\u00b7eint", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Noch aber gilt's sich zu erheben,", "tokens": ["Noch", "a\u00b7ber", "gilt's", "sich", "zu", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu ringen nach dem ew'gen Licht;", "tokens": ["Zu", "rin\u00b7gen", "nach", "dem", "ew'\u00b7gen", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch gilt's dem Pfund, das Gott gegeben,", "tokens": ["Noch", "gilt's", "dem", "Pfund", ",", "das", "Gott", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch gilt's der Tagesarbeit Pflicht.", "tokens": ["Noch", "gilt's", "der", "Ta\u00b7ges\u00b7ar\u00b7beit", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O, Herr! hilf unserm kranken Streben,", "tokens": ["O", ",", "Herr", "!", "hilf", "un\u00b7serm", "kran\u00b7ken", "Stre\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$.", "VVIMP", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df es die Bahn zu Dir sich bricht!", "tokens": ["Da\u00df", "es", "die", "Bahn", "zu", "Dir", "sich", "bricht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O, lehr' uns glauben, lieben, leben!", "tokens": ["O", ",", "lehr'", "uns", "glau\u00b7ben", ",", "lie\u00b7ben", ",", "le\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "VVIMP", "PPER", "VVFIN", "$,", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "O, sende Deines Geistes Licht!", "tokens": ["O", ",", "sen\u00b7de", "Dei\u00b7nes", "Geis\u00b7tes", "Licht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.7": {"line.1": {"text": "Tr\u00fcb dacht' ich jener harten Stunde,", "tokens": ["Tr\u00fcb", "dacht'", "ich", "je\u00b7ner", "har\u00b7ten", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da ein geliebtes Leben schied,", "tokens": ["Da", "ein", "ge\u00b7lieb\u00b7tes", "Le\u00b7ben", "schied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Um das des Schmerzes Doppelwunde", "tokens": ["Um", "das", "des", "Schmer\u00b7zes", "Dop\u00b7pel\u00b7wun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Stets frisch mir in dem Herzen gl\u00fcht,", "tokens": ["Stets", "frisch", "mir", "in", "dem", "Her\u00b7zen", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bis 