{"dta.poem.19035": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "XvI.  Auf eines Freundes Jahrs-Tag. Am 12. May.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Christum \u00fcber alles lieben,", "tokens": ["Chris\u00b7tum", "\u00fc\u00b7ber", "al\u00b7les", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ubertrifft die Wissenschafft:", "tokens": ["U\u00b7ber\u00b7trifft", "die", "Wis\u00b7sen\u00b7schafft", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist sie noch so hoch getrieben,", "tokens": ["Ist", "sie", "noch", "so", "hoch", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bleibt sie ohne alle Krafft;", "tokens": ["Bleibt", "sie", "oh\u00b7ne", "al\u00b7le", "Krafft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo nicht JEsu Christi Geist", "tokens": ["Wo", "nicht", "Je\u00b7su", "Chris\u00b7ti", "Geist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PTKNEG", "NE", "NE", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sich zugleich in ihr erweist:", "tokens": ["Sich", "zu\u00b7gleich", "in", "ihr", "er\u00b7weist", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Jesum recht im Glauben k\u00fcssen", "tokens": ["Je\u00b7sum", "recht", "im", "Glau\u00b7ben", "k\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPRART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ist das allerh\u00f6chste Wissen.", "tokens": ["Ist", "das", "al\u00b7ler\u00b7h\u00f6chs\u00b7te", "Wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Christum lieben ist die Kette,", "tokens": ["Chris\u00b7tum", "lie\u00b7ben", "ist", "die", "Ket\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVINF", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So die Freundschafft veste macht:", "tokens": ["So", "die", "Freund\u00b7schafft", "ves\u00b7te", "macht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Liebt man Christum um die Wette,", "tokens": ["Liebt", "man", "Chris\u00b7tum", "um", "die", "Wet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "NE", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Wird der Lauff mit Lust vollbracht.", "tokens": ["Wird", "der", "Lauff", "mit", "Lust", "voll\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jesus, unser h\u00f6chster Schatz,", "tokens": ["Je\u00b7sus", ",", "un\u00b7ser", "h\u00f6chs\u00b7ter", "Schatz", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "H\u00e4lt auf dieser Bahn den Platz;", "tokens": ["H\u00e4lt", "auf", "die\u00b7ser", "Bahn", "den", "Platz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PDAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und am abgeme\u00dfnen Ende", "tokens": ["Und", "am", "ab\u00b7ge\u00b7me\u00df\u00b7nen", "En\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Lauffen wir in seine H\u00e4nde.", "tokens": ["Lauf\u00b7fen", "wir", "in", "sei\u00b7ne", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Christi wohl-gepr\u00fcfte Liebe", "tokens": ["Chris\u00b7ti", "wohl\u00b7ge\u00b7pr\u00fcf\u00b7te", "Lie\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gegen seine L\u00e4mmerlein", "tokens": ["Ge\u00b7gen", "sei\u00b7ne", "L\u00e4m\u00b7mer\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fordert gleiche Liebes-Triebe:", "tokens": ["For\u00b7dert", "glei\u00b7che", "Lie\u00b7bes\u00b7Trie\u00b7be", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Er ist unser, wir sind sein.", "tokens": ["Er", "ist", "un\u00b7ser", ",", "wir", "sind", "sein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "$,", "PPER", "VAFIN", "VAINF", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Schafe wissen nichts von M\u00fch,", "tokens": ["Scha\u00b7fe", "wis\u00b7sen", "nichts", "von", "M\u00fch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "APPR", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Christus hebt und tr\u00e4get sie:", "tokens": ["Chris\u00b7tus", "hebt", "und", "tr\u00e4\u00b7get", "sie", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KON", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Seine ausgesuchte Heerde", "tokens": ["Sei\u00b7ne", "aus\u00b7ge\u00b7such\u00b7te", "Heer\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Fraget wenig nach der Erde.", "tokens": ["Fra\u00b7get", "we\u00b7nig", "nach", "der", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Christum lieben, lehrt die Weise,", "tokens": ["Chris\u00b7tum", "lie\u00b7ben", ",", "lehrt", "die", "Wei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVINF", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie man kl\u00fcglich handeln soll,", "tokens": ["Wie", "man", "kl\u00fcg\u00b7lich", "han\u00b7deln", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die gantze Himmels-Reise", "tokens": ["Und", "die", "gant\u00b7ze", "Him\u00b7mels\u00b7Rei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist der Liebe JEsu voll:", "tokens": ["Ist", "der", "Lie\u00b7be", "Je\u00b7su", "voll", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NE", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alle Weg und Stege sind", "tokens": ["Al\u00b7le", "Weg", "und", "Ste\u00b7ge", "sind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "NN", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Vor ein seligs Gnaden-Kind", "tokens": ["Vor", "ein", "se\u00b7ligs", "Gna\u00b7den\u00b7Kind"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Auf das Beste zubereitet,", "tokens": ["Auf", "das", "Bes\u00b7te", "zu\u00b7be\u00b7rei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df es ja nicht etwa gleidet.", "tokens": ["Da\u00df", "es", "ja", "nicht", "et\u00b7wa", "glei\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Christum lieben giebt die Masse,", "tokens": ["Chris\u00b7tum", "lie\u00b7ben", "giebt", "die", "Mas\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVINF", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie ich heilig leben mu\u00df:", "tokens": ["Wie", "ich", "hei\u00b7lig", "le\u00b7ben", "mu\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was ich thue, was ich lasse,", "tokens": ["Was", "ich", "thue", ",", "was", "ich", "las\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Lehrt sie mich in Uberflu\u00df,", "tokens": ["Lehrt", "sie", "mich", "in", "U\u00b7ber\u00b7flu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und wie weit ich Tag vor Tag", "tokens": ["Und", "wie", "weit", "ich", "Tag", "vor", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADJD", "PPER", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "In der Liebe wachsen mag:", "tokens": ["In", "der", "Lie\u00b7be", "wach\u00b7sen", "mag", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Alle gute Werck und Triebe", "tokens": ["Al\u00b7le", "gu\u00b7te", "Werck", "und", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "W\u00fcrckt die muntre JEsus-Liebe.", "tokens": ["W\u00fcrckt", "die", "mun\u00b7tre", "Je\u00b7sus\u00b7Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Christum lieben machet weiser,", "tokens": ["Chris\u00b7tum", "lie\u00b7ben", "ma\u00b7chet", "wei\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVINF", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn die alt-Erfahrnen sind:", "tokens": ["Denn", "die", "al\u00b7tEr\u00b7fahr\u00b7nen", "sind", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf die Liebe bau ich H\u00e4user", "tokens": ["Auf", "die", "Lie\u00b7be", "bau", "ich", "H\u00e4u\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gegen allen Sturm und Wind:", "tokens": ["Ge\u00b7gen", "al\u00b7len", "Sturm", "und", "Wind", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Christum lieben, ist gewi\u00df", "tokens": ["Chris\u00b7tum", "lie\u00b7ben", ",", "ist", "ge\u00b7wi\u00df"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "VVINF", "$,", "VAFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Satans gr\u00f6ste Hinderni\u00df;", "tokens": ["Sa\u00b7tans", "gr\u00f6s\u00b7te", "Hin\u00b7der\u00b7ni\u00df", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wo er Liebe Christi siehet,", "tokens": ["Wo", "er", "Lie\u00b7be", "Chris\u00b7ti", "sie\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da ists ausgemacht: Er fliehet.", "tokens": ["Da", "ists", "aus\u00b7ge\u00b7macht", ":", "Er", "flie\u00b7het", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "$.", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Christum lieben macht die Banden", "tokens": ["Chris\u00b7tum", "lie\u00b7ben", "macht", "die", "Ban\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVINF", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aller andern Liebe sest:", "tokens": ["Al\u00b7ler", "an\u00b7dern", "Lie\u00b7be", "sest", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber alles wird zu Schanden,", "tokens": ["A\u00b7ber", "al\u00b7les", "wird", "zu", "Schan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was sich hier nicht gr\u00fcnden l\u00e4st.", "tokens": ["Was", "sich", "hier", "nicht", "gr\u00fcn\u00b7den", "l\u00e4st", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ADV", "PTKNEG", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Christi Lieb in seiner Maa\u00df", "tokens": ["Chris\u00b7ti", "Lieb", "in", "sei\u00b7ner", "Maa\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Bringt uns wohl der Menschen Ha\u00df;", "tokens": ["Bringt", "uns", "wohl", "der", "Men\u00b7schen", "Ha\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Aber wer sich drein versencket,", "tokens": ["A\u00b7ber", "wer", "sich", "drein", "ver\u00b7sen\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PRF", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dem wird mancher Feind geschencket.", "tokens": ["Dem", "wird", "man\u00b7cher", "Feind", "ge\u00b7schen\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Christi Liebe, Einfalt, Wahrheit,", "tokens": ["Chris\u00b7ti", "Lie\u00b7be", ",", "Ein\u00b7falt", ",", "Wahr\u00b7heit", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der Bruder-Liebe Band,", "tokens": ["Und", "der", "Bru\u00b7der\u00b7Lie\u00b7be", "Band", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die bestehn in Krafft und Klarheit", "tokens": ["Die", "be\u00b7stehn", "in", "Krafft", "und", "Klar\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hier und auch im Vaterland.", "tokens": ["Hier", "und", "auch", "im", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "wo