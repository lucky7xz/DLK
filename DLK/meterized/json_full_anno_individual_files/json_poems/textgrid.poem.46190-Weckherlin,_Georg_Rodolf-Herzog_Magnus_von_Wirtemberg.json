{"textgrid.poem.46190": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Herzog Magnus von Wirtemberg", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbdemnach der himmel dich wegraufet aus misgunst", "tokens": ["\u00bb", "dem\u00b7nach", "der", "him\u00b7mel", "dich", "we\u00b7grau\u00b7fet", "aus", "mis\u00b7gunst"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PAV", "ART", "NN", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die deinethalb, o prinz, er wider mich getragen,", "tokens": ["die", "dei\u00b7net\u00b7halb", ",", "o", "prinz", ",", "er", "wi\u00b7der", "mich", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "FM", "FM", "$,", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "so folget dir numehr auch meines herzens brunst", "tokens": ["so", "fol\u00b7get", "dir", "nu\u00b7mehr", "auch", "mei\u00b7nes", "her\u00b7zens", "brunst"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und meiner augen flu\u00df und meines schmerzens klagen.", "tokens": ["und", "mei\u00b7ner", "au\u00b7gen", "flu\u00df", "und", "mei\u00b7nes", "schmer\u00b7zens", "kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "KON", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Vil gr\u00f6\u00dfer dan dein nam, war dein zu gro\u00dfes herz,", "tokens": ["Vil", "gr\u00f6\u00b7\u00dfer", "dan", "dein", "nam", ",", "war", "dein", "zu", "gro\u00b7\u00dfes", "herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "PPOSAT", "VVFIN", "$,", "VAFIN", "PPOSAT", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "als dem nichts lieber war, dan was schwer und gef\u00e4hrlich:", "tokens": ["als", "dem", "nichts", "lie\u00b7ber", "war", ",", "dan", "was", "schwer", "und", "ge\u00b7f\u00e4hr\u00b7lich", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PIS", "ADV", "VAFIN", "$,", "ADV", "PWS", "ADJD", "KON", "ADJD", "$."], "meter": "---+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "daher mir dan dein fall und mein verlust und schmerz", "tokens": ["da\u00b7her", "mir", "dan", "dein", "fall", "und", "mein", "ver\u00b7lust", "und", "schmerz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "ADV", "PPOSAT", "NN", "KON", "PPOSAT", "VVFIN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "um so vil desto mehr empfindlich und beschwerlich.", "tokens": ["um", "so", "vil", "des\u00b7to", "mehr", "emp\u00b7find\u00b7lich", "und", "be\u00b7schwer\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ADV", "ADV", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "Darum bewein ich stets geb\u00fcrlich deinen tod", "tokens": ["Da\u00b7rum", "be\u00b7wein", "ich", "stets", "ge\u00b7b\u00fcr\u00b7lich", "dei