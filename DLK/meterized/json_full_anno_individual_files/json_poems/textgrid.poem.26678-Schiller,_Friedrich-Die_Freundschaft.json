{"textgrid.poem.26678": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Freundschaft", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freund! gen\u00fcgsam ist der Wesenlenker \u2013", "tokens": ["Freund", "!", "ge\u00b7n\u00fcg\u00b7sam", "ist", "der", "We\u00b7sen\u00b7len\u00b7ker", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADJD", "VAFIN", "ART", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sch\u00e4men sich kleinmeisterische Denker,", "tokens": ["Sch\u00e4\u00b7men", "sich", "klein\u00b7meis\u00b7te\u00b7ri\u00b7sche", "Den\u00b7ker", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJA", "NN", "$,"], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Die so \u00e4ngstlich nach Gesetzen sp\u00e4hn \u2013", "tokens": ["Die", "so", "\u00e4ngst\u00b7lich", "nach", "Ge\u00b7set\u00b7zen", "sp\u00e4hn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "APPR", "NN", "VVINF", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Geisterreich und K\u00f6rperweltgew\u00fchle", "tokens": ["Geis\u00b7ter\u00b7reich", "und", "K\u00f6r\u00b7per\u00b7welt\u00b7ge\u00b7w\u00fch\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "W\u00e4lzet ", "tokens": ["W\u00e4l\u00b7zet"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.2": {"line.1": {"text": "Um das Herz des gro\u00dfen Weltenraumes", "tokens": ["Um", "das", "Herz", "des", "gro\u00b7\u00dfen", "Wel\u00b7ten\u00b7rau\u00b7mes"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Labyrinthenbahnen ziehn \u2013", "tokens": ["La\u00b7by\u00b7rin\u00b7then\u00b7bah\u00b7nen", "ziehn", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nach ", "tokens": ["Nach"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Wie zum Meere B\u00e4che fliehn.", "tokens": ["Wie", "zum", "Mee\u00b7re", "B\u00e4\u00b7che", "fliehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wars nicht dies allm\u00e4chtige Getriebe,", "tokens": ["Wars", "nicht", "dies", "all\u00b7m\u00e4ch\u00b7ti\u00b7ge", "Ge\u00b7trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "PDS", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Das zum ewgen Jubelbund der Liebe", "tokens": ["Das", "zum", "ew\u00b7gen", "Ju\u00b7bel\u00b7bund", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "APPRART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Raphael, an ", "tokens": ["Ra\u00b7phael", ",", "an"], "token_info": ["word", "punct", "word"], "pos": ["NE", "$,", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Wag auch ich zur gro\u00dfen Geistersonne", "tokens": ["Wag", "auch", "ich", "zur", "gro\u00b7\u00dfen", "Geis\u00b7ter\u00b7son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "PPER", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Freudigmutig den Vollendungsgang.", "tokens": ["Freu\u00b7dig\u00b7mu\u00b7tig", "den", "Vol\u00b7len\u00b7dungs\u00b7gang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Gl\u00fccklich! gl\u00fccklich! ", "tokens": ["Gl\u00fcck\u00b7lich", "!", "gl\u00fcck\u00b7lich", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$.", "ADJD", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Hab aus Millionen ", "tokens": ["Hab", "aus", "Mil\u00b7lion\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und aus Millionen ", "tokens": ["Und", "aus", "Mil\u00b7lion\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "La\u00df das Chaos diese Welt umr\u00fctteln,", "tokens": ["La\u00df", "das", "Chaos", "die\u00b7se", "Welt", "um\u00b7r\u00fct\u00b7teln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PDAT", "NN", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Durcheinander die Atomen sch\u00fctteln:", "tokens": ["Durch\u00b7ein\u00b7an\u00b7der", "die", "A\u00b7to\u00b7men", "sch\u00fct\u00b7teln", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Ewig fliehn sich unsre Herzen zu.", "tokens": ["E\u00b7wig", "fliehn", "sich", "uns\u00b7re", "Her\u00b7zen", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Mu\u00df ich nicht aus ", "tokens": ["Mu\u00df", "ich", "nicht", "aus"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PTKNEG", "PTKVZ"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Nur in ", "tokens": ["Nur", "in"], "token_info": ["word", "word"], "pos": ["ADV", "APPR"], "meter": "++", "measure": "spondeus"}, "line.3": {"text": "Sch\u00f6ner malt sich mir die sch\u00f6ne Erde,", "tokens": ["Sch\u00f6\u00b7ner", "malt", "sich", "mir", "die", "sch\u00f6\u00b7ne", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Heller spiegelt in des Freunds Geb\u00e4rde,", "tokens": ["Hel\u00b7ler", "spie\u00b7gelt", "in", "des", "Freunds", "Ge\u00b7b\u00e4r\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Reizender der Himmel sich.", "tokens": ["Rei\u00b7zen\u00b7der", "der", "Him\u00b7mel", "sich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Schwermut wirft die bange Tr\u00e4nenlasten,", "tokens": ["Schwer\u00b7mut", "wirft", "die", "ban\u00b7ge", "Tr\u00e4\u00b7nen\u00b7las\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "S\u00fc\u00dfer von des Leidens Sturm zu rasten,", "tokens": ["S\u00fc\u00b7\u00dfer", "von", "des", "Lei\u00b7dens", "Sturm", "zu", "ras\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "In der Liebe Busen ab; \u2013", "tokens": ["In", "der", "Lie\u00b7be", "Bu\u00b7sen", "ab", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sucht nicht selbst das folternde Entz\u00fccken", "tokens": ["Sucht", "nicht", "selbst", "das", "fol\u00b7tern\u00b7de", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.5": {"text": "In des Freunds beredten Strahlenblicken", "tokens": ["In", "des", "Freunds", "be\u00b7red\u00b7ten", "Strah\u00b7len\u00b7bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Ungeduldig ein woll\u00fcstges Grab? \u2013", "tokens": ["Un\u00b7ge\u00b7dul\u00b7dig", "ein", "wol\u00b7l\u00fcst\u00b7ges", "Grab", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "St\u00fcnd im All der Sch\u00f6pfung ich alleine,", "tokens": ["St\u00fcnd", "im", "All", "der", "Sch\u00f6p\u00b7fung", "ich", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Seelen tr\u00e4umt' ich in die Felsensteine", "tokens": ["See\u00b7len", "tr\u00e4umt'", "ich", "in", "die", "Fel\u00b7sen\u00b7stei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und umarmend k\u00fc\u00dft' ich sie \u2013", "tokens": ["Und", "um\u00b7ar\u00b7mend", "k\u00fc\u00dft'", "ich", "sie", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VVFIN", "PPER", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine Klagen st\u00f6hnt' ich in die L\u00fcfte,", "tokens": ["Mei\u00b7ne", "Kla\u00b7gen", "st\u00f6hnt'", "ich", "in", "die", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Freute mich, antworteten die Kl\u00fcfte,", "tokens": ["Freu\u00b7te", "mich", ",", "ant\u00b7wor\u00b7te\u00b7ten", "die", "Kl\u00fcf\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.6": {"text": "Tor genug! der s\u00fc\u00dfen Sympathie.", "tokens": ["Tor", "ge\u00b7nug", "!", "der", "s\u00fc\u00b7\u00dfen", "Sym\u00b7pa\u00b7thie", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$.", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Tote Gruppen sind wir \u2013 wenn wir hassen,", "tokens": ["To\u00b7te", "Grup\u00b7pen", "sind", "wir", "\u2013", "wenn", "wir", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPER", "$(", "KOUS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "G\u00f6tter \u2013 wenn wir liebend uns umfassen!", "tokens": ["G\u00f6t\u00b7ter", "\u2013", "wenn", "wir", "lie\u00b7bend", "uns", "um\u00b7fas\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOUS", "PPER", "ADJD", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Lechzen nach dem s\u00fc\u00dfen Fesselzwang \u2013", "tokens": ["Lech\u00b7zen", "nach", "dem", "s\u00fc\u00b7\u00dfen", "Fes\u00b7sel\u00b7zwang", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Aufw\u00e4rts durch die tausendfache Stufen", "tokens": ["Auf\u00b7w\u00e4rts", "durch", "die", "tau\u00b7send\u00b7fa\u00b7che", "Stu\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Zahlenloser Geister, die nicht schufen,", "tokens": ["Zah\u00b7len\u00b7lo\u00b7ser", "Geis\u00b7ter", ",", "die", "nicht", "schu\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Waltet g\u00f6ttlich dieser Drang.", "tokens": ["Wal\u00b7tet", "g\u00f6tt\u00b7lich", "die\u00b7ser", "Drang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Arm in Arme, h\u00f6her stets und h\u00f6her,", "tokens": ["Arm", "in", "Ar\u00b7me", ",", "h\u00f6\u00b7her", "stets", "und", "h\u00f6\u00b7her", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "ADJD", "ADV", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Vom Mongolen bis zum griechschen Seher,", "tokens": ["Vom", "Mon\u00b7go\u00b7len", "bis", "zum", "griech\u00b7schen", "Se\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "APPRART", "ADJA", "NN", "$,"], "meter": "----+-+-+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Der sich an den letzten Seraph reiht,", "tokens": ["Der", "sich", "an", "den", "letz\u00b7ten", "Se\u00b7raph", "reiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wallen wir, einm\u00fctgen Ringeltanzes,", "tokens": ["Wal\u00b7len", "wir", ",", "ein\u00b7m\u00fct\u00b7gen", "Rin\u00b7gel\u00b7tan\u00b7zes", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Bis sich dort im Meer des ewgen Glanzes", "tokens": ["Bis", "sich", "dort", "im", "Meer", "des", "ew\u00b7gen", "Glan\u00b7zes"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRF", "ADV", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Sterbend untertauchen Ma\u00df und Zeit. \u2013", "tokens": ["Ster\u00b7bend", "un\u00b7ter\u00b7tau\u00b7chen", "Ma\u00df", "und", "Zeit", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "ADJA", "NN", "KON", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Freundlos war der gro\u00dfe Weltenmeister,", "tokens": ["Freund\u00b7los", "war", "der", "gro\u00b7\u00dfe", "Wel\u00b7ten\u00b7meis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "F\u00fchlte ", "tokens": ["F\u00fchl\u00b7te"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Selge Spiegel ", "tokens": ["Sel\u00b7ge", "Spie\u00b7gel"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Fand das h\u00f6chste Wesen schon kein gleiches,", "tokens": ["Fand", "das", "h\u00f6chs\u00b7te", "We\u00b7sen", "schon", "kein", "glei\u00b7ches", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADV", "PIAT", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Aus dem Kelch des ganzen Seelenreiches", "tokens": ["Aus", "dem", "Kelch", "des", "gan\u00b7zen", "See\u00b7len\u00b7rei\u00b7ches"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Sch\u00e4umt ", "tokens": ["Sch\u00e4umt"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}}, "stanza.11": {"line.1": {"text": "Freund! gen\u00fcgsam ist der Wesenlenker \u2013", "tokens": ["Freund", "!", "ge\u00b7n\u00fcg\u00b7sam", "ist", "der", "We\u00b7sen\u00b7len\u00b7ker", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADJD", "VAFIN", "ART", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sch\u00e4men sich kleinmeisterische Denker,", "tokens": ["Sch\u00e4\u00b7men", "sich", "klein\u00b7meis\u00b7te\u00b7ri\u00b7sche", "Den\u00b7ker", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", 