, "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "PPER", "ADV", "$,", "APPR", "NN"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.16": {"text": "Und mit Ehrfurcht, ernstlich nach. Letztlich kam ich von dem Vieh", "tokens": ["Und", "mit", "Ehr\u00b7furcht", ",", "ernst\u00b7lich", "nach", ".", "Letzt\u00b7lich", "kam", "ich", "von", "dem", "Vieh"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "$,", "ADJD", "PTKVZ", "$.", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "--+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Gar auf Menschen Stimm' und T\u00f6ne, lautes Ruffen, und Geschrey,", "tokens": ["Gar", "auf", "Men\u00b7schen", "Stimm'", "und", "T\u00f6\u00b7ne", ",", "lau\u00b7tes", "Ruf\u00b7fen", ",", "und", "Ge\u00b7schrey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "NN", "KON", "NN", "$,", "ADJA", "NN", "$,", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.18": {"text": "Doch insonderheit aufs Beten, welches laut von uns geschicht,", "tokens": ["Doch", "in\u00b7son\u00b7der\u00b7heit", "aufs", "Be\u00b7ten", ",", "wel\u00b7ches", "laut", "von", "uns", "ge\u00b7schicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "$,", "PRELS", "ADJD", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.19": {"text": "Wenn uns etwan H\u00fclfe n\u00f6thig, wenn uns etwas hier gebricht.", "tokens": ["Wenn", "uns", "et\u00b7wan", "H\u00fcl\u00b7fe", "n\u00f6\u00b7thig", ",", "wenn", "uns", "et\u00b7was", "hier", "ge\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "wo