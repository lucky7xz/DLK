{"textgrid.poem.40031": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Neulich rannt' ein grosser Hund, mit erb\u00e4rmlichem Geschrey,", "genre": "verse", "period": "N.A.", "pub_year": 1713, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Neulich rannt' ein grosser Hund, mit erb\u00e4rmlichem Geschrey,", "tokens": ["Neu\u00b7lich", "rannt'", "ein", "gros\u00b7ser", "Hund", ",", "mit", "er\u00b7b\u00e4rm\u00b7li\u00b7chem", "Ge\u00b7schrey", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Weil man ihn geschlagen hatte, Sporenstreichs mein Haus vorbey,", "tokens": ["Weil", "man", "ihn", "ge\u00b7schla\u00b7gen", "hat\u00b7te", ",", "Spo\u00b7rens\u00b7treichs", "mein", "Haus", "vor\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVPP", "VAFIN", "$,", "NN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Als ich an der Th\u00fcre stand. Dieser laute Ton durchdrang", "tokens": ["Als", "ich", "an", "der", "Th\u00fc\u00b7re", "stand", ".", "Die\u00b7ser", "lau\u00b7te", "Ton", "durch\u00b7drang"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$.", "PDAT", "ADJA", "NN", "VVFIN"], "meter": "--+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Nicht nur mein beleidigt Ohr, sondern der zu scharfe Klang", "tokens": ["Nicht", "nur", "mein", "be\u00b7lei\u00b7digt", "Ohr", ",", "son\u00b7dern", "der", "zu", "schar\u00b7fe", "Klang"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "PPOSAT", "VVFIN", "NN", "$,", "KON", "ART", "PTKZU", "ADJA", "NN"], "meter": "+-+-+-++-+-+-+", "measure": "unknown.measure.octa.plus"}, "line.5": {"text": "Drang mir durchs Geh\u00f6r ins Hertz. Da ich denn bewunderte,", "tokens": ["Drang", "mir", "durchs", "Ge\u00b7h\u00f6r", "ins", "Hertz", ".", "Da", "ich", "denn", "be\u00b7wun\u00b7der\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "APPRART", "NN", "$.", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "+---+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Wie, durch wunderbare Wege, die Natur so gar den Thieren,", "tokens": ["Wie", ",", "durch", "wun\u00b7der\u00b7ba\u00b7re", "We\u00b7ge", ",", "die", "Na\u00b7tur", "so", "gar", "den", "Thie\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "APPR", "ADJA", "NN", "$,", "ART", "NN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Wenn sie Ungemach und Weh,", "tokens": ["Wenn", "sie", "Un\u00b7ge\u00b7mach", "und", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Welches ihren C\u00f6rpern sch\u00e4dlich, und beschwehrlich ist, versp\u00fchren,", "tokens": ["Wel\u00b7ches", "ih\u00b7ren", "C\u00f6r\u00b7pern", "sch\u00e4d\u00b7lich", ",", "und", "be\u00b7schwehr\u00b7lich", "ist", ",", "ver\u00b7sp\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "ADJD", "$,", "KON", "ADJD", "VAFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Nicht nur einen Trieb zu schreyen, sondern Werck-Zeug' ihnen schenckt,", "tokens": ["Nicht", "nur", "ei\u00b7nen", "Trieb", "zu", "schre\u00b7yen", ",", "son\u00b7dern", "Wer\u00b7ck\u00b7Zeug'", "ih\u00b7nen", "schenckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "PTKZU", "VVINF", "$,", "KON", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+--+", "measure": "iambic.octa.plus.octa.plus.chol"}, "line.10": {"text": "Wodurch laute T\u00f6n' erreget, und wir zur Aufmercksamkeit,", "tokens": ["Wo\u00b7durch", "lau\u00b7te", "T\u00f6n'", "er\u00b7re\u00b7get", ",", "und", "wir", "zur", "Auf\u00b7merck\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "NN", "VVFIN", "$,", "KON", "PPER", "APPRART", "NN", "$,"], "meter": "--+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Ja zum Mittleid, wenigstens zur Verdrie\u00dflichkeit, gelencket,", "tokens": ["Ja", "zum", "Mitt\u00b7leid", ",", "we\u00b7nigs\u00b7tens", "zur", "Ver\u00b7drie\u00df\u00b7lich\u00b7keit", ",", "ge\u00b7len\u00b7cket", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKANT", "APPRART", "NN", "$,", "ADV", "APPRART", "NN", "$,", "VVPP", "$,"], "meter": "+-+-+-++-+-+-+-", "measure": "unknown.measure.octa.plus"}, "line.12": {"text": "Ein so w\u00fcst Geschrey zu h\u00f6ren, wodurch sie denn oft befreyt.", "tokens": ["Ein", "so", "w\u00fcst", "Ge\u00b7schrey", "zu", "h\u00f6\u00b7ren", ",", "wo\u00b7durch", "sie", "denn", "oft", "be\u00b7freyt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "NN", "PTKZU", "VVINF", "$,", "PWAV", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.13": {"text": "Bald aus Mitleid zu uns selbst, bald aus Mitleid gegen sie.", "tokens": ["Bald", "aus", "Mit\u00b7leid", "zu", "uns", "selbst", ",", "bald", "aus", "Mit\u00b7leid", "ge\u00b7gen", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "APPR", "PPER", "ADV", "$,", "ADV", "APPR", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.14": {"text": "Diesem Wunder in den T\u00f6nen, und den herrlichen Gesetzen", "tokens": ["Die\u00b7sem", "Wun\u00b7der", "in", "den", "T\u00f6\u00b7nen", ",", "und", "den", "herr\u00b7li\u00b7chen", "Ge\u00b7set\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "APPR", "ART", "NN", "$,", "KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.15": {"text": "Der verst\u00e4ndigen Natur, dacht' ich ferner, mit Erg\u00f6tzen", "tokens": ["Der", "ver\u00b7st\u00e4n\u00b7di\u00b7gen", "Na\u00b7tur", ",", "dacht'", "ich", "fer\u00b7ner", ",", "mit", "Er\u00b7g\u00f6t\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "PPER", "ADV", "$,", "APPR", "NN"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.16": {"text": "Und mit Ehrfurcht, ernstlich nach. Letztlich kam ich von dem Vieh", "tokens": ["Und", "mit", "Ehr\u00b7furcht", ",", "ernst\u00b7lich", "nach", ".", "Letzt\u00b7lich", "kam", "ich", "von", "dem", "Vieh"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "$,", "ADJD", "PTKVZ", "$.", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "--+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Gar auf Menschen Stimm' und T\u00f6ne, lautes Ruffen, und Geschrey,", "tokens": ["Gar", "auf", "Men\u00b7schen", "Stimm'", "und", "T\u00f6\u00b7ne", ",", "lau\u00b7tes", "Ruf\u00b7fen", ",", "und", "Ge\u00b7schrey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "NN", "KON", "NN", "$,", "ADJA", "NN", "$,", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.18": {"text": "Doch insonderheit aufs Beten, welches laut von uns geschicht,", "tokens": ["Doch", "in\u00b7son\u00b7der\u00b7heit", "aufs", "Be\u00b7ten", ",", "wel\u00b7ches", "laut", "von", "uns", "ge\u00b7schicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "$,", "PRELS", "ADJD", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.19": {"text": "Wenn uns etwan H\u00fclfe n\u00f6thig, wenn uns etwas hier gebricht.", "tokens": ["Wenn", "uns", "et\u00b7wan", "H\u00fcl\u00b7fe", "n\u00f6\u00b7thig", ",", "wenn", "uns", "et\u00b7was", "hier", "ge\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "ADJD", "$,", "KOUS", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.20": {"text": "Da\u00df nun die\u00df in Ansehn Gottes unn\u00fctz, \u00fcberfl\u00fc\u00dfig sey,", "tokens": ["Da\u00df", "nun", "die\u00df", "in", "An\u00b7sehn", "Got\u00b7tes", "un\u00b7n\u00fctz", ",", "\u00fc\u00b7berf\u00b7l\u00fc\u00b7\u00dfig", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PDS", "APPR", "NN", "NN", "ADJD", "$,", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.21": {"text": "Meynt' ich damahls; glaub' auch noch. Weil dem Sch\u00f6pfer, was uns fehlet,", "tokens": ["Meynt'", "ich", "da\u00b7mahls", ";", "glaub'", "auch", "noch", ".", "Weil", "dem", "Sch\u00f6p\u00b7fer", ",", "was", "uns", "feh\u00b7let", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$.", "VVFIN", "ADV", "ADV", "$.", "KOUS", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.22": {"text": "Was uns n\u00fctzlich, was uns n\u00f6thig, uns erfreut, und was uns qu\u00e4let", "tokens": ["Was", "uns", "n\u00fctz\u00b7lich", ",", "was", "uns", "n\u00f6\u00b7thig", ",", "uns", "er\u00b7freut", ",", "und", "was", "uns", "qu\u00e4\u00b7let"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADJD", "$,", "PRELS", "PPER", "ADJD", "$,", "PPER", "VVFIN", "$,", "KON", "PWS", "PPER", "VVFIN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.23": {"text": "Besser, als uns selbst, bekannt. Er auch minder nicht, nicht mehr,", "tokens": ["Bes\u00b7ser", ",", "als", "uns", "selbst", ",", "be\u00b7kannt", ".", "Er", "auch", "min\u00b7der", "nicht", ",", "nicht", "mehr", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "PPER", "ADV", "$,", "ADJD", "$.", "PPER", "ADV", "ADV", "PTKNEG", "$,", "PTKNEG", "ADV", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.24": {"text": "Durch's Geschrey, beweget wird. Wenn jedoch dadurch nicht nur", "tokens": ["Durch's", "Ge\u00b7schrey", ",", "be\u00b7we\u00b7get", "wird", ".", "Wenn", "je\u00b7doch", "da\u00b7durch", "nicht", "nur"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "VVFIN", "VAFIN", "$.", "KOUS", "ADV", "PAV", "PTKNEG", "ADV"], "meter": "+-+-+-+-+--+-+", "measure": "trochaic.septa.relaxed"}, "line.25": {"text": "Andre Menschen, sondern auch noch wohl manche Creatur,", "tokens": ["And\u00b7re", "Men\u00b7schen", ",", "son\u00b7dern", "auch", "noch", "wohl", "man\u00b7che", "Crea\u00b7tur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "ADV", "ADV", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.26": {"text": "Engel oder andre Geister, dem Allm\u00e4chtigen zur Ehr',", "tokens": ["En\u00b7gel", "o\u00b7der", "and\u00b7re", "Geis\u00b7ter", ",", "dem", "All\u00b7m\u00e4ch\u00b7ti\u00b7gen", "zur", "Ehr'", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJA", "NN", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+---+-+-+", "measure": "unknown.measure.septa"}, "line.27": {"text": "(von der \u00e4usserlichen Andacht, auch aufs Innere zu schliessen,", "tokens": ["(", "von", "der", "\u00e4us\u00b7ser\u00b7li\u00b7chen", "An\u00b7dacht", ",", "auch", "aufs", "In\u00b7ne\u00b7re", "zu", "schlies\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "ADJA", "NN", "$,", "ADV", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.28": {"text": "Und wie etwa wir zuweilen, durch der Nachtigallen singen", "tokens": ["Und", "wie", "et\u00b7wa", "wir", "zu\u00b7wei\u00b7len", ",", "durch", "der", "Nach\u00b7ti\u00b7gal\u00b7len", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADV", "PPER", "ADV", "$,", "APPR", "ART", "NN", "VVINF"], "meter": "----+-+-+-+-+-+-", "measure": "unknown.measure.hexa"}, "line.29": {"text": "Uns ger\u00fchret sehn) dem Sch\u00f6pfer auch ein Lob-Lied mitzubringen,", "tokens": ["Uns", "ge\u00b7r\u00fch\u00b7ret", "sehn", ")", "dem", "Sch\u00f6p\u00b7fer", "auch", "ein", "Lob\u00b7Lied", "mit\u00b7zu\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "VVINF", "$(", "ART", "NN", "ADV", "ART", "NN", "VVIZU", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.30": {"text": "Angetrieben werden k\u00f6nnen: Ja so gar die Eigenschaft", "tokens": ["An\u00b7ge\u00b7trie\u00b7ben", "wer\u00b7den", "k\u00f6n\u00b7nen", ":", "Ja", "so", "gar", "die", "Ei\u00b7gen\u00b7schaft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VAINF", "VMFIN", "$.", "PTKANT", "ADV", "ADV", "ART", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.31": {"text": "Unsers menschlichen Gem\u00fcths diese wircklich scheint zu seyn;", "tokens": ["Un\u00b7sers", "menschli\u00b7chen", "Ge\u00b7m\u00fcths", "die\u00b7se", "wir\u00b7ck\u00b7lich", "scheint", "zu", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PDAT", "ADJD", "VVFIN", "PTKZU", "VAINF", "$."], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.32": {"text": "Da\u00df auch selber, wenn wir Beten, selbst-gesprochner W\u00f6rter Kraft,", "tokens": ["Da\u00df", "auch", "sel\u00b7ber", ",", "wenn", "wir", "Be\u00b7ten", ",", "selbst\u00b7ge\u00b7sproch\u00b7ner", "W\u00f6r\u00b7ter", "Kraft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "$,", "KOUS", "PPER", "NN", "$,", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.33": {"text": "Sonderlich wenns laut geschehn, wircklich sich so weit erstrecket", "tokens": ["Son\u00b7der\u00b7lich", "wenns", "laut", "ge\u00b7schehn", ",", "wir\u00b7ck\u00b7lich", "sich", "so", "weit", "er\u00b7stre\u00b7cket"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ADJD", "VVPP", "$,", "ADJD", "PRF", "ADV", "ADJD", "VVFIN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.34": {"text": "Da\u00df die Andacht noch vermehrt, da\u00df der Geist dadurch erwecket", "tokens": ["Da\u00df", "die", "An\u00b7dacht", "noch", "ver\u00b7mehrt", ",", "da\u00df", "der", "Geist", "da\u00b7durch", "er\u00b7we\u00b7cket"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "ADJD", "$,", "KOUS", "ART", "NN", "PAV", "VVFIN"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.35": {"text": "Das Vertrauen st\u00e4rcker wird; kann man klar daraus ersehen,", "tokens": ["Das", "Ver\u00b7trau\u00b7en", "st\u00e4r\u00b7cker", "wird", ";", "kann", "man", "klar", "da\u00b7raus", "er\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VAFIN", "$.", "VMFIN", "PIS", "ADJD", "PAV", "VVINF", "$,"], "meter": "--+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.36": {"text": "Da\u00df ein lauter Gottes-Dienst n\u00fctzlich, gut und n\u00f6thig sey,", "tokens": ["Da\u00df", "ein", "lau\u00b7ter", "Got\u00b7tes\u00b7Dienst", "n\u00fctz\u00b7lich", ",", "gut", "und"