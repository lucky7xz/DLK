, "an", "der", "Th\u00fc\u00b7re", "stand", ".", "Die\u00b7ser", "lau\u00b7te", "Ton", "durch\u00b7drang"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$.", "PDAT", "ADJA", "NN", "VVFIN"], "meter": "--+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Nicht nur mein beleidigt Ohr, sondern der zu scharfe Klang", "tokens": ["Nicht", "nur", "mein", "be\u00b7lei\u00b7digt", "Ohr", ",", "son\u00b7dern", "der", "zu", "schar\u00b7fe", "Klang"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "PPOSAT", "VVFIN", "NN", "$,", "KON", "ART", "PTKZU", "ADJA", "NN"], "meter": "+-+-+-++-+-+-+", "measure": "unknown.measure.octa.plus"}, "line.5": {"text": "Drang mir durchs Geh\u00f6r ins Hertz. Da ich denn bewunderte,", "tokens": ["Drang", "mir", "durchs", "Ge\u00b7h\u00f6r", "ins", "Hertz", ".", "Da", "ich", "denn", "be\u00b7wun\u00b7der\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "APPRART", "NN", "$.", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "+---+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Wie, durch wunderbare Wege, die Natur so gar den Thieren,", "tokens": ["Wie", ",", "durch", "wun\u00b7der\u00b7ba\u00b7re", "We\u00b7ge", ",", "die", "Na\u00b7tur", "so", "gar", "den", "Thie\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "APPR", "ADJA", "NN", "$,", "ART", "NN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Wenn sie Ungemach und Weh,", "tokens": ["Wenn", "sie", "Un\u00b7ge\u00b7mach", "und", "Weh", ","], "token_info": ["word", "w