{"dta.poem.17222": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "72.  Den Br\u00e4utgam w\u00fcnscht die Braut.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Verwundere dich nicht da\u00df ich nach GOtt verlange:", "tokens": ["Ver\u00b7wun\u00b7de\u00b7re", "dich", "nicht", "da\u00df", "ich", "nach", "Gott", "ver\u00b7lan\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "KOUS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Braut ist allezeit nach jhrem Br\u00e4utgam bange.", "tokens": ["Der", "Braut", "ist", "al\u00b7le\u00b7zeit", "nach", "jhrem", "Br\u00e4ut\u00b7gam", "ban\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "PPOSAT", "NE", "VVFIN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}}}}