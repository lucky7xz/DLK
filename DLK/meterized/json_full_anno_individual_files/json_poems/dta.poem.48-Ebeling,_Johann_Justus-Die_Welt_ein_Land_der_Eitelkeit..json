{"dta.poem.48": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die Welt ein Land der Eitelkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198797", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer diese Welt ansieht, das Haupt\nGesch\u00f6pf erweget,", "tokens": ["Wer", "die\u00b7se", "Welt", "an\u00b7sieht", ",", "das", "Haupt", "Ge\u00b7sch\u00f6pf", "er\u00b7we\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "VVFIN", "$,", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Menschen und sein Thun sich", "tokens": ["Den", "Men\u00b7schen", "und", "sein", "Thun", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "PPOSAT", "NN", "PRF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der findet alsobald, wie wahr der", "tokens": ["Der", "fin\u00b7det", "al\u00b7so\u00b7bald", ",", "wie", "wahr", "der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "$,", "PWAV", "ADJD", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der ihre Eitelkeit in seiner Schrift beklagt.", "tokens": ["Der", "ih\u00b7re", "Ei\u00b7tel\u00b7keit", "in", "sei\u00b7ner", "Schrift", "be\u00b7klagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Erden Wunderbau ist herrlich ausgezieret,", "tokens": ["Der", "Er\u00b7den", "Wun\u00b7der\u00b7bau", "ist", "herr\u00b7lich", "aus\u00b7ge\u00b7zie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Den eine ewge Macht nach weisen Rath regieret,", "tokens": ["Den", "ei\u00b7ne", "ew\u00b7ge", "Macht", "nach", "wei\u00b7sen", "Rath", "re\u00b7gie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Zur Wohnung und zur Lust ist er bequem gemacht,", "tokens": ["Zur", "Woh\u00b7nung", "und", "zur", "Lust", "ist", "er", "be\u00b7quem", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und zur Erg\u00f6zlichkeit f\u00fcr uns recht ausgedacht.", "tokens": ["Und", "zur", "Er\u00b7g\u00f6z\u00b7lich\u00b7keit", "f\u00fcr", "uns", "recht", "aus\u00b7ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "APPR", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Sinnen finden hier die allersch\u00f6nste Weide,", "tokens": ["Die", "Sin\u00b7nen", "fin\u00b7den", "hier", "die", "al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "Wei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die Augen sehen stets Gelegenheit zur Freude,", "tokens": ["Die", "Au\u00b7gen", "se\u00b7hen", "stets", "Ge\u00b7le\u00b7gen\u00b7heit", "zur", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Das Ohr das wird erquikt durch manchen Zauber-", "tokens": ["Das", "Ohr", "das", "wird", "er\u00b7quikt", "durch", "man\u00b7chen", "Zau\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PDS", "VAFIN", "VVPP", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Durch einen lieblichen vermischten Lustgesang,", "tokens": ["Durch", "ei\u00b7nen", "lieb\u00b7li\u00b7chen", "ver\u00b7mischten", "Lust\u00b7ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Wenn in der Fr\u00fchlings-Zeit, im Sommer ganze", "tokens": ["Wenn", "in", "der", "Fr\u00fch\u00b7lings\u00b7Zeit", ",", "im", "Som\u00b7mer", "gan\u00b7ze"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "$,", "APPRART", "NN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Von V\u00f6geln in der Luft mit hellen Stimmen fah-", "tokens": ["Von", "V\u00f6\u00b7geln", "in", "der", "Luft", "mit", "hel\u00b7len", "Stim\u00b7men", "fah"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Der s\u00fcsse Blumen-Duft, der in den L\u00fcften fliegt,", "tokens": ["Der", "s\u00fcs\u00b7se", "Blu\u00b7men\u00b7Duft", ",", "der", "in", "den", "L\u00fcf\u00b7ten", "fliegt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Vergn\u00fcget das Gehirn, wenn man denselben", "tokens": ["Ver\u00b7gn\u00fc\u00b7get", "das", "Ge\u00b7hirn", ",", "wenn", "man", "den\u00b7sel\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "KOUS", "PIS", "PDAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Die Zunge labet sich an saftig s\u00fcssen Fr\u00fcchten,", "tokens": ["Die", "Zun\u00b7ge", "la\u00b7bet", "sich", "an", "saf\u00b7tig", "s\u00fcs\u00b7sen", "Fr\u00fcch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und b\u00fcssen ihre Lust an mancherlei Gerichten.", "tokens": ["Und", "b\u00fcs\u00b7sen", "ih\u00b7re", "Lust", "an", "man\u00b7cher\u00b7lei", "Ge\u00b7rich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Allein so reizend sch\u00f6n, dies unsern Sinnen ist;", "tokens": ["Al\u00b7lein", "so", "rei\u00b7zend", "sch\u00f6n", ",", "dies", "un\u00b7sern", "Sin\u00b7nen", "ist", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "ADJD", "$,", "PDS", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "So bleibet dennoch wahr, was man im Worte liest.", "tokens": ["So", "blei\u00b7bet", "den\u00b7noch", "wahr", ",", "was", "man", "im", "Wor\u00b7te", "liest", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADJD", "$,", "PRELS", "PIS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}, "line.22": {"text": "An jeder Kreatur, an jedem Dinge lesen.", "tokens": ["An", "je\u00b7der", "Kre\u00b7a\u00b7tur", ",", "an", "je\u00b7dem", "Din\u00b7ge", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Es ist die Welt ein Land das voller Eitelkeit,", "tokens": ["Es", "ist", "die", "Welt", "ein", "Land", "das", "vol\u00b7ler", "Ei\u00b7tel\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Warum? es fehlt uns hier Ruh und Zufriedenheit.", "tokens": ["Wa\u00b7rum", "?", "es", "fehlt", "uns", "hier", "Ruh", "und", "Zu\u00b7frie\u00b7den\u00b7heit", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PPER", "VVFIN", "PPER", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "Das Aug ist nimmer satt, es will in seinem Se-", "tokens": ["Das", "Aug", "ist", "nim\u00b7mer", "satt", ",", "es", "will", "in", "sei\u00b7nem", "Se"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,", "PPER", "VMFIN", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Jemehr es immer sieht, doch immer weiter gehen.", "tokens": ["Je\u00b7mehr", "es", "im\u00b7mer", "sieht", ",", "doch", "im\u00b7mer", "wei\u00b7ter", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$,", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "So gehts mit jedem Sinn, wenn er Vergn\u00fcgen", "tokens": ["So", "gehts", "mit", "je\u00b7dem", "Sinn", ",", "wenn", "er", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PIAT", "NN", "$,", "KOUS", "PPER", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "So wird er nicht gestillt, vielmehr dadurch erwekt", "tokens": ["So", "wird", "er", "nicht", "ge\u00b7stillt", ",", "viel\u00b7mehr", "da\u00b7durch", "er\u00b7wekt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,", "ADV", "PAV", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Und gleichsam angeflammt, dasjenige zu haben,", "tokens": ["Und", "gleich\u00b7sam", "an\u00b7ge\u00b7flammt", ",", "das\u00b7je\u00b7ni\u00b7ge", "zu", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "$,", "PDS", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Das da verm\u00f6gend ist die Seele recht zu laben.", "tokens": ["Das", "da", "ver\u00b7m\u00f6\u00b7gend", "ist", "die", "See\u00b7le", "recht", "zu", "la\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVPP", "VAFIN", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ein jeder Mensche merkt im irdischen Gew\u00fchl,", "tokens": ["Ein", "je\u00b7der", "Men\u00b7sche", "merkt", "im", "ir\u00b7di\u00b7schen", "Ge\u00b7w\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Er komme in der Welt nicht zum erw\u00fcnschten Ziel,", "tokens": ["Er", "kom\u00b7me", "in", "der", "Welt", "nicht", "zum", "er\u00b7w\u00fcnschten", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PTKNEG", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.33": {"text": "Er m\u00fcsse immerfort, bei allen seinen Trachten,", "tokens": ["Er", "m\u00fcs\u00b7se", "im\u00b7mer\u00b7fort", ",", "bei", "al\u00b7len", "sei\u00b7nen", "Trach\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "$,", "APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Dennoch in dem Genus nach h\u00f6hern Gute schmach-", "tokens": ["Den\u00b7noch", "in", "dem", "Ge\u00b7nus", "nach", "h\u00f6\u00b7hern", "Gu\u00b7te", "schmach"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Dies lehrt uns deutlich schon, es sey ohn allen", "tokens": ["Dies", "lehrt", "uns", "deut\u00b7lich", "schon", ",", "es", "sey", "ohn", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "ADV", "$,", "PPER", "VAFIN", "APPR", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Die Aendrungs-volle Welt ein Land der Eitelkeit.", "tokens": ["Die", "A\u00b7en\u00b7drungs\u00b7vol\u00b7le", "Welt", "ein", "Land", "der", "Ei\u00b7tel\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.37": {"text": "Dies wird noch deutlicher, wenn man bedachtsam", "tokens": ["Dies", "wird", "noch", "deut\u00b7li\u00b7cher", ",", "wenn", "man", "be\u00b7dacht\u00b7sam"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "$,", "KOUS", "PIS", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Der Menschen ihr Gesch\u00e4ft, wornach man sich be-", "tokens": ["Der", "Men\u00b7schen", "ihr", "Ge\u00b7sch\u00e4ft", ",", "wor\u00b7nach", "man", "sich", "be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "PWAV", "PIS", "PRF", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Der eine w\u00fchlet stets in einem Element,", "tokens": ["Der", "ei\u00b7ne", "w\u00fch\u00b7let", "stets", "in", "ei\u00b7nem", "E\u00b7le\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Das die Vernunft mit Recht der Unruh Nahrung", "tokens": ["Das", "die", "Ver\u00b7nunft", "mit", "Recht", "der", "Un\u00b7ruh", "Nah\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ART", "NN", "APPR", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Er trachtet immerfort da\u00df er mit blanker Erde,", "tokens": ["Er", "trach\u00b7tet", "im\u00b7mer\u00b7fort", "da\u00df", "er", "mit", "blan\u00b7ker", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KOUS", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Mit einem gelben Koth ganz \u00fcberh\u00e4uffet werde.", "tokens": ["Mit", "ei\u00b7nem", "gel\u00b7ben", "Koth", "ganz", "\u00fc\u00b7berh\u00b7\u00e4uf\u00b7fet", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Er sammlet immer mehr, und wird doch nimmer", "tokens": ["Er", "samm\u00b7let", "im\u00b7mer", "mehr", ",", "und", "wird", "doch", "nim\u00b7mer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "KON", "VAFIN", "ADV", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "Er w\u00fcnschet immer mehr, wenn er genug schon", "tokens": ["Er", "w\u00fcn\u00b7schet", "im\u00b7mer", "mehr", ",", "wenn", "er", "ge\u00b7nug", "schon"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "KOUS", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.45": {"text": "So wie der Hauffe w\u00e4chst; so w\u00e4chset das Verlan-", "tokens": ["So", "wie", "der", "Hauf\u00b7fe", "w\u00e4chst", ";", "so", "w\u00e4ch\u00b7set", "das", "Ver\u00b7lan"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "VVFIN", "$.", "ADV", "VVFIN", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Mit einem eitlen Koth, in dieser Welt zu prangen,", "tokens": ["Mit", "ei\u00b7nem", "eit\u00b7len", "Koth", ",", "in", "die\u00b7ser", "Welt", "zu", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPR", "PDAT", "NN", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Ist das nicht Eitelkeit? Jemehr der Geiz besizt,", "tokens": ["Ist", "das", "nicht", "Ei\u00b7tel\u00b7keit", "?", "Je\u00b7mehr", "der", "Geiz", "be\u00b7sizt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "PTKNEG", "NN", "$.", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.48": {"text": "Jeweniger wird es zu seinem Wol gen\u00fczt,", "tokens": ["Je\u00b7we\u00b7ni\u00b7ger", "wird", "es", "zu", "sei\u00b7nem", "Wol", "ge\u00b7n\u00fczt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Er suchet seine Ruh, den Kummer zu besiegen,", "tokens": ["Er", "su\u00b7chet", "sei\u00b7ne", "Ruh", ",", "den", "Kum\u00b7mer", "zu", "be\u00b7sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Und sammlet dennoch nur zu seinem Misvergn\u00fc-", "tokens": ["Und", "samm\u00b7let", "den\u00b7noch", "nur", "zu", "sei\u00b7nem", "Mis\u00b7ver\u00b7gn\u00fc"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADV", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Die Armuth h\u00e4uffet sich wenn sich sein Gut ver-", "tokens": ["Die", "Ar\u00b7muth", "h\u00e4uf\u00b7fet", "sich", "wenn", "sich", "sein", "Gut", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "KOUS", "PRF", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.52": {"text": "Weil man in Ueberflus ihn immer klagen h\u00f6rt:", "tokens": ["Weil", "man", "in", "Ue\u00b7ber\u00b7flus", "ihn", "im\u00b7mer", "kla\u00b7gen", "h\u00f6rt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NE", "PPER", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Ein solcher Erdenwurm, mu\u00df Koth mit M\u00fch er-", "tokens": ["Ein", "sol\u00b7cher", "Er\u00b7den\u00b7wurm", ",", "mu\u00df", "Koth", "mit", "M\u00fch", "er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "$,", "VMFIN", "NN", "APPR", "NE", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.54": {"text": "Und plagt sich Lebenslang um misvergn\u00fcgt zu ster-", "tokens": ["Und", "plagt", "sich", "Le\u00b7bens\u00b7lang", "um", "mis\u00b7ver\u00b7gn\u00fcgt", "zu", "ster"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "NN", "APPR", "VVFIN", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Ein andrer sieht die Welt, als einen Schauplaz", "tokens": ["Ein", "an\u00b7drer", "sieht", "die", "Welt", ",", "als", "ei\u00b7nen", "Schau\u00b7plaz"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "VVFIN", "ART", "NN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.56": {"text": "Allwo der gl\u00fcklich ist, der nur hoch sitzen kan,", "tokens": ["All\u00b7wo", "der", "gl\u00fck\u00b7lich", "ist", ",", "der", "nur", "hoch", "sit\u00b7zen", "kan", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJD", "VAFIN", "$,", "PRELS", "ADV", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Er rennt mit Angst und Schweis, mit Kummer", "tokens": ["Er", "rennt", "mit", "Angst", "und", "Schweis", ",", "mit", "Kum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.58": {"text": "Nach einem Ehrenziel um nur geplagt zu werden.", "tokens": ["Nach", "ei\u00b7nem", "Eh\u00b7ren\u00b7ziel", "um", "nur", "ge\u00b7plagt", "zu", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ADV", "VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Ee greift nach eitlen Dunst, und merket in der", "tokens": ["E\u00b7e", "greift", "nach", "eit\u00b7len", "Dunst", ",", "und", "mer\u00b7ket", "in", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN", "$,", "KON", "VVFIN", "APPR", "ART"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.60": {"text": "Er habe Woll gesucht, gefunden Angst und Weh,", "tokens": ["Er", "ha\u00b7be", "Woll", "ge\u00b7sucht", ",", "ge\u00b7fun\u00b7den", "Angst", "und", "Weh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "VVPP", "$,", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "Er jagt den Schatten nach, der ihm in Glanz be-", "tokens": ["Er", "jagt", "den", "Schat\u00b7ten", "nach", ",", "der", "ihm", "in", "Glanz", "be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,", "PRELS", "PPER", "APPR", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.62": {"text": "Und wenn er ihn erlangt; so ist der Schein verflo-", "tokens": ["Und", "wenn", "er", "ihn", "er\u00b7langt", ";", "so", "ist", "der", "Schein", "ver\u00b7flo"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PPER", "VVPP", "$.", "ADV", "VAFIN", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Er klimmet in die H\u00f6h die er sich vorgestellt,", "tokens": ["Er", "klim\u00b7met", "in", "die", "H\u00f6h", "die", "er", "sich", "vor\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "Da merkt er, erst zu sp\u00e4t, wie eitel diese Welt,", "tokens": ["Da", "merkt", "er", ",", "erst", "zu", "sp\u00e4t", ",", "wie", "ei\u00b7tel", "die\u00b7se", "Welt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "PTKA", "ADJD", "$,", "PWAV", "ADJD", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.65": {"text": "Der Schwindel nimt ihn ein, er f\u00e4nget an zu sin-", "tokens": ["Der", "Schwin\u00b7del", "nimt", "ihn", "ein", ",", "er", "f\u00e4n\u00b7get", "an", "zu", "sin"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VVFIN", "APPR", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.66": {"text": "Und mu\u00df zu seinem Spott, oft als ein Kr\u00fcppel", "tokens": ["Und", "mu\u00df", "zu", "sei\u00b7nem", "Spott", ",", "oft", "als", "ein", "Kr\u00fcp\u00b7pel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "APPR", "PPOSAT", "NN", "$,", "ADV", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.67": {"text": "Der dritte glaubt er sei vollkommen erst begl\u00fckt,", "tokens": ["Der", "drit\u00b7te", "glaubt", "er", "sei", "voll\u00b7kom\u00b7men", "erst", "be\u00b7gl\u00fckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "VAFIN", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.68": {"text": "Wenn ihn der Wollust Nez ins Labirinth gestrikt,", "tokens": ["Wenn", "ihn", "der", "Wol\u00b7lust", "Nez", "ins", "La\u00b7bi\u00b7rinth", "ge\u00b7strikt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NE", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.69": {"text": "Er rennet Tag und Nacht, ein Paradies zu fin-", "tokens": ["Er", "ren\u00b7net", "Tag", "und", "Nacht", ",", "ein", "Pa\u00b7ra\u00b7dies", "zu", "fin"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,", "ART", "NN", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.70": {"text": "Er rennet immer zu nach Art der armen Blinden,", "tokens": ["Er", "ren\u00b7net", "im\u00b7mer", "zu", "nach", "Art", "der", "ar\u00b7men", "Blin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKZU", "APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.71": {"text": "Da ihn die Einbildung, als wie ein Jrrlicht", "tokens": ["Da", "ihn", "die", "Ein\u00b7bil\u00b7dung", ",", "als", "wie", "ein", "Jrr\u00b7licht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "KOUS", "KOKOM", "ART", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.72": {"text": "Dem er so lange folgt, bis da\u00df er sich verliehrt.", "tokens": ["Dem", "er", "so", "lan\u00b7ge", "folgt", ",", "bis", "da\u00df", "er", "sich", "ver\u00b7liehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VVFIN", "$,", "KON", "KOUS", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.73": {"text": "Er kommt ins Paradies, geniesset seine Freude:", "tokens": ["Er", "kommt", "ins", "Pa\u00b7ra\u00b7dies", ",", "ge\u00b7nies\u00b7set", "sei\u00b7ne", "Freu\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.74": {"text": "Doch wenn ers recht besieht; so ist er auf der", "tokens": ["Doch", "wenn", "ers", "recht", "be\u00b7sieht", ";", "so", "ist", "er", "auf", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PIS", "ADV", "VVFIN", "$.", "ADV", "VAFIN", "PPER", "APPR", "ART"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.75": {"text": "Worauf das Vieh sich n\u00e4hrt, das da vergn\u00fcgt und", "tokens": ["Wo\u00b7rauf", "das", "Vieh", "sich", "n\u00e4hrt", ",", "das", "da", "ver\u00b7gn\u00fcgt", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "PRF", "VVFIN", "$,", "PRELS", "ADV", "VVPP", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.76": {"text": "Allwo der Mensche nichts, zu seiner Nahrung hat.", "tokens": ["All\u00b7wo", "der", "Men\u00b7sche", "nichts", ",", "zu", "sei\u00b7ner", "Nah\u00b7rung", "hat", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PIS", "$,", "APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.77": {"text": "Er schmekt den Bitter-Klee und lernet auch erken-", "tokens": ["Er", "schmekt", "den", "Bit\u00b7ter\u00b7Klee", "und", "ler\u00b7net", "auch", "er\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "ADV", "TRUNC"], "meter": "-+-+-+-+-+++", "measure": "unknown.measure.septa"}, "line.78": {"text": "Die Welt die m\u00fcsse man kein Paradies mehr nen-", "tokens": ["Die", "Welt", "die", "m\u00fcs\u00b7se", "man", "kein", "Pa\u00b7r