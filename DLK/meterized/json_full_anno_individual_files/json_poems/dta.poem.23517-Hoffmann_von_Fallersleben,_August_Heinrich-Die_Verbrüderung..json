{"dta.poem.23517": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Verbr\u00fcderung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nur im Osten kann es tagen,", "tokens": ["Nur", "im", "Os\u00b7ten", "kann", "es", "ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VMFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und es tagte wunderbar,", "tokens": ["Und", "es", "tag\u00b7te", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und im Osten ward geschlagen", "tokens": ["Und", "im", "Os\u00b7ten", "ward", "ge\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Frankreichs sieggewohnte Schaar.", "tokens": ["Fran\u00b7kreichs", "sieg\u00b7ge\u00b7wohn\u00b7te", "Schaar", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "----+-+", "measure": "unknown.measure.di"}}, "stanza.2": {"line.1": {"text": "Und die Moscowiterhorden", "tokens": ["Und", "die", "Mo\u00b7sco\u00b7wi\u00b7ter\u00b7hor\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Haben uns das Heil gebracht,", "tokens": ["Ha\u00b7ben", "uns", "das", "Heil", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sind mit uns verbr\u00fcdert worden", "tokens": ["Sind", "mit", "uns", "ver\u00b7br\u00fc\u00b7dert", "wor\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PPER", "VVPP", "VAPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch die deutsche Freiheitsschlacht.", "tokens": ["Durch", "die", "deut\u00b7sche", "Frei\u00b7heits\u00b7schlacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Verbr&#252;drung kann nur fruchten,", "tokens": ["Die", "Ver\u00b7br", "&#252;", "drung", "kann", "nur", "fruch\u00b7ten", ","], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "VMFIN", "ADV", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bringt uns Segen immerdar:", "tokens": ["Bringt", "uns", "Se\u00b7gen", "im\u00b7mer\u00b7dar", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dankbar gehn wir drum in Juchten,", "tokens": ["Dank\u00b7bar", "gehn", "wir", "drum", "in", "Juch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PAV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Essen dankbar Kaviar.", "tokens": ["Es\u00b7sen", "dank\u00b7bar", "Ka\u00b7vi\u00b7ar", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}