{"dta.poem.20105": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Der junge Graf von Hohenstein", "tokens": ["Der", "jun\u00b7ge", "Graf", "von", "Ho\u00b7hen\u00b7stein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "War sonst kein Waidgeselle,", "tokens": ["War", "sonst", "kein", "Waid\u00b7ge\u00b7sel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was h\u00e4lt sein Ro\u00df tagaus, tagein", "tokens": ["Was", "h\u00e4lt", "sein", "Ro\u00df", "ta\u00b7gaus", ",", "ta\u00b7gein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jetzt an des F\u00f6rsters Schwelle?", "tokens": ["Jetzt", "an", "des", "F\u00f6rs\u00b7ters", "Schwel\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er tr\u00e4gt kein H\u00fcfthorn um den Leib, \u2014", "tokens": ["Er", "tr\u00e4gt", "kein", "H\u00fcf\u00b7thorn", "um", "den", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPR", "ART", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was will der Graf erjagen?", "tokens": ["Was", "will", "der", "Graf", "er\u00b7ja\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ihr m\u00fc\u00dft des F\u00f6rsters junges Weib,", "tokens": ["Ihr", "m\u00fc\u00dft", "des", "F\u00f6rs\u00b7ters", "jun\u00b7ges", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die sch\u00f6ne Gertrud fragen.", "tokens": ["Die", "sch\u00f6\u00b7ne", "Ger\u00b7trud", "fra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die sch\u00f6ne Gertrud horcht gespannt", "tokens": ["Die", "sch\u00f6\u00b7ne", "Ger\u00b7trud", "horcht", "ge\u00b7spannt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NE", "VVFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bei D\u00e4mmerschein, im Garten;", "tokens": ["Bei", "D\u00e4m\u00b7mer\u00b7schein", ",", "im", "Gar\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Durch ihre Brust zieht, Hand in Hand,", "tokens": ["Durch", "ih\u00b7re", "Brust", "zieht", ",", "Hand", "in", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Bangen und Erwarten;", "tokens": ["Ein", "Ban\u00b7gen", "und", "Er\u00b7war\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da schallt ein Huf, der Hund schl\u00e4gt an,", "tokens": ["Da", "schallt", "ein", "Huf", ",", "der", "Hund", "schl\u00e4gt", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie spricht: \u201eGott, hab Erbarmen!\u201c", "tokens": ["Sie", "spricht", ":", "\u201e", "Gott", ",", "hab", "Er\u00b7bar\u00b7men", "!", "\u201c"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "NN", "$,", "VAFIN", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und eh sie weiter beten kann,", "tokens": ["Und", "eh", "sie", "wei\u00b7ter", "be\u00b7ten", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "H\u00e4lt sie der Graf in Armen.", "tokens": ["H\u00e4lt", "sie", "der", "Graf", "in", "Ar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er spricht: \u201enun halt\u2019 es endlich mir,", "tokens": ["Er", "spricht", ":", "\u201e", "nun", "halt'", "es", "end\u00b7lich", "mir", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "ADV", "VVFIN", "PPER", "ADV", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was Du mir oft versprochen,", "tokens": ["Was", "Du", "mir", "oft", "ver\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mir ist die Zeit seit Monden schier", "tokens": ["Mir", "ist", "die", "Zeit", "seit", "Mon\u00b7den", "schier"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf Schnecken fortgekrochen;", "tokens": ["Auf", "Schne\u00b7cken", "fort\u00b7ge\u00b7kro\u00b7chen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sprich nicht, auf\u2019s Neue, hin und her", "tokens": ["Sprich", "nicht", ",", "auf's", "Neu\u00b7e", ",", "hin", "und", "her"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PTKNEG", "$,", "APPRART", "ADJA", "$,", "ADV", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von Schwur, Altar und Treue, \u2014", "tokens": ["Von", "Schwur", ",", "Al\u00b7tar", "und", "Treu\u00b7e", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "VVFIN", "$,", "NN", "KON", "NN", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die Treu\u2019 ist eine alte M\u00e4hr\u2019,", "tokens": ["Die", "Treu'", "ist", "ei\u00b7ne", "al\u00b7te", "M\u00e4hr'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und Schwachheit ist die Reue.\u201c", "tokens": ["Und", "Schwach\u00b7heit", "ist", "die", "Reu\u00b7e", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "NN", "VAFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er spricht\u2019s, und als die Nacht erscheint", "tokens": ["Er", "spricht's", ",", "und", "als", "die", "Nacht", "er\u00b7scheint"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "KOUS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da hat das Spiel ein Ende,", "tokens": ["Da", "hat", "das", "Spiel", "ein", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Fortjagt der Graf, Sch\u00f6n-Gertrud weint,", "tokens": ["Fort\u00b7jagt", "der", "Graf", ",", "Sch\u00f6n\u00b7Ger\u00b7trud", "weint", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ringt die s\u00fcndgen H\u00e4nde;", "tokens": ["Und", "ringt", "die", "s\u00fcnd\u00b7gen", "H\u00e4n\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ihr Mann kehrt heim mit Gru\u00df und Ku\u00df,", "tokens": ["Ihr", "Mann", "kehrt", "heim", "mit", "Gru\u00df", "und", "Ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie Abschied er genommen,", "tokens": ["Wie", "Ab\u00b7schied", "er", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sie heuchelt, weil sie heucheln mu\u00df,", "tokens": ["Sie", "heu\u00b7chelt", ",", "weil", "sie", "heu\u00b7cheln", "mu\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und hei\u00dft ihn froh willkommen. \u2014", "tokens": ["Und", "hei\u00dft", "ihn", "froh", "will\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ADJD", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ein Jahr und wenig Tage sind\u2019s,", "tokens": ["Ein", "Jahr", "und", "we\u00b7nig", "Ta\u00b7ge", "sin\u00b7d's", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PIAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Graf zieht andre F\u00e4hrte,", "tokens": ["Der", "Graf", "zieht", "and\u00b7re", "F\u00e4hr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zur Taufe nur des F\u00f6rsterkinds", "tokens": ["Zur", "Tau\u00b7fe", "nur", "des", "F\u00f6rs\u00b7ter\u00b7kinds"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u2019nen Becher Wein er leerte.", "tokens": ["'nen", "Be\u00b7cher", "Wein", "er", "leer\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Wein war n\u00fcchtern wie die Leut\u2019,", "tokens": ["Der", "Wein", "war", "n\u00fcch\u00b7tern", "wie", "die", "Leut'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und konnt ihn wenig laben,", "tokens": ["Und", "konnt", "ihn", "we\u00b7nig", "la\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Nur mocht an F\u00f6rsters Vaterfreud\u2019", "tokens": ["Nur", "mocht", "an", "F\u00f6rs\u00b7ters", "Va\u00b7ter\u00b7freud'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Er ", "tokens": ["Er"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}}}}}