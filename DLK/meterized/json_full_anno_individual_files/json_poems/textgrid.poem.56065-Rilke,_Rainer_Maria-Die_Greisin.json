{"textgrid.poem.56065": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Die Greisin", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wei\u00dfe Freundinnen mitten im Heute", "tokens": ["Wei\u00b7\u00dfe", "Freun\u00b7din\u00b7nen", "mit\u00b7ten", "im", "Heu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADV", "APPRART", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "lachen und horchen und planen f\u00fcr morgen;", "tokens": ["la\u00b7chen", "und", "hor\u00b7chen", "und", "pla\u00b7nen", "f\u00fcr", "mor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVINF", "KON", "ADJA", "KON", "VVFIN", "APPR", "ADV", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "abseits erw\u00e4gen gelassene Leute", "tokens": ["ab\u00b7seits", "er\u00b7w\u00e4\u00b7gen", "ge\u00b7las\u00b7se\u00b7ne", "Leu\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "langsam ihre besonderen Sorgen,", "tokens": ["lang\u00b7sam", "ih\u00b7re", "be\u00b7son\u00b7de\u00b7ren", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "das Warum und das Wann und das Wie,", "tokens": ["das", "Wa\u00b7rum", "und", "das", "Wann", "und", "das", "Wie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "und man h\u00f6rt sie sagen: Ich glaube \u2013;", "tokens": ["und", "man", "h\u00f6rt", "sie", "sa\u00b7gen", ":", "Ich", "glau\u00b7be", "\u2013", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "VVINF", "$.", "PPER", "VVFIN", "$(", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "aber in ihrer Spitzenhaube", "tokens": ["a\u00b7ber", "in", "ih\u00b7rer", "Spit\u00b7zen\u00b7hau\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "ist sie sicher, als w\u00fc\u00dfte sie,", "tokens": ["ist", "sie", "si\u00b7cher", ",", "als", "w\u00fc\u00df\u00b7te", "sie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "KOUS", "VVFIN", "PPER", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "da\u00df sie sich irren, diese und alle.", "tokens": ["da\u00df", "sie", "sich", "ir\u00b7ren", ",", "die\u00b7se", "und", "al\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "$,", "PDAT", "KON", "PIS", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und das Kinn, im Niederfalle,", "tokens": ["Und", "das", "Kinn", ",", "im", "Nie\u00b7der\u00b7fal\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "lehnt sich an die wei\u00dfe Koralle,", "tokens": ["lehnt", "sich", "an", "die", "wei\u00b7\u00dfe", "Ko\u00b7ral\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "die den Schal zur Stirne stimmt.", "tokens": ["die", "den", "Schal", "zur", "Stir\u00b7ne", "stimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Einmal aber, bei einem Gelache,", "tokens": ["Ein\u00b7mal", "a\u00b7ber", ",", "bei", "ei\u00b7nem", "Ge\u00b7la\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "holt sie aus springenden Lidern zwei wache", "tokens": ["holt", "sie", "aus", "sprin\u00b7gen\u00b7den", "Li\u00b7dern", "zwei", "wa\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "CARD", "ADJA"], "meter": "+--+--+-++-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Blicke und zeigt diese harte Sache,", "tokens": ["Bli\u00b7cke", "und", "zeigt", "die\u00b7se", "har\u00b7te", "Sa\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PDAT", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "wie man aus einem geheimen Fache", "tokens": ["wie", "man", "aus", "ei\u00b7nem", "ge\u00b7hei\u00b7men", "Fa\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "sch\u00f6ne ererbte Steine nimmt.", "tokens": ["sch\u00f6\u00b7ne", "er\u00b7erb\u00b7te", "Stei\u00b7ne", "nimmt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "VVF