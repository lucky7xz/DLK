{"dta.poem.19908": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Zechlied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1778", "urn": "urn:nbn:de:kobv:b4-20090519672", "language": ["de:0.99"], "booktitle": "B\u00fcrger, Gottfried August: Gedichte. G\u00f6ttingen, 1778."}, "poem": {"stanza.1": {"line.1": {"text": "Ich wil einst, bei Ja und Nein!               ", "tokens": ["Ich", "wil", "einst", ",", "bei", "Ja", "und", "Nein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "$,", "APPR", "NN", "KON", "PTKANT", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Vor dem Zapfen sterben.", "tokens": ["Vor", "dem", "Zap\u00b7fen", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Alles, meinen Wein nur nicht,", "tokens": ["Al\u00b7les", ",", "mei\u00b7nen", "Wein", "nur", "nicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PPOSAT", "NN", "ADV", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lass\u2019 ich frohen Erben.", "tokens": ["Lass'", "ich", "fro\u00b7hen", "Er\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Nach der lezten Oelung sol", "tokens": ["Nach", "der", "lez\u00b7ten", "O\u00b7e\u00b7lung", "sol"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VMFIN"], "meter": "--+----+", "measure": "anapaest.init"}, "line.6": {"text": "Hefen noch mich f\u00e4rben.", "tokens": ["He\u00b7fen", "noch", "mich", "f\u00e4r\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Dann zertr\u00fcmre mein Pokal", "tokens": ["Dann", "zer\u00b7tr\u00fcm\u00b7re", "mein", "Po\u00b7kal"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "In zehntausend Scherben!", "tokens": ["In", "zehn\u00b7tau\u00b7send", "Scher\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Jederman hat von Natur", "tokens": ["Je\u00b7der\u00b7man", "hat", "von", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Seine sondre Weise.", "tokens": ["Sei\u00b7ne", "sond\u00b7re", "Wei\u00b7se", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Mir gelinget jedes Werk", "tokens": ["Mir", "ge\u00b7lin\u00b7get", "je\u00b7des", "Werk"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur nach Trank und Speise.", "tokens": ["Nur", "nach", "Trank", "und", "Spei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Speis\u2019 und Trank erhalten mich", "tokens": ["Speis'", "und", "Trank", "er\u00b7hal\u00b7ten", "mich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "In dem rechten Gleise.", "tokens": ["In", "dem", "rech\u00b7ten", "Glei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Wer gut schmiert, der f\u00e4hrt auch gut,", "tokens": ["Wer", "gut", "schmiert", ",", "der", "f\u00e4hrt", "auch", "gut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "$,", "PRELS", "VVFIN", "ADV", "ADJD", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.8": {"text": "Auf der Lebensreise.", "tokens": ["Auf", "der", "Le\u00b7bens\u00b7rei\u00b7se", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Ich bin gar ein armer Wicht,", "tokens": ["Ich", "bin", "gar", "ein", "ar\u00b7mer", "Wicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Bin die feigste Memme,", "tokens": ["Bin", "die", "feigs\u00b7te", "Mem\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Halten Durst und Hungerqual", "tokens": ["Hal\u00b7ten", "Durst", "und", "Hun\u00b7ger\u00b7qual"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich in Angst und Klemme.", "tokens": ["Mich", "in", "Angst", "und", "Klem\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Schon ein Kn\u00e4bchen sch\u00fcttelt mich,", "tokens": ["Schon", "ein", "Kn\u00e4b\u00b7chen", "sch\u00fct\u00b7telt", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Was ich auch mich stemme.", "tokens": ["Was", "ich", "auch", "mich", "stem\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Einem Riesen halt\u2019 ich Stand,", "tokens": ["Ei\u00b7nem", "Rie\u00b7sen", "halt'", "ich", "Stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wann ich zech\u2019 und schlemme.", "tokens": ["Wann", "ich", "zech'", "und", "schlem\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Aechter Wein ist \u00e4chtes Oel", "tokens": ["A\u00b7ech\u00b7ter", "Wein", "ist", "\u00e4ch\u00b7tes", "O\u00b7el"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NN", "VAFIN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zur Verstandeslampe;", "tokens": ["Zur", "Ver\u00b7stan\u00b7des\u00b7lam\u00b7pe", ";"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Giebt der Seele Kraft und Schwung", "tokens": ["Giebt", "der", "See\u00b7le", "Kraft", "und", "Schwung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis zum Sternenkampe.", "tokens": ["Bis", "zum", "Ster\u00b7nen\u00b7kam\u00b7pe", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wiz und Weisheit dunsten auf", "tokens": ["Wiz", "und", "Weis\u00b7heit", "duns\u00b7ten", "auf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Aus gef\u00fclter Wampe.", "tokens": ["Aus", "ge\u00b7f\u00fcl\u00b7ter", "Wam\u00b7pe", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Bas gl\u00fckt Harfenspiel und Sang,", "tokens": ["Bas", "gl\u00fckt", "Har\u00b7fen\u00b7spiel", "und", "Sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NE", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wann ich brav schlampampe.", "tokens": ["Wann", "ich", "brav", "schlam\u00b7pam\u00b7pe", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "N\u00fcchtern bin ich immerdar", "tokens": ["N\u00fcch\u00b7tern", "bin", "ich", "im\u00b7mer\u00b7dar"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur ein Harfenst\u00fcmper.", "tokens": ["Nur", "ein", "Har\u00b7fen\u00b7st\u00fcm\u00b7per", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Mir erlamen Hand und Grif,", "tokens": ["Mir", "er\u00b7la\u00b7men", "Hand", "und", "Grif", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Welken Haupt und Wimper.", "tokens": ["Wel\u00b7ken", "Haupt", "und", "Wim\u00b7per", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wann der Wein in Himmelsklang", "tokens": ["Wann", "der", "Wein", "in", "Him\u00b7mels\u00b7klang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wandelt mein Geklimper,", "tokens": ["Wan\u00b7delt", "mein", "Ge\u00b7klim\u00b7per", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Sind Homer und Ossian", "tokens": ["Sind", "Ho\u00b7mer", "und", "Os\u00b7si\u00b7an"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "NE", "KON", "NE"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Gegen mich nur St\u00fcmper.", "tokens": ["Ge\u00b7gen", "mich", "nur", "St\u00fcm\u00b7per", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Nimmer hat durch meinen Mund", "tokens": ["Nim\u00b7mer", "hat", "durch", "mei\u00b7nen", "Mund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hoher Geist gesungen,", "tokens": ["Ho\u00b7her", "Geist", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Bis ich meinen lieben Bauch", "tokens": ["Bis", "ich", "mei\u00b7nen", "lie\u00b7ben", "Bauch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Weidlich volgeschlungen.", "tokens": ["Weid\u00b7lich", "vol\u00b7ge\u00b7schlun\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wann mein Kapitolium", "tokens": ["Wann", "mein", "Ka\u00b7pi\u00b7to\u00b7li\u00b7um"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Bacchus Kraft erschwungen,", "tokens": ["Bac\u00b7chus", "Kraft", "er\u00b7schwun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Sing\u2019 und red\u2019 ich wundersam", "tokens": ["Sing'", "und", "red'", "ich", "wun\u00b7der\u00b7sam"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KON", "VVFIN", "PPER", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Gar in fremden Zungen.", "tokens": ["Gar", "in", "frem\u00b7den", "Zun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Drum wil ich, bei Ja und Nein!", "tokens": ["Drum", "wil", "ich", ",", "bei", "Ja", "und", "Nein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "$,", "APPR", "NN", "KON", "PTKANT", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Vor dem Zapfen sterben.", "tokens": ["Vor", "dem", "Zap\u00b7fen", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Nach der lezten Oelung sol", "tokens": ["Nach", "der", "lez\u00b7ten", "O\u00b7e\u00b7lung", "sol"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VMFIN"], "meter": "--+----+", "measure": "anapaest.init"}, "line.4": {"text": "Hefen noch mich f\u00e4rben.", "tokens": ["He\u00b7fen", "noch", "mich", "f\u00e4r\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Engelch\u00f6re weihen dann", "tokens": ["En\u00b7gelc\u00b7h\u00f6\u00b7re", "wei\u00b7hen", "dann"], "token_info": ["word", "word", "word"], "pos": ["NE", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mich zum Nektarerben:", "tokens": ["Mich", "zum", "Nek\u00b7ta\u00b7rer\u00b7ben", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "\u201ediesen Trinker gnade Gott!", "tokens": ["\u201e", "die\u00b7sen", "Trin\u00b7ker", "gna\u00b7de", "Gott", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDAT", "NN", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Lass\u2019 ihn nicht verderben!\u201e", "tokens": ["Lass'", "ihn", "nicht", "ver\u00b7der\u00b7ben", "!", "\u201e"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "VVFIN", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}