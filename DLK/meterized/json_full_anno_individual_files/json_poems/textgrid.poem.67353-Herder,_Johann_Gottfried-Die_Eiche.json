{"textgrid.poem.67353": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Die Eiche", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie wenn die Eiche, die Jahrhunderte", "tokens": ["Wie", "wenn", "die", "Ei\u00b7che", ",", "die", "Jahr\u00b7hun\u00b7der\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOKOM", "KOUS", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf ihrem Berge tiefe Wurzel schlug", "tokens": ["Auf", "ih\u00b7rem", "Ber\u00b7ge", "tie\u00b7fe", "Wur\u00b7zel", "schlug"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und bot die Stirn den Winden und dem Sturm,", "tokens": ["Und", "bot", "die", "Stirn", "den", "Win\u00b7den", "und", "dem", "Sturm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wenn ringsum er die Fl\u00fcgel auf sie schlug;", "tokens": ["Wenn", "ring\u00b7sum", "er", "die", "Fl\u00fc\u00b7gel", "auf", "sie", "schlug", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ersch\u00fcttert und entrissen liegt sie itzt", "tokens": ["Er\u00b7sch\u00fct\u00b7tert", "und", "ent\u00b7ris\u00b7sen", "liegt", "sie", "itzt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "KON", "VVINF", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von Schicksals St\u00fcrmen und den Jahren, misset weit", "tokens": ["Von", "Schick\u00b7sals", "St\u00fcr\u00b7men", "und", "den", "Jah\u00b7ren", ",", "mis\u00b7set", "weit"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "NN", "KON", "ART", "NN", "$,", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Umher das Land mit ihrem m\u00e4cht'gen Arm,", "tokens": ["Um\u00b7her", "das", "Land", "mit", "ih\u00b7rem", "m\u00e4cht'\u00b7gen", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bedeckt den Boden weit mit dickem Haar.", "tokens": ["Be\u00b7deckt", "den", "Bo\u00b7den", "weit", "mit", "di\u00b7ckem", "Haar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da kommet denn das tapfre Bauernvolk", "tokens": ["Da", "kom\u00b7met", "denn", "das", "tapf\u00b7re", "Bau\u00b7ern\u00b7volk"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Aus H\u00f6hlen und aus H\u00fctten, hauet k\u00fchn", "tokens": ["Aus", "H\u00f6h\u00b7len", "und", "aus", "H\u00fct\u00b7ten", ",", "hau\u00b7et", "k\u00fchn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihm ab die Glieder, kappet ihm das Haupt,", "tokens": ["Ihm", "ab", "die", "Glie\u00b7der", ",", "kap\u00b7pet", "ihm", "das", "Haupt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Zerf\u00e4llt den ungemessnen starken Stamm", "tokens": ["Zer\u00b7f\u00e4llt", "den", "un\u00b7ge\u00b7mess\u00b7nen", "star\u00b7ken", "Stamm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und haut und haut, da\u00df rings der Berg erbebt,", "tokens": ["Und", "haut", "und", "haut", ",", "da\u00df", "rings", "der", "Berg", "er\u00b7bebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "$,", "KOUS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und jede Kluft und H\u00f6hle widerhallt.", "tokens": ["Und", "je\u00b7de", "Kluft", "und", "H\u00f6h\u00b7le", "wi\u00b7der\u00b7hallt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Dem\u00fcthig rollt der Arme jetzt hinab;", "tokens": ["De\u00b7m\u00fct\u00b7hig", "rollt", "der", "Ar\u00b7me", "jetzt", "hin\u00b7ab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zerschlagen tr\u00e4gt man auf den Schultern ihn", "tokens": ["Zer\u00b7schla\u00b7gen", "tr\u00e4gt", "man", "auf", "den", "Schul\u00b7tern", "ihn"], "token_info": ["word", "word", "word",