{"dta.poem.21087": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "An  \n Charlotte Schwarz.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Blumen sucht' ich meiner Holden,               ", "tokens": ["Blu\u00b7men", "sucht'", "ich", "mei\u00b7ner", "Hol\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blumen unter Schnee und Schlossen.", "tokens": ["Blu\u00b7men", "un\u00b7ter", "Schnee", "und", "Schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch der hochgeschwollnen Dolden", "tokens": ["Doch", "der", "hoch\u00b7ge\u00b7schwoll\u00b7nen", "Dol\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fand ich keine noch erbl\u00fcht.", "tokens": ["Fand", "ich", "kei\u00b7ne", "noch", "er\u00b7bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hyacinthen fand ich schossen,", "tokens": ["Hya\u00b7cin\u00b7then", "fand", "ich", "schos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Krokos, Primeln kaum erst sprossen.", "tokens": ["Kro\u00b7kos", ",", "Pri\u00b7meln", "kaum", "erst", "spros\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nimm denn, Traute, was die Noth beschied,", "tokens": ["Nimm", "denn", ",", "Trau\u00b7te", ",", "was", "die", "Noth", "be\u00b7schied", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "$,", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Nimm diess Buch, diess Band, diess Lied.", "tokens": ["Nimm", "diess", "Buch", ",", "diess", "Band", ",", "diess", "Lied", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PDAT", "NN", "$,", "PDAT", "NN", "$,", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "In des B\u00fcchleins Silberbl\u00e4ttern", "tokens": ["In", "des", "B\u00fcch\u00b7leins", "Sil\u00b7ber\u00b7bl\u00e4t\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6rst du itzt ein Heimchen schwirren,", "tokens": ["H\u00f6rst", "du", "itzt", "ein", "Heim\u00b7chen", "schwir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Itzt den Sprosser gellend schmettern,", "tokens": ["Itzt", "den", "Spros\u00b7ser", "gel\u00b7lend", "schmet\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Itzt den Bach, der schwatzend rollt.", "tokens": ["Itzt", "den", "Bach", ",", "der", "schwat\u00b7zend", "rollt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nie verlocken, nie verirren", "tokens": ["Nie", "ver\u00b7lo\u00b7cken", ",", "nie", "ver\u00b7ir\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "VVINF", "$,", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird dich dieser Zither Girren;", "tokens": ["Wird", "dich", "die\u00b7ser", "Zi\u00b7ther", "Gir\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Unentheiligt klingt ihr bebend Gold:", "tokens": ["Un\u00b7en\u00b7thei\u00b7ligt", "klingt", "ihr", "be\u00b7bend", "Gold", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADJD", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Darum sey dem B\u00fcchlein hold.", "tokens": ["Da\u00b7rum", "sey", "dem", "B\u00fcch\u00b7lein", "hold", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Dieses G\u00fcrtels Goldgewinde", "tokens": ["Die\u00b7ses", "G\u00fcr\u00b7tels", "Gold\u00b7ge\u00b7win\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird dich, Beste, nicht entstellen.", "tokens": ["Wird", "dich", ",", "Bes\u00b7te", ",", "nicht", "ent\u00b7stel\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "NN", "$,", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Heilig ist das Angebinde,", "tokens": ["Hei\u00b7lig", "ist", "das", "An\u00b7ge\u00b7bin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Heilig aus des Dichters Hand.", "tokens": ["Hei\u00b7lig", "aus", "des", "Dich\u00b7ters", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Seiner Schleifen lose Wellen", "tokens": ["Sei\u00b7ner", "Schlei\u00b7fen", "lo\u00b7se", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lass um deine H\u00fcften schwellen.", "tokens": ["Lass", "um", "dei\u00b7ne", "H\u00fcf\u00b7ten", "schwel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ehre, Freundin, in dem armen Band", "tokens": ["Eh\u00b7re", ",", "Freun\u00b7din", ",", "in", "dem", "ar\u00b7men", "Band"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Edler Freundschaft Unterpfand.", "tokens": ["Ed\u00b7ler", "Freund\u00b7schaft", "Un\u00b7ter\u00b7pfand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch, des Dichters beste Habe,", "tokens": ["Doch", ",", "des", "Dich\u00b7ters", "bes\u00b7te", "Ha\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seines Geistes s\u00fcsse Weide,", "tokens": ["Sei\u00b7nes", "Geis\u00b7tes", "s\u00fcs\u00b7se", "Wei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seiner Freundschaft sch\u00f6nste Gabe", "tokens": ["Sei\u00b7ner", "Freund\u00b7schaft", "sch\u00f6ns\u00b7te", "Ga\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist der heilige Gesang.", "tokens": ["Ist", "der", "hei\u00b7li\u00b7ge", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dieser adelt seine Freude,", "tokens": ["Die\u00b7ser", "a\u00b7delt", "sei\u00b7ne", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Tr\u00f6stet ihn im Lebensleide,", "tokens": ["Tr\u00f6s\u00b7tet", "ihn", "im", "Le\u00b7bens\u00b7lei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "K\u00fchlt ihn in der Leidenschaften Drang,", "tokens": ["K\u00fchlt", "ihn", "in", "der", "Lei\u00b7den\u00b7schaf\u00b7ten", "Drang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Sichert ihm der Enkel Dank.", "tokens": ["Si\u00b7chert", "ihm", "der", "En\u00b7kel", "Dank", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Also fl\u00f6tet Philomele,", "tokens": ["Al\u00b7so", "fl\u00f6\u00b7tet", "Phi\u00b7lo\u00b7me\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn die Abendwolken bl\u00fchen;", "tokens": ["Wenn", "die", "Ab\u00b7end\u00b7wol\u00b7ken", "bl\u00fc\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Flammen str\u00f6men in die Seele,", "tokens": ["Flam\u00b7men", "str\u00f6\u00b7men", "in", "die", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Brust entst\u00f6hnt ein Ach!", "tokens": ["Und", "der", "Brust", "ent\u00b7st\u00f6hnt", "ein", "Ach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Also zittern, also gl\u00fchen,", "tokens": ["Al\u00b7so", "zit\u00b7tern", ",", "al\u00b7so", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn des Liedes Strahlen spr\u00fchen,", "tokens": ["Wenn", "des", "Lie\u00b7des", "Strah\u00b7len", "spr\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Fromme Herzen, schlagen heissern Schlag,", "tokens": ["From\u00b7me", "Her\u00b7zen", ",", "schla\u00b7gen", "heis\u00b7sern", "Schlag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Und die Thr\u00e4ne st\u00fcrzet nach.", "tokens": ["Und", "die", "Thr\u00e4\u00b7ne", "st\u00fcr\u00b7zet", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Kennst du, Freundin, wohl die Feyen,", "tokens": ["Kennst", "du", ",", "Freun\u00b7din", ",", "wohl", "die", "Fe\u00b7yen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die des M\u00e4dchens Herz bewachen?", "tokens": ["Die", "des", "M\u00e4d\u00b7chens", "Herz", "be\u00b7wa\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jene Guten, jene Treuen,", "tokens": ["Je\u00b7ne", "Gu\u00b7ten", ",", "je\u00b7ne", "Treu\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die aus Eden hergesandt,", "tokens": ["Die", "aus", "E\u00b7den", "her\u00b7ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der Entschlafnen K\u00fchlung fachen,", "tokens": ["Der", "Ent\u00b7schlaf\u00b7nen", "K\u00fch\u00b7lung", "fa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die Erwachende umlachen,", "tokens": ["Die", "Er\u00b7wa\u00b7chen\u00b7de", "um\u00b7la\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die Tappende mit treuer Hand", "tokens": ["Und", "die", "Tap\u00b7pen\u00b7de", "mit", "treu\u00b7er", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Leiten in das bessre Land?", "tokens": ["Lei\u00b7ten", "in", "das", "bess\u00b7re", "Land", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Einfalt mit dem Taubenmuthe,               ", "tokens": ["Ein\u00b7falt", "mit", "dem", "Tau\u00b7ben\u00b7mu\u00b7the", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unschuld mit der Lammesbl\u00f6de,               ", "tokens": ["Un\u00b7schuld", "mit", "der", "Lam\u00b7mes\u00b7bl\u00f6\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Anmuth mit dem Sch&#228;ferhute               ", "tokens": ["An\u00b7muth", "mit", "dem", "Sch", "&#228;", "fer\u00b7hu\u00b7te"], "token_info": ["word", "word", "word", "word", "XML_entity", "word"], "pos": ["NN", "APPR", "ART", "NN", "$(", "FM.la"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Diese sind der Jungfrau hold.", "tokens": ["Die\u00b7se", "sind", "der", "Jung\u00b7frau", "hold", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.5": {"text": "Sorgsam leiten sie die Bl\u00f6de", "tokens": ["Sorg\u00b7sam", "lei\u00b7ten", "sie", "die", "Bl\u00f6\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Durch des Lebens W\u00fcst' und \u00d6de", "tokens": ["Durch", "des", "Le\u00b7bens", "W\u00fcst'", "und", "\u00d6\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "In das Land, wo keine Thr\u00e4ne rollt,", "tokens": ["In", "das", "Land", ",", "wo", "kei\u00b7ne", "Thr\u00e4\u00b7ne", "rollt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Keine T\u00fccke schmollt noch grollt.", "tokens": ["Kei\u00b7ne", "T\u00fc\u00b7cke", "schmollt", "noch", "grollt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Sahst du wohl die Einfalt sitzen,", "tokens": ["Sahst", "du", "wohl", "die", "Ein\u00b7falt", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fremd dem modischen Get\u00e4ndel?", "tokens": 