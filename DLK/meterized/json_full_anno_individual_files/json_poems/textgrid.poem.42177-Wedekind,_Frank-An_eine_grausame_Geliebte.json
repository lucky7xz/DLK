{"textgrid.poem.42177": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "An eine grausame Geliebte", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hetz deine Meute weit \u00fcber die Berge hin,", "tokens": ["Hetz", "dei\u00b7ne", "Meu\u00b7te", "weit", "\u00fc\u00b7ber", "die", "Ber\u00b7ge", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Sie kehrt wieder von Schwei\u00df und von Staub bedeckt.", "tokens": ["Sie", "kehrt", "wie\u00b7der", "von", "Schwei\u00df", "und", "von", "Staub", "be\u00b7deckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "KON", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Gib ihr die Peitsche, gewaltige J\u00e4gerin,", "tokens": ["Gib", "ihr", "die", "Peit\u00b7sche", ",", "ge\u00b7wal\u00b7ti\u00b7ge", "J\u00e4\u00b7ge\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Sieh, wie sie dir winselnd die F\u00fc\u00dfe leckt!", "tokens": ["Sieh", ",", "wie", "sie", "dir", "win\u00b7selnd", "die", "F\u00fc\u00b7\u00dfe", "leckt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPER", "PPER", "VVPP", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Eh der Bann zerrei\u00dft, eh die Koppel in St\u00fccke springt,", "tokens": ["Eh", "der", "Bann", "zer\u00b7rei\u00dft", ",", "eh", "die", "Kop\u00b7pel", "in", "St\u00fc\u00b7cke", "springt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$,", "KOUS", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Eh die Brut dir entgegensteht, wenn dein Hifthorn klingt,", "tokens": ["Eh", "die", "Brut", "dir", "ent\u00b7ge\u00b7gen\u00b7steht", ",", "wenn", "dein", "Hif\u00b7thorn", "klingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+--+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Eh dein Ohr ihn vernimmt, aus der Seele den dumpfen Schrei,", "tokens": ["Eh", "dein", "Ohr", "ihn", "ver\u00b7nimmt", ",", "aus", "der", "See\u00b7le", "den", "dum\u00b7pfen", "Schrei", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$,", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Eh rei\u00dfen Sehnen und Adern und Herz entzwei.", "tokens": ["Eh", "rei\u00b7\u00dfen", "Seh\u00b7nen", "und", "A\u00b7dern", "und", "Herz", "ent\u00b7zwei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "KON", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}}, "stanza.3": {"line.1": {"text": "Schwing deine Peitsche! Dein gellendes Halali", "tokens": ["Schwing", "dei\u00b7ne", "Peit\u00b7sche", "!", "Dein", "gel\u00b7len\u00b7