{"dta.poem.12699": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Eines Ober-post-amt-ver-  \n walters.  \n  D.  V. A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die post ist wohl bestellt: der leib geh\u00f6rt zur erden,", "tokens": ["Die", "post", "ist", "wohl", "be\u00b7stellt", ":", "der", "leib", "ge\u00b7h\u00f6rt", "zur", "er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$.", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus der er kommen ist; die seele trifft die bahn,", "tokens": ["Aus", "der", "er", "kom\u00b7men", "ist", ";", "die", "see\u00b7le", "trifft", "die", "bahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVINF", "VAFIN", "$.", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und steigt in Christi blut durch einschlu\u00df himmel-an:", "tokens": ["Und", "steigt", "in", "Chris\u00b7ti", "blut", "durch", "ein\u00b7schlu\u00df", "him\u00b7mel\u00b7an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NE", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kein porto kan daf\u00fcr iemals bezahlet werden.", "tokens": ["Kein", "por\u00b7to", "kan", "da\u00b7f\u00fcr", "ie\u00b7mals", "be\u00b7zah\u00b7let", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NE", "VMFIN", "PAV", "ADV", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}