{"textgrid.poem.55489": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "69", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am feuchten Fels, den dichtes Moos versteckt,", "tokens": ["Am", "feuch\u00b7ten", "Fels", ",", "den", "dich\u00b7tes", "Moos", "ver\u00b7steckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erbl\u00fchen Blumen, flattert manch Insekt;", "tokens": ["Er\u00b7bl\u00fc\u00b7hen", "Blu\u00b7men", ",", "flat\u00b7tert", "manch", "In\u00b7sekt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Scheint es auch d\u00fcrr den kahlen Berg hinan,", "tokens": ["Scheint", "es", "auch", "d\u00fcrr", "den", "kah\u00b7len", "Berg", "hi\u00b7nan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So n\u00e4hrt es doch, das Schaf bewollt sich dran.", "tokens": ["So", "n\u00e4hrt", "es", "doch", ",", "das", "Schaf", "be\u00b7wollt", "sich", "dran", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "ART", "NN", "VVFIN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Wiese gr\u00fcnt, geh\u00f6rnte Herde braunt,", "tokens": ["Die", "Wie\u00b7se", "gr\u00fcnt", ",", "ge\u00b7h\u00f6rn\u00b7te", "Her\u00b7de", "braunt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da wandeln Menschen, gut und b\u00f6s gelaunt,", "tokens": ["Da", "wan\u00b7deln", "Men\u00b7schen", ",", "gut", "und", "b\u00f6s", "ge\u00b7launt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$,", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Genie\u00dfen reichlich, sp\u00e4rlich, fr\u00fch und spat", "tokens": ["Ge\u00b7nie\u00b7\u00dfen", "reich\u00b7lich", ",", "sp\u00e4r\u00b7lich", ",", "fr\u00fch", "und", "spat"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "ADJD", "$,", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Den Wunderwuchs der folgereichen Saat.", "tokens": ["Den", "Wun\u00b7der\u00b7wuchs", "der", "fol\u00b7ge\u00b7rei\u00b7chen", "Saat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und wenn der Kranke fast am Ziel erliegt,", "tokens": ["Und", "wenn", "der", "Kran\u00b7ke", "fast", "am", "Ziel", "er\u00b7liegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "So steigt die Quelle rasch, die Hoffnung siegt.", "tokens": ["So", "steigt", "die", "Quel\u00b7le", "rasch", ",", "die", "Hoff\u00b7nung", "siegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "