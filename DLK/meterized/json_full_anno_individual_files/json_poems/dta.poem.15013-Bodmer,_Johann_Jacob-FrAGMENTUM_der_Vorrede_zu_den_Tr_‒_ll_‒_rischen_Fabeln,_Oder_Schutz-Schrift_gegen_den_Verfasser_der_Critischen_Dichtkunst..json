{"dta.poem.15013": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "FrAGMENTUM  \n der Vorrede zu den  \n  Tr \u2012 ll \u2012 rischen Fabeln,  \n Oder  \n  Schutz-Schrift  \n gegen den Verfasser der  \n  Critischen Dichtkunst.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1741", "urn": "urn:nbn:de:kobv:b4-200905198526", "language": ["de:0.85", "sk:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So sprach er, jeder fiel ihm bey,", "tokens": ["So", "sprach", "er", ",", "je\u00b7der", "fiel", "ihm", "bey", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PIS", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus unversch\u00e4mter Schmeicheley,", "tokens": ["Aus", "un\u00b7ver\u00b7sch\u00e4m\u00b7ter", "Schmei\u00b7che\u00b7ley", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Selbst Salomo, ", "tokens": ["Selbst", "Sa\u00b7lo\u00b7mo", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "NE", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "K\u00f6nnt weiser nicht und besser w\u00e4hlen.", "tokens": ["K\u00f6nnt", "wei\u00b7ser", "nicht", "und", "bes\u00b7ser", "w\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PTKNEG", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}