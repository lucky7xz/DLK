{"textgrid.poem.61743": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Eiserne Zeit", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unterm alten Eichenbaum,", "tokens": ["Un\u00b7term", "al\u00b7ten", "Ei\u00b7chen\u00b7baum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo das Volk ihm lauscht im Kreise,", "tokens": ["Wo", "das", "Volk", "ihm", "lauscht", "im", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dumpf, gleichwie aus bangem Traum,", "tokens": ["Dumpf", ",", "gleich\u00b7wie", "aus", "ban\u00b7gem", "Traum", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Singt der Spielmann seine Weise:", "tokens": ["Singt", "der", "Spiel\u00b7mann", "sei\u00b7ne", "Wei\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00bbhaltet Mut und Schwert bereit!", "tokens": ["\u00bb", "hal\u00b7tet", "Mut", "und", "Schwert", "be\u00b7reit", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "NN", "KON", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Eisern, eisern ist die Zeit.", "tokens": ["Ei\u00b7sern", ",", "ei\u00b7sern", "ist", "die", "Zeit", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "S\u00fchnung hofft' ich manches Jahr,", "tokens": ["S\u00fch\u00b7nung", "hofft'", "ich", "man\u00b7ches", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und getrost zu neuen Siegen", "tokens": ["Und", "ge\u00b7trost", "zu", "neu\u00b7en", "Sie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sah ich schon den Doppelaar", "tokens": ["Sah", "ich", "schon", "den", "Dop\u00b7pe\u00b7laar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit dem Aar der Zollern fliegen.", "tokens": ["Mit", "dem", "Aar", "der", "Zol\u00b7lern", "flie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Weh, der Sieg gebar den Streit,", "tokens": ["Weh", ",", "der", "Sieg", "ge\u00b7bar", "den", "Streit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Eisern, eisern ist die Zeit.", "tokens": ["Ei\u00b7sern", ",", "ei\u00b7sern", "ist", "die", "Zeit", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Dort ein Kaisertum im Ost,", "tokens": ["Dort", "ein", "Kai\u00b7ser\u00b7tum", "im", "Ost", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hier ein Reich vom Fels zum Meere,", "tokens": ["Hier", "ein", "Reich", "vom", "Fels", "zum", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eins des andern Schirm und Trost,", "tokens": ["Eins", "des", "an\u00b7dern", "Schirm", "und", "Trost", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Beide gleich an Macht und Ehre \u2013", "tokens": ["Bei\u00b7de", "gleich", "an", "Macht", "und", "Eh\u00b7re", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "APPR", "NN", "KON", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sch\u00f6ner Traum, wie liegst du weit!", "tokens": ["Sch\u00f6\u00b7ner", "Traum", ",", "wie", "liegst", "du", "weit", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWAV", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Eisern, eisern ist die Zeit.", "tokens": ["Ei\u00b7sern", ",", "ei\u00b7sern", "ist", "die", "Zeit", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Trotz im Auge, Groll im Mund", "tokens": ["Trotz", "im", "Au\u00b7ge", ",", "Groll", "im", "Mund"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "APPRART", "NN", "$,", "ADJD", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Stehn, die j\u00fcngst noch Kampfgesellen;", "tokens": ["Stehn", ",", "die", "j\u00fcngst", "noch", "Kampf\u00b7ge\u00b7sel\u00b7len", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "ADV", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, nicht birgt das Land am Sund", "tokens": ["Ach", ",", "nicht", "birgt", "das", "Land", "am", "Sund"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PTKNEG", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihres Haders tiefste Quellen.", "tokens": ["Ih\u00b7res", "Ha\u00b7ders", "tiefs\u00b7te", "Quel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Deutschland gilt, was sie entzweit;", "tokens": ["Deutschland", "gilt", ",", "was", "sie", "ent\u00b7zweit", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Eisern, eisern ist die Zeit.", "tokens": ["Ei\u00b7sern", ",", "ei\u00b7sern", "ist", "die", "Zeit", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Deutschland gilt's, und ruhelos", "tokens": ["Deutschland", "gilt's", ",", "und", "ru\u00b7he\u00b7los"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "$,", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Glimmt die Zwietracht fort der beiden,", "tokens": ["Glimmt", "die", "Zwiet\u00b7racht", "fort", "der", "bei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "ART", "PIAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df in aller Gauen Scho\u00df,", "tokens": ["Da\u00df", "in", "al\u00b7ler", "Gau\u00b7en", "Scho\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PIAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die da Br\u00fcder sind, sich scheiden,", "tokens": ["Die", "da", "Br\u00fc\u00b7der", "sind", ",", "sich", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "VAFIN", "$,", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und des Hasses Saat gedeiht;", "tokens": ["Und", "des", "Has\u00b7ses", "Saat", "ge\u00b7deiht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Eisern, eisern ist die Zeit.", "tokens": ["Ei\u00b7sern", ",", "ei\u00b7sern", "ist", "die", "Zeit", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Horch, schon l\u00e4\u00dft sich dumpf bei Nacht", "tokens": ["Horch", ",", "schon", "l\u00e4\u00dft", "sich", "dumpf", "bei", "Nacht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADV", "VVFIN", "PRF", "ADJD", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Unterm Grund ein Brausen sp\u00fcren,", "tokens": ["Un\u00b7term", "Grund", "ein", "Brau\u00b7sen", "sp\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hoch zu Rosse wie zur Schlacht", "tokens": ["Hoch", "zu", "Ros\u00b7se", "wie", "zur", "Schlacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "NN", "KOKOM", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ziehn in Wolken die Walk\u00fcren,", "tokens": ["Ziehn", "in", "Wol\u00b7ken", "die", "Wal\u00b7k\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Angst und Schw\u00fcle weit und breit!", "tokens": ["Angst", "und", "Schw\u00fc\u00b7le", "weit", "und", "breit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Eisern, eisern ist die Zeit.", "tokens": ["Ei\u00b7sern", ",", "ei\u00b7sern", "ist", "die", "Zeit", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Brich herein denn, Schicksalstag!", "tokens": ["Brich", "her\u00b7ein", "denn", ",", "Schick\u00b7sals\u00b7tag", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "PTKVZ", "KON", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ende diese Not im Wetter!", "tokens": ["En\u00b7de", "die\u00b7se", "Not", "im", "Wet\u00b7ter", 