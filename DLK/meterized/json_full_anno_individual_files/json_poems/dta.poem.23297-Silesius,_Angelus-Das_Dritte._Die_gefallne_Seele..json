{"dta.poem.23297": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "Das Dritte.  \n Die gefallne Seele.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Ich war ein Englisch Bild: nu bin ich gleich den", "tokens": ["Ich", "war", "ein", "Eng\u00b7lisch", "Bild", ":", "nu", "bin", "ich", "gleich", "den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$.", "ADV", "VAFIN", "PPER", "ADV", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Thieren:", "tokens": ["Thie\u00b7ren", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Ich schwebt\u2019 im Paradei\u00df in lautrer Fr\u00f6lichkeit:", "tokens": ["Ich", "schwebt'", "im", "Pa\u00b7ra\u00b7dei\u00df", "in", "laut\u00b7rer", "Fr\u00f6\u00b7lich\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nu sitz\u2019 ich auf der Erd\u2019 in lauter Augst und Leid:", "tokens": ["Nu", "sitz'", "ich", "auf", "der", "Erd'", "in", "lau\u00b7ter", "Augst", "und", "Leid", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "APPR", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es konte mich kein Grimm der untren Welt ber\u00fchre\u0303:", "tokens": ["Es", "kon\u00b7te", "mich", "kein", "Grimm", "der", "un\u00b7tren", "Welt", "be\u00b7r\u00fchr\u1ebd", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIAT", "NE", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nu schmeltz\u2019 ich fast f\u00fcr Hitz\u2019/ und mu\u00df f\u00fcr Frost er-", "tokens": ["Nu", "schmeltz'", "ich", "fast", "f\u00fcr", "Hitz'", "/", "und", "mu\u00df", "f\u00fcr", "Frost", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NE", "$(", "KON", "VMFIN", "APPR", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "frieren/", "tokens": ["frie\u00b7ren", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Und f\u00fchle tausend Weh. Ich war ein Herr der Zeit:", "tokens": ["Und", "f\u00fch\u00b7le", "tau\u00b7send", "Weh", ".", "Ich", "war", "ein", "Herr", "der", "Zeit", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "CARD", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nu meistert sie mich selbst. Ich war mir selbst mein", "tokens": ["Nu", "meis\u00b7tert", "sie", "mich", "selbst", ".", "Ich", "war", "mir", "selbst", "mein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "$.", "PPER", "VAFIN", "PPER", "ADV", "PPOSAT"], "meter": "-+---+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "Kleid/(ziehren.", "tokens": ["Kleid", "/", "(", "zieh\u00b7ren", "."], "token_info": ["word", "punct", "punct", "word", "punct"], "pos": ["NN", "$(", "$(", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Nu mu\u00df ich mich au\u00df Noth mit frembden Federn", "tokens": ["Nu", "mu\u00df", "ich", "mich", "au\u00df", "Noth", "mit", "fremb\u00b7den", "Fe\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "APPR", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Gott sah mich freundlich an/ un\u0303 hie\u00df mich liebes Kind:", "tokens": ["Gott", "sah", "mich", "freund\u00b7lich", "an", "/", "u\u00f1", "hie\u00df", "mich", "lie\u00b7bes", "Kind", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$(", "KON", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Nu schr\u00f6kket mich sein zorn/ un\u0303 st\u00f6st mich weg die s\u00fcnd.", "tokens": ["Nu", "schr\u00f6k\u00b7ket", "mich", "sein", "zorn", "/", "u\u00f1", "st\u00f6st", "mich", "weg", "die", "s\u00fcnd", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$(", "KON", "VVFIN", "PPER", "ADV", "ART", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ich bin mit st\u00e4ter Furcht erf\u00fcllet und umbgeben:", "tokens": ["Ich", "bin", "mit", "st\u00e4\u00b7ter", "Furcht", "er\u00b7f\u00fcl\u00b7let", "und", "umb\u00b7ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "VVFIN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ich schau mein Ungel\u00fck mit eignen Augen an:", "tokens": ["Ich", "schau", "mein", "Un\u00b7ge\u00b7l\u00fck", "mit", "eig\u00b7nen", "Au\u00b7gen", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Der Teuffel und der Tod die stehn mir nach dem Lebe\u0303.", "tokens": ["Der", "Teuf\u00b7fel", "und", "der", "Tod", "die", "stehn", "mir", "nach", "dem", "Le\u00b7b\u1ebd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "ART", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.17": {"text": "Ach ach ich orme Seel! Was hab ich doch gethan!", "tokens": ["Ach", "ach", "ich", "or\u00b7me", "Seel", "!", "Was", "hab", "ich", "doch", "ge\u00b7than", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "PPER", "ADJA", "NN", "$.", "PWS", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}