{"textgrid.poem.36490": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "14. Der schlafende L\u00f6we", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der K\u00f6nig L\u00f6we schlief. Von weitem sa\u00df ein Luchs;", "tokens": ["Der", "K\u00f6\u00b7nig", "L\u00f6\u00b7we", "schlief", ".", "Von", "wei\u00b7tem", "sa\u00df", "ein", "Luchs", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "$.", "APPR", "PIS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dieser hatte Lust, den L\u00f6wen aufzuwecken.", "tokens": ["Und", "die\u00b7ser", "hat\u00b7te", "Lust", ",", "den", "L\u00f6\u00b7wen", "auf\u00b7zu\u00b7we\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "NN", "$,", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Den L\u00f6wen, unsern Herrn? den L\u00f6wen, unser Schrecken,", "tokens": ["Den", "L\u00f6\u00b7wen", ",", "un\u00b7sern", "Herrn", "?", "den", "L\u00f6\u00b7wen", ",", "un\u00b7ser", "Schre\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "$.", "ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "La\u00df, la\u00df ihn schlafen! sprach ein Fuchs.", "tokens": ["La\u00df", ",", "la\u00df", "ihn", "schla\u00b7fen", "!", "sprach", "ein", "Fuchs", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "VVIMP", "PPER", "VVINF", "$.", "VVFIN", "ART", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Herr Schmeichler, gut! ich will den L\u00f6wen schlafen lassen,", "tokens": ["Herr", "Schmeich\u00b7ler", ",", "gut", "!", "ich", "will", "den", "L\u00f6\u00b7wen", "schla\u00b7fen", "las\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADJD", "$.", "PPER", "VMFIN", "ART", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil, wenn er schl\u00e4ft, er uns kein Todesurteil spricht!", "tokens": ["Weil", ",", "wenn", "er", "schl\u00e4ft", ",", "er", "uns", "kein", "To\u00b7des\u00b7ur\u00b7teil", "spricht", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPER", "VVFIN", "$,", "PPER", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Antwortete der Luchs, mit sp\u00f6ttischem Gesicht.", "tokens": ["Ant\u00b7wor\u00b7te\u00b7te", "der", "Luchs", ",", "mit", "sp\u00f6t\u00b7ti\u00b7schem", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NE", "$,", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.3": {"line.1": {"text": "Auf manchen K\u00f6nig mag die kleine Fabel passen,", "tokens": ["Auf", "man\u00b7chen", "K\u00f6\u00b7nig", "mag", "die", "klei\u00b7ne", "Fa\u00b7bel", "pas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Au