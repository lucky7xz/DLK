{"textgrid.poem.48724": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "11. An das Frauenzimmer, wider die Misg\u00f6nner der deutschen Poesie", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kan denn der Himmel auch, die G\u00f6tter und G\u00f6ttinnen,", "tokens": ["Kan", "denn", "der", "Him\u00b7mel", "auch", ",", "die", "G\u00f6t\u00b7ter", "und", "G\u00f6t\u00b7tin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "ADV", "$,", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "f\u00fcr dir nicht bleiben frei, du schlangenarger Neid?", "tokens": ["f\u00fcr", "dir", "nicht", "blei\u00b7ben", "frei", ",", "du", "schlan\u00b7gen\u00b7ar\u00b7ger", "Neid", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKNEG", "VVINF", "ADJD", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du bl\u00e4sest deinen Gift auch auf die Ewigkeit,", "tokens": ["Du", "bl\u00e4\u00b7sest", "dei\u00b7nen", "Gift", "auch", "auf", "die", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "auf ", "tokens": ["auf"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}}, "stanza.2": {"line.1": {"text": "Schm\u00e4h immer, was du kanst, du Kind der Plutoninnen,", "tokens": ["Schm\u00e4h", "im\u00b7mer", ",", "was", "du", "kanst", ",", "du", "Kind", "der", "Plu\u00b7to\u00b7nin\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PWS", "PPER", "VMFIN", "$,", "PPER", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "nein, ", "tokens": ["nein", ","], "token_info": ["word", "punct"], "pos": ["PTKANT", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "und dich und deinen Schein. Dein Lohn ist f\u00fcr dein Leid,", "tokens": ["und", "dich", "und", "dei\u00b7nen", "Schein", ".", "Dein", "Lohn", "ist", "f\u00fcr", "dein", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "KON", "PPOSAT", "NN", "$.", "PPOSAT", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "wo Styx und Phlegethon mit Harz und Schwefel rinnen.", "tokens": ["wo", "Styx", "und", "Phle\u00b7ge\u00b7thon", "mit", "Harz", "und", "Schwe\u00b7fel", "rin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "KON", "NN", "APPR", "NE", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wenn ihr nur, sch\u00f6nstes Volk, ihr Menschg\u00f6ttinnen, wi\u00dft,", "tokens": ["Wenn", "ihr", "nur", ",", "sch\u00f6ns\u00b7tes", "Volk", ",", "ihr", "Menschg\u00f6t\u00b7tin\u00b7nen", ",", "wi\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "ADJA", "NN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "da\u00df euch die neue Kunst zu Lob erfunden ist,", "tokens": ["da\u00df", "euch", "die", "neu\u00b7e", "Kunst", "zu", "Lob", "er\u00b7fun\u00b7den", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "wie denn ihr euren Preis so h\u00f6rt durch sie vermehren,", "tokens": ["wie", "denn", "ihr", "eu\u00b7ren", "Preis", "so", "h\u00f6rt", "durch", "sie", "ver\u00b7meh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "PPOSAT", "NN", "ADV", "VVFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "so mag der Lobesalp gleich dr\u00fccken, was er kan,", "tokens": ["so", "mag", "der", "Lo\u00b7be\u00b7salp", "gleich", "dr\u00fc\u00b7cken", ",", "was", "er", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADV", "VVINF", "$,", "PWS", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "wir kommen doch empor. Das steiget Himmel an,", "tokens": ["wir", "kom\u00b7men", "doch", "em\u00b7por", ".", "Das", "stei\u00b7get", "Him\u00b7mel", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$.", "PDS", "VVFIN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "was Gott und G\u00f6tter gleich auf diesem Boden ehren.", "tokens": ["was", "Gott", "und", "G\u00f6t\u00b7ter", "gleich", "auf", "die\u00b7sem", "Bo\u00b7den", "eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"],