{"textgrid.poem.61239": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Im M\u00e4rz", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist mir eben angetan,", "tokens": ["Es", "ist", "mir", "e\u00b7ben", "an\u00b7ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zwei sch\u00f6ne Augen sahn mich an,", "tokens": ["Zwei", "sch\u00f6\u00b7ne", "Au\u00b7gen", "sahn", "mich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und in den s\u00fc\u00dfen, feuchten Schein", "tokens": ["Und", "in", "den", "s\u00fc\u00b7\u00dfen", ",", "feuch\u00b7ten", "Schein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Blickt' ich zu tief, zu tief hinein.", "tokens": ["Blickt'", "ich", "zu", "tief", ",", "zu", "tief", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKA", "ADJD", "$,", "PTKA", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mir schwirrt der Kopf, mir gl\u00fchn die Wangen,", "tokens": ["Mir", "schwirrt", "der", "Kopf", ",", "mir", "gl\u00fchn", "die", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra