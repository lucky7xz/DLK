{"dta.poem.408": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Auf  Solindens  Nahmens-Tag.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "O Sonne! die du pflegst best\u00e4ndig zu erfreuen", "tokens": ["O", "Son\u00b7ne", "!", "die", "du", "pflegst", "be\u00b7st\u00e4n\u00b7dig", "zu", "er\u00b7freu\u00b7en"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "PRELS", "PPER", "VVFIN", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dis grosse Rund der Welt/ zieh deine Strahlen ein/", "tokens": ["Dis", "gros\u00b7se", "Rund", "der", "Welt", "/", "zieh", "dei\u00b7ne", "Strah\u00b7len", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "ART", "NN", "$(", "VVFIN", "PPOSAT", "NN", "ART", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "O Sonne! die du pflegst die Erde zu erfreuen/", "tokens": ["O", "Son\u00b7ne", "!", "die", "du", "pflegst", "die", "Er\u00b7de", "zu", "er\u00b7freu\u00b7en", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PRELS", "PPER", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Heut mu\u00df zur\u00fccke stehn dein angenehmer Schein.", "tokens": ["Heut", "mu\u00df", "zu\u00b7r\u00fc\u00b7cke", "stehn", "dein", "an\u00b7ge\u00b7neh\u00b7mer", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Heut geht die Sonne auf die uns ", "tokens": ["Heut", "geht", "die", "Son\u00b7ne", "auf", "die", "uns"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PRELS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Indem ein holder Glantz aus ihren Augen bricht/", "tokens": ["In\u00b7dem", "ein", "hol\u00b7der", "Glantz", "aus", "ih\u00b7ren", "Au\u00b7gen", "bricht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Nahme kommt dazu/ einjeder der sie liebet/", "tokens": ["Der", "Nah\u00b7me", "kommt", "da\u00b7zu", "/", "ein\u00b7je\u00b7der", "der", "sie", "lie\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "$(", "APPR", "PRELS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Spricht/ Sonne weiche doch vor diesem Wunder-Licht.", "tokens": ["Spricht", "/", "Son\u00b7ne", "wei\u00b7che", "doch", "vor", "die\u00b7sem", "Wun\u00b7der\u00b7Licht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "NN", "VVFIN", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}