{"textgrid.poem.48767": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "54. Auf Jungfrau Christinen M\u00fcllers mit Herrn Kunrad M\u00e4u\u00dflern Hochzeit zu Reval in Liefland", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Reicht, edle ", "tokens": ["Reicht", ",", "ed\u00b7le"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "wie er euch seine beut, und schlagt ihm zu mit Treuen,", "tokens": ["wie", "er", "euch", "sei\u00b7ne", "beut", ",", "und", "schlagt", "ihm", "zu", "mit", "Treu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "PPOSAT", "VVFIN", "$,", "KON", "VVFIN", "PPER", "APPR", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "da\u00df, wie er euer sich, ihr seiner euch wolt freuen,", "tokens": ["da\u00df", ",", "wie", "er", "eu\u00b7er", "sich", ",", "ihr", "sei\u00b7ner", "euch", "wolt", "freu\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWAV", "PPER", "PPOSAT", "PRF", "$,", "PPER", "PPOSAT", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und leget hinter euch den unerfreuten Stand.", "tokens": ["und", "le\u00b7get", "hin\u00b7ter", "euch", "den", "un\u00b7er\u00b7freu\u00b7ten", "Stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was wolt ihr l\u00e4nger euch sein lassen unbekant", "tokens": ["Was", "wolt", "ihr", "l\u00e4n\u00b7ger", "euch", "sein", "las\u00b7sen", "un\u00b7be\u00b7kant"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "ADJD", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "das weltbekante Tun und etwas Solches scheuen,", "tokens": ["das", "welt\u00b7be\u00b7kan\u00b7te", "Tun", "und", "et\u00b7was", "Sol\u00b7ches", "scheu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ADV", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "das euch die Tr\u00e4ume doch stets in die Sinnen streuen", "tokens": ["das", "euch", "die", "Tr\u00e4u\u00b7me", "doch", "stets", "in", "die", "Sin\u00b7nen", "streu\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "ART", "NN", "ADV", "ADV", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "und k\u00fchne wird ver\u00fcbt durch di\u00df und jenes Land?", "tokens": ["und", "k\u00fch\u00b7ne", "wird", "ver\u00b7\u00fcbt", "durch", "di\u00df", "und", "je\u00b7nes", "Land", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "VAFIN", "VVPP", "APPR", "PDS", "KON", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Vollzieht den starken Bund mit einem treuen Leben,", "tokens": ["Voll\u00b7zieht", "den", "star\u00b7ken", "Bund", "mit", "ei\u00b7nem", "treu\u00b7en", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und wi\u00dft, was ihr euch nehmt, das k\u00f6nnt ihr euch auch geben.", "tokens": ["und", "wi\u00dft", ",", "was", "ihr", "euch", "nehmt", ",", "das", "k\u00f6nnt", "ihr", "euch", "auch", "ge\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWS", "PPER", "PPER", "VVFIN", "$,", "PDS", "VVFIN", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verwechselt euren Stand. Die ", "tokens": ["Ver\u00b7wech\u00b7selt", "eu\u00b7ren", "Stand", ".", "Die"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "der ", "tokens": ["der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "so ist die M\u00e4usefall und Mehlsack allzeit voll.", "tokens": ["so", "ist", "die", "M\u00e4u\u00b7se\u00b7fall", "und", "Mehl\u00b7sack", "all\u00b7zeit", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "KON", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo solche M\u00fchlen sind, da sind auch solche M\u00e4use.", "tokens": ["Wo", "sol\u00b7che", "M\u00fch\u00b7len", "sind", ",", "da", "sind", "auch", "sol\u00b7che", "M\u00e4u\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VAFIN", "$,", "ADV", "VAFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1":