{"dta.poem.12695": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An einen Wohlseeligen.  \n  D.  V. A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So rei\u00dft sein edler geist sich endlich von den bauden", "tokens": ["So", "rei\u00dft", "sein", "ed\u00b7ler", "geist", "sich", "end\u00b7lich", "von", "den", "bau\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "PRF", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des siechen leibes lo\u00df, und schwingt sich himmel-an!", "tokens": ["Des", "sie\u00b7chen", "lei\u00b7bes", "lo\u00df", ",", "und", "schwingt", "sich", "him\u00b7mel\u00b7an", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,", "KON", "VVFIN", "PRF", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das ist ein helden-sprung, den er zu GOtt gethan.", "tokens": ["Das", "ist", "ein", "hel\u00b7den\u00b7sprung", ",", "den", "er", "zu", "Gott", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wo bleibt das podagra? hier in den Niederlanden.", "tokens": ["Wo", "bleibt", "das", "po\u00b7da\u00b7gra", "?", "hier", "in", "den", "Nie\u00b7der\u00b7lan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART",