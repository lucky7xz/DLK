{"dta.poem.2247": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Epigramma  \n auf einen erbaulich lehrenden  \n  aber \u00fcbel lebenden Priester,  \n  wie selbiger starb.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Sch\u00f6pfer, gieb mir doch die Gabe,", "tokens": ["Mein", "Sch\u00f6p\u00b7fer", ",", "gieb", "mir", "doch", "die", "Ga\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVIMP", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ich empfinde, was ich habe!", "tokens": ["Da\u00df", "ich", "emp\u00b7fin\u00b7de", ",", "was", "ich", "ha\u00b7be", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "PWS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}