{"dta.poem.19055": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "XxXVI.  Auf Graf Erdman Ludwig  \n Henckels Ende.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Und du wirst wegger\u00fcckt, des Vaters Augen-Weide,", "tokens": ["Und", "du", "wirst", "weg\u00b7ge\u00b7r\u00fcckt", ",", "des", "Va\u00b7ters", "Au\u00b7gen\u00b7Wei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "VVPP", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein ", "tokens": ["Ein"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Wer richtet ihn wol auf bey einem solchen Leide,", "tokens": ["Wer", "rich\u00b7tet", "ihn", "wol", "auf", "bey", "ei\u00b7nem", "sol\u00b7chen", "Lei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "APPR", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und wie bezeigen wir, was treue Br\u00fcder sind?", "tokens": ["Und", "wie", "be\u00b7zei\u00b7gen", "wir", ",", "was", "treu\u00b7e", "Br\u00fc\u00b7der", "sind", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "PPER", "$,", "PWS", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Hier, schreibet mir ein Freund, soll sich dein Kiel bem\u00fchn:", "tokens": ["Hier", ",", "schrei\u00b7bet", "mir", "ein", "Freund", ",", "soll", "sich", "dein", "Kiel", "be\u00b7m\u00fchn", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPER", "ART", "NN", "$,", "VMFIN", "PRF", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das ists auch, was ich kan, ", "tokens": ["Das", "ists", "auch", ",", "was", "ich", "kan", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "$,", "PWS", "PPER", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Nur m\u00f6cht\u2019 ein andrer Kopf den Plan dazu entwerffen,", "tokens": ["Nur", "m\u00f6cht'", "ein", "an\u00b7drer", "Kopf", "den", "Plan", "da\u00b7zu", "ent\u00b7werf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "ART", "NN", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ein Kopf, der ein Gem\u00fcth von z\u00e4rtrer Neigung f\u00fchrt:", "tokens": ["Ein", "Kopf", ",", "der", "ein", "Ge\u00b7m\u00fcth", "von", "z\u00e4r\u00b7trer", "Nei\u00b7gung", "f\u00fchrt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der meine hat die Art dem Leser einzusch\u00e4rffen,", "tokens": ["Der", "mei\u00b7ne", "hat", "die", "Art", "dem", "Le\u00b7ser", "ein\u00b7zu\u00b7sch\u00e4rf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da\u00df einen Christen leicht kein zeitlich Leiden r\u00fchrt,", "tokens": ["Da\u00df", "ei\u00b7nen", "Chris\u00b7ten", "leicht", "kein", "zeit\u00b7lich", "Lei\u00b7den", "r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "PIAT", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Auch hat er Grund dazu; weil Petrus ungescheut,", "tokens": ["Auch", "hat", "er", "Grund", "da\u00b7zu", ";", "weil", "Pet\u00b7rus", "un\u00b7ge\u00b7scheut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "PAV", "$.", "KOUS", "FM", "FM", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Beym Leiden froh zu seyn, den Gl\u00e4ubigen gebeut.", "tokens": ["Beym", "Lei\u00b7den", "froh", "zu", "seyn", ",", "den", "Gl\u00e4u\u00b7bi\u00b7gen", "ge\u00b7beut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "PTKZU", "VAINF", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und wenn ich von ", "tokens": ["Und", "wenn", "ich", "von"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR"], "meter": "-+-+", "measure": "iambic.di"}, "line.14": {"text": "Verschrieb ich mich vielleicht, und spr\u00e4che von ", "tokens": ["Ver\u00b7schrieb", "ich", "mich", "viel\u00b7leicht", ",", "und", "spr\u00e4\u00b7che", "von"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PRF", "ADV", "$,", "KON", "VVFIN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Wenn auch ein Vater-Hertz vor Schmertzen brechen wolte,", "tokens": ["Wenn", "auch", "ein", "Va\u00b7ter\u00b7Hertz", "vor", "Schmert\u00b7zen", "bre\u00b7chen", "wol\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ich fragte dennoch wohl: Wo mit den Kindern hin?", "tokens": ["Ich", "frag\u00b7te", "den\u00b7noch", "wohl", ":", "Wo", "mit", "den", "Kin\u00b7dern", "hin", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$.", "PWAV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Sie sollen mit der Zeit vor GOttes Throne stehn,", "tokens": ["Sie", "sol\u00b7len", "mit", "der", "Zeit", "vor", "Got\u00b7tes", "Thro\u00b7ne", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "APPR", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ists n\u00f6thig, da\u00df sie erst der S\u00fcnd ins Netze gehn?", "tokens": ["Ists", "n\u00f6\u00b7thig", ",", "da\u00df", "sie", "erst", "der", "S\u00fcnd", "ins", "Net\u00b7ze", "gehn", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "O seliger Verlust! So will ich stets verliehren,", "tokens": ["O", "se\u00b7li\u00b7ger", "Ver\u00b7lust", "!", "So", "will", "ich", "stets", "ver\u00b7lieh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "ADV", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wenn dadurch, da\u00df mirs fehlt, die Liebe Zugang kriegt;", "tokens": ["Wenn", "da\u00b7durch", ",", "da\u00df", "mirs", "fehlt", ",", "die", "Lie\u00b7be", "Zu\u00b7gang", "kriegt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PAV", "$,", "KOUS", "NE", "VVFIN", "$,", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ein solcher Sieg ist werth darum zu triumphiren,", "tokens": ["Ein", "sol\u00b7cher", "Sieg", "ist", "werth", "da\u00b7rum", "zu", "tri\u00b7um\u00b7phi\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "ADJD", "PAV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Wo man den Feind zugleich gesehen und besiegt.", "tokens": ["Wo", "man", "den", "Feind", "zu\u00b7gleich", "ge\u00b7se\u00b7hen", "und", "be\u00b7siegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "ADV", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Pflegt bey dergleichen Fall der Lust was abzugehn;", "tokens": ["Pflegt", "bey", "derg\u00b7lei\u00b7chen", "Fall", "der", "Lust", "was", "ab\u00b7zu\u00b7gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIS", "NN", "ART", "NN", "PIS", "VVIZU", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "So kan uns auch daraus nicht so viel Streit entstehn.", "tokens": ["So", "kan", "uns", "auch", "da\u00b7raus", "nicht", "so", "viel", "Streit", "ent\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PAV", "PTKNEG", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.25": {"text": "Allein, wem wolte ich hiebey den Argwohn wehren,", "tokens": ["Al\u00b7lein", ",", "wem", "wol\u00b7te", "ich", "hie\u00b7bey", "den", "Arg\u00b7wohn", "weh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWS", "VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Da\u00df dieser Ausspruch nur ein Eigenwille sey?", "tokens": ["Da\u00df", "die\u00b7ser", "Aus\u00b7spruch", "nur", "ein", "Ei\u00b7gen\u00b7wil\u00b7le", "sey", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Vielleicht beschweret es, die Kinder zu ern\u00e4hren?", "tokens": ["Viel\u00b7leicht", "be\u00b7schwe\u00b7ret", "es", ",", "die", "Kin\u00b7der", "zu", "er\u00b7n\u00e4h\u00b7ren", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Sie binden heftig an, man ist wohl lieber frey;", "tokens": ["Sie", "bin\u00b7den", "hef\u00b7tig", "an", ",", "man", "ist", "wohl", "lie\u00b7ber", "frey", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "PTKVZ", "$,", "PIS", "VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Sie brauchen Sorg und M\u00fch, man m\u00fchet sich nicht gern,", "tokens": ["Sie", "brau\u00b7chen", "Sorg", "und", "M\u00fch", ",", "man", "m\u00fc\u00b7het", "sich", "nicht", "gern", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,", "PIS", "VMFIN", "PRF", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Drum \u00fcberl\u00e4st man sie aus Tr\u00e4gheit an den HErrn?", "tokens": ["Drum", "\u00fc\u00b7berl\u00b7\u00e4st", "man", "sie", "aus", "Tr\u00e4g\u00b7heit", "an", "den", "Herrn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIS", "PPER", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wohlan! ich suche nicht dem Vorwurf auszuweichen:", "tokens": ["Wo\u00b7hlan", "!", "ich", "su\u00b7che", "nicht", "dem", "Vor\u00b7wurf", "aus\u00b7zu\u00b7wei\u00b7chen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VVFIN", "PTKNEG", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Ich halte, GOtt sey Danck! nichts auf Entschuldigung,", "tokens": ["Ich", "hal\u00b7te", ",", "Gott", "sey", "Danck", "!", "nichts", "auf", "Ent\u00b7schul\u00b7di\u00b7gung", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "VAFIN", "NN", "$.", "PIS", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Um aber meinen Zweck im Haupt-Punct zu erreichen;", "tokens": ["Um", "a\u00b7ber", "mei\u00b7nen", "Zweck", "im", "Haup\u00b7tPunct", "zu", "er\u00b7rei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "PPOSAT", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "So ist di\u00df eine Wort vielleicht Bewei\u00df genung.", "tokens": ["So", "ist", "di\u00df", "ei\u00b7ne", "Wort", "viel\u00b7leicht", "Be\u00b7wei\u00df", "ge\u00b7nung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PDS", "ART", "NN", "ADV", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Es setze sich ein Kind vergeblich aus der Ruh,", "tokens": ["Es", "set\u00b7ze", "sich", "ein", "Kind", "ver\u00b7geb\u00b7lich", "aus", "der", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "So bald es Kummer hat, was doch der Vater thu.", "tokens": ["So", "bald", "es", "Kum\u00b7mer", "hat", ",", "was", "doch", "der", "Va\u00b7ter", "thu", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "NN", "VAFIN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Gewi\u00df, ", "tokens": ["Ge\u00b7wi\u00df", ","], "token_info": ["word", "punct"], "pos": ["PTKANT", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.38": {"text": "Da\u00df sich Jmmanuels schon lange nicht mehr sch\u00e4mt,", "tokens": ["Da\u00df", "sich", "Jm\u00b7ma\u00b7nu\u00b7els", "schon", "lan\u00b7ge", "nicht", "mehr", "sch\u00e4mt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "NE", "ADV", "ADV", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Di\u00df eben sammlet dir die ausgeschweiften Triebe;", "tokens": ["Di\u00df", "e\u00b7ben", "samm\u00b7let", "dir", "die", "aus\u00b7ge\u00b7schweif\u00b7ten", "Trie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Di\u00df macht, da\u00df dein Gem\u00fcth sich heute nicht mehr gr\u00e4mt;", "tokens": ["Di\u00df", "macht", ",", "da\u00df", "dein", "Ge\u00b7m\u00fcth", "sich", "heu\u00b7te", "nicht", "mehr", "gr\u00e4mt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "PRF", "ADV", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Die Sorge presset dir kein banges Stehnen aus,", "tokens": ["Die", "Sor\u00b7ge", "pres\u00b7set", "dir", "kein", "ban\u00b7ges", "Steh\u00b7nen", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PIAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Wer einen Vater hat, dem h\u00e4lt der Vater hau\u00df.", "tokens": ["Wer", "ei\u00b7nen", "Va\u00b7ter", "hat", ",", "dem", "h\u00e4lt", "der", "Va\u00b7ter", "hau\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VAFIN", "$,", "PRELS", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Du l\u00e4ssest deinen Sohn in allzu guten H\u00e4nden,", "tokens": ["Du", "l\u00e4s\u00b7sest", "dei\u00b7nen", "Sohn", "in", "all\u00b7zu", "gu\u00b7ten", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PTKA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Die Schule, die er hier nur angefangen hat,", "tokens": ["Die", "Schu\u00b7le", ",", "die", "er", "hier", "nur", "an\u00b7ge\u00b7fan\u00b7gen", "hat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Die mag er auf dem Schoo\u00df der Liebe vollends enden;", "tokens": ["Die", "mag", "er", "auf", "dem", "Schoo\u00df", "der", "Lie\u00b7be", "vol\u00b7lends", "en\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Wo lernt man hurtiger? als in der neuen Stadt.", "tokens": ["Wo", "lernt", "man", "hur\u00b7ti\u00b7ger", "?", "als", "in", "der", "neu\u00b7en", "Stadt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PIS", "ADJD", "$.", "KOUS", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Dein Auge, das bi\u00dfher vor diesen Sohn gewacht,", "tokens": ["Dein", "Au\u00b7ge", ",", "das", "bi\u00df\u00b7her", "vor", "die\u00b7sen", "Sohn", "ge\u00b7w