{"dta.poem.12825": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Grabschrifft einer alten jungfer.  \n G. L.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die hitze war vorbey, und schiene zu erkalten,", "tokens": ["Die", "hit\u00b7ze", "war", "vor\u00b7bey", ",", "und", "schie\u00b7ne", "zu", "er\u00b7kal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "KON", "ADJA", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die lippen wurden bla\u00df, die augen roth und weit:", "tokens": ["Die", "lip\u00b7pen", "wur\u00b7den", "bla\u00df", ",", "die", "au\u00b7gen", "roth", "und", "weit", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die stirne wie\u00df nunmehr die ungeschickten falten;", "tokens": ["Die", "stir\u00b7ne", "wie\u00df", "nun\u00b7mehr", "die", "un\u00b7ge\u00b7schick\u00b7ten", "fal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ADV", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Drum deckte man mich zu, es war die h\u00f6chste zeit.", "tokens": ["Drum", "deck\u00b7te", "man", "mich", "zu", ",", "es", "war", "die", "h\u00f6chs\u00b7te", "zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIS", "PRF", "PTKVZ", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nur dieses wundert mich, da\u00df du mir in dem leben", "tokens": ["Nur", "die\u00b7ses", "wun\u00b7dert", "mich", ",", "da\u00df", "du", "mir", "in", "dem", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PDS", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PRF", "APPR", "ART", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Den jungfer-titul noch best\u00e4ndig hast gegeben;", "tokens": ["Den", "jung\u00b7fer\u00b7ti\u00b7tul", "noch", "be\u00b7st\u00e4n\u00b7dig", "hast", "ge\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "VAFIN", "VVPP", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Da du ihn doch wohl kennst, der mir die knospe nahm,", "tokens": ["Da", "du", "ihn", "doch", "wohl", "kennst", ",", "der", "mir", "die", "knos\u00b7pe", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADV", "VVFIN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Eh\u2019 noch das rothe blatt recht vorgekrochen kam.", "tokens": ["Eh'", "noch", "das", "ro\u00b7the", "blatt", "recht", "vor\u00b7ge\u00b7kro\u00b7chen", "kam", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "ADJD", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}