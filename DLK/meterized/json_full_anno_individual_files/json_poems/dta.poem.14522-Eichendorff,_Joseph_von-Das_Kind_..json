{"dta.poem.14522": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Das Kind .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Das Kind ruht aus vom Spielen,", "tokens": ["Das", "Kind", "ruht", "aus", "vom", "Spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Am Fenster rauscht die Nacht,", "tokens": ["Am", "Fens\u00b7ter", "rauscht", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Engel Gott's im K\u00fchlen", "tokens": ["Die", "En\u00b7gel", "Gott's", "im", "K\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Getreulich halten Wacht,", "tokens": ["Ge\u00b7treu\u00b7lich", "hal\u00b7ten", "Wacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Am Bettlein still sie stehen,", "tokens": ["Am", "Bet\u00b7tlein", "still", "sie", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Morgen graut noch kaum,", "tokens": ["Der", "Mor\u00b7gen", "graut", "noch", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sie k\u00fcssen's, eh sie gehen,", "tokens": ["Sie", "k\u00fcs\u00b7sen's", ",", "eh", "sie", "ge\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das Kindlein lacht im Traum.", "tokens": ["Das", "Kin\u00b7dlein", "lacht", "im", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}