{"dta.poem.18638": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "D ie  P hantasie in lieben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Wie offt ver\u00e4ndern sich doch lieben und Gedancken?", "tokens": ["Wie", "offt", "ver\u00b7\u00e4n\u00b7dern", "sich", "doch", "lie\u00b7ben", "und", "Ge\u00b7dan\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PRF", "ADV", "VVINF", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr wunderlicher Trieb ist selten einerley:", "tokens": ["Ihr", "wun\u00b7der\u00b7li\u00b7cher", "Trieb", "ist", "sel\u00b7ten", "ei\u00b7ner\u00b7ley", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bald f\u00fchren sie den Geist in die verbuhlten Schrancken/", "tokens": ["Bald", "f\u00fch\u00b7ren", "sie", "den", "Geist", "in", "die", "ver\u00b7buhl\u00b7ten", "Schran\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und zeigen/ da\u00df daselbst die Lust unsch\u00e4tzbar sey/", "tokens": ["Und", "zei\u00b7gen", "/", "da\u00df", "da\u00b7selbst", "die", "Lust", "un\u00b7sch\u00e4tz\u00b7bar", "sey", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$(", "KOUS", "PAV", "ART", "NN", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bald lehren sie uns auch/ es w\u00e4ren Kinder possen/", "tokens": ["Bald", "leh\u00b7ren", "sie", "uns", "auch", "/", "es", "w\u00e4\u00b7ren", "Kin\u00b7der", "pos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "$(", "PPER", "VAFIN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und ein Verliebter sey mit Hasen-Schroot geschossen.", "tokens": ["Und", "ein", "Ver\u00b7lieb\u00b7ter", "sey", "mit", "Ha\u00b7sen\u00b7Schroot", "ge\u00b7schos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein M\u00e4dgen mu\u00df hier offt ein sch\u00f6ner Engel heissen/", "tokens": ["Ein", "M\u00e4d\u00b7gen", "mu\u00df", "hier", "offt", "ein", "sch\u00f6\u00b7ner", "En\u00b7gel", "heis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ADV", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein neues Himmelreich/ woran zwey Sonnen seyn/", "tokens": ["Ein", "neu\u00b7es", "Him\u00b7mel\u00b7reich", "/", "wo\u00b7ran", "zwey", "Son\u00b7nen", "seyn", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "PWAV", "CARD", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die rundte Stirne kan wie Alabaster gleisen/", "tokens": ["Die", "rund\u00b7te", "Stir\u00b7ne", "kan", "wie", "A\u00b7la\u00b7bas\u00b7ter", "glei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "KOKOM", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Nase steht sehr wohl/ der Hal\u00df ist Helffenbein.", "tokens": ["Die", "Na\u00b7se", "steht", "sehr", "wohl", "/", "der", "Hal\u00df", "ist", "Helf\u00b7fen\u00b7bein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "$(", "ART", "NN", "VAFIN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Marmor kan noch nicht den weissen Z\u00e4hnen gleichen/", "tokens": ["Der", "Mar\u00b7mor", "kan", "noch", "nicht", "den", "weis\u00b7sen", "Z\u00e4h\u00b7nen", "glei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "PTKNEG", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Schnee ist viel zu schwartz/ und mu\u00df den Br\u00fcsten weichen.", "tokens": ["Der", "Schnee", "ist", "viel", "zu", "schwartz", "/", "und", "mu\u00df", "den", "Br\u00fcs\u00b7ten", "wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "$(", "KON", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Lippen k\u00f6nnen so wie Purpur Nelcken bl\u00fchen/", "tokens": ["Die", "Lip\u00b7pen", "k\u00f6n\u00b7nen", "so", "wie", "Pur\u00b7pur", "Nel\u00b7cken", "bl\u00fc\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "KOKOM", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Rose wird besch\u00e4mt vor ihren Wangen stehn/", "tokens": ["Die", "Ro\u00b7se", "wird", "be\u00b7sch\u00e4mt", "vor", "ih\u00b7ren", "Wan\u00b7gen", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein Schwan/ wie wei\u00df er ist/ mu\u00df vor dem Leibe fliehen/", "tokens": ["Ein", "Schwan", "/", "wie", "wei\u00df", "er", "ist", "/", "mu\u00df", "vor", "dem", "Lei\u00b7be", "flie\u00b7hen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PWAV", "VVFIN", "PPER", "VAFIN", "$(", "VMFIN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der schlancke Fu\u00df kan hier in sch\u00f6nster Zierde gehn.", "tokens": ["Der", "schlan\u00b7cke", "Fu\u00df", "kan", "hier", "in", "sch\u00f6ns\u00b7ter", "Zier\u00b7de", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Man z\u00fcrnet/ wer sie nicht ein Meister-St\u00fcck will nennen/", "tokens": ["Man", "z\u00fcr\u00b7net", "/", "wer", "sie", "nicht", "ein", "Meis\u00b7ter\u00b7St\u00fcck", "will", "nen\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$(", "PWS", "PPER", "PTKNEG", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und wer nicht den Verstand vor himmlisch will erkennen.", "tokens": ["Und", "wer", "nicht", "den", "Ver\u00b7stand", "vor", "himm\u00b7lisch", "will", "er\u00b7ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PTKNEG", "ART", "NN", "APPR", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "So ist das Conterfait im Hertzen ein gegraben/", "tokens": ["So", "ist", "das", "Con\u00b7ter\u00b7fait", "im", "Hert\u00b7zen", "ein", "ge\u00b7gra\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPRART", "NN", "ART", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bi\u00df unser Monden-Sinn die Farben anders macht:", "tokens": ["Bi\u00df", "un\u00b7ser", "Mon\u00b7den\u00b7Sinn", "die", "Far\u00b7ben", "an\u00b7ders", "macht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die H\u00f6lle soll als denn kein solch Gespenste haben/", "tokens": ["Die", "H\u00f6l\u00b7le", "soll", "als", "denn", "kein", "solch", "Ge\u00b7spens\u00b7te", "ha\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "KOKOM", "ADV", "PIAT", "PIAT", "NN", "VAFIN", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "So wie ein Laugen-Sack triefft nun der Augen-Pracht/", "tokens": ["So", "wie", "ein", "Lau\u00b7gen\u00b7Sack", "triefft", "nun", "der", "Au\u00b7gen\u00b7Pracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "VVFIN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Stirne scheinet krump und gleichet Ziegel-Steinen/", "tokens": ["Die", "Stir\u00b7ne", "schei\u00b7net", "krump", "und", "glei\u00b7chet", "Zie\u00b7gel\u00b7Stei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "VVFIN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Nasen solte man von zwantzig Pfunden meinen:", "tokens": ["Die", "Na\u00b7sen", "sol\u00b7te", "man", "von", "zwant\u00b7zig", "Pfun\u00b7den", "mei\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PIS", "APPR", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Des Halses Helffenbein ist nun mit Ru\u00df beschmieret/", "tokens": ["Des", "Hal\u00b7ses", "Helf\u00b7fen\u00b7bein", "ist", "nun", "mit", "Ru\u00df", "be\u00b7schmie\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "ADV", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die blancke Z\u00e4hne sehn wie schwartzer Marmor aus/", "tokens": ["Die", "blan\u00b7cke", "Z\u00e4h\u00b7ne", "sehn", "wie", "schwart\u00b7zer", "Mar\u00b7mor", "aus", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "KOKOM", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Saffran hat die Brust recht wundersch\u00f6n gezieret/", "tokens": ["Der", "Saf\u00b7fran", "hat", "die", "Brust", "recht", "wun\u00b7der\u00b7sch\u00f6n", "ge\u00b7zie\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und auf den Lippen w\u00e4chst der beste Veilgen-Strau\u00df/", "tokens": ["Und", "auf", "den", "Lip\u00b7pen", "w\u00e4chst", "der", "bes\u00b7te", "Veil\u00b7gen\u00b7Strau\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Narcissen gelber Art bebl\u00fcmen ihre Wangen/", "tokens": ["Nar\u00b7cis\u00b7sen", "gel\u00b7ber", "Art", "be\u00b7bl\u00fc\u00b7men", "ih\u00b7re", "Wan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Leib kan wie die Haut des weissen Schwanes Prangen.", "tokens": ["Der", "Leib", "kan", "wie", "die", "Haut", "des", "weis\u00b7sen", "Schwa\u00b7nes", "Pran\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "KOKOM", "ART", "NN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Die Beine scheinen uns gekr\u00fcmmet und gebogen/", "tokens": ["Die", "Bei\u00b7ne", "schei\u00b7nen", "uns", "ge\u00b7kr\u00fcm\u00b7met", "und", "ge\u00b7bo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVPP", "KON", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bald ist der Schritt zu eng/ bald ist er gar zu weit/", "tokens": ["Bald", "ist", "der", "Schritt", "zu", "eng", "/", "bald", "ist", "er", "gar", "zu", "weit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKA", "ADJD", "$(", "ADV", "VAFIN", "PPER", "ADV", "PTKA", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da k\u00f6mmt ein Wackel-Ar - im gehen aufgezogen/", "tokens": ["Da", "k\u00f6mmt", "ein", "Wa\u00b7ckel\u00b7Ar", "im", "ge\u00b7hen", "auf\u00b7ge\u00b7zo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$(", "APPRART", "VVINF", "VVPP", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Der Kopff ist gantz geb\u00fcckt/ der Leib ist wie ein Scheit.", "tokens": ["Der", "Kopff", "ist", "gantz", "ge\u00b7b\u00fcckt", "/", "der", "Leib", "ist", "wie", "ein", "Scheit", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$(", "ART", "NN", "VAFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wir wollen uns vor Zorn im Leibe fast zerreissen/", "tokens": ["Wir", "wol\u00b7len", "uns", "vor", "Zorn", "im", "Lei\u00b7be", "fast", "zer\u00b7reis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "NN", "APPRART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df wir die Mi\u00dfgeburth ein Meister-St\u00fcck geheissen.", "tokens": ["Da\u00df", "wir", "die", "Mi\u00df\u00b7ge\u00b7burth", "ein", "Meis\u00b7ter\u00b7St\u00fcck", "ge\u00b7heis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Erst f\u00e4llet mancher Narr zu ihren F\u00fcssen nieder/", "tokens": ["Erst", "f\u00e4l\u00b7let", "man\u00b7cher", "Narr", "zu", "ih\u00b7ren", "F\u00fcs\u00b7sen", "nie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wird dem Affter-Pabst ein ketzerisches Rom.", "tokens": ["Und", "wird", "dem", "Aff\u00b7ter\u00b7Pabst", "ein", "ket\u00b7ze\u00b7ri\u00b7sches", "Rom", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ART", "ADJA", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gantz Babylon singt nicht so viele Klage-Lieder/", "tokens": ["Gantz", "Ba\u00b7by\u00b7lon", "singt", "nicht", "so", "vie\u00b7le", "Kla\u00b7ge\u00b7Lie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VVFIN", "PTKNEG", "ADV", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So eine Wasser-Fluth hegt nicht der S\u00fcnden-Strohm/", "tokens": ["So", "ei\u00b7ne", "Was\u00b7ser\u00b7Fluth", "hegt", "nicht", "der", "S\u00fcn\u00b7den\u00b7Strohm", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PTKNEG", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measu