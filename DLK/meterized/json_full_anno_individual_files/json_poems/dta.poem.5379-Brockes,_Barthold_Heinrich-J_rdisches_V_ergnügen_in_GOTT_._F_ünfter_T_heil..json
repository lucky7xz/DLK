{"dta.poem.5379": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hier sahe man, nicht sonder Freuden,", "tokens": ["Hier", "sa\u00b7he", "man", ",", "nicht", "son\u00b7der", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "PTKNEG", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Monden g\u00fcldnes Rund durch glatter luckrer Weiden", "tokens": ["Des", "Mon\u00b7den", "g\u00fcld\u00b7nes", "Rund", "durch", "glat\u00b7ter", "luck\u00b7rer", "Wei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gespitztes Laub gebrochen strahlen", "tokens": ["Ge\u00b7spitz\u00b7tes", "Laub", "ge\u00b7bro\u00b7chen", "strah\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVPP", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ihre Bl\u00e4tter sich, auf seinem hellen Schein,", "tokens": ["Und", "ih\u00b7re", "Bl\u00e4t\u00b7ter", "sich", ",", "auf", "sei\u00b7nem", "hel\u00b7len", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PRF", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Als wie auf g\u00fcldnen Grunde, mahlen.", "tokens": ["Als", "wie", "auf", "g\u00fcld\u00b7nen", "Grun\u00b7de", ",", "mah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "KOKOM", "APPR", "ADJA", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dort sieht man, wie der Glantz durch dunckle B\u00fcsche bricht,", "tokens": ["Dort", "sieht", "man", ",", "wie", "der", "Glantz", "durch", "dunck\u00b7le", "B\u00fc\u00b7sche", "bricht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "PWAV", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und in der Dunckelheit um desto heller blitzet,", "tokens": ["Und", "in", "der", "Dun\u00b7ckel\u00b7heit", "um", "des\u00b7to", "hel\u00b7ler", "blit\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wobey sein Strahl, wie zugespitzet,", "tokens": ["Wo\u00b7bey", "sein", "Strahl", ",", "wie", "zu\u00b7ge\u00b7spit\u00b7zet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "PWAV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Durch dichter Bl\u00e4tter Oefnung sticht.", "tokens": ["Durch", "dich\u00b7ter", "Bl\u00e4t\u00b7ter", "Oef\u00b7nung", "sticht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die sanfte Harmonie von Schatten und von Licht", "tokens": ["Die", "sanf\u00b7te", "Har\u00b7mo\u00b7nie", "von", "Schat\u00b7ten", "und", "von", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Vergn\u00fcgte mich", "tokens": ["Ver\u00b7gn\u00fcg\u00b7te", "mich"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Recht inniglich", "tokens": ["Recht", "in\u00b7nig\u00b7lich"], "token_info": ["word", "word"], "pos": ["NN", "ADJD"], "meter": "+---", "measure": "dactylic.init"}, "line.13": {"text": "Wie aber nichts vollkommenes allhier,", "tokens": ["Wie", "a\u00b7ber", "nichts", "voll\u00b7kom\u00b7me\u00b7nes", "all\u00b7hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIS", "ADJA", "ADV", "$,"], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.14": {"text": "So f\u00fchlt\u2019 ich, da\u00df, bey aller Zier,", "tokens": ["So", "f\u00fchlt'", "ich", ",", "da\u00df", ",", "bey", "al\u00b7ler", "Zier", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "In alle Lust, die mich erfrischte,", "tokens": ["In", "al\u00b7le", "Lust", ",", "die", "mich", "er\u00b7frischte", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Sich etwas bittres mischte.", "tokens": ["Sich", "et\u00b7was", "bitt\u00b7res", "mischte", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADJA", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Ich f\u00fchlt\u2019 ein inniges Betr\u00fcben,", "tokens": ["Ich", "f\u00fchlt'", "ein", "in\u00b7ni\u00b7ges", "Be\u00b7tr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Da\u00df, bey so mancherley Vollkommenheit", "tokens": ["Da\u00df", ",", "bey", "so", "man\u00b7cher\u00b7ley", "Voll\u00b7kom\u00b7men\u00b7heit"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "APPR", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "So mancher Art Gesch\u00f6pf\u2019, und ihrer Herrlichkeit,", "tokens": ["So", "man\u00b7cher", "Art", "Ge\u00b7sch\u00f6pf'", ",", "und", "ih\u00b7rer", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "NN", "$,", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ich ihren HErrn und Sch\u00f6pfer recht zu lieben,", "tokens": ["Ich", "ih\u00b7ren", "Herrn", "und", "Sch\u00f6p\u00b7fer", "recht", "zu", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "KON", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Zwar einen Zug, doch eine solche Kraft", "tokens": ["Zwar", "ei\u00b7nen", "Zug", ",", "doch", "ei\u00b7ne", "sol\u00b7che", "Kraft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "ADV", "ART", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Und F\u00e4higkeit von solcher Eigenschaft,", "tokens": ["Und", "F\u00e4\u00b7hig\u00b7keit", "von", "sol\u00b7cher", "Ei\u00b7gen\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Wie einem solchen GOtt geb\u00fchrte,", "tokens": ["Wie", "ei\u00b7nem", "sol\u00b7chen", "Gott", "ge\u00b7b\u00fchr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "In meinem Wesen nicht versp\u00fchrte.", "tokens": ["In", "mei\u00b7nem", "We\u00b7sen", "nicht", "ver\u00b7sp\u00fchr\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Indessen dacht\u2019 ich auch: mit dem, was ich hienieden", "tokens": ["In\u00b7des\u00b7sen", "dacht'", "ich", "auch", ":", "mit", "dem", ",", "was", "ich", "hien\u00b7ie\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$.", "APPR", "ART", "$,", "PWS", "PPER", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "In meiner Schwachheit f\u00fchl,", "tokens": ["In", "mei\u00b7ner", "Schwach\u00b7heit", "f\u00fchl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "Ist GOtt verhoffentlich zu frieden.", "tokens": ["Ist", "Gott", "ver\u00b7hof\u00b7fent\u00b7lich", "zu", "frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "So wie der C\u00f6rper, hat auch hier der Geist sein Ziel.", "tokens": ["So", "wie", "der", "C\u00f6r\u00b7per", ",", "hat", "auch", "hier", "der", "Geist", "sein", "Ziel", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "$,", "VAFIN", "ADV", "ADV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Alsdann wird erst mein Geist zur Vollenkommenheit", "tokens": ["Als\u00b7dann", "wird", "erst", "mein", "Geist", "zur", "Vol\u00b7len\u00b7kom\u00b7men\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "In Lust und Lob gelangen,", "tokens": ["In", "Lust", "und", "Lob", "ge\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "Wenn er dereinst annoch wird gr\u00f6ssre F\u00e4higkeit", "tokens": ["Wenn", "er", "de\u00b7reinst", "an\u00b7noch", "wird", "gr\u00f6ss\u00b7re", "F\u00e4\u00b7hig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VAFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Zum Loben und zur Lust empfangen.", "tokens": ["Zum", "Lo\u00b7ben", "und", "zur", "Lust", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}