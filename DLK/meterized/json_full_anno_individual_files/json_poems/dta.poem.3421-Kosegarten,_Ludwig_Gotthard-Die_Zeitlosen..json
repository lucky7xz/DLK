{"dta.poem.3421": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Die Zeitlosen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193292", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Noch immer schm&#252;ckt ihr meine Flur,", "tokens": ["Noch", "im\u00b7mer", "schm", "&#252;", "ckt", "ihr", "mei\u00b7ne", "Flur", ","], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$(", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ihr Bl\u00fcmchen treuer Art,", "tokens": ["Ihr", "Bl\u00fcm\u00b7chen", "treu\u00b7er", "Art", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Lieblingskinder der Natur,", "tokens": ["Ihr", "Lieb\u00b7lings\u00b7kin\u00b7der", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So einfach und so zart.", "tokens": ["So", "ein\u00b7fach", "und", "so", "zart", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dahin sind Ros' und Tulipan,", "tokens": ["Da\u00b7hin", "sind", "Ros'", "und", "Tu\u00b7li\u00b7pan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Violen und Jasmin;", "tokens": ["Vi\u00b7o\u00b7len", "und", "Jas\u00b7min", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Der himmelblaue Enzian,", "tokens": ["Der", "him\u00b7mel\u00b7blau\u00b7e", "En\u00b7zi\u00b7an", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die goldne Malv' ist hin.", "tokens": ["Die", "gold\u00b7ne", "Ma\u00b7lv'", "ist", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Euch aber, Bl\u00fcmchen, zeugt und s\u00e4ugt", "tokens": ["Euch", "a\u00b7ber", ",", "Bl\u00fcm\u00b7chen", ",", "zeugt", "und", "s\u00e4ugt"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "$,", "NN", "$,", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch immer die Natur.", "tokens": ["Noch", "im\u00b7mer", "die", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Noch unversehrt und unverbleicht,", "tokens": ["Noch", "un\u00b7ver\u00b7sehrt", "und", "un\u00b7ver\u00b7bleicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bekr\u00e4nzt ihr meine Flur.", "tokens": ["Be\u00b7kr\u00e4nzt", "ihr", "mei\u00b7ne", "Flur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Euch tilgte nicht des Hundsterns Gluth,", "tokens": ["Euch", "tilg\u00b7te", "nicht", "des", "Hunds\u00b7terns", "Gluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und nicht der Herbstnacht Frost.", "tokens": ["Und", "nicht", "der", "Herbst\u00b7nacht", "Frost", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Euch knickte nicht des Nordes Wuth,", "tokens": ["Euch", "knick\u00b7te", "nicht", "des", "Nor\u00b7des", "Wuth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und nicht der Hauch des Ost.", "tokens": ["Und", "nicht", "der", "Hauch", "des", "Ost", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Diess Bl\u00fcmchen, Beste, ewig jung", "tokens": ["Diess", "Bl\u00fcm\u00b7chen", ",", "Bes\u00b7te", ",", "e\u00b7wig", "jung"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "NN", "$,", "ADJD", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ewig frisch und neu,", "tokens": ["Und", "e\u00b7wig", "frisch", "und", "neu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sey Sinnbild unsrer Einigung,", "tokens": ["Sey", "Sinn\u00b7bild", "uns\u00b7rer", "Ei\u00b7ni\u00b7gung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und unsrer Lieb' und Treu.", "tokens": ["Und", "uns\u00b7rer", "Lieb'", "und", "Treu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Nicht wetterwendisch sey dein Herz;", "tokens": ["Nicht", "wet\u00b7ter\u00b7wen\u00b7disch", "sey", "dein", "Herz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Unwandelbar, wie Gold,", "tokens": ["Un\u00b7wan\u00b7del\u00b7bar", ",", "wie", "Gold", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In Freud' und Leid, in Lust und Schmerz", "tokens": ["In", "Freud'", "und", "Leid", ",", "in", "Lust", "und", "Schmerz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sey es mir treu und hold.", "tokens": ["Sey", "es", "mir", "treu", "und", "hold", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.7": {"line.1": {"text": "Nicht Menschengunst, noch Menschengroll,", "tokens": ["Nicht", "Men\u00b7schen\u00b7gunst", ",", "noch", "Men\u00b7schen\u00b7groll", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein L\u00e4cheln und kein Dr\u00e4un,", "tokens": ["Kein", "L\u00e4\u00b7cheln", "und", "kein", "Dr\u00e4un", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kein Blitz noch Schlag des Schicksals soll", "tokens": ["Kein", "Blitz", "noch", "Schlag", "des", "Schick\u00b7sals", "soll"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "ADV", "NN", "ART", "NN", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geliebte, uns entzweyn.", "tokens": ["Ge\u00b7lieb\u00b7te", ",", "uns", "ent\u00b7zweyn", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Noch bl\u00fchet unsers Lebens May \u2014", "tokens": ["Noch", "bl\u00fc\u00b7het", "un\u00b7sers", "Le\u00b7bens", "May"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und lass, lass ihn verbl\u00fchn!", "tokens": ["Und", "lass", ",", "lass", "ihn", "ver\u00b7bl\u00fchn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bleibt unsre Lieb' und Huld und Treu", "tokens": ["Bleibt", "uns\u00b7re", "Lieb'", "und", "Huld", "und", "Treu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur ewig frisch und gr\u00fcn.", "tokens": ["Nur", "e\u00b7wig", "frisch", "und", "gr\u00fcn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Getreu, getreu lass, Liebste, uns", "tokens": ["Ge\u00b7treu", ",", "ge\u00b7treu", "lass", ",", "Liebs\u00b7te", ",", "uns"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["ADJD", "$,", "ADJD", "VVFIN", "$,", "NN", "$,", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Lebens Strasse ziehn.", "tokens": ["Des", "Le\u00b7bens", "Stras\u00b7se", "ziehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es wird ja hier und da f\u00fcr uns", "tokens": ["Es", "wird", "ja", "hier", "und", "da", "f\u00fcr", "uns"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "KON", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein kleines Masslieb bl\u00fchn.", "tokens": ["Ein", "klei\u00b7nes", "Mass\u00b7lieb", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Getreu, getreu und m\u00fcd' und matt", "tokens": ["Ge\u00b7treu", ",", "ge\u00b7treu", "und", "m\u00fcd'", "und", "matt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "ADJD", "KON", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lass einst uns schlafen gehn.", "tokens": ["Lass", "einst", "uns", "schla\u00b7fen", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPER", "VVINF", "VVINF", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Es bl\u00fchn um unsre Ruhestatt", "tokens": ["Es", "bl\u00fchn", "um", "uns\u00b7re", "Ru\u00b7he\u00b7statt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Masslieb und Tausendsch\u00f6n.", "tokens": ["Mass\u00b7lieb", "und", "Tau\u00b7send\u00b7sch\u00f6n", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}