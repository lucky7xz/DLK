{"dta.poem.1634": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die Schiff-Fahrt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Der muste wohl ein Hertz aus Stal und Eisen tragen/", "tokens": ["Der", "mus\u00b7te", "wohl", "ein", "Hertz", "aus", "Stal", "und", "Ei\u00b7sen", "tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "ART", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit dreyer M\u00e4nner Mutt bepantzern seine Brust/", "tokens": ["Mit", "drey\u00b7er", "M\u00e4n\u00b7ner", "Mutt", "be\u00b7pant\u00b7zern", "sei\u00b7ne", "Brust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NE", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der zwischen See und Lufft sein Leben hinzuwagen/", "tokens": ["Der", "zwi\u00b7schen", "See", "und", "Lufft", "sein", "Le\u00b7ben", "hin\u00b7zu\u00b7wa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sich erstlich unterstand: der Tod war seine Lust/", "tokens": ["Sich", "erst\u00b7lich", "un\u00b7ter\u00b7stand", ":", "der", "Tod", "war", "sei\u00b7ne", "Lust", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVFIN", "$.", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das Leben seine Pein: O Menschen harter Sinnen/", "tokens": ["Das", "Le\u00b7ben", "sei\u00b7ne", "Pein", ":", "O", "Men\u00b7schen", "har\u00b7ter", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$.", "NE", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Stieff-S\u00f6hne der Natur/ Schos-Kinder wilder Flutt/", "tokens": ["Stief\u00b7fS\u00f6h\u00b7ne", "der", "Na\u00b7tur", "/", "Schos\u00b7Kin\u00b7der", "wil\u00b7der", "Flutt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$(", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Denckt ihr nicht/ da\u00df ohn euch die Fische leben k\u00fcnnen?", "tokens": ["Denckt", "ihr", "nicht", "/", "da\u00df", "ohn", "euch", "die", "Fi\u00b7sche", "le\u00b7ben", "k\u00fcn\u00b7nen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$(", "KOUS", "APPR", "PPER", "ART", "NN", "VVINF", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.8": {"text": "Das weite Meer ist gro\u00df genung ohn euer Blutt.", "tokens": ["Das", "wei\u00b7te", "Meer", "ist", "gro\u00df", "ge\u00b7nung", "ohn", "eu\u00b7er", "Blutt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Was suchet ihr den Tod bey den entlegnen Inden/", "tokens": ["Was", "su\u00b7chet", "ihr", "den", "Tod", "bey", "den", "ent\u00b7leg\u00b7nen", "In\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der euch zu Hau\u00df und da gleich bald erreichen kan?", "tokens": ["Der", "euch", "zu", "Hau\u00df", "und", "da", "gleich", "bald", "er\u00b7rei\u00b7chen", "kan", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "KON", "ADV", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ihr wollet Perl und Gold in fernen Landen finden/", "tokens": ["Ihr", "wol\u00b7let", "Perl", "und", "Gold", "in", "fer\u00b7nen", "Lan\u00b7den", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NE", "KON", "NN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Trefft theuren Schaum der Flutt und kostbar Erdreich an/", "tokens": ["Trefft", "theu\u00b7ren", "Schaum", "der", "Flutt", "und", "kost\u00b7bar", "Er\u00b7dreich", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "ART", "NN", "KON", "ADJD", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Dem schn\u00f6der Wahnwitz hat so hohen Prei\u00df gegeben/", "tokens": ["Dem", "schn\u00f6\u00b7der", "Wahn\u00b7witz", "hat", "so", "ho\u00b7hen", "Prei\u00df", "ge\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Bringt in die Alte Welt der Neuen Uberflu\u00df/", "tokens": ["Bringt", "in", "die", "Al\u00b7te", "Welt", "der", "Neu\u00b7en", "U\u00b7ber\u00b7flu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Den Raub von Land und See/ der wider euer Leben/", "tokens": ["Den", "Raub", "von", "Land", "und", "See", "/", "der", "wi\u00b7der", "eu\u00b7er", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$(", "ART", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Zur Rache Rauber/ See und Land verhetzen mu\u00df.", "tokens": ["Zur", "Ra\u00b7che", "Rau\u00b7ber", "/", "See", "und", "Land", "ver\u00b7het\u00b7zen", "mu\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "$(", "NN", "KON", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Was traget ihr darvon? vom Scharbock matte Glieder/", "tokens": ["Was", "tra\u00b7get", "ihr", "dar\u00b7von", "?", "vom", "Schar\u00b7bock", "mat\u00b7te", "Glie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PAV", "$.", "APPRART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Frost/ Hunger/ Hitze/ Durst/ Sturm/ Ungemach und Tod/", "tokens": ["Frost", "/", "Hun\u00b7ger", "/", "Hit\u00b7ze", "/", "Durst", "/", "Sturm", "/", "Un\u00b7ge\