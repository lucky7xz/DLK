{"textgrid.poem.36466": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "16. Die Gemse und die Ziege", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf hohen Alpen kletterte", "tokens": ["Auf", "ho\u00b7hen", "Al\u00b7pen", "klet\u00b7ter\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Schweizerin, die Gemse. Fl\u00fcchtige!", "tokens": ["Die", "Schwei\u00b7ze\u00b7rin", ",", "die", "Gem\u00b7se", ".", "Fl\u00fcch\u00b7ti\u00b7ge", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$.", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Rief eine Ziege, warte doch!", "tokens": ["Rief", "ei\u00b7ne", "Zie\u00b7ge", ",", "war\u00b7te", "doch", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So hoch komm' ich ja auch wohl noch!", "tokens": ["So", "hoch", "komm'", "ich", "ja", "auch", "wohl", "noch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "ADV", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Gemse wartet, und mit leichter M\u00fch'", "tokens": ["Die", "Gem\u00b7se", "war\u00b7tet", ",", "und", "mit", "leich\u00b7ter", "M\u00fch'"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erreicht die Ziege sie;", "tokens": ["Er\u00b7reicht", "die", "Zie\u00b7ge", "sie", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Siehst du, bin ich nicht da?", "tokens": ["Siehst", "du", ",", "bin", "ich", "nicht", "da", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VAFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.4": {"text": "Kann ich nicht klettern?", "tokens": ["Kann", "ich", "nicht", "klet\u00b7tern", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Ja!", "tokens": ["Ja", "!"], "token_info": ["word", "punct"], "pos": ["PTKANT", "$."], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Du kannst, allein,", "tokens": ["Du", "kannst", ",", "al\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Nimm dich in Acht, du brichst, ich wette, Hals und Bein;", "tokens": ["Nimm", "dich", "in", "Acht", ",", "du", "brichst", ",", "ich", "wet\u00b7te", ",", "Hals", "und", "Bein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "CARD", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Denn, sieh hinauf!", "tokens": ["Denn", ",", "sieh", "hin\u00b7auf", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "VVIMP", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Nach jener H\u00f6h, dem Himmel nah,", "tokens": ["Nach", "je\u00b7ner", "H\u00f6h", ",", "dem", "Him\u00b7mel", "nah", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Will ich hinauf!", "tokens": ["Will", "ich", "hin\u00b7auf", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Und pl\u00f6tzlich rafft die Fl\u00fcchtige sich auf,", "tokens": ["Und", "pl\u00f6tz\u00b7lich", "rafft", "die", "Fl\u00fcch\u00b7ti\u00b7ge", "sich", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ist bald", "tokens": ["Ist", "bald"], "token_info": ["word", "word"], "pos": ["VAFIN", "ADV"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Auf einer Felsenspitze, steht", "tokens": ["Auf", "ei\u00b7ner", "Fel\u00b7sen\u00b7spit\u00b7ze", ",", "steht"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In kaum zu sehender Gestalt,", "tokens": ["In", "kaum", "zu", "se\u00b7hen\u00b7der", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Ziege sichtbar, oben drauf,", "tokens": ["Der", "Zie\u00b7ge", "sicht\u00b7bar", ",", "o\u00b7ben", "drauf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic