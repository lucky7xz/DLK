{"textgrid.poem.67539": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Laokoon's Haupte", "genre": "verse", "period": "N.A.", "pub_year": 1768, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Du, in ", "tokens": ["O", "Du", ",", "in"], "token_info": ["word", "word", "punct", "word"], "pos": ["NE", "NE", "$,", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Gen Himmel ziehend, zeuchst aus tiefstem Herzensabgrund", "tokens": ["Gen", "Him\u00b7mel", "zie\u00b7hend", ",", "zeuchst", "aus", "tiefs\u00b7tem", "Her\u00b7zen\u00b7sab\u00b7grund"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NN", "VVPP", "$,", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "Der Deinen Seel' auf diesem Seufzer", "tokens": ["Der", "Dei\u00b7nen", "Seel'", "auf", "die\u00b7sem", "Seuf\u00b7zer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gen Himmel mit empor!", "tokens": ["Gen", "Him\u00b7mel", "mit", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Den drachumwundnen Erdenk\u00f6rper,", "tokens": ["Den", "drach\u00b7um\u00b7wund\u00b7nen", "Er\u00b7den\u00b7k\u00f6r\u00b7per", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie giftgeschwellt er unterliegt! Die d\u00fcrre Scherbe", "tokens": ["Wie", "gift\u00b7ge\u00b7schwellt", "er", "un\u00b7ter\u00b7liegt", "!", "Die", "d\u00fcr\u00b7re", "Scher\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "VVPP", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zerbricht den Todeston der Hyder,", "tokens": ["Zer\u00b7bricht", "den", "To\u00b7des\u00b7ton", "der", "Hy\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie sie erliegt, die Hand!", "tokens": ["Wie", "sie", "er\u00b7liegt", ",", "die", "Hand", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ohnm\u00e4chtig, was uns G\u00f6tter flochten,", "tokens": ["Ohn\u00b7m\u00e4ch\u00b7tig", ",", "was", "uns", "G\u00f6t\u00b7ter", "floch\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PRELS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu l\u00f6sen! Schlangenkn\u00e4ul hinwegzuheben! Wie ihn", "tokens": ["Zu", "l\u00f6\u00b7sen", "!", "Schlan\u00b7gen\u00b7kn\u00e4ul", "hin\u00b7weg\u00b7zu\u00b7he\u00b7ben", "!", "Wie", "ihn"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PTKZU", "VVINF", "$.", "NN", "VVINF", "$.", "PWAV", "PPER"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein nacktes Angstgerippe hebet,", "tokens": ["Ein", "nack\u00b7tes", "Angst\u00b7ge\u00b7rip\u00b7pe", "he\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Seufzer, und ermorscht!", "tokens": ["Den", "Seuf\u00b7zer", ",", "und", "er\u00b7morscht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und Vaters Ohr umheulen Klagen,", "tokens": ["Und", "Va\u00b7ters", "Ohr", "um\u00b7heu\u00b7len", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Weh der Unm\u00fcndigen, aus lautem offnen Schlunde,", "tokens": ["Weh", "der", "Un\u00b7m\u00fcn\u00b7di\u00b7gen", ",", "aus", "lau\u00b7tem", "off\u00b7nen", "Schlun\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "+-++-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.3": {"text": "Die statt der Vaterarme Schlangen", "tokens": ["Die", "statt", "der", "Va\u00b7te\u00b7rar\u00b7me", "Schlan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ergreifen \u2013 grausend Bild.", "tokens": ["Er\u00b7grei\u00b7fen", "\u2013", "grau\u00b7send", "Bild", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$(", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Du bist versunken \u2013 bist gesunken", "tokens": ["Du", "bist", "ver\u00b7sun\u00b7ken", "\u2013", "bist", "ge\u00b7sun\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "VVINF", "$(", "VAFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zum H\u00fcgelstaub, der dann, des Wandrers letzte Ruhstatt,", "tokens": ["Zum", "H\u00fc\u00b7gel\u00b7staub", ",", "der", "dann", ",", "des", "Wand\u00b7rers", "letz\u00b7te", "Ruh\u00b7statt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ADV", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "F\u00fcr aller mit ihm Wandrer Augen", "tokens": ["F\u00fcr", "al\u00b7ler", "mit", "ihm", "Wand\u00b7rer", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "APPR", "PPER", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Aschenklo\u00df erscheint!", "tokens": ["Ein", "A\u00b7schen\u00b7klo\u00df", "er\u00b7scheint", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Nur dieses ", "tokens": ["Nur", "die\u00b7ses"], "token_info": ["word", "word"], "pos": ["ADV", "PDAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Auf ihm, wie er aus tiefster Herzensjammerh\u00f6hle", "tokens": ["Auf", "ihm", ",", "wie", "er", "aus", "tiefs\u00b7ter", "Her\u00b7zens\u00b7jam\u00b7mer\u00b7h\u00f6h\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "PWAV", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der todtgequ\u00e4lten Seinen Seele", "tokens": ["Der", "todt\u00b7ge\u00b7qu\u00e4l\u00b7ten", "Sei\u00b7nen", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gen Himmel mit sich zeucht!", "tokens": ["Gen", "Him\u00b7mel", "mit", "sich", "zeucht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Gen Himmel zeucht und schwer beladen", "tokens": ["Gen", "Him\u00b7mel", "zeucht", "und", "schwer", "be\u00b7la\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "VVFIN", "KON", "ADJD", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ermattet, jammernd weg sich wendet, und wie Hauch nur", "tokens": ["Er\u00b7mat\u00b7tet", ",", "jam\u00b7mernd", "weg", "sich", "wen\u00b7det", ",", "und", "wie", "Hauch", "nur"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "ADJD", "ADV", "PRF", "VVFIN", "$,", "KON", "PWAV", "NN", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auf unsichtbaren Freundes Sterbekissen", "tokens": ["Auf", "un\u00b7sicht\u00b7ba\u00b7ren", "Freun\u00b7des", "Ster\u00b7be\u00b7kis\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Haupt danieder senkt!", "tokens": ["Das", "Haupt", "da\u00b7nie\u00b7der", "senkt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "O Du, der hohen Himmelsg\u00f6tter", "tokens": ["O", "Du", ",", "der", "ho\u00b7hen", "Him\u00b7mels\u00b7g\u00f6t\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein stumm Erbarmungsbild! in aller Himmel Mitte", "tokens": ["Ein", "stumm", "Er\u00b7bar\u00b7mungs\u00b7bild", "!", "in", "al\u00b7ler", "Him\u00b7mel", "Mit\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "$.", "APPR", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verlassen! \u2013 aller armen Menschheit", "tokens": ["Ver\u00b7las\u00b7sen", "!", "\u2013", "al\u00b7ler", "ar\u00b7men", "Menschheit"], "token_info": ["word", "punct", "punct", "word", "word", "word"], "pos": ["VVPP", "$.", "$(", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die h\u00f6chste Majest\u00e4t", "tokens": ["Die", "h\u00f6chs\u00b7te", "Ma\u00b7jes\u00b7t\u00e4t"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Des Leidens! Ach, wo bist Du, bist Du", "tokens": ["Des", "Lei\u00b7dens", "!", "Ach", ",", "wo", "bist", "Du", ",", "bist", "Du"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$.", "ITJ", "$,", "PWAV", "VAFIN", "PPER", "$,", "VAFIN", "PPER"], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Belohnt, Laokoon! Als nun des Sterbeseufzers", "tokens": ["Be\u00b7lohnt", ",", "Lao\u00b7ko\u00b7on", "!", "Als", "nun", "des", "Ster\u00b7be\u00b7seuf\u00b7zers"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "NE", "$.", "KOUS", "ADV", "ART", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Erddumpfer Trauerfall in Wonne,", "tokens": ["Erd\u00b7dump\u00b7fer", "Trau\u00b7er\u00b7fall", "in", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "In Wehmuthlust zerflo\u00df,", "tokens": ["In", "Weh\u00b7mut\u00b7hlust", "zer\u00b7flo\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Und alle Engel ein Dich holten,", "tokens": ["Und", "al\u00b7le", "En\u00b7gel", "ein", "Dich", "hol\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ART", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und offnen, lauten Munds Dich Engel, Deine Kinder,", "tokens": ["Und", "off\u00b7nen", ",", "lau\u00b7ten", "Munds", "Dich", "En\u00b7gel", ",", "Dei\u00b7ne", "Kin\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJA", "$,", "ADJA", "NN", "PPER", "NE", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Umarmeten (der Drachenknote", "tokens": ["Um\u00b7ar\u00b7me\u00b7ten", "(", "der", "Dra\u00b7chen\u00b7kno\u00b7te"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$(", "ART", "NN"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Des Schicksals war zerst\u00fcckt,", "tokens": ["Des", "Schick\u00b7sals", "war", "zer\u00b7st\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "War weggehoben!), und auf Fl\u00fcgeln", "tokens": ["War", "weg\u00b7ge\u00b7ho\u00b7ben", "!", ")", ",", "und", "auf", "Fl\u00fc\u00b7geln"], "token_info": ["word", "word", "punct", "punct", "punct", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "$.", "$(", "$,", "KON", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des hocherhabensten der Seufzer, als Du kraftlos", "tokens": ["Des", "ho\u00b7cher\u00b7ha\u00b7bens\u00b7ten", "der", "Seuf\u00b7zer", ",", "als", "Du", "kraft\u00b7los"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "ART", "NN", "$,", "KOUS", "PPER", "ADJD"], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.3": {"text": "In neuer Welt (soll ich ihn nennen,", "tokens": ["In", "neu\u00b7er", "Welt", "(", "soll", "ich", "ihn", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$(", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Allgewaltigen,", "tokens": ["Den", "All\u00b7ge\u00b7wal\u00b7ti\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Den Hohen, unter dem wir leiden", "tokens": ["Den", "Ho\u00b7hen", ",", "un\u00b7ter", "dem", "wir", "lei\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Staub sind! oft verlassen leiden! unterm Himmel", "tokens": ["Und", "Staub", "sind", "!", "oft", "ver\u00b7las\u00b7sen", "lei\u00b7den", "!", "un\u00b7term", "Him\u00b7mel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "NN", "VAFIN", "$.", "ADV", "VVINF", "VVINF", "$.", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie unerh\u00f6rt und einsam sterben,", "tokens": ["Wie", "un\u00b7er\u00b7h\u00f6rt", "und", "ein\u00b7sam", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verlieren uns wie Hauch!),", "tokens": ["Ver\u00b7lie\u00b7ren", "uns", "wie", "Hauch", "!", ")", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "NN", "$.", "$(", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Als Du \u2013 bist Du zu seinen F\u00fc\u00dfen", "tokens": ["Als", "Du", "\u2013", "bist", "Du", "zu", "sei\u00b7nen", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$(", "VAFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gesunken? sahest ihn? sahst erderein Dein Leben?", "tokens": ["Ge\u00b7sun\u00b7ken", "?", "sa\u00b7hest", "ihn", "?", "sahst", "er\u00b7de\u00b7rein", "Dein", "Le\u00b7ben", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPER", "$.", "VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Des Ganzen Sch\u00f6ne? und im Tode", "tokens": ["Des", "Gan\u00b7zen", "Sch\u00f6\u00b7ne", "?", "und", "im", "To\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$.", "KON", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die h\u00f6chste Sch\u00f6ne? \u2013 sahst", "tokens": ["Die", "h\u00f6chs\u00b7te", "Sch\u00f6\u00b7ne", "?", "\u2013", "sahst"], "token_info": ["word", "word", "word", "punct", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$.", "$(", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Und feiertest, wie hier auf Erden", "tokens": ["Und", "fei\u00b7er\u00b7test", ",", "wie", "hier", "auf", "Er\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"