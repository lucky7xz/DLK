{"textgrid.poem.49126": {"metadata": {"author": {"name": "R\u00f6ling, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Von der Verm\u00e4hlung der Seelen mit Christo", "genre": "verse", "period": "N.A.", "pub_year": 1656, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer bin ich, Jesu, o mein Heyl,", "tokens": ["Wer", "bin", "ich", ",", "Je\u00b7su", ",", "o", "mein", "Heyl", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "NE", "$,", "FM", "PPOSAT", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Da\u00df du dich selbst mir gibst zu Theil", "tokens": ["Da\u00df", "du", "dich", "selbst", "mir", "gibst", "zu", "Theil"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADV", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und h\u00e4lst mich wie f\u00fcr deines Gleichen?", "tokens": ["Und", "h\u00e4lst", "mich", "wie", "f\u00fcr", "dei\u00b7nes", "Glei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "KOKOM", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du bist ja Herr, und ich bin Knecht,", "tokens": ["Du", "bist", "ja", "Herr", ",", "und", "ich", "bin", "Knecht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "$,", "KON", "PPER", "VAFIN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich b\u00f6s' und eitel, du gerecht,", "tokens": ["Ich", "b\u00f6s'", "und", "ei\u00b7tel", ",", "du", "ge\u00b7recht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "ADJD", "$,", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du bleibst ohn' End', ich mu\u00df verbleichen,", "tokens": ["Du", "bleibst", "ohn'", "End'", ",", "ich", "mu\u00df", "ver\u00b7blei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und dennoch tr\u00e4gest du zu mir", "tokens": ["Und", "den\u00b7noch", "tr\u00e4\u00b7gest", "du", "zu", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So unvergleichliche Begier.", "tokens": ["So", "un\u00b7ver\u00b7gleich\u00b7li\u00b7che", "Be\u00b7gier", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Eh' als der Grund gelegt zur Welt", "tokens": ["Eh'", "als", "der", "Grund", "ge\u00b7legt", "zur", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "KOKOM", "ART", "NN", "VVPP", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hastu dein Hau\u00df f\u00fcr mich bestellt", "tokens": ["Has\u00b7tu", "dein", "Hau\u00df", "f\u00fcr", "mich", "be\u00b7stellt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und nachmals mir zu gut verlassen;", "tokens": ["Und", "nach\u00b7mals", "mir", "zu", "gut", "ver\u00b7las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PTKA", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So nimmstu das, was ich bin, an,", "tokens": ["So", "nimms\u00b7tu", "das", ",", "was", "ich", "bin", ",", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PDS", "$,", "PWS", "PPER", "VAFIN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Um so vertraut mich umzufassen;", "tokens": ["Um", "so", "ver\u00b7traut", "mich", "um\u00b7zu\u00b7fas\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ADJD", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was aber kost ich, Liebster, dir,", "tokens": ["Was", "a\u00b7ber", "kost", "ich", ",", "Liebs\u00b7ter", ",", "dir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "PPER", "$,", "NN", "$,", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Eh du so weit es bringst mit mir.", "tokens": ["Eh", "du", "so", "weit", "es", "bringst", "mit", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie ich nur erst das Licht betrat,", "tokens": ["Wie", "ich", "nur", "erst", "das", "Licht", "be\u00b7trat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So wusch mich fort dein seligs Bad", "tokens": ["So", "wusch", "mich", "fort", "dein", "se\u00b7ligs", "Bad"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "PTKVZ", "PPOSAT", "ADJA", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und gab mir deinen Geist zu eigen,", "tokens": ["Und", "gab", "mir", "dei\u00b7nen", "Geist", "zu", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das unsch\u00e4tzbare Himmels-Pfand,", "tokens": ["Das", "un\u00b7sch\u00e4tz\u00b7ba\u00b7re", "Him\u00b7mels\u00b7Pfand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das, o mein Hertz, von unserm Band", "tokens": ["Das", ",", "o", "mein", "Hertz", ",", "von", "un\u00b7serm", "Band"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "$,", "FM", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In meinem Hertzen solle zeugen;", "tokens": ["In", "mei\u00b7nem", "Hert\u00b7zen", "sol\u00b7le", "zeu\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ja selbst du kehrtest offt gemein", "tokens": ["Ja", "selbst", "du", "kehr\u00b7test", "offt", "ge\u00b7mein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "ADV", "PPER", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Durch s\u00fc\u00dfe Regung bey mir ein.", "tokens": ["Durch", "s\u00fc\u00b7\u00dfe", "Re\u00b7gung", "bey", "mir", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach, aber so auch kan ich dein,", "tokens": ["Ach", ",", "a\u00b7ber", "so", "auch", "kan", "ich", "dein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "ADV", "ADV", "VMFIN", "PPER", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Heyland, nicht ohn Anspruch seyn;", "tokens": ["Mein", "Hey\u00b7land", ",", "nicht", "ohn", "An\u00b7spruch", "seyn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PTKNEG", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die S\u00fcnde will mich von dir scheiden,", "tokens": ["Die", "S\u00fcn\u00b7de", "will", "mich", "von", "dir", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Satan h\u00e4lt um mich auch an,", "tokens": ["Der", "Sa\u00b7tan", "h\u00e4lt", "um", "mich", "auch", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hie mustu, theurer Himmels-Mann,", "tokens": ["Hie", "mus\u00b7tu", ",", "theu\u00b7rer", "Him\u00b7mels\u00b7Mann", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "F\u00fcr mich so schweren Todt erst leiden", "tokens": ["F\u00fcr", "mich", "so", "schwe\u00b7ren", "Todt", "erst", "lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ADV", "ADJA", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und unsre Liebe durch dein Blut", "tokens": ["Und", "uns\u00b7re", "Lie\u00b7be", "durch", "dein", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bey deinem Vater machen gut.", "tokens": ["Bey", "dei\u00b7nem", "Va\u00b7ter", "ma\u00b7chen", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "O, hochbelobtes Gottes-Lamm,", "tokens": ["O", ",", "hoch\u00b7be\u00b7lob\u00b7tes", "Got\u00b7tes\u00b7Lamm", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Bruder, Burg' und Br\u00e4utigam,", "tokens": ["Mein", "Bru\u00b7der", ",", "Bur\u00b7g'", "und", "Br\u00e4u\u00b7ti\u00b7gam", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "KON", "NE", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie wehrt hast du mich doch gesch\u00e4tzet,", "tokens": ["Wie", "wehrt", "hast", "du", "mich", "doch", "ge\u00b7sch\u00e4t\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "VAFIN", "PPER", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie hoch hast du mich angebracht,", "tokens": ["Wie", "hoch", "hast", "du", "mich", "an\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie herrlich, reich und gro\u00df gemacht,", "tokens": ["Wie", "herr\u00b7lich", ",", "reich", "und", "gro\u00df", "ge\u00b7macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da du dich mir zur Seit gesetzet;", "tokens": ["Da", "du", "dich", "mir", "zur", "Seit", "ge\u00b7set\u00b7zet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "F\u00fcr deine Braut, o Herr, zu sein,", "tokens": ["F\u00fcr", "dei\u00b7ne", "Braut", ",", "o", "Herr", ",", "zu", "sein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "FM", "NN", "$,", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nehm' ich die gantze Welt nicht ein.", "tokens": ["Nehm'", "ich", "die", "gant\u00b7ze", "Welt", "nicht", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Sprech ich hief\u00fcr: Ich liebe dich,", "tokens": ["Sprech", "ich", "hie\u00b7f\u00fcr", ":", "Ich", "lie\u00b7be", "dich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PAV", "$.", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dies ist zu schlecht f\u00fcr dich und mich;", "tokens": ["Dies", "ist", "zu", "schlecht", "f\u00fcr", "dich", "und", "mich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKA", "ADJD", "APPR", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich sterbe, schaue, f\u00fcr Verlangen,", "tokens": ["Ich", "ster\u00b7be", ",", "schau\u00b7e", ",", "f\u00fcr", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Schatten ist nur mehr an mir,", "tokens": ["Mein", "Schat\u00b7ten", "ist", "nur", "mehr", "an", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hertz, Geist und Seele wohnt bey dir.", "tokens": ["Hertz", ",", "Geist", "und", "See\u00b7le", "wohnt", "bey", "dir", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ach, wenn soll ich dich gantz umfangen?", "tokens": ["Ach", ",", "wenn", "soll", "ich", "dich", "gantz", "um\u00b7fan\u00b7gen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "VMFIN", "PPER", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Herr, meinstu mich, wie ich dich mein,", "tokens": ["Herr", ",", "meins\u00b7tu", "mich", ",", "wie", "ich", "dich", "mein", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "PWAV", "PPER", "PRF", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So la\u00df es doch nicht lange seyn.", "tokens": ["So", "la\u00df", "es", "doch", "nicht", "lan\u00b7ge", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "PTKNEG", "ADV", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Sieh an, wie schmerzlich mir die Welt", "tokens": ["Sieh", "an", ",", "wie", "schmerz\u00b7lich", "mir", "die", "Welt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "PTKVZ", "$,", "PWAV", "ADJD", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von wegen unsers Bundes f\u00e4llt;", "tokens": ["Von", "we\u00b7gen", "un\u00b7sers", "Bun\u00b7des", "f\u00e4llt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was hat ein Frommer mehr auff Erden,", "tokens": ["Was", "hat", "ein", "From\u00b7mer", "mehr", "auff", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als Wunden, Neid, Beschwer und M\u00fch?", "tokens": ["Als", "Wun\u00b7den", ",", "Neid", ",", "Be\u00b7schwer", "und", "M\u00fch", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nur, Jesu, dies erquickt mich hie,", "tokens": ["Nur", ",", "Je\u00b7su", ",", "dies", "er\u00b7quickt", "mich", "hie", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NE", "$,", "PDS", "VVFIN", "PPER", "ADV", "$,"], "met