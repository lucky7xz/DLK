, "punct"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich kan doch dein nicht w\u00fcrdig seyn,", "tokens": ["Ich", "kan", "doch", "dein", "nicht", "w\u00fcr\u00b7dig", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPOSAT", "PTKNEG", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Werd' ich nicht durch viel Creutz und Pein", "tokens": ["Werd'", "ich", "nicht", "durch", "viel", "Creutz", "und", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PTKNEG", "APPR", "PIAT", "NN", "KON", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Dir, liebster Heyland, \u00e4hnlich werden.", "tokens": ["Dir", ",", "liebs\u00b7ter", "Hey\u00b7land", ",", "\u00e4hn\u00b7lich", "wer\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$,", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Drauff leb' ich, Herr, drauff sterb' ich dein,", "tokens": ["Drauff", "leb'", "ich", ",", "Herr", ",", "drauff", "sterb'", "ich", "dein", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "NN", "$,", "PAV", "VVFIN", "PPER", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nie k\u00f6nnen wir getrennet seyn.", "tokens": ["Nie", "k\u00f6n\u00b7nen", "wir", "ge\u00b7tren\u00b7net", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Wer bin ich, Jesu, o mein Heyl,", "toke