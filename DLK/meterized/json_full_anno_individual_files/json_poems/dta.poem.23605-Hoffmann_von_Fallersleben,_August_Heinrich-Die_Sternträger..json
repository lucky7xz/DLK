{"dta.poem.23605": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Sterntr\u00e4ger.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn ihr den Stern am R\u00fccken traget,", "tokens": ["Wenn", "ihr", "den", "Stern", "am", "R\u00fc\u00b7cken", "tra\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo auch sein Kreuz der Esel tr\u00e4gt,", "tokens": ["Wo", "auch", "sein", "Kreuz", "der", "E\u00b7sel", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gewi\u00df, nicht eine Seele fraget,", "tokens": ["Ge\u00b7wi\u00df", ",", "nicht", "ei\u00b7ne", "See\u00b7le", "fra\u00b7get", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ob drunter auch ein Herz wohl schl\u00e4gt.", "tokens": ["Ob", "drun\u00b7ter", "auch", "ein", "Herz", "wohl", "schl\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PAV", "ADV", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch so, ich k\u00f6nnt' es nicht verschmerzen,", "tokens": ["Doch", "so", ",", "ich", "k\u00f6nnt'", "es", "nicht", "ver\u00b7schmer\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erging' es mir wie diesen Herrn,", "tokens": ["Er\u00b7ging'", "es", "mir", "wie", "die\u00b7sen", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "KOKOM", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nichts auf dem Herzen, nichts im Herzen,", "tokens": ["Nichts", "auf", "dem", "Her\u00b7zen", ",", "nichts", "im", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "NN", "$,", "PIS", "APPRART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und doch am Herzen einen Stern.", "tokens": ["Und", "doch", "am", "Her\u00b7zen", "ei\u00b7nen", "Stern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}