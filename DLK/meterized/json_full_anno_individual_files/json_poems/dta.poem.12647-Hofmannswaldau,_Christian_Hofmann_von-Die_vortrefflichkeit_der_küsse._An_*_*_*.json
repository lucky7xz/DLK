{"dta.poem.12647": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die vortrefflichkeit der k\u00fcsse.  \n An  * * *", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach blumen sch\u00f6ner art, die deine lippen zieren!", "tokens": ["Ach", "blu\u00b7men", "sch\u00f6\u00b7ner", "art", ",", "die", "dei\u00b7ne", "lip\u00b7pen", "zie\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von rosen will auf dir ein holder purpur bl\u00fchn,", "tokens": ["Von", "ro\u00b7sen", "will", "auf", "dir", "ein", "hol\u00b7der", "pur\u00b7pur", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVINF", "VMFIN", "APPR", "PPER", "ART", "ADJA", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mich will ein heisser trieb zu deinen knospen f\u00fchren,", "tokens": ["Mich", "will", "ein", "heis\u00b7ser", "trieb", "zu", "dei\u00b7nen", "knos\u00b7pen", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "VVFIN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "La\u00df mich den bienen gleich nach s\u00fcsser beute ziehn,", "tokens": ["La\u00df", "mich", "den", "bie\u00b7nen", "gleich", "nach", "s\u00fcs\u00b7ser", "beu\u00b7te", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Kein hartes ungest\u00fcm soll deine knospen brechen,", "tokens": ["Kein", "har\u00b7tes", "un\u00b7ge\u00b7st\u00fcm", "soll", "dei\u00b7ne", "knos\u00b7pen", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Kein scharffer stachel soll die zarten lippen stechen.", "tokens": ["Kein", "scharf\u00b7fer", "sta\u00b7chel", "soll", "die", "zar\u00b7ten", "lip\u00b7pen", "ste\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch so du selber wilst die s\u00fcsse wohnung bauen,", "tokens": ["Doch", "so", "du", "sel\u00b7ber", "wilst", "die", "s\u00fcs\u00b7se", "woh\u00b7nung", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "ADV", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "La\u00df meine lippen dir an statt des stockes seyn,", "tokens": ["La\u00df", "mei\u00b7ne", "lip\u00b7pen", "dir", "an", "statt", "des", "sto\u00b7ckes", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PPER", "APPR", "APPR", "ART", "ADJA", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "La\u00df mich bald deinen flei\u00df, o zartes bieuchen! schauen,", "tokens": ["La\u00df", "mich", "bald", "dei\u00b7nen", "flei\u00df", ",", "o", "zar\u00b7tes", "bie\u00b7u\u00b7chen", "!", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PPOSAT", "NN", "$,", "FM", "ADJA", "VVINF", "$.", "VVINF", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Komm trag in meinen mund der s\u00e4ffte necktar ein;", "tokens": ["Komm", "trag", "in", "mei\u00b7nen", "mund", "der", "s\u00e4ff\u00b7te", "neck\u00b7tar", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So kan, wenn ich gleich nicht darff deine bl\u00fcten k\u00fcssen,", "tokens": ["So", "kan", ",", "wenn", "ich", "gleich", "nicht", "darff", "dei\u00b7ne", "bl\u00fc\u00b7ten", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "$,", "KOUS", "PPER", "ADV", "PTKNEG", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ein s\u00fcsser alieant von deinen lippen fliessen.", "tokens": ["Ein", "s\u00fcs\u00b7ser", "a\u00b7lie\u00b7ant", "von", "dei\u00b7nen", "lip\u00b7pen", "flies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}