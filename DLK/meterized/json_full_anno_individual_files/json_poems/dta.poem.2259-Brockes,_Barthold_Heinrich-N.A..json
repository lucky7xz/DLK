{"dta.poem.2259": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Seit ich die Pamela gelesen,", "tokens": ["Seit", "ich", "die", "Pa\u00b7me\u00b7la", "ge\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "K\u00f6mmt, nebst dem Bau der Erden, mir", "tokens": ["K\u00f6mmt", ",", "nebst", "dem", "Bau", "der", "Er\u00b7den", ",", "mir"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "$,", "APPR", "ART", "NN", "ART", "NN", "$,", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die ganze Menschheit sch\u00f6ner f\u00fcr,", "tokens": ["Die", "gan\u00b7ze", "Menschheit", "sch\u00f6\u00b7ner", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "APPR", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Als mir dieselbe sonst gewesen.", "tokens": ["Als", "mir", "die\u00b7sel\u00b7be", "sonst", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDAT", "ADV", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "An Voll- und Unvollkommenheiten", "tokens": ["An", "Voll", "und", "Un\u00b7voll\u00b7kom\u00b7men\u00b7hei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hat jedes Ding bey uns zwo Seiten.", "tokens": ["Hat", "je\u00b7des", "Ding", "bey", "uns", "zwo", "Sei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "APPR", "PPER", "CARD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Vorhero sah ich jedermann", "tokens": ["Vor\u00b7he\u00b7ro", "sah", "ich", "je\u00b7der\u00b7mann"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIS"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Von seiner schlimmen Seiten an:", "tokens": ["Von", "sei\u00b7ner", "schlim\u00b7men", "Sei\u00b7ten", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Jetzt wird mein Blick, von ihr belehrt,", "tokens": ["Jetzt", "wird", "mein", "Blick", ",", "von", "ihr", "be\u00b7lehrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$,", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Meist auf die gute hingekehrt.", "tokens": ["Meist", "auf", "die", "gu\u00b7te", "hin\u00b7ge\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Sie zeigt mir zur Vollkommenheit", "tokens": ["Sie", "zeigt", "mir", "zur", "Voll\u00b7kom\u00b7men\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Nicht nur die M\u00f6glichkeit allein;", "tokens": ["Nicht", "nur", "die", "M\u00f6g\u00b7lich\u00b7keit", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Zugleich auch eine Leichtigkeit", "tokens": ["Zu\u00b7gleich", "auch", "ei\u00b7ne", "Leich\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Begl\u00fcckt und tugendhaft zu seyn;", "tokens": ["Be\u00b7gl\u00fcckt", "und", "tu\u00b7gend\u00b7haft", "zu", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Auch in uns eine F\u00e4higkeit", "tokens": ["Auch", "in", "uns", "ei\u00b7ne", "F\u00e4\u00b7hig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Jm Guten weiter fortzugehen,", "tokens": ["Jm", "Gu\u00b7ten", "wei\u00b7ter", "fort\u00b7zu\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Wenn wir nur gut\u2019 Exempel sehen.", "tokens": ["Wenn", "wir", "nur", "gut'", "Ex\u00b7em\u00b7pel", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Man merke doch des Beyspiels Kraft,", "tokens": ["Man", "mer\u00b7ke", "doch", "des", "Bey\u00b7spiels", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Das von der Pamela zu nehmen:", "tokens": ["Das", "von", "der", "Pa\u00b7me\u00b7la", "zu", "neh\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NE", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Das Laster selber lernt sich sch\u00e4men;", "tokens": ["Das", "Las\u00b7ter", "sel\u00b7ber", "lernt", "sich", "sch\u00e4\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Ein ganzes Land wird tugendhaft.", "tokens": ["Ein", "gan\u00b7zes", "Land", "wird", "tu\u00b7gend\u00b7haft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}