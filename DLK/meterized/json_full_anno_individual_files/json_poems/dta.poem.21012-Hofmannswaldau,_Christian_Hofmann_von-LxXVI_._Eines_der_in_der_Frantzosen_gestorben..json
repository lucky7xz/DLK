{"dta.poem.21012": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "LxXVI .  \n Eines der in der Frantzosen  \n gestorben.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Di\u00df was uns neben Gold hat Indien geschickt/", "tokens": ["Di\u00df", "was", "uns", "ne\u00b7ben", "Gold", "hat", "In\u00b7di\u00b7en", "ge\u00b7schickt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRELS", "PPER", "APPR", "NN", "VAFIN", "NE", "VVPP", "$("], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Hat diese fromme Haut aus dieser Welt ger\u00fcckt.", "tokens": ["Hat", "die\u00b7se", "from\u00b7me", "Haut", "aus", "die\u00b7ser", "Welt", "ge\u00b7r\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "ADJA", "NN", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein Leser sol es dir nicht auch also ergehen/", "tokens": ["Mein", "Le\u00b7ser", "sol", "es", "dir", "nicht", "auch", "al\u00b7so", "er\u00b7ge\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "PPER", "PTKNEG", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So la\u00df den Siegelring der geilen ", "tokens": ["So", "la\u00df", "den", "Sie\u00b7gel\u00b7ring", "der", "gei\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}