{"textgrid.poem.60309": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Der R\u00e4uber im Bakony", "genre": "verse", "period": "N.A.", "pub_year": 1841, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Eichenwald im Winde rauscht,", "tokens": ["Der", "Ei\u00b7chen\u00b7wald", "im", "Win\u00b7de", "rauscht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Schatten still der R\u00e4uber lauscht,", "tokens": ["Im", "Schat\u00b7ten", "still", "der", "R\u00e4u\u00b7ber", "lauscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ob nicht ein Wagen auf der Bahn", "tokens": ["Ob", "nicht", "ein", "Wa\u00b7gen", "auf", "der", "Bahn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Fern rollt heran.", "tokens": ["Fern", "rollt", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$."], "meter": "++-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Der R\u00e4uber ist ein Schweinehirt,", "tokens": ["Der", "R\u00e4u\u00b7ber", "ist", "ein", "Schwei\u00b7ne\u00b7hirt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Herde grunzend w\u00fchlt und irrt", "tokens": ["Die", "Her\u00b7de", "grun\u00b7zend", "w\u00fchlt", "und", "irrt"], "token_info": ["word", "word", "word", "word", "word", "word"],