{"textgrid.poem.67069": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Selbstqu\u00e4lerei", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hasse mich! es ist ein ekles Ding", "tokens": ["Ich", "has\u00b7se", "mich", "!", "es", "ist", "ein", "ek\u00b7les", "Ding"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$.", "PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Des Menschen Herz, so kindischschwach, so stolz,", "tokens": ["Des", "Men\u00b7schen", "Herz", ",", "so", "kin\u00b7dischschwach", ",", "so", "stolz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "So freundlich, wie Tobias H\u00fcndlein ist,", "tokens": ["So", "freund\u00b7lich", ",", "wie", "To\u00b7bias", "H\u00fcnd\u00b7lein", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAV", "NE", "NN", "VAFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und doch so h\u00e4misch wieder! weg! ich hasse mich!", "tokens": ["Und", "doch", "so", "h\u00e4\u00b7misch", "wie\u00b7der", "!", "weg", "!", "ich", "has\u00b7se", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "ADV", "$.", "PTKVZ", "$.", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So schw\u00e4rmerisch, wenn es des Dichters Flamme w\u00e4rmt,", "tokens": ["So", "schw\u00e4r\u00b7me\u00b7risch", ",", "wenn", "es", "des", "Dich\u00b7ters", "Flam\u00b7me", "w\u00e4rmt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PPER", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und ha! wenn sich ein freundeloser Junge", "tokens": ["Und", "ha", "!", "wenn", "sich", "ein", "freun\u00b7de\u00b7lo\u00b7ser", "Jun\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "$.", "KOUS", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "An unsre Seite schmiegt, so stolz, so kalt!", "tokens": ["An", "uns\u00b7re", "Sei\u00b7te", "schmiegt", ",", "so", "stolz", ",", "so", "kalt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "So fromm, wenn uns des Lebens Sturm", "tokens": ["So", "fromm", ",", "wenn", "uns", "des", "Le\u00b7bens", "Sturm"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Den Nacken beugt, ...", "tokens": ["Den", "Na\u00b7cken", "beugt", ",", "..."], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ich hasse mich! es ist ein ekles Ding", "tokens": ["Ich", "has\u00b7se", "mich", "!", "es", "ist", "ein", "ek\u00b7les", "Ding"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos":