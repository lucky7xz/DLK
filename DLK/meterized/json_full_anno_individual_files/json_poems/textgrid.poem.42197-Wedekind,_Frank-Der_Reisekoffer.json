{"textgrid.poem.42197": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Der Reisekoffer", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bei Tafel sa\u00dfen in bunter Reih", "tokens": ["Bei", "Ta\u00b7fel", "sa\u00b7\u00dfen", "in", "bun\u00b7ter", "Reih"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Damen und Herren; auch sa\u00df dabei", "tokens": ["Da\u00b7men", "und", "Her\u00b7ren", ";", "auch", "sa\u00df", "da\u00b7bei"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$.", "ADV", "VVFIN", "PAV"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Ein junger Mann von blassem Gesicht,", "tokens": ["Ein", "jun\u00b7ger", "Mann", "von", "blas\u00b7sem", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "In Haltung und Ausdruck ernst und schlicht,", "tokens": ["In", "Hal\u00b7tung", "und", "Aus\u00b7druck", "ernst", "und", "schlicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Durchaus bescheiden, zwar etwas gefr\u00e4\u00dfig,", "tokens": ["Durc\u00b7haus", "be\u00b7schei\u00b7den", ",", "zwar", "et\u00b7was", "ge\u00b7fr\u00e4\u00b7\u00dfig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Aber schweigsam verh\u00e4ltnism\u00e4\u00dfig.", "tokens": ["A\u00b7ber", "schweig\u00b7sam", "ver\u00b7h\u00e4lt\u00b7nis\u00b7m\u00e4\u00b7\u00dfig", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Und wie ein Bach in der Sonne Blinken", "tokens": ["Und", "wie", "ein", "Bach", "in", "der", "Son\u00b7ne", "Blin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "ART", "NN", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Glitt das Gespr\u00e4ch zwischen Scherzen und Trinken.", "tokens": ["Glitt", "das", "Ge\u00b7spr\u00e4ch", "zwi\u00b7schen", "Scher\u00b7zen", "und", "Trin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Man sprach \u00fcber dieses, man sprach \u00fcber jenes,", "tokens": ["Man", "sprach", "\u00fc\u00b7ber", "die\u00b7ses", ",", "man", "sprach", "\u00fc\u00b7ber", "je\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PDAT", "$,", "PIS", "VVFIN", "APPR", "PDAT", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Man sprach \u00fcber N\u00fctzliches, \u00fcber Sch\u00f6nes,", "tokens": ["Man", "sprach", "\u00fc\u00b7ber", "N\u00fctz\u00b7li\u00b7ches", ",", "\u00fc\u00b7ber", "Sch\u00f6\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NE", "$,", "APPR", "NE", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und kam \u00fcber Unf\u00e4lle und Verbrechen", "tokens": ["Und", "kam", "\u00fc\u00b7ber", "Un\u00b7f\u00e4l\u00b7le", "und", "Ver\u00b7bre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-++-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Schlie\u00dflich auf Reisekoffer zu sprechen.", "tokens": ["Schlie\u00df\u00b7lich", "auf", "Rei\u00b7se\u00b7kof\u00b7fer", "zu", "spre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Da waren nun, wie das so geht hienieden,", "tokens": ["Da", "wa\u00b7ren", "nun", ",", "wie", "das", "so", "geht", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "$,", "PWAV", "PDS", "ADV", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Urteil und Ansichten sehr verschieden;", "tokens": ["Ur\u00b7teil", "und", "An\u00b7sich\u00b7ten", "sehr", "ver\u00b7schie\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "VVPP", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Die Damen lobten die gro\u00dfen, schweren,", "tokens": ["Die", "Da\u00b7men", "lob\u00b7ten", "die", "gro\u00b7\u00dfen", ",", "schwe\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Bequem zu packen und rasch zu leeren,", "tokens": ["Be\u00b7quem", "zu", "pa\u00b7cken", "und", "rasch", "zu", "lee\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "KON", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ohne da\u00df dabei die Toilette", "tokens": ["Oh\u00b7ne", "da\u00df", "da\u00b7bei", "die", "To\u00b7i\u00b7let\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PAV", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Jemals Schaden genommen h\u00e4tte.", "tokens": ["Je\u00b7mals", "Scha\u00b7den", "ge\u00b7nom\u00b7men", "h\u00e4t\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVPP", "VAFIN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Den Herren hingegen wollte es scheinen,", "tokens": ["Den", "Her\u00b7ren", "hin\u00b7ge\u00b7gen", "woll\u00b7te", "es", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Angenehmer w\u00e4ren die kleinen,", "tokens": ["An\u00b7ge\u00b7neh\u00b7mer", "w\u00e4\u00b7ren", "die", "klei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Die leichten, zusammengeklappten Dinger;", "tokens": ["Die", "leich\u00b7ten", ",", "zu\u00b7sam\u00b7men\u00b7ge\u00b7klapp\u00b7ten", "Din\u00b7ger", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Man k\u00f6nne sie heben mit einem Finger \u2013", "tokens": ["Man", "k\u00f6n\u00b7ne", "sie", "he\u00b7ben", "mit", "ei\u00b7nem", "Fin\u00b7ger", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Unser J\u00fcngling in guter Ruh", "tokens": ["Un\u00b7ser", "J\u00fcng\u00b7ling", "in", "gu\u00b7ter", "Ruh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Kaut seinen Bissen und schweigt dazu.", "tokens": ["Kaut", "sei\u00b7nen", "Bis\u00b7sen", "und", "schweigt", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "VVFIN", "PAV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Und wie im Schilfe der schaukelnde Nachen", "tokens": ["Und", "wie", "im", "Schil\u00b7fe", "der", "schau\u00b7keln\u00b7de", "Na\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Glitt das Gespr\u00e4ch zwischen Scherzen und Lachen", "tokens": ["Glitt", "das", "Ge\u00b7spr\u00e4ch", "zwi\u00b7schen", "Scher\u00b7zen", "und", "La\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Von Reisekoffern auf ferne Gefilde", "tokens": ["Von", "Rei\u00b7se\u00b7kof\u00b7fern", "auf", "fer\u00b7ne", "Ge\u00b7fil\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Im sch\u00f6nen Italien, auf Kunstgebilde", "tokens": ["Im", "sch\u00f6\u00b7nen", "I\u00b7ta\u00b7li\u00b7en", ",", "auf", "Kunst\u00b7ge\u00b7bil\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "ADJA", "NE", "$,", "APPR", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Und dann auf das Gl\u00fcck, auf das Gl\u00fccklicherscheinen", "tokens": ["Und", "dann", "auf", "das", "Gl\u00fcck", ",", "auf", "das", "Gl\u00fcck\u00b7li\u00b7cher\u00b7schei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Sowie auf die Liebe im allgemeinen.", "tokens": ["So\u00b7wie", "auf", "die", "Lie\u00b7be", "im", "all\u00b7ge\u00b7mei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPRART", "ADJA", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.6": {"line.1": {"text": "Unser J\u00fcngling kaut wacker fort,", "tokens": ["Un\u00b7ser", "J\u00fcng\u00b7ling", "kaut", "wa\u00b7cker", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "H\u00f6rt von dem allen kein Sterbenswort;", "tokens": ["H\u00f6rt", "von", "dem", "al\u00b7len", "kein", "Ster\u00b7bens\u00b7wort", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "PIAT", "PIAT", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Seine Gedanken, begreiflicherweise", "tokens": ["Sei\u00b7ne", "Ge\u00b7dan\u00b7ken", ",", "be\u00b7greif\u00b7li\u00b7cher\u00b7wei\u00b7se"], "token_info": ["word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "$,", "ADJA"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "D\u00e4mmern so weiter im alten Gleise.", "tokens": ["D\u00e4m\u00b7mern", "so", "wei\u00b7ter", "im", "al\u00b7ten", "Glei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "Und wie er sich abm\u00fcht mit d\u00fcstrer Stirn,", "tokens": ["Und", "wie", "er", "sich", "ab\u00b7m\u00fcht", "mit", "d\u00fcst\u00b7rer", "Stirn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PRF", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "L\u00f6st sich ein Etwas in seinem Hirn", "tokens": ["L\u00f6st", "sich", "ein", "Et\u00b7was", "in", "sei\u00b7nem", "Hirn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-++--+-+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Und klettert herab, und erreicht seine Zung", "tokens": ["Und", "klet\u00b7tert", "her\u00b7ab", ",", "und", "er\u00b7reicht", "sei\u00b7ne", "Zung"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "$,", "KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Und wird nun allm\u00e4hlich zur \u00c4u\u00dferung.", "tokens": ["Und", "wird", "nun", "all\u00b7m\u00e4h\u00b7lich", "zur", "\u00c4u\u00b7\u00dfe\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "APPRART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "Und er tut den Mund auf, er winkt mit der Hand \u2013", "tokens": ["Und", "er", "tut", "den", "Mund", "auf", ",", "er", "winkt", "mit", "der", "Hand", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Die Damen im Kreise lauschen gespannt,", "tokens": ["Die", "Da\u00b7men", "im", "Krei\u00b7se", "lau\u00b7schen", "ge\u00b7spannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVINF", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Die Herren verstummen von Reminiszenzen", "tokens": ["Die", "Her\u00b7ren", "ver\u00b7stum\u00b7men", "von", "Re\u00b7mi\u00b7nis\u00b7zen\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+--+--+-+--", "measure": "amphibrach.tri.plus"}, "line.12": {"text": "Aus schw\u00fclen Garderoben mit welkenden Kr\u00e4nzen;", "tokens": ["Aus", "schw\u00fc\u00b7len", "Gar\u00b7de\u00b7ro\u00b7ben", "mit", "wel\u00b7ken\u00b7den", "Kr\u00e4n\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PWAT", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Alles starrt in verhaltenem Grimme,", "tokens": ["Al\u00b7les", "starrt", "in", "ver\u00b7hal\u00b7te\u00b7nem", "Grim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.14": {"text": "Und er fl\u00f6tet mit s\u00fc\u00df melodischer Stimme,", "tokens": ["Und", "er", "fl\u00f6\u00b7tet", "mit", "s\u00fc\u00df", "me\u00b7lo\u00b7di\u00b7scher", "Stim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.15": {"text": "Und dabei leuchtet sein Antlitz hell:", "tokens": ["Und", "da\u00b7bei", "leuch\u00b7tet", "sein", "Ant\u00b7litz", "hell", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "\u00bbich habe einen von Seehundsfell.\u00ab", "tokens": ["\u00bb", "ich", "ha\u00b7be", "ei\u00b7nen", "von", "See\u00b7hunds\u00b7fell", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "APPR", "NN", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Bei Tafel sa\u00dfen in bunter Reih", "tokens": ["Bei", "Ta\u00b7fel", "sa\u00b7\u00dfen", "in", "bun\u00b7ter", "Reih"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Damen und Herren; auch sa\u00df dabei", "tokens": ["Da\u00b7men", "und", "Her\u00b7ren", ";", "auch", "sa\u00df", "da\u00b7bei"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$.", "ADV", "VVFIN", "PAV"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Ein junger Mann von blassem Gesicht,", "tokens": ["Ein", "jun\u00b7ger", "Mann", "von", "blas\u00b7sem", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "In Haltung und Ausdruck ernst und schlicht,", "tokens": ["In", "Hal\u00b7tung", "und", "Aus\