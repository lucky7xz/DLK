{"textgrid.poem.42347": {"metadata": {"author": {"name": "Lachmann, Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Von einer Stimme eignem Klang getroffen", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von einer Stimme eignem Klang getroffen", "tokens": ["Von", "ei\u00b7ner", "Stim\u00b7me", "eig\u00b7nem", "Klang", "ge\u00b7trof\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erbebte ich \u2013 zu lang getragne Leiden,", "tokens": ["Er\u00b7beb\u00b7te", "ich", "\u2013", "zu", "lang", "ge\u00b7trag\u00b7ne", "Lei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "PTKA", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verj\u00e4hrter Gram, verzagtes Sichbescheiden,", "tokens": ["Ver\u00b7j\u00e4hr\u00b7ter", "Gram", ",", "ver\u00b7zag\u00b7tes", "Sich\u00b7be\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und scheue Armut, die nicht wagt zu hoffen.", "tokens": ["Und", "scheu\u00b7e", "Ar\u00b7mut", ",", "die", "nicht", "wagt", "zu", "hof\u00b7fen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "PRELS", "PTKNEG", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So dringen t\u00e4glich dir zahllose Zeichen", "tokens": ["So", "drin\u00b7gen", "t\u00e4g\u00b7lich", "dir", "zahl\u00b7lo\u00b7se", "Zei\u00b7chen"], "token_info": ["