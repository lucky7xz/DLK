{"textgrid.poem.65141": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im Dunkel rang ich meine H\u00e4nde,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Dunkel rang ich meine H\u00e4nde,", "tokens": ["Im", "Dun\u00b7kel", "rang", "ich", "mei\u00b7ne", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Blick nur flehte himmelw\u00e4rts:", "tokens": ["Mein", "Blick", "nur", "fleh\u00b7te", "him\u00b7mel\u00b7w\u00e4rts", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "ADV", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Zermalm' es, wenn du willst, doch wende", "tokens": ["Zer\u00b7malm'", "es", ",", "wenn", "du", "willst", ",", "doch", "wen\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "VMFIN", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dies dumpfe, dies erstorb'ne Herz!", "tokens": ["Dies", "dump\u00b7fe", ",", "dies", "er\u00b7stor\u00b7b'\u00b7ne", "Herz", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PDS", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Des Edens Licht ihm vorzumalen", "tokens": ["Des", "E\u00b7dens", "Licht", "ihm", "vor\u00b7zu\u00b7ma\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Entsend ihm deiner Gnade Schein,", "tokens": ["Ent\u00b7send", "ihm", "dei\u00b7ner", "Gna\u00b7de", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Und dr\u00e4ng' auch jeder ihrer Strahlen", "tokens": ["Und", "dr\u00e4ng'", "auch", "je\u00b7der", "ih\u00b7rer", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PIS", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Gleich einem spitzen Pfeil hinein!", "tokens": ["Gleich", "ei\u00b7nem", "spit\u00b7zen", "Pfeil", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "La\u00df mich, sei's Gl\u00fcck, sei's Leid, empfinden,", "tokens": ["La\u00df", "mich", ",", "sei's", "Gl\u00fcck", ",", "sei's", "Leid", ",", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Stand vor meinem Geiste h\u00e4lt,", "tokens": ["Das", "Stand", "vor", "mei\u00b7nem", "Geis\u00b7te", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das nicht, in fl\u00fcchtigem Entschwinden,", "tokens": ["Das", "nicht", ",", "in", "fl\u00fcch\u00b7ti\u00b7gem", "Ent\u00b7schwin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Staub in meiner Hand zerf\u00e4llt!", "tokens": ["Wie", "Staub", "in", "mei\u00b7ner", "Hand", "zer\u00b7f\u00e4llt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nimm sie hinweg die bange Schw\u00fcle", "tokens": ["Nimm", "sie", "hin\u00b7weg", "die", "ban\u00b7ge", "Schw\u00fc\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Darin die Seele mir erschlafft,", "tokens": ["Da\u00b7rin", "die", "See\u00b7le", "mir", "er\u00b7schlafft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df ich auf's neu dein Kind mich f\u00fchle,", "tokens": ["Da\u00df", "ich", "auf's", "neu", "dein", "Kind", "mich", "f\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJD", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dein Kind an heil'ger Liebeskraft! \u2013 \u2013", "tokens": ["Dein", "Kind", "an", "heil'\u00b7ger", "Lie\u00b7bes\u00b7kraft", "!", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich ward erh\u00f6rt! Die Fesseln fielen,", "tokens": ["Ich", "ward", "er\u00b7h\u00f6rt", "!", "Die", "Fes\u00b7seln", "fie\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$.", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der finstre Bann des Zweifels wich,", "tokens": ["Der", "finst\u00b7re", "Bann", "des", "Zwei\u00b7fels", "wich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie Lenzhauch f\u00fchlt' ich's mich umspielen, \u2013", "tokens": ["Wie", "Lenz\u00b7hauch", "f\u00fchlt'", "ich's", "mich", "um\u00b7spie\u00b7len", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "NN", "VVFIN", "PIS", "PRF", "VVINF", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich sah und ich erkannte dich!", "tokens": ["Ich", "sah", "und", "ich", "er\u00b7kann\u00b7te", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hell leuchtend tratst du mir entgegen", "tokens": ["Hell", "leuch\u00b7tend", "tratst", "du", "mir", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVPP", "VVFIN", "PPER", "PPER", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In ungetr\u00fcbter Herrlichkeit,", "tokens": ["In", "un\u00b7ge\u00b7tr\u00fcb\u00b7ter", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du meines Daseins Fluch und Segen,", "tokens": ["Du", "mei\u00b7nes", "Da\u00b7seins", "Fluch", "und", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mein h\u00f6chstes Gl\u00fcck, mein tiefstes Leid!", "tokens": ["Mein", "h\u00f6chs\u00b7tes", "Gl\u00fcck", ",", "mein", "tiefs\u00b7tes", "Leid", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Denn nicht an Dornen fehlt's dem Kranze,", "tokens": ["Denn", "nicht", "an", "Dor\u00b7nen", "fehlt's", "dem", "Kran\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der sich auf meine Stirn gesenkt;", "tokens": ["Der", "sich", "auf", "mei\u00b7ne", "Stirn", "ge\u00b7senkt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch wer ist, der im Siegesglanze", "tokens": ["Doch", "wer", "ist", ",", "der", "im", "Sie\u00b7ges\u00b7glan\u00b7ze"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWS", "VAFIN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch klagend seiner Wunden denkt?", "tokens": ["Noch", "kla\u00b7gend", "sei\u00b7ner", "Wun\u00b7den", "denkt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn mir zu tiefst das Herz zerrissen,", "tokens": ["Wenn", "mir", "zu", "tiefst", "das", "Herz", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKA", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich preis' und segne doch mein Los,", "tokens": ["Ich", "preis'", "und", "seg\u00b7ne", "doch", "mein", "Los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und keine Thr\u00e4nen m\u00f6cht' ich missen,", "tokens": ["Und", "kei\u00b7ne", "Thr\u00e4\u00b7nen", "m\u00f6cht'", "ich", "mis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VMFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die jemals ich um dich vergo\u00df!", "tokens": ["Die", "je\u00b7mals", "ich", "um", "dich", "ver\u00b7go\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich hab' in dir nicht mich erlesen!", "tokens": ["Ich", "hab'", "in", "dir", "nicht", "mich", "er\u00b7le\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "PTKNEG", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mehr als die Lust und als die Pein,", "tokens": ["Mehr", "als", "die", "Lust", "und", "als", "die", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "ART", "NN", "KON", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die mich durchbebt, gilt mir dein Wesen,", "tokens": ["Die", "mich", "durch\u00b7bebt", ",", "gilt", "mir", "dein", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dein innerst und lebendigst Sein.", "tokens": ["Dein", "in\u00b7nerst", "und", "le\u00b7ben\u00b7digst", "Sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "KON", "VVFIN", "PPOSAT", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Du stehst vor mir voll Gr\u00f6\u00df' und Klarheit,", "tokens": ["Du", "stehst", "vor", "mir", "voll", "Gr\u00f6\u00df'", "und", "Klar\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Frei von jedwedem niedern Joch,", "tokens": ["Frei", "von", "jed\u00b7we\u00b7dem", "nie\u00b7dern", "Joch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.7": {"text": "Dein Blick ist Licht, dein Wort ist Wahrheit, \u2013", "tokens": ["Dein", "Blick", "ist", "Licht", ",", "dein", "Wort", "ist", "Wahr\u00b7heit", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$,", "PPOSAT", "NN", "VAFIN", "NN", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Was brauch' ich mehr zum Gl\u00fccke noch? \u2013", "tokens": ["Was", "brauch'", "ich", "mehr", "zum", "Gl\u00fc\u00b7cke", "noch", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPRART", "NN", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Durch dich hat mir ein Gott verk\u00fcndet,", "tokens": ["Durch", "dich", "hat", "mir", "ein", "Gott", "ver\u00b7k\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Da\u00df Seligkeit auf Erden lebt!", "tokens": ["Da\u00df", "Se\u00b7lig\u00b7keit", "auf", "Er\u00b7den", "lebt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den farb'gen Strahl hast du entz\u00fcndet,", "tokens": ["Den", "fa\u00b7rb'\u00b7gen", "Strahl", "hast", "du", "ent\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Der sich um meine Dichtung webt,", "tokens": ["Der", "sich", "um", "mei\u00b7ne", "Dich\u00b7tung", "webt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dem m\u00fcden Geiste hast du wieder", "tokens": ["Dem", "m\u00fc\u00b7den", "Geis\u00b7te", "hast", "du", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein leuchtend edles Ziel gesteckt!", "tokens": ["Ein", "leuch\u00b7tend", "ed\u00b7les", "Ziel", "ge\u00b7steckt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Drum sind dein eigen diese Lieder,", "tokens": ["Drum", "sind", "dein", "ei\u00b7gen", "die\u00b7se", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPOSAT", "ADJD", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die du in meiner Brust geweckt!", "tokens": ["Die", "du", "in", "mei\u00b7ner", "Brust", "ge\u00b7weckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Im Dunkel rang ich meine H\u00e4nde,", "tokens": ["Im", "Dun\u00b7kel", "rang", "ich", "mei\u00b7ne", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Blick nur flehte himmelw\u00e4rts:", "tokens": ["Mein", "Blick", "nur", "fleh\u00b7te", "him\u00b7mel\u00b7w\u00e4rts", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "ADV", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Zermalm' es, wenn du willst, doch wende", "tokens": ["Zer\u00b7malm'", "es", ",", "wenn", "du", "willst", ",", "doch", "wen\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "VMFIN", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dies dumpfe, dies erstorb'ne Herz!", "tokens": ["Dies", "dump\u00b7fe", ",", "dies", "er\u00b7stor\u00b7b'\u00b7ne", "Herz", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PDS", "ADJA", "NN", "$