", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "ART", "NN", "KON", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die mich durchbebt, gilt mir dein Wesen,", "tokens": ["Die", "mich", "durch\u00b7bebt", ",", "gilt", "mir", "dein", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dein innerst und lebendigst Sein.", "tokens": ["Dein", "in\u00b7nerst", "und", "le\u00b7ben\u00b7digst", "Sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "KON", "VVFIN", "PPOSAT", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Du stehst vor mir voll Gr\u00f6\u00df' und Klarheit,", "tokens": ["Du", "stehst", "vor", "mir", "voll", "Gr\u00f6\u00df'", "und", "Klar\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Frei von jedwedem niedern Joch,", "tokens": ["Frei", "von", "jed\u00b7we\u00b7dem", "nie\u00b7dern", "Joch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.7": {"text": "Dein Blick ist Licht, dein Wort ist Wahrheit, \u2013", "tokens": ["Dein", "Blick", "ist", "Licht", ",", "dein", "Wort", "ist", "Wahr\u00b7heit", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$,", "PPOSAT", "NN", "VAFIN", "NN", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Was brauch' ich mehr zum Gl\u00fccke noch? \u2013", "tokens": ["Was", "brauch'", "ich", "mehr", "zum", "Gl\u00fc\u00b7cke", "noch", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPRART", "NN", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Durch dich hat mir ein Gott verk\u00fcndet,", "tokens": ["Durch", "dich", "hat", "mir", "ein", "Gott", "ver\u00b7k\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Da\u00df Seligkeit auf Erden lebt!", "tokens": ["Da\u00df", "Se\u00b7lig\u00b7keit", "auf", "Er\u00b7den", "lebt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den farb'gen Strahl hast du entz\u00fcndet,", "tokens": ["Den", "fa\u00b7rb'\u00b7gen", "Strahl", "hast", "du", "ent\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Der sich um meine Dichtung webt,", "tokens": ["Der", "sich", "um", "mei\u00b7ne", "Dich\u00b7tung", "webt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dem m\u00fcden Geiste hast du wieder", "tokens": ["Dem", "m\u00fc\u00b7den", "Geis\u00b7te", "hast", "du", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein leuchtend edles Ziel gesteckt!", "tokens": ["Ein", "leuch\u00b7tend", "ed\u00b7les", "Ziel", "ge\u00b7steckt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Drum sind dein eigen diese Lieder,", "tokens": ["Drum", "sind", "dein", "ei\u00b7gen", "die\u00b7se", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPOSAT", "ADJD", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die du in meiner Brust geweckt!", "tokens": ["Die", "du", "in", "mei\u00b7ner", "Brust", "ge\u00b7weckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}