{"textgrid.poem.58564": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "602. Liebe zur Verachtung", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gern woll'n verachtet sein, verschm\u00e4ht, zur\u00fcckgesetzt,", "tokens": ["Gern", "woll'n", "ver\u00b7ach\u00b7tet", "sein", ",", "ver\u00b7schm\u00e4ht", ",", "zu\u00b7r\u00fcck\u00b7ge\u00b7setzt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "VAINF", "$,", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gern woll'n verleumdet sein, geha\u00dft, verfolgt zuletzt,", "tokens": ["Gern", "woll'n", "ver\u00b7leum\u00b7det", "sein", ",", "ge\u00b7ha\u00dft", ",", "ver\u00b7folgt", "zu\u00b7letzt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "VAINF", "$,", "VVPP", "$,", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gern woll'n beneidet sein f\u00fcr Lieb' und Wohltat gar,", "tokens": ["Gern", "woll'n", "be\u00b7nei\u00b7det", "sein", "f\u00fcr", "Lieb'", "und", "Wohl\u00b7tat", "gar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "VAINF", "APPR", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das ist ein sel'ger Stand bei unserm Gott f\u00fcrwahr!", "tokens": ["Das", "ist", "ein", "sel'\u00b7ger", "Stand", "bei", "un\u00b7serm", "Gott", "f\u00fcr\u00b7wahr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN