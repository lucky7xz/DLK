{"dta.poem.13797": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Entschlu\u00df .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Mich fa\u00dft der Sturm, wild ringen Licht und Schatten,", "tokens": ["Mich", "fa\u00dft", "der", "Sturm", ",", "wild", "rin\u00b7gen", "Licht", "und", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ADJD", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch Wolkenri\u00df bricht flammendes Entz\u00fccken \u2014", "tokens": ["Durch", "Wol\u00b7ken\u00b7ri\u00df", "bricht", "flam\u00b7men\u00b7des", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur zu, mein Ro\u00df! wir finden noch zum Ziele!", "tokens": ["Nur", "zu", ",", "mein", "Ro\u00df", "!", "wir", "fin\u00b7den", "noch", "zum", "Zie\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}