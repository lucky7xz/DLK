{"dta.poem.11057": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Grabschrift.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von einem Geist, der in der Zeit,", "tokens": ["Von", "ei\u00b7nem", "Geist", ",", "der", "in", "der", "Zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch Bethen, oft den Sch\u00f6pfer priese,", "tokens": ["Durch", "Be\u00b7then", ",", "oft", "den", "Sch\u00f6p\u00b7fer", "prie\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der von Geduld und Fr\u00f6mmigkeit,", "tokens": ["Der", "von", "Ge\u00b7duld", "und", "Fr\u00f6m\u00b7mig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Andacht und Gelassenheit,", "tokens": ["Von", "An\u00b7dacht", "und", "Ge\u00b7las\u00b7sen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein unnachahmbar Muster wiese,", "tokens": ["Ein", "un\u00b7nac\u00b7hahm\u00b7bar", "Mus\u00b7ter", "wie\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der sich, von uns, zum Paradiese,", "tokens": ["Der", "sich", ",", "von", "uns", ",", "zum", "Pa\u00b7ra\u00b7die\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PRF", "$,", "APPR", "PPER", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Das Gott aus Gnaden ihm geschenkt,", "tokens": ["Das", "Gott", "aus", "Gna\u00b7den", "ihm", "ge\u00b7schenkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Voll br\u00fcnstger Zuversicht gelenkt,", "tokens": ["Voll", "br\u00fcnst\u00b7ger", "Zu\u00b7ver\u00b7sicht", "ge\u00b7lenkt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Kurz, von der redlichen Belise,", "tokens": ["Kurz", ",", "von", "der", "red\u00b7li\u00b7chen", "Be\u00b7li\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Sind hier die Schaalen eingesenkt.", "tokens": ["Sind", "hier", "die", "Schaa\u00b7len", "ein\u00b7ge\u00b7senkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}