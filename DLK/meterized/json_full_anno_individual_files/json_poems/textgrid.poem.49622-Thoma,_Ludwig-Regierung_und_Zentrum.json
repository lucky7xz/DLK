{"textgrid.poem.49622": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Regierung und Zentrum", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Jahre her ging glatt der Handel,", "tokens": ["Die", "Jah\u00b7re", "her", "ging", "glatt", "der", "Han\u00b7del", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APZR", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verstohlen hier, und dort geheim,", "tokens": ["Ver\u00b7stoh\u00b7len", "hier", ",", "und", "dort", "ge\u00b7heim", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da kam von ungef\u00e4hr ein Wandel;", "tokens": ["Da", "kam", "von", "un\u00b7ge\u00b7f\u00e4hr", "ein", "Wan\u00b7del", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Freundschaft ging jetzt aus dem Leim.", "tokens": ["Die", "Freund\u00b7schaft", "ging", "jetzt", "aus", "dem", "Leim", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das Zentrum tut, wie die Kokotte,", "tokens": ["Das", "Zent\u00b7rum", "tut", ",", "wie", "die", "Ko\u00b7kot\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit der ihr Louis nicht l\u00e4nger geht;", "tokens": ["Mit", "der", "ihr", "Lou\u00b7is", "nicht", "l\u00e4n\u00b7ger", "geht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "NE", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Sie \u00fcberliefert ihn dem Spotte,", "tokens": ["Sie", "\u00fc\u00b7berl\u00b7ie\u00b7fert", "ihn", "dem", "Spot\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie wird ganz furchtbar indiskret.", "tokens": ["Sie", "wird", "ganz", "furcht\u00b7bar", "in\u00b7dis\u00b7kret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und er, der von ihr ausgehalten", "tokens": ["Und", "er", ",", "der", "von", "ihr", "aus\u00b7ge\u00b7hal\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "PRELS", "APPR", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich wohlgef\u00fchlt so manches Jahr,", "tokens": ["Sich", "wohl\u00b7ge\u00b7f\u00fchlt", "so", "man\u00b7ches", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zeigt jetzt, wie sie doch mi\u00dfgestalten,", "tokens": ["Zeigt", "jetzt", ",", "wie", "sie", "doch", "mi\u00df\u00b7ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra