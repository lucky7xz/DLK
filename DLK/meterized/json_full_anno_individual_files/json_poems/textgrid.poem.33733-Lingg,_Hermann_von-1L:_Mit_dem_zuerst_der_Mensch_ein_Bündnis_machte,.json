{"textgrid.poem.33733": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mit dem zuerst der Mensch ein B\u00fcndnis machte,", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit dem zuerst der Mensch ein B\u00fcndnis machte,", "tokens": ["Mit", "dem", "zu\u00b7erst", "der", "Mensch", "ein", "B\u00fcnd\u00b7nis", "mach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das war der Berge starker Sohn, das Eisen,", "tokens": ["Das", "war", "der", "Ber\u00b7ge", "star\u00b7ker", "Sohn", ",", "das", "Ei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das half ihm treu auf Land- und Wasserreisen", "tokens": ["Das", "half", "ihm", "treu", "auf", "Lan\u00b7d", "und", "Was\u00b7ser\u00b7rei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "APPR", "TRUNC", "KON", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und als er unters Joch die Tiere brachte.", "tokens": ["Und", "als", "er", "un\u00b7ters", "Joch", "die", "Tie\u00b7re", "brach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Darob erz\u00fcrnte sich in seinem Schachte", "tokens": ["Da\u00b7rob", "er\u00b7z\u00fcrn\u00b7te", "sich", "in", "sei\u00b7nem", "Schach\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das eitle Gold und sprach: Seht mir die Weisen!", "tokens": ["Das", "eit\u00b7le", "Gold", "und", "sprach", ":", "Seht", "mir", "die", "Wei\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "VVFIN", "$.", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bald werdet mich ihr \u00fcber G\u00f6tter preisen,", "tokens": ["Bald", "wer\u00b7det", "mich", "ihr", "\u00fc\u00b7ber", "G\u00f6t\u00b7ter", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch wehe dem, den ich sodann verachte!", "tokens": ["Doch", "we\u00b7he", "dem", ",", "den", "ich", "so\u00b7dann", "ver\u00b7ach\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und wehe dem auch, den ich ganz entz\u00fccke!", "tokens": ["Und", "we\u00b7he", "dem", "auch", ",", "den", "ich", "ganz", "ent\u00b7z\u00fc\u00b7cke", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "ADV", "$,", "PRELS", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bald wird der Stolze unter euch gesendet,", "tokens": ["Bald", "wird", "der", "Stol\u00b7ze", "un\u00b7ter", "euch", "ge\u00b7sen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des Haupt so sehr mit meinem Glanz ich schm\u00fccke,", "tokens": ["Des", "Haupt", "so", "sehr", "mit", "mei\u00b7nem", "Glanz", "ich", "schm\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Da\u00df ihr, von seinem Anblick wahnverblendet,", "tokens": ["Da\u00df", "ihr", ",", "von", "sei\u00b7nem", "An\u00b7blick", "wahn\u00b7ver\u00b7blen\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erlernet, wie das Joch des Goldes dr\u00fccke,", "tokens": ["Er\u00b7ler\u00b7net", ",", "wie", "das", "Joch", "des", "Gol\u00b7des", "dr\u00fc\u00b7cke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und da\u00df ihr gegen euch das Eisen wendet!", "tokens": ["Und", "da\u00df", "ihr", "ge\u00b7gen", "euch", "das", "Ei\u00b7sen", "wen\u00b7det", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Mit dem zuerst der Mensch ein B\u00fcndnis machte,", "tokens": ["Mit", "dem", "zu\u00b7erst", "der", "Mensch", "ein", "