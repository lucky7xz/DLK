{"textgrid.poem.53633": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Kriegsandenken", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Daran mag keiner mehr denken, nicht wahr \u2013?", "tokens": ["Da\u00b7ran", "mag", "kei\u00b7ner", "mehr", "den\u00b7ken", ",", "nicht", "wahr", "\u2013", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PAV", "VMFIN", "PIS", "ADV", "VVINF", "$,", "PTKNEG", "ADJD", "$(", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da\u00df achtzig Millionen als Sklaven des Schwerts", "tokens": ["Da\u00df", "acht\u00b7zig", "Mil\u00b7lion\u00b7en", "als", "Skla\u00b7ven", "des", "Schwerts"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "CARD", "NN", "KOUS", "NN", "ART", "NN"], "meter": "-+---+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "sinnlos \u00fcber die Erde stampften,", "tokens": ["sinn\u00b7los", "\u00fc\u00b7ber", "die", "Er\u00b7de", "stampf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "schossen, r\u00e4uberten, pl\u00fcnderten, feilschten,", "tokens": ["schos\u00b7sen", ",", "r\u00e4u\u00b7ber\u00b7ten", ",", "pl\u00fcn\u00b7der\u00b7ten", ",", "feilschten", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "schlugen und geschlagen wurden,", "tokens": ["schlu\u00b7gen", "und", "ge\u00b7schla\u00b7gen", "wur\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "soffen, um das Leid zu vergessen,", "tokens": ["sof\u00b7fen", ",", "um", "das", "Leid", "zu", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUI", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Heimat und Menschtum zu vergessen . . .", "tokens": ["Hei\u00b7mat", "und", "Menschtum", "zu", "ver\u00b7ges\u00b7sen", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "KON", "NN", "PTKZU", "VVINF", "$.", "$.", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Daran mag keiner mehr denken, nicht wahr \u2013?", "tokens": ["Da\u00b7ran", "mag", "kei\u00b7ner", "mehr", "den\u00b7ken", ",", "nicht", "wahr", "\u2013", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PAV", "VMFIN", "PIS", "ADV", "VVINF", "$,", "PTKNEG", "ADJD", "$(", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Was ist denn in den K\u00e4sten geblieben \u2013?", "tokens": ["Was", "ist", "denn", "in", "den", "K\u00e4s\u00b7ten", "ge\u00b7blie\u00b7ben", "\u2013", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VAFIN", "ADV", "APPR", "ART", "NN", "VVPP", "$(", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "M\u00fcnzen, bunte Medaillen und Tand.", "tokens": ["M\u00fcn\u00b7zen", ",", "bun\u00b7te", "Me\u00b7dail\u00b7len", "und", "Tand", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Seidenb\u00e4nder, von deutschen F\u00fcrsten", "tokens": ["Sei\u00b7den\u00b7b\u00e4n\u00b7der", ",", "von", "deut\u00b7schen", "F\u00fcrs\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "braven Untertanen gespendet:", "tokens": ["bra\u00b7ven", "Un\u00b7ter\u00b7ta\u00b7nen", "ge\u00b7spen\u00b7det", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Seide f\u00fcr Blut.", "tokens": ["Sei\u00b7de", "f\u00fcr", "Blut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Fotografien liegen in den K\u00e4sten.", "tokens": ["Fo\u00b7to\u00b7gra\u00b7fi\u00b7en", "lie\u00b7gen", "in", "den", "K\u00e4s\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "\u00bbdas war mein Stabsquartier in Flandern.\u00ab", "tokens": ["\u00bb", "das", "war", "mein", "Stabs\u00b7quar\u00b7tier", "in", "Flan\u00b7dern", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PDS", "VAFIN", "PPOSAT", "NN", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "\u00bbhier, diese Ferme wurde beschossen.\u00ab", "tokens": ["\u00bb", "hier", ",", "die\u00b7se", "Fer\u00b7me", "wur\u00b7de", "be\u00b7schos\u00b7sen", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "$,", "PDAT", "NN", "VAFIN", "VVPP", "$.", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "\u00bbdas ist ein Weibsst\u00fcck aus Lomscha \u2013 ne Polin.\u00ab", "tokens": ["\u00bb", "das", "ist", "ein", "Weibs\u00b7st\u00fcck", "aus", "Lom\u00b7scha", "\u2013", "ne", "Po\u00b7lin", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "PDS", "VAFIN", "ART", "NN", "APPR", "NE", "$(", "NE", "NE", "$.", "$("], "meter": "-+-+--+---+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Was ist denn in den K\u00e4sten geblieben \u2013?", "tokens": ["Was", "ist", "denn", "in", "den", "K\u00e4s\u00b7ten", "ge\u00b7blie\u00b7ben", "\u2013", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VAFIN", "ADV", "APPR", "ART", "NN", "VVPP", "$(", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Was ist denn in den Seelen geblieben?", "tokens": ["Was", "ist", "denn", "in", "den", "See\u00b7len", "ge\u00b7blie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Habt ihr die L\u00e4use, den Durst auch vergessen \u2013:", "tokens": ["Habt", "ihr", "die", "L\u00e4u\u00b7se", ",", "den", "Durst", "auch", "ver\u00b7ges\u00b7sen", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "ADV", "VVPP", "$(", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "denkt an den Mord an Leibern und Seelen \u2013", "tokens": ["denkt", "an", "den", "Mord", "an", "Lei\u00b7bern", "und", "See\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPR", "NN", "KON", "NN", "$("], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "denkt an die Unterdr\u00fcckung des Mannes,", "tokens": ["denkt", "an", "die", "Un\u00b7ter\u00b7dr\u00fc\u00b7ckung", "des", "Man\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "denkt an die Reden der Feldpastoren,", "tokens": ["denkt", "an", "die", "Re\u00b7den", "der", "Feld\u00b7pas\u00b7to\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.6": {"text": "denkt an die Jahre, die ihr verloren,", "tokens": ["denkt", "an", "die", "Jah\u00b7re", ",", "die", "ihr", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.7": {"text": "denkt an die Frauen, die drinnen verdarben,", "tokens": ["denkt", "an", "die", "Frau\u00b7en", ",", "die", "drin\u00b7nen", "ver\u00b7dar\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "ADV", "VVINF", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.8": {"text": "w\u00e4hrend die S\u00f6hne im Trichter starben \u2013 \u2013", "tokens": ["w\u00e4h\u00b7rend", "die", "S\u00f6h\u00b7ne", "im", "Trich\u00b7ter", "star\u00b7ben", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN", "VVFIN", "$(", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.9": {"text": "denkt an die gro\u00dfe, die herrliche Zeit \u2013 \u2013!", "tokens": ["denkt", "an", "die", "gro\u00b7\u00dfe", ",", "die", "herr\u00b7li\u00b7che", "Zeit", "\u2013", "\u2013", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$(", "$(", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.6": {"line.1": {"text": "Vorbei? Vor\u00fcber? Vergangenheit?", "tokens": ["Vor\u00b7bei", "?", "Vor\u00b7\u00fc\u00b7ber", "?", "Ver\u00b7gan\u00b7gen\u00b7heit", "?"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "$.", "NN", "$.", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "La\u00dft ihrs treiben bei alten Standarten,", "tokens": ["La\u00dft", "ihrs", "trei\u00b7ben", "bei", "al\u00b7ten", "Stand\u00b7ar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "VVINF", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "lobt ihr, die euch vier Jahre lang narrten,", "tokens": ["lobt", "ihr", ",", "die", "euch", "vier", "Jah\u00b7re", "lang", "narr\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PRELS", "PPER", "CARD", "NN", "ADJD", "VVFIN", "$,"], "meter": "---+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "legt ihr euch wieder schl\u00e4frig aufs Ohr,", "tokens": ["legt", "ihr", "euch", "wie\u00b7der", "schl\u00e4f\u00b7rig", "aufs", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"t