{"textgrid.poem.61435": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Aus Griechenland", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sa\u00df im Abendschein", "tokens": ["Ich", "sa\u00df", "im", "A\u00b7bend\u00b7schein"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Auf Naxos' Traubenklippe;", "tokens": ["Auf", "Na\u00b7xos'", "Trau\u00b7ben\u00b7klip\u00b7pe", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Krug mit dunklem Wein", "tokens": ["Der", "Krug", "mit", "dunk\u00b7lem", "Wein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Erfrischte meine Lippe.", "tokens": ["Er\u00b7frischte", "mei\u00b7ne", "Lip\u00b7pe", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Da sah ich, wie im Tal", "tokens": ["Da", "sah", "ich", ",", "wie", "im", "Tal"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mit Frucht und Silberbl\u00fcten", "tokens": ["Mit", "Frucht", "und", "Sil\u00b7ber\u00b7bl\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die G\u00e4rten sonder Zahl", "tokens": ["Die", "G\u00e4r\u00b7ten", "son\u00b7der", "Zahl"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Im Sonnenduft vergl\u00fchten;", "tokens": ["Im", "Son\u00b7nen\u00b7duft", "ver\u00b7gl\u00fch\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich sah am Fels empor", "tokens": ["Ich", "sah", "am", "Fels", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Hoch \u00fcber luft'gen Stiegen,", "tokens": ["Hoch", "\u00fc\u00b7ber", "luft'\u00b7gen", "Stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Reblaub um S\u00e4ul' und Tor,", "tokens": ["Re\u00b7blaub", "um", "S\u00e4ul'", "und", "Tor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die schmucken H\u00e4user liegen;", "tokens": ["Die", "schmu\u00b7cken", "H\u00e4u\u00b7ser", "lie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich sah der Herde Zug,", "tokens": ["Ich", "sah", "der", "Her\u00b7de", "Zug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Den Hirten mit dem Stabe,", "tokens": ["Den", "Hir\u00b7ten", "mit", "dem", "Sta\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Jungfrau sch\u00f6pft' im Krug", "tokens": ["Die", "Jung\u00b7frau", "sch\u00f6pft'", "im", "Krug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Am Bach die frische Labe.", "tokens": ["Am", "Bach", "die", "fri\u00b7sche", "La\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und ferne blitzt' im Ring", "tokens": ["Und", "fer\u00b7ne", "blitzt'", "im", "Ring"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Das Meer vergoldet wieder,", "tokens": ["Das", "Meer", "ver\u00b7gol\u00b7det", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Denn hinter Paros ging", "tokens": ["Denn", "hin\u00b7ter", "Pa\u00b7ros", "ging"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "NE", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Sonne langsam nieder.", "tokens": ["Die", "Son\u00b7ne", "lang\u00b7sam", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Da kam's mir ins Gem\u00fct:", "tokens": ["Da", "kam's", "mir", "ins", "Ge\u00b7m\u00fct", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Hier unter diesem blauen", "tokens": ["Hier", "un\u00b7ter", "die\u00b7sem", "blau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PDAT", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gezelt, wo's ewig bl\u00fcht,", "tokens": ["Ge\u00b7zelt", ",", "wo's", "e\u00b7wig", "bl\u00fcht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWAV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wie gut w\u00e4r's H\u00fctten bauen!", "tokens": ["Wie", "gut", "w\u00e4r's", "H\u00fct\u00b7ten", "bau\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Es w\u00fcrde dir der Baum,", "tokens": ["Es", "w\u00fcr\u00b7de", "dir", "der", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Es w\u00fcrden Feld und Reben", "tokens": ["Es", "w\u00fcr\u00b7den", "Feld", "und", "Re\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dir m\u00fchlos wie im Traum", "tokens": ["Dir", "m\u00fch\u00b7los", "wie", "im", "Traum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "KOKOM", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Des Lebens Notdurft geben.", "tokens": ["Des", "Le\u00b7bens", "Not\u00b7durft", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ein Weib von dieses Lands", "tokens": ["Ein", "Weib", "von", "die\u00b7ses", "Lands"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Gott\u00e4hnlichem Geschlechte,", "tokens": ["Got\u00b7t\u00e4hn\u00b7li\u00b7chem", "Ge\u00b7schlech\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie fl\u00f6chte Liebesglanz", "tokens": ["Sie", "fl\u00f6ch\u00b7te", "Lie\u00b7bes\u00b7glanz"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In deine Tag' und N\u00e4chte.", "tokens": ["In", "dei\u00b7ne", "Tag'", "und", "N\u00e4ch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Nicht in gelahrten Wust,", "tokens": ["Nicht", "in", "ge\u00b7lahr\u00b7ten", "Wust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "In Nebel nicht begraben,", "tokens": ["In", "Ne\u00b7bel", "nicht", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gen\u00f6ssest du mit Lust", "tokens": ["Ge\u00b7n\u00f6s\u00b7sest", "du", "mit", "Lust"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der gro\u00dfen Mutter Gaben.", "tokens": ["Der", "gro\u00b7\u00dfen", "Mut\u00b7ter", "Ga\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Du s\u00e4hst im Sonnenschein", "tokens": ["Du", "s\u00e4hst", "im", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ihr formenbildend Walten", "tokens": ["Ihr", "for\u00b7men\u00b7bil\u00b7dend", "Wal\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVPP", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und d\u00fcrftest weise sein", "tokens": ["Und", "d\u00fcrf\u00b7test", "wei\u00b7se", "sein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADV", "PPOSAT"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und heiter wie die Alten.", "tokens": ["Und", "hei\u00b7ter", "wie", "die", "Al\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "So tr\u00e4umt' ich vor mich hin", "tokens": ["So", "tr\u00e4umt'", "ich", "vor", "mich", "hin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "In selig Schaun versunken,", "tokens": ["In", "se\u00b7lig", "Schaun", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "NN", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Es war mein ganzer Sinn", "tokens": ["Es", "war", "mein", "gan\u00b7zer", "Sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Vom Glanz des S\u00fcdens trunken.", "tokens": ["Vom", "Glanz", "des", "S\u00fc\u00b7dens", "trun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Doch froh gedacht' ich's kaum,", "tokens": ["Doch", "froh", "ge\u00b7dacht'", "ich's", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PIS", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da sprach das Herz mit Beben:", "tokens": ["Da", "sprach", "das", "Herz", "mit", "Be\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das ist ein sch\u00f6ner Traum,", "tokens": ["Das", "ist", "ein", "sch\u00f6\u00b7ner", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Doch ist's ein Traumbild eben.", "tokens": ["Doch", "ist's", "ein", "Traum\u00b7bild", "e\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Wie sollte dir, o Tor,", "tokens": ["Wie", "soll\u00b7te", "dir", ",", "o", "Tor", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "$,", "FM", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Erbl\u00fchen Rast und Friede,", "tokens": ["Er\u00b7bl\u00fc\u00b7hen", "Rast", "und", "Frie\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo nimmermehr ein Ohr", "tokens": ["Wo", "nim\u00b7mer\u00b7mehr", "ein", "Ohr"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Aufhorchte deinem Liede!", "tokens": ["Auf\u00b7horch\u00b7te", "dei\u00b7nem", "Lie\u00b7de", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Bei Palm' und Rebgewind'", "tokens": ["Bei", "Palm'", "und", "Reb\u00b7ge\u00b7wind'"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Bald w\u00fcrde dich's verlangen", "tokens": ["Bald", "w\u00fcr\u00b7de", "dich's", "ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zum Wald, wo du als Kind", "tokens": ["Zum", "Wald", ",", "wo", "du", "als", "Kind"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PWAV", "PPER", "KOUS", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Vertieft dahingegangen.", "tokens": ["Ver\u00b7tieft", "da\u00b7hin\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PAV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Von deinem Volke los", "tokens": ["Von", "dei\u00b7nem", "Vol\u00b7ke", "los"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und seinem Kampf und Trachten", "tokens": ["Und", "sei\u00b7nem", "Kampf", "und", "Trach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "M\u00fc\u00dft' aller F\u00fcll' im Scho\u00df", "tokens": ["M\u00fc\u00dft'", "al\u00b7ler", "F\u00fcll'", "im", "Scho\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PIAT", "NN", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dein einsam Herz verschmachten.", "tokens": ["Dein", "ein\u00b7sam", "Herz", "ver\u00b7schmach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Und ob ein griechisch Weib,", "tokens": ["Und", "ob", "ein", "grie\u00b7chisch", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Sch\u00f6n wie die Morgenr\u00f6te,", "tokens": ["Sch\u00f6n", "wie", "die", "Mor\u00b7gen\u00b7r\u00f6\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dir freudig Seel' und Leib", "tokens": ["Dir", "freu\u00b7dig", "Seel'", "und", "Leib"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zum Eigentume b\u00f6te:", "tokens": ["Zum", "Ei\u00b7gen\u00b7tu\u00b7me", "b\u00f6\u00b7te", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Es k\u00f6nnt' ihr fremder Brauch,", "tokens": ["Es", "k\u00f6nnt'", "ihr", "frem\u00b7der", "Brauch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ihr s\u00fcdlich Tun und Denken", "tokens": ["Ihr", "s\u00