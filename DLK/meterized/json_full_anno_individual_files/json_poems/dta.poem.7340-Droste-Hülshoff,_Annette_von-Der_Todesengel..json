{"dta.poem.7340": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Der Todesengel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "'s giebt eine Sage, da\u00df wenn pl\u00f6tzlich matt'", "tokens": ["'", "s", "giebt", "ei\u00b7ne", "Sa\u00b7ge", ",", "da\u00df", "wenn", "pl\u00f6tz\u00b7lich", "matt", "'"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "$,", "KOUS", "KOUS", "ADJD", "PTKVZ", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Unheimlich Schaudern Einen \u00fcbergleite,", "tokens": ["Un\u00b7heim\u00b7lich", "Schau\u00b7dern", "Ei\u00b7nen", "\u00fc\u00b7berg\u00b7lei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df dann ob seiner k\u00fcnft'gen Grabesstatt", "tokens": ["Da\u00df", "dann", "ob", "sei\u00b7ner", "k\u00fcnft'\u00b7gen", "Gra\u00b7bess\u00b7tatt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Todesengel schreite.", "tokens": ["Der", "To\u00b7de\u00b7sen\u00b7gel", "schrei\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich h\u00f6rte sie, und malte mir ein Bild", "tokens": ["Ich", "h\u00f6r\u00b7te", "sie", ",", "und", "mal\u00b7te", "mir", "ein", "Bild"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KON", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit Trauerlocken, mondbegl\u00e4nzter Stirne,", "tokens": ["Mit", "Trau\u00b7er\u00b7lo\u00b7cken", ",", "mond\u00b7be\u00b7gl\u00e4nz\u00b7ter", "Stir\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So schaurig sch\u00f6n, wie's wohl zuweilen quillt", "tokens": ["So", "schau\u00b7rig", "sch\u00f6n", ",", "wie's", "wohl", "zu\u00b7wei\u00b7len", "quillt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADJD", "$,", "VVFIN", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Im schwimmenden Gehirne.", "tokens": ["Im", "schwim\u00b7men\u00b7den", "Ge\u00b7hir\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "In seiner Hand sah ich den Ebenstab", "tokens": ["In", "sei\u00b7ner", "Hand", "sah", "ich", "den", "E\u00b7bens\u00b7tab"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit leisem Strich des Bettes Lage messen,", "tokens": ["Mit", "lei\u00b7sem", "Strich", "des", "Bet\u00b7tes", "La\u00b7ge", "mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "\u2014 So weit das Haupt \u2014 so weit der Fu\u00df \u2014 hinab!", "tokens": ["So", "weit", "das", "Haupt", "so", "weit", "der", "Fu\u00df", "hin\u00b7ab", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "ART", "NN", "$(", "ADV", "ADJD", "ART", "NN", "$(", "PTKVZ", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Versch\u00fcttet und vergessen!", "tokens": ["Ver\u00b7sch\u00fct\u00b7tet", "und", "ver\u00b7ges\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Mich graute, doch ich sprach dem Grauen Hohn,", "tokens": ["Mich", "grau\u00b7te", ",", "doch", "ich", "sprach", "dem", "Grau\u00b7en", "Hohn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich hielt das Bild in Reimes Netz gefangen,", "tokens": ["Ich", "hielt", "das", "Bild", "in", "Rei\u00b7mes", "Netz", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und frevelnd wagt' ich aus der Todtenkron'", "tokens": ["Und", "fre\u00b7velnd", "wagt'", "ich", "aus", "der", "Tod\u00b7ten\u00b7kron'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Lorbeerblatt zu langen.", "tokens": ["Ein", "Lor\u00b7beer\u00b7blatt", "zu", "lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O, manche Stunde denk ich jetzt daran,", "tokens": ["O", ",", "man\u00b7che", "Stun\u00b7de", "denk", "ich", "jetzt", "da\u00b7ran", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PIAT", "NN", "VVFIN", "PPER", "ADV", "PAV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fchl' ich mein Blut so matt und stockend schleichen,", "tokens": ["F\u00fchl'", "ich", "mein", "Blut", "so", "matt", "und", "sto\u00b7ckend", "schlei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "ADV", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Schaut aus dem Spiegel mich ein Antlitz an \u2014", "tokens": ["Schaut", "aus", "dem", "Spie\u00b7gel", "mich", "ein", "Ant\u00b7litz", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPER", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich mag es nicht vergleichen; \u2014", "tokens": ["Ich", "mag", "es", "nicht", "ver\u00b7glei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Als ich zuerst dich auf dem Friedhof fand,", "tokens": ["Als", "ich", "zu\u00b7erst", "dich", "auf", "dem", "Fried\u00b7hof", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Tiefsinnig um die Monumente streifend,", "tokens": ["Tief\u00b7sin\u00b7nig", "um", "die", "Mo\u00b7nu\u00b7men\u00b7te", "strei\u00b7fend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+---+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Den schwarzen Ebenstab in deiner Hand", "tokens": ["Den", "schwar\u00b7zen", "E\u00b7bens\u00b7tab", "in", "dei\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Entlang die H\u00fcgel schleifend;", "tokens": ["Ent\u00b7lang", "die", "H\u00fc\u00b7gel", "schlei\u00b7fend", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}