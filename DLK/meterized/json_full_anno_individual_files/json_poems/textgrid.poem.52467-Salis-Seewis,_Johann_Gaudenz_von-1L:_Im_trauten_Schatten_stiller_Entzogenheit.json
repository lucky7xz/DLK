ct"], "pos": ["NE", "ART", "ADJA", "NN", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.13": {"line.1": {"text": "Wer jeden duldet, liebt, was zu lieben ist,", "tokens": ["Wer", "je\u00b7den", "dul\u00b7det", ",", "liebt", ",", "was", "zu", "lie\u00b7ben", "ist", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVFIN", "$,", "VVFIN", "$,", "PRELS", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Von andern wenig, vieles von sich begehrt,", "tokens": ["Von", "an\u00b7dern", "we\u00b7nig", ",", "vie\u00b7les", "von", "sich", "be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PIS", "$,", "PIS", "APPR", "PRF", "VVPP", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dem spro\u00dft des heitern Friedens \u00d6lblatt,", "tokens": ["Dem", "spro\u00dft", "des", "hei\u00b7tern", "Frie\u00b7dens", "\u00d6l\u00b7blatt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das der Gen\u00fcgsamkeit Stirne k\u00fchlet.", "tokens": ["Das", "der", "Ge\u00b7n\u00fcg\u00b7sam\u00b7keit", "Stir\u00b7ne", "k\u00fch\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.14": {"line.1": {"text": "Mit Lotus kr\u00e4nz' ich meiner Penaten Haupt;", "tokens": ["Mit", "Lo\u00b7tus", "kr\u00e4nz'", "ich", "mei\u00b7ner", "Pe\u00b7na\u00b7ten", "Haupt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PPER", "PPOSAT", "NN", "NN", "$."