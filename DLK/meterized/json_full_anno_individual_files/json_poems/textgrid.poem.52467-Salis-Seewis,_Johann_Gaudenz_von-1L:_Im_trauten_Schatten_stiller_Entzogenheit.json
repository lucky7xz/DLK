{"textgrid.poem.52467": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im trauten Schatten stiller Entzogenheit", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im trauten Schatten stiller Entzogenheit", "tokens": ["Im", "trau\u00b7ten", "Schat\u00b7ten", "stil\u00b7ler", "Ent\u00b7zo\u00b7gen\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Fand ich den Frieden, der uns erweicht und st\u00e4rkt,", "tokens": ["Fand", "ich", "den", "Frie\u00b7den", ",", "der", "uns", "er\u00b7weicht", "und", "st\u00e4rkt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "KON", "VVFIN", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Der auf das Schicksal, wie der Weise", "tokens": ["Der", "auf", "das", "Schick\u00b7sal", ",", "wie", "der", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Heiter auf bl\u00fchende Gr\u00e4ber, schauet.", "tokens": ["Hei\u00b7ter", "auf", "bl\u00fc\u00b7hen\u00b7de", "Gr\u00e4\u00b7ber", ",", "schau\u00b7et", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "O du des Weltlaufs s\u00fc\u00dfe Vergessenheit,", "tokens": ["O", "du", "des", "Welt\u00b7laufs", "s\u00fc\u00b7\u00dfe", "Ver\u00b7ges\u00b7s