{"dta.poem.15031": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Nachrichten von dem Ursprung und  \n Wachsthum der Critik bey den  \n Deutschen.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1741", "urn": "urn:nbn:de:kobv:b4-200905198526", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Reim klingt vielen schon sehr matt und ohne Kraft:", "tokens": ["Mein", "Reim", "klingt", "vie\u00b7len", "schon", "sehr", "matt", "und", "oh\u00b7ne", "Kraft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PIS", "ADV", "ADV", "ADJD", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Warum? Ich tr\u00e4nck ihn nicht mit Muscateller-Saft;", "tokens": ["Wa\u00b7rum", "?", "Ich", "tr\u00e4nck", "ihn", "nicht", "mit", "Mus\u00b7ca\u00b7tel\u00b7ler\u00b7Saft", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ich speis\u2019 ihn auch nicht mehr mit theuren Ambrakuchen,", "tokens": ["Ich", "speis'", "ihn", "auch", "nicht", "mehr", "mit", "theu\u00b7ren", "A\u00b7mbra\u00b7ku\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Denn er ist alt genug, die Nahrung selbst zu suchen.", "tokens": ["Denn", "er", "ist", "alt", "ge\u00b7nug", ",", "die", "Nah\u00b7rung", "selbst", "zu", "su\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "ADV", "$,", "ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Zibeth und Biesam hat ihm manchen Dienst gethan:", "tokens": ["Zi\u00b7beth", "und", "Bie\u00b7sam", "hat", "ihm", "man\u00b7chen", "Dienst", "ge\u00b7than", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VAFIN", "PPER", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nun will ich einmahl sehn, was er alleine kan.", "tokens": ["Nun", "will", "ich", "ein\u00b7mahl", "sehn", ",", "was", "er", "al\u00b7lei\u00b7ne", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF", "$,", "PWS", "PPER", "ADV", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Alleine? Fraget ihr: Ja, wie gedacht, alleine.", "tokens": ["Al\u00b7lei\u00b7ne", "?", "Fra\u00b7get", "ihr", ":", "Ja", ",", "wie", "ge\u00b7dacht", ",", "al\u00b7lei\u00b7ne", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$.", "VVFIN", "PPER", "$.", "PTKANT", "$,", "PWAV", "VVPP", "$,", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Denn, was ich ehmahls schrieb, war weder mein noch seine.", "tokens": ["Denn", ",", "was", "ich", "eh\u00b7mahls", "schrieb", ",", "war", "we\u00b7der", "mein", "noch", "sei\u00b7ne", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWS", "PPER", "ADV", "VVFIN", "$,", "VAFIN", "KON", "PPOSAT", "ADV", "PPOSAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Hier hatte Seneca, dort Plato was gesagt;", "tokens": ["Hier", "hat\u00b7te", "Se\u00b7ne\u00b7ca", ",", "dort", "Pla\u00b7to", "was", "ge\u00b7sagt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "$,", "ADV", "NE", "PWS", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da hatt\u2019 ich einen Spruch dem Plautus abgejagt;", "tokens": ["Da", "hatt'", "ich", "ei\u00b7nen", "Spruch", "dem", "Plau\u00b7tus", "ab\u00b7ge\u00b7jagt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und etwann anderswo den Tacitus bestohlen.", "tokens": ["Und", "et\u00b7wann", "an\u00b7ders\u00b7wo", "den", "Ta\u00b7ci\u00b7tus", "be\u00b7stoh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ART", "NE", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Auf diesen schwachen Grund, ich sag es unverholen,", "tokens": ["Auf", "die\u00b7sen", "schwa\u00b7chen", "Grund", ",", "ich", "sag", "es", "un\u00b7ver\u00b7ho\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Baut\u2019 ich von Versen oft damahls ein gantzes Haus,", "tokens": ["Baut'", "ich", "von", "Ver\u00b7sen", "oft", "da\u00b7mahls", "ein", "gant\u00b7zes", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und ziert\u2019 es noch dazu mit Sinnebildern aus.", "tokens": ["Und", "ziert'", "es", "noch", "da\u00b7zu", "mit", "Sin\u00b7ne\u00b7bil\u00b7dern", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PAV", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wie oftmahls mu\u00df ich nicht der abgeschmackten Sachen,", "tokens": ["Wie", "oft\u00b7mahls", "mu\u00df", "ich", "nicht", "der", "ab\u00b7ge\u00b7schmack\u00b7ten", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wann ich zur\u00fccke seh, noch bey mir selber lachen.", "tokens": ["Wann", "ich", "zu\u00b7r\u00fc\u00b7cke", "seh", ",", "noch", "bey", "mir", "sel\u00b7ber", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "VVFIN", "$,", "ADV", "APPR", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Gleichwohl gefielen sie, und nahmen durch den Schein,", "tokens": ["Gleich\u00b7wohl", "ge\u00b7fie\u00b7len", "sie", ",", "und", "nah\u00b7men", "durch", "den", "Schein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wie schlecht er immer war, viel hundert Leser ein.", "tokens": ["Wie", "schlecht", "er", "im\u00b7mer", "war", ",", "viel", "hun\u00b7dert", "Le\u00b7ser", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "VAFIN", "$,", "ADV", "CARD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ha! schrie man hier und da; f\u00fcr dem mu\u00df Opitz weichen.", "tokens": ["Ha", "!", "schrie", "man", "hier", "und", "da", ";", "f\u00fcr", "dem", "mu\u00df", "O\u00b7pitz", "wei\u00b7chen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PIS", "ADV", "KON", "ADV", "$.", "APPR", "ART", "VMFIN", "NE", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ja, dacht ich, wenn ich ihn nur erstlich k\u00f6nnt erreichen!", "tokens": ["Ja", ",", "dacht", "ich", ",", "wenn", "ich", "ihn", "nur", "erst\u00b7lich", "k\u00f6nnt", "er\u00b7rei\u00b7chen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PPER", "ADV", "ADJD", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Den Willen h\u00e4tt ich wohl. So wie ich es gedacht,", "tokens": ["Den", "Wil\u00b7len", "h\u00e4tt", "ich", "wohl", ".", "So", "wie", "ich", "es", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "$.", "ADV", "KOKOM", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "So ist es auch geschehn. Ich habe manche Nacht", "tokens": ["So", "ist", "es", "auch", "ge\u00b7schehn", ".", "Ich", "ha\u00b7be", "man\u00b7che", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$.", "PPER", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und manchen Tag geschwizt; allein ich mu\u00df gestehen,", "tokens": ["Und", "man\u00b7chen", "Tag", "ge\u00b7schwizt", ";", "al\u00b7lein", "ich", "mu\u00df", "ge\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVPP", "$.", "ADV", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Da\u00df ich ihm noch umsonst versuche nachzugehen.", "tokens": ["Da\u00df", "ich", "ihm", "noch", "um\u00b7sonst", "ver\u00b7su\u00b7che", "nach\u00b7zu\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADV", "ADJA", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}