{"textgrid.poem.25000": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Bei Musik", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer bist du, dessen Odem auf den Wogen", "tokens": ["Wer", "bist", "du", ",", "des\u00b7sen", "O\u00b7dem", "auf", "den", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "$,", "PRELAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der T\u00f6ne mir entgegenquillt?", "tokens": ["Der", "T\u00f6\u00b7ne", "mir", "ent\u00b7ge\u00b7gen\u00b7quillt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Entz\u00fcckungen, die nicht von dieser Erde,", "tokens": ["Ent\u00b7z\u00fc\u00b7ckun\u00b7gen", ",", "die", "nicht", "von", "die\u00b7ser", "Er\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PTKNEG", "APPR", "PDAT", "NN", "$,"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Wehn leise mich aus ihnen an; ich werde", "tokens": ["Wehn", "lei\u00b7se", "mich", "aus", "ih\u00b7nen", "an", ";", "ich", "wer\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ADJD", "PRF", "APPR", "PPER", "PTKVZ", "$.", "PPER", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Hinunter an das bleiche Meer gezogen,", "tokens": ["Hin\u00b7un\u00b7ter", "an", "das", "blei\u00b7che", "Meer", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Das zwischen hier und dr\u00fcben schwillt.", "tokens": ["Das", "zwi\u00b7schen", "hier", "und", "dr\u00fc\u00b7ben", "schwillt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "KON", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mich f\u00fchrt ein Weib, verh\u00fcllt mit wei\u00dfem Schleier,", "tokens": ["Mich", "f\u00fchrt", "ein", "Weib", ",", "ver\u00b7h\u00fcllt", "mit", "wei\u00b7\u00dfem", "Schlei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In ihren Kahn; von dannen tr\u00e4gt", "tokens": ["In", "ih\u00b7ren", "Kahn", ";", "von", "dan\u00b7nen", "tr\u00e4gt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$.", "APPR", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Windeshauch uns auf dem Wellenspiele,", "tokens": ["Der", "Win\u00b7des\u00b7hauch", "uns", "auf", "dem", "Wel\u00b7len\u00b7spie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das sich melodisch bricht am Kiele", "tokens": ["Das", "sich", "me\u00b7lo\u00b7disch", "bricht", "am", "Kie\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PRF", "ADJD", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und t\u00f6nend bei den Kl\u00e4ngen ihrer Leier", "tokens": ["Und", "t\u00f6\u00b7nend", "bei", "den", "Kl\u00e4n\u00b7gen", "ih\u00b7rer", "Lei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Stets weitre, weitre Kreise schl\u00e4gt.", "tokens": ["Stets", "weit\u00b7re", ",", "weit\u00b7re", "Krei\u00b7se", "schl\u00e4gt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein Lispeln hallt um mich von Geisterstimmen,", "tokens": ["Ein", "Lis\u00b7peln", "hallt", "um", "mich", "von", "Geis\u00b7ters\u00b7tim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Laute, die ich nie gekannt,", "tokens": ["Und", "Lau\u00b7te", ",", "die", "ich", "nie", "ge\u00b7kannt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Murmeln h\u00f6r' ich ungesehner Quellen;", "tokens": ["Und", "Mur\u00b7meln", "h\u00f6r'", "ich", "un\u00b7ge\u00b7seh\u00b7ner", "Quel\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dann legt sich gro\u00dfe Stille auf die Wellen,", "tokens": ["Dann", "legt", "sich", "gro\u00b7\u00dfe", "Stil\u00b7le", "auf", "die", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Drauf wei\u00dfe, wunderbare Bl\u00fcten schwimmen,", "tokens": ["Drauf", "wei\u00b7\u00dfe", ",", "wun\u00b7der\u00b7ba\u00b7re", "Bl\u00fc\u00b7ten", "schwim\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wie Boten von dem Jenseitsstrand.", "tokens": ["Wie", "Bo\u00b7ten", "von", "dem", "Jen\u00b7seits\u00b7strand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "In eine Schale, w\u00e4hrend s\u00fc\u00dfes Beben", "tokens": ["In", "ei\u00b7ne", "Scha\u00b7le", ",", "w\u00e4h\u00b7rend", "s\u00fc\u00b7\u00dfes", "Be\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom Haupt zum Fu\u00dfe mich durchschleicht,", "tokens": ["Vom", "Haupt", "zum", "Fu\u00b7\u00dfe", "mich", "durch\u00b7schleicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sch\u00f6pft von den blassen Wellen die Verh\u00fcllte", "tokens": ["Sch\u00f6pft", "von", "den", "blas\u00b7sen", "Wel\u00b7len", "die", "Ver\u00b7h\u00fcll\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und bietet mir zum Trank die randgef\u00fcllte;", "tokens": ["Und", "bie\u00b7tet", "mir", "zum", "Trank", "die", "rand\u00b7ge\u00b7f\u00fcll\u0