tanza.14": {"line.1": {"text": "Ach, wie treu h\u00e4lt er sein Wort!", "tokens": ["Ach", ",", "wie", "treu", "h\u00e4lt", "er", "sein", "Wort", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kaum sind zehen Tage fort,", "tokens": ["Kaum", "sind", "ze\u00b7hen", "Ta\u00b7ge", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "CARD", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie ein starker Wind entstehet,", "tokens": ["Wie", "ein", "star\u00b7ker", "Wind", "ent\u00b7ste\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der dir, sanffter West, vorgehet,", "tokens": ["Der", "dir", ",", "sanff\u00b7ter", "West", ",", "vor\u00b7ge\u00b7het", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "$,", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und du in der Flammen Bild", "tokens": ["Und", "du", "in", "der", "Flam\u00b7men", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "ART", "NN", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Seine Diener hast erf\u00fcllt.", "tokens": ["Sei\u00b7ne", "Die\u00b7ner", "hast", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Feurig war die Seul bei Nacht,", "tokens": ["Feu\u00b7rig", "war", "die", "Seul", "bei", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die Israel hat bewacht,", "tokens": ["Die", "Is\u00b7rael", "hat", "be\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Feurig war Elias Wagen,", "tokens": ["Feu\u00b7rig", "war", "E\u00b7lias", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "NE", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "So ward Gott selbst angetragen,", "tokens": ["So", "ward", "Gott", "selbst", "an\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Feurig ist so ins gemein,", "tokens": ["Feu\u00b7rig", "ist", "so", "ins", "ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ADV", "APPRART", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn was g\u00f6ttlich gantz soll seyn.", "tokens": ["Wenn", "was", "g\u00f6tt\u00b7lich", "gantz", "soll", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "ADV", "VMFIN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Und wie wol gleicht dir die Glut,", "tokens": ["Und", "wie", "wol", "gleicht", "dir", "die", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.2": {"text":