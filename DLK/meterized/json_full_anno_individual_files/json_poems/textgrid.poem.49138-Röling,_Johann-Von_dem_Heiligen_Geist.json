{"textgrid.poem.49138": {"metadata": {"author": {"name": "R\u00f6ling, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Heiligen Geist", "genre": "verse", "period": "N.A.", "pub_year": 1656, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr des h\u00f6chsten Himmels-Throns,", "tokens": ["Herr", "des", "h\u00f6chs\u00b7ten", "Him\u00b7mels\u00b7Throns", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Band des Vaters und des Sohns,", "tokens": ["Band", "des", "Va\u00b7ters", "und", "des", "Sohns", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Freuden-Quelle der Betr\u00fcbten,", "tokens": ["Freu\u00b7den\u00b7Quel\u00b7le", "der", "Be\u00b7tr\u00fcb\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Feuer der in dich Verliebten,", "tokens": ["Feu\u00b7er", "der", "in", "dich", "Ver\u00b7lieb\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hochgelobet und gepreyst", "tokens": ["Hoch\u00b7ge\u00b7lo\u00b7bet", "und", "ge\u00b7preyst"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sey dein' Ankunfft, Heilger Geist.", "tokens": ["Sey", "dein'", "An\u00b7kunfft", ",", "Heil\u00b7ger", "Geist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Unser Heyl zog Himmel ein;", "tokens": ["Un\u00b7ser", "Heyl", "zog", "Him\u00b7mel", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit wie schwerer Hertzens-Pein", "tokens": ["Mit", "wie", "schwe\u00b7rer", "Hert\u00b7zens\u00b7Pein"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "KOKOM", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wir sein' Auffahrt angesehen,", "tokens": ["Wir", "sein'", "Auf\u00b7fahrt", "an\u00b7ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Must' er selber uns gestehen,", "tokens": ["Must'", "er", "sel\u00b7ber", "uns", "ge\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da er uns den Trost einsprach,", "tokens": ["Da", "er", "uns", "den", "Trost", "ein\u00b7sprach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df du ihm soltst folgen nach.", "tokens": ["Da\u00df", "du", "ihm", "soltst", "fol\u00b7gen", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VMFIN", "VVINF", "PTKVZ", "$."], "meter": "+---+-+", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Ach, wie treu h\u00e4lt er sein Wort!", "tokens": ["Ach", ",", "wie", "treu", "h\u00e4lt", "er", "sein", "Wort", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kaum sind zehen Tage fort,", "tokens": ["Kaum", "sind", "ze\u00b7hen", "Ta\u00b7ge", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "CARD", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie ein starker Wind entstehet,", "tokens": ["Wie", "ein", "star\u00b7ker", "Wind", "ent\u00b7ste\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der dir, sanffter West, vorgehet,", "tokens": ["Der", "dir", ",", "sanff\u00b7ter", "West", ",", "vor\u00b7ge\u00b7het", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "$,", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und du in der Flammen Bild", "tokens": ["Und", "du", "in", "der", "Flam\u00b7men", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "ART", "NN", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Seine Diener hast erf\u00fcllt.", "tokens": ["Sei\u00b7ne", "Die\u00b7ner", "hast", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Feurig war die Seul bei Nacht,", "tokens": ["Feu\u00b7rig", "war", "die", "Seul", "bei", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die Israel hat bewacht,", "tokens": ["Die", "Is\u00b7rael", "hat", "be\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Feurig war Elias Wagen,", "tokens": ["Feu\u00b7rig", "war", "E\u00b7lias", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "NE", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "So ward Gott selbst angetragen,", "tokens": ["So", "ward", "Gott", "selbst", "an\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Feurig ist so ins gemein,", "tokens": ["Feu\u00b7rig", "ist", "so", "ins", "ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ADV", "APPRART", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn was g\u00f6ttlich gantz soll seyn.", "tokens": ["Wenn", "was", "g\u00f6tt\u00b7lich", "gantz", "soll", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "ADV", "VMFIN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und wie wol gleicht dir die Glut,", "tokens": ["Und", "wie", "wol", "gleicht", "dir", "die", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.2": {"text": "Sie ist, die belebt das Blut,", "tokens": ["Sie", "ist", ",", "die", "be\u00b7lebt", "das", "Blut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PRELS", "VVFIN", "ART", "NN", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Unsre Geister mehrt und nehret,", "tokens": ["Uns\u00b7re", "Geis\u00b7ter", "mehrt", "und", "neh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "All' Unsauberkeit verzehret", "tokens": ["All'", "Un\u00b7sau\u00b7ber\u00b7keit", "ver\u00b7zeh\u00b7ret"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und f\u00fcr Unlust, M\u00fc\u00df' und K\u00e4lt", "tokens": ["Und", "f\u00fcr", "Un\u00b7lust", ",", "M\u00fc\u00df'", "und", "K\u00e4lt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "$,", "NE", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Trieb und Muth in uns erh\u00e4lt.", "tokens": ["Trieb", "und", "Muth", "in", "uns", "er\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Sch\u00f6n trifft dieses mit dir ein!", "tokens": ["Sch\u00f6n", "trifft", "die\u00b7ses", "mit", "dir", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PDAT", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wer macht unser' Hertzen rein,", "tokens": ["Wer", "macht", "un\u00b7ser'", "Hert\u00b7zen", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wer erw\u00e4rmt der Seelen Geister,", "tokens": ["Wer", "er\u00b7w\u00e4rmt", "der", "See\u00b7len", "Geis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als du, o der Seelen Meister,", "tokens": ["Als", "du", ",", "o", "der", "See\u00b7len", "Meis\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "FM", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und wer regt uns immerzu,", "tokens": ["Und", "wer", "regt", "uns", "im\u00b7mer\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df wir th\u00e4tig sind, als du?", "tokens": ["Da\u00df", "wir", "th\u00e4\u00b7tig", "sind", ",", "als", "du", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$,", "KOUS", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Denk, wie der Apostel Chor", "tokens": ["Denk", ",", "wie", "der", "A\u00b7pos\u00b7tel", "Chor"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sich durch dich fort that hervor;", "tokens": ["Sich", "durch", "dich", "fort", "that", "her\u00b7vor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPER", "PTKVZ", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sachen, die sie nie geh\u00f6ret,", "tokens": ["Sa\u00b7chen", ",", "die", "sie", "nie", "ge\u00b7h\u00f6\u00b7ret", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sprachen, die sie nie gelehret,", "tokens": ["Spra\u00b7chen", ",", "die", "sie", "nie", "ge\u00b7leh\u00b7ret", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Thaten, die gesehen nie,", "tokens": ["Tha\u00b7ten", ",", "die", "ge\u00b7se\u00b7hen", "nie", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "VVPP", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sah und h\u00f6rte man durch sie.", "tokens": ["Sah", "und", "h\u00f6r\u00b7te", "man", "durch", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PIS", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Thu an mir, gottreiche Brunst,", "tokens": ["Thu", "an", "mir", ",", "got\u00b7trei\u00b7che", "Brunst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Thu an mir auch diese Gunst,", "tokens": ["Thu", "an", "mir", "auch", "die\u00b7se", "Gunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ADV", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Z\u00fcnd' in mir an deine Flammen,", "tokens": ["Z\u00fcnd'", "in", "mir", "an", "dei\u00b7ne", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie, der wahren Andacht Ammen,", "tokens": ["Sie", ",", "der", "wah\u00b7ren", "An\u00b7dacht", "Am\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "ADJA", "NN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und mach, da\u00df dein Geist allein", "tokens": ["Und", "mach", ",", "da\u00df", "dein", "Geist", "al\u00b7lein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "ADV"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Meines Geistes Trieb mag sein.", "tokens": ["Mei\u00b7nes", "Geis\u00b7tes", "Trieb", "mag", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VMFIN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Saubre mich von meinem Wust,", "tokens": ["Saub\u00b7re", "mich", "von", "mei\u00b7nem", "Wust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Aller Welt- und Fleisches Lust,", "tokens": ["Al\u00b7ler", "Welt", "und", "Flei\u00b7sches", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gib mir eine reine Seele,", "tokens": ["Gib", "mir", "ei\u00b7ne", "rei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit der sich ihr Heil verm\u00e4hle", "tokens": ["Mit", "der", "sich", "ihr", "Heil", "ver\u00b7m\u00e4h\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PRF", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und erw\u00e4rm mich, wenn ichs kalt", "tokens": ["Und", "er\u00b7w\u00e4rm", "mich", ",", "wenn", "ichs", "kalt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PIS", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Oder laulicht mit dir halt.", "tokens": ["O\u00b7der", "lau\u00b7licht", "mit", "dir", "halt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "\u00d6ffne mir die Gnaden-Sch\u00e4tz,", "tokens": ["\u00d6ff\u00b7ne", "mir", "die", "Gna\u00b7den\u00b7Sch\u00e4tz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ich lerne dein Gesetz,", "tokens": ["Da\u00df", "ich", "ler\u00b7ne", "dein", "Ge\u00b7setz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dein Gesetz der Huld und G\u00fcte;", "tokens": ["Dein", "Ge\u00b7setz", "der", "Huld", "und", "G\u00fc\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fclle hievon mein Gem\u00fcthe,", "tokens": ["F\u00fcl\u00b7le", "hie\u00b7von", "mein", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PAV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sinne, Willen und Begier,", "tokens": ["Sin\u00b7ne", ",", "Wil\u00b7len", "und", "Be\u00b7gier", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df ich trunken sey von dir.", "tokens": ["Da\u00df", "ich", "trun\u00b7ken", "sey", "von", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Wenn ich auch zuletzt erkalt", "tokens": ["Wenn", "ich", "auch", "zu\u00b7letzt", "er\u00b7kalt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und den Othem kaum mehr halt,", "tokens": ["Und", "den", "O\u00b7them", "kaum", "mehr", "halt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, so la\u00df mich Glauben halten", "tokens": ["Ach", ",", "so", "la\u00df", "mich", "Glau\u00b7ben", "hal\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "VVIMP", "PPER", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und in Jesu nicht erkalten,", "tokens": ["Und", "in", "Je\u00b7su", "nicht", "er\u00b7kal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und entzieh durch de\u00dfen Blut", "tokens": ["Und", "ent\u00b7zieh", "durch", "de\