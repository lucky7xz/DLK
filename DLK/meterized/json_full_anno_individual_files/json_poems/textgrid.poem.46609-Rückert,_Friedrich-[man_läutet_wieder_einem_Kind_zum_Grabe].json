{"textgrid.poem.46609": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[man l\u00e4utet wieder einem Kind zum Grabe]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man l\u00e4utet wieder einem Kind zum Grabe,", "tokens": ["Man", "l\u00e4u\u00b7tet", "wie\u00b7der", "ei\u00b7nem", "Kind", "zum", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und seine Eltern trifft wohl schwer der Klang;", "tokens": ["Und", "sei\u00b7ne", "El\u00b7tern", "trifft", "wohl", "schwer", "der", "Klang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mir aber w\u00e4r' er eine Segensgabe,", "tokens": ["Mir", "a\u00b7ber", "w\u00e4r'", "er", "ei\u00b7ne", "Se\u00b7gens\u00b7ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn er geleitete zum letzten Gang", "tokens": ["Wenn", "er", "ge\u00b7lei\u00b7te\u00b7te", "zum", "letz\u00b7ten", "Gang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mein eignes Kind, das liebste, das ich habe,", "tokens": ["Mein", "eig\u00b7nes", "Kind", ",", "das", "liebs\u00b7te", ",", "das", "ich", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "pu