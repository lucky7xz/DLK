{"textgrid.poem.63763": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Im Coliseo", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gelinder flie\u00dft in dieser Luft das Blut.", "tokens": ["Ge\u00b7lin\u00b7der", "flie\u00dft", "in", "die\u00b7ser", "Luft", "das", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PDAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Seele lernt ihr st\u00fcrmisch Weh bez\u00e4hmen,", "tokens": ["Die", "See\u00b7le", "lernt", "ihr", "st\u00fcr\u00b7misch", "Weh", "be\u00b7z\u00e4h\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des Haftens am Verg\u00e4nglichen sich sch\u00e4men,", "tokens": ["Des", "Haf\u00b7tens", "am", "Ver\u00b7g\u00e4ng\u00b7li\u00b7chen", "sich", "sch\u00e4\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo eine stolze Welt in Tr\u00fcmmern ruht.", "tokens": ["Wo", "ei\u00b7ne", "stol\u00b7ze", "Welt", "in", "Tr\u00fcm\u00b7mern", "ruht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "H\u00f6hnt hier nicht jede Quader: Eintagsbrut,", "tokens": ["H\u00f6hnt", "hier", "nicht", "je\u00b7de", "Qua\u00b7der", ":", "Ein\u00b7tags\u00b7brut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKNEG", "PIAT", "NN", "$.", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Willst du dein Zwergen-Ich so wichtig nehmen?", "tokens": ["Willst", "du", "dein", "Zwer\u00b7gen\u00b7Ich", "so", "wich\u00b7tig", "neh\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was ist dein Sehnen, Jauchzen oder Gr\u00e4men?", "tokens": ["Was", "ist", "dein", "Seh\u00b7nen", ",", "Jauch\u00b7zen", "o\u00b7der", "Gr\u00e4\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPOSAT", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Tropfen nur im All der Geisterflut.", "tokens": ["Ein", "Trop\u00b7fen", "nur", "im", "All", "der", "Geis\u00b7ter\u00b7flut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch w\u00e4hrend mich umrauscht das ew'ge Flie\u00dfen", "tokens": ["Doch", "w\u00e4h\u00b7rend", "mich", "um\u00b7rauscht", "das", "ew'\u00b7ge", "Flie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des uferlosen Meers, in dessen Bette", "tokens": ["Des", "u\u00b7fer\u00b7lo\u00b7sen", "Meers", ",", "in", "des\u00b7sen", "Bet\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PRELAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Spurlos versinkt, was hoch und herrlich war,", "tokens": ["Spur\u00b7los", "ver\u00b7sinkt", ",", "was", "hoch", "und", "herr\u00b7lich", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "PRELS", "ADJD", "KON", "ADJD", "VAFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "Kann wie ein schweres Unheil mich verdrie\u00dfen", "tokens": ["Kann", "wie", "ein", "schwe\u00b7res", "Un\u00b7heil", "mich", "ver\u00b7drie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "KOKOM", "ART", "ADJA", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein ungef\u00fcgig Reimwort im Sonette \u2013", "tokens": ["Ein", "un\u00b7ge\u00b7f\u00fc\u00b7gig", "Reim\u00b7wort", "im", "So\u00b7net\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O Widerspruch, dein Nam' ist Mensch f\u00fcrwahr!", "tokens": ["O", "Wi\u00b7der\u00b7spruch", ",", "dein", "Nam'", "ist", "Mensch", "f\u00fcr\u00b7wahr", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPOSAT", "NN", "VAFIN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Gelinder flie\u00dft in dieser Luft das Blut.", "tokens": ["Ge\u00b7lin\u00b7der", "flie\u00dft", "in", "die\u00b7ser", "Luft", "das", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PDAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Seele lernt ihr st\u00fcrmisch W