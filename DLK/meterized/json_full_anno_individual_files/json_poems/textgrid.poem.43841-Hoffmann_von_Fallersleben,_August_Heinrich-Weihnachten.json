{"textgrid.poem.43841": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Weihnachten", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwar ist das Jahr an Festen reich,", "tokens": ["Zwar", "ist", "das", "Jahr", "an", "Fes\u00b7ten", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch ist kein Fest dem Feste gleich,", "tokens": ["Doch", "ist", "kein", "Fest", "dem", "Fes\u00b7te", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIAT", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Worauf wir Kinder Jahr aus Jahr ein", "tokens": ["Wo\u00b7rauf", "wir", "Kin\u00b7der", "Jahr", "aus", "Jahr", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "NN", "NN", "APPR", "NN", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Stets harren in s\u00fc\u00dfer Lust und Pein.", "tokens": ["Stets", "har\u00b7ren", "in", "s\u00fc\u00b7\u00dfer", "Lust", "und", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "O sch\u00f6ne, herrliche Weihnachtszeit,", "tokens": ["O", "sch\u00f6\u00b7ne", ",", "herr\u00b7li\u00b7che", "Weih\u00b7nachts\u00b7zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Was bringst du Lust und Fr\u00f6hlichkeit!", "tokens": ["Was", "bringst", "du", "Lust", "und", "Fr\u00f6h\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn der heilige Christ in jedem Haus", "tokens": ["Wenn", "der", "hei\u00b7li\u00b7ge", "Christ", "in", "je\u00b7dem", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "PIAT", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Theilt seine lieben Gaben aus.", "tokens": ["Theilt", "sei\u00b7ne", "lie\u00b7ben", "Ga\u00b7ben", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und ist das H\u00e4uschen noch so klein,", "tokens": ["Und", "ist", "das", "H\u00e4usc\u00b7hen", "noch", "so", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So kommt der heilige Christ hinein,", "tokens": ["So", "kommt", "der", "hei\u00b7li\u00b7ge", "Christ", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und Alle sind ihm lieb wie die Seinen,", "tokens": ["Und", "Al\u00b7le", "sind", "ihm", "lieb", "wie", "die", "Sei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "PPER", "ADJD", "KOKOM", "ART", "PPOSS", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Armen und Reichen, die Gro\u00dfen und Kleinen.", "tokens": ["Die", "Ar\u00b7men", "und", "Rei\u00b7chen", ",", "die", "Gro\u00b7\u00dfen", "und", "Klei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,", "ART", "NN", "KON", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Der heilige Christ an Alle denkt,", "tokens": ["Der", "hei\u00b7li\u00b7ge", "Christ", "an", "Al\u00b7le", "denkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PIS", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein Jedes wird von ihm beschenkt.", "tokens": ["Ein", "Je\u00b7des", "wird", "von", "ihm", "be\u00b7schenkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Drum la\u00dft uns freu'n und dankbar sein!", "tokens": ["Drum", "la\u00dft", "uns", "freu'n", "und", "dank\u00b7bar", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "VVINF", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er denkt auch unser, mein und dein.", "tokens": ["Er", "denkt", "auch", "un\u00b7ser", ",", "mein", "und", "dein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "$,", "PPOSAT", "KON", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Zwar ist das Jahr an Festen reich,", "tokens": ["Zwar", "ist", "das", "Jahr", "an", "