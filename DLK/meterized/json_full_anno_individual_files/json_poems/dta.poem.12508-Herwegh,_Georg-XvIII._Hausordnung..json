{"dta.poem.12508": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "XvIII.  \n  Hausordnung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201enegatives Geschlecht!\u201c \u2014 Nur Geduld; erst", "tokens": ["\u201e", "ne\u00b7ga\u00b7ti\u00b7ves", "Ge\u00b7schlecht", "!", "\u201c", "Nur", "Ge\u00b7duld", ";", "erst"], "token_info": ["punct", "word", "word", "punct", "punct", "punct", "word", "word", "punct", "word"], "pos": ["$(", "ADJA", "NN", "$.", "$(", "$(", "ADV", "NN", "$.", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "h\u00e4lt man die Aerndte,", "tokens": ["h\u00e4lt", "man", "die", "A\u00b7ernd\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Dann aus dem frischern Korn backen die", "tokens": ["Dann", "aus", "dem", "fri\u00b7schern", "Korn", "ba\u00b7cken", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVFIN", "ART"], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}, "line.4": {"text": "S\u00f6hne das Brod:", "tokens": ["S\u00f6h\u00b7ne", "das", "Brod", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Und zwar besseres Brod als j\u00fcngst uns ", "tokens": ["Und", "zwar", "bes\u00b7se\u00b7res", "Brod", "als", "j\u00fcngst", "uns"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJA", "NN", "KOKOM", "ADV", "PPER"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.6": {"text": "gebacken,", "tokens": ["ge\u00b7ba\u00b7cken", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Das den Germanen auf lang wieder den", "tokens": ["Das", "den", "Ger\u00b7ma\u00b7nen", "auf", "lang", "wie\u00b7der", "den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ART", "NN", "APPR", "ADJD", "ADV", "ART"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Magen verdarb.", "tokens": ["Ma\u00b7gen", "ver\u00b7darb", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}