{"textgrid.poem.61198": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: O h\u00fcte dich, zu spielen mit dem Schwert!", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O h\u00fcte dich, zu spielen mit dem Schwert!", "tokens": ["O", "h\u00fc\u00b7te", "dich", ",", "zu", "spie\u00b7len", "mit", "dem", "Schwert", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "PTKZU", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein D\u00e4mon wohnt, ein feindlicher, im Eisen;", "tokens": ["Ein", "D\u00e4\u00b7mon", "wohnt", ",", "ein", "feind\u00b7li\u00b7cher", ",", "im", "Ei\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "ADJA", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du wei\u00dft nicht, l\u00e4ssest du es leuchtend kreisen,", "tokens": ["Du", "wei\u00dft", "nicht", ",", "l\u00e4s\u00b7sest", "du", "es", "leuch\u00b7tend", "krei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "VVFIN", "PPER", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ob's nicht in deines Freundes Busen f\u00e4hrt.", "tokens": ["Ob's", "nicht", "in", "dei\u00b7nes", "Freun\u00b7des", "Bu\u00b7sen", "f\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "APPR", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "U