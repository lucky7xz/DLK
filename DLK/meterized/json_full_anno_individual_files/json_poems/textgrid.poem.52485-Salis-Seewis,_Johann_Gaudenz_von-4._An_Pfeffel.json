{"textgrid.poem.52485": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "4. An Pfeffel", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schweigen, Pfeffel! kann ich itzt nicht l\u00e4nger!", "tokens": ["Schwei\u00b7gen", ",", "Pfef\u00b7fel", "!", "kann", "ich", "itzt", "nicht", "l\u00e4n\u00b7ger", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$.", "VMFIN", "PPER", "ADV", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mein Gef\u00fchl ergie\u00dft sich in Gesang:", "tokens": ["Mein", "Ge\u00b7f\u00fchl", "er\u00b7gie\u00dft", "sich", "in", "Ge\u00b7sang", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Zwar noch sch\u00fcchtern r\u00fchrt der junge S\u00e4nger", "tokens": ["Zwar", "noch", "sch\u00fcch\u00b7tern", "r\u00fchrt", "der", "jun\u00b7ge", "S\u00e4n\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVINF", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Seine Harfe, bebend, leis' und bang.", "tokens": ["Sei\u00b7ne", "Har\u00b7fe", ",", "be\u00b7bend", ",", "leis'", "und", "bang", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVPP", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Doch, wer liebt die J\u00fcnglinge wie du?", "tokens": ["Doch", ",", "wer", "liebt", "die", "J\u00fcng\u00b7lin\u00b7ge", "wie", "du", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWS", "VVFIN", "ART", "NN", "KOKOM", "PPER", "$."], "meter": "--+-++-++", "measure": "anapaest.init"}, "line.6": {"text": "Du vergiebst und l\u00e4chelst Mut mir zu.", "tokens": ["Du", "ver\u00b7giebst", "und", "l\u00e4\u00b7chelst", "Mut", "mir", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Unverge\u00dflich bleibt mir jene Stunde,", "tokens": ["Un\u00b7ver\u00b7ge\u00df\u00b7lich", "bleibt", "mir", "je\u00b7ne", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Da ich staunend dir zur Seite sa\u00df;", "tokens": ["Da", "ich", "stau\u00b7nend", "dir", "zur", "Sei\u00b7te", "sa\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Trunken hing mein Haupt an deinem Munde,", "tokens": ["Trun\u00b7ken", "hing", "mein", "Haupt", "an", "dei\u00b7nem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und in deinen offnen Z\u00fcgen las", "tokens": ["Und", "in", "dei\u00b7nen", "off\u00b7nen", "Z\u00fc\u00b7gen", "las"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Ich entz\u00fcckt der reinsten Tugend Gl\u00fcck \u2013", "tokens": ["Ich", "ent\u00b7z\u00fcckt", "der", "reins\u00b7ten", "Tu\u00b7gend", "Gl\u00fcck", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Ach! Warum nicht auch in deinem Blick?", "tokens": ["Ach", "!", "Wa\u00b7rum", "nicht", "auch", "in", "dei\u00b7nem", "Blick", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Murren will ich nicht, ich will nicht klagen,", "tokens": ["Mur\u00b7ren", "will", "ich", "nicht", ",", "ich", "will", "nicht", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PTKNEG", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schmelzt gleich stille Wehmut mein Gef\u00fchl:", "tokens": ["Schmelzt", "gleich", "stil\u00b7le", "Weh\u00b7mut", "mein", "Ge\u00b7f\u00fchl", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Hoher Mut ward dir in tr\u00fcben Tagen \u2013", "tokens": ["Ho\u00b7her", "Mut", "ward", "dir", "in", "tr\u00fc\u00b7ben", "Ta\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPER", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und wie herrlich schimmert dir das Ziel!", "tokens": ["Und", "wie", "herr\u00b7lich", "schim\u00b7mert", "dir", "das", "Ziel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADJD", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Himmelsfriede, Heiterkeit und Ruh'", "tokens": ["Him\u00b7mels\u00b7frie\u00b7de", ",", "Hei\u00b7ter\u00b7keit", "und", "Ruh'"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Str\u00f6met dir aus deinen Thaten zu.", "tokens": ["Str\u00f6\u00b7met", "dir", "aus", "dei\u00b7nen", "Tha\u00b7ten", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Zwar die Erde scheint dir eine H\u00f6hle,", "tokens": ["Zwar", "die", "Er\u00b7de", "scheint", "dir", "ei\u00b7ne", "H\u00f6h\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Voll von Nacht, durch die kein Schimmer bricht;", "tokens": ["Voll", "von", "Nacht", ",", "durch", "die", "kein", "Schim\u00b7mer", "bricht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$,", "APPR", "ART", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Aber ewig gl\u00e4nzt um deine Seele,", "tokens": ["A\u00b7ber", "e\u00b7wig", "gl\u00e4nzt", "um", "dei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Hell und hehr der heitern Weisheit Licht;", "tokens": ["Hell", "und", "hehr", "der", "hei\u00b7tern", "Weis\u00b7heit", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADV", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Und die Freuden, die du hier entbehrt,", "tokens": ["Und", "die", "Freu\u00b7den", ",", "die", "du", "hier", "ent\u00b7behrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Werden einst dir tausendfach gew\u00e4hrt.", "tokens": ["Wer\u00b7den", "einst", "dir", "tau\u00b7send\u00b7fach", "ge\u00b7w\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Knaben, die du durch das Pilgerleben", "tokens": ["Kna\u00b7ben", ",", "die", "du", "durch", "das", "Pil\u00b7ger\u00b7le\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Zu der Tugend Strahlenziel gef\u00fchrt,", "tokens": ["Zu", "der", "Tu\u00b7gend", "Strah\u00b7len\u00b7ziel", "ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Werden zu dir dringen, dich umgeben,", "tokens": ["Wer\u00b7den", "zu", "dir", "drin\u00b7gen", ",", "dich", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "VVFIN", "$,", "PPER", "VVPP", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Aus der Palme, die den Sieger ziert,", "tokens": ["Aus", "der", "Pal\u00b7me", ",", "die", "den", "Sie\u00b7ger", "ziert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Einen Kranz dir winden, der dein Haupt", "tokens": ["Ei\u00b7nen", "Kranz", "dir", "win\u00b7den", ",", "der", "dein", "Haupt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "VVINF", "$,", "PRELS", "PPOSAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Ewig frisch und unverwelkt umlaubt.", "tokens": ["E\u00b7wig", "frisch", "und", "un\u00b7ver\u00b7welkt", "um\u00b7laubt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Sieh! des Auferstandnen Aug' entsinket", "tokens": ["Sieh", "!", "des", "Auf\u00b7er\u00b7stand\u00b7nen", "Aug'", "ent\u00b7sin\u00b7ket"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "ART", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Einst der dichte Schleier um ihn her:", "tokens": ["Einst", "der", "dich\u00b7te", "Schlei\u00b7er", "um", "ihn", "her", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Neue Erden sieht er bl\u00fchen; trinket", "tokens": ["Neu\u00b7e", "Er\u00b7den", "sieht", "er", "bl\u00fc\u00b7hen", ";", "trin\u00b7ket"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "VVFIN", "$.", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Aus der neuen Sonne Strahlenmeer \u2013", "tokens": ["Aus", "der", "neu\u00b7en", "Son\u00b7ne", "Strah\u00b7len\u00b7meer", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "O! dann senkst du den gest\u00e4rkten Blick,", "tokens": ["O", "!", "dann", "senkst", "du", "den", "ge\u00b7st\u00e4rk\u00b7ten", "Blick", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Neuverkl\u00e4rter! auch auf mich zur\u00fcck.", "tokens": ["Neu\u00b7ver\u00b7kl\u00e4r\u00b7ter", "!", "auch", "auf", "mich", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Denk' ich diesem Wonnetag entgegen,", "tokens": ["Denk'", "ich", "die\u00b7sem", "Won\u00b7ne\u00b7tag", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PDAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "J\u00fcnglings-Vater, S\u00e4nger, edler Mann!", "tokens": ["J\u00fcng\u00b7lings\u00b7Va\u00b7ter", ",", "S\u00e4n\u00b7ger", ",", "ed\u00b7ler", "Mann", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "O dann klopft mein Herz, mit lauten Schl\u00e4gen,", "tokens": ["O", "dann", "klopft", "mein", "Herz", ",", "mit", "lau\u00b7ten", "Schl\u00e4\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVFIN", "PPOSAT", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und die Z\u00e4hre rinnet, wie sie rann,", "tokens": ["Und", "die", "Z\u00e4h\u00b7re", "rin\u00b7net", ",", "wie", "sie", "rann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Als ich stumm und bebend an dir hing", "tokens": ["Als", "ich", "stumm", "und", "be\u00b7bend", "an", "dir", "hing"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "KON", "ADJD", "APPR", "PPER", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Und den letzten Abschiedsku\u00df empfing.", "tokens": ["Und", "den", "letz\u00b7ten", "Ab\u00b7schieds\u00b7ku\u00df", "emp\u00b7fing", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Schweigen, Pfeffel! kann ich itzt nicht l\u00e4nger!", "tokens": ["Schwei\u00b7gen", ",", "Pfef\u00b7fel", "!", "kann", "ich", "itzt", "nicht", "l\u00e4n\u00b7ger", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$.", "VMFIN", "PPER", "ADV", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mein Gef\u00fchl ergie\u00dft sich in Gesang:", "tokens": ["Mein", "Ge\u00b7f\u00fchl", "er\u00b7gie\u00dft", "sich", "in", "Ge\u00b7sang", ":"], "token_info": ["word", "