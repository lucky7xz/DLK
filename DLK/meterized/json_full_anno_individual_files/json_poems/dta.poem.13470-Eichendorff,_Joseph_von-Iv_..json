{"dta.poem.13470": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Iv .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Bist du manchmal auch verstimmt,", "tokens": ["Bist", "du", "manch\u00b7mal", "auch", "ver\u00b7stimmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dr\u00fcck' dich z\u00e4rtlich an mein Herze,", "tokens": ["Dr\u00fcck", "dich", "z\u00e4rt\u00b7lich", "an", "mein", "Her\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "APPR", "PPOSAT", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df mir's fast den Athem nimmt,", "tokens": ["Da\u00df", "mir's", "fast", "den", "A\u00b7them", "nimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Streich' und kneif' in s\u00fc\u00dfem Scherze,", "tokens": ["Streich'", "und", "kneif'", "in", "s\u00fc\u00b7\u00dfem", "Scher\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie ein rechter Liebes-Thor,", "tokens": ["Wie", "ein", "rech\u00b7ter", "Lie\u00b7bes\u00b7Thor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Lehn' ich sanft an dich die Wange", "tokens": ["Lehn'", "ich", "sanft", "an", "dich", "die", "Wan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADJD", "APPR", "PRF", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und du singst mir fein ins Ohr.", "tokens": ["Und", "du", "singst", "mir", "fein", "ins", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wohl im Hofe bei dem Klange", "tokens": ["Wohl", "im", "Ho\u00b7fe", "bei", "dem", "Klan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Katze miaut, Hund heult und bellt,", "tokens": ["Kat\u00b7ze", "mi\u00b7aut", ",", "Hund", "heult", "und", "bellt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+---+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Nachbar schimpft mit wilder Miene \u2014", "tokens": ["Nach\u00b7bar", "schimpft", "mit", "wil\u00b7der", "Mie\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch was k\u00fcmmert uns die Welt,", "tokens": ["Doch", "was", "k\u00fcm\u00b7mert", "uns", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "S\u00fc\u00dfe, traute Violine!", "tokens": ["S\u00fc\u00b7\u00dfe", ",", "trau\u00b7te", "Vi\u00b7o\u00b7li\u00b7ne", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}