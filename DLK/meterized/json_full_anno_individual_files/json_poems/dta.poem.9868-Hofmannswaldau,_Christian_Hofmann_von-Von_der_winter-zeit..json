{"dta.poem.9868": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Von der winter-zeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Soll ich des winters art nicht lieben/", "tokens": ["Soll", "ich", "des", "win\u00b7ters", "art", "nicht", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Weil er des jahres alter heist/", "tokens": ["Weil", "er", "des", "jah\u00b7res", "al\u00b7ter", "heist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der g\u00e4rte zierrath nieder reist/", "tokens": ["Der", "g\u00e4r\u00b7te", "zier\u00b7rath", "nie\u00b7der", "reist", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn er der felder lust vertrieben/", "tokens": ["Wenn", "er", "der", "fel\u00b7der", "lust", "ver\u00b7trie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Als ein unangenehmer grei\u00df/", "tokens": ["Als", "ein", "un\u00b7an\u00b7ge\u00b7neh\u00b7mer", "grei\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Verkleidet liegt in schnee und ei\u00df?", "tokens": ["Ver\u00b7klei\u00b7det", "liegt", "in", "schnee", "und", "ei\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "APPR", "ADJA", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der winter macht aus wasser br\u00fccken/", "tokens": ["Der", "win\u00b7ter", "macht", "aus", "was\u00b7ser", "br\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf welchen ich kan sicher stehn/", "tokens": ["Auf", "wel\u00b7chen", "ich", "kan", "si\u00b7cher", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "PPER", "VMFIN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf welchen pferd und wagen gehn/", "tokens": ["Auf", "wel\u00b7chen", "pferd", "und", "wa\u00b7gen", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "KON", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der winter heilt mir brust und r\u00fccken/", "tokens": ["Der", "win\u00b7ter", "heilt", "mir", "brust", "und", "r\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVFIN", "KON", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn mir des ofens liebligkeit", "tokens": ["Wenn", "mir", "des", "o\u00b7fens", "lieb\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Macht in der stube sommer-zeit.", "tokens": ["Macht", "in", "der", "stu\u00b7be", "som\u00b7mer\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Jm winter hab ich zu geniessen/", "tokens": ["Jm", "win\u00b7ter", "hab", "ich", "zu", "ge\u00b7nies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was mir der k\u00fchne j\u00e4ger bringt/", "tokens": ["Was", "mir", "der", "k\u00fch\u00b7ne", "j\u00e4\u00b7ger", "bringt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was nicht mehr in dem winter singt/", "tokens": ["Was", "nicht", "mehr", "in", "dem", "win\u00b7ter", "singt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "ADV", "APPR", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kan in den magen sich verschliessen/", "tokens": ["Kan", "in", "den", "ma\u00b7gen", "sich", "ver\u00b7schlies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "ADJA", "PRF", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Des winters k\u00e4lte machet hitz", "tokens": ["Des", "win\u00b7ters", "k\u00e4l\u00b7te", "ma\u00b7chet", "hitz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In der verborgnen liebe sitz.", "tokens": ["In", "der", "ver\u00b7borg\u00b7nen", "lie\u00b7be", "sitz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Jm winter k\u00f6nnen wir die sternen", "tokens": ["Jm", "win\u00b7ter", "k\u00f6n\u00b7nen", "wir", "die", "ster\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "VMFIN", "PPER", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Viel besser als im sommer sehn/", "tokens": ["Viel", "bes\u00b7ser", "als", "im", "som\u00b7mer", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ist im winter nicht geschehn/", "tokens": ["Und", "ist", "im", "win\u00b7ter", "nicht", "ge\u00b7schehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPRART", "ADJA", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was der ehristmonat giebt zu lernen?", "tokens": ["Was", "der", "e\u00b7hrist\u00b7mo\u00b7nat", "giebt", "zu", "ler\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Als du stiegst von des himmels thron/", "tokens": ["Als", "du", "stiegst", "von", "des", "him\u00b7mels", "thron", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "NE", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Und wardst ein mensch/ o GOttes sohn!", "tokens": ["Und", "wardst", "ein", "mensch", "/", "o", "Got\u00b7tes", "sohn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "$(", "FM", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Uns haben dein reich vorzumahlen/", "tokens": ["Uns", "ha\u00b7ben", "dein", "reich", "vor\u00b7zu\u00b7mah\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df in der gnaden-reichen zeit", "tokens": ["Da\u00df", "in", "der", "gna\u00b7den\u00b7rei\u00b7chen", "zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Uns schenckt die lust der ewigkeit/", "tokens": ["Uns", "schenckt", "die", "lust", "der", "e\u00b7wig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O JEsu! deine klare strahlen/", "tokens": ["O", "Je\u00b7su", "!", "dei\u00b7ne", "kla\u00b7re", "strah\u00b7len", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Den sch\u00f6nsten tag aus schwartzer nacht/", "tokens": ["Den", "sch\u00f6ns\u00b7ten", "tag", "aus", "schwart\u00b7zer", "nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den winter zu dem lentz gemacht.", "tokens": ["Den", "win\u00b7ter", "zu", "dem", "lentz", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}