{"textgrid.poem.32563": {"metadata": {"author": {"name": "Wieland, Christoph Martin", "birth": "N.A.", "death": "N.A."}, "title": "Ode an Herrn Bodmer", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der die Seelen einst schuf, weich zu Empfindungen", "tokens": ["Der", "die", "See\u00b7len", "einst", "schuf", ",", "weich", "zu", "Emp\u00b7fin\u00b7dun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ART", "NN", "ADV", "VVFIN", "$,", "ADJD", "APPR", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Und unendlicher Triebe voll,", "tokens": ["Und", "un\u00b7end\u00b7li\u00b7cher", "Trie\u00b7be", "voll", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJD", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Gab schon damals ins Herz jeder Erhabneren", "tokens": ["Gab", "schon", "da\u00b7mals", "ins", "Herz", "je\u00b7der", "Er\u00b7hab\u00b7ne\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "APPRART", "NN", "PIAT", "NN"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Ihrer Freundinnen dunkles Bild.", "tokens": ["Ih\u00b7rer", "Freun\u00b7din\u00b7nen", "dunk\u00b7les", "Bild", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Damals schuf er den Trieb, welchen ein edles Herz", "tokens": ["Da\u00b7mals", "schuf", "er", "den", "Trieb", ",", "wel\u00b7chen", "ein", "ed\u00b7les", "Herz"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "PWAT", "ART", "ADJA", "NN"], "meter": "--+--+---+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Staunend vor den Geliebten f\u00fchlt,", "tokens": ["Stau\u00b7nend", "vor", "den", "Ge\u00b7lieb\u00b7ten", "f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Den ein trennendes Land oder Jahrhunderte", "tokens": ["Den", "ein", "tren\u00b7nen\u00b7des", "Land", "o\u00b7der", "Jahr\u00b7hun\u00b7der\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN", "KON", "NN"], "meter": "+-+--+--++-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Seinem z\u00e4rtlichen Arm entziehn.", "tokens": ["Sei\u00b7nem", "z\u00e4rt\u00b7li\u00b7chen", "Arm", "ent\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "In den Schlummern der Nacht, wenn sich die Zukunft oft", "tokens": ["In", "den", "Schlum\u00b7mern", "der", "Nacht", ",", "wenn", "sich", "die", "Zu\u00b7kunft", "oft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "KOUS", "PRF", "ART", "NN", "ADV"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Im weissagenden Geist enth\u00fcllt,", "tokens": ["Im", "weis\u00b7sa\u00b7gen\u00b7den", "Geist", "ent\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Stieg vom goldnen Olymp oft das geliebte Bild", "tokens": ["Stieg", "vom", "gold\u00b7nen", "O\u00b7lymp", "oft", "das", "ge\u00b7lieb\u00b7te", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "ADJA", "NN", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Meiner Freundin vor meinen Blick.", "tokens": ["Mei\u00b7ner", "Freun\u00b7din", "vor", "mei\u00b7nen", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+----+-+", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Lang noch eh ich sie sah, und mich ihr g\u00f6ttlich Herz", "tokens": ["Lang", "noch", "eh", "ich", "sie", "sah", ",", "und", "mich", "ihr", "g\u00f6tt\u00b7lich", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "KOUS", "PPER", "PPER", "VVFIN", "$,", "KON", "PPER", "PPER", "ADJD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Fremd und himmlisch empfinden lie\u00df;", "tokens": ["Fremd", "und", "himm\u00b7lisch", "emp\u00b7fin\u00b7den", "lie\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVINF", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Als die Muse mich noch, Elbe, an deinem Schilf,", "tokens": ["Als", "die", "Mu\u00b7se", "mich", "noch", ",", "El\u00b7be", ",", "an", "dei\u00b7nem", "Schilf", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "ADV", "$,", "NE", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Oder irrend in Hainen fand.", "tokens": ["O\u00b7der", "ir\u00b7rend", "in", "Hai\u00b7nen", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "APPR", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "Wenn der kommende Lenz in mein sanft wallend Herz", "tokens": ["Wenn", "der", "kom\u00b7men\u00b7de", "Lenz", "in", "mein", "sanft", "wal\u00b7lend", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJD", "VVPP", "NN"], "meter": "+-+--+-+++-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Neue dichtrische Freuden go\u00df;", "tokens": ["Neu\u00b7e", "dich\u00b7tri\u00b7sche", "Freu\u00b7den", "go\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "F\u00fchlt ich klopfend in mir einen geheimen Wunsch,", "tokens": ["F\u00fchlt", "ich", "klop\u00b7fend", "in", "mir", "ei\u00b7nen", "ge\u00b7hei\u00b7men", "Wunsch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Unbefriedigt und still beweint.", "tokens": ["Un\u00b7be\u00b7frie\u00b7digt", "und", "still", "be\u00b7weint", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.6": {"line.1": {"text": "Oft am silbernen Flu\u00df oder vom Weidenwald", "tokens": ["Oft", "am", "sil\u00b7ber\u00b7nen", "Flu\u00df", "o\u00b7der", "vom", "Wei\u00b7den\u00b7wald"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN", "KON", "APPRART", "NN"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Sanft vom Zephyr herbeigef\u00fchrt,", "tokens": ["Sanft", "vom", "Ze\u00b7phyr", "her\u00b7bei\u00b7ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "H\u00f6rt ich dann einen Laut, der mich zu rufen schien,", "tokens": ["H\u00f6rt", "ich", "dann", "ei\u00b7nen", "Laut", ",", "der", "mich", "zu", "ru\u00b7fen", "schien", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Oder Seufzer der Z\u00e4rtlichkeit.", "tokens": ["O\u00b7der", "Seuf\u00b7zer", "der", "Z\u00e4rt\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.7": {"line.1": {"text": "Die ihr, Freunde, mit mir gro\u00df und harmonisch denkt,", "tokens": ["Die", "ihr", ",", "Freun\u00b7de", ",", "mit", "mir", "gro\u00df", "und", "har\u00b7mo\u00b7nisch", "denkt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "NN", "$,", "APPR", "PPER", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.2": {"text": "Du, o g\u00f6ttlicher Sokrates!", "tokens": ["Du", ",", "o", "g\u00f6tt\u00b7li\u00b7cher", "Sok\u00b7ra\u00b7tes", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "FM", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Du, geliebter Horaz, und du mein Plinius,", "tokens": ["Du", ",", "ge\u00b7lieb\u00b7ter", "Ho\u00b7raz", ",", "und", "du", "mein", "Pli\u00b7nius", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$,", "KON", "PPER", "PPOSAT", "NE", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "O wie oft schlug mein Herz f\u00fcr euch?", "tokens": ["O", "wie", "oft", "schlug", "mein", "Herz", "f\u00fcr", "euch", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADV", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.8": {"line.1": {"text": "Bodmer, der Du vom Pind l\u00e4chelnd herunter siehst,", "tokens": ["Bod\u00b7mer", ",", "der", "Du", "vom", "Pind", "l\u00e4\u00b7chelnd", "her\u00b7un\u00b7ter", "siehst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "APPRART", "NN", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "+--+-++--+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Mit homerischem Laub bekr\u00e4nzt,", "tokens": ["Mit", "ho\u00b7me\u00b7ri\u00b7schem", "Laub", "be\u00b7kr\u00e4nzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn, vom Satyr verfolgt, sich an des Berges Fu\u00df", "tokens": ["Wenn", ",", "vom", "Sa\u00b7tyr", "ver\u00b7folgt", ",", "sich", "an", "des", "Ber\u00b7ges", "Fu\u00df"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "APPRART", "NN", "VVPP", "$,", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Der barbarische Schwarm erm\u00fcdt.", "tokens": ["Der", "bar\u00b7ba\u00b7ri\u00b7sche", "Schwarm", "er\u00b7m\u00fcdt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Ja, ich wag's, was ich stets heimlich vor Dich empfand,", "tokens": ["Ja", ",", "ich", "wag's", ",", "was", "ich", "stets", "heim\u00b7lich", "vor", "Dich", "emp\u00b7fand", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "$,", "PWS", "PPER", "ADV", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Auf Dein freies erhabnes Herz", "tokens": ["Auf", "Dein", "frei\u00b7es", "er\u00b7hab\u00b7nes", "Herz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Wag ich's, nenne Dich Freund, z\u00e4hl Dich den Weisen bei,", "tokens": ["Wag", "ich's", ",", "nen\u00b7ne", "Dich", "Freund", ",", "z\u00e4hl", "Dich", "den", "Wei\u00b7sen", "bei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "$,", "VVFIN", "PPER", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.4": {"text": "Deren Namen mir heilig sind.", "tokens": ["De\u00b7ren", "Na\u00b7men", "mir", "hei\u00b7lig", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "PPER", "ADJD", "VAFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.10": {"line.1": {"text": "In der z\u00e4rtlichen Brust, die mir der Himmel gab,", "tokens": ["In", "der", "z\u00e4rt\u00b7li\u00b7chen", "Brust", ",", "die", "mir", "der", "Him\u00b7mel", "gab", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Sind die edelsten Triebe Dein;", "tokens": ["Sind", "die", "e\u00b7dels\u00b7ten", "Trie\u00b7be", "Dein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "PPOSAT", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Dein, und Langen, und Dir, lehrender Breitinger,", "tokens": ["Dein", ",", "und", "Lan\u00b7gen", ",", "und", "Dir", ",", "leh\u00b7ren\u00b7der", "Brei\u00b7tin\u00b7ger", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "$,", "KON", "NN", "$,", "KON", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Und dem, der den Messias singt.", "tokens": ["Und", "dem", ",", "der", "den", "Mes\u00b7si\u00b7as", "singt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.11": {"line.1": {"text": "O! wie selig, wenn einst mein noch umw\u00f6lkt Geschick", "tokens": ["O", "!", "wie", "se\u00b7lig", ",", "wenn", "einst", "mein", "noch", "um\u00b7w\u00f6lkt", "Ge\u00b7schick"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "PWAV", "ADJD", "$,", "KOUS", "ADV", "PPOSAT", "ADV", "VVFIN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Eurem Blick mich entgegen bringt!", "tokens": ["Eu\u00b7rem", "Blick", "mich", "ent\u00b7ge\u00b7gen", "bringt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "PTKVZ", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Kaum empfandest du mehr, Klopstock, da du zuerst", "tokens": ["Kaum", "emp\u00b7fan\u00b7dest", "du", "mehr", ",", "Klops\u00b7tock", ",", "da", "du", "zu\u00b7erst"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "NN", "$,", "KOUS", "PPER", "ADV"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Bodmers Armen entgegen kamst.", "tokens": ["Bod\u00b7mers", "Ar\u00b7men", "ent\u00b7ge\u00b7gen", "kamst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PTKVZ", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.12": {"line.1": {"text": "Der die Seelen einst schuf, weich zu Empfindungen", "tokens": ["Der", "die", "See\u00b7len", "einst", "schuf", ",", "weich", "zu", "Emp\u00b7fin\u00b7dun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ART", "NN", "ADV", "VVFIN", "$,", "ADJD", "APPR", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Und unendlicher Triebe voll,", "tokens": ["Und", "un\u00b7end\u00b7li\u00b7cher", "Trie\u00b7be", "voll", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJD", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Gab schon damals ins Herz jeder Erhabneren", "tokens": ["Gab", "schon", "da\u00b7mals", "ins", "Herz", "je\u00b7der", "Er\u00b7hab\u00b7ne\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "APPRART", "NN", "PIAT", "NN"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Ihrer Freundinnen dunkles Bild.", "tokens": ["Ih\u00b7rer", "Freun\u00b7din\u00b7nen", "dunk\u00b7les", "Bild", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.13": {"line.1": {"text": "Damals sc