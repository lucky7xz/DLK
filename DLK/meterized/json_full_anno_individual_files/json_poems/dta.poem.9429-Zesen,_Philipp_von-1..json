{"dta.poem.9429": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie? h\u00f6r\u2019 ich den Liebsten nicht ruffen und singen?", "tokens": ["Wie", "?", "h\u00f6r'", "ich", "den", "Liebs\u00b7ten", "nicht", "ruf\u00b7fen", "und", "sin\u00b7gen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VVFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF", "KON", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Er nahet/ ich h\u00f6re den lieblichen klang/", "tokens": ["Er", "na\u00b7het", "/", "ich", "h\u00f6\u00b7re", "den", "lieb\u00b7li\u00b7chen", "klang", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VVFIN", "ART", "ADJA", "VVFIN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Er k\u00f6mmet und h\u00fcpffet mit freudigen springen/", "tokens": ["Er", "k\u00f6m\u00b7met", "und", "h\u00fcpf\u00b7fet", "mit", "freu\u00b7di\u00b7gen", "sprin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "ADJA", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Die H\u00fcgel und Th\u00e4ler empfinden den gang.", "tokens": ["Die", "H\u00fc\u00b7gel", "und", "Th\u00e4\u00b7ler", "emp\u00b7fin\u00b7den", "den", "gang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Mein Liebster sich zeiget/", "tokens": ["Mein", "Liebs\u00b7ter", "sich", "zei\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "VVFIN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Die Berge naufsteiget/", "tokens": ["Die", "Ber\u00b7ge", "nauf\u00b7stei\u00b7get", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Die m\u00e4chtige h\u00f6h;", "tokens": ["Die", "m\u00e4ch\u00b7ti\u00b7ge", "h\u00f6h", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Der Gemse sich gleichet/", "tokens": ["Der", "Gem\u00b7se", "sich", "glei\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVFIN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Den Hirschen nicht weichet/", "tokens": ["Den", "Hir\u00b7schen", "nicht", "wei\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVFIN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "An hurtigkeit weichet jhm selbsten bas Reh.", "tokens": ["An", "hur\u00b7tig\u00b7keit", "wei\u00b7chet", "jhm", "selbs\u00b7ten", "bas", "Reh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "VVFIN", "NE", "NE", "$."], "meter": "+---+--+--+", "measure": "trochaic.tetra.relaxed"}}}}}