{"textgrid.poem.46248": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Die lieb ist leben und tod", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das leben, so ich f\u00fchr, ist wie der wahre tod,", "tokens": ["Das", "le\u00b7ben", ",", "so", "ich", "f\u00fchr", ",", "ist", "wie", "der", "wah\u00b7re", "tod", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ADV", "PPER", "VVFIN", "$,", "VAFIN", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ja \u00fcber den tod selbs ist mein trostloses leben:", "tokens": ["ja", "\u00fc\u00b7ber", "den", "tod", "selbs", "ist", "mein", "trost\u00b7lo\u00b7ses", "le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "VAFIN", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "es endet ja der tod des menschen pein und leben,", "tokens": ["es", "en\u00b7det", "ja", "der", "tod", "des", "men\u00b7schen", "pein", "und", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ART", "ADJA", "NN", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "mein leben aber kan nicht enden diser tod.", "tokens": ["mein", "le\u00b7ben", "a\u00b7ber", "kan", "nicht", "en\u00b7den", "di\u00b7ser", "tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "ADV", "VMFIN", "PTKNEG", "VVINF", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bald kan ein anblick mich verletzen auf den tod,", "tokens": ["Bald", "kan", "ein", "an\u00b7blick", "mich", "ver\u00b7let\u00b7zen", "auf", "den", "tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "ein andrer anblick bald kan mich widrum beleben,", "tokens": ["ein", "an\u00b7drer", "an\u00b7blick", "bald", "kan", "mich", "wid\u00b7rum", "be\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "da\u00df ich von blicken mu\u00df dan sterben und dan leben", "tokens": ["da\u00df", "ich", "von", "bli\u00b7cken", "mu\u00df", "dan", "ster\u00b7ben", "und", "dan", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "VVINF", "VMFIN", "ADV", "VVINF", "KON", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "und bin in einer stund bald lebendig, bald tod.", "tokens": ["und", "bin", "in", "ei\u00b7ner", "stund", "bald", "le\u00b7ben\u00b7dig", ",", "bald", "tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "ART", "NN", "ADV", "ADJD", "$,", "ADV", "NN", "$."], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}, "line.9": {"text": "Ach lieb! verleih mir doch numehr ein andres leben,", "tokens": ["Ach", "lieb", "!", "ver\u00b7leih", "mir", "doch", "nu\u00b7mehr", "ein", "and\u00b7res", "le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJD", "$.", "VVIMP", "PPER", "ADV", "ADV", "ART", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "wan ich ja leben soll, oder den andern tod,", "tokens": ["wan", "ich", "ja", "le\u00b7ben", "soll", ",", "o\u00b7der", "den", "an\u00b7dern", "tod", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVINF", "VMFIN", "$,", "KON", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.11": {"text": "dan weder disen tod lieb ich, noch dises leben.", "tokens": ["dan", "we\u00b7der", "di\u00b7sen", "tod", "lieb", "ich", ",", "noch", "di\u00b7ses", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "PDAT", "NN", "ADJD", "PPER", "$,", "ADV", "PDS", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.12": {"text": "Verzeih mir, lieb, ich bin dein lebendig und tod,", "tokens": ["Ver\u00b7zeih", "mir", ",", "lieb", ",", "ich", "bin", "dein", "le\u00b7ben\u00b7dig", "und", "tod", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "ADJD", "$,", "PPER", "VAFIN", "PPOSAT", "ADJD", "KON", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "und ist der tod mit dir ein k\u00f6stlich s\u00fc\u00dfes leben", "tokens": ["und", "ist", "der", "tod", "mit", "dir", "ein", "k\u00f6st\u00b7lich", "s\u00fc\u00b7\u00dfes", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NN", "APPR", "PPER", "ART", "ADJD", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "und leben von dir fern ist ein ganz bittrer tod.", "tokens": ["und", "le\u00b7ben", "von", "dir", "fern", "ist", "ein", "ganz", "bit\u00b7trer", "tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ADJD", "VAFIN", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das leben, so ich f\u00fchr, ist wie der wahre tod,", "tokens": ["Das", "le\u00b7ben", ",", "so", "ich", "f\u00fchr", ",", "ist", "wie", "der", "wah\u00b7re", "tod", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ADV", "PPER", "VVFIN", "$,", "VAFIN", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ja \u00fcber den tod selbs ist mein trostloses leben:", "tokens": ["ja", "\u00fc\u00b7ber", "den", "tod", "selbs", "ist", "mein", "trost\u00b7lo\u00b7ses", "le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "VAFIN", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "es endet ja der tod des menschen pein und leben,", "tokens": ["es", "en\u00b7det", "ja", "der", "tod", "des", "men\u00b7schen", "pein", "und", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ART", "ADJA", "NN", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "mein leben aber kan nicht enden diser tod.", "tokens": ["mein", "le\u00b7ben", "