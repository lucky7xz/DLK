{"dta.poem.17508": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "129.  Das jnnere bedarf Nicht de\u00df \u00e4useren.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Wer seine Sinnen hat ins jnnere gebracht/", "tokens": ["Wer", "sei\u00b7ne", "Sin\u00b7nen", "hat", "ins", "jn\u00b7ne\u00b7re", "ge\u00b7bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VAFIN", "APPRART", "ADJA", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der h\u00f6rt was man nicht redt/ und siehet in der Nacht.", "tokens": ["Der", "h\u00f6rt", "was", "man", "nicht", "redt", "/", "und", "sie\u00b7het", "in", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PWS", "PIS", "PTKNEG", "VVFIN", "$(", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}