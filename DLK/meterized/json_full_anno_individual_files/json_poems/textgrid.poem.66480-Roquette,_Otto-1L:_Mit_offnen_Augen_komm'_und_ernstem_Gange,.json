{"textgrid.poem.66480": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mit offnen Augen komm' und ernstem Gange,", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit offnen Augen komm' und ernstem Gange,", "tokens": ["Mit", "off\u00b7nen", "Au\u00b7gen", "komm'", "und", "erns\u00b7tem", "Gan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fchlst du zum Dienst der Musen dich getrieben!", "tokens": ["F\u00fchlst", "du", "zum", "Dienst", "der", "Mu\u00b7sen", "dich", "ge\u00b7trie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Frei sei dein Innerstes von fremdem Drange,", "tokens": ["Frei", "sei", "dein", "In\u00b7ners\u00b7tes", "von", "frem\u00b7dem", "Dran\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Denn h\u00f6chstem Wirken gilt ein h\u00f6chstes Lieben.", "tokens": ["Denn", "h\u00f6chs\u00b7tem", "Wir\u00b7ken", "gilt", "ein", "h\u00f6chs\u00b7tes", "Lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Den Schauer auch empfind' um Brust und Wange", "tokens": ["Den", "Schau\u00b7er", "auch", "emp\u00b7find'", "um", "Brust", "und", "Wan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Von banger Scheu, ob w\u00fcrdig du geblieben,", "tokens": ["Von", "ban\u00b7ger", "Scheu", ",", "ob", "w\u00fcr\u00b7dig", "du", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "KOUS", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Im Augenblicke, da dich hochbegnadet", "tokens": ["Im", "Au\u00b7gen\u00b7bli\u00b7cke", ",", "da", "dich", "hoch\u00b7be\u00b7gna\u00b7det"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Zu ihrem Heiligthum die Dichtung ladet!", "tokens": ["Zu", "ih\u00b7rem", "Hei\u00b7lig\u00b7thum", "die", "Dich\u00b7tung", "la\u00b7det", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ob du des Waldgezeltes gr\u00fcne Schwelle", "tokens": ["Ob", "du", "des", "Wald\u00b7ge\u00b7zel\u00b7tes", "gr\u00fc\u00b7ne", "Schwel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Betrittst, in Einsamkeit dich zu bereiten;", "tokens": ["Be\u00b7trittst", ",", "in", "Ein\u00b7sam\u00b7keit", "dich", "zu", "be\u00b7rei\u00b7ten", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ob in gestirnter N\u00e4chte D\u00e4mmrungshelle", "tokens": ["Ob", "in", "ge\u00b7stirn\u00b7ter", "N\u00e4ch\u00b7te", "D\u00e4mm\u00b7rungs\u00b7hel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gedanken zu dir selbst zur\u00fcck dich leiten;", "tokens": ["Ge\u00b7dan\u00b7ken", "zu", "dir", "selbst", "zu\u00b7r\u00fcck", "dich", "lei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ADV", "PTKVZ", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ob deiner engen Welt vertraute Zelle", "tokens": ["Ob", "dei\u00b7ner", "en\u00b7gen", "Welt", "ver\u00b7trau\u00b7te", "Zel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Du hoffst den Tag unendlich zu erweiten;", "tokens": ["Du", "hoffst", "den", "Tag", "un\u00b7end\u00b7lich", "zu", "er\u00b7wei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Im Tempel f\u00fchle dich f\u00fcr dein Beginnen!", "tokens": ["Im", "Tem\u00b7pel", "f\u00fch\u00b7le", "dich", "f\u00fcr", "dein", "Be\u00b7gin\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Er wird dir's, bist du da mit reinen Sinnen.", "tokens": ["Er", "wird", "dir's", ",", "bist", "du", "da", "mit", "rei\u00b7nen", "Sin\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "$,", "VAFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Still mu\u00df es in dir sein von Sorg' und Grollen,", "tokens": ["Still", "mu\u00df", "es", "in", "dir", "sein", "von", "Sor\u00b7g'", "und", "Grol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "APPR", "PPER", "PPOSAT", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+---+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das irrend dir dein Besseres beth\u00f6rte.", "tokens": ["Das", "ir\u00b7rend", "dir", "dein", "Bes\u00b7se\u00b7res", "be\u00b7th\u00f6r\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVPP", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "La\u00df drau\u00dfen, die dich noch ber\u00fccken wollen,", "tokens": ["La\u00df", "drau\u00b7\u00dfen", ",", "die", "dich", "noch", "be\u00b7r\u00fc\u00b7cken", "wol\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PRELS", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Huldvolle Bilder, die der Tag zerst\u00f6rte!", "tokens": ["Huld\u00b7vol\u00b7le", "Bil\u00b7der", ",", "die", "der", "Tag", "zer\u00b7st\u00f6r\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Den schnell gewelkten Kranz, den bl\u00fcthenvollen,", "tokens": ["Den", "schnell", "ge\u00b7welk\u00b7ten", "Kranz", ",", "den", "bl\u00fc\u00b7then\u00b7vol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nimm ihn vom Haupt, dem gestern er geh\u00f6rte!", "tokens": ["Nimm", "ihn", "vom", "Haupt", ",", "dem", "ge\u00b7stern", "er", "ge\u00b7h\u00f6r\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPRART", "NN", "$,", "PRELS", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Tritt ein, wie einer heil'gen Pflicht verbunden,", "tokens": ["Tritt", "ein", ",", "wie", "ei\u00b7ner", "heil'\u00b7gen", "Pflicht", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PWAV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Du wei\u00dft, was drinnen immer du gefunden!", "tokens": ["Du", "wei\u00dft", ",", "was", "drin\u00b7nen", "im\u00b7mer", "du", "ge\u00b7fun\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "ADV", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dich fordert von des Heiligthumes Stufen", "tokens": ["Dich", "for\u00b7dert", "von", "des", "Hei\u00b7lig\u00b7thu\u00b7mes", "Stu\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Tag zur\u00fcck noch in die Bahn und Schranke,", "tokens": ["Der", "Tag", "zu\u00b7r\u00fcck", "noch", "in", "die", "Bahn", "und", "Schran\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "ADV", "APPR", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du bist zur Welt, und so zum Kampf berufen.", "tokens": ["Du", "bist", "zur", "Welt", ",", "und", "so", "zum", "Kampf", "be\u00b7ru\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "$,", "KON", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des B\u00f6sen Saat, gebraut zum Taumeltranke,", "tokens": ["Des", "B\u00f6\u00b7sen", "Saat", ",", "ge\u00b7braut", "zum", "Tau\u00b7mel\u00b7tran\u00b7ke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Gedeiht auch, wo die Besten Edles schufen.", "tokens": ["Ge\u00b7deiht", "auch", ",", "wo", "die", "Bes\u00b7ten", "Ed\u00b7les", "schu\u00b7fen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PWAV", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Drum l\u00e4utre sich am H\u00f6chsten dein Gedanke,", "tokens": ["Drum", "l\u00e4ut\u00b7re", "sich", "am", "H\u00f6chs\u00b7ten", "dein", "Ge\u00b7dan\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PRF", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Gest\u00e4hlt, und jedem Niedrigen enthoben,", "tokens": ["Ge\u00b7st\u00e4hlt", ",", "und", "je\u00b7dem", "Nied\u00b7ri\u00b7gen", "ent\u00b7ho\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KON", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dich selber im Zerw\u00fcrfni\u00df zu erproben!", "tokens": ["Dich", "sel\u00b7ber", "im", "Zer\u00b7w\u00fcrf\u00b7ni\u00df", "zu", "er\u00b7pro\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "So ruf' ich mahnend dir, o Seel', auch heute,", "tokens": ["So", "ruf'", "ich", "mah\u00b7nend", "dir", ",", "o", "Seel'", ",", "auch", "heu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PPER", "$,", "FM", "NN", "$,", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df, was zum Werk du immer dir erkoren,", "tokens": ["Da\u00df", ",", "was", "zum", "Werk", "du", "im\u00b7mer", "dir", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PRELS", "APPRART", "NN", "PPER", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein weihevolles M\u00fchen dir bedeute!", "tokens": ["Ein", "wei\u00b7he\u00b7vol\u00b7les", "M\u00fc\u00b7hen", "dir", "be\u00b7deu\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "La\u00df drau\u00dfen vor des Tempels Friedensthoren,", "tokens": ["La\u00df", "drau\u00b7\u00dfen", "vor", "des", "Tem\u00b7pels", "Frie\u00b7denst\u00b7ho\u00b7ren", ","], "to