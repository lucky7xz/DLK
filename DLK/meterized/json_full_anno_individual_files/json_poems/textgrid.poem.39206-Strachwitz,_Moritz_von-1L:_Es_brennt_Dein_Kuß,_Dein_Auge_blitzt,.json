7fe", "den", "Mon\u00b7dens\u00b7trahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Als wie die Glut den Brand.", "tokens": ["Als", "wie", "die", "Glut", "den", "Brand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Ich liebte sie, wie die Welt das Licht,", "tokens": ["Ich", "lieb\u00b7te", "sie", ",", "wie", "die", "Welt", "das", "Licht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und mehr noch, viel mehr noch! \u2013", "tokens": ["Und", "mehr", "noch", ",", "viel", "mehr", "noch", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "ADV", "$,", "ADV", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "H\u00f6r' an, Geliebte, und schaudre nicht:", "tokens": ["H\u00f6r'", "an", ",", "Ge\u00b7lieb\u00b7te", ",", "und", "schaud\u00b7re", "nicht", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "NN", "$,", "KON", "VVFIN", "PTKNEG", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Und \u2013 treulos ward ich doch!", "tokens": ["Und", "\u2013", "treu\u00b7los", "ward", "ich", "doch", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "ADJD", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Das ist's, was mir die Stirne tr\u00fcbt", "tokens": ["Das", "ist's", ",", "was", "mir", "die", "Stir\u00b7ne", "tr\u00fcbt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "$,", "PWS", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und st\u00f6rt die selige Ruh':", "tokens": ["Und", "st\u00f6rt", "die", "se\u00b7li\u00b7ge", "Ruh'", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Du liebst mich, wie ich ", "tokens": ["Du", "liebst", "mich", ",", "wie", "ich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Und treulos wirst auch ", "tokens": ["Und", "treu\u00b7los", "wirst", "auch"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "ADV"], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}