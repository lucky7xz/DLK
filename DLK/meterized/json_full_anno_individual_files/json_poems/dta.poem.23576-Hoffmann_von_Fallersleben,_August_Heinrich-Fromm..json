{"dta.poem.23576": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Fromm.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer an das Vaterland nur dachte,", "tokens": ["Wer", "an", "das", "Va\u00b7ter\u00b7land", "nur", "dach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Vaterland sich dienstbar machte", "tokens": ["Dem", "Va\u00b7ter\u00b7land", "sich", "dienst\u00b7bar", "mach\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "ADJD", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Rath und That, mit Hab' und Gut", "tokens": ["Mit", "Rath", "und", "That", ",", "mit", "Hab'", "und", "Gut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und, wo es galt, mit Leib und Blut,", "tokens": ["Und", ",", "wo", "es", "galt", ",", "mit", "Leib", "und", "Blut", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PPER", "VVFIN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wer so das Himmelreich gewann,", "tokens": ["Wer", "so", "das", "Him\u00b7mel\u00b7reich", "ge\u00b7wann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hie\u00df weiland nur ein frommer Mann.", "tokens": ["Hie\u00df", "wei\u00b7land", "nur", "ein", "from\u00b7mer", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Was aber sind die frommen Leute", "tokens": ["Was", "a\u00b7ber", "sind", "die", "from\u00b7men", "Leu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr unser Vaterland doch heute?", "tokens": ["F\u00fcr", "un\u00b7ser", "Va\u00b7ter\u00b7land", "doch", "heu\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie haben sich von uns gewandt,", "tokens": ["Sie", "ha\u00b7ben", "sich", "von", "uns", "ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Himmel ist ihr Vaterland,", "tokens": ["Der", "Him\u00b7mel", "ist", "ihr", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das Leben ihnen eine Last,", "tokens": ["Das", "Le\u00b7ben", "ih\u00b7nen", "ei\u00b7ne", "Last", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Tod nur lieb, die Welt verhasst.", "tokens": ["Der", "Tod", "nur", "lieb", ",", "die", "Welt", "ver\u00b7hasst", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}