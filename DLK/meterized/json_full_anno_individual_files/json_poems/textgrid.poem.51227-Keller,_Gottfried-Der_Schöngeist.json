{"textgrid.poem.51227": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Der Sch\u00f6ngeist", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbo welch ein Duften, Rosalinde!", "tokens": ["\u00bb", "o", "welch", "ein", "Duf\u00b7ten", ",", "Ro\u00b7sa\u00b7lin\u00b7de", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "FM", "PWAT", "ART", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im bl\u00fcten\u00fcberf\u00fcllten Tal!", "tokens": ["Im", "bl\u00fc\u00b7ten\u00b7\u00fc\u00b7berf\u00b7\u00fcll\u00b7ten", "Tal", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch das Gew\u00f6lk, zerstreut vom Winde,", "tokens": ["Durch", "das", "Ge\u00b7w\u00f6lk", ",", "zer\u00b7streut", "vom", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Bricht brennendrot der Abendstrahl;", "tokens": ["Bricht", "bren\u00b7nen\u00b7drot", "der", "A\u00b7bend\u00b7strahl", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie Feuer flie\u00dft der Fr\u00fchlingsregen,", "tokens": ["Wie", "Feu\u00b7er", "flie\u00dft", "der", "Fr\u00fch\u00b7lings\u00b7re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie Feuer rollt es auf den Wegen", "tokens": ["Wie", "Feu\u00b7er", "rollt", "es", "auf", "den", "We\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und trieft's von jedem Zweig zumal!", "tokens": ["Und", "trieft's", "von", "je\u00b7dem", "Zweig", "zu\u00b7mal", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und siehst du dort die Gruppe ragen,", "tokens": ["Und", "siehst", "du", "dort", "die", "Grup\u00b7pe", "ra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Am Kreuzweg, finster in die Glut,", "tokens": ["Am", "Kreuz\u00b7weg", ",", "fins\u00b7ter", "in", "die", "Glut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In sich geschart, wie stumme Klagen,", "tokens": ["In", "sich", "ge\u00b7schart", ",", "wie", "stum\u00b7me", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "VVPP", "$,", "PWAV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die malerische Lumpenbrut?", "tokens": ["Die", "ma\u00b7le\u00b7ri\u00b7sche", "Lum\u00b7pen\u00b7brut", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein volles Bild ist hier errichtet,", "tokens": ["Ein", "vol\u00b7les", "Bild", "ist", "hier", "er\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein jeder Zug ist wie gedichtet \u2013", "tokens": ["Ein", "je\u00b7der", "Zug", "ist", "wie", "ge\u00b7dich\u00b7tet", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "KOKOM", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Heut sind uns, traun! die Musen gut!", "tokens": ["Heut", "sind", "uns", ",", "traun", "!", "die", "Mu\u00b7sen", "gut", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "VVINF", "$.", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Gib Stift und Mappe, da\u00df die rasche,", "tokens": ["Gib", "Stift", "und", "Map\u00b7pe", ",", "da\u00df", "die", "ra\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "KON", "NN", "$,", "KOUS", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die kecke Dilettantenhand", "tokens": ["Die", "ke\u00b7cke", "Di\u00b7let\u00b7tan\u00b7ten\u00b7hand"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Perle dieses Bildes hasche,", "tokens": ["Die", "Per\u00b7le", "die\u00b7ses", "Bil\u00b7des", "ha\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das ich so unverhofft hier fand!", "tokens": ["Das", "ich", "so", "un\u00b7ver\u00b7hofft", "hier", "fand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zu sch\u00f6ner Stunden heitrem Schauen,", "tokens": ["Zu", "sch\u00f6\u00b7ner", "Stun\u00b7den", "heit\u00b7rem", "Schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Gem\u00fct und Augen zu erbauen,", "tokens": ["Ge\u00b7m\u00fct", "und", "Au\u00b7gen", "zu", "er\u00b7bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sei es f\u00fcr immer festgebannt.", "tokens": ["Sei", "es", "f\u00fcr", "im\u00b7mer", "fest\u00b7ge\u00b7bannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Siehst du, o teure Rosalinde!", "tokens": ["Siehst", "du", ",", "o", "teu\u00b7re", "Ro\u00b7sa\u00b7lin\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den b\u00e4rt'gen Mann mit breitem Hut,", "tokens": ["Den", "b\u00e4rt'\u00b7gen", "Mann", "mit", "brei\u00b7tem", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An dem die Mutter mit dem Kinde", "tokens": ["An", "dem", "die", "Mut\u00b7ter", "mit", "dem", "Kin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u2013 Madonnenurbild! \u2013 s\u00e4ugend ruht?", "tokens": ["\u2013", "Ma\u00b7don\u00b7nen\u00b7ur\u00b7bild", "!", "\u2013", "s\u00e4u\u00b7gend", "ruht", "?"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "NN", "$.", "$(", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es ragt das dunkle Haupt des Gatten,", "tokens": ["Es", "ragt", "das", "dunk\u00b7le", "Haupt", "des", "Gat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In sich gekehrt, im braunen Schatten,", "tokens": ["In", "sich", "ge\u00b7kehrt", ",", "im", "brau\u00b7nen", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "VVPP", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Das ihre schwimmt in Purpurglut.", "tokens": ["Das", "ih\u00b7re", "schwimmt", "in", "Pur\u00b7pur\u00b7glut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Jedoch, da\u00df von der ebnen Erde", "tokens": ["Je\u00b7doch", ",", "da\u00df", "von", "der", "eb\u00b7nen", "Er\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Bild gerundet auf sich schwingt,", "tokens": ["Das", "Bild", "ge\u00b7run\u00b7det", "auf", "sich", "schwingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Siehst du der Kinder scheue Herde,", "tokens": ["Siehst", "du", "der", "Kin\u00b7der", "scheu\u00b7e", "Her\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie sie der Eltern Knie umringt;", "tokens": ["Wie", "sie", "der", "El\u00b7tern", "Knie", "um\u00b7ringt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und d\u00fcster, stumm, wie erzgegossen,", "tokens": ["Und", "d\u00fcs\u00b7ter", ",", "stumm", ",", "wie", "erz\u00b7ge\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADJD", "$,", "PWAV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von Licht und Regen \u00fcberflossen", "tokens": ["Von", "Licht", "und", "Re\u00b7gen", "\u00fc\u00b7berf\u00b7los\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Es gl\u00e4nzend in die Augen springt.", "tokens": ["Es", "gl\u00e4n\u00b7zend", "in", "die", "Au\u00b7gen", "springt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Welch einen Adel haucht das Ganze,", "tokens": ["Welch", "ei\u00b7nen", "A\u00b7del", "haucht", "das", "Gan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Stolz wie ein ehern K\u00f6nigsgrab!", "tokens": ["Stolz", "wie", "ein", "e\u00b7hern", "K\u00f6\u00b7nigs\u00b7grab", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie thront in seines Jammers Glanze", "tokens": ["Wie", "thront", "in", "sei\u00b7nes", "Jam\u00b7mers", "Glan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Mann mit seinem Wanderstab!", "tokens": ["Der", "Mann", "mit", "sei\u00b7nem", "Wan\u00b7der\u00b7stab", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dank dir, o freundlichste der Musen,", "tokens": ["Dank", "dir", ",", "o", "freund\u00b7lichs\u00b7te", "der", "Mu\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "FM", "ADJA", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die ein empf\u00e4nglich Herz im Busen,", "tokens": ["Die", "ein", "emp\u00b7f\u00e4ng\u00b7lich", "Herz", "im", "Bu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJD", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Den Sinn f\u00fcr ewig Sch\u00f6nes gab!\u00ab", "tokens": ["Den", "Sinn", "f\u00fcr", "e\u00b7wig", "Sch\u00f6\u00b7nes", "gab", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "ADJD", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Da sind, im Tau des Grames schwimmend,", "tokens": ["Da", "sind", ",", "im", "Tau", "des", "Gra\u00b7mes", "schwim\u00b7mend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "APPRART", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In dem der Abendstrahl sich bricht,", "tokens": ["In", "dem", "der", "A\u00b7bend\u00b7strahl", "sich", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Augen jener aufgericht;", "tokens": ["Die", "Au\u00b7gen", "je\u00b7ner", "auf\u00b7ge\u00b7richt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie starren wundernd nach dem Bogen,", "tokens": ["Sie", "star\u00b7ren", "wun\u00b7dernd", "nach", "dem", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Von dem ihr Konterfei, gezogen", "tokens": ["Von", "dem", "ihr", "Kon\u00b7ter\u00b7fei", ",", "ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "PPOSAT", "NN", "$,", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von wei\u00dfer Hand, schon deutlich spricht.", "tokens": ["Von", "wei\u00b7\u00dfer", "Hand", ",", "schon", "deut\u00b7lich", "spricht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und hoch aus seines Elends Mitte", "tokens": ["Und", "hoch", "aus", "sei\u00b7nes", "E\u00b7lends", "Mit\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hub sich der arme Mann empor,", "tokens": ["Hub", "sich", "der", "ar\u00b7me", "Mann", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und langsam trugen m\u00fcde Schritte", "tokens": ["Und", "lang\u00b7sam", "tru\u00b7gen", "m\u00fc\u00b7de", "Schrit\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die finstere Gestalt hervor;", "tokens": ["Die", "fins\u00b7te\u00b7re", "Ge\u00b7stalt", "her\u00b7vor", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es schlossen fest sich seine Z\u00e4hne:", "tokens": ["Es", "schlos\u00b7sen", "fest", "sich", "sei\u00b7ne", "Z\u00e4h\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PRF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Im Aug der Kr\u00e4nkung bittre Tr\u00e4ne,", "tokens": ["Im", "Aug", "der", "Kr\u00e4n\u00b7kung", "bitt\u00b7re", "Tr\u00e4\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Im