{"textgrid.poem.61946": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "1L: Allein, allein, doch nicht auf freier Erde,", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Allein, allein, doch nicht auf freier Erde,", "tokens": ["Al\u00b7lein", ",", "al\u00b7lein", ",", "doch", "nicht", "auf", "frei\u00b7er", "Er\u00b7de", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "$,", "ADV", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In einer Zelle engem Raum allein \u2013", "tokens": ["In", "ei\u00b7ner", "Zel\u00b7le", "en\u00b7gem", "Raum", "al\u00b7lein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "ADV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "D\u00e4monen steigen auf im d\u00fcstren Schrein,", "tokens": ["D\u00e4\u00b7mo\u00b7nen", "stei\u00b7gen", "auf", "im", "d\u00fcst\u00b7ren", "Schrein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Als Ton ein Schrei, \u2013 als Bild wahnsinnige Geberde.", "tokens": ["Als", "Ton", "ein", "Schrei", ",", "\u2013", "als", "Bild", "wahn\u00b7sin\u00b7ni\u00b7ge", "Ge\u00b7ber\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "$,", "$(", "KOUS", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nacht \u2013 Tag \u2013 Nichts \u2013 Nichts \u2013 die