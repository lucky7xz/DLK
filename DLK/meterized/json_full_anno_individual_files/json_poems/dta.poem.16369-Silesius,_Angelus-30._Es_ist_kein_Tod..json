{"dta.poem.16369": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "30.  Es ist kein Tod.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Ich glaube keinen Tod: Sterb ich gleich alle Stunden/", "tokens": ["Ich", "glau\u00b7be", "kei\u00b7nen", "Tod", ":", "Sterb", "ich", "gleich", "al\u00b7le", "Stun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$.", "VVFIN", "PPER", "ADV", "PIAT", "NN", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So hab ich jedesmahl ein besser Leben funden.", "tokens": ["So", "hab", "ich", "je\u00b7des\u00b7mahl", "ein", "bes\u00b7ser", "Le\u00b7ben", "fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}