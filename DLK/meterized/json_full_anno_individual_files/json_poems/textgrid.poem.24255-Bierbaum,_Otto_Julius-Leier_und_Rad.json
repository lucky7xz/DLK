{"textgrid.poem.24255": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Leier und Rad", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da es nun wieder Fr\u00fchling geworden ist,", "tokens": ["Da", "es", "nun", "wie\u00b7der", "Fr\u00fch\u00b7ling", "ge\u00b7wor\u00b7den", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "NN", "VAPP", "VAFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ziemts dem Poeten, die werte Leier,", "tokens": ["Ziemts", "dem", "Po\u00b7et\u00b7en", ",", "die", "wer\u00b7te", "Lei\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Die, gelehnt an das unge\u00f6lte", "tokens": ["Die", ",", "ge\u00b7lehnt", "an", "das", "un\u00b7ge\u00b7\u00f6l\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ART", "$,", "VVPP", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zweirad, g\u00e4nzlich verstaubt hinter dem Kleiderschrank", "tokens": ["Zwei\u00b7rad", ",", "g\u00e4nz\u00b7lich", "ver\u00b7staubt", "hin\u00b7ter", "dem", "Klei\u00b7der\u00b7schrank"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADJD", "VVPP", "APPR", "ART", "NN"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Lange mit Mi\u00dfmut ruhte, hervorzuziehn.", "tokens": ["Lan\u00b7ge", "mit", "Mi\u00df\u00b7mut", "ruh\u00b7te", ",", "her\u00b7vor\u00b7zu\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "$,", "VVIZU", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Wahrlich! (so ruft er und schl\u00e4gt mit Macht,", "tokens": ["Wahr\u00b7lich", "!", "(", "so", "ruft", "er", "und", "schl\u00e4gt", "mit", "Macht", ","], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "$(", "ADV", "VVFIN", "PPER", "KON", "VVFIN", "APPR", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Pingt\u00fct\u00fcping, in die schnarrenden Saiten)", "tokens": ["Ping\u00b7t\u00fc\u00b7t\u00fc\u00b7ping", ",", "in", "die", "schnar\u00b7ren\u00b7den", "Sai\u00b7ten", ")"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Wahrlich! Diesmal verlohnt es sich,", "tokens": ["Wahr\u00b7lich", "!", "Dies\u00b7mal", "ver\u00b7lohnt", "es", "sich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ADV", "VVFIN", "PPER", "PRF", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Fr\u00fchlingslieder zu rupfen voll Inbrunst.", "tokens": ["Fr\u00fch\u00b7lings\u00b7lie\u00b7der", "zu", "rup\u00b7fen", "voll", "In\u00b7brunst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "ADJD", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Siehe, es schlagen nicht blo\u00df die B\u00e4ume aus,", "tokens": ["Sie\u00b7he", ",", "es", "schla\u00b7gen", "nicht", "blo\u00df", "die", "B\u00e4u\u00b7me", "aus", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PPER", "VVFIN", "PTKNEG", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sondern auch Russen sowohl wie Japaner, und", "tokens": ["Son\u00b7dern", "auch", "Rus\u00b7sen", "so\u00b7wohl", "wie", "Ja\u00b7pa\u00b7ner", ",", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADV", "NE", "KON", "PWAV", "NE", "$,", "KON"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "M\u00f6glichenfalls, ehe es Sommer wird,", "tokens": ["M\u00f6g\u00b7li\u00b7chen\u00b7falls", ",", "e\u00b7he", "es", "Som\u00b7mer", "wird", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "KOUS", "PPER", "NN", "VAFIN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Gibts auch auf dem biederen Balkan", "tokens": ["Gibts", "auch", "auf", "dem", "bie\u00b7de\u00b7ren", "Bal\u00b7kan"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Die mit Recht so beliebte Metzelsuppe.", "tokens": ["Die", "mit", "Recht", "so", "be\u00b7lieb\u00b7te", "Met\u00b7zel\u00b7sup\u00b7pe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "ADV", "ADJA", "NN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Ja, die Welt wird sch\u00f6ner mit jedem Tag!", "tokens": ["Ja", ",", "die", "Welt", "wird", "sch\u00f6\u00b7ner", "mit", "je\u00b7dem", "Tag", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "VAFIN", "ADJD", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Einige Primeln schon fand ich an Waldes Rand,", "tokens": ["Ei\u00b7ni\u00b7ge", "Pri\u00b7meln", "schon", "fand", "ich", "an", "Wal\u00b7des", "Rand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VVFIN", "PPER", "APPR", "NN", "NN", "$,"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Und die Amsel mit gelbem Schnabel singt", "tokens": ["Und", "die", "Am\u00b7sel", "mit", "gel\u00b7bem", "Schna\u00b7bel", "singt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Angenehm im Birnbaumzweigicht.", "tokens": ["An\u00b7ge\u00b7nehm", "im", "Birn\u00b7baum\u00b7zwei\u00b7gicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Frischer Schnittlauch, siehe, spitzt auch hervor,", "tokens": ["Fri\u00b7scher", "Schnitt\u00b7lauch", ",", "sie\u00b7he", ",", "spitzt", "auch", "her\u00b7vor", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVIMP", "$,", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Und mir ahnt es, \u00fcber ein kleines, bald,", "tokens": ["Und", "mir", "ahnt", "es", ",", "\u00fc\u00b7ber", "ein", "klei\u00b7nes", ",", "bald", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "APPR", "ART", "ADJA", "$,", "ADV", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Bald entzupf ich dem lockeren Erdreich", "tokens": ["Bald", "ent\u00b7zupf", "ich", "dem", "lo\u00b7cke\u00b7ren", "Er\u00b7dreich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Schamhaft err\u00f6tende Fr\u00fchradieschen,", "tokens": ["Scham\u00b7haft", "er\u00b7r\u00f6\u00b7ten\u00b7de", "Fr\u00fch\u00b7ra\u00b7die\u00b7schen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "Bis dann endlich der dreimal gepriesene", "tokens": ["Bis", "dann", "end\u00b7lich", "der", "drei\u00b7mal", "ge\u00b7prie\u00b7se\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADV", "ART", "ADV", "ADJA"], "meter": "--+--+--+--", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Tag des andachtsvollen, ersten", "tokens": ["Tag", "des", "an\u00b7dachts\u00b7vol\u00b7len", ",", "ers\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["NN", "ART", "ADJA", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Spargelstechens ambrosisch herannaht.", "tokens": ["Spar\u00b7gel\u00b7ste\u00b7chens", "am\u00b7bro\u00b7sisch", "her\u00b7an\u00b7naht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VVPP", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Wird, bis dies sich begibt, die Knute", "tokens": ["Wird", ",", "bis", "dies", "sich", "be\u00b7gibt", ",", "die", "Knu\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "$,", "KOUS", "PDS", "PRF", "VVFIN", "$,", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Oder der Bambuspr\u00fcgel den Sieg", "tokens": ["O\u00b7der", "der", "Bam\u00b7bu\u00b7spr\u00fc\u00b7gel", "den", "Sieg"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Im mandschurischen Schnee gewonnen haben?", "tokens": ["Im", "mand\u00b7schu\u00b7ri\u00b7schen", "Schnee", "ge\u00b7won\u00b7nen", "ha\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "VAINF", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Diese Frage (das merkt jeder Erfahrene)", "tokens": ["Die\u00b7se", "Fra\u00b7ge", "(", "das", "merkt", "je\u00b7der", "Er\u00b7fah\u00b7re\u00b7ne", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$(", "PDS", "VVFIN", "PIAT", "NN", "$("], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Ist rhetorisch gemeint, und niemand", "tokens": ["Ist", "rhe\u00b7to\u00b7risch", "ge\u00b7meint", ",", "und", "nie\u00b7mand"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ADJD", "VVPP", "$,", "KON", "PIS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wartet auf Antwort darauf. \u2013 Der Dichter", "tokens": ["War\u00b7tet", "auf", "Ant\u00b7wort", "da\u00b7rauf", ".", "\u2013", "Der", "Dich\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["VAFIN", "APPR", "NN", "PAV", "$.", "$(", "ART", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.7": {"text": "Stellt das Leiergestell behutsam", "tokens": ["Stellt", "das", "Lei\u00b7er\u00b7ge\u00b7stell", "be\u00b7hut\u00b7sam"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADJD"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Wieder hinter den Kleiderschrank.", "tokens": ["Wie\u00b7der", "hin\u00b7ter", "den", "Klei\u00b7der\u00b7schrank", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "Aber mit pr\u00fcfendem Ohre schiebt er", "tokens": ["A\u00b7ber", "mit", "pr\u00fc\u00b7fen\u00b7dem", "Oh\u00b7re", "schiebt", "er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "PPER"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "(weh, wie wimmerts und pfeifts in den Lagern!)", "tokens": ["(", "weh", ",", "wie", "wim\u00b7merts", "und", "pfeifts", "in", "den", "La\u00b7gern", "!", ")"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKVZ", "$,", "PWAV", "ADV", "KON", "VVFIN", "APPR", "ART", "NN", "$.", "$("], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Langsam das Zweirad hervor und l\u00e4\u00dft ihm", "tokens": ["Lang\u00b7sam", "das", "Zwei\u00b7rad", "her\u00b7vor", "und", "l\u00e4\u00dft", "ihm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ART", "NN", "PTKVZ", "KON", "VVFIN", "PPER"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Kundigen Sinns am entsprechenden Orte", "tokens": ["Kun\u00b7di\u00b7gen", "Sinns", "am", "ent\u00b7spre\u00b7chen\u00b7den", "Or\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPRART", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "Sanft einschl\u00fcpfenden \u00d6ls genug", "tokens": ["Sanft", "ein\u00b7schl\u00fcp\u00b7fen\u00b7den", "\u00d6ls", "ge\u00b7nug"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "ADV"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "In die vertrockneten Lager tr\u00e4ufeln.", "tokens": ["In", "die", "ver\u00b7trock\u00b7ne\u00b7ten", "La\u00b7ger", "tr\u00e4u\u00b7feln", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "---+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Dies getan, ergreift er mit hurtiger", "tokens": ["Dies", "ge\u00b7tan", ",", "er\u00b7greift", "er", "mit", "hur\u00b7ti\u00b7ger"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVPP", "$,", "VVFIN", "PPER", "APPR", "NN"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Hand die zum Lenken bestimmte Stange,", "tokens": ["Hand", "die", "zum", "Len\u00b7ken", "be\u00b7stimm\u00b7te", "Stan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Setzt mit Mut und Anstand die linke", "tokens": ["Setzt", "mit", "Mut", "und", "An\u00b7stand", "die", "lin\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "ART", "ADJA"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Sohle auf den gekerbten Stift am", "tokens": ["Soh\u00b7le", "auf", "den", "ge\u00b7kerb\u00b7ten", "Stift", "am"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "APPRART"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Hinterrade und hupft mit dem rechten", "tokens": ["Hin\u00b7ter\u00b7ra\u00b7de", "und", "hupft", "mit", "dem", "rech\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KON", "VVFIN", "APPR", "ART", "ADJA"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Beine gewaltig ein-, zwei-, dreimal,", "tokens": ["Bei\u00b7ne", "ge\u00b7wal\u00b7tig", "ein", ",", "zwei", ",", "drei\u00b7mal", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "ADJD", "TRUNC", "$,", "TRUNC", "$,", "ADV", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Bis er, gelobt sei der Geist der Balance,", "tokens": ["Bis", "er", ",", "ge\u00b7lobt", "sei", "der", "Geist", "der", "Ba\u00b7lan\u00b7ce", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "VVPP", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.8": {"text": "Sicheren Schwunges sich hebt in den Sattel", "tokens": ["Si\u00b7che\u00b7ren", "Schwun\u00b7ges", "sich", "hebt", "in", "den", "Sat\u00b7tel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "PRF", "VVFIN", "APPR", "ART", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.9": {"text": "Und mit dem Rhythmus, der Dichtern eigen,", "tokens": ["Und", "mit", "dem", "Rhyth\u00b7mus", ",", "der", "Dich\u00b7tern", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "ART", "NN", "ADJD", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.10": {"text": "Ruhig hinauspedalt in die Landschaft.", "tokens": ["Ru\u00b7hig", "hin\u00b7aus\u00b7pe\u00b7dalt", "in", "die", "Land\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.9": {"line.1": {"text": "Da es nun wieder Fr\u00fchling geworden ist,", "tokens": ["Da", "es", "nun", "wie\u00b7der", "Fr\u00fch\u00b7ling", "ge\u00b7wor\u00b7den", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "NN", "VAPP", "VAFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ziemts dem Poeten, die werte Leier,", "tokens": ["Ziemts", "dem", "Po\u00b7et\u00b7en", ",", "die", "wer\u00b7te", "Lei\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Die, gelehnt an das unge\u00f6lte", "tokens": ["Die", ",", "ge\u00b7lehnt", "an", "das", "un\u00b7ge\u00b7\u00f6l\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ART", "$,", "VVPP", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zweirad, g\u00e4nzlich verstaubt hinter dem Kleiderschrank", "tokens": ["Zwei\u00b7rad", ",", "g\u00e4nz\u00b7lich", "ver\u00b7staubt", "hin\u00b7ter", "dem", "Klei\u00b7der\u00b7schrank"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADJD", "VVPP", "APPR", "ART", "NN"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Lange mit Mi\u00dfmut ruhte, hervorzuziehn.", "tokens": ["Lan\u00b7ge", "mit", "Mi\u00df\u00b7mut", "ruh\u00b7te", ",", "her\u00b7vor\u00b7zu\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "$,", "VVIZU", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}}, "stanza.10": {"line.1": {"text": "Wahrlich! (so ruft er und schl\u00e4gt mit Macht,", "tokens": ["Wahr\u00b7lich", "!", "(", "so", "ruft", "er", "und", "schl\u00e4gt", "mit", "Macht", ","], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "$(", "ADV", "VVFIN", "PPER", "KON", "VVFIN", "APPR", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Pingt\u00fct\