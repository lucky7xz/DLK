{"dta.poem.2091": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Jgel .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Unm\u00f6glich kann man die\u00df Gesch\u00f6pf ohn einige Be-", "tokens": ["Un\u00b7m\u00f6g\u00b7lich", "kann", "man", "die\u00df", "Ge\u00b7sch\u00f6pf", "ohn", "ei\u00b7ni\u00b7ge", "Be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VMFIN", "PIS", "PDS", "NN", "APPR", "PIAT", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "wundrung sehn,", "tokens": ["wund\u00b7rung", "sehn", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Als dessen sonderlicher K\u00f6rper, zumalen wie er einge-", "tokens": ["Als", "des\u00b7sen", "son\u00b7der\u00b7li\u00b7cher", "K\u00f6r\u00b7per", ",", "zu\u00b7ma\u00b7len", "wie", "er", "ein\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PDS", "ADJA", "NN", "$,", "VVFIN", "KOKOM", "PPER", "TRUNC"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "kleidet,", "tokens": ["klei\u00b7det", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Von allen uns bekannten Thieren verwunderlich sich un-", "tokens": ["Von", "al\u00b7len", "uns", "be\u00b7kann\u00b7ten", "Thie\u00b7ren", "ver\u00b7wun\u00b7der\u00b7lich", "sich", "un"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "PPER", "ADJA", "NN", "ADJD", "PRF", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "terscheidet.", "tokens": ["ter\u00b7schei\u00b7det", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Man sieht auf seiner ganzen Haut, anstatt der Haare,", "tokens": ["Man", "sieht", "auf", "sei\u00b7ner", "gan\u00b7zen", "Haut", ",", "an\u00b7statt", "der", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,", "KOUI", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "H\u00f6rner stehn,", "tokens": ["H\u00f6r\u00b7ner", "stehn", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Die ihn auf eine Weise sch\u00fctzen und so f\u00fcr allen Anfall", "tokens": ["Die", "ihn", "auf", "ei\u00b7ne", "Wei\u00b7se", "sch\u00fct\u00b7zen", "und", "so", "f\u00fcr", "al\u00b7len", "An\u00b7fall"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "VVINF", "KON", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "decken,", "tokens": ["de\u00b7cken", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Da\u00df es nur seltene Gefahren, die ihm bedrohen und ihn", "tokens": ["Da\u00df", "es", "nur", "sel\u00b7te\u00b7ne", "Ge\u00b7fah\u00b7ren", ",", "die", "ihm", "be\u00b7dro\u00b7hen", "und", "ihn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "NN", "$,", "PRELS", "PPER", "VVINF", "KON", "PPER"], "meter": "+--+---+-+--+--+", "measure": "dactylic.di.plus"}, "line.12": {"text": "schrecken.", "tokens": ["schre\u00b7cken", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Es wird, in dieses Thierchens Bildung, ein recht ab-", "tokens": ["Es", "wird", ",", "in", "die\u00b7ses", "Thier\u00b7chens", "Bil\u00b7dung", ",", "ein", "recht", "ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "APPR", "PDAT", "NN", "NN", "$,", "ART", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "sichtlicher Verstand,", "tokens": ["sicht\u00b7li\u00b7cher", "Ver\u00b7stand", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+---+", "measure": "dactylic.init"}, "line.15": {"text": "Ein richtig \u00fcberlegter Endzweck, vor vielen andern noch", "tokens": ["Ein", "rich\u00b7tig", "\u00fc\u00b7ber\u00b7leg\u00b7ter", "End\u00b7zweck", ",", "vor", "vie\u00b7len", "an\u00b7dern", "noch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,", "APPR", "PIAT", "PIS", "ADV"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.16": {"text": "erkannt.", "tokens": ["er\u00b7kannt", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.17": {"text": "Der ungez\u00e4hlten harten, starren und sonderbaren Sta-", "tokens": ["Der", "un\u00b7ge\u00b7z\u00e4hl\u00b7ten", "har\u00b7ten", ",", "star\u00b7ren", "und", "son\u00b7der\u00b7ba\u00b7ren", "Sta"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "$,", "VVFIN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.18": {"text": "cheln Spitzen,", "tokens": ["cheln", "Spit\u00b7zen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.19": {"text": "Die zur Erhaltung ihm so n\u00f6thig, und die ihn rings", "tokens": ["Die", "zur", "Er\u00b7hal\u00b7tung", "ihm", "so", "n\u00f6\u00b7thig", ",", "und", "die", "ihn", "rings"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "APPRART", "NN", "PPER", "ADV", "ADJD", "$,", "KON", "PRELS", "PPER", "ADV"], "meter": "+--+----+-+-+", "measure": "dactylic.di.plus"}, "line.20": {"text": "umgeben, sitzen", "tokens": ["um\u00b7ge\u00b7ben", ",", "sit\u00b7zen"], "token_info": ["word", "punct", "word"], "pos": ["VVPP", "$,", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.21": {"text": "In seiner Haut so ordentlich, und so beweglich einge-", "tokens": ["In", "sei\u00b7ner", "Haut", "so", "or\u00b7dent\u00b7lich", ",", "und", "so", "be\u00b7weg\u00b7lich", "ein\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ADJD", "$,", "KON", "ADV", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.22": {"text": "senkt,", "tokens": ["senkt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.23": {"text": "Da\u00df er dieselbe, nach Gefallen, erhebt, sie durch ein-", "tokens": ["Da\u00df", "er", "die\u00b7sel\u00b7be", ",", "nach", "Ge\u00b7fal\u00b7len", ",", "er\u00b7hebt", ",", "sie", "durch", "ein"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PDAT", "$,", "APPR", "NN", "$,", "VVFIN", "$,", "PPER", "APPR", "TRUNC"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "ander schrenkt,", "tokens": ["an\u00b7der", "schrenkt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.25": {"text": "Verbreitet, spreizt und von sich strecket, auch schnell sie", "tokens": ["Ver\u00b7brei\u00b7tet", ",", "spreizt", "und", "von", "sich", "stre\u00b7cket", ",", "auch", "schnell", "sie"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "$,", "VVFIN", "KON", "APPR", "PRF", "VVFIN", "$,", "ADV", "ADJD", "PPER"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "wieder niederleget,", "tokens": ["wie\u00b7der", "nie\u00b7der\u00b7le\u00b7get", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.27": {"text": "Und sie, dem Schein nach sonder Ordnung, dennoch", "tokens": ["Und", "sie", ",", "dem", "Schein", "nach", "son\u00b7der", "Ord\u00b7nung", ",", "den\u00b7noch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PPER", "$,", "ART", "NN", "APPR", "ADJA", "NN", "$,", "ADV"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.28": {"text": "ganz ordentlich beweget.", "tokens": ["ganz", "or\u00b7dent\u00b7lich", "be\u00b7we\u00b7get", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.29": {"text": "Er richtet aus sich selber gleichsam lebend\u2019ge Palisaden", "tokens": ["Er", "rich\u00b7tet", "aus", "sich", "sel\u00b7ber", "gleich\u00b7sam", "le\u00b7ben\u00b7d'\u00b7ge", "Pa\u00b7li\u00b7sa\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "ADV", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.30": {"text": "auf;", "tokens": ["auf", ";"], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$."], "meter": "+", "measure": "single.up"}}}}}