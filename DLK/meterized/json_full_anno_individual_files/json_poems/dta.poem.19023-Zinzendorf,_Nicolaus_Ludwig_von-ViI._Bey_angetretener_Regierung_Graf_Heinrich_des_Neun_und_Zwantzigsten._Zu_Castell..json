{"dta.poem.19023": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "ViI.   Bey angetretener Regierung Graf  \n Heinrich des Neun und Zwantzigsten. Zu Castell.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Als der Mensch nach GOttes Bilde", "tokens": ["Als", "der", "Mensch", "nach", "Got\u00b7tes", "Bil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ehemals bereitet war,", "tokens": ["E\u00b7he\u00b7mals", "be\u00b7rei\u00b7tet", "war", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "O wie war er nicht so milde,", "tokens": ["O", "wie", "war", "er", "nicht", "so", "mil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VAFIN", "PPER", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "O wie sah er nicht so klar!", "tokens": ["O", "wie", "sah", "er", "nicht", "so", "klar", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VVFIN", "PPER", "PTKNEG", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Seit er dieses Bild verlohren,", "tokens": ["Seit", "er", "die\u00b7ses", "Bild", "ver\u00b7loh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird er fast verr\u00fcckt gebohren:", "tokens": ["Wird", "er", "fast", "ver\u00b7r\u00fcckt", "ge\u00b7boh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Was ihn gl\u00fccklich machen kan,", "tokens": ["Was", "ihn", "gl\u00fcck\u00b7lich", "ma\u00b7chen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Siehet er vor sch\u00e4dlich an.", "tokens": ["Sie\u00b7het", "er", "vor", "sch\u00e4d\u00b7lich", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJD", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Was vor unerh\u00f6rte Sorgen", "tokens": ["Was", "vor", "un\u00b7er\u00b7h\u00f6r\u00b7te", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Macht man seinen Eltern nicht,", "tokens": ["Macht", "man", "sei\u00b7nen", "El\u00b7tern", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Von des Lebens ersten Morgen", "tokens": ["Von", "des", "Le\u00b7bens", "ers\u00b7ten", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Durch das gantze Jugend-Licht?", "tokens": ["Durch", "das", "gant\u00b7ze", "Ju\u00b7gend\u00b7Licht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Mit wie vielem Flehn und Beten,", "tokens": ["Mit", "wie", "vie\u00b7lem", "Flehn", "und", "Be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAV", "PIS", "VVINF", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "M\u00fcssen sie uns nicht vertreten?", "tokens": ["M\u00fcs\u00b7sen", "sie", "uns", "nicht", "ver\u00b7tre\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Sie bem\u00fchn sich offte viel,", "tokens": ["Sie", "be\u00b7m\u00fchn", "sich", "off\u00b7te", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Und verfehlen doch das Ziel.", "tokens": ["Und", "ver\u00b7feh\u00b7len", "doch", "das", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Wie viel tausend Eltern leben,", "tokens": ["Wie", "viel", "tau\u00b7send", "El\u00b7tern", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "CARD", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Welche um der Kinder-Zucht", "tokens": ["Wel\u00b7che", "um", "der", "Kin\u00b7der\u00b7Zucht"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAT", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Sich nicht viel Bem\u00fchung geben;", "tokens": ["Sich", "nicht", "viel", "Be\u00b7m\u00fc\u00b7hung", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKNEG", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Und ihr Heyl darin gesucht,", "tokens": ["Und", "ihr", "Heyl", "da\u00b7rin", "ge\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PAV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Jhnen Lehrer zu benennen,", "tokens": ["Jh\u00b7nen", "Leh\u00b7rer", "zu", "be\u00b7nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Deren ", "tokens": ["De\u00b7ren"], "token_info": ["word"], "pos": ["PDS"], "meter": "+-", "measure": "trochaic.single"}, "line.23": {"text": "Die man nur auf andrer Rath,", "tokens": ["Die", "man", "nur", "auf", "an\u00b7drer", "Rath", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Und Bericht gew\u00e4hlet hat.", "tokens": ["Und", "Be\u00b7richt", "ge\u00b7w\u00e4h\u00b7let", "hat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Viele junge Leute lauffen", "tokens": ["Vie\u00b7le", "jun\u00b7ge", "Leu\u00b7te", "lauf\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "In der Jugend-Hitze fort;", "tokens": ["In", "der", "Ju\u00b7gend\u00b7Hit\u00b7ze", "fort", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Und man l\u00e4sset sie verschnauffen,", "tokens": ["Und", "man", "l\u00e4s\u00b7set", "sie", "ver\u00b7schnauf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "(das ist ein gemeines Wort:)", "tokens": ["(", "das", "ist", "ein", "ge\u00b7mei\u00b7nes", "Wort", ":)"], "token_info": ["punct", "word", "word", "word", "word", "word", "emoticon"], "pos": ["$(", "PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Aber, seht! die meiste Jugend!", "tokens": ["A\u00b7ber", ",", "seht", "!", "die", "meis\u00b7te", "Ju\u00b7gend", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "$.", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Sie vers\u00e4umt die Zeit der Tugend,", "tokens": ["Sie", "ver\u00b7s\u00e4umt", "die", "Zeit", "der", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Manchen, eh er ausgeschnaubt,", "tokens": ["Man\u00b7chen", ",", "eh", "er", "aus\u00b7ge\u00b7schnaubt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "KOUS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.32": {"text": "Hat ein j\u00e4her Tod geraubt.", "tokens": ["Hat", "ein", "j\u00e4\u00b7her", "Tod", "ge\u00b7raubt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "So ist schwerlich zu errathen,", "tokens": ["So", "ist", "schwer\u00b7lich", "zu", "er\u00b7ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Ob der lieben Alten ", "tokens": ["Ob", "der", "lie\u00b7ben", "Al\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.35": {"text": "Die den HErrn um Kinder baten,", "tokens": ["Die", "den", "Herrn", "um", "Kin\u00b7der", "ba\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Jhr nicht selbst zuwider war?", "tokens": ["Ihr", "nicht", "selbst", "zu\u00b7wi\u00b7der", "war", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.37": {"text": "Und ob der nicht gl\u00fccklich heisset,", "tokens": ["Und", "ob", "der", "nicht", "gl\u00fcck\u00b7lich", "heis\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "PTKNEG", "ADJD", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.38": {"text": "Den der HErr von hinnen reisset?", "tokens": ["Den", "der", "Herr", "von", "hin\u00b7nen", "reis\u00b7set", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Eh er sich ins Weite irrt,", "tokens": ["Eh", "er", "sich", "ins", "Wei\u00b7te", "irrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.40": {"text": "Und der Welt recht inne wird.", "tokens": ["Und", "der", "Welt", "recht", "in\u00b7ne", "wird", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "PTKVZ", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.41": {"text": "Aber meine Sinnen blicken", "tokens": ["A\u00b7ber", "mei\u00b7ne", "Sin\u00b7nen", "bli\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Jetzo in ein ander Feld", "tokens": ["Jet\u00b7zo", "in", "ein", "an\u00b7der", "Feld"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.43": {"text": "Da sich junge Pflantzen schicken,", "tokens": ["Da", "sich", "jun\u00b7ge", "Pflant\u00b7zen", "schi\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Wies der G\u00e4rtner dienlich h\u00e4lt,", "tokens": ["Wies", "der", "G\u00e4rt\u00b7ner", "dien\u00b7lich", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.45": {"text": "Welche ihm sein m\u00fchsam Frohnen", "tokens": ["Wel\u00b7che", "ihm", "sein", "m\u00fch\u00b7sam", "Froh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "PPOSAT", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Mit der sch\u00f6nsten Bl\u00fcthe lohnen;", "tokens": ["Mit", "der", "sch\u00f6ns\u00b7ten", "Bl\u00fc\u00b7the", "loh\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.47": {"text": "Diese zeigen sattsam an,", "tokens": ["Die\u00b7se", "zei\u00b7gen", "satt\u00b7sam", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.48": {"text": "Was ein treuer G\u00e4rtner kan.", "tokens": ["Was", "ein", "treu\u00b7er", "G\u00e4rt\u00b7ner", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.49": {"text": "Als ich auf dem Krancken-Bette", "tokens": ["Als", "ich", "auf", "dem", "Kran\u00b7cken\u00b7Bet\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "In der Ungewi\u00dfheit lag,", "tokens": ["In", "der", "Un\u00b7ge\u00b7wi\u00df\u00b7heit", "lag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.51": {"text": "Was ich zu erfahren h\u00e4tte?", "tokens": ["Was", "ich", "zu", "er\u00b7fah\u00b7ren", "h\u00e4t\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.52": {"text": "Kam ein aufgekl\u00e4rter Tag:", "tokens": ["Kam", "ein", "auf\u00b7ge\u00b7kl\u00e4r\u00b7ter", "Tag", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.53": {"text": "Da mir eines G\u00e4rtners Name", "tokens": ["Da", "mir", "ei\u00b7nes", "G\u00e4rt\u00b7ners", "Na\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.54": {"text": "Unverhofft zu Ohren kame,", "tokens": ["Un\u00b7ver\u00b7hofft", "zu", "Oh\u00b7ren", "ka\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.55": {"text": "Welcher seiner Pflantze Prei\u00df", "tokens": ["Wel\u00b7cher", "sei\u00b7ner", "Pflant\u00b7ze", "Prei\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAT", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.56": {"text": "Jederman zu sagen wei\u00df.", "tokens": ["Je\u00b7der\u00b7man", "zu", "sa\u00b7gen", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.57": {"text": "Jhr begl\u00fcckten G\u00e4rtners-H\u00e4nde,", "tokens": ["Ihr", "be\u00b7gl\u00fcck\u00b7ten", "G\u00e4rt\u00b7ner\u00b7sH\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.58": {"text": "Deren Tage-Werck und That,", "tokens": ["De\u00b7ren", "Ta\u00b7ge\u00b7\u00b7Werck", "und", "That", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.59": {"text": "Bi\u00df zu dem erw\u00fcnschten Ende,", "tokens": ["Bi\u00df", "zu", "dem", "er\u00b7w\u00fcnschten", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.60": {"text": "Sich geschickt erwiesen hat:", "tokens": ["Sich", "ge\u00b7schickt", "er\u00b7wie\u00b7sen", "hat", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.61": {"text": "Wie m\u00f6cht ihr den Thau von oben", "tokens": ["Wie", "m\u00f6cht", "ihr", "den", "Thau", "von", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "ART", "NN", "APPR", "ADV"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.62": {"text": "Mit erfreuten Hertzen loben,", "tokens": ["Mit", "er\u00b7freu\u00b7ten", "Hert\u00b7zen", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.63": {"text": "Welcher ohne Maa\u00df und Ziel", "tokens": ["Wel\u00b7cher", "oh\u00b7ne", "Maa\u00df", "und", "Ziel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAT", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.64": {"text": "Auf die sch\u00f6ne Pflantze fiel.", "tokens": ["Auf", "die", "sch\u00f6\u00b7ne", "Pflant\u00b7ze", "fiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.65": {"text": "Bruder, ich kan nicht verschweigen,", "tokens": ["Bru\u00b7der", ",", "ich", "kan", "nicht", "ver\u00b7schwei\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.66": {"text": "Da\u00df der Pflantze Ruhm dir bleibt;", "tokens": ["Da\u00df", "der", "Pflant\u00b7ze", "Ruhm", "dir", "bleibt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.67": {"text": "Die mit ausgespannten Zweigen", "tokens": ["Die", "mit", "aus\u00b7ge\u00b7spann\u00b7ten", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.68": {"text": "Alle Tage h\u00f6her treibt;", "tokens": ["Al\u00b7le", "Ta\u00b7ge", "h\u00f6\u00b7her", "treibt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.69": {"text": "Welche jedem, der sie liebet,", "tokens": ["Wel\u00b7che", "je\u00b7dem", ",", "der", "sie", "lie\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.70": {"text": "So viel sch\u00f6ne Hoffnung giebet,", "tokens": ["So", "viel", "sch\u00f6\u00b7ne", "Hoff\u00b7nung", "gie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.71": {"text": "Da\u00df man GOtt, den Seegens-Mann,", "tokens": ["Da\u00df", "man", "Gott", ",", "den", "See\u00b7gens\u00b7Mann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PIS", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.72": {"text": "Nicht genugsam loben kan.", "tokens": ["Nicht", "ge\u00b7nug\u00b7sam", "lo\u00b7ben", "kan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.73": {"text": "Gl\u00fccklich waren jene Stunden,", "tokens": ["Gl\u00fcck\u00b7lich", "wa\u00b7ren", "je\u00b7ne", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.74": {"text": "Welche ich im Nieder-Land,", "tokens": ["Wel\u00b7che", "ich", "im", "Nie\u00b7der\u00b7Land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.75": {"text": "Als ich dich am Rhein gefunden,", "tokens": ["Als", "ich", "dich", "am", "Rhein", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPRART", "NE", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.76": {"text": "Deiner Freundschafft zugewand.", "tokens": ["Dei\u00b7ner", "Freund\u00b7schafft", "zu\u00b7ge\u00b7wand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.77": {"text": "Gl\u00fccklich waren auch die Stunden,", "tokens": ["Gl\u00fcck\u00b7lich", "wa\u00b7ren", "auch", "die", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.78": {"text": "Da wir uns getrost verbunden,", "tokens": ["Da", "wir", "uns", "ge\u00b7trost", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.79": {"text": "Da\u00df es alle Menschen s\u00e4hn,", "tokens": ["Da\u00df", "es", "al\u00b7le", "Men\u00b7schen", "s\u00e4hn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.80": {"text": "Christi Wandel nachzugehn.", "tokens": ["Chris\u00b7ti", "Wan\u00b7del", "nach\u00b7zu\u00b7gehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVIZU", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.81": {"text": "Wie der Anfang, war das Ende,", "tokens": ["Wie", "der", "An\u00b7fang", ",", "war", "das", "En\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.82": {"text": "Du giengst unter GOttes Huld,", "tokens": ["Du", "giengst", "un\u00b7ter", "Got\u00b7tes", "Huld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.83": {"text": "Und behieltest reine H\u00e4nde", "tokens": ["Und", "be\u00b7hiel\u00b7test", "rei\u00b7ne", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.84": {"text": "Von gemeiner Jugend Schuld;", "tokens": ["Von", "ge\u00b7mei\u00b7ner", "Ju\u00b7gend", "Schuld", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.85": {"text": "Welches, die im Jrrthum waren,", "tokens": ["Wel\u00b7ches", ",", "die", "im", "Jrr\u00b7thum", "wa\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "PRELS", "APPRART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.86": {"text": "Mehr als allzu wol erfahren,", "tokens": ["Mehr", "als", "all\u00b7zu", "wol", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "PTKA", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.87": {"text": "Was die Welt erstaunen macht,", "tokens": ["Was", "die", "Welt", "er\u00b7stau\u00b7nen", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.88": {"text": "Hat dein Tage-Buch verlacht.", "tokens": ["Hat", "dein", "Ta\u00b7ge\u00b7Buch", "ver\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.89": {"text": "Endlich hat es sich geschicket,", "tokens": ["End\u00b7lich", "hat", "es", "sich", "ge\u00b7schi\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.90": {"text": "Da\u00df ich annoch zu Paris", "tokens": ["Da\u00df", "ich", "an\u00b7noch", "zu", "Pa\u00b7ris"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.91": {"text": "Deinem Abschied vorger\u00fccket,", "tokens": ["Dei\u00b7nem", "Ab\u00b7schied", "vor\u00b7ge\u00b7r\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.92": {"text": "Da es aller Orten hie\u00df,", "tokens": ["Da", "es", "al\u00b7ler", "Or\u00b7ten", "hie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.93": {"text": "Auch bey denen guten Leuten,", "tokens": ["Auch", "bey", "de\u00b7nen", "gu\u00b7ten", "Leu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRELS", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.94": {"text": "Welche uns als irrig scheuten,", "tokens": ["Wel\u00b7che", "uns", "als", "ir\u00b7rig", "scheu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "KOUS", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.95": {"text": "Da\u00df du gegen jederman", "tokens": ["Da\u00df", "du", "ge\u00b7gen", "je\u00b7der\u00b7man"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PIS"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.96": {"text": "Als ein wahrer Christ gethan.", "tokens": ["Als", "ein", "wah\u00b7rer", "Christ", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.97": {"text": "Damals lobten wir den Meister", "tokens": ["Da\u00b7mals", "lob\u00b7ten", "wir", "den", "Meis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.98": {"text": "Der allein bewehrten Kunst,", "tokens": ["Der", "al\u00b7lein", "be\u00b7wehr\u00b7ten", "Kunst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.99": {"text": "Der dich vor der falschen Geister", "tokens": ["Der", "dich", "vor", "der", "fal\u00b7schen", "Geis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.100": {"text": "Und der schn\u00f6den Erde Gunst", "tokens": ["Und", "der", "schn\u00f6\u00b7den", "Er\u00b7de", "Gunst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.101": {"text": "V\u00e4terlich bewahren wollen,", "tokens": ["V\u00e4\u00b7ter\u00b7lich", "be\u00b7wah\u00b7ren", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.102": {"text": "Da\u00df sie dich nicht reitzen sollen,", "tokens": ["Da\u00df", "sie", "dich", "nicht", "reit\u00b7zen", "sol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.103": {"text": "Denn dergleichen Wegefahrt", "tokens": ["Denn", "derg\u00b7lei\u00b7chen", "We\u00b7ge\u00b7fahrt"], "token_info": ["word", "word", "word"], "pos": ["KON", "PIS", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.104": {"text": "Ist entfernt von ihrer Art.", "tokens": ["Ist", "ent\u00b7fernt", "von", "ih\u00b7rer", "Art", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.105": {"text": "Wann sich andere erg\u00f6tzten", "tokens": ["Wann", "sich", "an\u00b7de\u00b7re", "er\u00b7g\u00f6tz\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PRF", "PIS", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.106": {"text": "Uber allem, was geschehn,", "tokens": ["U\u00b7ber", "al\u00b7lem", ",", "was", "ge\u00b7schehn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.107": {"text": "Und sich dann zusammen setzten,", "tokens": ["Und", "sich", "dann", "zu\u00b7sam\u00b7men", "setz\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ADV", "ADV", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.108": {"text": "Es aufs neue zu besehn,", "tokens": ["Es", "aufs", "neu\u00b7e", "zu", "be\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "ADJA", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.109": {"text": "Kamst du von des Hofes Brause", "tokens": ["Kamst", "du", "von", "des", "Ho\u00b7fes", "Brau\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.110": {"text": "Oeffters mi\u00dfvergn\u00fcgt nach Hause,", "tokens": ["Oeff\u00b7ters", "mi\u00df\u00b7ver\u00b7gn\u00fcgt", "nach", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.111": {"text": "Wende, sprachst du, meinen Blick,", "tokens": ["Wen\u00b7de", ",", "sprachst", "du", ",", "mei\u00b7nen", "Blick", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.112": {"text": "Und das war dein gr\u00f6stes Gl\u00fcck.", "tokens": ["Und", "das", "war", "dein", "gr\u00f6s\u00b7tes", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.113": {"text": "Also gieng es auf der Reise", "tokens": ["Al\u00b7so", "gieng", "es", "auf", "der", "Rei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.114": {"text": "Nach der werthen Mutter Sinn;", "tokens": ["Nach", "der", "wert\u00b7hen", "Mut\u00b7ter", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.115": {"text": "Und du folgetest der Weise", "tokens": ["Und", "du", "fol\u00b7ge\u00b7test", "der", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.116": {"text": "Deines ehrlichen Bonin.", "tokens": ["Dei\u00b7nes", "ehr\u00b7li\u00b7chen", "Bo\u00b7nin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.117": {"text": "Wo man seine Mutter ehret,", "tokens": ["Wo", "man", "sei\u00b7ne", "Mut\u00b7ter", "eh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.118": {"text": "Und die Vorgesetzte h\u00f6ret,", "tokens": ["Und", "die", "Vor\u00b7ge\u00b7setz\u00b7te", "h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.119": {"text": "Da weicht nach der Liebe Zweck", "tokens": ["Da", "weicht", "nach", "der", "Lie\u00b7be", "Zweck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.120": {"text": "Alles Unvergn\u00fcgen weg.", "tokens": ["Al\u00b7les", "Un\u00b7ver\u00b7gn\u00fc\u00b7gen", "weg", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.121": {"text": "Darum wird im Regimente", "tokens": ["Da\u00b7rum", "wird", "im", "Re\u00b7gi\u00b7men\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "APPRART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.122": {"text": "Gottes Rechte um dich seyn.", "tokens": ["Got\u00b7tes", "Rech\u00b7te", "um", "dich", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "PPER", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.123": {"text": "Der sich sonst gehorsam nennte,", "tokens": ["Der", "sich", "sonst", "ge\u00b7hor\u00b7sam", "nenn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.124": {"text": "Fordert nun Gehorsam ein;", "tokens": ["For\u00b7dert", "nun", "Ge\u00b7hor\u00b7sam", "ein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.125": {"text": "Und nach dem Vergeltungs-Rechte,", "tokens": ["Und", "nach", "dem", "Ver\u00b7gel\u00b7tungs\u00b7Rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.126": {"text": "Sehen alle deine Knechte,", "tokens": ["Se\u00b7hen", "al\u00b7le", "dei\u00b7ne", "Knech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.127": {"text": "Und wer Dir sonst zugethan,", "tokens": ["Und", "wer", "Dir", "sonst", "zu\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.128": {"text": "Dich mit Ehrerbietung an.", "tokens": ["Dich", "mit", "Ehr\u00b7er\u00b7bie\u00b7tung", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.129": {"text": "Du hingegen kanst den Banden", "tokens": ["Du", "hin\u00b7ge\u00b7gen", "kanst", "den", "Ban\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VMFIN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.130": {"text": "Deiner Knechtschafft nicht entgehn;", "tokens": ["Dei\u00b7ner", "Knecht\u00b7schafft", "nicht", "ent\u00b7gehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.131": {"text": "Es ist noch ein HErr vorhanden,", "tokens": ["Es", "ist", "noch", "ein", "Herr", "vor\u00b7han\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.132": {"text": "Dem Du must zu Dienste stehn:", "tokens": ["Dem", "Du", "must", "zu", "Diens\u00b7te", "stehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.133": {"text": "Seine Fesseln sind gelinde,", "tokens": ["Sei\u00b7ne", "Fes\u00b7seln", "sind", "ge\u00b7lin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.134": {"text": "Dieser Dienst bekommt geschwinde", "tokens": ["Die\u00b7ser", "Dienst", "be\u00b7kommt", "ge\u00b7schwin\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "VVFIN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.135": {"text": "Eine andere Gestalt,", "tokens": ["Ei\u00b7ne", "an\u00b7de\u00b7re", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.136": {"text": "Und wir ewige Gewalt.", "tokens": ["Und", "wir", "e\u00b7wi\u00b7ge", "Ge\u00b7walt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.137": {"text": "Diese Hoffnung wird dir bleiben,", "tokens": ["Die\u00b7se", "Hoff\u00b7nung", "wird", "dir", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.138": {"text": "Wann der andern Hoffnung f\u00e4llt:", "tokens": ["Wann", "der", "an\u00b7dern", "Hoff\u00b7nung", "f\u00e4llt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.139": {"text": "Die sich GOttes Hand verschreiben,", "tokens": ["Die", "sich", "Got\u00b7tes", "Hand", "ver\u00b7schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.140": {"text": "Sind schon seelig in der Welt.", "tokens": ["Sind", "schon", "see\u00b7lig", "in", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.141": {"text": "Wann sie alle Menschen hassen,", "tokens": ["Wann", "sie", "al\u00b7le", "Men\u00b7schen", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.142": {"text": "Wird der Freund sie nicht verlassen,", "tokens": ["Wird", "der", "Freund", "sie", "nicht", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.143": {"text": "Dessen treue Liebes-Hand", "tokens": ["Des\u00b7sen", "treu\u00b7e", "Lie\u00b7bes\u00b7Hand"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.144": {"text": "Sich genau an sie verband.", "tokens": ["Sich", "ge\u00b7nau", "an", "sie", "ver\u00b7band", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.145": {"text": "In dem Freunde, lieber Bruder!", "tokens": ["In", "dem", "Freun\u00b7de", ",", "lie\u00b7ber", "Bru\u00b7der", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.146": {"text": "Sind wir ewig ungetrennt:", "tokens": ["Sind", "wir", "e\u00b7wig", "un\u00b7ge\u00b7trennt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.147": {"text": "Durch ihn f\u00fchrest Du das Ruder", "tokens": ["Durch", "ihn", "f\u00fch\u00b7rest", "Du", "das", "Ru\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.148": {"text": "Von dem gantzen Regiment,", "tokens": ["Von", "dem", "gant\u00b7zen", "Re\u00b7gi\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.149": {"text": "Das er Dir in deinem Leben", "tokens": ["Das", "er", "Dir", "in", "dei\u00b7nem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.150": {"text": "Zu bestreiten heim gegeben;", "tokens": ["Zu", "be\u00b7strei\u00b7ten", "heim", "ge\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PTKVZ", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.151": {"text": "Und in seinem Friedens-Schein", "tokens": ["Und", "in", "sei\u00b7nem", "Frie\u00b7dens\u00b7Schein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.152": {"text": "Wirst Du immer ruhig seyn.", "tokens": ["Wirst", "Du", "im\u00b7mer", "ru\u00b7hig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "VAINF", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.153": {"text": "Du must aber nicht vergessen,", "tokens": ["Du", "must", "a\u00b7ber", "nicht", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.154": {"text": "Da\u00df du vor das grosse Heyl,", "tokens": ["Da\u00df", "du", "vor", "das", "gros\u00b7se", "Heyl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.155": {"text": "So der HErr Dir zugemessen,", "tokens": ["So", "der", "Herr", "Dir", "zu\u00b7ge\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.156": {"text": "Jhme auch an deinem Theil", "tokens": ["Jh\u00b7me", "auch", "an", "dei\u00b7nem", "Theil"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.157": {"text": "Ewiglich verbunden bleibest,", "tokens": ["E\u00b7wig\u00b7lich", "ver\u00b7bun\u00b7den", "blei\u00b7best", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.158": {"text": "Und sein Werck nicht l\u00e4\u00dfig treibest:", "tokens": ["Und", "sein", "Werck", "nicht", "l\u00e4\u00b7\u00dfig", "trei\u00b7best", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.159": {"text": "Du must, bi\u00df zum letzten Schein,", "tokens": ["Du", "must", ",", "bi\u00df", "zum", "letz\u00b7ten", "Schein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.160": {"text": "Ein ", "tokens": ["Ein"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.161": {"text": "Wann Du nun genug gestritten,", "tokens": ["Wann", "Du", "nun", "ge\u00b7nug", "ge\u00b7strit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.162": {"text": "Und dein Amt bew\u00e4hret hast;", "tokens": ["Und", "dein", "Amt", "be\u00b7w\u00e4h\u00b7ret", "hast", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.163": {"text": "Wann Du hie und da gelitten,", "tokens": [