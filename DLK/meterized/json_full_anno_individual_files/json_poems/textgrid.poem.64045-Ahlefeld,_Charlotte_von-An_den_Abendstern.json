, "Au\u00b7ge", "mit", "Ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf Deinem hohen, fernen Licht;", "tokens": ["Auf", "Dei\u00b7nem", "ho\u00b7hen", ",", "fer\u00b7nen", "Licht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O m\u00f6chtest Du doch ahnend schauen,", "tokens": ["O", "m\u00f6ch\u00b7test", "Du", "doch", "ah\u00b7nend", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was seine stumme Bitte spricht.", "tokens": ["Was", "sei\u00b7ne", "stum\u00b7me", "Bit\u00b7te", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.20": {"line.1": {"text": "Wenn Er \u2013 Du weisst ja, wen ich meine \u2013", "tokens": ["Wenn", "Er", "\u2013", "Du", "weisst", "ja", ",", "wen", "ich", "mei\u00b7ne", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$(", "PPER", "VVFIN", "ADV", "$,", "PWS", "PPER", "PPOSAT", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Auge still zu Dir erhebt,", "tokens": ["Sein", "Au\u00b7ge", "still", "zu", "Dir", "er\u00b7hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So gr\u00fcss' ihn mit dem sch\u00f6nsten 