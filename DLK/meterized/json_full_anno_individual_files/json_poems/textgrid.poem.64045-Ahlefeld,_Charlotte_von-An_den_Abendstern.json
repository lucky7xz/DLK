{"textgrid.poem.64045": {"metadata": {"author": {"name": "Ahlefeld, Charlotte von", "birth": "N.A.", "death": "N.A."}, "title": "An den Abendstern", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du blickst so l\u00e4chelnd auf mich nieder,", "tokens": ["Du", "blickst", "so", "l\u00e4\u00b7chelnd", "auf", "mich", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du heller, lieber Abendstern,", "tokens": ["Du", "hel\u00b7ler", ",", "lie\u00b7ber", "A\u00b7bends\u00b7tern", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$,", "ADV", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Als h\u00f6rtest Du die leisen Lieder", "tokens": ["Als", "h\u00f6r\u00b7test", "Du", "die", "lei\u00b7sen", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der ahnungsvollen Schwermuth gern.", "tokens": ["Der", "ah\u00b7nungs\u00b7vol\u00b7len", "Schwer\u00b7muth", "gern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn alles schl\u00e4ft, erweckt die Feier", "tokens": ["Wenn", "al\u00b7les", "schl\u00e4ft", ",", "er\u00b7weckt", "die", "Fei\u00b7er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PIS", "VVFIN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der stillen Nacht wie Melodie", "tokens": ["Der", "stil\u00b7len", "Nacht", "wie", "Me\u00b7lo\u00b7die"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KOKOM", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Sehnsucht Klage, und ihr Schleier", "tokens": ["Der", "Sehn\u00b7sucht", "Kla\u00b7ge", ",", "und", "ihr", "Schlei\u00b7er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verr\u00e4th die heissen Thr\u00e4nen nie.", "tokens": ["Ver\u00b7r\u00e4\u00b7th", "die", "heis\u00b7sen", "Thr\u00e4\u00b7nen", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Dann strahlst Du, holder Himmelsfunken,", "tokens": ["Dann", "strahlst", "Du", ",", "hol\u00b7der", "Him\u00b7mels\u00b7fun\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mir Trost in's kranke Herz herab,", "tokens": ["Mir", "Trost", "in's", "kran\u00b7ke", "Herz", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPRART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und es ersteht mir, wonnetrunken,", "tokens": ["Und", "es", "er\u00b7steht", "mir", ",", "won\u00b7ne\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "PWAV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Hoffnung aus der Zeiten Grab.", "tokens": ["Die", "Hoff\u00b7nung", "aus", "der", "Zei\u00b7ten", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Oft schon, wenn ich mit heissem Sehnen", "tokens": ["Oft", "schon", ",", "wenn", "ich", "mit", "heis\u00b7sem", "Seh\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Begr\u00fcsste meiner Liebe Bild,", "tokens": ["Be\u00b7gr\u00fcs\u00b7ste", "mei\u00b7ner", "Lie\u00b7be", "Bild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da lachtest Du in meine Thr\u00e4nen", "tokens": ["Da", "lach\u00b7test", "Du", "in", "mei\u00b7ne", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und machtest meinen Kummer mild.", "tokens": ["Und", "mach\u00b7test", "mei\u00b7nen", "Kum\u00b7mer", "mild", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Oft, wenn ich mich des Lebens freute,", "tokens": ["Oft", ",", "wenn", "ich", "mich", "des", "Le\u00b7bens", "freu\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da folgte mir im dunklen Hain,", "tokens": ["Da", "folg\u00b7te", "mir", "im", "dunk\u00b7len", "Hain", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Deines Himmels blauer Weite,", "tokens": ["Von", "Dei\u00b7nes", "Him\u00b7mels", "blau\u00b7er", "Wei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Freundesblick, Dein Silberschein.", "tokens": ["Wie", "Freun\u00b7des\u00b7blick", ",", "Dein", "Sil\u00b7ber\u00b7schein", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und es bewegte ernst und leise", "tokens": ["Und", "es", "be\u00b7weg\u00b7te", "ernst", "und", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit wunderbarer Ahnung mich,", "tokens": ["Mit", "wun\u00b7der\u00b7ba\u00b7rer", "Ah\u00b7nung", "mich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn in dem ewig festen Gleise", "tokens": ["Wenn", "in", "dem", "e\u00b7wig", "fes\u00b7ten", "Glei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dein reiner Schimmer still erblich.", "tokens": ["Dein", "rei\u00b7ner", "Schim\u00b7mer", "still", "er\u00b7blich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Du schienest dann mir zuzuwinken:", "tokens": ["Du", "schie\u00b7nest", "dann", "mir", "zu\u00b7zu\u00b7win\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbleb' wohl, bis wir uns wiedersehn!", "tokens": ["\u00bb", "leb'", "wohl", ",", "bis", "wir", "uns", "wie\u00b7der\u00b7sehn", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "ADV", "$,", "KOUS", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jetzt muss mein letztes, mattes Blinken", "tokens": ["Jetzt", "muss", "mein", "letz\u00b7tes", ",", "mat\u00b7tes", "Blin\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VMFIN", "PPOSAT", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Morgendufte untergehn!\u00ab", "tokens": ["Im", "Mor\u00b7gen\u00b7duf\u00b7te", "un\u00b7ter\u00b7gehn", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Noch weilt mein Auge mit Vertrauen", "tokens": ["Noch", "weilt", "mein", "Au\u00b7ge", "mit", "Ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf Deinem hohen, fernen Licht;", "tokens": ["Auf", "Dei\u00b7nem", "ho\u00b7hen", ",", "fer\u00b7nen", "Licht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O m\u00f6chtest Du doch ahnend schauen,", "tokens": ["O", "m\u00f6ch\u00b7test", "Du", "doch", "ah\u00b7nend", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was seine stumme Bitte spricht.", "tokens": ["Was", "sei\u00b7ne", "stum\u00b7me", "Bit\u00b7te", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Wenn Er \u2013 Du weisst ja, wen ich meine \u2013", "tokens": ["Wenn", "Er", "\u2013", "Du", "weisst", "ja", ",", "wen", "ich", "mei\u00b7ne", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$(", "PPER", "VVFIN", "ADV", "$,", "PWS", "PPER", "PPOSAT", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Auge still zu Dir erhebt,", "tokens": ["Sein", "Au\u00b7ge", "still", "zu", "Dir", "er\u00b7hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So gr\u00fcss' ihn mit dem sch\u00f6nsten Scheine,", "tokens": ["So", "gr\u00fcss'", "ihn", "mit", "dem", "sch\u00f6ns\u00b7ten", "Schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dass freudiger sein Herz erbebt.", "tokens": ["Dass", "freu\u00b7di\u00b7ger", "sein", "Herz", "er\u00b7bebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Und strahl' ihm s\u00fcssen, reinen Frieden;", "tokens": ["Und", "strahl'", "ihm", "s\u00fcs\u00b7sen", ",", "rei\u00b7nen", "Frie\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVINF", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ach nimm den meinigen dazu!", "tokens": ["Ach", "nimm", "den", "mei\u00b7ni\u00b7gen", "da\u00b7zu", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVIMP", "ART", "PPOSS", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ist ihm einst ein Schmerz beschieden,", "tokens": ["Und", "ist", "ihm", "einst", "ein", "Schmerz", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So gl\u00e4nz' ihm Hoffnung, Muth und Ruh.", "tokens": ["So", "gl\u00e4nz'", "ihm", "Hoff\u00b7nung", ",", "Muth", "und", "Ruh", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Und stets, Du freundlichster der Sterne,", "tokens": ["Und", "stets", ",", "Du", "freund\u00b7lichs\u00b7ter", "der", "Ster\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PPER", "ADJA", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erheitre ihn mit Deinem Licht,", "tokens": ["Er\u00b7hei\u00b7tre", "ihn", "mit", "Dei\u00b7nem", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sag' ihm leis': auch in der Ferne", "tokens": ["Und", "sag'", "ihm", "leis'", ":", "auch", "in", "der", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$.", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vergisst Dich Deine Freundin nicht!", "tokens": ["Ver\u00b7gisst", "Dich", "Dei\u00b7ne", "Freun\u00b7din", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Du blickst so l\u00e4chelnd auf mich nieder,", "tokens": ["Du", "blickst", "so", "l\u00e4\u00b7chelnd", "auf", "mich", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du heller, lieber Abendstern,", "tokens": ["Du", "hel\u00b7ler", ",", "lie\u00b7ber", "A\u00b7bends\u00b7tern", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$,", "ADV", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Als h\u00f6rtest Du die leisen Lieder", "tokens": ["Als", "h\u00f6r\u00b7test", "Du", "die", "lei\u00b7sen", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der ahnungsvollen Schwermuth gern.", "tokens": ["Der", "ah\u00b7nungs\u00b7vol\u00b7len", "Schwer\u00b7muth", "gern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Wenn alles schl\u00e4ft, erweckt die Feier", "tokens": ["Wenn", "al\u00b7les", "schl\u00e4ft", ",", "er\u00b7weckt", "die", "Fei\u00b7er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PIS", "VVFIN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der stillen Nacht wie Melodie", "tokens": ["Der", "stil\u00b7len", "Nacht", "wie", "Me\u00b7lo\u00b7die"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KOKOM", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Sehnsucht Klage, und ihr Schleier", "tokens": ["Der", "Sehn\u00b7sucht", "Kla\u00b7ge", ",", "und", "ihr", "Schlei\u00b7er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verr\u00e4th die heissen Thr\u00e4nen nie.", "tokens": ["Ver\u00b7r\u00e4\u00b7th", "die", "heis\u00b7sen", "Thr\u00e4\u00b7nen", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.14": {"line.1": {"text": "Dann strahlst Du, holder Himmelsfunken,", "tokens": ["Dann", "strahlst", "Du", ",", "hol\u00b7der", "Him\u00b7mels\u00b7fun\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mir Trost in's kranke Herz herab,", "tokens": ["Mir", "Trost", "in's", "kran\u00b7ke", "Herz", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPRART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und es ersteht mir, wonnetrunken,", "tokens": ["Und", "es", "er\u00b7steht", "mir", ",", "won\u00b7ne\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "PWAV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Hoffnung aus der Zeiten Grab.", "tokens": ["Die", "Hoff\u00b7nung", "aus", "der", "Zei\u00b7ten", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Oft schon, wenn ich mit heissem Sehnen", "tokens": ["Oft", "schon", ",", "wenn", "ich", "mit", "heis\u00b7sem", "Seh\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Begr\u00fcsste meiner Liebe Bild,", "tokens": ["Be\u00b7gr\u00fcs\u00b7ste", "mei\u00b7ner", "Lie\u00b7be", "Bild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da lachtest Du in meine Thr\u00e4nen", "tokens": ["Da", "lach\u00b7test", "Du", "in", "mei\u00b7ne", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und machtest meinen Kummer mild.", "tokens": ["Und", "mach\u00b7test", "mei\u00b7nen", "Kum\u00b7mer", "mild", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Oft, wenn ich mich des Lebens freute,", "tokens": ["Oft", ",", "wenn", "ich", "mich", "des", "Le\u00b7bens", "freu\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da folgte mir im dunklen Hain,", "tokens": ["Da", "folg\u00b7te", "mir", "im", "dunk\u00b7len", "Hain", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Deines Himmels blauer Weite,", "tokens": ["Von", "Dei\u00b7nes", "Him\u00b7mels", "blau\u00b7er", "Wei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Freundesblick, Dein Silberschein.", "tokens": ["Wie", "Freun\u00b7des\u00b7blick", ",", "Dein", "Sil\u00b7ber\u00b7schein", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.17": {"line.1": {"text": "Und es bewegte ernst und leise", "tokens": ["Und", "es", "be\u00b7weg\u00b7te", "ernst", "und", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit wunderbarer Ahnung mich,", "tokens": ["Mit", "wun\u00b7der\u00b7ba\u00b7rer", "Ah\u00b7nung", "mich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn in dem ewig festen Gleise", "tokens"