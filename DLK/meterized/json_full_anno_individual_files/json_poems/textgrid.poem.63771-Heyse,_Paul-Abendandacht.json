{"textgrid.poem.63771": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Abendandacht", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr sollt mich nicht in eure Kreise locken,", "tokens": ["Ihr", "sollt", "mich", "nicht", "in", "eu\u00b7re", "Krei\u00b7se", "lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo, was daheim ich floh, ich wiederfinde,", "tokens": ["Wo", ",", "was", "da\u00b7heim", "ich", "floh", ",", "ich", "wie\u00b7der\u00b7fin\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "$,", "PRELS", "ADV", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In Routs, wo von den Farben schwatzt der Blinde,", "tokens": ["In", "Routs", ",", "wo", "von", "den", "Far\u00b7ben", "schwatzt", "der", "Blin\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "PWAV", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo Armut prahlt mit aufgelesnen Brocken.", "tokens": ["Wo", "Ar\u00b7mut", "prahlt", "mit", "auf\u00b7ge\u00b7les\u00b7nen", "Bro\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nie darf das rieselnde Geplauder stocken,", "tokens": ["Nie", "darf", "das", "rie\u00b7seln\u00b7de", "