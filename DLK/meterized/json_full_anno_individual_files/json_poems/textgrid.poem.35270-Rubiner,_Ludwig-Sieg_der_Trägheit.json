{"textgrid.poem.35270": {"metadata": {"author": {"name": "Rubiner, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Sieg der Tr\u00e4gheit", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der erste Blutstropfen hatte dick und schwarz die Erde erreicht.", "tokens": ["Der", "ers\u00b7te", "Bluts\u00b7trop\u00b7fen", "hat\u00b7te", "dick", "und", "schwarz", "die", "Er\u00b7de", "er\u00b7reicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "KON", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+-+-+-+--+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Das himmlische Licht war verschwunden schr\u00e4g zuckend \u00fcber die spitzen D\u00e4cher hin.", "tokens": ["Das", "himm\u00b7li\u00b7sche", "Licht", "war", "ver\u00b7schwun\u00b7den", "schr\u00e4g", "zu\u00b7ckend", "\u00fc\u00b7ber", "die", "spit\u00b7zen", "D\u00e4\u00b7cher", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "ADJD", "VVPP", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+-----+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Der Abend stieg wie Schnalzen aus dem Fett der geilen St\u00e4dte.", "tokens": ["Der", "A\u00b7bend", "stieg", "wie", "Schnal\u00b7zen", "aus", "dem", "Fett", "der", "gei\u00b7len", "St\u00e4d\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "NN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Die bleichen Lampen bissen Schatten um Herren mit Mappen unterm schwitzenden Arm,", "