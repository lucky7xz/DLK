{"dta.poem.21689": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Aristoteles Hymne an die Tugend.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die du, o heil'ge St&#228;rke, der Sterblichen", "tokens": ["Die", "du", ",", "o", "heil'\u00b7ge", "St", "&#228;", "rke", ",", "der", "Sterb\u00b7li\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "XML_entity", "word", "punct", "word", "word"], "pos": ["ART", "PPER", "$,", "FM", "ADJA", "NN", "$(", "VVFIN", "$,", "ART", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "M\u00fchselige Geschlechter zu Thaten spornst,", "tokens": ["M\u00fch\u00b7se\u00b7li\u00b7ge", "Ge\u00b7schlech\u00b7ter", "zu", "Tha\u00b7ten", "spornst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "O Tugend, unsrer schw\u00fclsten Schweisse", "tokens": ["O", "Tu\u00b7gend", ",", "uns\u00b7rer", "schw\u00fcls\u00b7ten", "Schweis\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "K\u00f6stlicher Preis und gew\u00fcnschtes Kleinod.", "tokens": ["K\u00f6st\u00b7li\u00b7cher", "Preis", "und", "ge\u00b7w\u00fcnschtes", "Klei\u00b7nod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+--+---+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Entbrannt in deiner Sch\u00f6ne, Holdselige,", "tokens": ["Ent\u00b7brannt", "in", "dei\u00b7ner", "Sch\u00f6\u00b7ne", ",", "Hold\u00b7se\u00b7li\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von deinem Blick begeistert, Jungfr\u00e4uliche,", "tokens": ["Von", "dei\u00b7nem", "Blick", "be\u00b7geis\u00b7tert", ",", "Jung\u00b7fr\u00e4u\u00b7li\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,", "NN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Trotzt Hellas Jugend jeder Arbeit,", "tokens": ["Trotzt", "Hel\u00b7las", "Ju\u00b7gend", "je\u00b7der", "Ar\u00b7beit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Lacht der Gefahr, und frohlockt im Tode.", "tokens": ["Lacht", "der", "Ge\u00b7fahr", ",", "und", "froh\u00b7lockt", "im", "To\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "KON", "ADJD", "APPRART", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "So schmeichelt nicht dem Auge des Goldes Glanz,", "tokens": ["So", "schmei\u00b7chelt", "nicht", "dem", "Au\u00b7ge", "des", "Gol\u00b7des", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "ART", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So mundet nicht dem M\u00fcden der s\u00fcsse Schlaf,", "tokens": ["So", "mun\u00b7det", "nicht", "dem", "M\u00fc\u00b7den", "der", "s\u00fcs\u00b7se", "Schlaf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "So kos't die Amme nicht dem S\u00e4ugling,", "tokens": ["So", "kos't", "die", "Am\u00b7me", "nicht", "dem", "S\u00e4ug\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie du dem ewigen Geiste kosest.", "tokens": ["Wie", "du", "dem", "e\u00b7wi\u00b7gen", "Geis\u00b7te", "ko\u00b7sest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Um deinetwillen st\u00fcrzte Heracles sich", "tokens": ["Um", "dei\u00b7net\u00b7wil\u00b7len", "st\u00fcrz\u00b7te", "He\u00b7ra\u00b7cles", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "PRF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In endelose M\u00fchen; es st\u00fcrzten sich", "tokens": ["In", "en\u00b7de\u00b7lo\u00b7se", "M\u00fc\u00b7hen", ";", "es", "st\u00fcrz\u00b7ten", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$.", "PPER", "VVFIN", "PRF"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Um deinetwillen Ledens S\u00f6hne", "tokens": ["Um", "dei\u00b7net\u00b7wil\u00b7len", "Le\u00b7dens", "S\u00f6h\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Freudigen Muthes in Noth und Arbeit.", "tokens": ["Freu\u00b7di\u00b7gen", "Mu\u00b7thes", "in", "Noth", "und", "Ar\u00b7beit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+---+-+-+-", "measure": "dactylic.init"}}, "stanza.5": {"line.1": {"text": "Nach dir verlangend, wallte Patroklos Freund", "tokens": ["Nach", "dir", "ver\u00b7lan\u00b7gend", ",", "wall\u00b7te", "Pat\u00b7ro\u00b7klos", "Freund"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "VVPP", "$,", "VVFIN", "NE", "NN"], "meter": "---+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und R\u00e4cher jung noch nieder zur Schattenwelt;", "tokens": ["Und", "R\u00e4\u00b7cher", "jung", "noch", "nie\u00b7der", "zur", "Schat\u00b7ten\u00b7welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "ADV", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nach dir verschmachtend, zu des Ais", "tokens": ["Nach", "dir", "ver\u00b7schmach\u00b7tend", ",", "zu", "des", "Ais"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "VVPP", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "D\u00fcstrer Behausung der starke Ajas.", "tokens": ["D\u00fcst\u00b7rer", "Be\u00b7hau\u00b7sung", "der", "star\u00b7ke", "A\u00b7jas", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Dein werth zu werden, Br\u00fcnstigumschlungene,", "tokens": ["Dein", "werth", "zu", "wer\u00b7den", ",", "Br\u00fcns\u00b7ti\u00b7gum\u00b7schlun\u00b7ge\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJD", "PTKZU", "VAINF", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verschm\u00e4ht' Acharnens Z\u00f6gling der Knechtschaft", "tokens": ["Ver\u00b7schm\u00e4ht'", "A\u00b7char\u00b7nens", "Z\u00f6g\u00b7ling", "der", "Knecht\u00b7schaft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "NE", "NE", "ART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Verschm\u00e4hte selbst des s\u00fcssen Lichtes", "tokens": ["Ver\u00b7schm\u00e4h\u00b7te", "selbst", "des", "s\u00fcs\u00b7sen", "Lich\u00b7tes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wonnegenuss und des Aethers Heiltrank.", "tokens": ["Won\u00b7ne\u00b7ge\u00b7nuss", "und", "des", "A\u00b7e\u00b7thers", "Heil\u00b7trank", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ART", "NN", "NN", "$."], "meter": "+--+--++-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Nur dir geb\u00fchrt der Hymnus. Es huldigen", "tokens": ["Nur", "dir", "ge\u00b7b\u00fchrt", "der", "Hym\u00b7nus", ".", "Es", "hul\u00b7di\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "ART", "NN", "$.", "PPER", "VVINF"], "meter": "-+-+-+-++--", "measure": "unknown.measure.penta"}, "line.2": {"text": "Nur dir die ew'gen T\u00f6chter Mnemosynens.", "tokens": ["Nur", "dir", "die", "ew'\u00b7gen", "T\u00f6ch\u00b7ter", "Mne\u00b7mo\u00b7sy\u00b7nens", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zevs Xenios ist ihres Liedes", "tokens": ["Zevs", "Xe\u00b7ni\u00b7os", "ist", "ih\u00b7res", "Lie\u00b7des"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Inhalt. Ihr P\u00e4an ist heil'ge Freundschaft.", "tokens": ["In\u00b7halt", ".", "Ihr", "P\u00e4\u00b7an", "ist", "heil'\u00b7ge", "Freund\u00b7schaft", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}}}}