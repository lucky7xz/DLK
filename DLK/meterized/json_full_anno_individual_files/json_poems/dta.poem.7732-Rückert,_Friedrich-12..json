{"dta.poem.7732": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Welt ist wirklich; nur ein Wirkliches allein", "tokens": ["Die", "Welt", "ist", "wirk\u00b7lich", ";", "nur", "ein", "Wirk\u00b7li\u00b7ches", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "ADV", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bringt Wirkliches hervor, Gott mu\u00df drum wirklich seyn.", "tokens": ["Bringt", "Wirk\u00b7li\u00b7ches", "her\u00b7vor", ",", "Gott", "mu\u00df", "drum", "wirk\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PTKVZ", "$,", "NN", "VMFIN", "PAV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Welt ist Leben; nur Lebendiges allein", "tokens": ["Die", "Welt", "ist", "Le\u00b7ben", ";", "nur", "Le\u00b7ben\u00b7di\u00b7ges", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "NN", "$.", "ADV", "NN", "ADV"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Kann Leben wirken, drum mu\u00df Gott lebendig seyn.", "tokens": ["Kann", "Le\u00b7ben", "wir\u00b7ken", ",", "drum", "mu\u00df", "Gott", "le\u00b7ben\u00b7dig", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "VVINF", "$,", "PAV", "VMFIN", "NN", "ADJD", "VAINF", "$."], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Der Geist des Menschen denkt; nur Denkendes allein", "tokens": ["Der", "Geist", "des", "Men\u00b7schen", "denkt", ";", "nur", "Den\u00b7ken\u00b7des", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$.", "ADV", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kann Denken schaffen, Gott mu\u00df also denkend seyn.", "tokens": ["Kann", "Den\u00b7ken", "schaf\u00b7fen", ",", "Gott", "mu\u00df", "al\u00b7so", "den\u00b7kend", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "VVINF", "$,", "NN", "VMFIN", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Des Menschen Wille will; nur Wollendes allein", "tokens": ["Des", "Men\u00b7schen", "Wil\u00b7le", "will", ";", "nur", "Wol\u00b7len\u00b7des", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VMFIN", "$.", "ADV", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kann Willen wirken, Gott mu\u00df selber wollend seyn.", "tokens": ["Kann", "Wil\u00b7len", "wir\u00b7ken", ",", "Gott", "mu\u00df", "sel\u00b7ber", "wol\u00b7lend", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "VVINF", "$,", "NN", "VMFIN", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Darum im heiligen Sanskrit, wie dir bekannt,", "tokens": ["Da\u00b7rum", "im", "hei\u00b7li\u00b7gen", "Sans\u00b7krit", ",", "wie", "dir", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "APPRART", "ADJA", "NN", "$,", "PWAV", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist er Swaiambhu, der Selbwesende, genannt;", "tokens": ["Ist", "er", "Swai\u00b7amb\u00b7hu", ",", "der", "Selb\u00b7we\u00b7sen\u00b7de", ",", "ge\u00b7nannt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "NE", "$,", "ART", "NN", "$,", "VVPP", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Der Unbedingte, der sein eignes Seyn bedingt,", "tokens": ["Der", "Un\u00b7be\u00b7ding\u00b7te", ",", "der", "sein", "eig\u00b7nes", "Seyn", "be\u00b7dingt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Selbst durch Hervorbringung der Welt hervor sich bringt.", "tokens": ["Selbst", "durch", "Her\u00b7vor\u00b7brin\u00b7gung", "der", "Welt", "her\u00b7vor", "sich", "bringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ART", "NN", "PTKVZ", "PRF", "VVFIN", "$."], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}}}}}