{"textgrid.poem.51369": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sonn ist hell und klar, auch deiner Augen Licht,", "tokens": ["Die", "Sonn", "ist", "hell", "und", "klar", ",", "auch", "dei\u00b7ner", "Au\u00b7gen", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,", "ADV", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Sonne brennt die Welt, dein Augen k\u00fchlen nicht;", "tokens": ["Die", "Son\u00b7ne", "brennt", "die", "Welt", ",", "dein", "Au\u00b7gen", "k\u00fch\u00b7len", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "PPOSAT", "NN", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Sonn ist hoch und gro\u00df, dein Augen sind erhaben;", "tokens": ["Die", "Sonn", "ist", "hoch", "und", "gro\u00df", ",", "dein", "Au\u00b7gen", "sind", "er\u00b7ha\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Sonne liebt das Gold, dein Augen goldne Gaben;", "tokens": ["Die", "Son\u00b7ne", "liebt", "das", "Gold", ",", "dein", "Au\u00b7gen", "gold\u00b7ne", "Ga\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ist gleich die Sonne so, doch sol sie fleckicht seyn;", "tokens": ["Ist", "gleich", "die", "Son\u00b7ne", "so", ",", "doch", "sol", "sie", "flec\u00b7kicht", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADV", "$,", "ADV", "VMFIN", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sind gleich dein Augen so, ist doch was falsch ihr Schein.", "tokens": ["Sind", "gleich", "dein", "Au\u00b7gen", "so", ",", "ist", "doch", "was", "falsch", "ihr", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "ADV", "$,", "VAFIN", "ADV", "PWS", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Sonn ist hell und klar, auch deiner Augen Licht,", "tokens": ["Die", "Sonn", "ist", "hell", "und", "klar", ",", "auch", "dei\u00b7ner", "Au\u00b7gen", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,", "ADV", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Sonne brennt die Welt, dein Augen k\u00fchlen nicht;", "tokens": ["Die", "Son\u00b7ne", "brennt", "die", "Welt", ",", "dein", "Au\u00b7gen", "k\u00fch\u00b7len", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "PPOSAT", "NN", "VVFIN", "PTKNEG", "$."], "meter": "-