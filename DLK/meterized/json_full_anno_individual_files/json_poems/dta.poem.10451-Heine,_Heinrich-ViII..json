{"dta.poem.10451": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "ViII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Du sch\u00f6nes Fischerm\u00e4dchen,", "tokens": ["Du", "sch\u00f6\u00b7nes", "Fi\u00b7scher\u00b7m\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Treibe den Kahn an's Land;", "tokens": ["Trei\u00b7be", "den", "Kahn", "an's", "Land", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Komm zu mir und setze dich nieder,", "tokens": ["Komm", "zu", "mir", "und", "set\u00b7ze", "dich", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Wir kosen Hand in Hand.", "tokens": ["Wir", "ko\u00b7sen", "Hand", "in", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Leg' an mein Herz dein K\u00f6pfchen.", "tokens": ["Leg'", "an", "mein", "Herz", "dein", "K\u00f6pf\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und f\u00fcrchte dich nicht zu sehr,", "tokens": ["Und", "f\u00fcrch\u00b7te", "dich", "nicht", "zu", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "PTKA", "ADV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Vertrau'st du dich doch sorglos", "tokens": ["Ver\u00b7trau'st", "du", "dich", "doch", "sorg\u00b7los"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "ADJD"], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}, "line.4": {"text": "T\u00e4glich dem wilden Meer.", "tokens": ["T\u00e4g\u00b7lich", "dem", "wil\u00b7den", "Meer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Mein Herz gleicht ganz dem Meere,", "tokens": ["Mein", "Herz", "gleicht", "ganz", "dem", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hat Sturm und Ebb' und Fluth,", "tokens": ["Hat", "Sturm", "und", "Ebb'", "und", "Fluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und manche sch\u00f6ne Perle", "tokens": ["Und", "man\u00b7che", "sch\u00f6\u00b7ne", "Per\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In seiner Tiefe ruht.", "tokens": ["In", "sei\u00b7ner", "Tie\u00b7fe", "ruht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}