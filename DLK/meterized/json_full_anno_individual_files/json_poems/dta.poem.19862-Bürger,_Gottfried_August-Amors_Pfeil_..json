{"dta.poem.19862": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Amors Pfeil .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1778", "urn": "urn:nbn:de:kobv:b4-20090519672", "language": ["de:0.99"], "booktitle": "B\u00fcrger, Gottfried August: Gedichte. G\u00f6ttingen, 1778."}, "poem": {"stanza.1": {"line.1": {"text": "Amors Pfeil hat Widerspizen.             ", "tokens": ["A\u00b7mors", "Pfeil", "hat", "Wi\u00b7der\u00b7spi\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wen er traf, der lass\u2019 ihn sizen,", "tokens": ["Wen", "er", "traf", ",", "der", "lass'", "ihn", "si\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PRELS", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und erduld\u2019 ein wenig Schmerz!", "tokens": ["Und", "er\u00b7duld'", "ein", "we\u00b7nig", "Schmerz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Welcher meinen Rath verachtet,", "tokens": ["Wel\u00b7cher", "mei\u00b7nen", "Rath", "ver\u00b7ach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und ihn auszureissen trachtet,", "tokens": ["Und", "ihn", "aus\u00b7zu\u00b7reis\u00b7sen", "trach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVIZU", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der zerfleischet ganz sein Herz.", "tokens": ["Der", "zer\u00b7flei\u00b7schet", "ganz", "sein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}