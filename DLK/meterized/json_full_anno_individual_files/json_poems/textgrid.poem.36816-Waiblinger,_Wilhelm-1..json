{"textgrid.poem.36816": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verschied'nes Lob ist jedem. Mir sei der Kranz,", "tokens": ["Ver\u00b7schie\u00b7d'\u00b7nes", "Lob", "ist", "je\u00b7dem", ".", "Mir", "sei", "der", "Kranz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PIS", "$.", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der weinlaubduft'ge, den mir die G\u00f6tterhand", "tokens": ["Der", "wein\u00b7laub\u00b7duft'\u00b7ge", ",", "den", "mir", "die", "G\u00f6t\u00b7ter\u00b7hand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "PRELS", "PPER", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Des holden sch\u00f6pferischen J\u00fcnglings", "tokens": ["Des", "hol\u00b7den", "sch\u00f6p\u00b7fe\u00b7ri\u00b7schen", "J\u00fcng\u00b7lings"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dr\u00fcckt in die Schl\u00e4fe, mir sei Begeist'rung!", "tokens": ["Dr\u00fcckt", "in", "die", "Schl\u00e4\u00b7fe", ",", "mir", "sei", "Be\u00b7geist'\u00b7rung", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "PPER", "VAFIN", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Sei's, da\u00df verbl\u00fchter Fr\u00fchlinge Liebeslust", "tokens": ["Sei's", ",", "da\u00df", "ver\u00b7bl\u00fch\u00b7ter", "Fr\u00fch\u00b7lin\u00b7ge", "Lie\u00b7bes\u00b7lust"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "ADJA", "NN", "NN"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Voll Nachtigallenstimmen, voll M\u00e4dchenreiz,", "tokens": ["Voll", "Nach\u00b7ti\u00b7gal\u00b7len\u00b7stim\u00b7men", ",", "voll", "M\u00e4d\u00b7chen\u00b7reiz", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sei's, da\u00df der traur'gen Herbste Schwermuth", "tokens": ["Sei's", ",", "da\u00df", "der", "traur'\u00b7gen", "Herbs\u00b7te", "Schwer\u00b7muth"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wieder ins klagende Herz zur\u00fcckkehrt:", "tokens": ["Wie\u00b7der", "ins", "kla\u00b7gen\u00b7de", "Herz", "zu\u00b7r\u00fcck\u00b7kehrt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Sei's, da\u00df Neapels Inseln der Fabel Duft,", "tokens": ["Sei's", ",", "da\u00df", "Nea\u00b7pels", "In\u00b7seln", "der", "Fa\u00b7bel", "Duft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "NE", "NN", "ART", "NN", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Und der Geschichte lebenerweckender", "tokens": ["Und", "der", "Ge\u00b7schich\u00b7te", "le\u00b7be\u00b7ner\u00b7we\u00b7cken\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Gluthvoller Hauch mit Morgenr\u00f6then,", "tokens": ["Gluth\u00b7vol\u00b7ler", "Hauch", "mit", "Mor\u00b7gen\u00b7r\u00f6\u00b7then", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Str\u00f6men von purpurnem Blut verkl\u00e4re,", "tokens": ["Str\u00f6\u00b7men", "von", "pur\u00b7pur\u00b7nem", "Blut", "ver\u00b7kl\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Da\u00df in Sorrents Orangengeruch, am Fels,", "tokens": ["Da\u00df", "in", "Sor\u00b7rents", "O\u00b7ran\u00b7gen\u00b7ge\u00b7ruch", ",", "am", "Fels", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "NN", "$,", "APPRART", "NN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Den mir die Fluthen klarer als Aug' und Herz", "tokens": ["Den", "mir", "die", "Flut\u00b7hen", "kla\u00b7rer", "als", "Aug'", "und", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "ADJD", "KOKOM", "NN", "KON", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Des reinsten Engels wiederstrahlen,", "tokens": ["Des", "reins\u00b7ten", "En\u00b7gels", "wie\u00b7der\u00b7strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Tasso's gereinigter Geist mir aufsteigt,", "tokens": ["Tas\u00b7so's", "ge\u00b7rei\u00b7nig\u00b7ter", "Geist", "mir", "auf\u00b7steigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.5": {"line.1": {"text": "Da\u00df mir des Dreizacks schrecklicher Gott am Strand", "tokens": ["Da\u00df", "mir", "des", "Drei\u00b7zacks", "schreck\u00b7li\u00b7cher", "Gott", "am", "Strand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Tyrrhen'schen Meers der S\u00e4ulen gigant'sche Pracht,", "tokens": ["Tyr\u00b7rhen'\u00b7schen", "Meers", "der", "S\u00e4u\u00b7len", "gi\u00b7gant'\u00b7sche", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Den Tempelbau mir zeigt, der ewig", "tokens": ["Den", "Tem\u00b7pel\u00b7bau", "mir", "zeigt", ",", "der", "e\u00b7wig"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,", "PRELS", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie das unsterbliche Element ist.", "tokens": ["Wie", "das", "uns\u00b7terb\u00b7li\u00b7che", "E\u00b7le\u00b7ment", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Stets f\u00fchl' ich mir das gl\u00fchende Herz bewegt:", "tokens": ["Stets", "f\u00fchl'", "ich", "mir", "das", "gl\u00fc\u00b7hen\u00b7de", "Herz", "be\u00b7wegt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dem Gold vergleich' ich seine Gedanken, die", "tokens": ["Dem", "Gold", "ver\u00b7gleich'", "ich", "sei\u00b7ne", "Ge\u00b7dan\u00b7ken", ",", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "PRELS"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Erst roh und unrein, endlich lauter", "tokens": ["Erst", "roh", "und", "un\u00b7rein", ",", "end\u00b7lich", "lau\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "KON", "NN", "$,", "ADV", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus der Begeisterung Flamme springen.", "tokens": ["Aus", "der", "Be\u00b7geis\u00b7te\u00b7rung", "Flam\u00b7me", "sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Dann nicht der Erde kleinliche Sorgen mehr,", "tokens": ["Dann", "nicht", "der", "Er\u00b7de", "klein\u00b7li\u00b7che", "Sor\u00b7gen", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "NN", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Noth unbeugsam dr\u00fcckende Kraft, den Sieg", "tokens": ["Der", "Noth", "un\u00b7beug\u00b7sam", "dr\u00fc\u00b7cken\u00b7de", "Kraft", ",", "den", "Sieg"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADJD", "ADJA", "NN", "$,", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nur f\u00fchl' ich, den ich mir erk\u00e4mpfe,", "tokens": ["Nur", "f\u00fchl'", "ich", ",", "den", "ich", "mir", "er\u00b7k\u00e4mp\u00b7fe", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fchle den Stolz nur des nahen Lorbeers.", "tokens": ["F\u00fch\u00b7le", "den", "Stolz", "nur", "des", "na\u00b7hen", "Lor\u00b7beers", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Schon in den Bl\u00fcthen ehrt man die Frucht. Am Grab", "tokens": ["Schon", "in", "den", "Bl\u00fc\u00b7then", "ehrt", "man", "die", "Frucht", ".", "Am", "Grab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "PIS", "ART", "NN", "$.", "APPRART", "NN"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Achills einst stand der junge Eroberer", "tokens": ["A\u00b7chills", "einst", "stand", "der", "jun\u00b7ge", "Er\u00b7o\u00b7be\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+--+-+--+--", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und weint': in Einer Thr\u00e4ne gl\u00e4nzten", "tokens": ["Und", "weint'", ":", "in", "Ei\u00b7ner", "Thr\u00e4\u00b7ne", "gl\u00e4nz\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Alle Triumphe der k\u00fcnft'gen Hoheit.", "tokens": ["Al\u00b7le", "Tri\u00b7um\u00b7phe", "der", "k\u00fcnft'\u00b7gen", "Ho\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Blind treibt der Gott, der innre, beseelende,", "tokens": ["Blind", "treibt", "der", "Gott", ",", "der", "inn\u00b7re", ",", "be\u00b7see\u00b7len\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "$,", "ADJA", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So in der Knospe, da\u00df sie zur Rose sich", "tokens": ["So", "in", "der", "Knos\u00b7pe", ",", "da\u00df", "sie", "zur", "Ro\u00b7se", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "KOUS", "PPER", "APPRART", "NN", "PRF"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Entfalte, wie im Menschenherzen,", "tokens": ["Ent\u00b7fal\u00b7te", ",", "wie", "im", "Men\u00b7schen\u00b7her\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df es zu h\u00f6herem Wort sich \u00f6ffne.", "tokens": ["Da\u00df", "es", "zu", "h\u00f6\u00b7he\u00b7rem", "Wort", "sich", "\u00f6ff\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "Der Berg Vesuv auch, wenn ihn des Feuers Strom,", "tokens": ["Der", "Berg", "Ve\u00b7suv", "auch", ",", "wenn", "ihn", "des", "Feu\u00b7ers", "Strom", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dem Weine glei