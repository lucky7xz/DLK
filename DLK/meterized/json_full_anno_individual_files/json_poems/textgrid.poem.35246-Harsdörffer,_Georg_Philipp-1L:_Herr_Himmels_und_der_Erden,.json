POSAT", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Durch deine Gnad' und G\u00fcte", "tokens": ["Durch", "dei\u00b7ne", "Gnad'", "und", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mich allezeit beh\u00fcte", "tokens": ["Mich", "al\u00b7le\u00b7zeit", "be\u00b7h\u00fc\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sammt dieser Stadt und Land.", "tokens": ["Sammt", "die\u00b7ser", "Stadt", "und", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Dein' Engel zu mir sende,", "tokens": ["Dein'", "En\u00b7gel", "zu", "mir", "sen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df ihre Hand abwende,", "tokens": ["Da\u00df", "ih\u00b7re", "Hand", "ab\u00b7wen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was Seel' und Leib versehrt.", "tokens": ["Was", "Seel'", "und", "Leib", "ver\u00b7sehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "La\u00df meine S\u00fcnd' vers\u00f6hnen,", "tokens": ["La\u00df", "mei\u00b7ne", "S\u00fcnd'", "ver\u00b7s\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gleich wie des Thaues Thr\u00e4nen", "tokens": ["Gleich", "wie", "des", "Thau\u00b7es", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Sonnen Glanz verzehrt.", "tokens": ["Der", "Son\u00b7nen", "Glanz", "ver\u00b7zehrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}