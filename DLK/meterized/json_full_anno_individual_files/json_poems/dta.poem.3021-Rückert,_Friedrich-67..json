{"dta.poem.3021": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "67.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du f\u00fchlst, du bist aus Gott, doch hast du nicht vernommen,", "tokens": ["Du", "f\u00fchlst", ",", "du", "bist", "aus", "Gott", ",", "doch", "hast", "du", "nicht", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "APPR", "NN", "$,", "ADV", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie, wenn, warum, wozu du bist aus ihm gekommen.", "tokens": ["Wie", ",", "wenn", ",", "wa\u00b7rum", ",", "wo\u00b7zu", "du", "bist", "aus", "ihm", "ge\u00b7kom\u00b7men", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KOUS", "$,", "PWAV", "$,", "PWAV", "PPER", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ob du von ihm verbannt, ob von ihm ausgesandt,", "tokens": ["Ob", "du", "von", "ihm", "ver\u00b7bannt", ",", "ob", "von", "ihm", "aus\u00b7ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVPP", "$,", "KOUS", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob ausgewandert bist, es ist dir unbekannt.", "tokens": ["Ob", "aus\u00b7ge\u00b7wan\u00b7dert", "bist", ",", "es", "ist", "dir", "un\u00b7be\u00b7kannt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVPP", "VAFIN", "$,", "PPER", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Bist du verbannt, so wird er die Verbannung wenden;", "tokens": ["Bist", "du", "ver\u00b7bannt", ",", "so", "wird", "er", "die", "Ver\u00b7ban\u00b7nung", "wen\u00b7den", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "ADV", "VAFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bist du gesandt, so wird er wieder dich besenden.", "tokens": ["Bist", "du", "ge\u00b7sandt", ",", "so", "wird", "er", "wie\u00b7der", "dich", "be\u00b7sen\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "ADV", "VAFIN", "PPER", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Bist du gewandert, wird die Wanderlust vergehn,", "tokens": ["Bist", "du", "ge\u00b7wan\u00b7dert", ",", "wird", "die", "Wan\u00b7der\u00b7lust", "ver\u00b7gehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und deine Heimat wirst du freudig wiedersehn.", "tokens": ["Und", "dei\u00b7ne", "Hei\u00b7mat", "wirst", "du", "freu\u00b7dig", "wie\u00b7der\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}