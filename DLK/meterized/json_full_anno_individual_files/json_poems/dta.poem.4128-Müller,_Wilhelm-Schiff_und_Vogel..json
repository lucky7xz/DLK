{"dta.poem.4128": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Schiff und Vogel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vor\u00fcber an Burgen und St\u00e4dten,", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "an", "Bur\u00b7gen", "und", "St\u00e4d\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die Winde blasen hinterher", "tokens": ["Die", "Win\u00b7de", "bla\u00b7sen", "hin\u00b7ter\u00b7her"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit lustigen Trompeten.", "tokens": ["Mit", "lus\u00b7ti\u00b7gen", "Trom\u00b7pe\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Wolken ziehen hoch voran,", "tokens": ["Die", "Wol\u00b7ken", "zie\u00b7hen", "hoch", "vo\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir V\u00f6glein mitten drinnen,", "tokens": ["Wir", "V\u00f6\u00b7glein", "mit\u00b7ten", "drin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und Alles, was fliegen und singen kann,", "tokens": ["Und", "Al\u00b7les", ",", "was", "flie\u00b7gen", "und", "sin\u00b7gen", "kann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PWS", "VVINF", "KON", "VVINF", "VMFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Nur nach, nur mit uns, nur von hinnen!", "tokens": ["Nur", "nach", ",", "nur", "mit", "uns", ",", "nur", "von", "hin\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "ADV", "APPR", "PPER", "$,", "ADV", "APPR", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich gr\u00fc\u00dfe dich, Schifflein! Wohin, woher,", "tokens": ["Ich", "gr\u00fc\u00b7\u00dfe", "dich", ",", "Schif\u00b7flein", "!", "Wo\u00b7hin", ",", "wo\u00b7her", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "NN", "$.", "PWAV", "$,", "PWAV", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Mit dem flatternden goldenen Bande?", "tokens": ["Mit", "dem", "flat\u00b7tern\u00b7den", "gol\u00b7de\u00b7nen", "Ban\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "\u00abich gr\u00fc\u00dfe dich, V\u00f6glein! In's weite Meer", "tokens": ["\u00ab", "ich", "gr\u00fc\u00b7\u00dfe", "dich", ",", "V\u00f6\u00b7glein", "!", "In's", "wei\u00b7te", "Meer"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "PPER", "$,", "NN", "$.", "APPRART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "\u00abfahr' ich hin aus dem engen Lande.", "tokens": ["\u00ab", "fahr'", "ich", "hin", "aus", "dem", "en\u00b7gen", "Lan\u00b7de", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-----+-+-", "measure": "unknown.measure.di"}}, "stanza.4": {"line.1": {"text": "\u00aball' meine Segel sind geschwellt,", "tokens": ["\u00ab", "all'", "mei\u00b7ne", "Se\u00b7gel", "sind", "ge\u00b7schwellt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIAT", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00abkein Berg ist mehr zu sehen:", "tokens": ["\u00ab", "kein", "Berg", "ist", "mehr", "zu", "se\u00b7hen", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIAT", "NN", "VAFIN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00abich hab' mein' Sach' auf den Wind gestellt,", "tokens": ["\u00ab", "ich", "hab'", "mein'", "Sach'", "auf", "den", "Wind", "ge\u00b7stellt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "\u00abder Wind l\u00e4\u00dft mich nicht stehen.", "tokens": ["\u00ab", "der", "Wind", "l\u00e4\u00dft", "mich", "nicht", "ste\u00b7hen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u00abund willst du, V\u00f6glein, mit hinaus,", "tokens": ["\u00ab", "und", "willst", "du", ",", "V\u00f6\u00b7glein", ",", "mit", "hin\u00b7aus", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "KON", "VMFIN", "PPER", "$,", "NN", "$,", "APPR", "APZR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00abmagst dich auf den Mastbaum stellen;", "tokens": ["\u00ab", "magst", "dich", "auf", "den", "Mast\u00b7baum", "stel\u00b7len", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00abdenn voll zum Sinken ist mein Haus", "tokens": ["\u00ab", "denn", "voll", "zum", "Sin\u00b7ken", "ist", "mein", "Haus"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "KON", "ADJD", "APPRART", "NN", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00abvon gl\u00fccklichen Gesellen.", "tokens": ["\u00ab", "von", "gl\u00fcck\u00b7li\u00b7chen", "Ge\u00b7sel\u00b7len", "."], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u00absie tanzen und springen den ganzen Tag,", "tokens": ["\u00ab", "sie", "tan\u00b7zen", "und", "sprin\u00b7gen", "den", "gan\u00b7zen", "Tag", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVINF", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "\u00abund klimpern und spielen und trinken,", "tokens": ["\u00ab", "und", "klim\u00b7pern", "und", "spie\u00b7len", "und", "trin\u00b7ken", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVINF", "KON", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "\u00abund wer nicht mehr tanzen und trinken mag,", "tokens": ["\u00ab", "und", "wer", "nicht", "mehr", "tan\u00b7zen", "und", "trin\u00b7ken", "mag", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PWS", "PTKNEG", "ADV", "VVINF", "KON", "VVINF", "VMFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "\u00abseiner Nachbarin mu\u00df er winken.\u00bb", "tokens": ["\u00ab", "sei\u00b7ner", "Nach\u00b7ba\u00b7rin", "mu\u00df", "er", "win\u00b7ken", ".", "\u00bb"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$.", "$("], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Gesellen, die brauch' ich und such' ich nicht,", "tokens": ["Ge\u00b7sel\u00b7len", ",", "die", "brauch'", "ich", "und", "such'", "ich", "nicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Lieb Schifflein, ich kann ja noch singen;", "tokens": ["Lieb", "Schif\u00b7flein", ",", "ich", "kann", "ja", "noch", "sin\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PPER", "VMFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dem Mastbaum w\u00e4r' ich ein b\u00f6ses Gewicht,", "tokens": ["Dem", "Mast\u00b7baum", "w\u00e4r'", "ich", "ein", "b\u00f6\u00b7ses", "Ge\u00b7wicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Lieb Schifflein, ich habe ja Schwingen.", "tokens": ["Lieb", "Schif\u00b7flein", ",", "ich", "ha\u00b7be", "ja", "Schwin\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PPER", "VAFIN", "ADV", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.8": {"line.1": {"text": "Hoch \u00fcber dem Segel, hoch \u00fcber dem Mast,", "tokens": ["Hoch", "\u00fc\u00b7ber", "dem", "Se\u00b7gel", ",", "hoch", "\u00fc\u00b7ber", "dem", "Mast", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wer will mir die Lust verwehren?", "tokens": ["Wer", "will", "mir", "die", "Lust", "ver\u00b7weh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und h\u00e4lt deine wilde Gesellschaft Rast,", "tokens": ["Und", "h\u00e4lt", "dei\u00b7ne", "wil\u00b7de", "Ge\u00b7sell\u00b7schaft", "Rast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "So sollst du mich singen h\u00f6ren.", "tokens": ["So", "sollst", "du", "mich", "sin\u00b7gen", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Und wer nicht ruhen und horchen mag,", "tokens": ["Und", "wer", "nicht", "ru\u00b7hen", "und", "hor\u00b7chen", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PTKNEG", "VVINF", "KON", "VVINF", "VMFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Gott gesegn' ihm die bessere Freude!", "tokens": ["Gott", "ge\u00b7segn'", "ihm", "die", "bes\u00b7se\u00b7re", "Freu\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "So schwing' ich mich auf in den blauen Tag,", "tokens": ["So", "schwing'", "ich", "mich", "auf", "in", "den", "blau\u00b7en", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In die goldene Sonnenweide.", "tokens": ["In", "die", "gol\u00b7de\u00b7ne", "Son\u00b7nen\u00b7wei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.10": {"line.1": {"text": "So sing' ich meinen Jubelgesang", "tokens": ["So", "sing'", "ich", "mei\u00b7nen", "Ju\u00b7bel\u00b7ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Hinaus in alle vier Winde,", "tokens": ["Hin\u00b7aus", "in", "al\u00b7le", "vier", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "CARD", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da\u00df ihn mein und sein Lebelang", "tokens": ["Da\u00df", "ihn", "mein", "und", "sein", "Le\u00b7be\u00b7lang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "KON", "PPOSAT", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Kein Schreiber und Drucker finde!", "tokens": ["Kein", "Schrei\u00b7ber", "und", "Dru\u00b7cker", "fin\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}