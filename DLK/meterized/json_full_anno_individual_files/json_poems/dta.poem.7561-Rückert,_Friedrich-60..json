{"dta.poem.7561": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "60.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vergeistigen die Welt ist geistiges Ergetzen,", "tokens": ["Ver\u00b7geis\u00b7ti\u00b7gen", "die", "Welt", "ist", "geis\u00b7ti\u00b7ges", "Er\u00b7get\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch ein entsetzliches, sie nur durch Geist zersetzen.", "tokens": ["Doch", "ein", "ent\u00b7setz\u00b7li\u00b7ches", ",", "sie", "nur", "durch", "Geist", "zer\u00b7set\u00b7zen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "$,", "PPER", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Schad' um die sch\u00f6ne Welt, wenn sie hinweg nur thaut", "tokens": ["Schad'", "um", "die", "sch\u00f6\u00b7ne", "Welt", ",", "wenn", "sie", "hin\u00b7weg", "nur", "thaut"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,", "KOUS", "PPER", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Geist, und nicht daraus mir eine sch\u00f6nre baut;", "tokens": ["Der", "Geist", ",", "und", "nicht", "da\u00b7raus", "mir", "ei\u00b7ne", "sch\u00f6n\u00b7re", "baut", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "PTKNEG", "PAV", "PPER", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wie Wintersonnenstral Frostblumen nur zerthauen,", "tokens": ["Wie", "Win\u00b7ter\u00b7son\u00b7nen\u00b7stral", "Frost\u00b7blu\u00b7men", "nur", "zer\u00b7thau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch Fr\u00fchlingsblumen nicht kann wecken auf den Auen.", "tokens": ["Doch", "Fr\u00fch\u00b7lings\u00b7blu\u00b7men", "nicht", "kann", "we\u00b7cken", "auf", "den", "Au\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKNEG", "VMFIN", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}