{"dta.poem.20392": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Die dreyfache gl\u00fccks- und ehren-krone/  \n bey dem seligen hintritt Frauen Catharina von der  \n Lith/ gebohrner Wesenfeldin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als Franckreich seinen sohn den Pohlen wieder nahm/\nUnd Heinrich durch die flucht zum zweyten throne kam/\nDa warff er die vernunfft erst auff der welt get\u00fcmmel/\nNahm Solons lehren an/ die er dem Cr\u00f6sus gab/\nUnd ri\u00df auff reinen grund drey g\u00f6ldne kronen ab/\nMit dieser \u00fcberschrifft: die dritte bleibt im himmel.", "tokens": ["Als", "Fran\u00b7ck\u00b7reich", "sei\u00b7nen", "sohn", "den", "Poh\u00b7len", "wie\u00b7der", "nahm", "/", "Und", "Hein\u00b7rich", "durch", "die", "flucht", "zum", "zwey\u00b7ten", "thro\u00b7ne", "kam", "/", "Da", "warff", "er", "die", "ver\u00b7nunfft", "erst", "auff", "der", "welt", "ge\u00b7t\u00fcm\u00b7mel", "/", "Nahm", "So\u00b7lons", "leh\u00b7ren", "an", "/", "die", "er", "dem", "Cr\u00f6\u00b7sus", "gab", "/", "Und", "ri\u00df", "auff", "rei\u00b7nen", "grund", "drey", "g\u00f6ld\u00b7ne", "kro\u00b7nen", "ab", "/", "Mit", "die\u00b7ser", "\u00fc\u00b7bersc\u00b7hrifft", ":", "die", "drit\u00b7te", "bleibt", "im", "him\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPOSAT", "NN", "ART", "NN", "ADV", "VVFIN", "$(", "KON", "NE", "APPR", "ART", "VVFIN", "APPRART", "ADJA", "NN", "VVFIN", "$(", "ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "APPR", "ART", "NN", "NN", "$(", "NN", "NE", "VVFIN", "PTKVZ", "$(", "PRELS", "PPER", "ART", "NE", "VVFIN", "$(", "KON", "VVFIN", "APPR", "ADJA", "NN", "CARD", "ADJA", "NN", "PTKVZ", "$(", "APPR", "PDAT", "VVPP", "$.", "ART", "ADJA", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+--+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Betr\u00fcbte! darff ich mich zu r\u00fchmen unterstehn/", "tokens": ["Be\u00b7tr\u00fcb\u00b7te", "!", "darff", "ich", "mich", "zu", "r\u00fch\u00b7men", "un\u00b7ter\u00b7stehn", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VMFIN", "PPER", "PRF", "PTKZU", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was jenem Pohlen war/ das war ihr jungfer-stand/", "tokens": ["Was", "je\u00b7nem", "Poh\u00b7len", "war", "/", "das", "war", "ihr", "jung\u00b7fer\u00b7stand", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "VAFIN", "$(", "PDS", "VAFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch diese krone brach die flamme der natur/", "tokens": ["Doch", "die\u00b7se", "kro\u00b7ne", "brach", "die", "flam\u00b7me", "der", "na\u00b7tur", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVFIN", "ART", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wie wenn ein glimmend feur auff einmahl lufft erh\u00e4lt/", "tokens": ["Wie", "wenn", "ein", "glim\u00b7mend", "feur", "auff", "ein\u00b7mahl", "lufft", "er\u00b7h\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "ART", "ADJD", "NN", "APPR", "ADV", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ihr pol-stern war allein ihr allerliebster Lith/", "tokens": ["Ihr", "pol\u00b7stern", "war", "al\u00b7lein", "ihr", "al\u00b7ler\u00b7liebs\u00b7ter", "Lith", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VAFIN", "ADV", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Was Artemisia/ was Portia gethan/", "tokens": ["Was", "Ar\u00b7te\u00b7mi\u00b7sia", "/", "was", "Por\u00b7tia", "ge\u00b7than", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "$(", "PWS", "NE", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Denn statt der aschen tranck sie Christi freuden-wein/", "tokens": ["Denn", "statt", "der", "asc\u00b7hen", "tranck", "sie", "Chris\u00b7ti", "freu\u00b7den\u00b7wein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "PPER", "NE", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Wie sie sein hertze nun mit zucker \u00fcberstreut/", "tokens": ["Wie", "sie", "sein", "hert\u00b7ze", "nun", "mit", "zu\u00b7cker", "\u00fc\u00b7bers\u00b7treut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "ADV", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Sie wuste/ da\u00df um klee und gr\u00fcnen ro\u00dfmarin/", "tokens": ["Sie", "wus\u00b7te", "/", "da\u00df", "um", "klee", "und", "gr\u00fc\u00b7nen", "ro\u00df\u00b7ma\u00b7rin", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "APPR", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Das gl\u00fccke dieser welt und seiner ehren-bahn/", "tokens": ["Das", "gl\u00fc\u00b7cke", "die\u00b7ser", "welt", "und", "sei\u00b7ner", "eh\u00b7ren\u00b7bahn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PDAT", "NN", "KON", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Und endlich gab ihr leib der erden gute nacht/", "tokens": ["Und", "end\u00b7lich", "gab", "ihr", "leib", "der", "er\u00b7den", "gu\u00b7te", "nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Da\u00df er/ Hoch-Edler/ nun in thr\u00e4nen-saltze schwimmt/", "tokens": ["Da\u00df", "er", "/", "Hoch\u00b7Ed\u00b7ler", "/", "nun", "in", "thr\u00e4\u00b7nen\u00b7salt\u00b7ze", "schwimmt", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$(", "NE", "$(", "ADV", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.14": {"line.1": {"text": "Hier steht sein werthes haus/ und klaget seine frau/", "tokens": ["Hier", "steht", "sein", "wert\u00b7hes", "haus", "/", "und", "kla\u00b7get", "sei\u00b7ne", "frau", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$(", "KON", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.15": {"line.1": {"text": "Und darum glaub ich leicht/ wie seine seele schwitzt/", "tokens": ["Und", "da\u00b7rum", "glaub", "ich", "leicht", "/", "wie", "sei\u00b7ne", "see\u00b7le", "schwitzt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "PPER", "ADJD", "$(", "KOKOM", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.16": {"line.1": {"text": "Was aber bringen uns die thr\u00e4nen endlich ein?", "tokens": ["Was", "a\u00b7ber", "brin\u00b7gen", "uns", "die", "thr\u00e4\u00b7nen", "end\u00b7lich", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.17": {"line.1": {"text": "Es lebt die selige nun aller angst befreyt/", "tokens": ["Es", "lebt", "die", "se\u00b7li\u00b7ge", "nun", "al\u00b7ler", "angst", "be\u00b7freyt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "ADV", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.18": {"line.1": {"text": "Gesetzt/ da\u00df S\u00fcdland nun gesunder l\u00fcffte sey;", "tokens": ["Ge\u00b7setzt", "/", "da\u00df", "S\u00fcd\u00b7land", "nun", "ge\u00b7sun\u00b7der", "l\u00fcff\u00b7te", "sey", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$(", "KOUS", "NN", "ADV", "ADJD", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.19": {"line.1": {"text": "Denn ihre burg ist nun der thron der ewigkeit/", "tokens": ["Denn", "ih\u00b7re", "burg", "ist", "nun", "der", "thron", "der", "e\u00b7wig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.20": {"line.1": {"text": "Dr\u00fcm zieht/ betr\u00fcbteste/ die schwere thr\u00e4nen ein/", "tokens": ["Dr\u00fcm", "zieht", "/", "be\u00b7tr\u00fcb\u00b7tes\u00b7te", "/", "die", "schwe\u00b7re", "thr\u00e4\u00b7nen", "ein", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$(", "VVFIN", "$(", "ART", "ADJA", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}