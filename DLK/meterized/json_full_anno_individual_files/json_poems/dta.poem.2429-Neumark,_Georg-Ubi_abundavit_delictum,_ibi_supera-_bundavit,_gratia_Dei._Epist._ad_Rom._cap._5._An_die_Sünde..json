{"dta.poem.2429": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Ubi abundavit delictum, ibi supera-  \n bundavit, gratia Dei.  Epist. ad Rom. cap.  5.  \n An die S\u00fcnde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Du wilst gleich oder nicht du must doch unten liegen/", "tokens": ["Du", "wilst", "gleich", "o\u00b7der", "nicht", "du", "must", "doch", "un\u00b7ten", "lie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "KON", "PTKNEG", "PPER", "VMFIN", "ADV", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du Feindin\u0303 Gottes du mit deinem gantzen Heer\u2019/", "tokens": ["Du", "Fein\u00b7di\u00f1", "Got\u00b7tes", "du", "mit", "dei\u00b7nem", "gant\u00b7zen", "Heer'", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "NN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ach bilde dir nicht ein da\u00df du wilst etwa siegen/", "tokens": ["Ach", "bil\u00b7de", "dir", "nicht", "ein", "da\u00df", "du", "wilst", "et\u00b7wa", "sie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "PTKNEG", "ART", "KOUS", "PPER", "VMFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und wenn auch deine Macht noch zehnmal st\u00e4r-", "tokens": ["Und", "wenn", "auch", "dei\u00b7ne", "Macht", "noch", "zehn\u00b7mal", "st\u00e4r"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "PPOSAT", "NN", "ADV", "ADV", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "ker wer'/", "tokens": ["ker", "wer'", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$("], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Es ist doch nur \u00fcmsonst mit Gottes Gnade kriegen", "tokens": ["Es", "ist", "doch", "nur", "\u00fcm\u00b7sonst", "mit", "Got\u00b7tes", "Gna\u00b7de", "krie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "APPR", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die mit im Streiten ist/ die tausendmal noch mehr/", "tokens": ["Die", "mit", "im", "Strei\u00b7ten", "ist", "/", "die", "tau\u00b7send\u00b7mal", "noch", "mehr", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "APPRART", "NN", "VAFIN", "$(", "ART", "ADV", "ADV", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und m\u00e4chtiger als du. Steht diese mir zur Rechten", "tokens": ["Und", "m\u00e4ch\u00b7ti\u00b7ger", "als", "du", ".", "Steht", "die\u00b7se", "mir", "zur", "Rech\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "KOUS", "PPER", "$.", "VVFIN", "PDAT", "PPER", "APPRART", "NN"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "So wil ich ritterlich mit dir her\u00fcmmer fechten.", "tokens": ["So", "wil", "ich", "rit\u00b7ter\u00b7lich", "mit", "dir", "he\u00b7r\u00fcm\u00b7mer", "fech\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "APPR", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}