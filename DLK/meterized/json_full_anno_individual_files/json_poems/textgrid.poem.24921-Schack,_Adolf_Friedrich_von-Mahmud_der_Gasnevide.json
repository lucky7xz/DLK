{"textgrid.poem.24921": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Mahmud der Gasnevide", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vor Mahmuds Thron kniet Nureddin: \u00bbO Padischah! ich fordre Recht!", "tokens": ["Vor", "Mah\u00b7muds", "Thron", "kniet", "Nu\u00b7red\u00b7din", ":", "\u00bb", "O", "Pa\u00b7di\u00b7schah", "!", "ich", "ford\u00b7re", "Recht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "NE", "$.", "$(", "NE", "NE", "$.", "PPER", "VVFIN", "NN", "$."], "meter": "-+-+--+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Ein Krieger deines Hofes hat ruchloser Unbill sich erfrecht!", "tokens": ["Ein", "Krie\u00b7ger", "dei\u00b7nes", "Ho\u00b7fes", "hat", "ruch\u00b7lo\u00b7ser", "Un\u00b7bill", "sich", "er\u00b7frecht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAFIN", "ADJD", "ADJD", "PRF", "VVPP", "$."], "meter": "-+-+-+-++--+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "Aus meiner Wohnung, meinem Bett trieb der Verfluchte mich heraus", "tokens": ["Aus", "mei\u00b7ner", "Woh\u00b7nung", ",", "mei\u00b7nem", "Bett", "trieb", "der", "Ver\u00b7fluch\u00b7te", "mich", "he\u00b7raus"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "VVFIN", "ART", "NN", "PRF", "APZR"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Und schwelgt mit meinen Weibern nun, als w\u00e4re sein mein Herd und Haus.\u00ab", "tokens": ["Und", "schwelgt", "mit", "mei\u00b7nen", "Wei\u00b7bern", "nun", ",", "als", "w\u00e4\u00b7re", "sein", "mein", "Herd", "und", "Haus", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "ADV", "$,", "KOKOM", "VAFIN", "PPOSAT", "PPOSAT", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Der Schah vernimmt es und erbleicht; stumm starrt er lang zu Boden hin.", "tokens": ["Der", "Schah", "ver\u00b7nimmt", "es", "und", "er\u00b7bleicht", ";", "stumm", "starrt", "er", "lang", "zu", "Bo\u00b7den", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "KON", "VVPP", "$.", "ADJD", "VVFIN", "PPER", "ADJD", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "\u00bbgeht!\u00ab \u2013 heischt er zu den Sklaven dann \u2013 \u00bbbesetzt das Haus des Nureddin,", "tokens": ["\u00bb", "geht", "!", "\u00ab", "\u2013", "heischt", "er", "zu", "den", "Skla\u00b7ven", "dann", "\u2013", "\u00bb", "be\u00b7setzt", "das", "Haus", "des", "Nu\u00b7red\u00b7din", ","], "token_info": ["punct", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$.", "$(", "$(", "VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "$(", "$(", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Da\u00df keiner draus entrinnen mag; wenn Finsternis die Erde deckt,", "tokens": ["Da\u00df", "kei\u00b7ner", "draus", "ent\u00b7rin\u00b7nen", "mag", ";", "wenn", "Fins\u00b7ter\u00b7nis", "die", "Er\u00b7de", "deckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PAV", "VVINF", "VMFIN", "$.", "KOUS", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Ruft mich, und sehen soll die Welt, wie Sultan Mahmud Recht vollstreckt.\u00ab", "tokens": ["Ruft", "mich", ",", "und", "se\u00b7hen", "soll", "die", "Welt", ",", "wie", "Sul\u00b7tan", "Mah\u00b7mud", "Recht", "voll\u00b7streckt", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "VVINF", "VMFIN", "ART", "NN", "$,", "PWAV", "NE", "NE", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.3": {"line.1": {"text": "Sie alle gehn; er aber tritt in die Moschee, verschlie\u00dft das Thor", "tokens": ["Sie", "al\u00b7le", "gehn", ";", "er", "a\u00b7ber", "tritt", "in", "die", "Mo\u00b7schee", ",", "ver\u00b7schlie\u00dft", "das", "Thor"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PIS", "VVINF", "$.", "PPER", "ADV", "VVFIN", "APPR", "ART", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Und liegt vor Allah im Gebet, bis sich der Tagesschein verlor;", "tokens": ["Und", "liegt", "vor", "Al\u00b7lah", "im", "Ge\u00b7bet", ",", "bis", "sich", "der", "Ta\u00b7ges\u00b7schein", "ver\u00b7lor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "APPRART", "NN", "$,", "KOUS", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Mit Nureddin als F\u00fchrer eilt er nach dem Haus des Frevels dann,", "tokens": ["Mit", "Nu\u00b7red\u00b7din", "als", "F\u00fch\u00b7rer", "eilt", "er", "nach", "dem", "Haus", "des", "Fre\u00b7vels", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KOUS", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Vier seiner Schergen hinter ihm, mit scharfen Beilen Mann f\u00fcr Mann.", "tokens": ["Vier", "sei\u00b7ner", "Scher\u00b7gen", "hin\u00b7ter", "ihm", ",", "mit", "schar\u00b7fen", "Bei\u00b7len", "Mann", "f\u00fcr", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "PPOSAT", "NN", "APPR", "PPER", "$,", "APPR", "ADJA", "NN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.4": {"line.1": {"text": "\u00bbl\u00f6scht aus die Fackeln!\u00ab donnert er. Im Hause wird es schreckenstumm;", "tokens": ["\u00bb", "l\u00f6scht", "aus", "die", "Fa\u00b7ckeln", "!", "\u00ab", "don\u00b7nert", "er", ".", "Im", "Hau\u00b7se", "wird", "es", "schre\u00b7cken\u00b7stumm", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "APPR", "ART", "NN", "$.", "$(", "VVFIN", "PPER", "$.", "APPRART", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Nur matt durchblinkt der Sterne Schein die tiefe Finsternis ringsum;", "tokens": ["Nur", "matt", "durch\u00b7blinkt", "der", "Ster\u00b7ne", "Schein", "die", "tie\u00b7fe", "Fins\u00b7ter\u00b7nis", "ring\u00b7sum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "NN", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Ins Thor voran st\u00fcrmt Nureddin; mit seinen Schergen folgt der Schah", "tokens": ["Ins", "Thor", "vo\u00b7ran", "st\u00fcrmt", "Nu\u00b7red\u00b7din", ";", "mit", "sei\u00b7nen", "Scher\u00b7gen", "folgt", "der", "Schah"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PTKVZ", "VVFIN", "NE", "$.", "APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+--+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Durch G\u00e4nge und durch S\u00e4ulen hin. \u00bbDa!\u00ab \u2013 fl\u00fcstert dumpf der F\u00fchrer \u2013 \u00bbda!\u00ab", "tokens": ["Durch", "G\u00e4n\u00b7ge", "und", "durch", "S\u00e4u\u00b7len", "hin", ".", "\u00bb", "Da", "!", "\u00ab", "\u2013", "fl\u00fcs\u00b7tert", "dumpf", "der", "F\u00fch\u00b7rer", "\u2013", "\u00bb", "da", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "PTKVZ", "$.", "$(", "ADV", "$.", "$(", "$(", "VVFIN", "ADJD", "ART", "NN", "$(", "$(", "ADV", "$.", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.5": {"line.1": {"text": "Die Schergen stellen sich im Kreis. \u00bbDes Frevlers Todeskampf sei kurz!\u00ab", "tokens": ["Die", "Scher\u00b7gen", "stel\u00b7len", "sich", "im", "Kreis", ".", "\u00bb", "Des", "Frev\u00b7lers", "To\u00b7des\u00b7kampf", "sei", "kurz", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPRART", "NN", "$.", "$(", "ART", "NN", "NN", "VAFIN", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Ruft Mahmud aus und z\u00fcckt das Schwert; ein halb erstickter Schrei, ein Sturz!", "tokens": ["Ruft", "Mah\u00b7mud", "aus", "und", "z\u00fcckt", "das", "Schwert", ";", "ein", "halb", "er\u00b7stick\u00b7ter", "Schrei", ",", "ein", "Sturz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NE", "PTKVZ", "KON", "VVFIN", "ART", "NN", "$.", "ART", "ADJD", "ADJA", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "\u00bblicht her!\u00ab Man bringt's. Flugs beugt der Schah sich zu des Toten Angesicht,", "tokens": ["\u00bb", "licht", "her", "!", "\u00ab", "Man", "bringt'", "s.", "Flugs", "beugt", "der", "Schah", "sich", "zu", "des", "To\u00b7ten", "An\u00b7ge\u00b7sicht", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "abbreviation", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "PTKVZ", "$.", "$(", "PIS", "VVFIN", "ADJA", "NN", "VVFIN", "ART", "NN", "PRF", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Dann kniet er nieder. \u00bbAllah, Dank! Der, den ich meine, war es nicht.", "tokens": ["Dann", "kniet", "er", "nie\u00b7der", ".", "\u00bb", "Al\u00b7lah", ",", "Dank", "!", "Der", ",", "den", "ich", "mei\u00b7ne", ",", "war", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$.", "$(", "NN", "$,", "NN", "$.", "ART", "$,", "PRELS", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.6": {"line.1": {"text": "Ihr aber, die ihr staunt, erfahrt! Ich glaubte, da\u00df mein eigner Sohn", "tokens": ["Ihr", "a\u00b7ber", ",", "die", "ihr", "staunt", ",", "er\u00b7fahrt", "!", "Ich", "glaub\u00b7te", ",", "da\u00df", "mein", "eig\u00b7ner", "Sohn"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "$.", "PPER", "VVFIN", "$,", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Der Th\u00e4ter sei; auf schlimmem Pfad argw\u00f6hnt' ich ihn seit lange schon,", "tokens": ["Der", "Th\u00e4\u00b7ter", "sei", ";", "auf", "schlim\u00b7mem", "Pfad", "arg\u00b7w\u00f6hnt'", "ich", "ihn", "seit", "lan\u00b7ge", "schon", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$.", "APPR", "ADJA", "NN", "VVFIN", "PPER", "PPER", "APPR", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Und, da\u00df sein Anblick nicht die Hand mir hemmte bei dem Strafgericht,", "tokens": ["Und", ",", "da\u00df", "sein", "An\u00b7blick", "nicht", "die", "Hand", "mir", "hemm\u00b7te", "bei", "dem", "Straf\u00b7ge\u00b7richt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPOSAT", "NN", "PTKNEG", "ART", "NN", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Vollstreckt' ich es in Finsternis; dem Himmel Dank, er war es nicht!\u00ab", "tokens": ["Voll\u00b7streckt'", "ich", "es", "in", "Fins\u00b7ter\u00b7nis", ";", "dem", "Him\u00b7mel", "Dank", ",", "er", "war", "es", "nicht", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "NE", "$.", "ART", "NN", "NN", "$,", "PPER", "VAFIN", "PPER", "PTKNEG", "$.", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.7": {"line.1": {"text": "Vor Mahmuds Thron kniet Nureddin: \u00bbO Padischah! ich fordre Recht!", "tokens": ["Vor", "Mah\u00b7muds", "Thron", "kniet", "Nu\u00b7red\u00b7din", ":", "\u00bb", "O", "Pa\u00b7di\u00b7schah", "!", "ich", "ford\u00b7re", "Recht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "NE", "$.", "$(", "NE", "NE", "$.", "PPER", "VVFIN", "NN", "$."], "meter": "-+-+--+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Ein Krieger deines Hofes hat ruchloser Unbill sich erfrecht!", "tokens": ["Ein", "Krie\u00b7ger", "dei\u00b7nes", "Ho\u00b7fes", "hat", "ruch\u00b7lo\u00b7ser", "Un\u00b7bill", "sich", "er\u00b7frecht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAFIN", "ADJD", "ADJD", "PRF", "VVPP", "$."], "meter": "-+-+-+-++--+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "Aus meiner Wohnung, meinem Bett trieb der Verfluchte mich heraus", "tokens": ["Aus", "mei\u00b7ner", "Woh\u00b7nung", ",", "mei\u00b7nem", "Bett", "trieb", "der", "Ver\u00b7fluch\u00b7te", "mich", "he\u00b7raus"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "VVFIN", "ART", "NN", "PRF", "APZR"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Und schwelgt mit meinen Weibern nun, als w\u00e4re sein mein Herd und Haus.\u00ab", "tokens": ["Und", "schwelgt", "mit", "mei\u00b7nen", "Wei\u00b7bern", "nun", ",", "als", "w\u00e4\u00b7re", "sein", "mein", "Herd", "und", "Haus", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "ADV", "$,", "KOKOM", "VAFIN", "PPOSAT", "PPOSAT", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.8": {"line.1": {"text": "Der Schah vernimmt es und erbleicht; stumm starrt er lang zu Boden hin.", "tokens": ["Der", "Schah", "ver\u00b7nimmt", "es", "und", "er\u00b7bleicht", ";", "stumm", "starrt", "er", "lang", "zu", "Bo\u00b7den", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "KON", "VVPP", "$.", "ADJD", "VVFIN", "PPER", "ADJD", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "\u00bbgeht!\u00ab \u2013 heischt er zu den Sklaven dann \u2013 \u00bbbesetzt das Haus des Nureddin,", "tokens": ["\u00bb", "geht", "!", "\u00ab", "\u2013", "heischt", "er", "zu", "den", "Skla\u00b7ven", "dann", "\u2013", "\u00bb", "be\u00b7setzt", "das", "Haus", "des", "Nu\u00b7red\u00b7din", ","], "token_info": ["punct", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$.", "$(", "$(", "VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "$(", "$(", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Da\u00df keiner draus entrinnen mag; wenn Finsternis die Erde deckt,", "tokens": ["Da\u00df", "kei\u00b7ner", "draus", "ent\u00b7rin\u00b7nen", "mag", ";", "wenn", "Fins\u00b7ter\u00b7nis", "die", "Er\u00b7de", "deckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PAV", "VVINF", "VMFIN", "$.", "KOUS", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Ruft mich, und sehen soll die Welt, wie Sultan Mahmud Recht vollstreckt.\u00ab", "tokens": ["Ruft", "mich", ",", "und", "se\u00b7hen", "soll", "die", "Welt", ",", "wie", "Sul\u00b7tan", "Mah\u00b7mud", "Recht", "voll\u00b7streckt", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "VVINF", "VMFIN", "ART", "NN", "$,", "PWAV", "NE", "NE", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.9": {"line.1": {"text": "Sie alle gehn; er aber tritt in die Moschee, verschlie\u00dft das Thor", "tokens": ["Sie", "al\u00b7le", "gehn", ";", "er", "a\u00b7ber", "tritt", "in", "die", "Mo\u00b7schee", ",", "ver\u00b7schlie\u00dft", "das", "Thor"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PIS", "VVINF", "$.", "PPER", "ADV", "VVFIN", "APPR", "ART", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Und liegt vor Allah im Gebet, bis sich der Tagesschein verlor;", "tokens": ["Und", "liegt", "vor", "Al\u00b7lah", "im", "Ge\u00b7bet", ",", "bis", "sich", "der", "Ta\u00b7ges\u00b7schein", "ver\u00b7lor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "APPRART", "NN", "$,", "KOUS", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Mit Nureddin als F\u00fchrer eilt er nach dem Haus des Frevels 