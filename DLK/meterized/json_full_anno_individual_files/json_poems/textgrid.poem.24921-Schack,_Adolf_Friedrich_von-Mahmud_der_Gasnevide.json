 "NN", "NN", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Ins Thor voran st\u00fcrmt Nureddin; mit seinen Schergen folgt der Schah", "tokens": ["Ins", "Thor", "vo\u00b7ran", "st\u00fcrmt", "Nu\u00b7red\u00b7din", ";", "mit", "sei\u00b7nen", "Scher\u00b7gen", "folgt", "der", "Schah"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PTKVZ", "VVFIN", "NE", "$.", "APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+--+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Durch G\u00e4nge und durch S\u00e4ulen hin. \u00bbDa!\u00ab \u2013 fl\u00fcstert dumpf der F\u00fchrer \u2013 \u00bbda!\u00ab", "tokens": ["Durch", "G\u00e4n\u00b7ge", "und", "durch", "S\u00e4u\u00b7len", "hin", ".", "\u00bb", "Da", "!", "\u00ab", "\u2013", "fl\u00fcs\u00b7tert", "dumpf", "der", "F\u00fch\u00b7rer", "\u2013", "\u00bb", "da", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "PTKVZ", "$.", "$(", "ADV", "$.", "$(", "$(", "VVFIN", "ADJD", "ART", "NN", "$(", "$(", "ADV", "$.", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.11": {"line.1": {"text": "Die Schergen stellen sich im Kreis. \u00bbDes Frevlers Todeskampf sei kurz!\u00ab", "tokens": ["Die", "Scher\u00b7gen", "stel\u00b7len", "sich", "im", "Kreis", ".", "\u00bb", "Des", "Frev\u00b7lers", "To\u00b7des\u00b7kampf", "sei", "kurz", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPRART", "NN", "$.", "$(", "ART", "NN", "NN", "VAFIN", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Ruft Mahmud aus und z\u00fcckt das Schwert; ein halb erstickter Schrei, ein Sturz!", "tokens": ["Ruft", "Mah\u00b7mud", "aus", "und", "z\u00fcckt", "das", "Schwert", ";", "ein", "halb", "er\u00b7stick\u00b7ter", "Schrei", ",", "ein", "Sturz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NE", "PTKVZ", "KON", "VVFIN", "ART", "NN", "$.", "ART", "ADJD", "ADJA", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "\u00bblicht her!\u00ab Man bringt's. Flugs beugt der Schah sich zu des Toten Angesicht,", "tokens": ["\u00bb", "licht", "her", "!", "\u00ab", "Man", "bringt'", "s.", "Flugs", "beugt", "der", "Schah", "sich", "zu", "des", "To\u00b7ten", "An\u00b7ge\u00b7sicht", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "abbreviation", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "PTKVZ", "$.", "$(", "PIS", "VVFIN", "ADJA", "NN", "VVFIN", "ART", "NN", "PRF", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Dann kniet er nieder. \u00bbAllah, Dank! Der, den ich meine, war es nicht.", "tokens": ["Dann", "kniet", "er", "nie\u00b7der", ".", "\u00bb", "Al\u00b7lah", ",", "Dank", "!", "Der", ",", "den", "ich", "mei\u00b7ne", ",", "war", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$.", "$(", "NN", "$,", "NN", "$.", "ART", "$,", "PRELS", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.12": {"line.1": {"text": "Ihr aber, 