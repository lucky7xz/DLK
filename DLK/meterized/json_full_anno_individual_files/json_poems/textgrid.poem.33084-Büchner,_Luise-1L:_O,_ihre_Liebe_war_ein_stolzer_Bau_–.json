{"textgrid.poem.33084": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: O, ihre Liebe war ein stolzer Bau \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O, ihre Liebe war ein stolzer Bau \u2013", "tokens": ["O", ",", "ih\u00b7re", "Lie\u00b7be", "war", "ein", "stol\u00b7zer", "Bau", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Freude Flagge weht' auf seinen Zinnen,", "tokens": ["Der", "Freu\u00b7de", "Flag\u00b7ge", "weht'", "auf", "sei\u00b7nen", "Zin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Kr\u00e4nze, feucht von s\u00fc\u00dfer Thr\u00e4nen Thau,", "tokens": ["Und", "Kr\u00e4n\u00b7ze", ",", "feucht", "von", "s\u00fc\u00b7\u00dfer", "Thr\u00e4\u00b7nen", "Thau", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ADJD", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sie schm\u00fcckten ihn von au\u00dfen und von innen.", "tokens": ["Sie", "schm\u00fcck\u00b7ten", "ihn", "von", "au\u00b7\u00dfen", "und", "von", "in\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADV", "KON", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Vertrauen, st\u00e4rker noch als Marmorstein,", "tokens": ["Ver\u00b7trau\u00b7en", ",", "st\u00e4r\u00b7ker", "noch", "als", "Mar\u00b7mor\u00b7stein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "ADV", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Als S\u00e4ule trug die Kuppel in der Mitten,", "tokens": ["Als", "S\u00e4u\u00b7le", "trug", "die", "Kup\u00b7pel", "in", "der", "Mit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Fenster prangt' im reinsten Demantschein,", "tokens": ["Das", "Fens\u00b7ter", "prangt'", "im", "reins\u00b7ten", "De\u00b7mant\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Aus \u00e4chter Treue festem Stoff geschnitten.", "tokens": ["Aus", "\u00e4ch\u00b7ter", "Treu\u00b7e", "fes\u00b7tem", "Stoff", "ge\u00b7schnit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und s\u00fc\u00dfes, heitres Himmelslicht ergo\u00df", "tokens": ["Und", "s\u00fc\u00b7\u00dfes", ",", "heit\u00b7res", "Him\u00b7mels\u00b7licht", "er\u00b7go\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJA", "$,", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Des Herzens Reinheit durch die klare Scheibe,", "tokens": ["Des", "Her\u00b7zens", "Rein\u00b7heit", "durch", "die", "kla\u00b7re", "Schei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie Heil'genschein sein Inn'res ganz umflo\u00df,", "tokens": ["Wie", "Heil'\u00b7gen\u00b7schein", "sein", "Inn'\u00b7res", "ganz", "um\u00b7flo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Was Liebe Gutes, Sch\u00f6nes weckt im Weibe.", "tokens": ["Was", "Lie\u00b7be", "Gu\u00b7tes", ",", "Sch\u00f6\u00b7nes", "weckt", "im", "Wei\u00b7be", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "NN", "$,", "ADJA", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So stand gebaut er f\u00fcr die Ewigkeit;", "tokens": ["So", "stand", "ge\u00b7baut", "er", "f\u00fcr", "die", "E\u00b7wig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VVPP", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Weh' da\u00df das Heut' ihn schon in Tr\u00fcmmern findet \u2013", "tokens": ["Weh'", "da\u00df", "das", "Heut'", "ihn", "schon", "in", "Tr\u00fcm\u00b7mern", "fin\u00b7det", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "ART", "NN", "PPER", "ADV", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihn st\u00fcrzte nicht die allgewalt'ge Zeit,", "tokens": ["Ihn", "st\u00fcrz\u00b7te", "nicht", "die", "all\u00b7ge\u00b7walt'\u00b7ge", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ach, nein! er war auf losen Sand gegr\u00fcndet!", "tokens": ["Ach", ",", "nein", "!", "er", "war", "auf", "lo\u00b7sen", "Sand", "ge\u00b7gr\u00fcn\u00b7det", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PTKANT", "$.", "PPER", "VAFIN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Er selbst hat ihn gebrochen und zerst\u00f6rt,", "tokens": ["Er", "selbst", "hat", "ihn", "ge\u00b7bro\u00b7chen", "und", "zer\u00b7st\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PPER", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den stolzen Bau, in dem sie ihn verehret,", "tokens": ["Den", "stol\u00b7zen", "Bau", ",", "in", "dem", "sie", "ihn", "ver\u00b7eh\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PRELS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von seiner eignen Schw\u00e4che \u00fcberth\u00f6rt,", "tokens": ["Von", "sei\u00b7ner", "eig\u00b7nen", "Schw\u00e4\u00b7che", "\u00fc\u00b7bert\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hat er ihn selbst mit frevler Hand verheeret.", "tokens": ["Hat", "er", "ihn", "selbst", "mit", "frev\u00b7ler", "Hand", "ver\u00b7hee\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Erst sank die Flagge von der K\u00e4lte Hauch,", "tokens": ["Erst", "sank", "die", "Flag\u00b7ge", "von", "der", "K\u00e4l\u00b7te", "Hauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Kr\u00e4nze welkten bei der Launen Spiele,", "tokens": ["Die", "Kr\u00e4n\u00b7ze", "welk\u00b7ten", "bei", "der", "Lau\u00b7nen", "Spie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die S\u00e4ule br\u00f6ckelte am Ende auch \u2013", "tokens": ["Die", "S\u00e4u\u00b7le", "br\u00f6\u00b7ckel\u00b7te", "am", "En\u00b7de", "auch", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "ADV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Im Staub erst, sah ihr Glaube sich am Ziele.", "tokens": ["Im", "Staub", "erst", ",", "sah", "ihr", "Glau\u00b7be", "sich", "am", "Zie\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$,", "VVFIN", "PPOSAT", "NN", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Die Treue nur prangt noch im Demantschein,", "tokens": ["Die", "Treu\u00b7e", "nur", "prangt", "noch", "im", "De\u00b7mant\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Kann sie so schnell den theuren Tempel lassen?", "tokens": ["Kann", "sie", "so", "schnell", "den", "theu\u00b7ren", "Tem\u00b7pel", "las\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Gram wie bleiches Mondlicht f\u00e4llt hinein", "tokens": ["Der", "Gram", "wie", "blei\u00b7ches", "Mond\u00b7licht", "f\u00e4llt", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KOKOM", "ADJA", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und findet nirgends, nirgends Raum zum Hassen! \u2013", "tokens": ["Und", "fin\u00b7det", "nir\u00b7gends", ",", "nir\u00b7gends", "Raum", "zum", "Has\u00b7sen", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "ADV", "NN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Wer wei\u00df, wenn er dereinst die Stra\u00dfe geht", "tokens": ["Wer", "wei\u00df", ",", "wenn", "er", "de\u00b7reinst", "die", "Stra\u00b7\u00dfe", "geht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und ihres Baues Tr\u00fcmmer vor ihm ragen,", "tokens": ["Und", "ih\u00b7res", "Bau\u00b7es", "Tr\u00fcm\u00b7mer", "vor", "ihm", "ra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ob dann voll Trauer er nicht stille steht,", "tokens": ["Ob", "dann", "voll", "Trau\u00b7er", "er", "nicht", "stil\u00b7le", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "NN", "PPER", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Voll bitt'rer Reue, da\u00df er ihn zerschlagen!", "tokens": ["Voll", "bitt'\u00b7rer", "Reu\u00b7e", ",", "da\u00df", "er", "ihn", "zer\u00b7schla\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,", "KOUS", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "O, ihre Liebe war ein stolzer Bau \u2013", "tokens": ["O", ",", "ih\u00b7re", "Lie\u00b7be", "war", "ein", "stol\u00b7zer", "Bau", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Freude Flagge weht' auf seinen Zinnen,", "tokens": ["Der", "Freu\u00b7de", "Flag\u00b7ge", "weht'", "auf", "sei\u00b7nen", "Zin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Kr\u00e4nze, feucht von s\u00fc\u00dfer Thr\u00e4nen Thau,", "tokens": ["Und", "Kr\u00e4n\u00b7ze", ",", "feucht", "von", "s\u00fc\u00b7\u00dfer", "Thr\u00e4\u00b7nen", "Thau", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ADJD", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sie schm\u00fcckten ihn von au\u00dfen und von innen.", "tokens": ["Sie", "schm\u00fcck\u00b7t