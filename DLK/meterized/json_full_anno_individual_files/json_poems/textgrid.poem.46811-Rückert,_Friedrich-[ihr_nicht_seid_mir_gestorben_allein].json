{"textgrid.poem.46811": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ihr nicht seid mir gestorben allein]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr nicht seid mir gestorben allein,", "tokens": ["Ihr", "nicht", "seid", "mir", "ge\u00b7stor\u00b7ben", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "VAFIN", "PPER", "VVPP", "ADV", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Es ist gestorben der Freudenschein,", "tokens": ["Es", "ist", "ge\u00b7stor\u00b7ben", "der", "Freu\u00b7den\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der mir die Welt umwoben,", "tokens": ["Der", "mir", "die", "Welt", "um\u00b7wo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es ist gestorben der blendende Tag,", "tokens": ["Es", "ist", "ge\u00b7stor\u00b7ben", "der", "blen\u00b7den\u00b7de", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Der auf den Tiefen des Todes lag,", "tokens": ["Der", "auf", "den", "Tie\u00b7fen", "des", "To\u00b7des", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die Decke hat sich gehoben,", "tokens": ["Die", "De\u00b7cke", "hat", "sich", "ge\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PRF", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Der Flitterglanz ist zerstoben.", "tokens": ["Der", "Flit\u00b7ter\u00b7glanz", "ist", "zer\u00b7sto\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Nun seh' ich Betr\u00fcbte fern und nah,", "tokens": ["Nun", "seh'", "ich", "Be\u00b7tr\u00fcb\u00b7te", "fern", "und", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wo ich sonst lauter Gl\u00fcckliche sah,", "tokens": ["Wo", "ich", "sonst", "lau\u00b7ter", "Gl\u00fcck\u00b7li\u00b7che", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Die 's waren oder mir schienen;", "tokens": ["Die", "'s", "wa\u00b7ren", "o\u00b7der", "mir", "schie\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VAFIN", "KON", "PPER", "VVFIN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ansah ich so genau sie nicht,", "tokens": ["An\u00b7sah", "ich", "so", "ge\u00b7nau", "sie", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nun blick' ich sch\u00e4rfer in jedes Gesicht,", "tokens": ["Nun", "blick'", "ich", "sch\u00e4r\u00b7fer", "in", "je\u00b7des", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und les' in allen Mienen,", "tokens": ["Und", "les'", "in", "al\u00b7len", "Mie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df etwas starb auch ihnen.", "tokens": ["Da\u00df", "et\u00b7was", "starb", "auch", "ih\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "ADV", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sonst hab' ich Trauerkunden geh\u00f6rt,", "tokens": ["Sonst", "hab'", "ich", "Trau\u00b7er\u00b7kun\u00b7den", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Es hat im Gl\u00fccke mich nicht gest\u00f6rt.", "tokens": ["Es", "hat", "im", "Gl\u00fc\u00b7cke", "mich", "nicht", "ge\u00b7st\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Es waren nur leere Schalle.", "tokens": ["Es", "wa\u00b7ren", "nur", "lee\u00b7re", "Schal\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Nun kann ich in keine Zeitung sehn.", "tokens": ["Nun", "kann", "ich", "in", "kei\u00b7ne", "Zei\u00b7tung", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "So seh' ich die Todsanzeigen stehn;", "tokens": ["So", "seh'", "ich", "die", "Tod\u00b7san\u00b7zei\u00b7gen", "stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Sonst \u00e4rgert' ich mich am Schwalle,", "tokens": ["Sonst", "\u00e4r\u00b7gert'", "ich", "mich", "am", "Schwal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Nun mu\u00df ich sie lesen alle.", "tokens": ["Nun", "mu\u00df", "ich", "sie", "le\u00b7sen", "al\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVFIN", "PIS", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich h\u00f6re wie sonst das Glockengel\u00e4ut,", "tokens": ["Ich", "h\u00f6\u00b7re", "wie", "sonst", "das", "Glo\u00b7cken\u00b7ge\u00b7l\u00e4ut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ADV", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bedeutender, als es mir klang, klingt's heut,", "tokens": ["Be\u00b7deu\u00b7ten\u00b7der", ",", "als", "es", "mir", "klang", ",", "klingt's", "heut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und ich frage, was es bedeute?", "tokens": ["Und", "ich", "fra\u00b7ge", ",", "was", "es", "be\u00b7deu\u00b7te", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "So \u00fcbles hat mirs bedeutet schon;", "tokens": ["So", "\u00fcb\u00b7les", "hat", "mirs", "be\u00b7deu\u00b7tet", "schon", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VAFIN", "NE", "VVFIN", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wem gilt nun heute der Trauerton?", "tokens": ["Wem", "gilt", "nun", "heu\u00b7te", "der", "Trau\u00b7er\u00b7ton", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Sonst meint' ich nur, man l\u00e4ute", "tokens": ["Sonst", "meint'", "ich", "nur", ",", "man", "l\u00e4u\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PIS", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "F\u00fcr lauter T\u00e4ufling' und Br\u00e4ute.", "tokens": ["F\u00fcr", "lau\u00b7ter", "T\u00e4uf\u00b7ling'", "und", "Br\u00e4u\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Mit Epheu ist mein Garten geschm\u00fcckt,", "tokens": ["Mit", "E\u00b7pheu", "ist", "mein", "Gar\u00b7ten", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+---+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Den haben auch sonst die Leute gepfl\u00fcckt", "tokens": ["Den", "ha\u00b7ben", "auch", "sonst", "die", "Leu\u00b7te", "ge\u00b7pfl\u00fcckt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ADV", "ADV", "ART", "NN", "VVPP"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Aus der Stadt, und ich lie\u00df sie pfl\u00fccken,", "tokens": ["Aus", "der", "Stadt", ",", "und", "ich", "lie\u00df", "sie", "pfl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KON", "PPER", "VVFIN", "PPER", "VVINF", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und fragte nicht, zu welchem Behuf?", "tokens": ["Und", "frag\u00b7te", "nicht", ",", "zu", "wel\u00b7chem", "Be\u00b7huf", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "APPR", "PWAT", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Nun aber hab' ich zu fragen Beruf,", "tokens": ["Nun", "a\u00b7ber", "hab'", "ich", "zu", "fra\u00b7gen", "Be\u00b7ruf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "PTKZU", "VVINF", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Auf welches Haupt sie dr\u00fccken", "tokens": ["Auf", "wel\u00b7ches", "Haupt", "sie", "dr\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Den dunkeln Kranz, den sie pfl\u00fccken.", "tokens": ["Den", "dun\u00b7keln", "Kranz", ",", "den", "sie", "pfl\u00fc\u00b7cken", "."], "token_info":