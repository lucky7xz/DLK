{"textgrid.poem.65398": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Das R\u00f6schen", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du kleine junge Kellnerin,", "tokens": ["Du", "klei\u00b7ne", "jun\u00b7ge", "Kell\u00b7ne\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Warum so gar verlegen?", "tokens": ["Wa\u00b7rum", "so", "gar", "ver\u00b7le\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wer sch\u00fcttet doch den Wein dahin", "tokens": ["Wer", "sch\u00fct\u00b7tet", "doch", "den", "Wein", "da\u00b7hin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "ART", "NN", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um eines Kusses wegen?", "tokens": ["Um", "ei\u00b7nes", "Kus\u00b7ses", "we\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "APPR", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Komm, da\u00df der Alt' es nur nicht seh';", "tokens": ["Komm", ",", "da\u00df", "der", "Alt'", "es", "nur", "nicht", "seh'", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "PPER", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich will es auf mich nehmen.", "tokens": ["Ich", "will", "es", "auf", "mich", "neh\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Schenk' ein, Gesichtchen in die H\u00f6h'!", "tokens": ["Schenk'", "ein", ",", "Ge\u00b7sicht\u00b7chen", "in", "die", "H\u00f6h'", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich b\u00fc\u00dfe dir das Sch\u00e4men.", "tokens": ["Ich", "b\u00fc\u00b7\u00dfe", "dir", "das", "Sch\u00e4\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du schauest in das Glas hinein", "tokens": ["Du", "schau\u00b7est", "in", "das", "Glas", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit purpurrothen Wangen:", "tokens": ["Mit", "pur\u00b7pur\u00b7ro\u00b7then", "Wan\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da schwimmt hoch oben auf dem Wein", "tokens": ["Da", "schwimmt", "hoch", "o\u00b7ben", "auf", "dem", "Wein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein R\u00f6schen unbefangen.", "tokens": ["Ein", "R\u00f6\u00b7schen", "un\u00b7be\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und sieh, ich k\u00fcss' es, wie ich will,", "tokens": ["Und", "sieh", ",", "ich", "k\u