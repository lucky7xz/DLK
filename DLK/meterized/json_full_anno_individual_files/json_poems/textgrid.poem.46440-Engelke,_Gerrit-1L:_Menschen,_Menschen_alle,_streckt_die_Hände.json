{"textgrid.poem.46440": {"metadata": {"author": {"name": "Engelke, Gerrit", "birth": "N.A.", "death": "N.A."}, "title": "1L: Menschen, Menschen alle, streckt die H\u00e4nde", "genre": "verse", "period": "N.A.", "pub_year": 1913, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Menschen, Menschen alle, streckt die H\u00e4nde", "tokens": ["Men\u00b7schen", ",", "Men\u00b7schen", "al\u00b7le", ",", "streckt", "die", "H\u00e4n\u00b7de"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "PIS", "$,", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ueber Meere, W\u00e4lder in die Welt zur Einigkeit!", "tokens": ["Ue\u00b7ber", "Mee\u00b7re", ",", "W\u00e4l\u00b7der", "in", "die", "Welt", "zur", "Ei\u00b7nig\u00b7keit", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "Da\u00df sich Herz zu Herzen sende:", "tokens": ["Da\u00df", "sich", "Herz", "zu", "Her\u00b7zen", "sen\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Neue Zeit!", "tokens": ["Neu\u00b7e", "Zeit", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Starke R\u00fchrung soll aus euren Aufenthalten", "tokens": ["Star\u00b7ke", "R\u00fch\u00b7rung", "soll", "aus", "eu\u00b7ren", "Auf\u00b7ent\u00b7hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-", "measure": "