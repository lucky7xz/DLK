{"textgrid.poem.49574": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "Sinngedicht", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Leben, ", "tokens": ["Das", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Durch W\u00fcsteneien voller Hecken,", "tokens": ["Durch", "W\u00fcs\u00b7te\u00b7nei\u00b7en", "vol\u00b7ler", "He\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dr\u00e4ngt uns, mit eines Stromes Kraft,", "tokens": ["Dr\u00e4ngt", "uns", ",", "mit", "ei\u00b7nes", "Stro\u00b7mes", "Kraft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hinab in dein gemeines Becken..", "tokens": ["Hin\u00b7ab", "in", "dein", "ge\u00b7mei\u00b7nes", "Be\u00b7cken", ".."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Solch Ende soll mich nicht erschrecken.", "tokens": ["Solch", "En\u00b7de", "soll", "mich", "nicht", "er\u00b7schre\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Packt Gold in einen B\u00fcndel ein:", "tokens": ["Packt", "Gold", "in", "ei\u00b7nen", "B\u00fcn\u00b7del", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "La\u00dft ihn in langen Flammen brennen!", "tokens": ["La\u00dft", "ihn", "in", "lan\u00b7gen", "Flam\u00b7men", "bren\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Schade wird unmerklich seyn.", "tokens": ["Der", "Scha\u00b7de", "wird", "un\u00b7mer\u00b7klich", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VAINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Die Einballirung wird allein;", "tokens": ["Die", "Ein\u00b7bal\u00b7li\u00b7rung", "wird", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.10": {"text": "Doch nie das Gold verbrennen k\u00f6nnen!", "tokens": ["Doch", "nie", "das", "Gold", "ver\u00b7bren\u00b7nen", "k\u00f6n\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das Leben, ", "tokens": ["Das", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Durch W\u00fcsteneien voller Hecken,", "tokens": ["Durch", "W\u00fcs\u00b7te\u00b7nei\u00b7en", "vol\u00b7ler", "He\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dr\u00e4ngt uns, mit eines Stromes Kraft,", "tokens": ["Dr\u00e4ngt", "uns"