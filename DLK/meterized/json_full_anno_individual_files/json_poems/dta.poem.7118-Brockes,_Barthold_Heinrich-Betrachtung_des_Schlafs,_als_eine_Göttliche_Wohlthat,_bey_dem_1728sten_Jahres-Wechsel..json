{"dta.poem.7118": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung des Schlafs,  \n als  \n eine G\u00f6ttliche Wohlthat,  \n bey dem 1728sten Jahres-Wechsel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sie sucht so gar durch ein geheimes Grauen,", "tokens": ["Sie", "sucht", "so", "gar", "durch", "ein", "ge\u00b7hei\u00b7mes", "Grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der duncklen Schatten Kind, uns g\u00e4ntzlich abzuschrecken", "tokens": ["Der", "dunck\u00b7len", "Schat\u00b7ten", "Kind", ",", "uns", "g\u00e4ntz\u00b7lich", "ab\u00b7zu\u00b7schre\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "$,", "PPER", "ADJD", "VVIZU"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Von allen Handlungen. Noch mehr, da\u00df nichts die Ruh", "tokens": ["Von", "al\u00b7len", "Hand\u00b7lun\u00b7gen", ".", "Noch", "mehr", ",", "da\u00df", "nichts", "die", "Ruh"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$.", "ADV", "ADV", "$,", "KOUS", "PIS", "ART", "NN"], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Uns leicht zu unterbrechen tauge;", "tokens": ["Uns", "leicht", "zu", "un\u00b7ter\u00b7bre\u00b7chen", "tau\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So zieht sie wunderbar uns \u00fcber jedes Auge", "tokens": ["So", "zieht", "sie", "wun\u00b7der\u00b7bar", "uns", "\u00fc\u00b7ber", "je\u00b7des", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Noch einen eignen Vorhang zu,", "tokens": ["Noch", "ei\u00b7nen", "eig\u00b7nen", "Vor\u00b7hang", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der, wenn man ihn genau erweget und ermisst,", "tokens": ["Der", ",", "wenn", "man", "ihn", "ge\u00b7nau", "er\u00b7we\u00b7get", "und", "er\u00b7misst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PIS", "PPER", "ADJD", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Nicht gnugsam zu bewundern ist.", "tokens": ["Nicht", "gnug\u00b7sam", "zu", "be\u00b7wun\u00b7dern", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}