{"textgrid.poem.67319": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "An die Freundschaft", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heil'ge Freundschaft, die auf Engelsfl\u00fcgeln", "tokens": ["Heil'\u00b7ge", "Freund\u00b7schaft", ",", "die", "auf", "En\u00b7gels\u00b7fl\u00fc\u00b7geln"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sich emporschwang zu den sel'gen H\u00fcgeln,", "tokens": ["Sich", "em\u00b7por\u00b7schwang", "zu", "den", "sel'\u00b7gen", "H\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Unser Erdenland verlie\u00df", "tokens": ["Un\u00b7ser", "Er\u00b7den\u00b7land", "ver\u00b7lie\u00df"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ging auf ins V\u00e4terparadies,", "tokens": ["Und", "ging", "auf", "ins", "V\u00e4\u00b7ter\u00b7pa\u00b7ra\u00b7dies", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Wo sie noch aus guten Mutterh\u00e4nden", "tokens": ["Wo", "sie", "noch", "aus", "gu\u00b7ten", "Mut\u00b7ter\u00b7h\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Uns ihr Kind zuweilen her will senden,", "tokens": ["Uns", "ihr", "Kind", "zu\u00b7wei\u00b7len", "her", "will", "sen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "ADV", "APZR", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Liebe, die auch irre geht", "tokens": ["Lie\u00b7be", ",", "die", "auch", "ir\u00b7re", "geht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "ADV", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und f\u00fcr Treue \u00f6fters Reu empf\u00e4ht \u2013", "tokens": ["Und", "f\u00fcr", "Treu\u00b7e", "\u00f6f\u00b7ters", "Reu", "em\u00b7pf\u00e4ht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADV", "NE", "VVFIN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Holde Freundschaft, kehr, o kehre wieder,", "tokens": ["Hol\u00b7de", "Freund\u00b7schaft", ",", "kehr", ",", "o", "keh\u00b7re", "wie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$,", "FM", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Hand und Herzen bindend, zu uns nieder!", "tokens": ["Hand", "und", "Her\u00b7zen", "bin\u00b7dend", ",", "zu", "uns", "nie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVPP", "$,", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ohne Dich ist Alles leer,", "tokens": ["Oh\u00b7ne", "Dich", "ist", "Al\u00b7les", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PIS", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch die Liebe selbst nicht Liebe mehr.", "tokens": ["Auch", "die", "Lie\u00b7be", "selbst", "nicht", "Lie\u00b7be", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "PTKNEG", "NN", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Wenn Du Dich uns l\u00e4nger, l\u00e4nger raubest", "tokens": ["Wenn", "Du", "Dich", "uns", "l\u00e4n\u00b7ger", ",", "l\u00e4n\u00b7ger", "rau\u00b7best"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "PRF", "ADJD", "$,", "ADJD", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und Dein Bild dem s\u00fc\u00dfen Trug erlaubest,", "tokens": ["Und", "Dein", "Bild", "dem", "s\u00fc\u00b7\u00dfen", "Trug", "er\u00b7lau\u00b7best", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "O, so wird Dein Menschenreich", "tokens": ["O", ",", "so", "wird", "Dein", "Men\u00b7schen\u00b7reich"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADV", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bald dem w\u00fcsten, wilden Chaos gleich.", "tokens": ["Bald", "dem", "w\u00fcs\u00b7ten", ",", "wil\u00b7den", "Chaos", "gleich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "VVFIN", "$,", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Heil'ge Freundschaft, die auf Engelsfl\u00fcgeln", "tokens": ["Heil'\u00b7ge", "Freund\u00b7schaft", ",", "die", "auf", "En\u00b7gels\u00b7fl\u00fc\u00b7geln"], "token_info": ["word", "word