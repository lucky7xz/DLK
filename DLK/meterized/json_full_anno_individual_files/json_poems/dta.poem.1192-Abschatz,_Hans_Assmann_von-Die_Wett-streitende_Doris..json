{"dta.poem.1192": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die Wett-streitende Doris.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Das sch\u00f6ne Kleeblat der G\u00f6ttinnen", "tokens": ["Das", "sch\u00f6\u00b7ne", "Klee\u00b7blat", "der", "G\u00f6t\u00b7tin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das um den Apffel f\u00fchrte Zanck/", "tokens": ["Das", "um", "den", "Apf\u00b7fel", "f\u00fchr\u00b7te", "Zanck", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "NE", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gedachte/ n\u00e4chst der Sch\u00f6nheit/ Danck", "tokens": ["Ge\u00b7dach\u00b7te", "/", "n\u00e4chst", "der", "Sch\u00f6n\u00b7heit", "/", "Danck"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "$(", "ADV", "ART", "NN", "$(", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr meiner Doris zu gewinnen;", "tokens": ["F\u00fcr", "mei\u00b7ner", "Do\u00b7ris", "zu", "ge\u00b7win\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NE", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch Venus selber gab ihr nach", "tokens": ["Doch", "Ve\u00b7nus", "sel\u00b7ber", "gab", "ihr", "nach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "ADV", "VVFIN", "PPER", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Eh noch jemand das Urtheil sprach.", "tokens": ["Eh", "noch", "je\u00b7mand", "das", "Ur\u00b7theil", "sprach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PIS", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Aglaja stund mit ihr im Streite", "tokens": ["Ag\u00b7la\u00b7ja", "stund", "mit", "ihr", "im", "Strei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "An wem der Vorzug solte seyn:", "tokens": ["An", "wem", "der", "Vor\u00b7zug", "sol\u00b7te", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWS", "ART", "NN", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der beyden Schwestern holder Schein", "tokens": ["Der", "bey\u00b7den", "Schwes\u00b7tern", "hol\u00b7der", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zog erst viel Hertzen auff die Seite/", "tokens": ["Zog", "erst", "viel", "Hert\u00b7zen", "auff", "die", "Sei\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIAT", "NN", "APPR", "ART", "NN", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Doch ward mit Warheit ausgef\u00fchrt/", "tokens": ["Doch", "ward", "mit", "War\u00b7heit", "aus\u00b7ge\u00b7f\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df ihr der erste Stand geb\u00fchrt.", "tokens": ["Da\u00df", "ihr", "der", "ers\u00b7te", "Stand", "ge\u00b7b\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Apollo lie\u00df die Wolcken schwinden/", "tokens": ["A\u00b7pol\u00b7lo", "lie\u00df", "die", "Wol\u00b7cken", "schwin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Braucht alle seine Glutt und Macht/", "tokens": ["Braucht", "al\u00b7le", "sei\u00b7ne", "Glutt", "und", "Macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPOSAT", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wolt ihrer hellen Augen Pracht", "tokens": ["Wolt", "ih\u00b7rer", "hel\u00b7len", "Au\u00b7gen", "Pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch seine Stralen \u00fcberwinden:", "tokens": ["Durch", "sei\u00b7ne", "Stra\u00b7len", "\u00fc\u00b7berw\u00b7in\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Was aber kunte gegen Zweyn", "tokens": ["Was", "a\u00b7ber", "kun\u00b7te", "ge\u00b7gen", "Zweyn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "VMFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Glantz von einer Sonne seyn?", "tokens": ["Der", "Glantz", "von", "ei\u00b7ner", "Son\u00b7ne", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Man h\u00f6rte sie die Wette singen", "tokens": ["Man", "h\u00f6r\u00b7te", "sie", "die", "Wet\u00b7te", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit einer stoltzen Nachtigall.", "tokens": ["Mit", "ei\u00b7ner", "stolt\u00b7zen", "Nach\u00b7ti\u00b7gall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wem h\u00e4tte dieser s\u00fcsse Schall", "tokens": ["Wem", "h\u00e4t\u00b7te", "die\u00b7ser", "s\u00fcs\u00b7se", "Schall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht durch das Hertze sollen dringen?", "tokens": ["Nicht", "durch", "das", "Hert\u00b7ze", "sol\u00b7len", "drin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PDS", "VVFIN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch ihrer reinen Stimme Zier", "tokens": ["Doch", "ih\u00b7rer", "rei\u00b7nen", "Stim\u00b7me", "Zier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gieng tausend Nachtigallen f\u00fcr.", "tokens": ["Gieng", "tau\u00b7send", "Nach\u00b7ti\u00b7gal\u00b7len", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "CARD", "NN", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "An dem gelinden Oder-Strande", "tokens": ["An", "dem", "ge\u00b7lin\u00b7den", "O\u00b7der\u00b7Stran\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da sezten sie und Amor an/", "tokens": ["Da", "sez\u00b7ten", "sie", "und", "A\u00b7mor", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "NE", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer am gewi\u00dften schi\u00dfen kan;", "tokens": ["Wer", "am", "ge\u00b7wi\u00df\u00b7ten", "schi\u00b7\u00dfen", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "ADJA", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr blieb der Sieg/ und ihm die Schande.", "tokens": ["Ihr", "blieb", "der", "Sieg", "/", "und", "ihm", "die", "Schan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "KON", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Was sonst Cupidens Pfeil verlacht/", "tokens": ["Was", "sonst", "Cu\u00b7pi\u00b7dens", "Pfeil", "ver\u00b7lacht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "NE", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das hat ihr Blicken wund gemacht.", "tokens": ["Das", "hat", "ihr", "Bli\u00b7cken", "wund", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wenn sie denn alles kan besiegen/", "tokens": ["Wenn", "sie", "denn", "al\u00b7les", "kan", "be\u00b7sie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIS", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und nichts ist/ das ihr widerspricht/", "tokens": ["Und", "nichts", "ist", "/", "das", "ihr", "wi\u00b7der\u00b7spricht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "$(", "PRELS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Warum soll meine Freyheit nicht", "tokens": ["Wa\u00b7rum", "soll", "mei\u00b7ne", "Frey\u00b7heit", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPOSAT", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu ihren edlen F\u00fcssen liegen?", "tokens": ["Zu", "ih\u00b7ren", "ed\u00b7len", "F\u00fcs\u00b7sen", "lie\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich bin ihr willig unterthan/", "tokens": ["Ich", "bin", "ihr", "wil\u00b7lig", "un\u00b7ter\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und bete meine F\u00e4ssel an.", "tokens": ["Und", "be\u00b7te", "mei\u00b7ne", "F\u00e4s\u00b7sel", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}