{"dta.poem.13670": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Freunde .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Zu jung, des Lebens Ernste zu entsagen \u2014", "tokens": ["Zu", "jung", ",", "des", "Le\u00b7bens", "Erns\u00b7te", "zu", "ent\u00b7sa\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "$,", "ART", "NN", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kann ich nicht l\u00e4nger spielen nun und schweigen,", "tokens": ["Kann", "ich", "nicht", "l\u00e4n\u00b7ger", "spie\u00b7len", "nun", "und", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADJD", "VVFIN", "ADV", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wer Herrlich's f\u00fchlt, der mu\u00df sich herrlich zeigen,", "tokens": ["Wer", "Herr\u00b7lich's", "f\u00fchlt", ",", "der", "mu\u00df", "sich", "herr\u00b7lich", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "VVFIN", "$,", "ART", "VMFIN", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein Ruhen ist ein ewig frisches Wagen.", "tokens": ["Mein", "Ru\u00b7hen", "ist", "ein", "e\u00b7wig", "fri\u00b7sches", "Wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}