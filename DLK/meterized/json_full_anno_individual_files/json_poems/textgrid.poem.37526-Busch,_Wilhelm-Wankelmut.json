{"textgrid.poem.37526": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Wankelmut", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was bin ich alter B\u00f6sewicht", "tokens": ["Was", "bin", "ich", "al\u00b7ter", "B\u00f6\u00b7se\u00b7wicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So wankelig von Sinne.", "tokens": ["So", "wan\u00b7ke\u00b7lig", "von", "Sin\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein leeres Glas gef\u00e4llt mir nicht,", "tokens": ["Ein", "lee\u00b7res", "Glas", "ge\u00b7f\u00e4llt", "mir", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich will, da\u00df was darinne.", "tokens": ["Ich", "will", ",", "da\u00df", "was", "da\u00b7rin\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Das ist mir so ein d\u00fcrr Geklirr;", "tokens": ["Das", "ist", "mir", "so", "ein", "d\u00fcrr", "Ge\u00b7klirr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "He, Kellnerin, erscheine!", "tokens": ["He", ",", "Kell\u00b7ne\u00b7rin", ",", "er\u00b7schei\u00b7ne", "!"], "token_info": ["word", "punct", "word",