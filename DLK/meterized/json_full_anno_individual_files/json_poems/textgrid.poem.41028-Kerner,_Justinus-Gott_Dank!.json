{"textgrid.poem.41028": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Gott Dank!", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In meinem Garten stehet ein Baum,", "tokens": ["In", "mei\u00b7nem", "Gar\u00b7ten", "ste\u00b7het", "ein", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Tr\u00e4gt \u00c4pfel so viel, da\u00df man's glaubet kaum.", "tokens": ["Tr\u00e4gt", "\u00c4p\u00b7fel", "so", "viel", ",", "da\u00df", "man's", "glau\u00b7bet", "kaum", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADV", "ADV", "$,", "KOUS", "PIS", "VVFIN", "ADV", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Wie herzlich mich diese erfreuen!", "tokens": ["Wie", "herz\u00b7lich", "mich", "die\u00b7se", "er\u00b7freu\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "PDS", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Gott schenke der Menge Gedeihen!", "tokens": ["Gott", "schen\u00b7ke", "der", "Men\u00b7ge", "Ge\u00b7dei\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "----+--+-", "measure": "iambic.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Da wehet ein kalter, ein b\u00f6ser Wind,", "tokens": ["Da", "we\u00b7het", "ein", "kal\u00b7ter", ",", "ein", "b\u00f6\u00b7ser", "Wind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "$,", "ART