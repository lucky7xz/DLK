{"textgrid.poem.64069": {"metadata": {"author": {"name": "Ahlefeld, Charlotte von", "birth": "N.A.", "death": "N.A."}, "title": "Liebe und Hoffnung", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf meinem fr\u00fchen, stillen Gang durch's Leben", "tokens": ["Auf", "mei\u00b7nem", "fr\u00fc\u00b7hen", ",", "stil\u00b7len", "Gang", "durch's", "Le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sah ich in holder, lieblicher Gestalt", "tokens": ["Sah", "ich", "in", "hol\u00b7der", ",", "lieb\u00b7li\u00b7cher", "Ge\u00b7stalt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Liebe und die Hoffnung vor mir schweben,", "tokens": ["Die", "Lie\u00b7be", "und", "die", "Hoff\u00b7nung", "vor", "mir", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von lichten Himmelsglorien umwallt.", "tokens": ["Von", "lich\u00b7ten", "Him\u00b7mels\u00b7glo\u00b7ri\u00b7en", "um\u00b7wallt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Liebe reichte mir den Kranz von Rosen;", "tokens": ["Die", "Lie\u00b7be", "reich\u00b7te", "mir", "den", "Kranz", "von", "Ro\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich schlang entz\u00fcckt ihn um mein freies Haupt.", "tokens": ["Ich", "schlang", "ent\u00b7z\u00fcckt", "ihn", "um", "mein", "frei\u00b7es", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Hoffnung hatte unter s\u00fcssem Kosen", "tokens": ["Die", "Hoff\u00b7nung", "hat\u00b7te", "un\u00b7ter", "s\u00fcs\u00b7sem", "Ko\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit frischem Gr\u00fcn es duftend schon umlaubt.", "tokens": ["Mit", "fri\u00b7schem", "Gr\u00fcn", "es", "duf\u00b7tend", "schon", "um\u00b7laubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So folgt' ich seelig meinem stillen Wege,", "tokens": ["So", "folgt'", "ich", "see\u00b7lig", "mei\u00b7nem", "stil\u00b7len", "We\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und scheute nicht des ernsten Schicksals Zorn;", "tokens": ["Und", "scheu\u00b7te", "nicht", "des", "erns\u00b7ten", "Schick\u00b7sals", "Zorn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch ach, die Kr\u00e4nze welkten ohne Pflege,", "tokens": ["Doch", "ach", ",", "die", "Kr\u00e4n\u00b7ze", "welk\u00b7ten", "oh\u00b7ne", "Pfle\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und mich verwundete der Rosen Dorn.", "tokens": ["Und", "mich", "ver\u00b7wun\u00b7de\u00b7te", "der", "Ro\u00b7sen", "Dorn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Da stand ich z\u00fcrnend, und im bittern Grimme", "tokens": ["Da", "stand", "ich", "z\u00fcr\u00b7nend", ",", "und", "im", "bit\u00b7tern", "Grim\u00b7me"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "$,", "KON", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Riss ich den bunten Schmuck mir aus dem Haar,", "tokens": ["Riss", "ich", "den", "bun\u00b7ten", "Schmuck", "mir", "aus", "dem", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bis mir aus hohen L\u00fcften eine Stimme", "tokens": ["Bis", "mir", "aus", "ho\u00b7hen", "L\u00fcf\u00b7ten", "ei\u00b7ne", "Stim\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "APPR", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In Busen drang, der tief beklommen war.", "tokens": ["In", "Bu\u00b7sen", "drang", ",", "der", "tief", "be\u00b7klom\u00b7men", "war", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "PRELS", "ADJD", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "\u00bbso wirfst Du, sprach sie, Deines Lebens Frieden", "tokens": ["\u00bb", "so", "wirfst", "Du", ",", "sprach", "sie", ",", "Dei\u00b7nes", "Le\u00b7bens", "Frie\u00b7den"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ADV", "VAFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie welke Blumen in den niedern Staub?", "tokens": ["Wie", "wel\u00b7ke", "Blu\u00b7men", "in", "den", "nie\u00b7dern", "Staub", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PWAT", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und jeder h\u00f6here Genuss, der Dir beschieden,", "tokens": ["Und", "je\u00b7der", "h\u00f6\u00b7he\u00b7re", "Ge\u00b7nuss", ",", "der", "Dir", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wird Deines Kleinmuths eigensinn'ger Raub?", "tokens": ["Wird", "Dei\u00b7nes", "Klein\u00b7muths", "ei\u00b7gen\u00b7sinn'\u00b7ger", "Raub", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Behandle achtungsvoll die theuern Gaben,", "tokens": ["Be\u00b7hand\u00b7le", "ach\u00b7tungs\u00b7voll", "die", "theu\u00b7ern", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit denen Lieb' und Hoffnung Dich erfreut,", "tokens": ["Mit", "de\u00b7nen", "Lieb'", "und", "Hoff\u00b7nung", "Dich", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "NN", "KON", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wirst Du sie aus reiner Quelle laben,", "tokens": ["Und", "wirst", "Du", "sie", "aus", "rei\u00b7ner", "Quel\u00b7le", "la\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PPER", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So siehst Du bald sie jugendlich erneut.\u00ab", "tokens": ["So", "siehst", "Du", "bald", "sie", "ju\u00b7gend\u00b7lich", "er\u00b7neut", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPER", "ADJD", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Da sprach ich weinend: ach, mein Pfad ist enge,", "tokens": ["Da", "sprach", "ich", "wei\u00b7nend", ":", "ach", ",", "mein", "Pfad", "ist", "en\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "$.", "XY", "$,", "PPOSAT", "NN", "VAFIN", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und windet immer steiler sich hinan;", "tokens": ["Und", "win\u00b7det", "im\u00b7mer", "stei\u00b7ler", "sich", "hi\u00b7nan", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "PRF", "PTKVZ", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Durch schroffer Felsen starrendes Gedr\u00e4nge", "tokens": ["Durch", "schrof\u00b7fer", "Fel\u00b7sen", "star\u00b7ren\u00b7des", "Ge\u00b7dr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fchrt er empor, und doch nicht himmelan.", "tokens": ["F\u00fchrt", "er", "em\u00b7por", ",", "und", "doch", "nicht", "him\u00b7me\u00b7lan", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "KON", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Wie kann ich hier die heil'gen Bl\u00fcthen pflegen,", "tokens": ["Wie", "kann", "ich", "hier", "die", "heil'\u00b7gen", "Bl\u00fc\u00b7then", "pfle\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo keine Quelle rauscht, kein B\u00e4chlein fliesst,", "tokens": ["Wo", "kei\u00b7ne", "Quel\u00b7le", "rauscht", ",", "kein", "B\u00e4ch\u00b7lein", "fliesst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVFIN", "$,", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wo auf den bedornten rauhen Wegen", "tokens": ["Und", "wo", "auf", "den", "be\u00b7dorn\u00b7ten", "rau\u00b7hen", "We\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sich nur der Wehmuth Thr\u00e4ne still ergiesst.", "tokens": ["Sich", "nur", "der", "Weh\u00b7muth", "Thr\u00e4\u00b7ne", "still", "er\u00b7giesst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ART", "NN", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "\u00bbso lass der Liebe Rosen denn verbleichen,", "tokens": ["\u00bb", "so", "lass", "der", "Lie\u00b7be", "Ro\u00b7sen", "denn", "ver\u00b7blei\u00b7chen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "ART", "NN", "NN", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch halte fest der Hoffnung helles Gr\u00fcn!\u00ab", "tokens": ["Doch", "hal\u00b7te", "fest", "der", "Hoff\u00b7nung", "hel\u00b7les", "Gr\u00fcn", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ert\u00f6nte mir die Stimme sonder Gleichen,", "tokens": ["Er\u00b7t\u00f6n\u00b7te", "mir", "die", "Stim\u00b7me", "son\u00b7der", "Glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und schweigend sah ich meinen Kranz verbl\u00fchn.", "tokens": ["Und", "schwei\u00b7gend", "sah", "ich", "mei\u00b7nen", "Kranz", "ver\u00b7bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Sie welkten hin, die Rosen, deren D\u00fcfte", "tokens": ["Sie", "welk\u00b7ten", "hin", ",", "die", "Ro\u00b7sen", ",", "de\u00b7ren", "D\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "ART", "NN", "$,", "PRELAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit Himmelsahnungen mich einst berauscht.", "tokens": ["Mit", "Him\u00b7mel\u00b7sah\u00b7nun\u00b7gen", "mich", "einst", "be\u00b7rauscht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.3": {"text": "Mit Seufzern, ach, vermischt'ich nun die L\u00fcfte,", "tokens": ["Mit", "Seuf\u00b7zern", ",", "ach", ",", "ver\u00b7mischt'\u00b7ich", "nun", "die", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ITJ", "$,", "ADJD", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mir war, als sei mein Inn'res umgetauscht.", "tokens": ["Mir", "war", ",", "als", "sei", "mein", "Inn'\u00b7res", "um\u00b7ge\u00b7tauscht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Es zogen Ungewitter, Regenschauer", "tokens": ["Es", "zo\u00b7gen", "Un\u00b7ge\u00b7wit\u00b7ter", ",", "Re\u00b7gen\u00b7schau\u00b7er"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und St\u00fcrme drohend \u00fcber mir empor,", "tokens": ["Und", "St\u00fcr\u00b7me", "dro\u00b7hend", "\u00fc\u00b7ber", "mir", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So dass in's finstere Gebiet der Trauer", "tokens": ["So", "dass", "in's", "fins\u00b7te\u00b7re", "Ge\u00b7biet", "der", "Trau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "APPRART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sich still und ernst mein heitrer Sinn verlohr.", "tokens": ["Sich", "still", "und", "ernst", "mein", "hei\u00b7trer", "Sinn", "ver\u00b7lohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "KON", "ADJD", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Doch blieb die Hoffnung tr\u00f6stend mir zur Seite,", "tokens": ["Doch", "blieb", "die", "Hoff\u00b7nung", "tr\u00f6s\u00b7tend", "mir", "zur", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und sorgsam pfleg'ich noch den zarten Zweig,", "tokens": ["Und", "sorg\u00b7sam", "pfleg'\u00b7ich", "noch", "den", "zar\u00b7ten", "Zweig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den sie mir gab zum irdischen Geleite,", "tokens": ["Den", "sie", "mir", "gab", "zum", "ir\u00b7di\u00b7schen", "Ge\u00b7lei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVFIN", "APPRART", "ADJA", "NN",