{"dta.poem.2607": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Seliger Hintritt  \n  J. A. R. M. den 19. Febr.  1665.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bj\u00dfhero haben wir verlebte hin getragen/", "tokens": ["Bj\u00df\u00b7he\u00b7ro", "ha\u00b7ben", "wir", "ver\u00b7leb\u00b7te", "hin", "ge\u00b7tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "VVFIN", "ADV", "VVPP", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Die selbst des Alters Last von Thr\u00e4nen lo\u00df gezehlt.", "tokens": ["Die", "selbst", "des", "Al\u00b7ters", "Last", "von", "Thr\u00e4\u00b7nen", "lo\u00df", "ge\u00b7zehlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "NN", "APPR", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nun \u00e4ndert sich das Spiel/ und was bey zarten Tagen", "tokens": ["Nun", "\u00e4n\u00b7dert", "sich", "das", "Spiel", "/", "und", "was", "bey", "zar\u00b7ten", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$(", "KON", "PWS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In voller Bl\u00fcthe stund/ hat jetzt den Sarch erwehlt.", "tokens": ["In", "vol\u00b7ler", "Bl\u00fc\u00b7the", "stund", "/", "hat", "jetzt", "den", "Sarch", "er\u00b7wehlt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$(", "VAFIN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Holdseligstes Geschlecht/ ihr sch\u00f6nen Anmuths-Sonnen/", "tokens": ["Hold\u00b7se\u00b7ligs\u00b7tes", "Ge\u00b7schlecht", "/", "ihr", "sch\u00f6\u00b7nen", "An\u00b7muths\u00b7Son\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Durch welche Bre\u00dflau sich gleich Paphos Tempel macht.", "tokens": ["Durch", "wel\u00b7che", "Bre\u00df\u00b7lau", "sich", "gleich", "Pa\u00b7phos", "Tem\u00b7pel", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "PRF", "ADV", "NE", "NE", "VVFIN", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.7": {"text": "Welch Leid hat \u00fcber euch die Oberhand gewonnen?", "tokens": ["Welch", "Leid", "hat", "\u00fc\u00b7ber", "euch", "die", "O\u00b7ber\u00b7hand", "ge\u00b7won\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "APPR", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Verg\u00e4llt sich eure Lust/ verfinstert sich die Pracht?", "tokens": ["Ver\u00b7g\u00e4llt", "sich", "eu\u00b7re", "Lust", "/", "ver\u00b7fins\u00b7tert", "sich", "die", "Pracht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "$(", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ach ja! Die Augen sind nur Zeichen rauher Blicke/", "tokens": ["Ach", "ja", "!", "Die", "Au\u00b7gen", "sind", "nur", "Zei\u00b7chen", "rau\u00b7her", "Bli\u00b7cke", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$.", "ART", "NN", "VAFIN", "ADV", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die Seuffzer Bothen/ so was schweres melden an.", "tokens": ["Die", "Seuff\u00b7zer", "Bo\u00b7then", "/", "so", "was", "schwe\u00b7res", "mel\u00b7den", "an", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "ADV", "PWS", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und angeborne Hold fleucht/ wie best\u00fcrtzt/ zu r\u00fccke/", "tokens": ["Und", "an\u00b7ge\u00b7bor\u00b7ne", "Hold", "fleucht", "/", "wie", "be\u00b7st\u00fcrtzt", "/", "zu", "r\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$(", "PWAV", "VVPP", "$(", "PTKZU", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wo sind die Rosen hin? Was soll der Majoran?", "tokens": ["Wo", "sind", "die", "Ro\u00b7sen", "hin", "?", "Was", "soll", "der", "Ma\u00b7jo\u00b7ran", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "PTKVZ", "$.", "PWS", "VMFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ist die Ergetzligkeit von eurer Brust gewichen?", "tokens": ["Ist", "die", "Er\u00b7getz\u00b7lig\u00b7keit", "von", "eu\u00b7rer", "Brust", "ge\u00b7wi\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Rauscht jetzt kein s\u00fcsser West durchs Kercker-freye Haar?", "tokens": ["Rauscht", "jetzt", "kein", "s\u00fcs\u00b7ser", "West", "durchs", "Ker\u00b7cker\u00b7freye", "Haar", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PIAT", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.15": {"text": "Ist Liebe von dem Mund/ von Wangen Lust geschlichen?", "tokens": ["Ist", "Lie\u00b7be", "von", "dem", "Mund", "/", "von", "Wan\u00b7gen", "Lust", "ge\u00b7schli\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "ART", "NN", "$(", "APPR", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Kr\u00e4nckt/ was beh\u00e4glich vor/ ein unverhoffte Bahr?", "tokens": ["Kr\u00e4nckt", "/", "was", "be\u00b7h\u00e4g\u00b7lich", "vor", "/", "ein", "un\u00b7ver\u00b7hoff\u00b7te", "Bahr", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "PWS", "ADJD", "PTKVZ", "$(", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.17": {"text": "Hoch m\u00e4chtig mu\u00df das Leid in weichen Hertzen w\u00fctten/", "tokens": ["Hoch", "m\u00e4ch\u00b7tig", "mu\u00df", "das", "Leid", "in", "wei\u00b7chen", "Hert\u00b7zen", "w\u00fct\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VMFIN", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Nun eures Ordens Ruhm und Blum\u2019 ins Grab verf\u00e4llt.", "tokens": ["Nun", "eu\u00b7res", "Or\u00b7dens", "Ruhm", "und", "Blum'", "ins", "Grab", "ver\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "NN", "KON", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Zu fr\u00fch hat Atropos den Faden abgeschnitten/", "tokens": ["Zu", "fr\u00fch", "hat", "A\u00b7tro\u00b7pos", "den", "Fa\u00b7den", "ab\u00b7ge\u00b7schnit\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VAFIN", "NE", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Der sch\u00f6nen ", "tokens": ["Der", "sch\u00f6\u00b7nen"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.21": {"text": "Das eintzig werthe Kind/ der Mutter Lust und Prangen/", "tokens": ["Das", "eint\u00b7zig", "wert\u00b7he", "Kind", "/", "der", "Mut\u00b7ter", "Lust", "und", "Pran\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVFIN", "NN", "$(", "ART", "NN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Der Freuden Inbegriff/ und aller Hoffnung Schatz/", "tokens": ["Der", "Freu\u00b7den", "In\u00b7be\u00b7griff", "/", "und", "al\u00b7ler", "Hoff\u00b7nung", "Schatz", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "KON", "PIAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "L\u00e4st freylich hinter sich ein schmertzliches Verlangen/", "tokens": ["L\u00e4st", "frey\u00b7lich", "hin\u00b7ter", "sich", "ein", "schmertz\u00b7li\u00b7ches", "Ver\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PRF", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Uud einer vollen See/ der Thr\u00e4nen Fluten/ Platz.", "tokens": ["U\u00b7ud", "ei\u00b7ner", "vol\u00b7len", "See", "/", "der", "Thr\u00e4\u00b7nen", "Flu\u00b7ten", "/", "Platz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$(", "ART", "NN", "NN", "$(", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.25": {"text": "Jm Anfang bester Jahr\u2019/ im Blumen-reichen Lentzen/", "tokens": ["Jm", "An\u00b7fang", "bes\u00b7ter", "Jahr'", "/", "im", "Blu\u00b7men\u00b7rei\u00b7chen", "Lent\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$(", "APPRART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Da man die Jugend fast wie eine F\u00fcrstin ehrt/", "tokens": ["Da", "man", "die", "Ju\u00b7gend", "fast", "wie", "ei\u00b7ne", "F\u00fcrs\u00b7tin", "ehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "ADV", "KOKOM", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Wenn jede Glieder/ wie die g\u00f6ldnen Sterne gl\u00e4ntzen/", "tokens": ["Wenn", "je\u00b7de", "Glie\u00b7der", "/", "wie", "die", "g\u00f6ld\u00b7nen", "Ster\u00b7ne", "gl\u00e4nt\u00b7zen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "$(", "KOKOM", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und der beredte Mund ein w\u00e4chsern Hertz beth\u00f6rt/", "tokens": ["Und", "der", "be\u00b7red\u00b7te", "Mund", "ein", "w\u00e4ch\u00b7sern", "Hertz", "be\u00b7th\u00f6rt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Verlacht man nur das Grab. Sie aber/ die verschieden/", "tokens": ["Ver\u00b7lacht", "man", "nur", "das", "Grab", ".", "Sie", "a\u00b7ber", "/", "die", "ver\u00b7schie\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ART", "NN", "$.", "PPER", "ADV", "$(", "ART", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Hat ihre Jahre so verschwenderisch nie geliebt/", "tokens": ["Hat", "ih\u00b7re", "Jah\u00b7re", "so", "ver\u00b7schwen\u00b7de\u00b7risch", "nie", "ge\u00b7liebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "ADV", "VVPP", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.31": {"text": "Der Wunsch war/ frey zu seyn und hin zufahrn im Frieden/", "tokens": ["Der", "Wunsch", "war", "/", "frey", "zu", "seyn", "und", "hin", "zu\u00b7fahrn", "im", "Frie\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$(", "ADJD", "PTKZU", "VAINF", "KON", "ADV", "ADJD", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Den dort/ den Gl\u00e4ubigen der Allerh\u00f6chste gibt.", "tokens": ["Den", "dort", "/", "den", "Gl\u00e4u\u00b7bi\u00b7gen", "der", "Al\u00b7ler\u00b7h\u00f6chs\u00b7te", "gibt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$(", "ART", "NN", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "De\u00df engen Lebens frist hat Tugend doch erweitert.", "tokens": ["De\u00df", "en\u00b7gen", "Le\u00b7bens", "frist", "hat", "Tu\u00b7gend", "doch", "er\u00b7wei\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VAFIN", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Es schien das Ebenbild der Gratien an ihr/", "tokens": ["Es", "schien", "das", "E\u00b7ben\u00b7bild", "der", "Gra\u00b7ti\u00b7en", "an", "ihr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "PPOSAT", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Wenn sich die Stirn in Zucht und Sch\u00f6nheit aufgeheitert/", "tokens": ["Wenn", "sich", "die", "Stirn", "in", "Zucht", "und", "Sch\u00f6n\u00b7heit", "auf\u00b7ge\u00b7hei\u00b7tert", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und stellt uns einen May der Liebligkeiten f\u00fcr.", "tokens": ["Und", "stellt", "uns", "ei\u00b7nen", "May", "der", "Lieb\u00b7lig\u00b7kei\u00b7ten", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Die niedliche Gestalt/ die sittsamen Geberden/", "tokens": ["Die", "nied\u00b7li\u00b7che", "Ge\u00b7stalt", "/", "die", "sitt\u00b7sa\u00b7men", "Ge\u00b7ber\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+++-+-", "measure": "unknown.measure.septa"}, "line.38": {"text": "Der Augen rasche Glut und s\u00fcsser Zwincker-Blitz", "tokens": ["Der", "Au\u00b7gen", "ra\u00b7sche", "Glut", "und", "s\u00fcs\u00b7ser", "Zwin\u00b7cker\u00b7Blitz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Bestrahlte gleichsam hier den Ball der grossen Erden/", "tokens": ["Be\u00b7strahl\u00b7te", "gleich\u00b7sam", "hier", "den", "Ball", "der", "gros\u00b7sen", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und im Gehirue nahm die Pallas ihren Sitz/", "tokens": ["Und", "im", "Ge\u00b7hi\u00b7rue", "nahm", "die", "Pal\u00b7las", "ih\u00b7ren", "Sitz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "So bald der Mund entschlo\u00df das Helffenbein der Pforte/", "tokens": ["So", "bald", "der", "Mund", "ent\u00b7schlo\u00df", "das", "Helf\u00b7fen\u00b7bein", "der", "Pfor\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text"