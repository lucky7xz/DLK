{"dta.poem.4129": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "An Ludwig Sigismund Ruhl.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Den R\u00fccken jenen Fluren zugewendet,", "tokens": ["Den", "R\u00fc\u00b7cken", "je\u00b7nen", "Flu\u00b7ren", "zu\u00b7ge\u00b7wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo sich der Himmel nimmer m\u00fcde spendet", "tokens": ["Wo", "sich", "der", "Him\u00b7mel", "nim\u00b7mer", "m\u00fc\u00b7de", "spen\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "ART", "NN", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit seines F\u00fcllhorns frischen Blumenschauern.", "tokens": ["Mit", "sei\u00b7nes", "F\u00fcll\u00b7horns", "fri\u00b7schen", "Blu\u00b7men\u00b7schau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da fa\u00dfte pl\u00f6tzlich dich ein hei\u00dfes Trauern,", "tokens": ["Da", "fa\u00df\u00b7te", "pl\u00f6tz\u00b7lich", "dich", "ein", "hei\u00b7\u00dfes", "Trau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das \u00fcber ihren Strom dir nachgesendet", "tokens": ["Das", "\u00fc\u00b7ber", "ih\u00b7ren", "Strom", "dir", "nach\u00b7ge\u00b7sen\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "PPOSAT", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Stadt, der du, ich wei\u00df nicht was, verpf\u00e4ndet:", "tokens": ["Die", "Stadt", ",", "der", "du", ",", "ich", "wei\u00df", "nicht", "was", ",", "ver\u00b7pf\u00e4n\u00b7det", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "$,", "PPER", "VVFIN", "PTKNEG", "PIS", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich h\u00f6rte deine Seufzer mit Bedauern.", "tokens": ["Ich", "h\u00f6r\u00b7te", "dei\u00b7ne", "Seuf\u00b7zer", "mit", "Be\u00b7dau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Germania, mach' auf dich ohne Weilen,", "tokens": ["Ger\u00b7ma\u00b7nia", ",", "mach'", "auf", "dich", "oh\u00b7ne", "Wei\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "APPR", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Geschm\u00fcckt mit aller deiner Reize Waffen,", "tokens": ["Ge\u00b7schm\u00fcckt", "mit", "al\u00b7ler", "dei\u00b7ner", "Rei\u00b7ze", "Waf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PIAT", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den hart gefeiten Fl\u00fcchtling zu begr\u00fc\u00dfen!", "tokens": ["Den", "hart", "ge\u00b7fei\u00b7ten", "Fl\u00fccht\u00b7ling", "zu", "be\u00b7gr\u00fc\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Hei\u00df der zw\u00f6lf Monde Schaar voraus dir eilen,", "tokens": ["Hei\u00df", "der", "zw\u00f6lf", "Mon\u00b7de", "Schaar", "vo\u00b7raus", "dir", "ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "CARD", "NE", "NN", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und was ein jeder Bestes kann erschaffen,", "tokens": ["Und", "was", "ein", "je\u00b7der", "Bes\u00b7tes", "kann", "er\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "PIAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Leg' er als Angebind' ihm gern zu F\u00fc\u00dfen.", "tokens": ["Leg'", "er", "als", "An\u00b7ge\u00b7bind'", "ihm", "gern", "zu", "F\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOUS", "NN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}