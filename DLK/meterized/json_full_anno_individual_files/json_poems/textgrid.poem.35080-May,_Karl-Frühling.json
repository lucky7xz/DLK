{"textgrid.poem.35080": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchling", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein linder Fr\u00fchlingshauch", "tokens": ["Es", "ist", "ein", "lin\u00b7der", "Fr\u00fch\u00b7lings\u00b7hauch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Heut \u00fcbers Feld gegangen,", "tokens": ["Heut", "\u00fc\u00b7bers", "Feld", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und nun will Wiese, Baum und Strauch", "tokens": ["Und", "nun", "will", "Wie\u00b7se", ",", "Baum", "und", "Strauch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "NN", "$,", "NE", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In tausend Bl\u00fcthen prangen.", "tokens": ["In", "tau\u00b7send", "Bl\u00fc\u00b7then", "pran\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Schon morgen wohl, schon \u00fcber Nacht", "tokens": ["Schon", "mor\u00b7gen", "wohl", ",", "schon", "\u00fc\u00b7ber", "Nacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "$,", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Giebts rings ein duftend Sprie\u00dfen;", "tokens": ["Giebts", "rings", "ein", "duf\u00b7tend", "Sprie\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word"