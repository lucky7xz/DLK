{"dta.poem.13188": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z ehndes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O k\u00e4men, sprachen wir, die K\u00f6nige der Erden!", "tokens": ["O", "k\u00e4\u00b7men", ",", "spra\u00b7chen", "wir", ",", "die", "K\u00f6\u00b7ni\u00b7ge", "der", "Er\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "VVFIN", "PPER", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie m\u00fc\u00dften theils erstaunt, theils eifers\u00fcchtig werden.", "tokens": ["Sie", "m\u00fc\u00df\u00b7ten", "theils", "er\u00b7staunt", ",", "theils", "ei\u00b7fer\u00b7s\u00fcch\u00b7tig", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Besonders die dem Haus die St\u00fcrzung zugedacht,", "tokens": ["Be\u00b7son\u00b7ders", "die", "dem", "Haus", "die", "St\u00fcr\u00b7zung", "zu\u00b7ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "50Und \u00fcber solchen Fall, doch allzu fr\u00fch, gelacht.", "tokens": ["\u00fc\u00b7ber", "sol\u00b7chen", "Fall", ",", "doch", "all\u00b7zu", "fr\u00fch", ",", "ge\u00b7lacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "ADV", "PTKA", "ADJD", "$,", "VVPP", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "Wie? wann wir, d\u00fcnckte mich, nun aus der Vorwelt Gr\u00fcften", "tokens": ["Wie", "?", "wann", "wir", ",", "d\u00fcnck\u00b7te", "mich", ",", "nun", "aus", "der", "Vor\u00b7welt", "Gr\u00fcf\u00b7ten"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$.", "PWAV", "PPER", "$,", "VVFIN", "PPER", "$,", "ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der alten Helden-Schaar zu diesem Umstand r\u00fcfften:", "tokens": ["Der", "al\u00b7ten", "Hel\u00b7den\u00b7Schaar", "zu", "die\u00b7sem", "Um\u00b7stand", "r\u00fcff\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Kommt! sehet diesen Hof, das h\u00f6chste K\u00f6nigs-Paar,", "tokens": ["Kommt", "!", "se\u00b7het", "die\u00b7sen", "Hof", ",", "das", "h\u00f6chs\u00b7te", "K\u00f6\u00b7nigs\u00b7Paar", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "VVFIN", "PDAT", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Das Hilff- und Waffen-blo\u00df im Krieg verflochten war!", "tokens": ["Das", "Hilff", "und", "Waf\u00b7fen\u00b7blo\u00df", "im", "Krieg", "ver\u00b7floch\u00b7ten", "war", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "NN", "APPRART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "55Sie k\u00e4men, diesen Saal, die Majest\u00e4t zu sehen;", "tokens": ["k\u00e4\u00b7men", ",", "die\u00b7sen", "Saal", ",", "die", "Ma\u00b7jes\u00b7t\u00e4t", "zu", "se\u00b7hen", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PDAT", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.10": {"text": "Was wurd\u2019 in ihrem Sinn, in ihrem Aug\u2019 entstehen?", "tokens": ["Was", "wurd'", "in", "ih\u00b7rem", "Sinn", ",", "in", "ih\u00b7rem", "Aug'", "ent\u00b7ste\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der Freund empf\u00e4nde Trost, der Feind erz\u00fcrnte sich,", "tokens": ["Der", "Freund", "em\u00b7pf\u00e4n\u00b7de", "Trost", ",", "der", "Feind", "er\u00b7z\u00fcrn\u00b7te", "sich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und die Begenbenheit w\u00e4r beyden wunderlich.", "tokens": ["Und", "die", "Be\u00b7gen\u00b7ben\u00b7heit", "w\u00e4r", "bey\u00b7den", "wun\u00b7der\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PIAT", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "F\u00fcr Ehrfurcht schl\u00fcgen sie der Augen Blicke nieder,", "tokens": ["F\u00fcr", "Ehr\u00b7furcht", "schl\u00fc\u00b7gen", "sie", "der", "Au\u00b7gen", "Bli\u00b7cke", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "60Und suchten Demuth-voll der Gr\u00fcfte Ruhe wieder.", "tokens": ["such\u00b7ten", "De\u00b7muth\u00b7voll", "der", "Gr\u00fcf\u00b7te", "Ru\u00b7he", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ART", "ADJA", "NN", "ADV", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}}}}}