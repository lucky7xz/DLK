{"textgrid.poem.48771": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "58. Auf Eines seinen Namenstag, in Astrachan geschrieben, 1638 im Julio", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sprach die Musen an auf diesen deinen Tag", "tokens": ["Ich", "sprach", "die", "Mu\u00b7sen", "an", "auf", "die\u00b7sen", "dei\u00b7nen", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "APPR", "PDAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "um einen saubern Vers, der f\u00fcr Gelehrte t\u00f6chte", "tokens": ["um", "ei\u00b7nen", "sau\u00b7bern", "Vers", ",", "der", "f\u00fcr", "Ge\u00b7lehr\u00b7te", "t\u00f6ch\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und in der Priefung recht gefunden werden m\u00f6chte;", "tokens": ["und", "in", "der", "Prie\u00b7fung", "recht", "ge\u00b7fun\u00b7den", "wer\u00b7den", "m\u00f6ch\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "VVPP", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "gleichwie ich vor der Zeit wol eh' zu tichten pflag,", "tokens": ["gleich\u00b7wie", "ich", "vor", "der", "Zeit", "wol", "eh'", "zu", "tich\u00b7ten", "pflag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ART", "NN", "ADV", "KOUS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "als ich frei aller Not an meiner ", "tokens": ["als", "ich", "frei", "al\u00b7ler", "Not", "an", "mei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "PIAT", "NN", "APPR", "PPOSAT"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "und mir kein \u00dcbermut die k\u00fchnen Sinnen schw\u00e4chte,", "tokens": ["und", "mir", "kein", "\u00dc\u00b7ber\u00b7mut", "die", "k\u00fch\u00b7nen", "Sin\u00b7nen", "schw\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PIAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "kein grober \u00dcbermut, den ich zwar hier verfechte,", "tokens": ["kein", "gro\u00b7ber", "\u00dc\u00b7ber\u00b7mut", ",", "den", "ich", "zwar", "hier", "ver\u00b7fech\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "wenn Rechten w\u00e4r' erl\u00e4ubt. Doch bleib es, wo es mag.", "tokens": ["wenn", "Rech\u00b7ten", "w\u00e4r'", "er\u00b7l\u00e4ubt", ".", "Doch", "bleib", "es", ",", "wo", "es", "mag."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "abbreviation"], "pos": ["KOUS", "NN", "VAFIN", "VVPP", "$.", "KON", "VVFIN", "PPER", "$,", "PWAV", "PPER", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich sprach die Musen an, ich rief sie an die Neune,", "tokens": ["Ich", "sprach", "die", "Mu\u00b7sen", "an", ",", "ich", "rief", "sie", "an", "die", "Neu\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ich rief, ich schrie sie an; nicht eine sprach ein Wort.", "tokens": ["ich", "rief", ",", "ich", "schrie", "sie", "an", ";", "nicht", "ei\u00b7ne", "sprach", "ein", "Wort", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$.", "PTKNEG", "ART", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es war kein Zynthius auf Pindus ganzem Haine;", "tokens": ["Es", "war", "kein", "Zynt\u00b7hi\u00b7us", "auf", "Pin\u00b7dus", "gan\u00b7zem", "Hai\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NE", "APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "leer war ich kommen hier, leer mu\u00dft' ich wieder fort.", "tokens": ["leer", "war", "ich", "kom\u00b7men", "hier", ",", "leer", "mu\u00dft'", "ich", "wie\u00b7der", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "VVFIN", "ADV", "$,", "ADJD", "VMFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Lie\u00df, Bruder, di\u00df Sonnet und schmei\u00df es \u00fcber Port,", "tokens": ["Lie\u00df", ",", "Bru\u00b7der", ",", "di\u00df", "Son\u00b7net", "und", "schmei\u00df", "es", "\u00fc\u00b7ber", "Port", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "w