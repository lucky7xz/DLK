{"textgrid.poem.55929": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Morgue", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da liegen sie bereit, als ob es g\u00e4lte,", "tokens": ["Da", "lie\u00b7gen", "sie", "be\u00b7reit", ",", "als", "ob", "es", "g\u00e4l\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "KOKOM", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "nachtr\u00e4glich eine Handlung zu erfinden,", "tokens": ["nach\u00b7tr\u00e4g\u00b7lich", "ei\u00b7ne", "Hand\u00b7lung", "zu", "er\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "die mit einander und mit dieser K\u00e4lte", "tokens": ["die", "mit", "ein\u00b7an\u00b7der", "und", "mit", "die\u00b7ser", "K\u00e4l\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PRF", "KON", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "sie zu vers\u00f6hnen wei\u00df und zu verbinden;", "tokens": ["sie", "zu", "ver\u00b7s\u00f6h\u00b7nen", "wei\u00df", "und", "zu", "ver\u00b7bin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "VVFIN", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "denn das ist alles noch wie ohne Schlu\u00df.", "tokens": ["denn", "das", "ist", "al\u00b7les", "noch", "wie", "oh\u00b7ne", "Schlu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "PIS", "ADV", "KOKOM", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wasf\u00fcr ein Name h\u00e4tte in den Taschen", "tokens": ["Was\u00b7f\u00fcr", "ein", "Na\u00b7me", "h\u00e4t\u00b7te", "in", "den", "Ta\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "sich finden sollen? An dem \u00dcberdru\u00df", "tokens": ["sich", "fin\u00b7den", "sol\u00b7len", "?", "An", "dem", "\u00dc\u00b7berd\u00b7ru\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "VVINF", "VMFIN", "$.", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "um ihren Mund hat man herumgewaschen:", "tokens": ["um", "ih\u00b7ren", "Mund", "hat", "man", "her\u00b7um\u00b7ge\u00b7wa\u00b7schen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "er ging nicht ab; er wurde nur ganz rein.", "tokens": ["er", "ging", "nicht", "ab", ";", "er", "wur\u00b7de", "nur", "ganz", "rein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PTKVZ", "$.", "PPER", "VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die B\u00e4rte stehen, noch ein wenig h\u00e4rter,", "tokens": ["Die", "B\u00e4r\u00b7te", "ste\u00b7hen", ",", "noch", "ein", "we\u00b7nig", "h\u00e4r\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "ART", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "doch ordentlicher im Geschmack der W\u00e4rter,", "tokens": ["doch", "or\u00b7dent\u00b7li\u00b7cher", "im", "Ge\u00b7schmack", "der", "W\u00e4r\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "nur um die Gaffenden nicht anzuwidern.", "tokens": ["nur", "um", "die", "Gaf\u00b7fen\u00b7den", "nicht", "an\u00b7zu\u00b7wi\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PTKNEG", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Augen haben hinter ihren Lidern", "tokens": ["Die", "Au\u00b7gen", "ha\u00b7ben", "hin\u00b7ter", "ih\u00b7ren", "Li\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "sich umgewandt und schauen jetzt hinein.", "tokens": ["sich", "um\u00b7ge\u00b7wandt", "und", "schau\u00b7en", "jetzt", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVPP", "KON", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Da liegen sie bereit, als ob es g\u00e4lte,", "tokens": ["Da", "lie\u00b7gen", "sie", "be\u00b7reit", ",", "als", "ob", "es", "g\u00e4l\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "KOKOM", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "nachtr\u00e4glich eine Handlung zu erfinden,", "tokens": ["nach\u00b7tr\u00e4g