{"dta.poem.9998": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die  \n Unterwerfung gefallner Engel  \n und ihre Bestimmung  \n zu  \n Schutzgeistern der Menschen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20674-3", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So der erhabne Gesandte von Gott. Ein leises", "tokens": ["So", "der", "er\u00b7hab\u00b7ne", "Ge\u00b7sand\u00b7te", "von", "Gott", ".", "Ein", "lei\u00b7ses"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "APPR", "NN", "$.", "ART", "ADJA"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Lief durch die ganze Versammlung. Als wenn frisch-", "tokens": ["Lief", "durch", "die", "gan\u00b7ze", "Ver\u00b7samm\u00b7lung", ".", "Als", "wenn", "frisch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$.", "KOUS", "KOUS", "TRUNC"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Durch ein Geh\u00f6lz von silbernen Eschen sich kr\u00e4useln,", "tokens": ["Durch", "ein", "Ge\u00b7h\u00f6lz", "von", "sil\u00b7ber\u00b7nen", "E\u00b7schen", "sich", "kr\u00e4u\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN", "PRF", "VVINF", "$,"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Um die Locken des Wanderers spielen, der, ganz schon", "tokens": ["Um", "die", "Lo\u00b7cken", "des", "Wan\u00b7de\u00b7rers", "spie\u00b7len", ",", "der", ",", "ganz", "schon"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KOUI", "ART", "NN", "ART", "NN", "VVINF", "$,", "PRELS", "$,", "ADV", "ADV"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Von der flammenden Gluth, leichtathmender durch sie", "tokens": ["Von", "der", "flam\u00b7men\u00b7den", "Gluth", ",", "leich\u00b7tath\u00b7men\u00b7der", "durch", "sie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "ADJD", "APPR", "PPER"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Aber bald sank das frohe Ger\u00e4usch in vorige Stille,", "tokens": ["A\u00b7ber", "bald", "sank", "das", "fro\u00b7he", "Ge\u00b7r\u00e4usch", "in", "vo\u00b7ri\u00b7ge", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.7": {"text": "Da mit freudegl\u00e4nzen der Stirn Orion so anhub:", "tokens": ["Da", "mit", "freu\u00b7de\u00b7gl\u00e4n\u00b7zen", "der", "Stirn", "O\u00b7rion", "so", "an\u00b7hub", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ART", "NN", "NN", "ADV", "VVFIN", "$."], "meter": "--+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}}}}