{"dta.poem.1199": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auff ihren Nahmens-Tag.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Auff Demant und Rubin/ auff Rosen und Narcissen/", "tokens": ["Auff", "De\u00b7mant", "und", "Ru\u00b7bin", "/", "auff", "Ro\u00b7sen", "und", "Nar\u00b7cis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NE", "$(", "APPR", "NN", "KON", "NN", "$("], "meter": "--+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Soll billig meine Hand ein Lied", "tokens": ["Soll", "bil\u00b7lig", "mei\u00b7ne", "Hand", "ein", "Lied"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADJD", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu setzen heute seyn bem\u00fcht;", "tokens": ["Zu", "set\u00b7zen", "heu\u00b7te", "seyn", "be\u00b7m\u00fcht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADV", "VAINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nichts will in solcher Eil aus meiner Feder fl\u00fcssen/", "tokens": ["Nichts", "will", "in", "sol\u00b7cher", "Eil", "aus", "mei\u00b7ner", "Fe\u00b7der", "fl\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nichts f\u00e4llet mir f\u00fcr Freuden bey/", "tokens": ["Nichts", "f\u00e4l\u00b7let", "mir", "f\u00fcr", "Freu\u00b7den", "bey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "NN", "APPR", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Amaranthens w\u00fcrdig sey.", "tokens": ["Das", "A\u00b7ma\u00b7ran\u00b7thens", "w\u00fcr\u00b7dig", "sey", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nimm/ Nimphe/ g\u00fctig an das Opffer treuer H\u00e4nde:", "tokens": ["Nimm", "/", "Nim\u00b7phe", "/", "g\u00fc\u00b7tig", "an", "das", "Opf\u00b7fer", "treu\u00b7er", "H\u00e4n\u00b7de", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "NE", "$(", "ADJD", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wer wenig/ aber willig giebt/", "tokens": ["Wer", "we\u00b7nig", "/", "a\u00b7ber", "wil\u00b7lig", "giebt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "$(", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ist bey den G\u00f6ttern auch beliebt.", "tokens": ["Ist", "bey", "den", "G\u00f6t\u00b7tern", "auch", "be\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Auff Jahre sonder Ziel/ auff Gl\u00fccke sonder Ende", "tokens": ["Auff", "Jah\u00b7re", "son\u00b7der", "Ziel", "/", "auff", "Gl\u00fc\u00b7cke", "son\u00b7der", "En\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$(", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ist zu Bezeugung seiner Pflicht", "tokens": ["Ist", "zu", "Be\u00b7zeu\u00b7gung", "sei\u00b7ner", "Pflicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Silvanders treuer Wunsch gericht.", "tokens": ["Sil\u00b7van\u00b7ders", "treu\u00b7er", "Wunsch", "ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}