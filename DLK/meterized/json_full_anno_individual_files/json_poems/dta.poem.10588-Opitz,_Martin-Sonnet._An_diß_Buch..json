{"dta.poem.10588": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet.  \n  An di\u00df Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "So wiltu dennoch jetzt au\u00df meinen H\u00e4ndenscheiden", "tokens": ["So", "wil\u00b7tu", "den\u00b7noch", "jetzt", "au\u00df", "mei\u00b7nen", "H\u00e4n\u00b7den\u00b7schei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du kleines Buch vnd auch mit andern sein veracht:", "tokens": ["Du", "klei\u00b7nes", "Buch", "vnd", "auch", "mit", "an\u00b7dern", "sein", "ver\u00b7acht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "KON", "ADV", "APPR", "PIS", "VAINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gewi\u00df du weissest nicht wie h\u00f6nisch man jetzt lacht/", "tokens": ["Ge\u00b7wi\u00df", "du", "weis\u00b7sest", "nicht", "wie", "h\u00f6\u00b7nisch", "man", "jetzt", "lacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VVFIN", "PTKNEG", "KOKOM", "ADJD", "PIS", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie schwerlich sey der Welt spitzfindigkeit zumeyden.", "tokens": ["Wie", "schwer\u00b7lich", "sey", "der", "Welt", "spitz\u00b7fin\u00b7dig\u00b7keit", "zu\u00b7mey\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es mu\u00df ein jeglich ding der Menschen vrtheil leyden", "tokens": ["Es", "mu\u00df", "ein", "jeg\u00b7lich", "ding", "der", "Men\u00b7schen", "vrtheil", "ley\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "PIAT", "NN", "ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Vnd/ ob es tauglich sey/ steht nicht in seiner Macht/", "tokens": ["Vnd", "/", "ob", "es", "taug\u00b7lich", "sey", "/", "steht", "nicht", "in", "sei\u00b7ner", "Macht", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "KOUS", "PPER", "ADJD", "VAFIN", "$(", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der meiste theil ist doch auff schm\u00e4hen nur bedacht/", "tokens": ["Der", "meis\u00b7te", "theil", "ist", "doch", "auff", "schm\u00e4\u00b7hen", "nur", "be\u00b7dacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "APPR", "VVFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Vnd denckt was er nicht kan/ dasselbe m\u00fc\u00df er neyden.", "tokens": ["Vnd", "denckt", "was", "er", "nicht", "kan", "/", "das\u00b7sel\u00b7be", "m\u00fc\u00df", "er", "ney\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PWS", "PPER", "PTKNEG", "VMFIN", "$(", "PDAT", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Noch dennoch (da\u00df du nicht so offt vnd viel von mir", "tokens": ["Noch", "den\u00b7noch", "(", "da\u00df", "du", "nicht", "so", "offt", "vnd", "viel", "von", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$(", "KOUS", "PPER", "PTKNEG", "ADV", "ADV", "KON", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Aufss newe dulden d\u00fcrffst da\u00df ich dich neme f\u00fcr", "tokens": ["Aufss", "ne\u00b7we", "dul\u00b7den", "d\u00fcrffst", "da\u00df", "ich", "dich", "ne\u00b7me", "f\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "VVINF", "VMFIN", "KOUS", "PPER", "PRF", "VVFIN", "APPR"], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}, "line.11": {"text": "Mu\u00df ich dir lo\u00df zu sein vnd au\u00dfzugehn erleuben/", "tokens": ["Mu\u00df", "ich", "dir", "lo\u00df", "zu", "sein", "vnd", "au\u00df\u00b7zu\u00b7gehn", "er\u00b7leu\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PTKVZ", "PTKZU", "VAINF", "KON", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So zieh derhalben hin/ weil dir es so gefellt/", "tokens": ["So", "zieh", "der\u00b7hal\u00b7ben", "hin", "/", "weil", "dir", "es", "so", "ge\u00b7fellt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PAV", "PTKVZ", "$(", "KOUS", "PPER", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Vnd h\u00f6r dein Vrthel an/ zieh hin/ zieh in die Welt/", "tokens": ["Vnd", "h\u00f6r", "dein", "Vr\u00b7thel", "an", "/", "zieh", "hin", "/", "zieh", "in", "die", "Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$(", "VVFIN", "PTKVZ", "$(", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Du hettest aber wol zu Hause k\u00f6nnen bleiben.", "tokens": ["Du", "het\u00b7test", "a\u00b7ber", "wol", "zu", "Hau\u00b7se", "k\u00f6n\u00b7nen", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "APPR", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}