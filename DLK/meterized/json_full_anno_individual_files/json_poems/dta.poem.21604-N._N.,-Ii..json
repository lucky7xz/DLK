{"dta.poem.21604": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Ii.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Was rauschtet ihr f\u00fcr wunderbare Hymnen,", "tokens": ["Was", "rauschtet", "ihr", "f\u00fcr", "wun\u00b7der\u00b7ba\u00b7re", "Hym\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ihr sanftgeneigten Birkenh\u00e4upter", "tokens": ["Ihr", "sanft\u00b7ge\u00b7neig\u00b7ten", "Bir\u00b7ken\u00b7h\u00e4up\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Durchs stumme, traumgewiegte Nachtesdunkel,", "tokens": ["Durchs", "stum\u00b7me", ",", "traum\u00b7ge\u00b7wieg\u00b7te", "Nach\u00b7tes\u00b7dun\u00b7kel", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das eure schneeige Hermelinumwandung nur,", "tokens": ["Das", "eu\u00b7re", "schne\u00b7ei\u00b7ge", "Her\u00b7me\u00b7li\u00b7num\u00b7wan\u00b7dung", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "ADJA", "NN", "ADV", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Und durch die schwarzen Laubesh\u00e4nge niedertropfend", "tokens": ["Und", "durch", "die", "schwar\u00b7zen", "Lau\u00b7bes\u00b7h\u00e4n\u00b7ge", "nie\u00b7der\u00b7trop\u00b7fend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Des Mantels Silberfluth zu lichten wagt?", "tokens": ["Des", "Man\u00b7tels", "Sil\u00b7ber\u00b7fluth", "zu", "lich\u00b7ten", "wagt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wie seltsamlich noch nie vernommene Melodie\u2019n", "tokens": ["Wie", "selt\u00b7sam\u00b7lich", "noch", "nie", "ver\u00b7nom\u00b7me\u00b7ne", "Me\u00b7lo\u00b7die'n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ADV", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Raunt mir des leisbefl\u00fcgelten Windes Mund? \u2014", "tokens": ["Raunt", "mir", "des", "leis\u00b7be\u00b7fl\u00fc\u00b7gel\u00b7ten", "Win\u00b7des", "Mund", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "NN", "$.", "$("], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.9": {"text": "Mir ists, als sei von jedem Dinge", "tokens": ["Mir", "ists", ",", "als", "sei", "von", "je\u00b7dem", "Din\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "VAFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die \u00e4u\u00dfere Trugumh\u00fcllung fortgezogen,", "tokens": ["Die", "\u00e4u\u00b7\u00dfe\u00b7re", "Tru\u00b7gum\u00b7h\u00fcl\u00b7lung", "fort\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Als ob ich Jedes k\u00f6nnt\u2019 erkennen", "tokens": ["Als", "ob", "ich", "Je\u00b7des", "k\u00f6nnt'", "er\u00b7ken\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "PIS", "VMFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "In seines Wesens tiefster Eigenheit,", "tokens": ["In", "sei\u00b7nes", "We\u00b7sens", "tiefs\u00b7ter", "Ei\u00b7gen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Als wenn ich lauschte an dem Urborn alles Seins und Werdes.", "tokens": ["Als", "wenn", "ich", "lauschte", "an", "dem", "Ur\u00b7born", "al\u00b7les", "Seins", "und", "Wer\u00b7des", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "PIAT", "NN", "KON", "NN", "$."], "meter": "+--+--+-+-+-+-", "measure": "elegiambus"}, "line.14": {"text": "Erhabenes f\u00fchl\u2019 ich auf mich niederst\u00fcrmen,", "tokens": ["Er\u00b7ha\u00b7be\u00b7nes", "f\u00fchl'", "ich", "auf", "mich", "nie\u00b7ders\u00b7t\u00fcr\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Noch nie geklungne Saiten beben sonderartge Lieder", "tokens": ["Noch", "nie", "ge\u00b7klung\u00b7ne", "Sai\u00b7ten", "be\u00b7ben", "son\u00b7der\u00b7art\u00b7ge", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJA", "NN", "VVINF", "KON", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.16": {"text": "Mir durch das Herz, das weltengro\u00df sich dehnet;", "tokens": ["Mir", "durch", "das", "Herz", ",", "das", "wel\u00b7ten\u00b7gro\u00df", "sich", "deh\u00b7net", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$,", "PDS", "VVFIN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Und Ungeheuerliches g\u00e4hret tief in meiner Brust,", "tokens": ["Und", "Un\u00b7ge\u00b7heu\u00b7er\u00b7li\u00b7ches", "g\u00e4h\u00b7ret", "tief", "in", "mei\u00b7ner", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.18": {"text": "Da\u00df heiligen Grauens ahnungsvoll es mich durchzittert.", "tokens": ["Da\u00df", "hei\u00b7li\u00b7gen", "Grau\u00b7ens", "ah\u00b7nungs\u00b7voll", "es", "mich", "durch\u00b7zit\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVFIN", "PPER", "PRF", "VVPP", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Ein mattes Nebelmeer umwallt mir die schwindelnden Sinne,", "tokens": ["Ein", "mat\u00b7tes", "Ne\u00b7bel\u00b7meer", "um\u00b7wallt", "mir", "die", "schwin\u00b7deln\u00b7den", "Sin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Und aus ihm l\u00f6sen sich geheimni\u00dfvolle Schattenbilder,", "tokens": ["Und", "aus", "ihm", "l\u00f6\u00b7sen", "sich", "ge\u00b7heim\u00b7ni\u00df\u00b7vol\u00b7le", "Schat\u00b7ten\u00b7bil\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "PRF", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.21": {"text": "Die immer sch\u00e4rfer, klarer zu deutlichster Gestaltung mir sich festen.", "tokens": ["Die", "im\u00b7mer", "sch\u00e4r\u00b7fer", ",", "kla\u00b7rer", "zu", "deut\u00b7lichs\u00b7ter", "Ge\u00b7stal\u00b7tung", "mir", "sich", "fes\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "$,", "ADJA", "APPR", "ADJA", "NN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+--++--+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.22": {"text": "Und was im Wandel fliehender Zeiten Gro\u00dfes erstanden,", "tokens": ["Und", "was", "im", "Wan\u00b7del", "flie\u00b7hen\u00b7der", "Zei\u00b7ten", "Gro\u00b7\u00dfes", "er\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPRART", "NN", "ADJA", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "Alles erscheint mir wie wiedergeboren,", "tokens": ["Al\u00b7les", "er\u00b7scheint", "mir", "wie", "wie\u00b7der\u00b7ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "KOKOM", "VVIZU", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.24": {"text": "Umschwebt mich zu wundergewaltger Erhebung.", "tokens": ["Um\u00b7schwebt", "mich", "zu", "wun\u00b7der\u00b7ge\u00b7walt\u00b7ger", "Er\u00b7he\u00b7bung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-+--", "measure": "amphibrach.tri.plus"}, "line.25": {"text": "Prometheus, nimmerm\u00fcder K\u00e4mpfer", "tokens": ["Pro\u00b7me\u00b7theus", ",", "nim\u00b7mer\u00b7m\u00fc\u00b7der", "K\u00e4mp\u00b7fer"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Wider falsche Scepter tragender G\u00f6tter Frevelmuth,", "tokens": ["Wi\u00b7der", "fal\u00b7sche", "Scep\u00b7ter", "tra\u00b7gen\u00b7der", "G\u00f6t\u00b7ter", "Fre\u00b7vel\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.27": {"text": "Und Moses, Heiligthumserwecker,", "tokens": ["Und", "Mo\u00b7ses", ",", "Hei\u00b7lig\u00b7thum\u00b7ser\u00b7we\u00b7cker", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KON", "NE", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Von des Dornbuschs flammenden Feuern Geweihter,", "tokens": ["Von", "des", "Dorn\u00b7buschs", "flam\u00b7men\u00b7den", "Feu\u00b7ern", "Ge\u00b7weih\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "NN", "$,"], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.29": {"text": "Und Jesus Dich, der Du in entsagender Hehrheit", "tokens": ["Und", "Je\u00b7sus", "Dich", ",", "der", "Du", "in", "ent\u00b7sa\u00b7gen\u00b7der", "Hehr\u00b7heit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "PPER", "$,", "PRELS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "Schwerster Leiden bittere Fr\u00fcchte gekostet,", "tokens": ["Schwers\u00b7ter", "Lei\u00b7den", "bit\u00b7te\u00b7re", "Fr\u00fcch\u00b7te", "ge\u00b7kos\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.31": {"text": "Euch alle schau\u2019 ich in staunenbefangener Seele,", "tokens": ["Euch", "al\u00b7le", "schau'", "ich", "in", "stau\u00b7nen\u00b7be\u00b7fan\u00b7ge\u00b7ner", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.32": {"text": "Von des heiligen Weltgeists Riesengriffen erfa\u00dft.", "tokens": ["Von", "des", "hei\u00b7li\u00b7gen", "Welt\u00b7geists", "Rie\u00b7sen\u00b7grif\u00b7fen", "er\u00b7fa\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "VVPP", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.33": {"text": "An der Pfort\u2019 des Herzens stocket des Blutes", "tokens": ["An", "der", "Pfort'", "des", "Her\u00b7zens", "sto\u00b7cket", "des", "Blu\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.34": {"text": "Str\u00f6mung, gehemmet von seligem Schreck.", "tokens": ["Str\u00f6\u00b7mung", ",", "ge\u00b7hem\u00b7met", "von", "se\u00b7li\u00b7gem", "Schreck", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.35": {"text": "In Wonneklarheit flammt es mir durch die Seele,", "tokens": ["In", "Won\u00b7nek\u00b7lar\u00b7heit", "flammt", "es", "mir", "durch", "die", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.36": {"text": "Der ewige Geist des Alls durchsch\u00fcttert sie mit seinem L\u00e4utrungsbade;", "tokens": ["Der", "e\u00b7wi\u00b7ge", "Geist", "des", "Alls", "durch\u00b7sch\u00fct\u00b7tert", "sie", "mit", "sei\u00b7nem", "L\u00e4ut\u00b7rungs\u00b7ba\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.37": {"text": "Zerrei\u00dfen f\u00fchl\u2019 ich alle irdschen Bande,", "tokens": ["Zer\u00b7rei\u00b7\u00dfen", "f\u00fchl'", "ich", "al\u00b7le", "ird\u00b7schen", "Ban\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Ich f\u00fchl\u2019s, ich wei\u00df\u2019s, ich bin geweiht und bin gesalbt,", "tokens": ["Ich", "f\u00fchl's", ",", "ich", "wei\u00df's", ",", "ich", "bin", "ge\u00b7weiht", "und", "bin", "ge\u00b7salbt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "VVPP", "KON", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Bin auserkoren, auferweckt zum Heile;", "tokens": ["Bin", "au\u00b7ser\u00b7ko\u00b7ren", ",", "au\u00b7fer\u00b7weckt", "zum", "Hei\u00b7le", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Und mag der Dornenkranz mit seinen Stacheln", "tokens": ["Und", "mag", "der", "Dor\u00b7nen\u00b7kranz", "mit", "sei\u00b7nen", "Sta\u00b7cheln"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Mir noch so tief die Stirn zerfurchen,", "tokens": ["Mir", "noch", "so", "tief", "die", "Stirn", "zer\u00b7fur\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Und jedes Leidens blut\u2019ge Qual sich auf mich th\u00fcrmen", "tokens": ["Und", "je\u00b7des", "Lei\u00b7dens", "blut'\u00b7ge", "Qual", "sich", "auf", "mich", "th\u00fcr\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "ADJA", "NN", "PRF", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Ich wei\u00df, ich wei\u00df, in mir erstanden ist ein neues Licht,", "tokens": ["Ich", "wei\u00df", ",", "ich", "wei\u00df", ",", "in", "mir", "er\u00b7stan\u00b7den", "ist", "ein", "neu\u00b7es", "Licht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "APPR", "PPER", "VVPP", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.44": {"text": "Und dieses Lichtes goldner Fackelbrand,", "tokens": ["Und", "die\u00b7ses", "Lich\u00b7tes", "gold\u00b7ner", "Fa\u00b7ckel\u00b7brand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.45": {"text": "Bald leuchtet hin er durch die schattendunklen Lande,", "tokens": ["Bald", "leuch\u00b7tet", "hin", "er", "durch", "die", "schat\u00b7ten\u00b7dunk\u00b7len", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "APPR", "A