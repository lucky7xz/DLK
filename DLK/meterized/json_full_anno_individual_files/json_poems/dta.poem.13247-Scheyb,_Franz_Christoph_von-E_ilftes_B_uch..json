{"dta.poem.13247": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "E ilftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Er schwiege; sie versezt\u2019 in Majest\u00e4t: \u201eEs sey", "tokens": ["Er", "schwie\u00b7ge", ";", "sie", "ver\u00b7sezt'", "in", "Ma\u00b7jes\u00b7t\u00e4t", ":", "\u201e", "Es", "sey"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "APPR", "NN", "$.", "$(", "PPER", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201ewas du von mir begehrst! ficht! eile! geh! zerstreu", "tokens": ["\u201e", "was", "du", "von", "mir", "be\u00b7gehrst", "!", "ficht", "!", "ei\u00b7le", "!", "geh", "!", "zer\u00b7streu"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["$(", "PWS", "PPER", "APPR", "PPER", "VVFIN", "$.", "PTKVZ", "$.", "VVFIN", "$.", "VVFIN", "$.", "XY"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201edie Schaaren unsers Feinds! ich bin schon \u00fcberzeuget,", "tokens": ["\u201e", "die", "Schaa\u00b7ren", "un\u00b7sers", "Feinds", "!", "ich", "bin", "schon", "\u00fc\u00b7berz\u00b7eu\u00b7get", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "200\u201dWie deine Treue mir und meinem Haus geneiget.", "tokens": ["\"", "Wie", "dei\u00b7ne", "Treu\u00b7e", "mir", "und", "mei\u00b7nem", "Haus", "ge\u00b7nei\u00b7get", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PPOSAT", "NN", "PPER", "KON", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201eich kenne dein Gem\u00fcth; du aber meinen Sinn;", "tokens": ["\u201e", "ich", "ken\u00b7ne", "dein", "Ge\u00b7m\u00fcth", ";", "du", "a\u00b7ber", "mei\u00b7nen", "Sinn", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPOSAT", "NN", "$.", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201edurch dich nennt mich die Welt die Ueberwinderinn.", "tokens": ["\u201e", "durch", "dich", "nennt", "mich", "die", "Welt", "die", "Ue\u00b7ber\u00b7win\u00b7de\u00b7rinn", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPER", "VVFIN", "PRF", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201eso stehet, Tugenden! ein neuer Kampf-Plaz offen;", "tokens": ["\u201e", "so", "ste\u00b7het", ",", "Tu\u00b7gen\u00b7den", "!", "ein", "neu\u00b7er", "Kampf\u00b7Plaz", "of\u00b7fen", ";"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "$,", "NN", "$.", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201ela\u00dft mich von euerm Amt auch jezt die Wirckung hoffen;", "tokens": ["\u201e", "la\u00dft", "mich", "von", "eu\u00b7erm", "Amt", "auch", "jezt", "die", "Wir\u00b7ckung", "hof\u00b7fen", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "APPR", "PPOSAT", "NN", "ADV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "205\u201dGeht und begleitet ihn! steht ihm mit Rath und That", "tokens": ["\"", "Geht", "und", "be\u00b7glei\u00b7tet", "ihn", "!", "steht", "ihm", "mit", "Rath", "und", "That"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "KON", "VVFIN", "PPER", "$.", "VVFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "\u201ein seinem Vorsaz bey, den er er\u00f6ffnet hat.", "tokens": ["\u201e", "in", "sei\u00b7nem", "Vor\u00b7saz", "bey", ",", "den", "er", "er\u00b7\u00f6ff\u00b7net", "hat", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "PTKVZ", "$,", "PRELS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}