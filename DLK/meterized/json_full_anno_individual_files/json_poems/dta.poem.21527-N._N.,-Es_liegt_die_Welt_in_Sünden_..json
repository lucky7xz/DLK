{"dta.poem.21527": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Es liegt die Welt in S\u00fcnden .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Es liegt die Welt in S\u00fcnden,", "tokens": ["Es", "liegt", "die", "Welt", "in", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Heiligste ist feil \u2014", "tokens": ["Das", "Hei\u00b7ligs\u00b7te", "ist", "feil"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ADJD", "$("], "meter": "-+---+", "measure": "dactylic.init"}, "line.3": {"text": "Aufreckt sich wie der schwarze Tod", "tokens": ["Auf\u00b7reckt", "sich", "wie", "der", "schwar\u00b7ze", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Laster wollustgeil!", "tokens": ["Das", "Las\u00b7ter", "wol\u00b7lust\u00b7geil", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es werfen seine Flammen", "tokens": ["Es", "wer\u00b7fen", "sei\u00b7ne", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den Brand in jede Brust \u2014", "tokens": ["Den", "Brand", "in", "je\u00b7de", "Brust"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Im Triumphatorwagen rauscht", "tokens": ["Im", "Tri\u00b7um\u00b7pha\u00b7tor\u00b7wa\u00b7gen", "rauscht"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Durch alle Welt die Lust!", "tokens": ["Durch", "al\u00b7le", "Welt", "die", "Lust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und Keiner hebt die Keule,", "tokens": ["Und", "Kei\u00b7ner", "hebt", "die", "Keu\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu morden das Pestgez\u00fccht!", "tokens": ["Zu", "mor\u00b7den", "das", "Pest\u00b7ge\u00b7z\u00fccht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und Keiner schreit nach and\u2019rem Heil", "tokens": ["Und", "Kei\u00b7ner", "schreit", "nach", "an\u00b7d'\u00b7rem", "Heil"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und bangt vor dem Gericht!", "tokens": ["Und", "bangt", "vor", "dem", "Ge\u00b7richt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In wilden Wollustschauern", "tokens": ["In", "wil\u00b7den", "Wol\u00b7lust\u00b7schau\u00b7ern"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Liegen wir staubbes\u00e4\u2019t", "tokens": ["Lie\u00b7gen", "wir", "staub\u00b7be\u00b7s\u00e4't"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "VVFIN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Und stammeln an schwellender Dirnenbrust", "tokens": ["Und", "stam\u00b7meln", "an", "schwel\u00b7len\u00b7der", "Dir\u00b7nen\u00b7brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "An die Venus ein Gebet:", "tokens": ["An", "die", "Ve\u00b7nus", "ein", "Ge\u00b7bet", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.3": {"line.1": {"text": "\u201eo gro\u00dfe Mutter, n\u00e4hre", "tokens": ["\u201e", "o", "gro\u00b7\u00dfe", "Mut\u00b7ter", ",", "n\u00e4h\u00b7re"], "token_info": ["punct", "word", "word", "word", "punct", "word"], "pos": ["$(", "FM", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dein liebelechzend Kind!", "tokens": ["Dein", "lie\u00b7be\u00b7lech\u00b7zend", "Kind", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schling\u2019 auch um mich dein Diadem,", "tokens": ["Schling'", "auch", "um", "mich", "dein", "Di\u00b7a\u00b7dem", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "PPER", "PPOSAT", "NN", "$,"], "meter": "+--++-+-", "measure": "dactylic.init"}, "line.4": {"text": "Deine Rosen, dein Traubengewind!", "tokens": ["Dei\u00b7ne", "Ro\u00b7sen", ",", "dein", "Trau\u00b7ben\u00b7ge\u00b7wind", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Sieh\u2019! meine verschmachteten Lippen", "tokens": ["Sieh'", "!", "mei\u00b7ne", "ver\u00b7schmach\u00b7te\u00b7ten", "Lip\u00b7pen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "D\u00fcrsten nach hei\u00dfem Genu\u00df \u2014", "tokens": ["D\u00fcrs\u00b7ten", "nach", "hei\u00b7\u00dfem", "Ge\u00b7nu\u00df"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "O gro\u00dfe Mutter, vergi\u00df mich nicht \u2014", "tokens": ["O", "gro\u00b7\u00dfe", "Mut\u00b7ter", ",", "ver\u00b7gi\u00df", "mich", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "VVIMP", "PPER", "PTKNEG", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Lass\u2019 trinken mich deinen Ku\u00df!", "tokens": ["Lass'", "trin\u00b7ken", "mich", "dei\u00b7nen", "Ku\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Lass\u2019, bis ich selig versunken", "tokens": ["Lass'", ",", "bis", "ich", "se\u00b7lig", "ver\u00b7sun\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "PPER", "ADJD", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "In Tr\u00e4ume, m\u00e4hrchenumkost,", "tokens": ["In", "Tr\u00e4u\u00b7me", ",", "m\u00e4hr\u00b7chen\u00b7um\u00b7kost", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Hinfluthen \u00fcber das d\u00fcrre Gefild", "tokens": ["Hin\u00b7flu\u00b7then", "\u00fc\u00b7ber", "das", "d\u00fcr\u00b7re", "Ge\u00b7fild"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Meiner Seele deinen Trost!", "tokens": ["Mei\u00b7ner", "See\u00b7le", "dei\u00b7nen", "Trost", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nicht mag ich kargen und dulden,", "tokens": ["Nicht", "mag", "ich", "kar\u00b7gen", "und", "dul\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "PPER", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Wie ein Sch\u00e4cher nach Brocken geh\u2019n \u2014", "tokens": ["Wie", "ein", "Sch\u00e4\u00b7cher", "nach", "Bro\u00b7cken", "geh'n"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "VVINF", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Es soll f\u00fcr meine verzehrende Brunst", "tokens": ["Es", "soll", "f\u00fcr", "mei\u00b7ne", "ver\u00b7zeh\u00b7ren\u00b7de", "Brunst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Ein Paradies ersteh\u2019n!", "tokens": ["Ein", "Pa\u00b7ra\u00b7dies", "er\u00b7steh'n", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wir haben vom Kreuze gerissen", "tokens": ["Wir", "ha\u00b7ben", "vom", "Kreu\u00b7ze", "ge\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "VVPP"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Des Heilands zermartert Gebein!", "tokens": ["Des", "Hei\u00b7lands", "zer\u00b7mar\u00b7tert", "Ge\u00b7bein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Wir warfen von uns das Pilgerkleid,", "tokens": ["Wir", "war\u00b7fen", "von", "uns", "das", "Pil\u00b7ger\u00b7kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wir lie\u00dfen den W\u00fcstenstein!", "tokens": ["Wir", "lie\u00b7\u00dfen", "den", "W\u00fcs\u00b7ten\u00b7stein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Was frommt uns bleiches Entsagen?", "tokens": ["Was", "frommt", "uns", "blei\u00b7ches", "Ent\u00b7sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Was frommt uns Dornengerank?", "tokens": ["Was", "frommt", "uns", "Dor\u00b7nen\u00b7ge\u00b7rank", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Wir schl\u00fcrfen den Kelch hintaumelnder Lust", "tokens": ["Wir", "schl\u00fcr\u00b7fen", "den", "Kelch", "hin\u00b7tau\u00b7meln\u00b7der", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "In seligem Ueberschwang!\u201c", "tokens": ["In", "se\u00b7li\u00b7gem", "Ue\u00b7ber\u00b7schwang", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "O sagt, ihr m\u00fcden Lippen,", "tokens": ["O", "sagt", ",", "ihr", "m\u00fc\u00b7den", "Lip\u00b7pen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kennt ihr kein and\u2019res Wort?", "tokens": ["Kennt", "ihr", "kein", "an\u00b7d'\u00b7res", "Wort", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist in der Seele tiefstem Grund", "tokens": ["Ist", "in", "der", "See\u00b7le", "tiefs\u00b7tem", "Grund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Bronnen all\u2019 verdorrt,", "tokens": ["Der", "Bron\u00b7nen", "all'", "ver\u00b7dorrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Daraus in lichten Str\u00f6men", "tokens": ["Da\u00b7raus", "in", "lich\u00b7ten", "Str\u00f6\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das Leben sich verj\u00fcngt?", "tokens": ["Das", "Le\u00b7ben", "sich", "ver\u00b7j\u00fcngt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Schreit ihr zur Aphrodite nur \u2014", "tokens": ["Schreit", "ihr", "zur", "A\u00b7phro\u00b7di\u00b7te", "nur"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "APPRART", "NN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zur Dirne, frech geschminkt?", "tokens": ["Zur", "Dir\u00b7ne", ",", "frech", "ge\u00b7schminkt", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Zur Dirne, der im Herzen", "tokens": ["Zur", "Dir\u00b7ne", ",", "der", "im", "Her\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nur Lug brennt und Verrath?", "tokens": ["Nur", "Lug", "brennt", "und", "Ver\u00b7rath", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VVFIN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die mit geschmeid\u2019ger Buhlerkunst", "tokens": ["Die", "mit", "ge\u00b7schmei\u00b7d'\u00b7ger", "Buh\u00b7ler\u00b7kunst"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Erstickt die freie That?", "tokens": ["Er\u00b7stickt", "die", "frei\u00b7e", "That", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Schreit ihr nach Wein und Rosen?", "tokens": ["Schreit", "ihr", "nach", "Wein", "und", "Ro\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nach \u00fcpp\u2019gem Bacchusgelag\u2019?", "tokens": ["Nach", "\u00fcpp'\u00b7gem", "Bac\u00b7chus\u00b7ge\u00b7lag'", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Nach sternendunkler, schw\u00fcler Nacht", "tokens": ["Nach", "ster\u00b7nen\u00b7dunk\u00b7ler", ",", "schw\u00fc\u00b7ler", "Nacht"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und flucht dem gold\u2019nen Tag?", "tokens": ["Und", "flucht", "dem", "gold'\u00b7nen", "Tag", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ihr Narr\u2019n! Es kommt die Stunde,", "tokens": ["Ihr", "Narr'n", "!", "Es", "kommt", "die", "Stun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da wieder am Kreuze einmal", "tokens": ["Da", "wie\u00b7der", "am", "Kreu\u00b7ze", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPRART", "NN", "ADV"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Bluttriefend ein ", "tokens": ["Blut\u00b7trie\u00b7fend", "ein"], "token_info": ["word", "word"], "pos": ["ADJD", "ART"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Im Herzen Prometheus-Qual!", "tokens": ["Im", "Her\u00b7zen", "Pro\u00b7me\u00b7theus\u00b7Qual", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Auch ", "tokens": ["Auch