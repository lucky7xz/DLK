{"dta.poem.3199": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "50.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das wei\u00dfe Grabtuch, das der Schnee auf's Gr\u00fcn gedeckt,", "tokens": ["Das", "wei\u00b7\u00dfe", "Grab\u00b7tuch", ",", "das", "der", "Schnee", "auf's", "Gr\u00fcn", "ge\u00b7deckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Lockert im Lebenstrieb, darunter still erweckt.", "tokens": ["Lo\u00b7ckert", "im", "Le\u00b7ben\u00b7strieb", ",", "da\u00b7run\u00b7ter", "still", "er\u00b7weckt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$,", "PAV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und also schwellen mir im Herzen neue Keime,", "tokens": ["Und", "al\u00b7so", "schwel\u00b7len", "mir", "im", "Her\u00b7zen", "neu\u00b7e", "Kei\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und also quellen mir aus Schmerzen neue Reime.", "tokens": ["Und", "al\u00b7so", "quel\u00b7len", "mir", "aus", "Schmer\u00b7zen", "neu\u00b7e", "Rei\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das Herz will hoffnungsvoll versuchen noch ein Jahr,", "tokens": ["Das", "Herz", "will", "hoff\u00b7nungs\u00b7voll", "ver\u00b7su\u00b7chen", "noch", "ein", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADJD", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob es ihm besser sei, als das vergangne war.", "tokens": ["Ob", "es", "ihm", "bes\u00b7ser", "sei", ",", "als", "das", "ver\u00b7gang\u00b7ne", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VAFIN", "$,", "KOUS", "ART", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Das hat am l\u00e4ngsten Tag und um die l\u00e4ngste Nacht", "tokens": ["Das", "hat", "am", "l\u00e4ngs\u00b7ten", "Tag", "und", "um", "die", "l\u00e4ngs\u00b7te", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPRART", "ADJA", "NN", "KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Ungl\u00fcck, jedes ohn' ein gleiches, mir gebracht.", "tokens": ["Ein", "Un\u00b7gl\u00fcck", ",", "je\u00b7des", "ohn'", "ein", "glei\u00b7ches", ",", "mir", "ge\u00b7bracht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PIAT", "APPR", "ART", "ADJA", "$,", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Nun bringe dieses mir in lang- und kurzen Tagen,", "tokens": ["Nun", "brin\u00b7ge", "die\u00b7ses", "mir", "in", "lang", "und", "kur\u00b7zen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDAT", "PPER", "APPR", "TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn nicht besondre Lust, doch Ruhe sonder Plagen.", "tokens": ["Wenn", "nicht", "be\u00b7sond\u00b7re", "Lust", ",", "doch", "Ru\u00b7he", "son\u00b7der", "Pla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ADJA", "NN", "$,", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}