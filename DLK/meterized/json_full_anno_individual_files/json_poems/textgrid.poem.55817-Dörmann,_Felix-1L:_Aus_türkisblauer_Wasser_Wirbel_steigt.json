["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$.", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das edle Haupt, das welk ein Lorbeerzweig", "tokens": ["Das", "ed\u00b7le", "Haupt", ",", "das", "welk", "ein", "Lor\u00b7beer\u00b7zweig"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und ein zersprung'ner Kronenreif umschlingt.", "tokens": ["Und", "ein", "zer\u00b7sprung'\u00b7ner", "Kro\u00b7nen\u00b7reif", "um\u00b7schlingt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und halb im Traum die bleiche Lippe murmelt:", "tokens": ["Und", "halb", "im", "Traum", "die", "blei\u00b7che", "Lip\u00b7pe", "mur\u00b7melt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "\u00bbder Sieger bin ich, der die Nacht bezwang,", "tokens": ["\u00bb", "der", "Sie\u00b7ger", "bin", "ich", ",", "der", "die", "Nacht", "be\u00b7zwang", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "PPER", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der Morgen naht f\u00fcr alle Erdens\u00f6hne,", "tokens": ["Der", "Mor\u00b7gen", "naht", "f\u00fcr", "al\u00b7le", "Er\u00b7den\u00b7s\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "i