unct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Diesem Vogel gleich, verdorben", "tokens": ["Die\u00b7sem", "Vo\u00b7gel", "gleich", ",", "ver\u00b7dor\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PDAT", "NN", "ADV", "$,", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zum Geripp, der Schwungkraft baar,", "tokens": ["Zum", "Ge\u00b7ripp", ",", "der", "Schwung\u00b7kraft", "baar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dem der edle Schmuck des Flaumes", "tokens": ["Dem", "der", "ed\u00b7le", "Schmuck", "des", "Flau\u00b7mes"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "St\u00fcck f\u00fcr St\u00fcck abf\u00e4llt vom Leib,", "tokens": ["St\u00fcck", "f\u00fcr", "St\u00fcck", "ab\u00b7f\u00e4llt", "vom", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+---+", "measure": "unknown.measure.tri"}, "line.7": {"text": "Wie das welke Blatt des Baumes,", "tokens": ["Wie", "das", "wel\u00b7ke", "Blatt", "des", "Bau\u00b7mes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Rauher Winde Zeitvertreib.", "tokens": ["Rau\u00b7her", "Win\u00b7de", "Zeit\u00b7ver\u00b7treib", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Habsburgs Fahnen sah man w