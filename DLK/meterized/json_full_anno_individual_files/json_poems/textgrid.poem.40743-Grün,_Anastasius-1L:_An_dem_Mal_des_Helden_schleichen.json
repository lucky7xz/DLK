{"textgrid.poem.40743": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: An dem Mal des Helden schleichen", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An dem Mal des Helden schleichen", "tokens": ["An", "dem", "Mal", "des", "Hel\u00b7den", "schlei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Siebzig Jahre tr\u00e4g vorbei;", "tokens": ["Sieb\u00b7zig", "Jah\u00b7re", "tr\u00e4g", "vor\u00b7bei", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wecken k\u00f6nnt' ihn von den Leichen", "tokens": ["We\u00b7cken", "k\u00f6nnt'", "ihn", "von", "den", "Lei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVINF", "VMFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Solch ein Wonnemond von Mai,", "tokens": ["Solch", "ein", "Won\u00b7ne\u00b7mond", "von", "Mai", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dessen goldne Morgenr\u00f6then", "tokens": ["Des\u00b7sen", "gold\u00b7ne", "Mor\u00b7gen\u00b7r\u00f6\u00b7then"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "St\u00e4dtebrand und Waffenblitz,", "tokens": ["St\u00e4d\u00b7te\u00b7brand", "und", "Waf\u00b7fen\u00b7blitz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Eingesungen, statt von Fl\u00f6ten,", "tokens": ["Ein\u00b7ge\u00b7sun\u00b7gen", ",", "statt", "von", "Fl\u00f6\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUI", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Von Trompeten und Gesch\u00fctz!", "tokens": ["Von", "Trom\u00b7pe\u00b7ten", "und", "Ge\u00b7sch\u00fctz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-++-+-+", "measure": "unknown.measure.tetra"}}, "stanza.2": {"line.1": {"text": "Zu Sch\u00f6nbrunn in laub'gen Hallen", "tokens": ["Zu", "Sch\u00f6n\u00b7brunn", "in", "laub'\u00b7gen", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Geht des Korseneilands Sohn;", "tokens": ["Geht", "des", "Kor\u00b7sen\u00b7ei\u00b7lands", "Sohn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lauscht sein Ohr den Nachtigallen,", "tokens": ["Lauscht", "sein", "Ohr", "den", "Nach\u00b7ti\u00b7gal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dr\u00f6hnt es doch von Schlachtenton;", "tokens": ["Dr\u00f6hnt", "es", "doch", "von", "Schlach\u00b7ten\u00b7ton", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "In das Knopfloch eine Rose", "tokens": ["In", "das", "Kno\u00b7pfloch", "ei\u00b7ne", "Ro\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Pfl\u00fcckt die schicksalschwere Hand,", "tokens": ["Pfl\u00fcckt", "die", "schick\u00b7sal\u00b7schwe\u00b7re", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Leise schwebt sein Fu\u00df im Moose,", "tokens": ["Lei\u00b7se", "schwebt", "sein", "Fu\u00df", "im", "Moo\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wenn er stampft, erbebt das Land.", "tokens": ["Wenn", "er", "stampft", ",", "er\u00b7bebt", "das", "Land", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Zu den Zwingern fremder Thiere", "tokens": ["Zu", "den", "Zwin\u00b7gern", "frem\u00b7der", "Thie\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lenkt der Kaiser jetzt den Tritt,", "tokens": ["Lenkt", "der", "Kai\u00b7ser", "jetzt", "den", "Tritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Pl\u00f6tzlich vor dem Steinquartiere", "tokens": ["Pl\u00f6tz\u00b7lich", "vor", "dem", "Stein\u00b7quar\u00b7tie\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eines Adlers stockt sein Schritt;", "tokens": ["Ei\u00b7nes", "Ad\u00b7lers", "stockt", "sein", "Schritt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Auf dem Block im Eisenringe", "tokens": ["Auf", "dem", "Block", "im", "Ei\u00b7sen\u00b7rin\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zittert ein uralter Aar,", "tokens": ["Zit\u00b7tert", "ein", "ur\u00b7al\u00b7ter", "Aar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+++-+", "measure": "unknown.measure.penta"}, "line.7": {"text": "Bl\u00f6den Aug's, gebrochner Schwinge,", "tokens": ["Bl\u00f6\u00b7den", "Aug's", ",", "ge\u00b7broch\u00b7ner", "Schwin\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Der einst F\u00fcrst der L\u00fcfte war!", "tokens": ["Der", "einst", "F\u00fcrst", "der", "L\u00fcf\u00b7te", "war", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Bild des Jammers ohne Gleichen", "tokens": ["Bild", "des", "Jam\u00b7mers", "oh\u00b7ne", "Glei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Solch geknickter Wolkensohn!", "tokens": ["Solch", "ge\u00b7knick\u00b7ter", "Wol\u00b7ken\u00b7sohn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sicher, selbst als Bild und Zeichen,", "tokens": ["Si\u00b7cher", ",", "selbst", "als", "Bild", "und", "Zei\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "KOUS", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sei die Majest\u00e4t vor Hohn!", "tokens": ["Sei", "die", "Ma\u00b7jes\u00b7t\u00e4t", "vor", "Hohn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und der Kaiser ruft den W\u00e4rter:", "tokens": ["Und", "der", "Kai\u00b7ser", "ruft", "den", "W\u00e4r\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "\u00bbalter, la\u00df den Vogel frei!\u00ab", "tokens": ["\u00bb", "al\u00b7ter", ",", "la\u00df", "den", "Vo\u00b7gel", "frei", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJA", "$,", "VVIMP", "ART", "NE", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Seine Z\u00fcge wurden h\u00e4rter:", "tokens": ["Sei\u00b7ne", "Z\u00fc\u00b7ge", "wur\u00b7den", "h\u00e4r\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "\u00bboder send' ihm ein St\u00fcck Blei!\u00ab", "tokens": ["\u00bb", "o\u00b7der", "send'", "ihm", "ein", "St\u00fcck", "Blei", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "VVIMP", "PPER", "ART", "NN", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbm\u00f6ge Gott den Sinn Euch lenken!\u00ab", "tokens": ["\u00bb", "m\u00f6\u00b7ge", "Gott", "den", "Sinn", "Euch", "len\u00b7ken", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VMFIN", "NN", "ART", "NN", "PPER", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sprach der Alte warm und weich;", "tokens": ["Sprach", "der", "Al\u00b7te", "warm", "und", "weich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbschont die\u00df theure Angedenken,", "tokens": ["\u00bb", "schont", "die\u00df", "theu\u00b7re", "An\u00b7ge\u00b7den\u00b7ken", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PDS", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Heilig Sinnbild ist's zugleich;", "tokens": ["Hei\u00b7lig", "Sinn\u00b7bild", "ist's", "zu\u00b7gleich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dieses Thier im Eisenrahmen", "tokens": ["Die\u00b7ses", "Thier", "im", "Ei\u00b7sen\u00b7rah\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hielt ein Held gar lieb und gut,", "tokens": ["Hielt", "ein", "Held", "gar", "lieb", "und", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Prinz Eugen, \u2013 Ihr kennt den Namen?\u00ab", "tokens": ["Prinz", "Eu\u00b7gen", ",", "\u2013", "Ihr", "kennt", "den", "Na\u00b7men", "?", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "NE", "$,", "$(", "PPER", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Frankreichs Kaiser r\u00fcckt den Hut.", "tokens": ["Fran\u00b7kreichs", "Kai\u00b7ser", "r\u00fcckt", "den", "Hut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "\u00bbaber seit sein Herr gestorben,", "tokens": ["\u00bb", "a\u00b7ber", "seit", "sein", "Herr", "ge\u00b7stor\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist ein sch\u00f6n'rer Wappenaar,", "tokens": ["Ist", "ein", "sch\u00f6n'\u00b7rer", "Wap\u00b7pe\u00b7naar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Diesem Vogel gleich, verdorben", "tokens": ["Die\u00b7sem", "Vo\u00b7gel", "gleich", ",", "ver\u00b7dor\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PDAT", "NN", "ADV", "$,", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zum Geripp, der Schwungkraft baar,", "tokens": ["Zum", "Ge\u00b7ripp", ",", "der", "Schwung\u00b7kraft", "baar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dem der edle Schmuck des Flaumes", "tokens": ["Dem", "der", "ed\u00b7le", "Schmuck", "des", "Flau\u00b7mes"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "St\u00fcck f\u00fcr St\u00fcck abf\u00e4llt vom Leib,", "tokens": ["St\u00fcck", "f\u00fcr", "St\u00fcck", "ab\u00b7f\u00e4llt", "vom", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+---+", "measure": "unknown.measure.tri"}, "line.7": {"text": "Wie das welke Blatt des Baumes,", "tokens": ["Wie", "das", "wel\u00b7ke", "Blatt", "des", "Bau\u00b7mes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Rauher Winde Zeitvertreib.", "tokens": ["Rau\u00b7her", "Win\u00b7de", "Zeit\u00b7ver\u00b7treib", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Habsburgs Fahnen sah man wandern,", "tokens": ["Habs\u00b7burgs", "Fah\u00b7nen", "sah", "man", "wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Federn gleich, am Po, am Rhein,", "tokens": ["Fe\u00b7dern", "gleich", ",", "am", "Po", ",", "am", "Rhein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "APPRART", "NE", "$,", "APPRART", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In Sicilien und in Flandern,", "tokens": ["In", "Si\u00b7ci\u00b7li\u00b7en", "und", "in", "Flan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "APPR", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Flattern fort von Belgrads Stein,", "tokens": ["Flat\u00b7tern", "fort", "von", "Bel\u00b7grads", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "APPR", "NE", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bis in Schlesiens reichem Garten", "tokens": ["Bis", "in", "Schle\u00b7si\u00b7ens", "rei\u00b7chem", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "NN", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Jene sch\u00f6nste Schwinge sank;", "tokens": ["Je\u00b7ne", "sch\u00f6ns\u00b7te", "Schwin\u00b7ge", "sank", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Traun, auch Oesterreichs Standarten", "tokens": ["Traun", ",", "auch", "O\u00b7es\u00b7ter\u00b7reichs", "Stand\u00b7ar\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "NE", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Sind an b\u00f6sen Mausern krank.", "tokens": ["Sind", "an", "b\u00f6\u00b7sen", "Mau\u00b7sern", "krank", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Als mein Aar im Belvedere", "tokens": ["Als", "mein", "Aar", "im", "Bel\u00b7ve\u00b7de\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "APPRART", "NN"], "meter": "+-+-+---", "measure": "unknown.measure.tri"}, "line.2": {"text": "Speise nahm aus Eugens Hand,", "tokens": ["Spei\u00b7se", "nahm", "aus", "Eu\u00b7gens", 