 "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJA", "$,", "VVIMP", "ART", "NE", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Seine Z\u00fcge wurden h\u00e4rter:", "tokens": ["Sei\u00b7ne", "Z\u00fc\u00b7ge", "wur\u00b7den", "h\u00e4r\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "\u00bboder send' ihm ein St\u00fcck Blei!\u00ab", "tokens": ["\u00bb", "o\u00b7der", "send'", "ihm", "ein", "St\u00fcck", "Blei", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "VVIMP", "PPER", "ART", "NN", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "\u00bbm\u00f6ge Gott den Sinn Euch lenken!\u00ab", "tokens": ["\u00bb", "m\u00f6\u00b7ge", "Gott", "den", "Sinn", "Euch", "len\u00b7ken", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VMFIN", "NN", "ART", "NN", "PPER", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sprach der Alte warm und weich;", "tokens": ["Sprach", "der", "Al\u00b7te", "warm", "und", "weich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbschont die\u00df theure Angedenken,", "tokens": ["\u00bb", "schont", "die\u00df", "theu\u00b7re", "An\u00b7ge\u00b7den\u00b7ken", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PDS", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Heilig Sinnbild ist's zugleich;", "tokens": ["Hei\u00b7lig", "Sinn\u00b7bild", "ist's", "zu\u00b