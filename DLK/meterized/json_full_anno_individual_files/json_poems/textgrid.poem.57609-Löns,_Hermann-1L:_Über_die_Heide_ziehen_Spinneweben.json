{"textgrid.poem.57609": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00dcber die Heide ziehen Spinneweben", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcber die Heide ziehen Spinneweben", "tokens": ["\u00dc\u00b7ber", "die", "Hei\u00b7de", "zie\u00b7hen", "Spin\u00b7ne\u00b7we\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Von Halm zu Halm ihr silberwei\u00dfes Tuch,", "tokens": ["Von", "Halm", "zu", "Halm", "ihr", "sil\u00b7ber\u00b7wei\u00b7\u00dfes", "Tuch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "NE", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Am Himmelsrande wei\u00dfe W\u00f6lkchen schweben", "tokens": ["Am", "Him\u00b7mels\u00b7ran\u00b7de", "wei\u00b7\u00dfe", "W\u00f6lk\u00b7chen", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wei\u00dfes Wollgras wimpelt \u00fcberm Bruch.", "tokens": ["Und", "wei\u00b7\u00dfes", "Woll\u00b7gras", "wim\u00b7pelt", "\u00fc\u00b7berm", "Bruch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es gl\u00fcht die Luft wie ein Maschinenofen,", "tokens": ["Es", "gl\u00fcht", "die", "Luft", "wie", "ein", "Ma\u00b7schi\u00b7ne\u00b7no\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kein Menschenleben regt sich weit und breit,", "tokens": ["Kein", "Men\u00b7schen\u00b7le\u00b7ben", "regt", "sich", "weit", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PRF", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Baumpieper nur schmettert seine Strophen", "tokens": ["Der", "Baum\u00b7pie\u00b7per", "nur", "schmet\u00b7tert", "sei\u00b7ne", "Stro\u00b7phen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und hoch im Blau der M\u00e4usebussard schreit.", "tokens": ["Und", "hoch", "im", "Blau", "der", "M\u00e4u\u00b7se\u00b7bus\u00b7sard", "schreit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "In rosa Heidekraut den Leib ich strecke,", "tokens": ["In", "ro\u00b7sa", "Hei\u00b7de\u00b7kraut", "den", "Leib", "ich", "stre\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Taschentuch ich auf die Augen breit',", "tokens": ["Das", "Ta\u00b7schen\u00b7tuch", "ich", "auf", "die", "Au\u00b7gen", "breit'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Weit von mir ich die schlaffen Glieder recke", "tokens": ["Weit", "von", "mir", "ich", "die", "schlaf\u00b7fen", "Glie\u00b7der", "re\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPER", "PPER", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und dehne mich in s\u00fc\u00dfer M\u00fcdigkeit.", "tokens": ["Und", "deh\u00b7ne", "mich", "in", "s\u00fc\u00b7\u00dfer", "M\u00fc\u00b7dig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O Grabesschlaf, woll\u00fcstiges Genie\u00dfen!", "tokens": ["O", "Gra\u00b7bes\u00b7schlaf", ",", "wol\u00b7l\u00fcs\u00b7ti\u00b7ges", "Ge\u00b7nie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn dieser m\u00fcde Menschenleib verwest,", "tokens": ["Wenn", "die\u00b7ser", "m\u00fc\u00b7de", "Men\u00b7schen\u00b7leib", "ver\u00b7west", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn die Atome auseinanderflie\u00dfen", "tokens": ["Wenn", "die", "A\u00b7to\u00b7me", "aus\u00b7ein\u00b7an\u00b7der\u00b7flie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Glied an Glied sich reckend, dehnend l\u00f6st.", "tokens": ["Und", "Glied", "an", "Glied", "sich", "re\u00b7ckend", ",", "deh\u00b7nend", "l\u00f6st", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "PRF",