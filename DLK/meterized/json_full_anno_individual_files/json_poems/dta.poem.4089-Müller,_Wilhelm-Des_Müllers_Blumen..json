{"dta.poem.4089": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Des M\u00fcllers Blumen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aus hellen blauen Augen sehn;", "tokens": ["Aus", "hel\u00b7len", "blau\u00b7en", "Au\u00b7gen", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Bach der ist des M\u00fcllers Freund,", "tokens": ["Der", "Bach", "der", "ist", "des", "M\u00fcl\u00b7lers", "Freund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und hellblau Liebchens Auge scheint,", "tokens": ["Und", "hell\u00b7blau", "Lieb\u00b7chens", "Au\u00b7ge", "scheint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Drum sind es meine Blumen.", "tokens": ["Drum", "sind", "es", "mei\u00b7ne", "Blu\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dicht unter ihrem Fensterlein", "tokens": ["Dicht", "un\u00b7ter", "ih\u00b7rem", "Fens\u00b7ter\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da will ich pflanzen die Blumen ein,", "tokens": ["Da", "will", "ich", "pflan\u00b7zen", "die", "Blu\u00b7men", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da ruft ihr zu, wenn Alles schweigt,", "tokens": ["Da", "ruft", "ihr", "zu", ",", "wenn", "Al\u00b7les", "schweigt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn sich ihr Haupt zum Schlummer neigt,", "tokens": ["Wenn", "sich", "ihr", "Haupt", "zum", "Schlum\u00b7mer", "neigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr wi\u00dft ja, was ich meine.", "tokens": ["Ihr", "wi\u00dft", "ja", ",", "was", "ich", "mei\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und wenn sie th\u00e4t die Aeuglein zu,", "tokens": ["Und", "wenn", "sie", "th\u00e4t", "die", "A\u00b7e\u00b7u\u00b7glein", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und schl\u00e4ft in s\u00fc\u00dfer, s\u00fc\u00dfer Ruh',", "tokens": ["Und", "schl\u00e4ft", "in", "s\u00fc\u00b7\u00dfer", ",", "s\u00fc\u00b7\u00dfer", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann lispelt als ein Traumgesicht", "tokens": ["Dann", "lis\u00b7pelt", "als", "ein", "Traum\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr zu: Vergi\u00df, vergi\u00df mein nicht!", "tokens": ["Ihr", "zu", ":", "Ver\u00b7gi\u00df", ",", "ver\u00b7gi\u00df", "mein", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$.", "VVIMP", "$,", "VVIMP", "PPOSAT", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das ist es, was ich meine.", "tokens": ["Das", "ist", "es", ",", "was", "ich", "mei\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und schlie\u00dft sie fr\u00fch die Laden auf,", "tokens": ["Und", "schlie\u00dft", "sie", "fr\u00fch", "die", "La\u00b7den", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dann schaut mit Liebesblick hinauf:", "tokens": ["Dann", "schaut", "mit", "Lie\u00b7bes\u00b7blick", "hin\u00b7auf", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Thau in euren Aeugelein,", "tokens": ["Der", "Thau", "in", "eu\u00b7ren", "A\u00b7e\u00b7u\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das sollen meine Thr\u00e4nen sein,", "tokens": ["Das", "sol\u00b7len", "mei\u00b7ne", "Thr\u00e4\u00b7nen", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die will ich auf euch weinen.", "tokens": ["Die", "will", "ich", "auf", "euch", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}