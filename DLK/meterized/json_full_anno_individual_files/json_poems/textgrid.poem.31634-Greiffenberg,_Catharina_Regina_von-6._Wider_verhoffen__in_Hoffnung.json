{"textgrid.poem.31634": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "6. Wider verhoffen / in Hoffnung", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Voll getr\u00f6ster Hoffnung leben / in des Gl\u00fccks Verzweiflungs Zeit /", "tokens": ["Voll", "ge\u00b7tr\u00f6s\u00b7ter", "Hoff\u00b7nung", "le\u00b7ben", "/", "in", "des", "Gl\u00fccks", "Ver\u00b7zwei\u00b7flungs", "Zeit", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "VVINF", "$(", "APPR", "ART", "NN", "NN", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "ist ein Kunst-und Wunderst\u00fccke des Entz\u00fcnders aller Freud.", "tokens": ["ist", "ein", "Kunst\u00b7\u00b7und", "Wun\u00b7der\u00b7st\u00fc\u00b7cke", "des", "Ent\u00b7z\u00fcn\u00b7ders", "al\u00b7ler", "Freud", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "ART", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Wie die Irdisch- helle Sonn' offt durch dicke Wolken strahlet:", "tokens": ["Wie", "die", "Ir\u00b7disch", "hel\u00b7le", "Sonn'", "offt", "durch", "di\u00b7cke", "Wol\u00b7ken", "strah\u00b7let", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "TRUNC", "ADJA", "NN", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "so der Himmlisch Freuden-Blitz / in der Noht die H\u00fclff vormahlet.", "tokens": ["so", "der", "Himm\u00b7lisch", "Freu\u00b7den\u00b7Blitz", "/", "in", "der", "Noht", "die", "H\u00fclff", "vor\u00b7mah\u00b7let", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NE", "$(", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Durch der Sonne W\u00fcrkungs-Kr\u00e4ffte / wird das Gold im Berg bereit /", "tokens": ["Durch", "der", "Son\u00b7ne", "W\u00fcr\u00b7kungs\u00b7Kr\u00e4ff\u00b7te", "/", "wird", "das", "Gold", "im", "Berg", "be\u00b7reit", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$(", "VAFIN", "ART", "NN", "APPRART", "NN", "ADJD", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "keinen Schein sieht man zwar drinnen: Eben die Gelegenheit", "tokens": ["kei\u00b7nen", "Schein", "sieht", "man", "zwar", "drin\u00b7nen", ":", "E\u00b7ben", "die", "Ge\u00b7le\u00b7gen\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "PIS", "ADV", "ADV", "$.", "ADV", "ART", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "hat es / in der H\u00f6chsten Noht / mit dem unverhofften Hoffen /", "tokens": ["hat", "es", "/", "in", "der", "H\u00f6chs\u00b7ten", "Noht", "/", "mit", "dem", "un\u00b7ver\u00b7hoff\u00b7ten", "Hof\u00b7fen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$(", "APPR", "ART", "ADJA", "NN", "$(", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "ist euch schon sein Raht verdeckt / steht der H\u00fclff der Weg doch offen.", "tokens": ["ist", "euch", "schon", "sein", "Raht", "ver\u00b7deckt", "/", "steht", "der", "H