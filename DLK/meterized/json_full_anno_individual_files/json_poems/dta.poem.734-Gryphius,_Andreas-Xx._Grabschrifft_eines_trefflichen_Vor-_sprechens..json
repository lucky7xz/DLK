{"dta.poem.734": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Xx.  Grabschrifft eines trefflichen Vor-  \n sprechens.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Ich der durch alle Netz der ernsten Rechte brach/ ", "tokens": ["Ich", "der", "durch", "al\u00b7le", "Netz", "der", "erns\u00b7ten", "Rech\u00b7te", "brach", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "APPR", "PIAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem an verstand vnd Kunst kaum jemand gleich zu 'sch\u00e4tz&#7869; ", "tokens": ["Dem", "an", "ver\u00b7stand", "vnd", "Kunst", "kaum", "je\u00b7mand", "gleich", "zu", "'sch\u00e4tz", "&#7869;"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "XML_entity"], "pos": ["ART", "APPR", "NE", "KON", "NN", "ADV", "PIS", "ADV", "APPR", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der sich f\u00fcr keinem Thron noch Richtstul kont entsetzen", "tokens": ["Der", "sich", "f\u00fcr", "kei\u00b7nem", "Thron", "noch", "Richt\u00b7stul", "kont", "ent\u00b7set\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "APPR", "PIAT", "NN", "ADV", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verlohr/ als mir der Todt mein endlich vrtheil sprach.", "tokens": ["Ver\u00b7lohr", "/", "als", "mir", "der", "Todt", "mein", "end\u00b7lich", "vrtheil", "sprach", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOUS", "PPER", "ART", "NN", "PPOSAT", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.2": {"line.1": {"text": "Der wolberedte Mund/ der gleich der stoltzen bach", "tokens": ["Der", "wol\u00b7be\u00b7red\u00b7te", "Mund", "/", "der", "gleich", "der", "stolt\u00b7zen", "bach"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sich vnverzagt ergo\u00df/ der jede zu verletzen", "tokens": ["Sich", "vn\u00b7ver\u00b7zagt", "er\u00b7go\u00df", "/", "der", "je\u00b7de", "zu", "ver\u00b7let\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PRF", "ADJD", "VVFIN", "$(", "ART", "PIS", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnd tr\u00f6sten m\u00e4chtig war/ verga\u00df sein weises schw\u00e4tzen/", "tokens": ["Vnd", "tr\u00f6s\u00b7ten", "m\u00e4ch\u00b7tig", "war", "/", "ver\u00b7ga\u00df", "sein", "wei\u00b7ses", "schw\u00e4t\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "VAFIN", "$(", "VVFIN", "PPOSAT", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der strenge Richter gab mir keine frist mehr nach.", "tokens": ["Der", "stren\u00b7ge", "Rich\u00b7ter", "gab", "mir", "kei\u00b7ne", "frist", "mehr", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PIAT", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Er schlo\u00df die Augen zu/ den nichts verschlossen war/ ", "tokens": ["Er", "schlo\u00df", "die", "Au\u00b7gen", "zu", "/", "den", "nichts", "ver\u00b7schlos\u00b7sen", "war", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKZU", "$(", "ART", "PIS", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der K\u00e4rcker brach vnd schlo\u00df/ den schleust die enge bahr:", "tokens": ["Der", "K\u00e4r\u00b7cker", "brach", "vnd", "schlo\u00df", "/", "den", "schleust", "die", "en\u00b7ge", "bahr", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$(", "ART", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was hilfft mich da\u00df ich vor bef\u00f6rdert so viel sachen?", "tokens": ["Was", "hilfft", "mich", "da\u00df", "ich", "vor", "be\u00b7f\u00f6r\u00b7dert", "so", "viel", "sa\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "KOUS", "PPER", "APPR", "VVFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Da\u00df meine widerpart mit schrecken mich geh\u00f6rt?", "tokens": ["Da\u00df", "mei\u00b7ne", "wi\u00b7der\u00b7part", "mit", "schre\u00b7cken", "mich", "ge\u00b7h\u00f6rt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df jeder dem ich kundt/ mich mit best\u00fcrtzung ehrt?", "tokens": ["Da\u00df", "je\u00b7der", "dem", "ich", "kundt", "/", "mich", "mit", "be\u00b7st\u00fcrt\u00b7zung", "ehrt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "PPER", "PTKVZ", "$(", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nun nichts mich von dem Spruch de\u00df Todes lo\u00df kan mach\u1ebd.", "tokens": ["Nun", "nichts", "mich", "von", "dem", "Spruch", "de\u00df", "To\u00b7des", "lo\u00df", "kan", "mach\u1ebd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "PRF", "APPR", "ART", "NN", "ART", "NN", "PTKVZ", "VMFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}