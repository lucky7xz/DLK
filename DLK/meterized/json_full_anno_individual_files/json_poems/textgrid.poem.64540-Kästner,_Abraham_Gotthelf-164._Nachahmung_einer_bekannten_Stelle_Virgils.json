{"textgrid.poem.64540": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "164. Nachahmung einer bekannten Stelle Virgils", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welch Volk, Thuiskons Volk! gesteht den Rang dir zu?", "tokens": ["Welch", "Volk", ",", "Thu\u00b7is\u00b7kons", "Volk", "!", "ge\u00b7steht", "den", "Rang", "dir", "zu", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "NE", "NN", "$.", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der W\u00e4lsche singt und mahlt vortrefflicher als du;", "tokens": ["Der", "W\u00e4l\u00b7sche", "singt", "und", "mahlt", "vor\u00b7treff\u00b7li\u00b7cher", "als", "du", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "ADV", "KOUS", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Witz, Z\u00e4rtlichkeit, Geschmack, sich putzen, kochen, tanzen,", "tokens": ["Witz", ",", "Z\u00e4rt\u00b7lich\u00b7keit", ",", "Ge\u00b7schmack", ",", "sich", "put\u00b7zen", ",", "ko\u00b7chen", ",", "tan\u00b7zen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "PRF", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und was noch Alles mehr? lernst du vom muntern Franzen;", "tokens": ["Und", "was", "noch", "Al\u00b7les", "mehr", "?", "lernst", "du", "vom", "mun\u00b7tern", "Fran\u00b7zen", ";"], "token_info": ["word", "word", "w