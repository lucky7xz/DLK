r zu.", "tokens": ["Da", "brei\u00b7te", "dich", "\u00fc\u00b7ber", "und", "deck'", "ihn", "mir", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "KON", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.12": {"line.1": {"text": "Und ruft ihn ein Engel zum j\u00fcngsten Gerichte,", "tokens": ["Und", "ruft", "ihn", "ein", "En\u00b7gel", "zum", "j\u00fcng\u00b7sten", "Ge\u00b7rich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Da f\u00fchlt er das T\u00fcchlein auf seinem Gesichte,", "tokens": ["Da", "f\u00fchlt", "er", "das", "T\u00fcch\u00b7lein", "auf", "sei\u00b7nem", "Ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und merket in seinem erwachenden Sinn,", "tokens": ["Und", "mer\u00b7ket", "in", "sei\u00b7nem", "er\u00b7wa\u00b7chen\u00b7den", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Wie treu ich im Tod ihm gewesen bin.", "tokens": ["Wie", "treu", "ich", "im", "Tod", "ihm", "ge\u00b7we\u00b7sen", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "APPRART", "NN", "PPER", "VAPP", "VAFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}