{"dta.poem.8092": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Versunken, wehe, Mast und Kiel!", "tokens": ["Ver\u00b7sun\u00b7ken", ",", "we\u00b7he", ",", "Mast", "und", "Kiel", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "NN", "KON", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Schiffer Ruf verschollen!", "tokens": ["Der", "Schif\u00b7fer", "Ruf", "ver\u00b7schol\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch sieh! wer schwimmet dort herbei,", "tokens": ["Doch", "sieh", "!", "wer", "schwim\u00b7met", "dort", "her\u00b7bei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$.", "PWS", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um den die Wogen rollen?", "tokens": ["Um", "den", "die", "Wo\u00b7gen", "rol\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er schl\u00e4gt mit starkem Arm die Flut", "tokens": ["Er", "schl\u00e4gt", "mit", "star\u00b7kem", "Arm", "die", "Flut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und f\u00fcrchtet die Wellen wenig,", "tokens": ["Und", "f\u00fcrch\u00b7tet", "die", "Wel\u00b7len", "we\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Tr\u00e4gt hoch das Haupt mit goldner Kron\u2019,", "tokens": ["Tr\u00e4gt", "hoch", "das", "Haupt", "mit", "gold\u00b7ner", "Kron'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er d\u00fcnkt mir wohl ein K\u00f6nig.", "tokens": ["Er", "d\u00fcnkt", "mir", "wohl", "ein", "K\u00f6\u00b7nig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}