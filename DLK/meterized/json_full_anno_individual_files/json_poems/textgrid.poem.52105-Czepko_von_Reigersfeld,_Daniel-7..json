{"textgrid.poem.52105": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Indem das Fieber, das uns Flecke setzen kan,", "tokens": ["In\u00b7dem", "das", "Fie\u00b7ber", ",", "das", "uns", "Fle\u00b7cke", "set\u00b7zen", "kan", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "PPER", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Haare mir beginnt vom Sch\u00e4del abzuzwacken,", "tokens": ["Die", "Haa\u00b7re", "mir", "be\u00b7ginnt", "vom", "Sch\u00e4\u00b7del", "ab\u00b7zu\u00b7zwa\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "APPRART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Darauff die Glatze gl\u00e4ntzt wie Elephantenbacken,", "tokens": ["Dar\u00b7auff", "die", "Glat\u00b7ze", "gl\u00e4ntzt", "wie", "E\u00b7le\u00b7phan\u00b7ten\u00b7ba\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VVFIN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So stell' ich meinem Haubt ein Leichbeg\u00e4ngn\u00fc\u00df an.", "tokens": ["So", "stell'", "ich", "mei\u00b7nem", "Haubt", "ein", "Leich\u00b7be\u00b7g\u00e4n\u00b7gn\u00fc\u00df", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich sehe mir selbst zu, wie ich ein Theil allhier,", "tokens": ["Ich", "se\u00b7he", "mir", "selbst", "zu", ",", "wie", "ich", "ein", "Theil", "all\u00b7hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "PWAV", "PPER", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Eh' ich gestorben bin, mu\u00df hier zu Grabe schicken:", "tokens": ["Eh'", "ich", "ge\u00b7stor\u00b7ben", "bin", ",", "mu\u00df", "hier", "zu", "Gra\u00b7be", "schi\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$,", "VMFIN", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bi\u00df unter mir sich drauff die Tr\u00e4ger werden b\u00fccken:", "tokens": ["Bi\u00df", "un\u00b7ter", "mir", "sich", "drauff", "die", "Tr\u00e4\u00b7ger", "wer\u00b7den", "b\u00fc\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPER", "PRF", "PAV", "ART", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch du gestehst es nicht, und kommst zu H\u00fclffe mir.", "tokens": ["Doch", "du", "ge\u00b7stehst", "es", "nicht", ",", "und", "kommst", "zu", "H\u00fclf\u00b7fe", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKNEG", "$,", "KON", "VVFIN", "APPR", "NN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Weil du der Stirnen Ruhm und Ehre sincken siehst", "tokens": ["Weil", "du", "der", "Stir\u