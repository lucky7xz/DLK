{"textgrid.poem.28711": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "26.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frucht-tilgende Gesellschafft hat viel bi\u00dfher vernichtet;", "tokens": ["Frucht\u00b7til\u00b7gen\u00b7de", "Ge\u00b7sell\u00b7schafft", "hat", "viel", "bi\u00df\u00b7her", "ver\u00b7nich\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "+-+--+---+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Frucht-bringende Gesellschaft hat viel bi\u00dfher verrichtet;", "tokens": ["Frucht\u00b7brin\u00b7gen\u00b7de", "Ge\u00b7sell\u00b7schaft", "hat", "viel", "bi\u00df\u00b7her", "ver\u00b7rich\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "+----+--++--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Frucht-tilgende Gesellschafft nam Deutschland manche Zier;", "tokens": ["Frucht\u00b7til\u00b7gen\u00b7de", "Ge\u00b7sell\u00b7schafft", "nam", "Deutschland", "man\u00b7che", "Zier", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "NE", "PIAT", "NN", "$."], "meter": "+----+-+-+-+", "measure": "dactylic.init"}, "line.4": {"text": "Frucht-bringende Gesellschafft gab derer viel herf\u00fcr;", "tokens": ["Frucht\u00b7brin\u00b7gen\u00b7de", "Ge\u00b7sell\u00b7schafft", "gab", "de\u00b7rer", "viel", "her\u00b7f\u00fcr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PDS", "ADV", "ADV", "$."], "meter": "+----+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Frucht-tilgende Gesellschafft hat ihr