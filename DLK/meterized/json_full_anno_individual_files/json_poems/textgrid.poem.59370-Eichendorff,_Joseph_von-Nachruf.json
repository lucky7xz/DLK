{"textgrid.poem.59370": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Nachruf", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du liebe, treue Laute,", "tokens": ["Du", "lie\u00b7be", ",", "treu\u00b7e", "Lau\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie manche Sommernacht,", "tokens": ["Wie", "man\u00b7che", "Som\u00b7mer\u00b7nacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bis da\u00df der Morgen graute,", "tokens": ["Bis", "da\u00df", "der", "Mor\u00b7gen", "grau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hab ich mit dir durchwacht!", "tokens": ["Hab", "ich", "mit", "dir", "durch\u00b7wacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die T\u00e4ler wieder nachten,", "tokens": ["Die", "T\u00e4\u00b7ler", "wie\u00b7der", "nach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kaum spielt noch Abendrot,", "tokens": ["Kaum", "spielt", "noch", "A\u00b7ben\u00b7drot", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch die sonst mit uns wachten,", "tokens": ["Doch", "die", "sonst", "mit", "uns", "wach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die liegen lange tot.", "tokens": ["Die", "lie\u00b7gen", "lan\u00b7ge", "tot", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Was wollen wir nun singen", "tokens": ["Was", "wol\u00b7len", "wir", "nun", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hier in der Einsamkeit,", "tokens": ["Hier", "in", "der", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Wenn alle von uns gingen,", "tokens": ["Wenn", "al\u00b7le", "von", "uns", "gin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die unser Lied erfreut?", "tokens": ["Die", "un\u00b7ser", "Lied", "er\u00b7freut", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wir wollen dennoch singen!", "tokens": ["Wir", "wol\u00b7len", "den\u00b7noch", "sin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So still ist's auf der Welt;", "tokens": ["So", "still", "ist's", "auf", "der", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer wei\u00df, die Lieder dringen", "tokens": ["Wer", "wei\u00df", ",", "die", "Lie\u00b7der", "drin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vielleicht zum Sternenzelt.", "tokens": ["Viel\u00b7leicht", "zum", "Ster\u00b7nen\u00b7zelt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wer wei\u00df, die da gestorben,", "tokens": ["Wer", "wei\u00df", ",", "die", "da", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie h\u00f6ren droben mich,", "tokens": ["Sie", "h\u00f6\u00b7ren", "dro\u00b7ben", "mich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und \u00f6ffnen leis die Pforten", "tokens": ["Und", "\u00f6ff\u00b7nen", "leis", "die", "Pfor\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und nehmen uns zu sich.", "tokens": ["Und", "neh\u00b7men", "uns", "zu", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Du liebe, treue Laute,", "tokens": ["Du", "lie\u00b7be", ",", "treu\u00b7e", "Lau\u00b7te", ","], "token_info"