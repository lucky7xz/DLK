{"dta.poem.23593": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Schlagverse.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nein, bestehen soll das Schlagen!", "tokens": ["Nein", ",", "be\u00b7ste\u00b7hen", "soll", "das", "Schla\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVINF", "VMFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zwar nicht gut ist Schlag und Hieb,", "tokens": ["Zwar", "nicht", "gut", "ist", "Schlag", "und", "Hieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADJD", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Werden ", "tokens": ["Wer\u00b7den"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Ist uns alles Schlagen lieb.", "tokens": ["Ist", "uns", "al\u00b7les", "Schla\u00b7gen", "lieb", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Denn wir sind gut eingeschlagen,", "tokens": ["Denn", "wir", "sind", "gut", "ein\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Nicht geschlagen aus der Art.", "tokens": ["Nicht", "ge\u00b7schla\u00b7gen", "aus", "der", "Art", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVPP", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn die Trommel wird geschlagen,", "tokens": ["Wenn", "die", "Trom\u00b7mel", "wird", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist schon unser Volk geschaart.", "tokens": ["Ist", "schon", "un\u00b7ser", "Volk", "ge\u00b7schaart", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Gegen Feindes Anschlag schlagen", "tokens": ["Ge\u00b7gen", "Fein\u00b7des", "An\u00b7schlag", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wir den Richtweg ein zur Schlacht,", "tokens": ["Wir", "den", "Richt\u00b7weg", "ein", "zur", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ART", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es wird die Schlacht geschlagen,", "tokens": ["Und", "es", "wird", "die", "Schlacht", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eh's der Feind noch hat gedacht.", "tokens": ["Eh's", "der", "Feind", "noch", "hat", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADV", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie ein Schiff im Meer verschlagen,", "tokens": ["Wie", "ein", "Schiff", "im", "Meer", "ver\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schl\u00e4gt sein b\u00f6ser Anschlag um,", "tokens": ["Schl\u00e4gt", "sein", "b\u00f6\u00b7ser", "An\u00b7schlag", "um", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Trommel wird geschlagen,", "tokens": ["Und", "die", "Trom\u00b7mel", "wird", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Siegreich kehrn wir wieder um.", "tokens": ["Sieg\u00b7reich", "kehrn", "wir", "wie\u00b7der", "um", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und so wagen und so schlagen", "tokens": ["Und", "so", "wa\u00b7gen", "und", "so", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVINF", "KON", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wir uns muthig durch die Welt,", "tokens": ["Wir", "uns", "mut\u00b7hig", "durch", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Bis das Herz hat ausgeschlagen", "tokens": ["Bis", "das", "Herz", "hat", "aus\u00b7ge\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und des Lebens Schlagbaum f\u00e4llt.", "tokens": ["Und", "des", "Le\u00b7bens", "Schlag\u00b7baum", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und so lange Finken schlagen", "tokens": ["Und", "so", "lan\u00b7ge", "Fin\u00b7ken", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Eichen schlagen aus,", "tokens": ["Und", "die", "Ei\u00b7chen", "schla\u00b7gen", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Werden deutsche Herzen schlagen,", "tokens": ["Wer\u00b7den", "deut\u00b7sche", "Her\u00b7zen", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und das Schlagen stirbt nicht aus.", "tokens": ["Und", "das", "Schla\u00b7gen", "stirbt", "nicht", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}