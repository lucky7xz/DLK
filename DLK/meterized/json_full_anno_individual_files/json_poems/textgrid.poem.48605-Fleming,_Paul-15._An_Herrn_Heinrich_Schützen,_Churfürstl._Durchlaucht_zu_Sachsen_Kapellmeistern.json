{"textgrid.poem.48605": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "15. An Herrn Heinrich Sch\u00fctzen, Churf\u00fcrstl. Durchlaucht zu Sachsen Kapellmeistern", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ists nicht so, ber\u00fchmter ", "tokens": ["Ists", "nicht", "so", ",", "be\u00b7r\u00fchm\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["NE", "PTKNEG", "ADV", "$,", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Deine Mutter war wie schon", "tokens": ["Dei\u00b7ne", "Mut\u00b7ter", "war", "wie", "schon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "KOKOM", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "an der schwarzen Lethenpf\u00fctze", "tokens": ["an", "der", "schwar\u00b7zen", "Le\u00b7then\u00b7pf\u00fct\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "und dem bleichen Phlegethon;", "tokens": ["und", "dem", "blei\u00b7chen", "Phle\u00b7ge\u00b7thon", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Charon, der erbla\u00dfte Man,", "tokens": ["Cha\u00b7ron", ",", "der", "er\u00b7bla\u00df\u00b7te", "Man", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "VVFIN", "PIS", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.6": {"text": "schrie sie schon ums F\u00e4hrgeld an;", "tokens": ["schrie", "sie", "schon", "ums", "F\u00e4hr\u00b7geld", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "die Vernunft war fast verloren,", "tokens": ["die", "Ver\u00b7nunft", "war", "fast", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "und sie war nun nicht mehr sie,", "tokens": ["und", "sie", "war", "nun", "nicht", "mehr", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "PTKNEG", "ADV", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "der in die halbtauben Ohren", "tokens": ["der", "in", "die", "halb\u00b7tau\u00b7ben", "Oh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "man ihr noch di\u00df Wort einschrie:", "tokens": ["man", "ihr", "noch", "di\u00df", "Wort", "ein\u00b7schrie", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PPER", "ADV", "PDS", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "kommt und seht, hier ist er schon!", "tokens": ["kommt", "und", "seht", ",", "hier", "ist", "er", "schon", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie erfrischt' ihr di\u00df ihr Leben,", "tokens": ["Wie", "er\u00b7frischt'", "ihr", "di\u00df", "ihr", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PDS", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "das beinah' erstorben war!", "tokens": ["das", "bei\u00b7nah'", "ers\u00b7tor\u00b7ben", "war", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ihr Geist war ihr wiedergeben,", "tokens": ["Ihr", "Geist", "war", "ihr", "wie\u00b7der\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "welcher fast verhauchet gar.", "tokens": ["wel\u00b7cher", "fast", "ver\u00b7hau\u00b7chet", "gar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRELS", "ADV", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Komm, Sohn, sprach sie, komm herzu!", "tokens": ["Komm", ",", "Sohn", ",", "sprach", "sie", ",", "komm", "her\u00b7zu", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "PPER", "$,", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Meines Todes Tod bist du!", "tokens": ["Mei\u00b7nes", "To\u00b7des", "Tod", "bist", "du", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VAFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Was ists not, da\u00df, wie vorzeiten", "tokens": ["Was", "ists", "not", ",", "da\u00df", ",", "wie", "vor\u00b7zei\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PWS", "VAFIN", "NN", "$,", "KOUS", "$,", "PWAV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "es \u00c4agers Sohn gemacht,", "tokens": ["es", "\u00c4\u00b7a\u00b7gers", "Sohn", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "NN", "VVPP", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "du mit Liedern, Spiel und Saiten", "tokens": ["du", "mit", "Lie\u00b7dern", ",", "Spiel", "und", "Sai\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "fahrest in den finstern Schacht?", "tokens": ["fah\u00b7rest", "in", "den", "fins\u00b7tern", "Schacht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "giebt der Tod die Toten los.", "tokens": ["giebt", "der", "Tod", "die", "To\u00b7ten", "los", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Du machst dir mit deinen Liedern", "tokens": ["Du", "machst", "dir", "mit", "dei\u00b7nen", "Lie\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hell' und Himmel untertan,", "tokens": ["Hell'", "und", "Him\u00b7mel", "un\u00b7ter\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "da\u00df dir keines nichts verwiedern,", "tokens": ["da\u00df", "dir", "kei\u00b7nes", "nichts", "ver\u00b7wie\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "keines nichts versagen kan,", "tokens": ["kei\u00b7nes", "nichts", "ver\u00b7sa\u00b7gen", "kan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PIS", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "weil auch, wenn du schon nicht singst,", "tokens": ["weil", "auch", ",", "wenn", "du", "schon", "nicht", "singst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "$,", "KOUS", "PPER", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "du sie beide doch bezwingst.", "tokens": ["du", "sie", "bei\u00b7de", "doch", "be\u00b7zwingst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "PIS", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Di\u00df kan eine sch\u00f6ne Seele,", "tokens": ["Di\u00df", "kan", "ei\u00b7ne", "sch\u00f6\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "die den Himmel Vater hei\u00dft,", "tokens": ["die", "den", "Him\u00b7mel", "Va\u00b7ter", "hei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "die aus der beleibten H\u00f6le", "tokens": ["die", "aus", "der", "be\u00b7leib\u00b7ten", "H\u00f6\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "\u00fcber sich und zu ihm reist", "tokens": ["\u00fc\u00b7ber", "sich", "und", "zu", "ihm", "reist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRF", "KON", "APPR", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "und ihm ein Gesetze schreibet", "tokens": ["und", "ihm", "ein", "Ge\u00b7set\u00b7ze", "schrei\u00b7bet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "durch das, was doch seine, bleibet.", "tokens": ["durch", "das", ",", "was", "doch", "sei\u00b7ne", ",", "blei\u00b7bet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PDS", "$,", "PRELS", "ADV", "PPOSAT", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Hilft mir Gott und will mein Gl\u00fccke,", "tokens": ["Hilft", "mir", "Gott", "und", "will", "mein", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "VMFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "da\u00df mirs auch noch wol soll gehn,", "tokens": ["da\u00df", "mirs", "auch", "noch", "wol", "soll", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADV", "ADV", "ADV", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "da\u00df ich nicht so stets zur\u00fccke", "tokens": ["da\u00df", "ich", "nicht", "so", "stets", "zu\u00b7r\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "und in schlechter Acht mu\u00df stehn,", "tokens": ["und", "in", "schlech\u00b7ter", "Acht", "mu\u00df", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "so ist deines F\u00fcrsten Gunst", "tokens": ["so", "ist", "dei\u00b7nes", "F\u00fcrs\u00b7ten", "Gunst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "mir nicht, hoffe, gar umsonst.", "tokens": ["mir", "nicht", ",", "hof\u00b7fe", ",", "gar", "um\u00b7sonst", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "$,", "VVFIN", "$,", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "So will ich einmal auch kommen,", "tokens": ["So", "will", "ich", "ein\u00b7mal", "auch", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "wo ihr sch\u00f6nen Leute seid.", "tokens": ["wo", "ihr", "sch\u00f6\u00b7nen", "Leu\u00b7te", "seid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was ich mir schon vorgenommen,", "tokens": ["Was", "ich", "mir", "schon", "vor\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "das er\u00f6ffnet jene Zeit.", "tokens": ["das", "er\u00b7\u00f6ff\u00b7net", "je\u00b7ne", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Di\u00df ists, das ich sagen will;", "tokens": ["Di\u00df", "ists", ",", "das", "ich", "sa\u00b7gen", "will", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "dort ist meiner Hoffnung Ziel.", "tokens": ["dort", "ist", "mei\u00b7ner", "Hoff\u00b7nung", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Vater Mars, la\u00df ab von ", "tokens": ["Va\u00b7ter", "Mars", ",", "la\u00df", "ab", "von"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NE", "$,", "VVIMP", "PTKVZ", "APPR"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "und verg\u00f6nn uns deine Ruh'!", "tokens": ["und", "ver\u00b7g\u00f6nn", "uns", "dei\u00b7ne", "Ruh'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df sich auch nun Fremde schmei\u00dfen,", "tokens": ["La\u00df", "sich", "auch", "nun", "Frem\u00b7de", "schmei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PRF", "ADV", "ADV", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "die uns lange satt sehn zu!", "tokens": ["die", "uns", "lan\u00b7ge", "satt", "sehn", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "weise nun auch deine Macht", "tokens": ["wei\u00b7se", "nun", "auch", "dei\u00b7ne", "Macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "dem, der sie noch itzt verlacht!", "tokens": ["dem", ",", "der", "sie", "noch", "itzt", "ver\u00b7lacht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Denn so will ich dich erheben,", "tokens": ["Denn", "so", "will", "ich", "dich", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "wenn du weit von uns wirst sein,", "tokens": ["wenn", "du", "weit", "von", "uns", "wirst", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "PPER", "VAFIN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "und mein ", "tokens": ["und", "mein"], "token_info": ["word", "word"], "pos": ["KON", "PPOSAT"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "seine Saiten stimmen drein,", "tokens": ["sei\u00b7ne", "Sai\u00b7ten", "stim\u00b7men", "drein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "da\u00df die Gegend weit und breit", "tokens": ["da\u00df", "die", "Ge\u00b7gend", "weit", "und", "breit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "froh sein wird bei solcher Zeit.", "tokens": ["froh", "sein", "wird", "bei", "sol\u00b7cher", "Zeit", "."], "token_info": ["wo