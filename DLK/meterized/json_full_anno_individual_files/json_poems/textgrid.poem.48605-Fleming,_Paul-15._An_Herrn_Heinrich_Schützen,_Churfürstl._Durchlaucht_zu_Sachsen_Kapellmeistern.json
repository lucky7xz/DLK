, "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "dort ist meiner Hoffnung Ziel.", "tokens": ["dort", "ist", "mei\u00b7ner", "Hoff\u00b7nung", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.19": {"line.1": {"text": "Vater Mars, la\u00df ab von ", "tokens": ["Va\u00b7ter", "Mars", ",", "la\u00df", "ab", "von"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NE", "$,", "VVIMP", "PTKVZ", "APPR"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "und verg\u00f6nn uns deine Ruh'!", "tokens": ["und", "ver\u00b7g\u00f6nn", "uns", "dei\u00b7ne", "Ruh'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df sich auch nun Fremde schmei\u00dfen,", "tokens": ["La\u00df", "sich", "auch", "nun", "Frem\u00b7de", "schmei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PRF", "ADV", "ADV", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "die uns lange satt sehn zu!", "tokens": ["die", "uns", "lan\u00b7ge", "satt", "sehn", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "weise nun auch deine Macht", "tokens": ["wei\u00b7se", "nun", "auch", "dei\u00b7ne", "Macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "dem, der sie noch itzt verlacht!", "tokens": ["dem", ",", "der", "sie", "noch", "itzt", "ver\u00b7lacht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.20": {"line.1": {"text": "Denn so will ich dich erheben,", "tokens": ["Denn", "so", "will", "ich", "dich", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "wenn du weit von uns wirst sein,", "tokens": ["wenn", "du", "weit", "von", "uns", "wirst", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "PPER", "VAFIN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "und mein ", "tokens": ["und", "mein"], "token_info": ["word", "word"], "pos": ["KON", "PPOSAT"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "seine Saiten stimmen drein,", "tokens": ["sei\u00b7ne", "Sai\u00b7ten", "stim\u00b7men", "drein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "da\u00df die Gegend weit und breit", "tokens": ["da\u00df", "die", "Ge\u00b7gend", "weit", "und", "breit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "froh sein wird bei solcher Zeit.", "tokens": ["froh", "sein", "wird", "bei", "sol\u00b7cher", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAINF", "VAFIN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}