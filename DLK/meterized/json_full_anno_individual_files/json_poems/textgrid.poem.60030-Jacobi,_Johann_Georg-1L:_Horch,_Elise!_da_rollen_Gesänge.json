{"textgrid.poem.60030": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Horch, Elise! da rollen Ges\u00e4nge", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Horch, Elise! da rollen Ges\u00e4nge", "tokens": ["Horch", ",", "E\u00b7li\u00b7se", "!", "da", "rol\u00b7len", "Ge\u00b7s\u00e4n\u00b7ge"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "NE", "$.", "ADV", "ADJA", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Goldner Harfen her;", "tokens": ["Gold\u00b7ner", "Har\u00b7fen", "her", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ueber hohe Felseng\u00e4nge", "tokens": ["Ue\u00b7ber", "ho\u00b7he", "Fel\u00b7sen\u00b7g\u00e4n\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rollen sie, wie der Donner, schwer.", "tokens": ["Rol\u00b7len", "sie", ",", "wie", "der", "Don\u00b7ner", ",", "schwer", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "$,", "PWAV", "ART", "NN", "$,", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Barden singen von der Welten", "tokens": ["Bar\u00b7den", "sin\u00b7gen", "von", "der", "Wel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vater, der als Richter k\u00f6mmt;", "tokens": ["Va\u00b7ter", ",", "der", "als", "Rich\u00b7ter", "k\u00f6mmt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "KOUS", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Singen, wie vor seinem Schelten", "tokens": ["Sin\u00b7gen", ",", "wie", "vor", "sei\u00b7nem", "Schel\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meere fliehen, und der Strom sich hemmt;", "tokens": ["Mee\u00b7re", "flie\u00b7hen", ",", "und", "der", "Strom", "sich", "hemmt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "KON", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Singen vom allm\u00e4chtigen Erstaunen,", "tokens": ["Sin\u00b7gen", "vom", "all\u00b7m\u00e4ch\u00b7ti\u00b7gen", "Er\u00b7stau\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Das die Sonne fa\u00dft in ihrer Bahn,", "tokens": ["Das", "die", "Son\u00b7ne", "fa\u00dft", "in", "ih\u00b7rer", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und von Gr\u00e4bern, aufgethan", "tokens": ["Und", "von", "Gr\u00e4\u00b7bern", ",", "auf\u00b7ge\u00b7than"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["KON", "APPR", "NN", "$,", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bey dem Rufe der Posaunen;", "tokens": ["Bey", "dem", "Ru\u00b7fe", "der", "Po\u00b7sau\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Von der Erden Untergang,", "tokens": ["Von", "der", "Er\u00b7den", "Un\u00b7ter\u00b7gang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Welche sich in Feuerflammen w\u00e4lzen,", "tokens": ["Wel\u00b7che", "sich", "in", "Feu\u00b7er\u00b7flam\u00b7men", "w\u00e4l\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und von Sternen, welche schmelzen,", "tokens": ["Und", "von", "Ster\u00b7nen", ",", "wel\u00b7che", "schmel\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$,", "PRELS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bey der Todesengel Gesang.", "tokens": ["Bey", "der", "To\u00b7de\u00b7sen\u00b7gel", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Deine Seele bebt, Elise!", "tokens": ["Dei\u00b7ne", "See\u00b7le", "bebt", ",", "E\u00b7li\u00b7se", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie das fromme L\u00e4mmchen bebt,", "tokens": ["Wie", "das", "from\u00b7me", "L\u00e4mm\u00b7chen", "bebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn sich \u00fcber seiner Wiese", "tokens": ["Wenn", "sich", "\u00fc\u00b7ber", "sei\u00b7ner", "Wie\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schnell ein hohler Sturm erhebt?", "tokens": ["Schnell", "ein", "hoh\u00b7ler", "Sturm", "er\u00b7hebt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Zittre nicht! Ein Gott will richten,", "tokens": ["Zitt\u00b7re", "nicht", "!", "Ein", "Gott", "will", "rich\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$.", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Richten will er jede That;", "tokens": ["Rich\u00b7ten", "will", "er", "je\u00b7de", "That", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber kann er diese Welt zernichten,", "tokens": ["A\u00b7ber", "kann", "er", "die\u00b7se", "Welt", "zer\u00b7nich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PDAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Diesen Boden den Elise betrat?", "tokens": ["Die\u00b7sen", "Bo\u00b7den", "den", "E\u00b7li\u00b7se", "be\u00b7trat", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Schaffende Liebe winket einst der Erde,", "tokens": ["Schaf\u00b7fen\u00b7de", "Lie\u00b7be", "win\u00b7ket", "einst", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Da\u00df ein neuer Fr\u00fchling werde;", "tokens": ["Da\u00df", "ein", "neu\u00b7er", "Fr\u00fch\u00b7ling", "wer\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zeichnet neuen Sonnen ihre Bahn;", "tokens": ["Zeich\u00b7net", "neu\u00b7en", "Son\u00b7nen", "ih\u00b7re", "Bahn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und ein besserer Tag bricht an.", "tokens": ["Und", "ein", "bes\u00b7se\u00b7rer", "Tag", "bricht", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}}, "stanza.8": {"line.1": {"text": "Friede zieht in jede H\u00f6hle;", "tokens": ["Frie\u00b7de", "zieht", "in", "je\u00b7de", "H\u00f6h\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Still und lieblich soll der Hain,", "tokens": ["Still", "und", "lieb\u00b7lich", "soll", "der", "Hain", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VMFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und so sch\u00f6n, wie deine Seele,", "tokens": ["Und", "so", "sch\u00f6n", ",", "wie", "dei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "$,", "PWAV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Soll die ganze Sch\u00f6pfung seyn.", "tokens": ["Soll", "die", "gan\u00b7ze", "Sch\u00f6p\u00b7fung", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Ueberall Fr\u00fchlingsluft:", "tokens": ["Ue\u00b7be\u00b7rall", "Fr\u00fch\u00b7lings\u00b7luft", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Ueberall ein ruhiges Wehen.", "tokens": ["Ue\u00b7be\u00b7rall", "ein", "ru\u00b7hi\u00b7ges", "We\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+----+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Blumen werden auferstehen", "tokens": ["Blu\u00b7men", "wer\u00b7den", "auf\u00b7er\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["NN", "VAFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dann um deine Gruft;", "tokens": ["Dann", "um", "dei\u00b7ne", "Gruft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "Blumen, welche deinem Schatten", "tokens": ["Blu\u00b7men", ",", "wel\u00b7che", "dei\u00b7nem", "Schat\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00e4dchenh\u00e4nde gestreuet hatten", "tokens": ["M\u00e4d\u00b7chen\u00b7h\u00e4n\u00b7de", "ge\u00b7streu\u00b7et", "hat\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVPP", "VAFIN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Hin auf jene Gefilde der Ruh;", "tokens": ["Hin", "auf", "je\u00b7ne", "Ge\u00b7fil\u00b7de", "der", "Ruh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "NN", "ART", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Blumen, l\u00e4ngst gestorben, wie du.", "tokens": ["Blu\u00b7men", ",", "l\u00e4ngst", "ge\u00b7stor\u00b7ben", ",", "wie", "du", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVPP", "$,", "PWAV", "PPER", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.11": {"line.1": {"text": "Wie sie bl\u00fchend auferstehen,", "tokens": ["Wie", "sie", "bl\u00fc\u00b7hend", "auf\u00b7er\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So erwachen, bey dem Wehen", "tokens": ["So", "er\u00b7wa\u00b7chen", ",", "bey", "dem", "We\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVINF", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Einer stilleren Luft,", "tokens": ["Ei\u00b7ner", "stil\u00b7le\u00b7ren", "Luft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Nachtigallen rings um deine Gruft.", "tokens": ["Nach\u00b7ti\u00b7gal\u00b7len", "rings", "um", "dei\u00b7ne", "Gruft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.12": {"line.1": {"text": "Neben ihr hatten sie gesungen", "tokens": ["Ne\u00b7ben", "ihr", "hat\u00b7ten", "sie", "ge\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "VVPP"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Durch die Gefilde der Ruh,", "tokens": ["Durch", "die", "Ge\u00b7fil\u00b7de", "der", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Neben ihr in Abendd\u00e4mmerungen:", "tokens": ["Ne\u00b7ben", "ihr", "in", "A\u00b7bend\u00b7d\u00e4m\u00b7me\u00b7run\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und sie starben l\u00e4ngst, wie du.", "tokens": ["Und", "sie", "star\u00b7ben", "l\u00e4ngst", ",", "wie", "du", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$,", "PWAV", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Komm, Elise! Ger\u00fcche wallen", "tokens": ["Komm", ",", "E\u00b7li\u00b7se", "!", "Ge\u00b7r\u00fc\u00b7che", "wal\u00b7len"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "NE", "$.", "NN", "VVINF"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Von verj\u00fcngten B\u00e4umen herab:", "tokens": ["Von", "ver\u00b7j\u00fcng\u00b7ten", "B\u00e4u\u00b7men", "her\u00b7ab", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "O, beym Gru\u00dfe der Nachtigallen,", "tokens": ["O", ",", "beym", "Gru\u00b7\u00dfe", "der", "Nach\u00b7ti\u00b7gal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Oeffnet sich dein Grab.", "tokens": ["Oeff\u00b7net", "sich", "dein", "Grab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.14": {"line.1": {"text": "Komm, Elise! Schon umringen,", "tokens": ["Komm", ",", "E\u00b7li\u00b7se", "!", "Schon", "um\u00b7rin\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NE", "$.", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie Gespielen einer Braut,", "tokens": ["Wie", "Ge\u00b7spie\u00b7len", "ei\u00b7ner", "Braut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Selige Geister dich, und singen", "tokens": ["Se\u00b7li\u00b7ge", "Geis\u00b7ter", "dich", ",", "und", "sin\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "PPER", "$,", "KON", "VVINF"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Deines Lebens stille Thaten laut.", "tokens": ["Dei\u00b7nes", "Le\u00b7bens", "stil\u00b7le", "Tha\u00b7ten", "laut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.15": {"line.1": {"text": "Und du gehst an ihrer Seite", "tokens": ["Und", "du", "gehst", "an", "ih\u00b7rer", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Nun mit sanfter Majest\u00e4t,", "tokens": ["Nun", 