{"textgrid.poem.67279": {"metadata": {"author": {"name": "Stolberg, Christian Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: India's liebliche Tochter, wohin sind entflohen die Gaben,", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "India's liebliche Tochter, wohin sind entflohen die Gaben,", "tokens": ["In\u00b7dia's", "lieb\u00b7li\u00b7che", "Toch\u00b7ter", ",", "wo\u00b7hin", "sind", "ent\u00b7flo\u00b7hen", "die", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PWAV", "VAFIN", "VVPP", "ART", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Schoo\u00dfkind du der Natur, die sie so milde dir gab.", "tokens": ["Schoo\u00df\u00b7kind", "du", "der", "Na\u00b7tur", ",", "die", "sie", "so", "mil\u00b7de", "dir", "gab", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADJD", "PPER", "VVFIN", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Sch\u00f6nheitsbl\u00fcthe, Zauber des Reitzes? \u2013 Sieh' es entschwebt aus", "tokens": ["Sch\u00f6n\u00b7heits\u00b7bl\u00fct\u00b7he", ",", "Zau\u00b7ber", "des", "Reit\u00b7zes", "?", "\u2013", "Sieh'", "es", "ent\u00b7schwebt", "aus"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "ART", "NN", "$.", "$(", "VVIMP", "PPER", "VVFIN", "PTKVZ"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Scheiterflammen der Geist, herrlich ein Ph\u00f6nix empor!", "tokens": ["Schei\u00b7ter\u00b7flam\u00b7men", "der", "Geist", ",", "herr\u00b7lich", "ein", "Ph\u00f6\u00b7nix", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Liebe hatte gewebt dein zartes Gewebe, dich opfernd", "tokens": ["Lie\u00b7be", "hat\u00b7te", "ge\u00b7webt", "dein", "zar\u00b7tes", "Ge\u00b7we\u00b7be", ",", "dich", "op\u00b7fernd"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VAFIN", "VVPP", "PPOSAT", "ADJA", "NN", "$,", "PPER", "VVPP"], "meter": "+-+--+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Deinem Geliebten, und wie? gab dir die Liebe den Tod.", "tokens": ["Dei\u00b7nem", "Ge\u00b7lieb\u00b7ten", ",", "und", "wie", "?", "gab", "dir", "die", "Lie\u00b7be", "den", "Tod", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "PWAV", "$.", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Kehrest du wieder zur Erde, so falle deiner Gespielinn", "tokens": ["Keh\u00b7rest", "du", "wie\u00b7der", "zur", "Er\u00b7de", ",", "so", "fal\u00b7le", "dei\u00b7ner", "Ge\u00b7spie\u00b7linn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "$,", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Seliges Loos auch dir, welches ihr S\u00e4nger uns sang!", "tokens": ["Se\u00b7li\u00b7ges", "Loos", "auch", "dir", ",", "wel\u00b7ches", "ihr", "S\u00e4n\u00b7ger", "uns", "sang", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "PPER", "$,", "PRELS", "PPOSAT", "