{"dta.poem.19554": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Weihgeschenk.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Heute deiner zu gedenken,", "tokens": ["Heu\u00b7te", "dei\u00b7ner", "zu", "ge\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deren Grab die Nacht bethaut,", "tokens": ["De\u00b7ren", "Grab", "die", "Nacht", "be\u00b7thaut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nahen wir mit Weihgeschenken", "tokens": ["Na\u00b7hen", "wir", "mit", "Weih\u00b7ge\u00b7schen\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ged\u00e4mpftem Klagelaut!", "tokens": ["Und", "ge\u00b7d\u00e4mpf\u00b7tem", "Kla\u00b7ge\u00b7laut", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Warum war dir's nicht gegeben,", "tokens": ["Wa\u00b7rum", "war", "dir's", "nicht", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PIS", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Muthig deinen Tag zu leben?", "tokens": ["Mut\u00b7hig", "dei\u00b7nen", "Tag", "zu", "le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Heil Dir, K\u00f6nigin der Nacht,", "tokens": ["Heil", "Dir", ",", "K\u00f6\u00b7ni\u00b7gin", "der", "Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Die Dein M\u00e4gdlein umgebracht!", "tokens": ["Die", "Dein", "M\u00e4gd\u00b7lein", "um\u00b7ge\u00b7bracht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Braune, schwermuthvolle Augen,", "tokens": ["Brau\u00b7ne", ",", "schwer\u00b7muth\u00b7vol\u00b7le", "Au\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Oeffnet euch ein letztes Mal!", "tokens": ["Oeff\u00b7net", "euch", "ein", "letz\u00b7tes", "Mal", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00dft aus euren Tiefen saugen", "tokens": ["La\u00dft", "aus", "eu\u00b7ren", "Tie\u00b7fen", "sau\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich noch einen s\u00fc\u00dfen Strahl!", "tokens": ["Mich", "noch", "ei\u00b7nen", "s\u00fc\u00b7\u00dfen", "Strahl", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "O wie hatt' ich euch so gerne,", "tokens": ["O", "wie", "hatt'", "ich", "euch", "so", "ger\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VAFIN", "PPER", "PPER", "ADV", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Traute, tr\u00e4umerische Sterne!", "tokens": ["Trau\u00b7te", ",", "tr\u00e4u\u00b7me\u00b7ri\u00b7sche", "Ster\u00b7ne", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Heil Dir, K\u00f6nigin der Nacht,", "tokens": ["Heil", "Dir", ",", "K\u00f6\u00b7ni\u00b7gin", "der", "Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Die Dein M\u00e4gdlein umgebracht!", "tokens": ["Die", "Dein", "M\u00e4gd\u00b7lein", "um\u00b7ge\u00b7bracht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie das Sch\u00fcttern zarter Saiten,", "tokens": ["Wie", "das", "Sch\u00fct\u00b7tern", "zar\u00b7ter", "Sai\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlichen sich in jedes Herz", "tokens": ["Schli\u00b7chen", "sich", "in", "je\u00b7des", "Herz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine stillen Lieblichkeiten,", "tokens": ["Dei\u00b7ne", "stil\u00b7len", "Lieb\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deiner Z\u00fcge leiser Schmerz!", "tokens": ["Dei\u00b7ner", "Z\u00fc\u00b7ge", "lei\u00b7ser", "Schmerz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Feuchte Waldesschatten lagen", "tokens": ["Feuch\u00b7te", "Wal\u00b7des\u00b7schat\u00b7ten", "la\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ueber dir in Lenzestagen \u2014", "tokens": ["Ue\u00b7ber", "dir", "in", "Len\u00b7zes\u00b7ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Heil Dir, K\u00f6nigin der Nacht,", "tokens": ["Heil", "Dir", ",", "K\u00f6\u00b7ni\u00b7gin", "der", "Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Die Dein M\u00e4gdlein umgebracht!", "tokens": ["Die", "Dein", "M\u00e4gd\u00b7lein", "um\u00b7ge\u00b7bracht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie ein Reh dem Wald entronnen,", "tokens": ["Wie", "ein", "Reh", "dem", "Wald", "ent\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das ein \u00fcppig Thal entdeckt,", "tokens": ["Das", "ein", "\u00fcp\u00b7pig", "Thal", "ent\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADJD", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nahtest sch\u00fcchtern du den Bronnen,", "tokens": ["Nah\u00b7test", "sch\u00fcch\u00b7tern", "du", "den", "Bron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Flohst, vom eignen Bild geschreckt!", "tokens": ["Flohst", ",", "vom", "eig\u00b7nen", "Bild", "ge\u00b7schreckt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aengstlich, wo sich Wege theilen,", "tokens": ["A\u00b7engst\u00b7lich", ",", "wo", "sich", "We\u00b7ge", "thei\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "PRF", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Seh' ich zweifeln dich und weilen \u2014", "tokens": ["Seh'", "ich", "zwei\u00b7feln", "dich", "und", "wei\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "PPER", "KON", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Heil Dir, K\u00f6nigin der Nacht,", "tokens": ["Heil", "Dir", ",", "K\u00f6\u00b7ni\u00b7gin", "der", "Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Die Dein M\u00e4gdlein umgebracht!", "tokens": ["Die", "Dein", "M\u00e4gd\u00b7lein", "um\u00b7ge\u00b7bracht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Zeigte jung ein arger Spiegel", "tokens": ["Zeig\u00b7te", "jung", "ein", "ar\u00b7ger", "Spie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dir den Wurm in jeder Frucht?", "tokens": ["Dir", "den", "Wurm", "in", "je\u00b7der", "Frucht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schwebte nahen Todes Fl\u00fcgel", "tokens": ["Schweb\u00b7te", "na\u00b7hen", "To\u00b7des", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ueber dir mit Eifersucht?", "tokens": ["Ue\u00b7ber", "dir", "mit", "Ei\u00b7fer\u00b7sucht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nie hat dich ein Arm umschlossen,", "tokens": ["Nie", "hat", "dich", "ein", "Arm", "um\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Liebe hast du nie genossen \u2014", "tokens": ["Lie\u00b7be", "hast", "du", "nie", "ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Heil Dir, K\u00f6nigin der Nacht,", "tokens": ["Heil", "Dir", ",", "K\u00f6\u00b7ni\u00b7gin", "der", "Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Die Dein M\u00e4gdlein umgebracht!", "tokens": ["Die", "Dein", "M\u00e4gd\u00b7lein", "um\u00b7ge\u00b7bracht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Willig stiegest du die Stufen", "tokens": ["Wil\u00b7lig", "stie\u00b7gest", "du", "die", "Stu\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nieder in dein fr\u00fches Grab,", "tokens": ["Nie\u00b7der", "in", "dein", "fr\u00fc\u00b7hes", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wandtest dich, von uns gerufen,", "tokens": ["Wand\u00b7test", "dich", ",", "von", "uns", "ge\u00b7ru\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00e4chelnd um \u2014 und stiegst hinab!", "tokens": ["L\u00e4\u00b7chelnd", "um", "und", "stiegst", "hin\u00b7ab", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "$(", "KON", "VVFIN", "PTKVZ", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Mit gelassener Geberde", "tokens": ["Mit", "ge\u00b7las\u00b7se\u00b7ner", "Ge\u00b7ber\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.6": {"text": "Schiedest du vom Gr\u00fcn der Erde \u2014", "tokens": ["Schie\u00b7dest", "du", "vom", "Gr\u00fcn", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", 