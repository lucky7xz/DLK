{"dta.poem.4080": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Wanderschaft.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Wandern!", "tokens": ["Das", "Wan\u00b7dern", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Das mu\u00df ein schlechter M\u00fcller sein,", "tokens": ["Das", "mu\u00df", "ein", "schlech\u00b7ter", "M\u00fcl\u00b7ler", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem niemals fiel das Wandern ein,", "tokens": ["Dem", "nie\u00b7mals", "fiel", "das", "Wan\u00b7dern", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Wandern.", "tokens": ["Das", "Wan\u00b7dern", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Vom Wasser haben wir's gelernt,", "tokens": ["Vom", "Was\u00b7ser", "ha\u00b7ben", "wir's", "ge\u00b7lernt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Wasser!", "tokens": ["Vom", "Was\u00b7ser", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Das hat nicht Rast bei Tag und Nacht,", "tokens": ["Das", "hat", "nicht", "Rast", "bei", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKNEG", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist stets auf Wanderschaft bedacht,", "tokens": ["Ist", "stets", "auf", "Wan\u00b7der\u00b7schaft", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das Wasser.", "tokens": ["Das", "Was\u00b7ser", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Das sehn wir auch den R\u00e4dern ab,", "tokens": ["Das", "sehn", "wir", "auch", "den", "R\u00e4\u00b7dern", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den R\u00e4dern!", "tokens": ["Den", "R\u00e4\u00b7dern", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Die gar nicht gerne stille stehn,", "tokens": ["Die", "gar", "nicht", "ger\u00b7ne", "stil\u00b7le", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PTKNEG", "ADV", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die sich mein Tag nicht m\u00fcde drehn,", "tokens": ["Die", "sich", "mein", "Tag", "nicht", "m\u00fc\u00b7de", "drehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "PPOSAT", "NN", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die R\u00e4der.", "tokens": ["Die", "R\u00e4\u00b7der", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "Die Steine selbst, so schwer sie sind,", "tokens": ["Die", "Stei\u00b7ne", "selbst", ",", "so", "schwer", "sie", "sind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ADV", "ADJD", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Steine!", "tokens": ["Die", "Stei\u00b7ne", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Sie tanzen mit den muntern Reihn", "tokens": ["Sie", "tan\u00b7zen", "mit", "den", "mun\u00b7tern", "Reihn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wollen gar noch schneller sein,", "tokens": ["Und", "wol\u00b7len", "gar", "noch", "schnel\u00b7ler", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Steine.", "tokens": ["Die", "Stei\u00b7ne", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.5": {"line.1": {"text": "O Wandern, Wandern, meine Lust,", "tokens": ["O", "Wan\u00b7dern", ",", "Wan\u00b7dern", ",", "mei\u00b7ne", "Lust", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Wandern!", "tokens": ["O", "Wan\u00b7dern", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Herr Meister und Frau Meisterin,", "tokens": ["Herr", "Meis\u00b7ter", "und", "Frau", "Meis\u00b7te\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "KON", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "La\u00dft mich in Frieden weiter ziehn", "tokens": ["La\u00dft", "mich", "in", "Frie\u00b7den", "wei\u00b7ter", "ziehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wandern.", "tokens": ["Und", "wan\u00b7dern", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}