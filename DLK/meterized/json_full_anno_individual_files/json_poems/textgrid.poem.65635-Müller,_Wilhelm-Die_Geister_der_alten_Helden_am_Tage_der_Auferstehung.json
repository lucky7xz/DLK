{"textgrid.poem.65635": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Geister der alten Helden am Tage der Auferstehung", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir haben tief geschlafen, wir haben schwer getr\u00e4umt \u2013", "tokens": ["Wir", "ha\u00b7ben", "tief", "ge\u00b7schla\u00b7fen", ",", "wir", "ha\u00b7ben", "schwer", "ge\u00b7tr\u00e4umt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "VVPP", "$,", "PPER", "VAFIN", "ADJD", "VVPP", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "O Tag der Auferstehung, wie lang' du hast ges\u00e4umt!", "tokens": ["O", "Tag", "der", "Auf\u00b7er\u00b7ste\u00b7hung", ",", "wie", "lang'", "du", "hast", "ge\u00b7s\u00e4umt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,", "PWAV", "ADV", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wir haben schwer getr\u00e4umet von Joch und Kett' und Band;", "tokens": ["Wir", "ha\u00b7ben", "schwer", "ge\u00b7tr\u00e4u\u00b7met", "von", "Joch", "und", "Kett'", "und", "Band", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "VVPP", "APPR", "NE", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Da haben unsre Wunden uns bis in's Herz gebrannt.", "tokens": ["Da", "ha\u00b7ben", "uns\u00b7re", "Wun\u00b7den", "uns", "bis", "in's", "Herz", "ge\u00b7brannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "PPER", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Wir sahn die Burgen fallen, die Tempel untergehn,", "tokens": ["Wir", "sahn", "die", "Bur\u00b7gen", "fal\u00b7len", ",", "die", "Tem\u00b7pel", "un\u00b7ter\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Wir sahen fremde Fahnen auf ihren Tr\u00fcmmern wehn;", "tokens": ["Wir", "sa\u00b7hen", "frem\u00b7de", "Fah\u00b7nen", "auf", "ih\u00b7ren", "Tr\u00fcm\u00b7mern", "wehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Barbarentritt zerstampfte den Rasen unsrer Gruft,", "tokens": ["Bar\u00b7ba\u00b7ren\u00b7tritt", "zer\u00b7stampf\u00b7te", "den", "Ra\u00b7sen", "uns\u00b7rer", "Gruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.8": {"text": "Die Kl\u00e4nge unsrer Sprache verhallten in die Luft;", "tokens": ["Die", "Kl\u00e4n\u00b7ge", "uns\u00b7rer", "Spra\u00b7che", "ver\u00b7hall\u00b7ten", "in", "die", "Luft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Und was auf unsren H\u00fcgeln beschwur des J\u00fcnglings Herz,", "tokens": ["Und", "was", "auf", "un\u00b7sren", "H\u00fc\u00b7geln", "be\u00b7schwur", "des", "J\u00fcng\u00b7lings", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Was uns die Jungfrau klagte von ihrem hei\u00dfen Schmerz,", "tokens": ["Was", "uns", "die", "Jung\u00b7frau", "klag\u00b7te", "von", "ih\u00b7rem", "hei\u00b7\u00dfen", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Wir konnten's nicht verstehen \u2013 doch zu vernehmlich drang", "tokens": ["Wir", "konn\u00b7ten's", "nicht", "ver\u00b7ste\u00b7hen", "\u2013", "doch", "zu", "ver\u00b7nehm\u00b7lich", "drang"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$(", "ADV", "PTKA", "ADJD", "VVFIN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Durch unsre Erdendecke der Sklavenketten Klang.", "tokens": ["Durch", "uns\u00b7re", "Er\u00b7den\u00b7de\u00b7cke", "der", "Skla\u00b7ven\u00b7ket\u00b7ten", "Klang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Heil uns! Es ist vor\u00fcber. Heil uns! Wir tr\u00e4umten nur:", "tokens": ["Heil", "uns", "!", "Es", "ist", "vor\u00b7\u00fc\u00b7ber", ".", "Heil", "uns", "!", "Wir", "tr\u00e4um\u00b7ten", "nur", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "PPER", "VAFIN", "ADV", 