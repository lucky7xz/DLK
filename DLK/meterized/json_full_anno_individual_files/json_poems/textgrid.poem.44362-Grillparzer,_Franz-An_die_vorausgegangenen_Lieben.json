{"textgrid.poem.44362": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "An die vorausgegangenen Lieben", "genre": "verse", "period": "N.A.", "pub_year": 1819, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seid ihr vorausgegangen,", "tokens": ["Seid", "ihr", "vor\u00b7aus\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Liebe Gef\u00e4hrten der Reise,", "tokens": ["Lie\u00b7be", "Ge\u00b7f\u00e4hr\u00b7ten", "der", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Wohnung mir zu bereiten,", "tokens": ["Woh\u00b7nung", "mir", "zu", "be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der noch im Staube des Wegs?", "tokens": ["Der", "noch", "im", "Stau\u00b7be", "des", "Wegs", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Sucht mir ein K\u00e4mmerchen, Liebe!", "tokens": ["Sucht", "mir", "ein", "K\u00e4m\u00b7mer\u00b7chen", ",", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Still und freundlich und klein,", "tokens": ["Still", "und", "freund\u00b7lich", "und", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "KON", "ADJD",