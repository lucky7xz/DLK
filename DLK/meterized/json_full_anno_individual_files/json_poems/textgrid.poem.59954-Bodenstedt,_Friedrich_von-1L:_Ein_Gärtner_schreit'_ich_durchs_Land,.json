{"textgrid.poem.59954": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein G\u00e4rtner schreit' ich durchs Land,", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein G\u00e4rtner schreit' ich durchs Land,", "tokens": ["Ein", "G\u00e4rt\u00b7ner", "schreit'", "ich", "durchs", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Die Blumen pflegend,", "tokens": ["Die", "Blu\u00b7men", "pfle\u00b7gend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Das Unkraut j\u00e4tend,", "tokens": ["Das", "Un\u00b7kraut", "j\u00e4\u00b7tend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Den Acker bereitend", "tokens": ["Den", "A\u00b7cker", "be\u00b7rei\u00b7tend"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Zur guten Empf\u00e4ngnis", "tokens": ["Zur", "gu\u00b7ten", "Emp\u00b7f\u00e4ng\u00b7nis"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Des Saatkorns der Weisheit.", "tokens": ["Des", "Saat\u00b7korns", "der", "Weis\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Befruchtende Wasser", "tokens": ["Be\u00b7fruch\u00b7ten\u00b7de", "Was\u00b7ser"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Durchrieseln die Felder", "tokens": ["Durc\u00b7hrie\u00b7seln", "die", "Fel\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Gemessenen Laufes,", "tokens": ["Ge\u00b7mes\u00b7se\u00b7nen", "Lau\u00b7fes", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Nutzbringend, bescheiden \u2013", "tokens": ["Nutz\u00b7brin\u00b7gend", ",", "be\u00b7schei\u00b7den", "\u2013"], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Derweilen der Springquell", "tokens": ["Der\u00b7wei\u00b7len", "der", "Spring\u00b7quell"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Aus marmornem Becken", "tokens": ["Aus", "mar\u00b7mor\u00b7nem", "Be\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Hochaufspringt und pl\u00e4tschert", "tokens": ["Hoch\u00b7auf\u00b7springt", "und", "pl\u00e4t\u00b7schert"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "In sprudelndem \u00dcbermut.", "tokens": ["In", "spru\u00b7deln\u00b7dem", "\u00dc\u00b7ber\u00b7mut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Das keuchende Zugtier,", "tokens": ["Das", "keu\u00b7chen\u00b7de", "Zug\u00b7tier", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Gepeitscht von dem F\u00fchrer,", "tokens": ["Ge\u00b7peitscht", "von", "dem", "F\u00fch\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Durchlockert den Boden,", "tokens": ["Durch\u00b7lo\u00b7ckert", "den", "Bo\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Kann nimmer genug tun \u2013", "tokens": ["Kann", "nim\u00b7mer", "ge\u00b7nug", "tun", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Derweilen die Nachtigall", "tokens": ["Der\u00b7wei\u00b7len", "die", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "S\u00fc\u00df fl\u00f6tend im Baum sitzt", "tokens": ["S\u00fc\u00df", "fl\u00f6\u00b7tend", "im", "Baum", "sitzt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "APPRART", "NN", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Und neckisch herablugt", "tokens": ["Und", "ne\u00b7ckisch", "her\u00b7ab\u00b7lugt"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Zur schmachtenden Rose.", "tokens": ["Zur", "schmach\u00b7ten\u00b7den", "Ro\u00b7se", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Das Gras wird zertreten,", "tokens": ["Das", "Gras", "wird", "zer\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Das saftig die Herde n\u00e4hrt,", "tokens": ["Das", "saf\u00b7tig", "die", "Her\u00b7de", "n\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und niemand beachtet", "tokens": ["Und", "nie\u00b7mand", "be\u00b7ach\u00b7tet"], "token_info": ["word", "word", "word"], "pos": ["KON", "PIS", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die heilenden Kr\u00e4uter,", "tokens": ["Die", "hei\u00b7len\u00b7den", "Kr\u00e4u\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Die wundert\u00e4tigen,", "tokens": ["Die", "wun\u00b7der\u00b7t\u00e4\u00b7ti\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "ADJA", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Verborgen im Grase \u2013", "tokens": ["Ver\u00b7bor\u00b7gen", "im", "Gra\u00b7se", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Derweilen der Efeu", "tokens": ["Der\u00b7wei\u00b7len", "der", "E\u00b7feu"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Sich stolz um den Baum rankt", "tokens": ["Sich", "stolz", "um", "den", "Baum", "rankt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "ADJD", "APPR", "ART", "NN", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Und die Blumen prangen", "tokens": ["Und", "die", "Blu\u00b7men", "pran\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.10": {"text": "In lieblichem Dufte", "tokens": ["In", "lieb\u00b7li\u00b7chem", "Duf\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Und blendendem Farbenspiel.", "tokens": ["Und", "blen\u00b7den\u00b7dem", "Far\u00b7ben\u00b7spiel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "So ist es im Leben,", "tokens": ["So", "ist", "es", "im", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "So ist es im Liede.", "tokens": ["So", "ist", "es", "im", "Lie\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Denn der S\u00e4nger vermag nicht", "tokens": ["Denn", "der", "S\u00e4n\u00b7ger", "ver\u00b7mag", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKNEG"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Die Ordnung zu st\u00f6ren,", "tokens": ["Die", "Ord\u00b7nung", "zu", "st\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Die ewige Ordnung,", "tokens": ["Die", "e\u00b7wi\u00b7ge", "Ord\u00b7nung", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Der alles sich f\u00fcgen mu\u00df.", "tokens": ["Der", "al\u00b7les", "sich", "f\u00fc\u00b7gen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "PRF", "VVINF", "VMFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "La\u00df die Nachtigall singen", "tokens": ["La\u00df", "die", "Nach\u00b7ti\u00b7gall", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "VVINF"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Sie kann nicht den Pflug ziehn", "tokens": ["Sie", "kann", "nicht", "den", "Pflug", "ziehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "ART", "NN", "VVINF"], "meter": "+---+-", "measure": "dactylic.init"}, "line.3": {"text": "Und es hat kein Zugtier", "tokens": ["Und", "es", "hat", "kein", "Zug\u00b7tier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "PIAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Die Stimme der Nachtigall.", "tokens": ["Die", "Stim\u00b7me", "der", "Nach\u00b7ti\u00b7gall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "La\u00df prangen die Blumen", "tokens": ["La\u00df", "pran\u00b7gen", "die", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "In \u00fcppiger Sch\u00f6ne;", "tokens": ["In", "\u00fcp\u00b7pi\u00b7ger", "Sch\u00f6\u00b7ne", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Ihr Duft, ihre Wohlgestalt", "tokens": ["Ihr", "Duft", ",", "ih\u00b7re", "Wohl\u00b7ge\u00b7stalt"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sind uns zur Freude da.", "tokens": ["Sind", "uns", "zur", "Freu\u00b7de", "da", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Die Blumen zu pflegen,", "tokens": ["Die", "Blu\u00b7men", "zu", "pfle\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Das Unkraut zu tilgen,", "tokens": ["Das", "Un\u00b7kraut", "zu", "til\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Ist Sache des G\u00e4rtners.", "tokens": ["Ist", "Sa\u00b7che", "des", "G\u00e4rt\u00b7ners", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die Sorgen zu bannen", "tokens": ["Die", "Sor\u00b7gen", "zu", "ban\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PTKZU", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "(das Unkraut des Geistes),", "tokens": ["(", "das", "Un\u00b7kraut", "des", "Geis\u00b7tes", ")", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "ART", "NN", "$(", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Den Kummer zu scheuchen,", "tokens": ["Den", "Kum\u00b7mer", "zu", "scheu\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Die Schmerzen zu lindern,", "tokens": ["Die", "Schmer\u00b7zen", "zu", "lin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Ist Sache des S\u00e4ngers.", "tokens": ["Ist", "Sa\u00b7che", "des", "S\u00e4n\u00b7gers", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.9": {"line.1": {"text": "Der Garten liegt vor euch", "tokens": ["Der", "Gar\u00b7ten", "liegt", "vor", "euch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mit saftigen Reben", "tokens": ["Mit", "saf\u00b7ti\u00b7gen", "Re\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und rankendem Efeu.", "tokens": ["Und", "ran\u00b7ken\u00b7dem", "E\u00b7feu", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mit klingenden Zweigen", "tokens": ["Mit", "klin\u00b7gen\u00b7den", "Zwei\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Und pl\u00e4tscherndem Springquell.", "tokens": ["Und", "pl\u00e4t\u00b7schern\u00b7dem", "Spring\u00b7quell", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Mit heilenden Kr\u00e4utern", "tokens": ["Mit", "hei\u00b7len\u00b7den", "Kr\u00e4u\u00b7tern"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Im schwellenden Grase,", "tokens": ["Im", "schwel\u00b7len\u00b7den", "Gra\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Schwarz\u00e4ugigen M\u00e4dchen", "tokens": ["Schwarz\u00b7\u00e4u\u00b7gi\u00b7gen", "M\u00e4d\u00b7chen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "In bl\u00fchenden Lauben,", "tokens": ["In", "bl\u00fc\u00b7hen\u00b7den", "Lau\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Mit Blumen und Fr\u00fcchten.", "tokens": ["Mit", "Blu\u00b7men", "und", "Fr\u00fcch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.10": {"line.1": {"text": "Erquickt euch daran", "tokens": ["Er\u00b7quickt", "euch", "da\u00b7ran"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "PAV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Nach den M\u00fchen des Tages;", "tokens": ["Nach", "den", "M\u00fc\u00b7hen", "des", "Ta\u00b7ges", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Genie\u00dfet das eine", "tokens": ["Ge\u00b7nie\u00b7\u00dfet", "das", "ei\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "ART"], "meter": 