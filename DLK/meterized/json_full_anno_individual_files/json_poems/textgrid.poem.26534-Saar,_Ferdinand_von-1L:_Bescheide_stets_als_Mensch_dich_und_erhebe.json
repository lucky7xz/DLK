{"textgrid.poem.26534": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bescheide stets als Mensch dich und erhebe", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bescheide stets als Mensch dich und erhebe", "tokens": ["Be\u00b7schei\u00b7de", "stets", "als", "Mensch", "dich", "und", "er\u00b7he\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "KOUS", "NN", "PPER", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Kunst nicht h\u00f6her als sie mag verdienen;", "tokens": ["Die", "Kunst", "nicht", "h\u00f6\u00b7her", "als", "sie", "mag", "ver\u00b7die\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADJD", "KOKOM", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie gro\u00df und einzig sie dir stets erschienen:", "tokens": ["Wie", "gro\u00df", "und", "ein\u00b7zig", "sie", "dir", "stets", "er\u00b7schie\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So Manches um dich her h\u00e4lt ihr die Schwebe.", "tokens": ["So", "Man\u00b7ches", "um", "dich", "her", "h\u00e4lt", "ihr", "die", "Schwe\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "APPR", "PRF", "APZR", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Welch' stolzes Hochgef\u00fchl dich auch durchbebe,", "tokens": ["Welch'", "s