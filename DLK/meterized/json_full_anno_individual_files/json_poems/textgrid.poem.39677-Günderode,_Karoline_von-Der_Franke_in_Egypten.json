{"textgrid.poem.39677": {"metadata": {"author": {"name": "G\u00fcnderode, Karoline von", "birth": "N.A.", "death": "N.A."}, "title": "Der Franke in Egypten", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie der Unmuth mir den Busen dr\u00fccket,", "tokens": ["Wie", "der", "Un\u00b7muth", "mir", "den", "Bu\u00b7sen", "dr\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wie das Gl\u00fcck mich h\u00e4misch l\u00e4chelnd flieht.", "tokens": ["Wie", "das", "Gl\u00fcck", "mich", "h\u00e4\u00b7misch", "l\u00e4\u00b7chelnd", "flieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "ADJD", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ist denn Nichts was meine Seele stillet?", "tokens": ["Ist", "denn", "Nichts", "was", "mei\u00b7ne", "See\u00b7le", "stil\u00b7let", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIS", "PWS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Nichts, was dieses Lebens bange Leere f\u00fcllet? \u2013", "tokens": ["Nichts", ",", "was", "die\u00b7ses", "Le\u00b7bens", "ban\u00b7ge", "Lee\u00b7re", "f\u00fcl\u00b7let", "?", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIS", "$,", "PWS", "PDAT", "NN", "ADV", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Dieses Sehnen, w\u00e4hnt' ich, sucht die Vorwelt,", "tokens": ["Die\u00b7ses", "Seh\u00b7nen", ",", "w\u00e4hnt'", "ich", ",", "sucht", "die", "Vor\u00b7welt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "VVFIN", "PPER", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Die Heroenzeit ersehnt mein kranker Geist.", "tokens": ["Die", "He\u00b7ro\u00b7en\u00b7zeit", "er\u00b7sehnt", "mein", "kran\u00b7ker", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "An vergang'ner Gr\u00f6\u00dfe will dies Herz sich heben,", "tokens": ["An", "ver\u00b7gang'\u00b7ner", "Gr\u00f6\u00b7\u00dfe", "will", "dies", "Herz", "sich", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "PDS", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.8": {"text": "Und so eilt' ich deinem Strande zu,", "tokens": ["Und", "so", "eilt'", "ich", "dei\u00b7nem", "Stran\u00b7de", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Du der Vorwelt heiligste Ruine,", "tokens": ["Du", "der", "Vor\u00b7welt", "hei\u00b7ligs\u00b7te", "Ru\u00b7i\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "Fabelhaftes Land, Egypten du!", "tokens": ["Fa\u00b7bel\u00b7haf\u00b7tes", "Land", ",", "E\u00b7gyp\u00b7ten", "du", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NE", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Ha! da w\u00e4hnt' ich aller Lasten mich entladen", "tokens": ["Ha", "!", "da", "w\u00e4hnt'", "ich", "al\u00b7ler", "Las\u00b7ten", "mich", "ent\u00b7la\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "PPER", "PIAT", "NN", "PPER", "VVPP"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.12": {"text": "Als der Heimath Gr\u00e4nze ich einteilet war.", "tokens": ["Als", "der", "Hei\u00b7math", "Gr\u00e4n\u00b7ze", "ich", "ein\u00b7tei\u00b7let", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.13": {"text": "Tr\u00e4umend wallt' ich mit der Vorzeit Schatten,", "tokens": ["Tr\u00e4u\u00b7mend", "wallt'", "ich", "mit", "der", "Vor\u00b7zeit", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Doch bald f\u00fchlt' ich, da\u00df ich unter Todten sey,", "tokens": ["Doch", "bald", "f\u00fchlt'", "ich", ",", "da\u00df", "ich", "un\u00b7ter", "Tod\u00b7ten", "sey", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "APPR", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.15": {"text": "Neu bewegte sich in mir das Leben,", "tokens": ["Neu", "be\u00b7weg\u00b7te", "sich", "in", "mir", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "APPR", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Antwort konnte mir das Grab nicht geben. \u2013", "tokens": ["Ant\u00b7wort", "konn\u00b7te", "mir", "das", "Grab", "nicht", "ge\u00b7ben", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VMFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Ins Gew\u00fchl der Schlachten,", "tokens": ["Ins", "Ge\u00b7w\u00fchl", "der", "Schlach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.18": {"text": "Warf ich durstig mich,", "tokens": ["Warf", "ich", "durs\u00b7tig", "mich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "PPER", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.19": {"text": "Aber Ruhm und Schlachten,", "tokens": ["A\u00b7ber", "Ruhm", "und", "Schlach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.20": {"text": "Lie\u00dfen traurig mich:", "tokens": ["Lie\u00b7\u00dfen", "trau\u00b7rig", "mich", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.21": {"text": "Der Lorbeer der die Stirne schm\u00fcckt,", "tokens": ["Der", "Lor\u00b7beer", "der", "die", "Stir\u00b7ne", "schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Er ists nicht immer der begl\u00fcckt.", "tokens": ["Er", "ists", "nicht", "im\u00b7mer", "der", "be\u00b7gl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "ART", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Da reichte mir die Wissenschaft die Hand,", "tokens": ["Da", "reich\u00b7te", "mir", "die", "Wis\u00b7sen\u00b7schaft", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Und folgsam gieng ich nun an ihrer Seite,", "tokens": ["Und", "folg\u00b7sam", "gieng", "ich", "nun", "an", "ih\u00b7rer", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "Ich stieg hinab in Pyramiden Nacht,", "tokens": ["Ich", "stieg", "hin\u00b7ab", "in", "Py\u00b7ra\u00b7mi\u00b7den", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Ich mas des M\u00f6ris See, des alten Memphis Gr\u00f6\u00dfe,", "tokens": ["Ich", "mas", "des", "M\u00f6\u00b7ris", "See", ",", "des", "al\u00b7ten", "Mem\u00b7phis", "Gr\u00f6\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "ART", "NN", "NN", "$,", "ART", "ADJA", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und all die Herrlichkeit, die sonst mein Herz geschwellt,", "tokens": ["Und", "all", "die", "Herr\u00b7lich\u00b7keit", ",", "die", "sonst", "mein", "Herz", "ge\u00b7schwellt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ART", "NN", "$,", "PRELS", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Sie reicht dem Durstigen nur der Erkenntni\u00df Becher.", "tokens": ["Sie", "reicht", "dem", "Durs\u00b7ti\u00b7gen", "nur", "der", "Er\u00b7kennt\u00b7ni\u00df", "Be\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ich dachte, forschte nur, verga\u00df da\u00df ich empfand. \u2013", "tokens": ["Ich", "dach\u00b7te", ",", "forschte", "nur", ",", "ver\u00b7ga\u00df", "da\u00df", "ich", "emp\u00b7fand", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "ADV", "$,", "VVFIN", "KOUS", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "Doch ach! die alte Sehnsucht ist erwacht,", "tokens": ["Doch", "ach", "!", "die", "al\u00b7te", "Sehn\u00b7sucht", "ist", "er\u00b7wacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Aufs neue f\u00fchl ich suchend ihre Macht,", "tokens": ["Aufs", "neu\u00b7e", "f\u00fchl", "ich", "su\u00b7chend", "ih\u00b7re", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Was geb ich ihr? Wohin soll ich mich st\u00fcrzen?", "tokens": ["Was", "geb", "ich", "ihr", "?", "Wo\u00b7hin", "soll", "ich", "mich", "st\u00fcr\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "$.", "PWAV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Was wird des Lebens lange Oede w\u00fcrzen?", "tokens": ["Was", "wird", "des", "Le\u00b7bens", "lan\u00b7ge", "O\u00b7e\u00b7de", "w\u00fcr\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "Ha! Sieh, ein M\u00e4dchen! wie voll Anmuth,", "tokens": ["Ha", "!", "Sieh", ",", "ein", "M\u00e4d\u00b7chen", "!", "wie", "voll", "An\u00b7muth", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "NE", "$,", "ART", "NN", "$.", "PWAV", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Wie lieblich hold erscheint sie mir!", "tokens": ["Wie", "lieb\u00b7lich", "hold", "er\u00b7scheint", "sie", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADJD", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Soll ich dem Zuge widerstehen?", "tokens": ["Soll", "ich", "dem", "Zu\u00b7ge", "wi\u00b7der\u00b7ste\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Doch nein! ich rede k\u00fchn zu ihr.", "tokens": ["Doch", "nein", "!", "ich", "re\u00b7de", "k\u00fchn", "zu", "ihr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$.", "PPER", "VVFIN", "ADJD", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Ist dies der Weg der Pyramiden?", "tokens": ["Ist", "dies", "der", "Weg", "der", "Py\u00b7ra\u00b7mi\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "O, sch\u00f6nes M\u00e4dchen! sag es mir!", "tokens": ["O", ",", "sch\u00f6\u00b7nes", "M\u00e4d\u00b7chen", "!", "sag", "es", "mir", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$.", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie der Unmuth mir den Busen dr\u00fccket,", "tokens": ["Wie", "der", "Un\u00b7muth", "mir", "den", "Bu\u00b7sen", "dr\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wie das Gl\u00fcck mich h\u00e4misch l\u00e4chelnd flieht.", "tokens": ["Wie", "das", "Gl\u00fcck", "mich", "h\u00e4\u00b7misch", "l\u00e4\u00b7chelnd", "flieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "ADJD", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ist denn Nichts was meine Seele stillet?", "tokens": ["Ist", "denn", "Nichts", "was", "mei\u00b7ne", "See\u00b7le", "stil\u00b7let", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIS", "PWS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Nichts, was dieses Lebens bange Leere f\u00fcllet? \u2013", "tokens": ["Nichts", ",", "was", "die\u00b7