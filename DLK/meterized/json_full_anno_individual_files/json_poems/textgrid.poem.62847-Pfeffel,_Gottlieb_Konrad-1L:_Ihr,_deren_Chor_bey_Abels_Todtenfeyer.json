, "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "PPOSAT", "NN", "$.", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aufs Wiedersehn wischt sie nicht ab;", "tokens": ["Aufs", "Wie\u00b7der\u00b7sehn", "wischt", "sie", "nicht", "ab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn ach, das B\u00e4umchen, das der Strahl getroffen,", "tokens": ["Denn", "ach", ",", "das", "B\u00e4um\u00b7chen", ",", "das", "der", "Strahl", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "War eines blinden Vaters Stab.", "tokens": ["War", "ei\u00b7nes", "blin\u00b7den", "Va\u00b7ters", "Stab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}