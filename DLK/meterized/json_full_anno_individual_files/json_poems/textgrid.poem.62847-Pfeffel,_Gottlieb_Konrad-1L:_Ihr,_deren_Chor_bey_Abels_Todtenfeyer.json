{"textgrid.poem.62847": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr, deren Chor bey Abels Todtenfeyer", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr, deren Chor bey Abels Todtenfeyer", "tokens": ["Ihr", ",", "de\u00b7ren", "Chor", "bey", "A\u00b7bels", "Tod\u00b7ten\u00b7fe\u00b7yer"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELAT", "NN", "APPR", "NN", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Die ersten Elegien sang,", "tokens": ["Die", "ers\u00b7ten", "E\u00b7le\u00b7gi\u00b7en", "sang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "L\u00f6st, Engel Gottes, mir den Augenschleyer", "tokens": ["L\u00f6st", ",", "En\u00b7gel", "Got\u00b7tes", ",", "mir", "den", "Au\u00b7gen\u00b7schle\u00b7yer"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "NE", "NN", "$,", "PPER", "ART", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Und stimmet meiner Harfe Klang.", "tokens": ["Und", "stim\u00b7met", "mei\u00b7ner", "Har\u00b7fe", "Klang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Geleitet mich zu jenem frohen H\u00fcgel,", "tokens": ["Ge\u00b7lei\u00b7tet", "mich", "zu", "je\u00b7nem", "fro\u00b7hen", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den eine Rosenkrone schm\u00fcckt.", "tokens": ["Den", "ei\u00b7ne", "Ro\u00b7sen\u00b7kro\u00b7ne", "schm\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das eiserne, verh\u00e4ngni\u00dfvolle Siegel", "tokens": ["Das", "ei\u00b7ser\u00b7ne", ",", "ver\u00b7h\u00e4ng\u00b7ni\u00df\u00b7vol\u00b7le", "Sie\u00b7gel"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Des Todes ist ihm aufgedr\u00fcckt.", "tokens": ["Des", "To\u00b7des", "ist", "ihm", "auf\u00b7ge\u00b7dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O tretet sanft, denn einer eurer Br\u00fcder", "tokens": ["O", "tre\u00b7tet", "sanft", ",", "denn", "ei\u00b7ner", "eu\u00b7rer", "Br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADJD", "$,", "KON", "ART", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schl\u00e4ft unter diesem frischen Moos.", "tokens": ["Schl\u00e4ft", "un\u00b7ter", "die\u00b7sem", "fri\u00b7schen", "Moos", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch nein, er\u00f6fnet seine Gruft mir wieder", "tokens": ["Doch", "nein", ",", "er\u00b7\u00f6f\u00b7net", "sei\u00b7ne", "Gruft", "mir", "wie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKANT", "$,", "VVFIN", "PPOSAT", "NN", "PPER", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und hebt ihn aus der Erde Schoo\u00df.", "tokens": ["Und", "hebt", "ihn", "aus", "der", "Er\u00b7de", "Schoo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie thut sich auf; der Sarg entsteigt, vom Schimmer", "tokens": ["Sie", "thut", "sich", "auf", ";", "der", "Sarg", "ent\u00b7steigt", ",", "vom", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "PTKVZ", "$.", "ART", "NN", "VVPP", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Monds bestrahlt, der Dunkelheit.", "tokens": ["Des", "Monds", "be\u00b7strahlt", ",", "der", "Dun\u00b7kel\u00b7heit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach! und mein Aug erkennt die sch\u00f6nen Tr\u00fcmmer", "tokens": ["Ach", "!", "und", "mein", "Aug", "er\u00b7kennt", "die", "sch\u00f6\u00b7nen", "Tr\u00fcm\u00b7mer"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "KON", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von seines Geistes Pilgerkleid.", "tokens": ["Von", "sei\u00b7nes", "Geis\u00b7tes", "Pil\u00b7ger\u00b7kleid", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "O la\u00df mich, la\u00df mich ihn noch einmal k\u00fcssen,", "tokens": ["O", "la\u00df", "mich", ",", "la\u00df", "mich", "ihn", "noch", "ein\u00b7mal", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "VVIMP", "PPER", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Allw\u00fcrger, deinen fr\u00fchen Raub.", "tokens": ["All\u00b7w\u00fcr\u00b7ger", ",", "dei\u00b7nen", "fr\u00fc\u00b7hen", "Raub", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Sunim, ach mein Kind, warum begie\u00dfen", "tokens": ["Mein", "Su\u00b7nim", ",", "ach", "mein", "Kind", ",", "wa\u00b7rum", "be\u00b7gie\u00b7\u00dfen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "XY", "PPOSAT", "NN", "$,", "PWAV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nicht deine Thr\u00e4nen meinen Staub!", "tokens": ["Nicht", "dei\u00b7ne", "Thr\u00e4\u00b7nen", "mei\u00b7nen", "Staub", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wo ist der Geist, der diesen Schutt bewohnte?", "tokens": ["Wo", "ist", "der", "Geist", ",", "der", "die\u00b7sen", "Schutt", "be\u00b7wohn\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$,", "PRELS", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er, der in seinem Raupenstand,", "tokens": ["Er", ",", "der", "in", "sei\u00b7nem", "Rau\u00b7pen\u00b7stand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem jungen Lenze gleich, auf Rosen thronte", "tokens": ["Dem", "jun\u00b7gen", "Len\u00b7ze", "gleich", ",", "auf", "Ro\u00b7sen", "thron\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "$,", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wie der junge Lenz verschwand?", "tokens": ["Und", "wie", "der", "jun\u00b7ge", "Lenz", "ver\u00b7schwand", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wo ist das Herz, das an des Meinen Seite", "tokens": ["Wo", "ist", "das", "Herz", ",", "das", "an", "des", "Mei\u00b7nen", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Noch j\u00fcngst so laut, so feurig schlug,", "tokens": ["Noch", "j\u00fcngst", "so", "laut", ",", "so", "feu\u00b7rig", "schlug", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADJD", "$,", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als ich zum Freunde mir den Knaben weihte,", "tokens": ["Als", "ich", "zum", "Freun\u00b7de", "mir", "den", "Kna\u00b7ben", "weih\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der schon den Tod im Busen trug?", "tokens": ["Der", "schon", "den", "Tod", "im", "Bu\u00b7sen", "trug", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Sein Geist entfloh, zu gro\u00df f\u00fcr seine Zelle;", "tokens": ["Sein", "Geist", "ent\u00b7floh", ",", "zu", "gro\u00df", "f\u00fcr", "sei\u00b7ne", "Zel\u00b7le", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PTKA", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sein Herz, f\u00fcr diese Welt zu rein,", "tokens": ["Sein", "Herz", ",", "f\u00fcr", "die\u00b7se", "Welt", "zu", "rein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "PDAT", "NN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verwelkte; doch der Liebe heil'ge Quelle", "tokens": ["Ver\u00b7welk\u00b7te", ";", "doch", "der", "Lie\u00b7be", "heil'\u00b7ge", "Quel\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schlie\u00dft kein Gef\u00e4\u00df von Muskeln ein.", "tokens": ["Schlie\u00dft", "kein", "Ge\u00b7f\u00e4\u00df", "von", "Mus\u00b7keln", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Er lebt, er liebet fort und seiner Jugend", "tokens": ["Er", "lebt", ",", "er", "lie\u00b7bet", "fort", "und", "sei\u00b7ner", "Ju\u00b7gend"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PTKVZ", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Reicht Gott des Mannes Palme dar,", "tokens": ["Reicht", "Gott", "des", "Man\u00b7nes", "Pal\u00b7me", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihm, dessen letzte That noch eine Tugend,", "tokens": ["Ihm", ",", "des\u00b7sen", "letz\u00b7te", "That", "noch", "ei\u00b7ne", "Tu\u00b7gend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELAT", "ADJA", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schon eines Engels w\u00fcrdig war.", "tokens": ["Schon", "ei\u00b7nes", "En\u00b7gels", "w\u00fcr\u00b7dig", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Du, der du sanft die holde Blume k\u00fc\u00dftest,", "tokens": ["Du", ",", "der", "du", "sanft", "die", "hol\u00b7de", "Blu\u00b7me", "k\u00fc\u00df\u00b7test", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als sie an meiner Brust verdarb;", "tokens": ["Als", "sie", "an", "mei\u00b7ner", "Brust", "ver\u00b7darb", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sprich, du sein Schutzgeist, wenn du sterben m\u00fc\u00dftest,", "tokens": ["Sprich", ",", "du", "sein", "Schutz\u00b7geist", ",", "wenn", "du", "ster\u00b7ben", "m\u00fc\u00df\u00b7test", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PPER", "PPOSAT", "NN", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "St\u00fcrbst du nicht wie mein Sunim starb?", "tokens": ["St\u00fcrbst", "du", "nicht", "wie", "mein", "Su\u00b7nim", "starb", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "KOKOM", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.11": {"line.1": {"text": "Ach und ich lebe; doch versiegt ihr Z\u00e4hren,", "tokens": ["Ach", "und", "ich", "le\u00b7be", ";", "doch", "ver\u00b7siegt", "ihr", "Z\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KON", "PPER", "VVFIN", "$.", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Versiegt; er k\u00f6nnte meinen Schmerz", "tokens": ["Ver\u00b7siegt", ";", "er", "k\u00f6nn\u00b7te", "mei\u00b7nen", "Schmerz"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$.", "PPER", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vom Himmel sehn und meine Thr\u00e4nen w\u00e4ren", "tokens": ["Vom", "Him\u00b7mel", "sehn", "und", "mei\u00b7ne", "Thr\u00e4\u00b7nen", "w\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVINF", "KON", "PPOSAT", "NN", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auch dort noch Dolche f\u00fcr sein Herz.", "tokens": ["Auch", "dort", "noch", "Dol\u00b7che", "f\u00fcr", "sein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "F\u00fchrt mich zur\u00fcck, ihr Engel! selbst das Hoffen", "tokens": ["F\u00fchrt", "mich", "zu\u00b7r\u00fcck", ",", "ihr", "En\u00b7gel", "!", "selbst", "das", "Hof\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "PPOSAT", "NN", "$.", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aufs Wiedersehn wischt sie nicht ab;", "tokens": ["Aufs", "Wie\u00b7der\u00b7sehn", "wischt", "sie", "nicht", "ab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn ach, das B\u00e4umchen, das der Strahl getroffen,", "tokens": ["Denn", "ach", ",", "das", "B\u00e4um\u00b7chen", ",", "das", "der", "Strahl", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "War eines blinden Vaters Stab.", "tokens": ["War", "ei\u00b7nes", "blin\u00b7den", "Va\u00b7ters", "Stab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Ihr, deren Chor bey Abels Todtenfeyer", "tokens": ["Ihr", ",", "de\u00b7ren", "Chor", "bey", "A\u00b7bels", "Tod\u00b7ten\u00b7fe\u00b7yer"], "token_info": ["word", "punct", "word", "wo