{"dta.poem.1162": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "48. Die Sch\u00f6ne Lange-Geliebte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Mein erster Hertzensbrand/ ob zwar der Jahre Schatten ", "tokens": ["Mein", "ers\u00b7ter", "Hert\u00b7zens\u00b7brand", "/", "ob", "zwar", "der", "Jah\u00b7re", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "KOUS", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den vormahls hellen Glantz der Wangen Feld bedeckt/", "tokens": ["Den", "vor\u00b7mahls", "hel\u00b7len", "Glantz", "der", "Wan\u00b7gen", "Feld", "be\u00b7deckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "ART", "NN", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ist doch das alte Blutt bey mir gantz unersteckt/", "tokens": ["Ist", "doch", "das", "al\u00b7te", "Blutt", "bey", "mir", "gantz", "un\u00b7er\u00b7steckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "APPR", "PPER", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich dencke noch mit Lust der alten Liebes-Thaten.", "tokens": ["Ich", "den\u00b7cke", "noch", "mit", "Lust", "der", "al\u00b7ten", "Lie\u00b7bes\u00b7Tha\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ist meinem tr\u00fcben Geist ein k\u00fchner Flug gerathen/", "tokens": ["Ist", "mei\u00b7nem", "tr\u00fc\u00b7ben", "Geist", "ein", "k\u00fch\u00b7ner", "Flug", "ge\u00b7ra\u00b7then", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Hat er zu Pindus Haubt die Federn ausgestreckt/", "tokens": ["Hat", "er", "zu", "Pin\u00b7dus", "Haubt", "die", "Fe\u00b7dern", "aus\u00b7ge\u00b7streckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NE", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So hat dein muntres Licht mein schl\u00e4ffrig Hertz erweckt/", "tokens": ["So", "hat", "dein", "mun\u00b7tres", "Licht", "mein", "schl\u00e4f\u00b7frig", "Hertz", "er\u00b7weckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "PPOSAT", "ADJD", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und meiner K\u00e4lte kam dein Feuer wohl zu statten.", "tokens": ["Und", "mei\u00b7ner", "K\u00e4l\u00b7te", "kam", "dein", "Feu\u00b7er", "wohl", "zu", "stat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Bey Fall und Grau\u00df beh\u00e4lt der Marmor seinen Prei\u00df:", "tokens": ["Bey", "Fall", "und", "Grau\u00df", "be\u00b7h\u00e4lt", "der", "Mar\u00b7mor", "sei\u00b7nen", "Prei\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Was erst ins Hertze kam/ wird nie daraus versehwinden:", "tokens": ["Was", "erst", "ins", "Hert\u00b7ze", "kam", "/", "wird", "nie", "da\u00b7raus", "ver\u00b7seh\u00b7win\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPRART", "VVFIN", "VVFIN", "$(", "VAFIN", "ADV", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Das Blutt gefrieret nicht/ wenn gleich das Haar bereifft.", "tokens": ["Das", "Blutt", "ge\u00b7frie\u00b7ret", "nicht", "/", "wenn", "gleich", "das", "Haar", "be\u00b7re\u00b7ifft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVFIN", "PTKNEG", "$(", "KOUS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.12": {"text": "Die Wunde bleibt/ ist gleich der Bogen nicht gesteifft.", "tokens": ["Die", "Wun\u00b7de", "bleibt", "/", "ist", "gleich", "der", "Bo\u00b7gen", "nicht", "ge\u00b7steifft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "VAFIN", "ADV", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Verh\u00e4ngnis/ das uns bindt/ kan keine Trennung finden;", "tokens": ["Ver\u00b7h\u00e4ng\u00b7nis", "/", "das", "uns", "bindt", "/", "kan", "kei\u00b7ne", "Tren\u00b7nung", "fin\u00b7den", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PRELS", "PPER", "VVFIN", "$(", "VMFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}}}}}