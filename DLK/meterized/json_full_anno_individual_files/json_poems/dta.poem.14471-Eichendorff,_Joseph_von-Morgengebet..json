{"dta.poem.14471": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Morgengebet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Die Welt mit ihrem Gram und Gl\u00fccke", "tokens": ["Die", "Welt", "mit", "ih\u00b7rem", "Gram", "und", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Will ich, ein Pilger frohbereit,", "tokens": ["Will", "ich", ",", "ein", "Pil\u00b7ger", "froh\u00b7be\u00b7reit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Betreten nur wie eine Br\u00fccke", "tokens": ["Be\u00b7tre\u00b7ten", "nur", "wie", "ei\u00b7ne", "Br\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu Dir, Herr, \u00fcber'n Strom der Zeit.", "tokens": ["Zu", "Dir", ",", "Herr", ",", "\u00fc\u00b7ber'n", "Strom", "der", "Zeit", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "NN", "$,", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}