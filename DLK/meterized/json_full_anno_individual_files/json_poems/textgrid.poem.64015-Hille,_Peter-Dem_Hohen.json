{"textgrid.poem.64015": {"metadata": {"author": {"name": "Hille, Peter", "birth": "N.A.", "death": "N.A."}, "title": "Dem Hohen", "genre": "verse", "period": "N.A.", "pub_year": 1879, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weltatmender, der du Geister,", "tokens": ["Welt\u00b7at\u00b7men\u00b7der", ",", "der", "du", "Geis\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Urfunken der Liebe,", "tokens": ["Ur\u00b7fun\u00b7ken", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Mit dem Brandmal der Geburt", "tokens": ["Mit", "dem", "Brand\u00b7mal", "der", "Ge\u00b7burt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In Leiber schlie\u00dfest", "tokens": ["In", "Lei\u00b7ber", "schlie\u00b7\u00dfest"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Und schleuderst fort den Schl\u00fcssel.", "tokens": ["Und", "schleu\u00b7derst", "fort", "den", "Schl\u00fcs\u00b7sel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und so finden sie sich", "tokens": ["Und", "so", "fin\u00b7den", "sie", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Und du f\u00fchlst sie", "tokens": ["Und", "du", "f\u00fchlst", "sie"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER"], "meter": "-+--", "measure": "dactylic.init"}, "line.8": {"text": "Und schwellend, voll brausender G\u00fcte,", "tokens": ["Und", "schwel\u00b7lend", ",", "voll", "brau\u00b7sen\u00b7der", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.9": {"text": "Zieht deiner Welle g\u00f6ttliche Flut", "tokens": ["Zieht", "dei\u00b7ner", "Wel\u00b7le", "g\u00f6tt\u00b7li\u00b7che", "Flut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJA", "NN"], "meter": "---+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "Schwellend entgegen", "tokens": ["Schwel\u00b7lend", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word"], "pos": ["ADJD", "PTKVZ"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Mit ganzer Seele dir Zud\u00fcrstendem ...", "tokens": ["Mit", "gan\u00b7zer", "See\u00b7le", "dir", "Zu\u00b7d\u00fcrs\u00b7ten\u00b7dem", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Oder sie verlieren die einsamen Schritte", "tokens": ["O\u00b7der", "sie", "ver\u00b7lie\u00b7ren", "die", "ein\u00b7sa\u00b7men", "Schrit\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "Und du wirst kalt mit ihnen", "tokens": ["Und", "du", "wirst", "kalt", "mit", "ih\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "APPR", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Kalt wie das, was nicht