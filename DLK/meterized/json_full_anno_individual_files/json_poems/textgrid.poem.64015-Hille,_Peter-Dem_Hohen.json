: "iambic.di"}, "line.21": {"text": "W\u00e4rmende Werke hegen", "tokens": ["W\u00e4r\u00b7men\u00b7de", "Wer\u00b7ke", "he\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.22": {"text": "Im quellenden Scho\u00dfe der Seele,", "tokens": ["Im", "quel\u00b7len\u00b7den", "Scho\u00b7\u00dfe", "der", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.23": {"text": "Das weckt dein Leben!", "tokens": ["Das", "weckt", "dein", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.24": {"text": "Gnadenquillend erwachend", "tokens": ["Gna\u00b7den\u00b7quil\u00b7lend", "er\u00b7wa\u00b7chend"], "token_info": ["word", "word"], "pos": ["ADJD", "VVPP"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.25": {"text": "St\u00fcrzt auf uns ein", "tokens": ["St\u00fcrzt", "auf", "uns", "ein"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "ART"], "meter": "-+-+", "measure": "iambic.di"}, "line.26": {"text": "H\u00f6henw\u00e4rts wirbelnder Segen.", "tokens": ["H\u00f6\u00b7hen\u00b7w\u00e4rts", "wir\u00b7beln\u00b7der", "Se\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.27": {"text": "Wie sollen wir z\u00e4hlen", "tokens": ["Wie", "sol\u00b7len", "wir", "z\u00e4h\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.28": {"text": "All deine ragenden Wipfel?", "tokens": ["All", "dei\u00b7ne", "ra\u00b7gen\u00b7den", "Wip\u00b7fel", "?"], "token_info": ["word", "word", "word", "