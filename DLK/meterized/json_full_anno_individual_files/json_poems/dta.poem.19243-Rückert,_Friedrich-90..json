{"dta.poem.19243": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "90.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Frosch im Laub versteht vom Wetter mehr als du,", "tokens": ["Der", "Frosch", "im", "Laub", "ver\u00b7steht", "vom", "Wet\u00b7ter", "mehr", "als", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "APPRART", "NN", "ADV", "KOUS", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und gift'ge Kr\u00e4uter kennt ehr als der Arzt die Kuh.", "tokens": ["Und", "gift'\u00b7ge", "Kr\u00e4u\u00b7ter", "kennt", "ehr", "als", "der", "Arzt", "die", "Kuh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ADJD", "KOKOM", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "In allem ist das Thier dem Menschen \u00fcberlegen,", "tokens": ["In", "al\u00b7lem", "ist", "das", "Thier", "dem", "Men\u00b7schen", "\u00fc\u00b7berl\u00b7e\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VAFIN", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was seiner Nothdurft dient auf dunklen Lebenswegen.", "tokens": ["Was", "sei\u00b7ner", "Noth\u00b7durft", "dient", "auf", "dunk\u00b7len", "Le\u00b7bens\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Des Menschen Augen sind darum im Einzlen blind,", "tokens": ["Des", "Men\u00b7schen", "Au\u00b7gen", "sind", "da\u00b7rum", "im", "Einz\u00b7len", "blind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PAV", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil offen sie allein dem Allgemeinen sind;", "tokens": ["Weil", "of\u00b7fen", "sie", "al\u00b7lein", "dem", "All\u00b7ge\u00b7mei\u00b7nen", "sind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "ADV", "ART", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Weil, was die Thierheit sp\u00fcrt mit eigenn\u00fctz'gem Triebe,", "tokens": ["Weil", ",", "was", "die", "Thier\u00b7heit", "sp\u00fcrt", "mit", "ei\u00b7gen\u00b7n\u00fctz'\u00b7gem", "Trie\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PRELS", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Menschheit forschet mit uneigenn\u00fctz'ger Liebe.", "tokens": ["Die", "Menschheit", "for\u00b7schet", "mit", "un\u00b7ei\u00b7gen\u00b7n\u00fctz'\u00b7ger", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Drum thuts ein dumpfer Sinn, verwandt mit thier'scher Zunft,", "tokens": ["Drum", "thuts", "ein", "dum\u00b7pfer", "Sinn", ",", "ver\u00b7wandt", "mit", "thier'\u00b7scher", "Zunft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NN", "$,", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Irdischen zuvor der g\u00f6ttlichsten Vernunft,", "tokens": ["Im", "Ir\u00b7di\u00b7schen", "zu\u00b7vor", "der", "g\u00f6tt\u00b7lichs\u00b7ten", "Ver\u00b7nunft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}}, "stanza.6": {"line.1": {"text": "Weil er nur seinem Zweck die Welt sucht zu bereiten,", "tokens": ["Weil", "er", "nur", "sei\u00b7nem", "Zweck", "die", "Welt", "sucht", "zu", "be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch sie mit Liebe hegt Weltangelegenheiten.", "tokens": ["Doch", "sie", "mit", "Lie\u00b7be", "hegt", "Wel\u00b7tan\u00b7ge\u00b7le\u00b7gen\u00b7hei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}}}}}