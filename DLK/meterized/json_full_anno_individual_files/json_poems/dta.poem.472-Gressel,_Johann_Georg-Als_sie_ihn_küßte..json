{"dta.poem.472": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als sie ihn k\u00fc\u00dfte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Gl\u00fccke bl\u00fcht recht sch\u00f6n/", "tokens": ["Mein", "Gl\u00fc\u00b7cke", "bl\u00fcht", "recht", "sch\u00f6n", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Weil die ", "tokens": ["Weil", "die"], "token_info": ["word", "word"], "pos": ["KOUS", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Mich ", "tokens": ["Mich"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Ach wunder-sch\u00f6ner Most/", "tokens": ["Ach", "wun\u00b7der\u00b7sch\u00f6\u00b7ner", "Most", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der vom Corall der zarten Lippen triefft/", "tokens": ["Der", "vom", "Co\u00b7rall", "der", "zar\u00b7ten", "Lip\u00b7pen", "triefft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Wie prangest du so sch\u00f6n in den Rubinern-Schalen.", "tokens": ["Wie", "pran\u00b7gest", "du", "so", "sch\u00f6n", "in", "den", "Ru\u00b7bi\u00b7nern\u00b7Scha\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Mein Kind ist dis der Liebe-Kost/", "tokens": ["Mein", "Kind", "ist", "dis", "der", "Lie\u00b7be\u00b7Kost", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PDS", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Oder s\u00fcsses Gifft/", "tokens": ["O\u00b7der", "s\u00fcs\u00b7ses", "Gifft", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Das sie mir jetzt zu bringet?", "tokens": ["Das", "sie", "mir", "jetzt", "zu", "brin\u00b7get", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PPER", "ADV", "PTKZU", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Doch still! ich f\u00fchle schon/ ach! ", "tokens": ["Doch", "still", "!", "ich", "f\u00fch\u00b7le", "schon", "/", "ach", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADJD", "$.", "PPER", "VVFIN", "ADV", "$(", "XY", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.11": {"text": "Da\u00df mir der Ku\u00df durch Blut und Seele dringet.", "tokens": ["Da\u00df", "mir", "der", "Ku\u00df", "durch", "Blut", "und", "See\u00b7le", "drin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}