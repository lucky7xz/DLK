{"dta.poem.9242": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "V.  \n Er will gerne eine Frau haben.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Sind das nicht unerh\u00f6rte straffen?", "tokens": ["Sind", "das", "nicht", "un\u00b7er\u00b7h\u00f6r\u00b7te", "straf\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "PTKNEG", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich bin so hurtig als ein mann/", "tokens": ["Ich", "bin", "so", "hur\u00b7tig", "als", "ein", "mann", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KOKOM", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und soll so lang alleine schlaffen/", "tokens": ["Und", "soll", "so", "lang", "al\u00b7lei\u00b7ne", "schlaf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ADJD", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bi\u00df ich ein weib ernehren kan:", "tokens": ["Bi\u00df", "ich", "ein", "weib", "er\u00b7neh\u00b7ren", "kan", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Indessen hab ich keine ruh/", "tokens": ["In\u00b7des\u00b7sen", "hab", "ich", "kei\u00b7ne", "ruh", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und fleisch und blut spricht nein darzu.", "tokens": ["Und", "fleisch", "und", "blut", "spricht", "nein", "dar\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "NN", "VVFIN", "PTKANT", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "2. Wem sol ich also folge leisten?", "tokens": ["Wem", "sol", "ich", "al\u00b7so", "fol\u00b7ge", "leis\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich bin f\u00fcrwar von holtze nicht/", "tokens": ["Ich", "bin", "f\u00fcr\u00b7war", "von", "holt\u00b7ze", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "VVFIN", "PTKNEG", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und f\u00fchle meine noth am meisten", "tokens": ["Und", "f\u00fch\u00b7le", "mei\u00b7ne", "noth", "am", "meis\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wann mich der lose kitzel sticht/", "tokens": ["Wann", "mich", "der", "lo\u00b7se", "kit\u00b7zel", "sticht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und aus den handel merck ich wohl/", "tokens": ["Und", "aus", "den", "han\u00b7del", "merck", "ich", "wohl", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df ich die welt vermehren soll.", "tokens": ["Da\u00df", "ich", "die", "welt", "ver\u00b7meh\u00b7ren", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "3. Was hilfft michs? da\u00df nun mit den jahren", "tokens": ["Was", "hilfft", "michs", "?", "da\u00df", "nun", "mit", "den", "jah\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PIS", "$.", "KOUS", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die kinder-schuh vertretten sind.", "tokens": ["Die", "kin\u00b7der\u00b7schuh", "ver\u00b7tret\u00b7ten", "sind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wann mir der schimpf soll wiederfahren", "tokens": ["Wann", "mir", "der", "schimpf", "soll", "wie\u00b7der\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "ADJD", "VMFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich mu\u00df leben als ein kind/", "tokens": ["Da\u00df", "ich", "mu\u00df", "le\u00b7ben", "als", "ein", "kind", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "VVINF", "KOKOM", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und da\u00df der zwang der einsamkeit", "tokens": ["Und", "da\u00df", "der", "zwang", "der", "ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mir alle freye lust verbeut.", "tokens": ["Mir", "al\u00b7le", "frey\u00b7e", "lust", "ver\u00b7beut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "4. Was seh ich doch vor sch\u00f6ne leute", "tokens": ["Was", "seh", "ich", "doch", "vor", "sch\u00f6\u00b7ne", "leu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bi\u00dfweilen auf der gassen stehn/", "tokens": ["Bi\u00df\u00b7wei\u00b7len", "auf", "der", "gas\u00b7sen", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die alle wolten lieber heute", "tokens": ["Die", "al\u00b7le", "wol\u00b7ten", "lie\u00b7ber", "heu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VMFIN", "ADV", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als morgen diese strasse gehn/", "tokens": ["Als", "mor\u00b7gen", "die\u00b7se", "stras\u00b7se", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch die gewonheit ist so scharff/", "tokens": ["Doch", "die", "ge\u00b7won\u00b7heit", "ist", "so", "scharff", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df niemand sich erkl\u00e4ren darff.", "tokens": ["Da\u00df", "nie\u00b7mand", "sich", "er\u00b7kl\u00e4\u00b7ren", "darff", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "5. Man soll der ehre noch erwarten/", "tokens": ["Man", "soll", "der", "eh\u00b7re", "noch", "er\u00b7war\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "VVFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch dieser trost der taug nicht viel/", "tokens": ["Doch", "die\u00b7ser", "trost", "der", "taug", "nicht", "viel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ART", "NN", "PTKNEG", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die pretzeln schimmeln und verharten/", "tokens": ["Die", "pret\u00b7zeln", "schim\u00b7meln", "und", "ver\u00b7har\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wann man sie frisch vers\u00e4umen will/", "tokens": ["Wann", "man", "sie", "frisch", "ver\u00b7s\u00e4u\u00b7men", "will", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "ADJD", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und der beliebten rosen-liecht", "tokens": ["Und", "der", "be\u00b7lieb\u00b7ten", "ro\u00b7sen\u00b7liecht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Scheint in den sp\u00e4ten winter nicht.", "tokens": ["Scheint", "in", "den", "sp\u00e4\u00b7ten", "win\u00b7ter", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "6. Ach freylich sind es lahme possen/", "tokens": ["Ach", "frey\u00b7lich", "sind", "es", "lah\u00b7me", "pos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VAFIN", "PPER", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Weil man sich noch ergetzen kan/", "tokens": ["Weil", "man", "sich", "noch", "er\u00b7get\u00b7zen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "ADV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So wird man allzeit ausgeschlossen/", "tokens": ["So", "wird", "man", "all\u00b7zeit", "aus\u00b7ge\u00b7schlos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hernachmahls geht der handel an/", "tokens": ["Her\u00b7nach\u00b7mahls", "geht", "der", "han\u00b7del", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn unsre l\u00e4mpgen auf das ziel", "tokens": ["Wenn", "uns\u00b7re", "l\u00e4mp\u00b7gen", "auf", "das", "ziel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gerathen und verleschen will.", "tokens": ["Ge\u00b7ra\u00b7then", "und", "ver\u00b7le\u00b7schen", "will", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "7. Indem ich dieses liedgen mache/", "tokens": ["In\u00b7dem", "ich", "die\u00b7ses", "lied\u00b7gen", "ma\u00b7che", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDAT", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So blick ich auf mein dinte-fa\u00df/", "tokens": ["So", "blick", "ich", "auf", "mein", "din\u00b7te\u00b7fa\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das schickt sich wohl zu dieser sache/", "tokens": ["Das", "schickt", "sich", "wohl", "zu", "die\u00b7ser", "sa\u00b7che", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADV", "APPR", "PDAT", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die dinte bleibt nicht immer na\u00df/", "tokens": ["Die", "din\u00b7te", "bleibt", "nicht", "im\u00b7mer", "na\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Man schreib/ und schreibe nicht daraus/", "tokens": ["Man", "schreib", "/", "und", "schrei\u00b7be", "nicht", "da\u00b7raus", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$(", "KON", "VVFIN", "PTKNEG", "PAV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So trocknet doch der boden aus.", "tokens": ["So", "trock\u00b7net", "doch", "der", "bo\u00b7den", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "8. Ach! d\u00fcrfft ich mich nur recht beklagen/", "tokens": ["Ach", "!", "d\u00fcrfft", "ich", "mich", "nur", "recht", "be\u00b7kla\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VMFIN", "PPER", "PRF", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So darff ich meine liebe noth", "tokens": ["So", "darff", "ich", "mei\u00b7ne", "lie\u00b7be", "noth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auch nicht dem besten freunde sagen/", "tokens": ["Auch", "nicht", "dem", "bes\u00b7ten", "freun\u00b7de", "sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und das ist \u00e4rger als der todt/", "tokens": ["Und", "das", "ist", "\u00e4r\u00b7ger", "als", "der", "todt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADJD", "KOKOM", "ART", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich warte/ weil mein leder h\u00e4lt/", "tokens": ["Ich", "war\u00b7te", "/", "weil", "mein", "le\u00b7der", "h\u00e4lt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPOSAT", "ADJA", "VVFIN", "$("], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Wo krieg ich dann mein warte-geld.", "tokens": ["Wo", "krieg", "ich", "dann", "mein", "war\u00b7te\u00b7geld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}