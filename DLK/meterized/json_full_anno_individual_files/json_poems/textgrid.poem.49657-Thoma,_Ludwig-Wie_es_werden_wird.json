{"textgrid.poem.49657": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Wie es werden wird", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So nach dem f\u00fcnften bis sechsten Gang,", "tokens": ["So", "nach", "dem", "f\u00fcnf\u00b7ten", "bis", "sechs\u00b7ten", "Gang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Da blickt der Pr\u00e4ses die Tafel entlang.", "tokens": ["Da", "blickt", "der", "Pr\u00e4\u00b7ses", "die", "Ta\u00b7fel", "ent\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Er stochert eifrig im hohlen Zahn.", "tokens": ["Er", "sto\u00b7chert", "eif\u00b7rig", "im", "hoh\u00b7len", "Zahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Jetzt geht's nicht l\u00e4nger, jetzt mu\u00df ich dran;", "tokens": ["Jetzt", "geht's", "nicht", "l\u00e4n\u00b7ger", ",", "jetzt", "mu\u00df", "ich", "dran", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "ADJD", "$,", "ADV", "VMFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Verlegen rutscht er am Sitze herum,", "tokens": ["Ver\u00b7le\u00b7gen", "rutscht", "er", "am", "Sit\u00b7ze", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word