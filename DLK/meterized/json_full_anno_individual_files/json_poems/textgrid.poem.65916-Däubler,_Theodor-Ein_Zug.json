{"textgrid.poem.65916": {"metadata": {"author": {"name": "D\u00e4ubler, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Ein Zug", "genre": "verse", "period": "N.A.", "pub_year": 1905, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sehr fromme Frauen ziehen fort zum Meere", "tokens": ["Sehr", "from\u00b7me", "Frau\u00b7en", "zie\u00b7hen", "fort", "zum", "Mee\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "PTKVZ", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u2013 Von Demeter ergriffen, weich geh\u00fcllt \u2013.", "tokens": ["\u2013", "Von", "De\u00b7me\u00b7ter", "er\u00b7grif\u00b7fen", ",", "weich", "ge\u00b7h\u00fcllt", "\u2013", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "NN", "VVPP", "$,", "ADJD", "VVPP", "$(", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Ein W\u00fcnschen, das Durchbangte lang erf\u00fcllt,", "tokens": ["Ein", "W\u00fcn\u00b7schen", ",", "das", "Durch\u00b7bang\u00b7te", "lang", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Beseligt sie, zu ihrer G\u00f6ttin Ehre,", "tokens": ["Be\u00b7se\u00b7ligt", "sie", ",", "zu", "ih\u00b7rer", "G\u00f6t\u00b7tin", "Eh\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Geweihtes Sagen, aus des Herzens Schwere,", "tokens": ["Ge\u00b7weih\u00b7tes", "Sa\u00b7gen", ",", "aus", "des", "Her\u00b7zens", "Schwe\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Kummer kr\u00fcmmt, sich im Gem\u00fct verkn\u00fcllt,", "tokens": ["Das", "Kum\u00b7mer", "kr\u00fcmmt", ",", "sich", "im", "Ge\u00b7m\u00fct", "ver\u00b7kn\u00fcllt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRF", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Emporzurufen, wo die Sturmsee br\u00fcllt:", "tokens": ["Em\u00b7por\u00b7zu\u00b7ru\u00b7fen", ",", "wo", "die", "Sturm\u00b7see", "br\u00fcllt", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und alter Hang zum Hades birgt die Lehre.", "tokens": ["Und", "al\u00b7ter", "Hang", "zum", "Ha\u00b7des", "birgt", "die", "Leh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sehr fromme Frauen schauen ihre Tiefe;", "tokens": ["Sehr", "from\u00b7me", "Frau\u00b7en", "schau\u00b7en", "ih\u00b7re", "Tie\u00b7fe", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Triebe welkten, flattern durch den Herbst,", "tokens": ["Die", "Trie\u00b7be", "welk\u00b7ten", ",", "flat\u00b7tern", "durch", "den", "Herbst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["AR