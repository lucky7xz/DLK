{"textgrid.poem.60225": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du junger Fr\u00fchling kommst herab", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du junger Fr\u00fchling kommst herab", "tokens": ["Du", "jun\u00b7ger", "Fr\u00fch\u00b7ling", "kommst", "her\u00b7ab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Sch\u00f6pfer, um ganz neues Leben", "tokens": ["Vom", "Sch\u00f6p\u00b7fer", ",", "um", "ganz", "neu\u00b7es", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "KOUI", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gesch\u00f6pfen seiner Hand zu geben.", "tokens": ["Ge\u00b7sch\u00f6p\u00b7fen", "sei\u00b7ner", "Hand", "zu", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Blumen-Volk verl\u00e4\u00dft sein Grab,", "tokens": ["Das", "Blu\u00b7men\u00b7Volk", "ver\u00b7l\u00e4\u00dft", "sein", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und mit empor gehobnem Haupte", "tokens": ["Und", "mit", "em\u00b7por", "ge\u00b7hob\u00b7nem", "Haup\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PTKVZ", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Besch\u00e4mt es den, der keinen Gott", "tokens": ["Be\u00b7sch\u00e4mt", "es", "den", ",", "der", "kei\u00b7nen", "Gott"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "$,", "PRELS", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und f\u00fcr sich selbst Vernichtung glaubte.", "tokens": ["Und", "f\u00fcr", "sich", "selbst", "Ver\u00b7nich\u00b7tung", "glaub\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PRF", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Vogel wiederspricht des Wiedersprechers Spott.", "tokens": ["Der", "Vo\u00b7gel", "wie\u00b7der\u00b7spricht", "des", "Wie\u00b7der\u00b7spre\u00b7chers", "Spott", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Saat mit Millionen Zungen", "tokens": ["Die", "Saat", "mit", "Mil\u00b7lion\u00b7en", "Zun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Aus schwarzer Erd herauf gedrungen", "tokens": ["Aus", "schwar\u00b7zer", "Erd", "her\u00b7auf", "ge\u00b7drun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Best\u00e4tiget, was er gesungen!", "tokens": ["Be\u00b7st\u00e4\u00b7ti\u00b7get", ",", "was", "er", "ge\u00b7sun\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Der Linde Bl\u00e4tter lispeln nach;", "tokens": ["Der", "Lin\u00b7de", "Bl\u00e4t\u00b7ter", "lis\u00b7peln", "nach", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Die Elbe rauscht und murmelnd spricht der Bach:", "tokens": ["Die", "El\u00b7be", "rauscht", "und", "mur\u00b7melnd", "spricht", "der", "Bach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "\u00bbes ist ein Gott, der laue Winde schickte,", "tokens": ["\u00bb", "es", "ist", "ein", "Gott", ",", "der", "lau\u00b7e", "Win\u00b7de", "schick\u00b7te", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "\u00bbden Schnee zerschmolz, das Eis zerbrach,", "tokens": ["\u00bb", "den", "Schnee", "zer\u00b7schmolz", ",", "das", "Eis", "zer\u00b7brach", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "ADJD", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "\u00bbmit jungem Gr\u00fcn das Ufer schm\u00fcckte", "tokens": ["\u00bb", "mit", "jun\u00b7gem", "Gr\u00fcn", "das", "U\u00b7fer", "schm\u00fcck\u00b7te"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "APPR", "ADJA", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "\u00bbund diese Sonne scheinen l\u00e4\u00dft!", "tokens": ["\u00bb", "und", "die\u00b7se", "Son\u00b7ne", "schei\u00b7nen", "l\u00e4\u00dft", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PDAT", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Nach sanft gefallnem Fr\u00fchlingsregen", "tokens": ["Nach", "sanft", "ge\u00b7fall\u00b7nem", "Fr\u00fch\u00b7lings\u00b7re\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Quackt der erweckte Frosch sein Fest,", "tokens": ["Quackt", "der", "er\u00b7weck\u00b7te", "Frosch", "sein", "Fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Und Fische scherzen ihr entgegen!", "tokens": ["Und", "Fi\u00b7sche", "scher\u00b7zen", "ihr", "ent\u00b7ge\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Der Hirt hei\u00dft seine Heerde leben!", "tokens": ["Der", "Hirt", "hei\u00dft", "sei\u00b7ne", "Heer\u00b7de", "le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Sie weidet jugendliches Gra\u00df,", "tokens": ["Sie", "wei\u00b7det", "ju\u00b7gend\u00b7li\u00b7ches", "Gra\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Bl\u00f6ckt ihre Freuden laut, und h\u00f6rt ohn Unterla\u00df", "tokens": ["Bl\u00f6ckt", "ih\u00b7re", "Freu\u00b7den", "laut", ",", "und", "h\u00f6rt", "ohn", "Un\u00b7ter\u00b7la\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "$,", "KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Sich Thal und H\u00fcgel Antwort geben!", "tokens": ["Sich", "Thal", "und", "H\u00fc\u00b7gel", "Ant\u00b7wort", "ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "KON", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Die Honigtr\u00e4gerin verl\u00e4\u00dft ihr kleines Haus", "tokens": ["Die", "Ho\u00b7nig\u00b7tr\u00e4\u00b7ge\u00b7rin", "ver\u00b7l\u00e4\u00dft", "ihr", "klei\u00b7nes", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und saugt den Veilchen, wenn sie duften,", "tokens": ["Und", "saugt", "den", "Veil\u00b7chen", ",", "wenn", "sie", "duf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Die S\u00fc\u00dfigkeit des kleinen Kelches aus.", "tokens": ["Die", "S\u00fc\u00b7\u00dfig\u00b7keit", "des", "klei\u00b7nen", "Kel\u00b7ches", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Die Schwalbe kommt aus Sumpf, wie aus verschlo\u00dfnen Gr\u00fcften", "tokens": ["Die", "Schwal\u00b7be", "kommt", "aus", "Sumpf", ",", "wie", "aus", "ver\u00b7schlo\u00df\u00b7nen", "Gr\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Einst unsre Leiber neu hervor,", "tokens": ["Einst", "uns\u00b7re", "Lei\u00b7ber", "neu", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Sie baut ihr Haus von Stroh und fetter Erde,", "tokens": ["Sie", "baut", "ihr", "Haus", "von", "Stroh", "und", "fet\u00b7ter", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Und schwitzert froh dem Menschen vor,", "tokens": ["Und", "schwit\u00b7zert", "froh", "dem", "Men\u00b7schen", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Da\u00df er auch wieder leben werde!", "tokens": ["Da\u00df", "er", "auch", "wie\u00b7der", "le\u00b7ben", "wer\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Hoch in der Wolken lauschend Ohr", "tokens": ["Hoch", "in", "der", "Wol\u00b7ken", "lau\u00b7schend", "Ohr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Singt mit nie heisch gewordner Kehle", "tokens": ["Singt", "mit", "nie", "heisch", "ge\u00b7word\u00b7ner", "Keh\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADV", "ADJD", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Das aufgeschwungne Lerchenchor.", "tokens": ["Das", "auf\u00b7ge\u00b7schwung\u00b7ne", "Ler\u00b7chen\u00b7chor", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O da\u00df der J\u00e4ger sie verfehle!", "tokens": ["O", "da\u00df", "der", "J\u00e4\u00b7ger", "sie", "ver\u00b7feh\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O da\u00df der Habicht, ihr Tyrann,", "tokens": ["O", "da\u00df", "der", "Ha\u00b7bicht", ",", "ihr", "Ty\u00b7rann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "KOUS", "ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der R\u00e4uber in dem Vogelreiche,", "tokens": ["Der", "R\u00e4u\u00b7ber", "in", "dem", "Vo\u00b7gel\u00b7rei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht eine hasche! da\u00df die Lerch ihm klug entweiche,", "tokens": ["Nicht", "ei\u00b7ne", "ha\u00b7sche", "!", "da\u00df", "die", "Lerch", "ihm", "klug", "ent\u00b7wei\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "$.", "KOUS", "ART", "NN", "PPER", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie vor dem Laster weicht, ein Christ, ein weiser Mann!", "tokens": ["Wie", "vor", "dem", "Las\u00b7ter", "weicht", ",", "ein", "Christ", ",", "ein", "wei\u00b7ser", "Mann", "!"], "token_info": ["word", "word", "word",