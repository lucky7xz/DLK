{"dta.poem.23723": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "An die Stadt Prag.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-200905197682", "language": ["de:0.57", "en:0.42"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach Prag, ich will dir rathen,", "tokens": ["Ach", "Prag", ",", "ich", "will", "dir", "ra\u00b7then", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PPER", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verspare deine Thaten.", "tokens": ["Ver\u00b7spa\u00b7re", "dei\u00b7ne", "Tha\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ergib dich an uns Preussen,", "tokens": ["Er\u00b7gib", "dich", "an", "uns", "Preus\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Eh wir die Bomben schmeissen,", "tokens": ["Eh", "wir", "die", "Bom\u00b7ben", "schmeis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sonst fallen deine Mauren,", "tokens": ["Sonst", "fal\u00b7len", "dei\u00b7ne", "Mau\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und deine Kinder trauren,", "tokens": ["Und", "dei\u00b7ne", "Kin\u00b7der", "trau\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wenn wir, auf deinen Gassen,", "tokens": ["Wenn", "wir", ",", "auf", "dei\u00b7nen", "Gas\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Bomben toben lassen.", "tokens": ["Die", "Bom\u00b7ben", "to\u00b7ben", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Auf, la\u00df von deinen Kindern", "tokens": ["Auf", ",", "la\u00df", "von", "dei\u00b7nen", "Kin\u00b7dern"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "$,", "VVIMP", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Ihr Toben schnell verhindern!", "tokens": ["Ihr", "To\u00b7ben", "schnell", "ver\u00b7hin\u00b7dern", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Du must die besten Sch\u00f6nen", "tokens": ["Du", "must", "die", "bes\u00b7ten", "Sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Mit Lorbeerzweigen cr\u00f6nen.", "tokens": ["Mit", "Lor\u00b7beer\u00b7zwei\u00b7gen", "cr\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Und mit gefaltnen H\u00e4nden", "tokens": ["Und", "mit", "ge\u00b7falt\u00b7nen", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Zu meinem K\u00f6nig senden.", "tokens": ["Zu", "mei\u00b7nem", "K\u00f6\u00b7nig", "sen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Dann werden sie, im Flehen,", "tokens": ["Dann", "wer\u00b7den", "sie", ",", "im", "Fle\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Sein gn\u00e4dig Antlitz sehen;", "tokens": ["Sein", "gn\u00e4\u00b7dig", "Ant\u00b7litz", "se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Dann wird der Held beweisen,", "tokens": ["Dann", "wird", "der", "Held", "be\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Es dien ihm Stahl und Eisen,", "tokens": ["Es", "di\u00b7en", "ihm", "Stahl", "und", "Ei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.19": {"text": "Es dienen ihm die Waffen", "tokens": ["Es", "die\u00b7nen", "ihm", "die", "Waf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Zu seegnen nicht zu strafen.", "tokens": ["Zu", "seeg\u00b7nen", "nicht", "zu", "stra\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Wie wirst du dann bedauren,", "tokens": ["Wie", "wirst", "du", "dann", "be\u00b7dau\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Da\u00df er durch deine Mauren,", "tokens": ["Da\u00df", "er", "durch", "dei\u00b7ne", "Mau\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}