 "ist", "dein", "Herz", "von", "Ei\u00b7sen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "+----+-+-", "measure": "dactylic.init"}, "line.26": {"text": "Was haben sie dir Leids gethan?", "tokens": ["Was", "ha\u00b7ben", "sie", "dir", "Leids", "ge\u00b7than", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PPER", "NE", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Die Wittwe bebt. Mit leisem St\u00f6hnen", "tokens": ["Die", "Witt\u00b7we", "bebt", ".", "Mit", "lei\u00b7sem", "St\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Reicht sie die V\u00e4ter ihren S\u00f6hnen,", "tokens": ["Reicht", "sie", "die", "V\u00e4\u00b7ter", "ih\u00b7ren", "S\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Die trugen sie vergn\u00fcgt nach Haus", "tokens": ["Die", "tru\u00b7gen", "sie", "ver\u00b7gn\u00fcgt", "nach", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Und fra\u00dfen sie zum Abendschmaus.", "tokens": ["Und", "fra\u00b7\u00dfen", "sie", "zum", "A\u00b7bendsc\u00b7hmaus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}