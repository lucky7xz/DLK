{"dta.poem.17435": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "56.  GOtte thut deine Verdamn\u00fc\u00df nicht  \n weh.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Der Sonne thuts nicht weh/ wenn du von ihr dich kehrst/", "tokens": ["Der", "Son\u00b7ne", "thuts", "nicht", "weh", "/", "wenn", "du", "von", "ihr", "dich", "kehrst", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "PTKVZ", "$(", "KOUS", "PPER", "APPR", "PPER", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Also auch GOtte nicht/ wenn du in Abgrund f\u00e4hrst.", "tokens": ["Al\u00b7so", "auch", "Got\u00b7te", "nicht", "/", "wenn", "du", "in", "Ab\u00b7grund", "f\u00e4hrst", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "PTKNEG", "$(", "KOUS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}}}}