{"textgrid.poem.41475": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Die verliebte Verzweiflung", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gewi\u00df! der ist beklagenswerth,", "tokens": ["Ge\u00b7wi\u00df", "!", "der", "ist", "be\u00b7kla\u00b7gens\u00b7werth", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ART", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den seine G\u00f6ttin nicht erh\u00f6rt;", "tokens": ["Den", "sei\u00b7ne", "G\u00f6t\u00b7tin", "nicht", "er\u00b7h\u00f6rt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem alle Seufzer nichts erwerben.", "tokens": ["Dem", "al\u00b7le", "Seuf\u00b7zer", "nichts", "er\u00b7wer\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er mu\u00df fast immer schlaflos sein,", "tokens": ["Er", "mu\u00df", "fast", "im\u00b7mer", "schlaf\u00b7los", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und weinen, girren, winseln, schrein,", "tokens": ["Und", "wei\u00b7nen", ",", "gir\u00b7ren", ",", "win\u00b7seln", ",", "schrein", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sich martern und dann sterben.", "tokens": ["Sich", "mar\u00b7tern", "und", "dann", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "KON", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Grausame Laura! rief Pedrill,", "tokens": ["Grau\u00b7sa\u00b7me", "Lau\u00b7ra", "!", "rief", "Ped\u00b7rill", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "VVFIN", "NE", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Grausame! die mein Ungl\u00fcck will,", "tokens": ["Grau\u00b7sa\u00b7me", "!", "die", "mein", "Un\u00b7gl\u00fcck", "will", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "PPOSAT", "NN", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr dich mu\u00df ich noch heut' erblassen.", "tokens": ["F\u00fcr", "dich", "mu\u00df", "ich", "noch", "heut'", "er\u00b7blas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Stracks rennet er in vollem Lauf", "tokens": ["Stracks", "ren\u00b7net", "er", "in", "vol\u00b7lem", "Lauf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bis an des Hauses Dach hinauf,", "tokens": ["Bis", "an", "des", "Hau\u00b7ses", "Dach", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und guckt dort in die Gassen.", "tokens": ["Und", "guckt", "dort", "in", "die", "Gas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Bald, als er Essen sah und roch,", "tokens": ["Bald", ",", "als", "er", "Es\u00b7sen", "sah", "und", "roch", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "NN", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Befragt' er sich: Wie! leb' ich noch?", "tokens": ["Be\u00b7fragt'", "er", "sich", ":", "Wie", "!", "leb'", "ich", "noch", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$.", "PWAV", "$.", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und zog ein Messer aus der Scheiden.", "tokens": ["Und", "zog", "ein", "Mes\u00b7ser", "aus", "der", "Schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "O Liebe! sagt' er, deiner Wuth", "tokens": ["O", "Lie\u00b7be", "!", "sagt'", "er", ",", "dei\u00b7ner", "Wuth"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$.", "VVFIN", "PPER", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Weih' ich den Mordstahl und mein Blut:", "tokens": ["Weih'", "ich", "den", "Mord\u00b7stahl", "und", "mein", "Blut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und fing an, Brod zu schneiden.", "tokens": ["Und", "fing", "an", ",", "Brod", "zu", "schnei\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$,", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Nach gl\u00fccklich eingenommnem Mahl", "tokens": ["Nach", "gl\u00fcck\u00b7lich", "ein\u00b7ge\u00b7nomm\u00b7nem", "Mahl"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erw\u00e4gt er seine Liebesqual,", "tokens": ["Er\u00b7w\u00e4gt", "er", "sei\u00b7ne", "Lie\u00b7bes\u00b7qual", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und will nunmehr durch Gift erbleichen.", "tokens": ["Und", "will", "nun\u00b7mehr", "durch", "Gift", "er\u00b7blei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er \u00f6ffnet eine Flasche Wein,", "tokens": ["Er", "\u00f6ff\u00b7net", "ei\u00b7ne", "Fla\u00b7sche", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und l\u00e4\u00dft, des Giftes voll zu sein,", "tokens": ["Und", "l\u00e4\u00dft", ",", "des", "Gif\u00b7tes", "voll", "zu", "sein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sich noch die zweite reichen.", "tokens": ["Sich", "noch", "die", "zwei\u00b7te", "rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Hernach verflucht er sein Geschick,", "tokens": ["Her\u00b7nach", "ver\u00b7flucht", "er", "sein", "Ge\u00b7schick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und holet Schemel, Nagel, Strick,", "tokens": ["Und", "ho\u00b7let", "Sche\u00b7mel", ",", "Na\u00b7gel", ",", "Strick", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$,", "NE", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und schw\u00f6rt, nun soll die That geschehen.", "tokens": ["Und", "schw\u00f6rt", ",", "nun", "soll", "die", "That", "ge\u00b7sche\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADV", "VMFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch, ach! was kann betr\u00fcbter sein!", "tokens": ["Doch", ",", "ach", "!", "was", "kann", "be\u00b7tr\u00fcb\u00b7ter", "sein", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ITJ", "$.", "PWS", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Strick ist schwach, der Nagel klein,", "tokens": ["Der", "Strick", "ist", "schwach", ",", "der", "Na\u00b7gel", "klein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NE", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Schemel will nicht