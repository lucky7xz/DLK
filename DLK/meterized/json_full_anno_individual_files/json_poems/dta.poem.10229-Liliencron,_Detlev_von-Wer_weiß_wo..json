{"dta.poem.10229": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Wer wei\u00df wo.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Auf Blut und Leichen, Schutt und Qualm,             ", "tokens": ["Auf", "Blut", "und", "Lei\u00b7chen", ",", "Schutt", "und", "Qualm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "ADJD", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf ro\u00dfzerstampften Sommerhalm", "tokens": ["Auf", "ro\u00df\u00b7zer\u00b7stampf\u00b7ten", "Som\u00b7mer\u00b7halm"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Sonne schien.", "tokens": ["Die", "Son\u00b7ne", "schien", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Es sank die Nacht. Die Schlacht ist aus,", "tokens": ["Es", "sank", "die", "Nacht", ".", "Die", "Schlacht", "ist", "aus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "ART", "NN", "VAFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und mancher kehrte nicht nach Haus", "tokens": ["Und", "man\u00b7cher", "kehr\u00b7te", "nicht", "nach", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "PTKNEG", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Einst von Kolin.", "tokens": ["Einst", "von", "Ko\u00b7lin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Ein Junker auch, ein Knabe noch,", "tokens": ["Ein", "Jun\u00b7ker", "auch", ",", "ein", "Kna\u00b7be", "noch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der heut das erste Pulver roch,", "tokens": ["Der", "heut", "das", "ers\u00b7te", "Pul\u00b7ver", "roch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er mu\u00dft\u2019 dahin.", "tokens": ["Er", "mu\u00dft'", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PAV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Wie hoch er auch die Fahne schwang,", "tokens": ["Wie", "hoch", "er", "auch", "die", "Fah\u00b7ne", "schwang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Tod in seinen Arm ihn zwang,", "tokens": ["Der", "Tod", "in", "sei\u00b7nen", "Arm", "ihn", "zwang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er mu\u00dft\u2019 dahin.", "tokens": ["Er", "mu\u00dft'", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PAV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Ihm nahe lag ein frommes Buch,", "tokens": ["Ihm", "na\u00b7he", "lag", "ein", "from\u00b7mes", "Buch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das stets der Junker bei sich trug,", "tokens": ["Das", "stets", "der", "Jun\u00b7ker", "bei", "sich", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Am Degenknauf.", "tokens": ["Am", "De\u00b7gen\u00b7knauf", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Ein Grenadier von Bevern fand", "tokens": ["Ein", "Gre\u00b7na\u00b7di\u00b7er", "von", "Be\u00b7vern", "fand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Den kleinen erdbeschmutzten Band", "tokens": ["Den", "klei\u00b7nen", "erd\u00b7be\u00b7schmutz\u00b7ten", "Band"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und hob ihn auf.", "tokens": ["Und", "hob", "ihn", "auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Und brachte heim mit schnellem Fu\u00df", "tokens": ["Und", "brach\u00b7te", "heim", "mit", "schnel\u00b7lem", "Fu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKVZ", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Vater diesen letzten Gru\u00df,", "tokens": ["Dem", "Va\u00b7ter", "die\u00b7sen", "letz\u00b7ten", "Gru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der klang nicht froh.", "tokens": ["Der", "klang", "nicht", "froh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Es schrieb hinein die Zitterhand:", "tokens": ["Es", "schrieb", "hin\u00b7ein", "die", "Zit\u00b7ter\u00b7hand", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u201ekolin. Mein Sohn verscharrt im Sand,", "tokens": ["\u201e", "ko\u00b7lin", ".", "Mein", "Sohn", "ver\u00b7scharrt", "im", "Sand", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "$.", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wer wei\u00df wo.\u201c", "tokens": ["Wer", "wei\u00df", "wo", ".", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "PWAV", "$.", "$("], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.5": {"line.1": {"text": "Und der gesungen dieses Lied,", "tokens": ["Und", "der", "ge\u00b7sun\u00b7gen", "die\u00b7ses", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und der es liest, im Leben zieht", "tokens": ["Und", "der", "es", "liest", ",", "im", "Le\u00b7ben", "zieht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "PPER", "VVFIN", "$,", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch frisch und froh.", "tokens": ["Noch", "frisch", "und", "froh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Doch einst bin ich, und bist auch du,", "tokens": ["Doch", "einst", "bin", "ich", ",", "und", "bist", "auch", "du", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "$,", "KON", "VAFIN", "ADV", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Verscharrt im Sand, in ewiger Ruh,", "tokens": ["Ver\u00b7scharrt", "im", "Sand", ",", "in", "e\u00b7wi\u00b7ger", "Ruh", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Wer wei\u00df wo.", "tokens": ["Wer", "wei\u00df", "wo", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PWAV", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}