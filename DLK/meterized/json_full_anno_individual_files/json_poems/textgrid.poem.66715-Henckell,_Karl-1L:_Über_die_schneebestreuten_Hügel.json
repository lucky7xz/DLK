{"textgrid.poem.66715": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00dcber die schneebestreuten H\u00fcgel", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcber die schneebestreuten H\u00fcgel", "tokens": ["\u00dc\u00b7ber", "die", "schnee\u00b7be\u00b7streu\u00b7ten", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Schreit' ich. Die Luft geht herb und rein.", "tokens": ["Schreit'", "ich", ".", "Die", "Luft", "geht", "herb", "und", "rein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Meiner Seele wuchsen die Fl\u00fcgel", "tokens": ["Mei\u00b7ner", "See\u00b7le", "wuch\u00b7sen", "die", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Neu. Schimmernd im Abendgold schwebt sie allein.", "tokens": ["Neu", ".", "Schim\u00b7mernd", "im", "A\u00b7bend\u00b7gold", "schwebt", "sie", "al\u00b7lein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "VVPP", "APPRART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "In den Nadelholzlichtungen rauchen", "tokens": ["In", "den", "Na\u00b7del\u00b7holz\u00b7lich\u00b7tun\u00b7gen", "rau\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+