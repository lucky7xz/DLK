{"textgrid.poem.36136": {"metadata": {"author": {"name": "Kosegarten, Gotthard Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du liebst mich nicht \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du liebst mich nicht \u2013", "tokens": ["Du", "liebst", "mich", "nicht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "O Gott, wie traf mein Herz mit schmetterndem Gewicht,", "tokens": ["O", "Gott", ",", "wie", "traf", "mein", "Herz", "mit", "schmet\u00b7tern\u00b7dem", "Ge\u00b7wicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie traf mit wilder M\u00f6rderkraft", "tokens": ["Wie", "traf", "mit", "wil\u00b7der", "M\u00f6r\u00b7der\u00b7kraft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Zeitung mich: Verschm\u00e4ht ist deine Leidenschaft!", "tokens": ["Die", "Zei\u00b7tung", "mich", ":", "Ver\u00b7schm\u00e4ht", "ist", "dei\u00b7ne", "Lei\u00b7den\u00b7schaft", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "$.", "VVPP", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Uns\u00e4glich hab' ich dich geliebt \u2013", "tokens": ["Un\u00b7s\u00e4g\u00b7lich", "hab'", "ich", "dich", "ge\u00b7liebt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "PRF", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Herz hat nah' und fern nach dir gestrebt, gesehnet,", "tokens": ["Mein", "Herz", "hat", "nah'", "und", "fern", "nach", "dir", "ge\u00b7strebt", ",", "ge\u00b7seh\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "APPR", "PPER", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein Auge heimlich dir gethr\u00e4net,", "tokens": ["Mein", "Au\u00b7ge", "heim\u00b7lich", "dir", "ge\u00b7thr\u00e4\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und jede Nerve sich nach dir gespannt, gedehnet \u2013", "tokens": ["Und", "je\u00b7de", "Ner\u00b7ve", "sich", "nach", "dir", "ge\u00b7spannt", ",", "ge\u00b7deh\u00b7net", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PRF", "APPR", "PPER", "VVPP", "$,", "VVPP", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "F\u00fcrwahr, ich hab' uns\u00e4glich dich geliebt.", "tokens": ["F\u00fcr\u00b7wahr", ",", "ich", "hab'", "un\u00b7s\u00e4g\u00b7lich", "dich", "ge\u00b7liebt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Noch jetzt sogar,", "tokens": ["Noch", "jetzt", "so\u00b7gar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Noch jetzt, zerrei\u00dft mein wundes Herz", "tokens": ["Noch", "jetzt", ",", "zer\u00b7rei\u00dft", "mein", "wun\u00b7des", "Herz"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Leidenschaft um dich mit jedem H\u00f6llenschmerz,", "tokens": ["Die", "Lei\u00b7den\u00b7schaft", "um", "dich", "mit", "je\u00b7dem", "H\u00f6l\u00b7len\u00b7schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PRF", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und tausend Mahl hab' ich in schwarzen Stunden", "tokens": ["Und", "tau\u00b7send", "Mahl", "hab'", "ich", "in", "schwar\u00b7zen", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "CARD", "NN", "VAFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Verdammter Geister Qual um dich, um dich empfunden.", "tokens": ["Ver\u00b7damm\u00b7ter", "Geis\u00b7ter", "Qual", "um", "dich", ",", "um", "dich", "emp\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "APPR", "PPER", "$,", "KOUI", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Zwar hab' ich auch in stillern Augenblicken,", "tokens": ["Zwar", "hab'", "ich", "auch", "in", "stil\u00b7lern", "Au\u00b7gen\u00b7bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit s\u00fc\u00dferm seeletrauerndem Entz\u00fccken,", "tokens": ["Mit", "s\u00fc\u00b7\u00dferm", "see\u00b7le\u00b7trau\u00b7ern\u00b7dem", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Um dich geweint!!", "tokens": ["Um", "dich", "ge\u00b7weint", "!!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Und sieh! Nicht ganz unselig ist der Mann,", "tokens": ["Und", "sieh", "!", "Nicht", "ganz", "un\u00b7se\u00b7lig", "ist", "der", "Mann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$.", "PTKNEG", "ADV", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "De\u00df Auge nur noch weinen kann!", "tokens": ["De\u00df", "Au\u00b7ge", "nur", "noch", "wei\u00b7nen", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch ach! in wenigen Sekunden", "tokens": ["Doch", "ach", "!", "in", "we\u00b7ni\u00b7gen", "Se\u00b7kun\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "XY", "$.", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist diese D\u00e4mmerung aus meiner Seele schwunden,", "tokens": ["Ist", "die\u00b7se", "D\u00e4m\u00b7me\u00b7rung", "aus", "mei\u00b7ner", "See\u00b7le", "schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und \u00f6d' und schwarz,", "tokens": ["Und", "\u00f6d'", "und", "schwarz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Wie Gr\u00e4ber, stand vor mir der stumme gro\u00dfe Schmerz,", "tokens": ["Wie", "Gr\u00e4\u00b7ber", ",", "stand", "vor", "mir", "der", "stum\u00b7me", "gro\u00b7\u00dfe", "Schmerz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "VVFIN", "APPR", "PPER", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und Eine lange Mitternacht", "tokens": ["Und", "Ei\u00b7ne", "lan\u00b7ge", "Mit\u00b7ter\u00b7nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hab' ich um dich verw\u00e4lzt, verseufzet und verwacht.", "tokens": ["Hab'", "ich", "um", "dich", "ver\u00b7w\u00e4lzt", ",", "ver\u00b7seuf\u00b7zet", "und", "ver\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPER", "VVFIN", "$,", "VVFIN", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Mit hellgeschliff'nem Feuerschein", "tokens": ["Mit", "hell\u00b7ge\u00b7schliff'\u00b7nem", "Feu\u00b7er\u00b7schein"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lud oft mitleidig mich mein Stahl zum Tode ein.", "tokens": ["Lud", "oft", "mit\u00b7lei\u00b7dig", "mich", "mein", "Stahl", "zum", "To\u00b7de", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJD", "PPER", "PPOSAT", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gefa\u00dft, gebl\u00f6\u00dft, gez\u00fcckt hab' ich ihn, doch sofort", "tokens": ["Ge\u00b7fa\u00dft", ",", "ge\u00b7bl\u00f6\u00dft", ",", "ge\u00b7z\u00fcckt", "hab'", "ich", "ihn", ",", "doch", "so\u00b7fort"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVPP", "$,", "VVFIN", "$,", "VVPP", "VAFIN", "PPER", "PPER", "$,", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gedacht' ich dein, und fuhr um dich zu leben fort!", "tokens": ["Ge\u00b7dacht'", "ich", "dein", ",", "und", "fuhr", "um", "dich", "zu", "le\u00b7ben", "fort", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "$,", "KON", "VVFIN", "APPR", "PPER", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Denn wehe mir!", "tokens": ["Denn", "we\u00b7he", "mir", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Noch unzertrennlich h\u00e4ngt mein ganzes Herz an dir.", "tokens": ["Noch", "un\u00b7zer\u00b7trenn\u00b7lich", "h\u00e4ngt", "mein", "gan\u00b7zes", "Herz", "an", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Ich saug' aus deinen Blicken", "tokens": ["Ich", "saug'", "aus", "dei\u00b7nen", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Tod!", "tokens": ["Den", "Tod", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Ich schlinge mit rasendem Entz\u00fccken", "tokens": ["Ich", "schlin\u00b7ge", "mit", "ra\u00b7sen\u00b7dem", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Den Gift von deiner Wangen Blumenroth.", "tokens": ["Den", "Gift", "von", "dei\u00b7ner", "Wan\u00b7gen", "Blu\u00b7men\u00b7roth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Du aber wandelst stolz und heiter", "tokens": ["Du", "a\u00b7ber", "wan\u00b7delst", "stolz", "und", "hei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vor meiner Qual vorbei, und bist", "tokens": ["Vor", "mei\u00b7ner", "Qual", "vor\u00b7bei", ",", "und", "bist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$,", "KON", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erfreut ob meinem Schmerz, siehst ihn, und wandelst weiter,", "tokens": ["Er\u00b7freut", "ob", "mei\u00b7nem", "Schmerz", ",", "siehst", "ihn", ",", "und", "wan\u00b7delst", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOUS", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "$,", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zufrieden, da\u00df mein Stolz herab erniedrigt ist \u2013 \u2013", "tokens": ["Zu\u00b7frie\u00b7den", ",", "da\u00df", "mein", "Stolz", "her\u00b7ab", "er\u00b7nied\u00b7rigt", "ist", "\u2013", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "KOUS", "PPOSAT", "NN", "ADV", "VVPP", "VAFIN", "$(", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Ich kann's nicht tragen \u2013", "tokens": ["Ich", "kann's", "nicht", "tra\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Mit wilden Schl\u00e4gen", "tokens": ["Mit", "wil\u00b7den", "Schl\u00e4\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Emp\u00f6rt sich mein Herz.", "tokens": ["Em\u00b7p\u00f6rt", "sich", "mein", "Herz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Ich kann's nicht fassen,", "tokens": ["Ich", "kann's", "nicht", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Ich will dich hassen,", "tokens": ["Ich", "will", "dich", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Und meinen Schmerz.", "tokens": ["Und", "mei\u00b7nen", "Schmerz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.11": {"line.1": {"text": "Noch Einen Tag,", "tokens": ["Noch", "Ei\u00b7nen", "Tag", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "So will ich dir enteilen,", "tokens": ["So", "will", "ich", "dir", "ent\u00b7ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von deinen Pfeilen", "tokens": ["Von", "dei\u00b7nen", "Pfei\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Verwund't und bis zum Tode schwach.", "tokens": ["Ver\u00b7wund't", "und", "bis", "zum", "To\u00b7de", "schwach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "APPR", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch diese tiefen Wunden, ach!", "tokens": ["Doch", "die\u00b7se", "tie\u00b7fen", "Wun\u00b7den", ",", "ach", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "$,", "ITJ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wird keiner Ferne Kraft, kein fremder Boden heilen \u2013 \u2013", "tokens": ["Wird", "kei\u00b7ner", "Fer\u00b7ne", "Kraft", ",", "kein", "frem\u00b7der", "Bo\u00b7den", "hei\u00b7len", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PIAT", "NN", "NN", "$,", "PIAT", "ADJA", "NN", "VVINF", "$(", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Es flieht, den Stahl im Busen,", "tokens": ["Es", "flieht", ",", "den", "Stahl", "im", "Bu\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Reh den Platz, wo es der Pfeilschu\u00df trifft.", "tokens": ["Das", "Reh", "den", "Platz", ",", "wo", "es", "der", "Pfeil\u00b7schu\u00df", "trifft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es fliehet, blutet, rennt, tr\u00e4gt fort den Tod im Busen,", "tokens": ["Es", "flie\u00b7het", ",", "blu\u00b7tet", ",", "rennt", ",", "tr\u00e4gt", "fort", "den", "Tod", "im", "Bu\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "PTKVZ", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "St\u00fcrzt, blutet, \u00e4chzt und stirbt auf fremder Trift.", "tokens": ["St\u00fcrzt", ",", "blu\u00b7tet", ",", "\u00e4chzt", "und", "stirbt", "auf", "frem\u00b7der", "Trift", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Du liebst mich nicht \u2013", "tokens": ["Du", "liebst", "mich", "nicht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "O Gott, wie traf mein Herz mit schmetterndem Gewicht,", "tokens": ["O", "Gott", ",", "wie", "traf", "mein", "Herz", "mit", "schmet\u00b7tern\u00b7dem", "Ge\u00b7wicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie traf mit wilder M\u00f6rderkraft", "tokens": ["Wie", "traf", "mit", "wil\u00b7der", "M\u00f6r\u00b7der\u00b7kraft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Zeitung mich: Verschm\u00e4ht ist deine Leidenschaft!", "tokens": ["Die", "Zei\u00b7tung", "mich", ":", "Ver\u00b7schm\u00e4ht", "ist", "dei\u00b7ne", "Lei\u00b7den\u00b7schaft", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "$.", "VVPP", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.14": {"line.1": {"text": "Uns\u00e4glich hab' ich dich geliebt \u2013", "tokens": ["Un\u00b7s\u00e4g\u00b7lich", "hab'", "ich", "dich", "ge\u00b7liebt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "PRF", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Herz hat nah' und fern nach dir gestrebt, gesehnet,", "tokens": ["Mein", "Herz", "hat", "nah'", "und", "fern", "nach", "dir", "ge\u00b7strebt", ",", "ge\u00b7seh\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "APPR", "PPER", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein Auge heimlich dir gethr\u00e4net,", "tokens": ["Mein", "Au\u00b7ge", "heim\u00b7lich", "dir", "ge\u00b7thr\u00e4\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und jede Nerve sich nach dir gespannt, gedehnet \u2013", "tokens": ["Und", "je\u00b7de", "Ner\u00b7ve", "sich", "nach", "dir", "ge\u00b7spannt", ",", "ge\u00b7deh\u00b7net", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PRF", "APPR", "PPER", "VVPP", "$,", "VVPP", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "F\u00fcrwahr, ich hab' uns\u00e4glich dich geliebt.", "tokens": ["F\u00fcr\u00b7wahr", ",", "ich", "hab'", "un\u00b7s\u00e4g\u00b7lich", "dich", "ge\u00b7liebt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.15": {"line.1": {"text": "Noch jetzt sogar,", "tokens": ["Noch", "jetzt", "so\u00b7gar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Noch jetzt, zerrei\u00dft mein wundes Herz", "tokens": ["Noch", "jetzt", ",", "zer\u00b7rei\u00dft", "mein", "wun\u00b7des", "Herz"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Leidenschaft um dich mit jedem H\u00f6llenschmerz,", "tokens": ["Die", "Lei\u00b7den\u00b7schaft", "um", "dich", "mit", "je\u00b7dem", "H\u00f6l\u00b7len\u00b7schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PRF", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und tausend Mahl hab' ich in schwarzen Stunden", "tokens": ["Und", "tau\u00b7send", "Mahl", "hab'", "ich", "in", "schwar\u00b7zen", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "CARD", "NN", "VAFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Verdammter Geister Qual um dich, um dich empfunden.", "tokens": ["Ver\u00b7damm\u00b7ter", "Geis\u00b7ter", "Qual", "um", "dich", ",", "um", "dich", "emp\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "APPR", "PPER", "$,", "KOUI", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.16": {"line.1": {"text": "Zwar hab' ich auch in stillern Augenblicken,", "tokens": ["Zwar", "hab'", "ich", "auch", "in", "stil\u00b7lern", "Au\u00b7gen\u00b7bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit s\u00fc\u00dferm seeletrauerndem Entz\u00fccken,", "tokens": ["Mit", "s\u00fc\u00b7\u00dferm", "see\u00b7le\u00b7trau\u00b7ern\u00b7dem", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Um dich geweint!!", "tokens": ["Um", "dich", "ge\u00b7weint", "!!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Und sieh! Nicht ganz unselig ist der Mann,", "tokens": ["Und", "sieh", "!", "Nicht", "ganz", "un\u00b7se\u00b7lig", "ist", "der", "Mann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$.", "PTKNEG", "ADV", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "De\u00df Auge nur noch weinen kann!", "tokens": ["De\u00df", "Au\u00b7ge", "nur", "noch", "wei\u00b7nen", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.17": {"line.1": {"text": "Doch ach! in wenigen Sekunden", "tokens": ["Doch", "ach", "!", "in", "we\u00b7ni\u00b7gen", "Se\u00b7kun\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "XY", "$.", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist diese D\u00e4mmerung aus meiner Seele schwunden,", "tokens": ["Ist", "die\u00b7se", "D\u00e4m\u00b7me\u00b7rung", "aus", "mei\u00b7ner", "See\u00b7le", "schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und \u00f6d' und schwarz,", "tokens": ["Und", "\u00f6d'", "und", "schwarz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Wie Gr\u00e4ber, stand vor mir der stumme gro\u00dfe Schmerz,", "tokens": ["Wie", "Gr\u00e4\u00b7ber", ",", "stand", "vor", "mir", "der", "stum\u00b7me", "gro\u00b7\u00dfe", "Schmerz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "VVFIN", "APPR", "PPER", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und Eine lange Mitternacht", "tokens": ["Und", "Ei\u00b7ne", "lan\u00b7ge", "Mit\u00b7ter\u00b7nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hab' ich um dich verw\u00e4lzt, verseufzet und verwacht.", "tokens": ["Hab'", "ich", "um", "dich", "ver\u00b7w\u00e4lzt", ",", "ver\u00b7seuf\u00b7zet", "und", "ver\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPER", "VVFIN", "$,", "VVFIN", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.18": {"line.1": {"text": "Mit hellgeschliff'nem Feuerschein", "tokens": ["Mit", "hell\u00b7ge\u00b7schliff'\u00b7nem", "Feu\u00b7er\u00b7schein"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lud oft mitleidig mich mein Stahl zum Tode ein.", "tokens": ["Lud", "oft", "mit\u00b7lei\u00b7dig", "mich", "mein", "Stahl", "zum", "To\u00b7de", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJD", "PPER", "PPOSAT", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gefa\u00dft, gebl\u00f6\u00dft, gez\u00fcckt hab' ich ihn, doch sofort", "tokens": ["Ge\u00b7fa\u00dft", ",", "ge\u00b7bl\u00f6\u00dft", ",", "ge\u00b7z\u00fcckt", "hab'", "ich", "ihn", ",", "doch", "so\u00b7fort"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVPP", "$,", "VVFIN", "$,", "VVPP", "VAFIN", "PPER", "PPER", "$,", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gedacht' ich dein, und fuhr um dich zu leben fort!", "tokens": ["Ge\u00b7dacht'", "ich", "dein", ",", "und", "fuhr", "um", "dich", "zu", "le\u00b7ben", "fort", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "$,", "KON", "VVFIN", "APPR", "PPER", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.19": {"line.1": {"text": "Denn wehe mir!", "tokens": ["Denn", "we\u00b7he", "mir", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Noch unzertrennlich h\u00e4ngt mein ganzes Herz an dir.", "tokens": ["Noch", "un\u00b7zer\u00b7trenn\u00b7lich", "h\u00e4ngt", "mein", "gan\u00b7zes", "Herz", "an", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.20": {"line.1": {"text": "Ich saug' aus deinen Blicken", "tokens": ["Ich", "saug'", "aus", "dei\u00b7nen", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Tod!", "tokens": ["Den", "Tod", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Ich schlinge mit rasendem Entz\u00fccken", "tokens": ["Ich", "schlin\u00b7ge", "mit", "ra\u00b7sen\u00b7dem", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Den Gift von deiner Wangen Blumenroth.", "tokens": ["Den", "Gift", "von", "dei\u00b7ner", "Wan\u00b7gen", "Blu\u00b7men\u00b7roth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.21": {"line.1": {"text": "Du aber wandelst stolz und heiter", "tokens": ["Du", "a\u00b7ber", "wan\u00b7delst", "stolz", "und", "hei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vor meiner Qual vorbei, und bist", "tokens": ["Vor", "mei\u00b7ner", "Qual", "vor\u00b7bei", ",", "und", "bist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$,", "KON", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erfreut ob meinem Schmerz, siehst ihn, und wandelst weiter,", "tokens": ["Er\u00b7freut", "ob", "mei\u00b7nem", "Schmerz", ",", "siehst", "ihn", ",", "und", "wan\u00b7delst", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOUS", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "$,", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zufrieden, da\u00df mein Stolz herab erniedrigt ist \u2013 \u2013", "tokens": ["Zu\u00b7frie\u00b7den", ",", "da\u00df", "mein", "Stolz", "her\u00b7ab", "er\u00b7nied\u00b7rigt", "ist", "\u2013", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "KOUS", "PPOSAT", "NN", "ADV", "VVPP", "VAFIN", "$(", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.22": {"line.1": {"text": "Ich kann's nicht tragen \u2013", "tokens": ["Ich", "kann's", "nicht", "tra\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Mit wilden Schl\u00e4gen", "tokens": ["Mit"