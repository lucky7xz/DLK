{"dta.poem.2851": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "52.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Am Tag des Gl\u00fcckes wird ein k\u00fchner Sprung dir gl\u00fccken,", "tokens": ["Am", "Tag", "des", "Gl\u00fc\u00b7ckes", "wird", "ein", "k\u00fch\u00b7ner", "Sprung", "dir", "gl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VAFIN", "ART", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Am Tag des Ungl\u00fccks st\u00fcrzt ein Fehltritt von der Br\u00fccken.", "tokens": ["Am", "Tag", "des", "Un\u00b7gl\u00fccks", "st\u00fcrzt", "ein", "Fehl\u00b7tritt", "von", "der", "Br\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Drum meide jeder Frist den Fehltritt! denn du bist", "tokens": ["Drum", "mei\u00b7de", "je\u00b7der", "Frist", "den", "Fehl\u00b7tritt", "!", "denn", "du", "bist"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PIAT", "NN", "ART", "NN", "$.", "KON", "PPER", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nie sicher, ob dein Ungl\u00fccks- oder Gl\u00fcckstag ist.", "tokens": ["Nie", "si\u00b7cher", ",", "ob", "dein", "Un\u00b7gl\u00fccks", "o\u00b7der", "Gl\u00fccks\u00b7tag", "ist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "wo