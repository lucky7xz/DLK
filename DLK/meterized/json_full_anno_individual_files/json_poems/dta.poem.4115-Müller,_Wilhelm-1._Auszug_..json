{"dta.poem.4115": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1.  Auszug .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als ob's ein Spa\u00df nur w\u00e4r':", "tokens": ["Als", "ob's", "ein", "Spa\u00df", "nur", "w\u00e4r'", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ART", "NN", "ADV", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Das macht, es wallt Feinliebchens Bild", "tokens": ["Das", "macht", ",", "es", "wallt", "Fein\u00b7lieb\u00b7chens", "Bild"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "PPER", "VVFIN", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gar helle vor mir her.", "tokens": ["Gar", "hel\u00b7le", "vor", "mir", "her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da merk' ich dann im Herzen bald:", "tokens": ["Da", "merk'", "ich", "dann", "im", "Her\u00b7zen", "bald", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich sei dort, oder hier,", "tokens": ["Ich", "sei", "dort", ",", "o\u00b7der", "hier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "KON", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich gehe fort, ich kehre heim,", "tokens": ["Ich", "ge\u00b7he", "fort", ",", "ich", "keh\u00b7re", "heim", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich ziehe doch immer zu ihr.", "tokens": ["Ich", "zie\u00b7he", "doch", "im\u00b7mer", "zu", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "PPOSAT", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Und wer zu seinem Liebchen reis't,", "tokens": ["Und", "wer", "zu", "sei\u00b7nem", "Lieb\u00b7chen", "reis't", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem wird kein Weg zu schwer,", "tokens": ["Dem", "wird", "kein", "Weg", "zu", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIAT", "NN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der l\u00e4uft bei Tag und l\u00e4uft bei Nacht,", "tokens": ["Der", "l\u00e4uft", "bei", "Tag", "und", "l\u00e4uft", "bei", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "KON", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ruht sich nimmermehr.", "tokens": ["Und", "ruht", "sich", "nim\u00b7mer\u00b7mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und ob es regnet, ob es st\u00fcrmt,", "tokens": ["Und", "ob", "es", "reg\u00b7net", ",", "ob", "es", "st\u00fcrmt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir thut kein Wetter weh:", "tokens": ["Mir", "thut", "kein", "Wet\u00b7ter", "weh", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es hat mein Liebchen mir gesagt", "tokens": ["Es", "hat", "mein", "Lieb\u00b7chen", "mir", "ge\u00b7sagt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein freundliches Ade!", "tokens": ["Ein", "freund\u00b7li\u00b7ches", "A\u00b7de", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}