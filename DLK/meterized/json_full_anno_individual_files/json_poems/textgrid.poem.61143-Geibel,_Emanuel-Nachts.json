{"textgrid.poem.61143": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Nachts", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dem Mondesaufgang wandl' ich gern entgegen,", "tokens": ["Dem", "Mon\u00b7des\u00b7auf\u00b7gang", "wandl'", "ich", "gern", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn alles schlummert, durch die stillen Gassen;", "tokens": ["Wenn", "al\u00b7les", "schlum\u00b7mert", ",", "durch", "die", "stil\u00b7len", "Gas\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des Marktes Brunnen rauschet noch verlassen,", "tokens": ["Des", "Mark\u00b7tes", "Brun\u00b7nen", "rau\u00b7schet", "noch", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sonst tiefes Schweigen rings auf allen Wegen.", "tokens": ["Sonst", "tie\u00b7fes", "Schwei\u00b7gen", "rings", "auf", "al\u00b7len", "We\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da spricht die Nacht auch \u00fcber mich den Segen,", "tokens": ["Da", "spricht", "die", "Nacht", "auch", "\u00fc\u00b7ber", "mich", "den", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In sanfte Wehmut schmilzt das trotz'ge Hassen,", "tokens": ["In", "sanf\u00b7te", "Weh\u00b7mut", "schmilzt", "das", "trotz'\u00b7ge", "Has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Liebe naht, mich gl\u00e4ubig zu umfassen,", "tokens": ["Die", "Lie\u00b7be", "naht", ",", "mich", "gl\u00e4u\u00b7big", "zu", "um\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und will das Haupt an meine Schulter legen.", "tokens": ["Und", "will", "das", "Haupt", "an", "mei\u00b7ne", "Schul\u00b7ter", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Mir ist's, als k\u00e4me mir die Jugend wieder,", "tokens": ["Mir", "ist's", ",", "als", "k\u00e4\u00b7me", "mir", "die", "Ju\u00b7gend", "wie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "VVFIN", "PPER", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wieder streben in sehns\u00fccht'ger Weise", "tokens": ["Und", "wie\u00b7der", "stre\u00b7ben", "in", "sehn\u00b7s\u00fccht'\u00b7ger", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Aus dieser Brust zur Heimat meine Lieder.", "tokens": ["Aus", "die\u00b7ser", "Brust", "zur", "Hei\u00b7mat", "mei\u00b7ne", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So schwingt von Schw\u00e4nen eine Schar sich leise", "tokens": ["So", "schwingt", "von", "Schw\u00e4\u00b7nen", "ei\u00b7ne", "Schar", "sich", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "ART", "NN", "PRF", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus dunklem See auf wallendem Gefieder,", "tokens": ["Aus", "dunk\u00b7lem", "See", "auf", "wal\u00b7len\u00b7dem", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn sie beginnt nach S\u00fcden ihre Reise.", "tokens": ["Wenn", "sie", "be\u00b7ginnt", "nach", "S\u00fc\u00b7den", "ih\u00b7re", "Rei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Dem Mondesaufgang wandl' ich gern entgegen,", "tokens": ["Dem", "Mon\u00b7des\u00b7auf\u00b7gang", "wandl'", "ich", "gern", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic