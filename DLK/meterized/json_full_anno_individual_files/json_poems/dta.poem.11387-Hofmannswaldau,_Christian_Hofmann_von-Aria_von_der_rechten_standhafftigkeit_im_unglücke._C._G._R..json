{"dta.poem.11387": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Aria von der rechten standhafftigkeit  \n im ungl\u00fccke.  \n C. G. R.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hemme deine rauhe klagen!", "tokens": ["Hem\u00b7me", "dei\u00b7ne", "rau\u00b7he", "kla\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stille deiner thr\u00e4nen lauff!", "tokens": ["Stil\u00b7le", "dei\u00b7ner", "thr\u00e4\u00b7nen", "lauff", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Werthe seele! denn die plagen", "tokens": ["Wert\u00b7he", "see\u00b7le", "!", "denn", "die", "pla\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$.", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00f6ren nicht durch kummer auf.", "tokens": ["H\u00f6\u00b7ren", "nicht", "durch", "kum\u00b7mer", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Warum frisst du dir das hertze", "tokens": ["Wa\u00b7rum", "frisst", "du", "dir", "das", "hert\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit erdachter bitterkeit?", "tokens": ["Mit", "er\u00b7dach\u00b7ter", "bit\u00b7ter\u00b7keit", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Deine seufftzer, nicht der schmertze,", "tokens": ["Dei\u00b7ne", "seufft\u00b7zer", ",", "nicht", "der", "schmert\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "PTKNEG", "ART", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sind dein gantzes hertzeleid.", "tokens": ["Sind", "dein", "gant\u00b7zes", "hert\u00b7ze\u00b7leid", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "In dem ungl\u00fcck fr\u00f6lich leben,", "tokens": ["In", "dem", "un\u00b7gl\u00fcck", "fr\u00f6\u00b7lich", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Will zwar sast unm\u00f6glich seyn;", "tokens": ["Will", "zwar", "sast", "un\u00b7m\u00f6g\u00b7lich", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wetter, die die erd erbeben,", "tokens": ["Wet\u00b7ter", ",", "die", "die", "erd", "er\u00b7be\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fl\u00f6sen billich schrecken ein.", "tokens": ["Fl\u00f6\u00b7sen", "bil\u00b7lich", "schre\u00b7cken", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wann die donner-schl\u00e4ge knallen,", "tokens": ["Wann", "die", "don\u00b7ner\u00b7schl\u00e4\u00b7ge", "knal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird der gr\u00f6ste fels ger\u00fchrt:", "tokens": ["Wird", "der", "gr\u00f6s\u00b7te", "fels", "ge\u00b7r\u00fchrt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und wenn schloss\u2019 und hagel fallen,", "tokens": ["Und", "wenn", "schloss'", "und", "ha\u00b7gel", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADJD", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wird uns blum und lust entf\u00fchrt.", "tokens": ["Wird", "uns", "blum", "und", "lust", "ent\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NE", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Dencke nur di\u00df zu behalten,", "tokens": ["Den\u00b7cke", "nur", "di\u00df", "zu", "be\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PDS", "PTKZU", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Da\u00df zwar furcht erlaubet sey;", "tokens": ["Da\u00df", "zwar", "furcht", "er\u00b7lau\u00b7bet", "sey", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "VVFIN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch vor schrecken gar erkalten,", "tokens": ["Doch", "vor", "schre\u00b7cken", "gar", "er\u00b7kal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "VVFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bleibet niemahls tadel-frey.", "tokens": ["Blei\u00b7bet", "nie\u00b7mahls", "ta\u00b7del\u00b7frey", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Niederfallen wird gestrafet;", "tokens": ["Nie\u00b7der\u00b7fal\u00b7len", "wird", "ge\u00b7stra\u00b7fet", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zittern nicht. Die wahre ruh,", "tokens": ["Zit\u00b7tern", "nicht", ".", "Die", "wah\u00b7re", "ruh", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$.", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Die gelassenheit verschaffet,", "tokens": ["Die", "ge\u00b7las\u00b7sen\u00b7heit", "ver\u00b7schaf\u00b7fet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "K\u00f6mmt nicht eiteln l\u00fcsten zu.", "tokens": ["K\u00f6mmt", "nicht", "ei\u00b7teln", "l\u00fcs\u00b7ten", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "VVINF", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "So, wie die geschlancken weiden,", "tokens": ["So", ",", "wie", "die", "ge\u00b7schlan\u00b7cken", "wei\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die kein sturm-wind nicht verletzt,", "tokens": ["Die", "kein", "stur\u00b7mwind", "nicht", "ver\u00b7letzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch sich nicht mit bl\u00fcthen kleiden;", "tokens": ["Doch", "sich", "nicht", "mit", "bl\u00fc\u00b7then", "klei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "PTKNEG", "APPR", "ADJA", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So bleib du auch fest gesetzt.", "tokens": ["So", "bleib", "du", "auch", "fest", "ge\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Der wird unverletzt getroffen,", "tokens": ["Der", "wird", "un\u00b7ver\u00b7letzt", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der, trotz auch der schwersten last!", "tokens": ["Der", ",", "trotz", "auch", "der", "schwers\u00b7ten", "last", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "ADV", "ART", "ADJA", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Durch geduld und stilles hoffen", "tokens": ["Durch", "ge\u00b7duld", "und", "stil\u00b7les", "hof\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "PIS", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In dem himmel wurtzeln fasst.", "tokens": ["In", "dem", "him\u00b7mel", "wurt\u00b7zeln", "fasst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}