{"textgrid.poem.48766": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "53. Mons. Johan Hillebranden, F\u00fcrstl. Holsteinischer Gesandten bestalten Violisten", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was deine kluge Faust uns macht f\u00fcr Lieblichkeiten,", "tokens": ["Was", "dei\u00b7ne", "klu\u00b7ge", "Faust", "uns", "macht", "f\u00fcr", "Lieb\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und wie du Ohr und Herz auf einmal nach dir zeuchst,", "tokens": ["und", "wie", "du", "Ohr", "und", "Herz", "auf", "ein\u00b7mal", "nach", "dir", "zeuchst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "NN", "KON", "NN", "APPR", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "wenn du nach deiner Art die scharfe Geige streichst,", "tokens": ["wenn", "du", "nach", "dei\u00b7ner", "Art", "die", "schar\u00b7fe", "Gei\u00b7ge", "streichst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "das h\u00f6rt' ich gestern noch auf unserm Pindus streiten.", "tokens": ["das", "h\u00f6rt'", "ich", "ge\u00b7stern", "noch", "auf", "un\u00b7serm", "Pin\u00b7dus", "strei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Stryx schriebs dem Bogen zu, der Zoilus den Saiten,", "tokens": ["Stryx", "schriebs", "dem", "Bo\u00b7gen", "zu", ",", "der", "Zoi\u00b7lus", "den", "Sai\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "PTKVZ", "$,", "ART", "NE", "ART", "NN", "$,"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "und Momus, wei\u00df nicht, wem, Amphion, dem du gleichst,", "tokens": ["und", "Mo\u00b7mus", ",", "wei\u00df", "nicht", ",", "wem", ",", "Am\u00b7phi\u00b7on", ",", "dem", "du", "gleichst", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "VVFIN", "PTKNEG", "$,", "PWS", "$,", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "der neuerfundnen Art, und dem du nichts nicht weichst,", "tokens": ["der", "neu\u00b7er\u00b7fund\u00b7nen", "Art", ",", "und", "dem", "du", "nichts", "nicht", "weichst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "ART", "PPER", "PIS", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Eagers Sohn, der Kunst, die dich macht wert bei Leuten.", "tokens": ["E\u00b7a\u00b7gers", "Sohn", ",", "der", "Kunst", ",", "die", "dich", "macht", "wert", "bei", "Leu\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Sie kunten lange sich hier\u00fcber nicht vertragen.", "tokens": ["Sie", "kun\u00b7ten", "lan\u00b7ge", "sich", "hier\u00b7\u00fc\u00b7ber", "nicht", "ver\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PRF", "PAV", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was auch die Klarien und Gratien nur sagen,", "tokens": ["Was", "auch", "die", "Kla\u00b7ri\u00b7en", "und", "Gra\u00b7ti\u00b7en", "nur", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "KON", "NE", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "das hat fast wenig Stat. Apollo rufte klar:", "tokens": ["das", "hat", "fast", "we\u00b7nig", "Stat", ".", "A\u00b7pol\u00b7lo", "ruf\u00b7te", "klar", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PIAT", "NN", "$.", "NE", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Was, sprach er, reden doch die Esel von Gelehrten?", "tokens": ["Was", ",", "sprach", "er", ",", "re\u00b7den", "doch", "die", "E\u00b7sel", "von", "Ge\u00b7lehr\u00b7ten", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "VVFIN", "PPER", "$,", "VVFIN", "ADV", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weg Stryx, weg Zoilus, weg Momus, ihr Verkehrten!", "tokens": ["Weg", "Stryx", ",", "weg", "Zoi\u00b7lus", ",", "weg", "Mo\u00b7mus", ",", "ihr", "Ver\u00b7kehr\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "ADV", "NE", "$,", "ADV", "NE", "$,", "PPOSAT", "NN", "$."], "meter": "++-+-+-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Amphion saget recht und Orpheus mehr als wahr!", "tokens": ["Am\u00b7phi\u00b7on", "sa\u00b7get", "recht", "und", "Or\u00b7pheus", "mehr", "als", "wahr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "KON", "NE", "PIAT", "KOKOM", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Was deine kluge Faust uns macht f\u00fcr Lieblichkeiten,", "tokens": ["Was", "dei\u00b7ne", "klu\u00b7ge", "Faust", "uns", "macht", "f\u00fcr", "Lieb\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und wie du Ohr und Herz auf einmal nach dir zeuchst,", "tokens": ["und", "wie", "du", "Ohr", "und", "Herz", "auf", "ein\u00b7mal", "nach", "dir", "zeuchst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "NN", "KON", "NN", "APPR", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "wenn du nach deiner Art die scharfe Geige streichst,", "tokens": ["wenn", "du", "nach", "dei\u00b7ner", "Art", "die", "schar\u00b7fe", "Gei\u00b7ge", "streichst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure