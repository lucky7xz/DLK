{"dta.poem.15772": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "32.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jemehr die Liebe gibt, jemehr empf\u00e4ngt sie wieder;", "tokens": ["Je\u00b7mehr", "die", "Lie\u00b7be", "gibt", ",", "je\u00b7mehr", "emp\u00b7f\u00e4ngt", "sie", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Darum versiegen nie des echten Dichters Lieder.", "tokens": ["Da\u00b7rum", "ver\u00b7sie\u00b7gen", "nie", "des", "ech\u00b7ten", "Dich\u00b7ters", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie sich der Erdschoo\u00df nie ersch\u00f6pft an Lust und Gl\u00fcck;", "tokens": ["Wie", "sich", "der", "Erd\u00b7schoo\u00df", "nie", "er\u00b7sch\u00f6pft", "an", "Lust", "und", "Gl\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "ADV", "VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn alles was er gibt, flie\u00dft auch in ihn zur\u00fcck.", "tokens": ["Denn", "al\u00b7les", "was", "er", "gibt", ",", "flie\u00dft", "auch", "in", "ihn", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PWS", "PPER", "V