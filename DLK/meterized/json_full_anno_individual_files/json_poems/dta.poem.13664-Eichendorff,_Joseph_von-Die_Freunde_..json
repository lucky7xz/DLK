{"dta.poem.13664": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Freunde .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Pr\u00e4cht'ger Wald, wo's k\u00fchl zu wohnen,", "tokens": ["Pr\u00e4cht'\u00b7ger", "Wald", ",", "wo's", "k\u00fchl", "zu", "woh\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWAV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stille wachsend Baum an Baum,", "tokens": ["Stil\u00b7le", "wach\u00b7send", "Baum", "an", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "APPR", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit den br\u00fcderlichen Kronen", "tokens": ["Mit", "den", "br\u00fc\u00b7der\u00b7li\u00b7chen", "Kro\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rauschend in dem Himmelsraum!", "tokens": ["Rau\u00b7schend", "in", "dem", "Him\u00b7mels\u00b7raum", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}