{"textgrid.poem.63352": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Gut Holz", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer hat dich so hoch da droben \u2013", "tokens": ["Wer", "hat", "dich", "so", "hoch", "da", "dro\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "ADJD", "ADV", "ADV", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das Kegelspiel ist schon seit ewigen Zeiten eine kulturelle Macht.", "tokens": ["Das", "Ke\u00b7gel\u00b7spiel", "ist", "schon", "seit", "e\u00b7wi\u00b7gen", "Zei\u00b7ten", "ei\u00b7ne", "kul\u00b7tu\u00b7rel\u00b7le", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "Urspr\u00fcnglich haben die G\u00f6tter mit dem Mond nach den Sternen geschoben", "tokens": ["Ur\u00b7spr\u00fcng\u00b7lich", "ha\u00b7ben", "die", "G\u00f6t\u00b7ter", "mit", "dem", "Mond", "nach", "den", "Ster\u00b7nen", "ge\u00b7scho\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+--+-+-+--+--+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Und erst sp\u00e4ter haben sie die Erfindung der Holzkugel gemacht.", "tokens": ["Und", "erst", "sp\u00e4\u00b7ter", "ha\u00b7ben", "sie", "die", "Er\u00b7fin\u00b7dung", "der", "Holz\u00b7ku\u00b7gel", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "PPER", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "--+-+--+-+--+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "N\u00e4mlich das kam so: Mit dem Holzkopf der Gott \u2013", "tokens": ["N\u00e4m\u00b7lich", "das", "kam", "so", ":", "Mit", "dem", "Holz\u00b7kopf", "der", "Gott", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VVFIN", "ADV", "$.", "APPR", "ART", "NN", "ART", "NN", "$("], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Wie hie\u00df er doch gleich? jedenfalls wars kein christlicher \u2013", "tokens": ["Wie", "hie\u00df", "er", "doch", "gleich", "?", "je\u00b7den\u00b7falls", "wars", "kein", "christ\u00b7li\u00b7cher", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADV", "$.", "ADV", "VAFIN", "PIAT", "ADJA", "$("], "meter": "-+---+--+-+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der heilige Geist trieb wieder einmal mit den heiligsten Dingen", "tokens": ["Der", "hei\u00b7li\u00b7ge", "Geist", "trieb", "wie\u00b7der", "ein\u00b7mal", "mit", "den", "hei\u00b7ligs\u00b7ten", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}}, "stanza.3": {"line.1": {"text": "seinen unw\u00fcrdigen Spott,", "tokens": ["sei\u00b7nen", "un\u00b7w\u00fcr\u00b7di\u00b7gen", "Spott", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-++--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Bezweifelte sich selbst, die unbefleckte Empf\u00e4ngnis \u2013 kurz und gut,", "tokens": ["Be\u00b7zwei\u00b7fel\u00b7te", "sich", "selbst", ",", "die", "un\u00b7be\u00b7fleck\u00b7te", "Emp\u00b7f\u00e4ng\u00b7nis", "\u2013", "kurz", "und", "gut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "$,", "ART", "ADJA", "NN", "$(", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+--+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Der betreffende Gott war sprachlos und verlor seinen Kopf.", "tokens": ["Der", "be\u00b7tref\u00b7fen\u00b7de", "Gott", "war", "sprach\u00b7los", "und", "ver\u00b7lor", "sei\u00b7nen", "Kopf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+--+--+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Aus Versehen schob Zeus mit ihm, und der Holzkopf erwies sich", "tokens": ["Aus", "Ver\u00b7se\u00b7hen", "schob", "Zeus", "mit", "ihm", ",", "und", "der", "Holz\u00b7kopf", "er\u00b7wies", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "NE", "APPR", "PPER", "$,", "KON", "ART", "NN", "VVFIN", "PRF"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}}, "stanza.5": {"line.1": {"text": "als unverw\u00fcstlicher denn (bzw. als) der Mond.", "tokens": ["als", "un\u00b7ver\u00b7w\u00fcst\u00b7li\u00b7cher", "denn", "(", "bzw.", "als", ")", "der", "Mond", "."], "token_info": ["word", "word", "word", "punct", "abbreviation", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "KON", "$(", "NE", "KOUS", "$(", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Vom Holz zum Eisen, von der Holzkugel zur Kanonenkugel ist nur ein Schritt.", "tokens": ["Vom", "Holz", "zum", "Ei\u0