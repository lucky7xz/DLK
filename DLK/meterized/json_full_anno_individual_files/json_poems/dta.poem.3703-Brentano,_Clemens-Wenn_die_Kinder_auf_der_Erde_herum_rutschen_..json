{"dta.poem.3703": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Wenn die Kinder auf der Erde herum  \n rutschen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Guck hin\u00fcber, fuff her\u00fcber,               ", "tokens": ["Guck", "hin\u00b7\u00fc\u00b7ber", ",", "fuff", "her\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wohl \u00fcber die Stra\u00df hinum,", "tokens": ["Wohl", "\u00fc\u00b7ber", "die", "Stra\u00df", "hi\u00b7num", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Kann Deutschland nicht finden,", "tokens": ["Kann", "Deutschland", "nicht", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Rutsch alleweil drauf rum.", "tokens": ["Rutsch", "al\u00b7le\u00b7weil", "drauf", "rum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PAV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}