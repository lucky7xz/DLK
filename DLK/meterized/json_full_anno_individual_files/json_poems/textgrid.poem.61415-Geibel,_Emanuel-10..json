{"textgrid.poem.61415": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Glaubt mir das eine: Das Recht ist nicht hier, und das Recht ist nicht dorten,", "tokens": ["Glaubt", "mir", "das", "ei\u00b7ne", ":", "Das", "Recht", "ist", "nicht", "hier", ",", "und", "das", "Recht", "ist", "nicht", "dor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "ART", "$.", "ART", "NN", "VAFIN", "PTKNEG", "ADV", "$,", "KON", "ART", "NN", "VAFIN", "PTKNEG", "ADV", "$,"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Aber der feurige Streit st\u00e4hlet und z\u00fcgelt die Kraft.", "tokens": ["A\u00b7ber", "der", "feu\u00b7ri\u00b7ge", "Streit", "st\u00e4h\u00b7let", "und", "z\u00fc\u00b7gelt", "die", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Wie kreuzweis im Geweb' sich die feindlichen F\u00e4den begegnen,", "tokens": ["Wie", "kreuz\u00b7weis", "im", "Ge\u00b7web'", "sich", "die", "feind\u00b7li\u00b7chen", "F\u00e4\u00b7den", "be\u00b7geg\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPRART", "NN", "PRF", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+---+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wirkt sich der Tag aus dem Kampf zweier Gedanken das Kleid.", "tokens": ["Wirkt", "sich", "der", "Tag", "aus", "dem", "Kampf", "zwei\u00b7er", "Ge\u00b7dan\u00b7ken", "das", "Kleid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "APPR", "ART", "NN", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Rastlos rollet der Wagen der Zeit, doch da\u00df er nicht st\u00fcrze,", "tokens": ["Rast\u00b7los", "rol\u00b7let", "der", "Wa\u00b7gen", "der", "Zeit", ",", "doch", "da\u00df", "er", "nicht", "st\u00fcr\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ART", "NN", "$,", "ADV", "KOUS", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "+-+--+--+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Hat ihm der waltende Geist doppelte Lenker gesellt.", "tokens": ["Hat", "ihm", "der", "wal\u00b7ten\u00b7de", "Geist", "dop\u00b7pel\u00b7te", "Len\u00b7ker", "ge\u00b7sellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Gei\u00dfelt der eine zu wild das Gespann in die st\u00e4ubende Rennbahn,", "tokens": ["Gei\u00b7\u00dfelt", "der", "ei\u00b7ne", "zu", "wild", "das", "Ge\u00b7spann", "in", "die", "st\u00e4u\u00b7ben\u00b7de", "Renn\u00b7bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ART", "PTKA", "ADJD", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "H\u00e4lt der andre daf\u00fcr straffer den hemmenden Zaum.", "tokens": ["H\u00e4lt", "der", "and\u00b7re", "da\u00b7f\u00fcr", "straf\u00b7fer", "den", "hem\u00b7men\u00b7den", "Zaum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "PIS", "PAV", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "Und so r\u00fccken wir dennoch vom Ort, und der Gott der Geschichte", "tokens": ["Und", "so", "r\u00fc\u00b7cken", "wir", "den\u00b7noch", "vom", "Ort", ",", "und", "der", "Gott", "der", "Ge\u00b7schich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,", "KON", "ART", "NN", "ART", "NN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.10": {"text": "F\u00fcgt es nach ew'gem Gesetz anders, als beide gedacht.", "tokens": ["F\u00fcgt", "es", "nach", "ew'\u00b7gem", "Ge\u00b7setz", "an\u00b7ders", ",", "als", "bei\u00b7de", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "ADV", "$,", "KOUS", "PIS", "VVPP", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Glaubt mir das eine: Das Recht ist nicht hier, und das Recht ist nicht dorten,", "tokens": ["Glaubt", "mir", "das", "ei\u00b7ne", ":", "Das", "Recht", "ist", "nicht", "hier", ",", "und", "das", "Recht", "ist", "nicht", "dor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "ART", "$.", "ART", "NN", "VAFIN", "PTKNEG", "ADV", "$,", "KON", "ART", "NN", "VAFIN", "PTKNEG", "ADV", "$,"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Aber der feurige Streit st\u00e4hlet und z\u00fcgelt die Kraft.", "tokens": ["A\u00b7ber", "d