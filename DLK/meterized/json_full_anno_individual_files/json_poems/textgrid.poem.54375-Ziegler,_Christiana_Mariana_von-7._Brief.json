{"textgrid.poem.54375": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "7. Brief", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Brich an gew\u00fcnschter Tag, verk\u00fcrze mein Verlangen;", "tokens": ["Brich", "an", "ge\u00b7w\u00fcnschter", "Tag", ",", "ver\u00b7k\u00fcr\u00b7ze", "mein", "Ver\u00b7lan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "O Tag! dem auf der Welt nichts zu vergleichen steht.", "tokens": ["O", "Tag", "!", "dem", "auf", "der", "Welt", "nichts", "zu", "ver\u00b7glei\u00b7chen", "steht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ART", "APPR", "ART", "NN", "PIS", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie z\u00e4rtlich werd ich dich, o Freundinn, nicht umfangen?", "tokens": ["Wie", "z\u00e4rt\u00b7lich", "werd", "ich", "dich", ",", "o", "Freun\u00b7dinn", ",", "nicht", "um\u00b7fan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "PRF", "$,", "FM", "NN", "$,", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ach! da\u00df nicht gleich ein Schiff durch Luft und Wolken geht!", "tokens": ["Ach", "!", "da\u00df", "nicht", "gleich", "ein", "Schiff", "durch", "Luft", "und", "Wol\u00b7ken", "geht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KOUS", "PTKNEG", "ADV", "ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das Blut in Adern wallt, das Innerste der Seelen", "tokens": ["Das", "Blut", "in", "A\u00b7dern", "wallt", ",", "das", "In\u00b7ners\u00b7te", "der", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$,", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nimmt Sehnsucht, Lieb, und Lust, ja fast Entz\u00fcckung ein.", "tokens": ["Nimmt", "Sehn\u00b7sucht", ",", "Lieb", ",", "und", "Lust", ",", "ja", "fast", "Ent\u00b7z\u00fc\u00b7ckung", "ein", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "NN", "$,", "KON", "NN", "$,", "ADV", "ADV", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch mein vergebner Wunsch sucht mich dabey zu qu\u00e4len;", "tokens": ["Doch", "mein", "ver\u00b7geb\u00b7ner", "Wunsch", "sucht", "mich", "da\u00b7bey", "zu", "qu\u00e4\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "PRF", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Hoffnung die verzeucht, mu\u00df auch mein Henker seyn.", "tokens": ["Die", "Hoff\u00b7nung", "die", "ver\u00b7zeucht", ",", "mu\u00df", "auch", "mein", "Hen\u00b7ker", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "VMFIN", "ADV", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Inde\u00df verehr ich stets nach wahrer Freundschaft Pflichten", "tokens": ["In\u00b7de\u00df", "ver\u00b7ehr", "ich", "stets", "nach", "wah\u00b7rer", "Freund\u00b7schaft", "Pflich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dein mehr als gutes Herz, und deine Trefflichkeit.", "tokens": ["Dein", "mehr", "als", "gu\u00b7tes", "Herz", ",", "und", "dei\u00b7ne", "Treff\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "PIAT", "KOKOM", "ADJA", "NN", "$,", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Drum kann ich anders nicht, als dir ein Loblied dichten;", "tokens": ["Drum", "kann", "ich", "an\u00b7ders", "nicht", ",", "als", "dir", "ein", "Lob\u00b7lied", "dich\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "PTKNEG", "$,", "KOUS", "PPER", "ART", "NN", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Denn das erfordert ja von mir die Billigkeit.", "tokens": ["Denn", "das", "er\u00b7for\u00b7dert", "ja", "von", "mir", "die", "Bil\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ADV", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Es ist l\u00e4ngst ausgemacht; das Gl\u00fcck ist nicht zu sch\u00e4tzen,", "tokens": ["Es", "ist", "l\u00e4ngst", "aus\u00b7ge\u00b7macht", ";", "das", "Gl\u00fcck", "ist", "nicht", "zu", "sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$.", "ART", "NN", "VAFIN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wer einen wahren Freund vor sich gefunden hat;", "tokens": ["Wer", "ei\u00b7nen", "wah\u00b7ren", "Freund", "vor", "sich", "ge\u00b7fun\u00b7den", "hat", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "APPR", "PRF", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Mit dem ist Geld und Gut nicht in Vergleich zu setzen:", "tokens": ["Mit", "dem", "ist", "Geld", "und", "Gut", "nicht", "in", "Ver\u00b7gleich", "zu", "set\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VAFIN", "NN", "KON", "NN", "PTKNEG", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und solchen edlen Trieb zeigst du auch in der That.", "tokens": ["Und", "sol\u00b7chen", "ed\u00b7len", "Trieb", "zeigst", "du", "auch", "in", "der", "That", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wie wohl ist meinem Geist! ich kann nicht unterliegen.", "tokens": ["Wie", "wohl", "ist", "mei\u00b7nem", "Geist", "!", "ich", "kann", "nicht", "un\u00b7ter\u00b7lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPOSAT", "NN", "$.", "PPER", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Weil deine Freundschaft stets vor meine Wohlfarth wacht,", "tokens": ["Weil", "dei\u00b7ne", "Freund\u00b7schaft", "stets", "vor", "mei\u00b7ne", "Wohl\u00b7farth", "wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So kann mein freyes Herz kein M\u00e4nnerblick besiegen,", "tokens": ["So", "kann", "mein", "frey\u00b7es", "Herz", "kein", "M\u00e4n\u00b7ner\u00b7blick", "be\u00b7sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "ADJA", "NN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Da mein Vergn\u00fcgen blo\u00df aus deinen Augen lacht.", "tokens": ["Da", "mein", "Ver\u00b7gn\u00fc\u00b7gen", "blo\u00df", "aus", "dei\u00b7nen", "Au\u00b7gen", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Vergi\u00df \u2013 \u2013 \u2013 nicht, die dich so hoch verehret,", "tokens": ["Ver\u00b7gi\u00df", "\u2013", "\u2013", "\u2013", "nicht", ",", "die", "dich", "so", "hoch", "ver\u00b7eh\u00b7ret", ","], "token_info": ["word", "punct", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$(", "$(", "$(", "PTKNEG", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "Vergi\u00df nicht, was du ihr so heilig zugesagt.", "tokens": ["Ver\u00b7gi\u00df", "nicht", ",", "was", "du", "ihr", "so", "hei\u00b7lig", "zu\u00b7ge\u00b7sagt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "$,", "PWS", "PPER", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Es bleibe weit entfernt was unsre Freundschaft st\u00f6ret;", "tokens": ["Es", "blei\u00b7be", "weit", "ent\u00b7fernt", "was", "uns\u00b7re", "Freund\u00b7schaft", "st\u00f6\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADJD", "PWS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "So mangelt mir kein Trost, wenn mich ein Kummer plagt.", "tokens": ["So", "man\u00b7gelt", "mir", "kein", "Trost", ",", "wenn", "mich", "ein", "Kum\u00b7mer", "plagt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Hochwohlgebohrne Frau, erlaubst du da\u00df ich gr\u00fcsse,", "tokens": ["Hoch\u00b7wohl\u00b7ge\u00b7bohr\u00b7ne", "Frau", ",", "er\u00b7laubst", "du", "da\u00df", "ich", "gr\u00fcs\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Den theuren Ehgemal, dein liebstes auf der Welt,", "tokens": ["Den", "theu\u00b7ren", "Eh\u00b7ge\u00b7mal", ",", "dein", "liebs\u00b7tes", "auf", "der", "Welt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "So wisse da\u00df ich dich davor ergebenst k\u00fcsse.", "tokens": ["So", "wis\u00b7se", "da\u00df", "ich", "dich", "da\u00b7vor", "er\u00b7ge\u00b7benst", "k\u00fcs\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOUS", "PPER", "PRF", "PAV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Ich unternehme nichts als was dir wohlgef\u00e4llt.", "tokens": ["Ich", "un\u00b7ter\u00b7neh\u00b7me", "nichts", "als", "was", "dir", "wohl\u00b7ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "KOKOM", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Indessen w\u00fcnsch ich dir so viel vergn\u00fcgte Tage,", "tokens": ["In\u00b7des\u00b7sen", "w\u00fcnsch", "ich", "dir", "so", "viel", "ver\u00b7gn\u00fcg\u00b7te", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Als V\u00f6gel in der Luft und Fisch im Meere ziehn.", "tokens": ["Als", "V\u00f6\u00b7gel", "in", "der", "Luft", "und", "Fisch", "im", "Mee\u00b7re", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "KON", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Genie\u00df der frohen Zeit, bis ich dir m\u00fcndlich sage:", "tokens": ["Ge\u00b7nie\u00df", "der", "fro\u00b7hen", "Zeit", ",", "bis", "ich", "dir", "m\u00fcnd\u00b7lich", "sa\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "KOUS", "PPER", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Ich will mich mit Bestand um deine Gunst bem\u00fchn.", "tokens": ["Ich", "will", "mich", "mit", "Be\u00b7stand", "um", "dei\u00b7ne", "Gunst", "be\u00b7m\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Brich an gew\u00fcnschter Tag, verk\u00fcrze mein Verlangen;", "tokens": ["Brich", "an", "ge\u00b7w\u00fcnschter", "Tag", ",", "ver\u00b7k\u00fcr\u00b7ze", "mein", "Ver\u00b7lan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "O Tag! dem auf der Welt nichts zu vergleichen steht.", "tokens": ["O", "Tag", "!", "dem", "auf", "der", "Welt", "nichts", "zu", "ver\u00b7glei\u00b7chen", "steht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ART", "APPR", "ART", "NN", "PIS", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie z\u00e4rtlich werd ich dich, o Freundinn, nicht umfangen?", "tokens": ["Wie", "z\u00e4rt\u00b7lich", "werd", "ich", "dich", ",", "o", "Freun\u00b7dinn", ",", "nicht", "um\u00b7fan\u00b7gen", "?"], "token_info": ["word", "word", "word", 