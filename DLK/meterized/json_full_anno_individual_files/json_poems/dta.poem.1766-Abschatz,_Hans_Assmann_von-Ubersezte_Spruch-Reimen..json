{"dta.poem.1766": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ubersezte Spruch-Reimen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Wer Lust zu H\u00e4ndeln hat/ entbl\u00f6\u00dfe nur das Eisen/", "tokens": ["Wer", "Lust", "zu", "H\u00e4n\u00b7deln", "hat", "/", "ent\u00b7bl\u00f6\u00b7\u00dfe", "nur", "das", "Ei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "APPR", "NN", "VAFIN", "$(", "VVFIN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es wird sich alsobald ein Widersacher weisen.", "tokens": ["Es", "wird", "sich", "al\u00b7so\u00b7bald", "ein", "Wi\u00b7der\u00b7sa\u00b7cher", "wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}