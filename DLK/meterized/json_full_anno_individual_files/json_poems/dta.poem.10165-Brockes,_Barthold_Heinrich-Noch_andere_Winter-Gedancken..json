{"dta.poem.10165": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Noch andere Winter-Gedancken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie hat es diese Nacht gereifft!", "tokens": ["Wie", "hat", "es", "die\u00b7se", "Nacht", "ge\u00b7reifft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein GOtt! wie grimmig starck mu\u00df es gefroren", "tokens": ["Mein", "Gott", "!", "wie", "grim\u00b7mig", "starck", "mu\u00df", "es", "ge\u00b7fro\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "PWAV", "ADJD", "ADJD", "VMFIN", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie schwirrt und schreit, wie knirrt und pfeifft", "tokens": ["Wie", "schwirrt", "und", "schreit", ",", "wie", "knirrt", "und", "pfeifft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "$,", "PWAV", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Schnee bey iedem Tritt! Mit den ietzt tr\u00e4gen Naben", "tokens": ["Der", "Schnee", "bey", "ie\u00b7dem", "Tritt", "!", "Mit", "den", "ietzt", "tr\u00e4\u00b7gen", "Na\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "$.", "APPR", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Knarrt, stockt, und schleppt der R\u00e4der starres Rund,", "tokens": ["Knarrt", ",", "stockt", ",", "und", "schleppt", "der", "R\u00e4\u00b7der", "star\u00b7res", "Rund", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$,", "KON", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ja wegert gleichsam sich, den kalten Grund,", "tokens": ["Ja", "we\u00b7gert", "gleich\u00b7sam", "sich", ",", "den", "kal\u00b7ten", "Grund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "ADJD", "PRF", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wie sonst, im drehen zu ber\u00fchren.", "tokens": ["Wie", "sonst", ",", "im", "dre\u00b7hen", "zu", "be\u00b7r\u00fch\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "APPRART", "VVINF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Fast alles drohet zu erfrieren;", "tokens": ["Fast", "al\u00b7les", "dro\u00b7het", "zu", "er\u00b7frie\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Fast alles droht f\u00fcr K\u00e4lte zu vergehn.", "tokens": ["Fast", "al\u00b7les", "droht", "f\u00fcr", "K\u00e4l\u00b7te", "zu", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie blendend wei\u00df ist alles, was ich schau,", "tokens": ["Wie", "blen\u00b7dend", "wei\u00df", "ist", "al\u00b7les", ",", "was", "ich", "schau", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "VAFIN", "PIS", "$,", "PWS", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So wol in Tieffen, als in H\u00f6hn!", "tokens": ["So", "wol", "in", "Tief\u00b7fen", ",", "als", "in", "H\u00f6hn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "$,", "KOUS", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie schwartz, wie dick, wie dunckel-grau", "tokens": ["Wie", "schwartz", ",", "wie", "dick", ",", "wie", "dun\u00b7ckel\u00b7grau"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADJD", "$,", "PWAV", "ADJD", "$,", "PWAV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hingegen ist der gantze Kreis der Lufft!", "tokens": ["Hin\u00b7ge\u00b7gen", "ist", "der", "gant\u00b7ze", "Kreis", "der", "Lufft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Zumahl da das noch niedre Sonnen-Licht", "tokens": ["Zu\u00b7mahl", "da", "das", "noch", "nied\u00b7re", "Son\u00b7nen\u00b7Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Annoch nicht durch die Nacht des dicken Nebels bricht.", "tokens": ["An\u00b7noch", "nicht", "durch", "die", "Nacht", "des", "di\u00b7cken", "Ne\u00b7bels", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Es scheint ob k\u00f6nne man, in einem greisen Dufft,", "tokens": ["Es", "scheint", "ob", "k\u00f6n\u00b7ne", "man", ",", "in", "ei\u00b7nem", "grei\u00b7sen", "Dufft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "VMFIN", "PIS", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die K\u00e4lte selbst anietzt recht sichtbar sehn.", "tokens": ["Die", "K\u00e4l\u00b7te", "selbst", "an\u00b7ietzt", "recht", "sicht\u00b7bar", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "ADJD", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie f\u00e4nget \u00fcberall ergrimmt an zu regieren.", "tokens": ["Sie", "f\u00e4n\u00b7get", "\u00fc\u00b7be\u00b7rall", "er\u00b7grimmt", "an", "zu", "re\u00b7gie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Drey Elemente selber m\u00fcssen", "tokens": ["Drey", "E\u00b7le\u00b7men\u00b7te", "sel\u00b7ber", "m\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "NN", "ADV", "VMFIN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Jhr schwer tyrannisch Joch versp\u00fchren,", "tokens": ["Ihr", "schwer", "ty\u00b7ran\u00b7nisch", "Joch", "ver\u00b7sp\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und deren B\u00fcrger all das strenge Scepter k\u00fcssen,", "tokens": ["Und", "de\u00b7ren", "B\u00fcr\u00b7ger", "all", "das", "stren\u00b7ge", "Scep\u00b7ter", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "PIAT", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der allem, was da lebt, Verl\u00e4hmung, Pein und Todt,", "tokens": ["Der", "al\u00b7lem", ",", "was", "da", "lebt", ",", "Ver\u00b7l\u00e4h\u00b7mung", ",", "Pein", "und", "Todt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$,", "PRELS", "ADV", "VVFIN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ja selber der Natur den Untergang fast droht.", "tokens": ["Ja", "sel\u00b7ber", "der", "Na\u00b7tur", "den", "Un\u00b7ter\u00b7gang", "fast", "droht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ART", "NN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die durch den scharffen Frost gepresste Lufft erstarrt.", "tokens": ["Die", "durch", "den", "scharf\u00b7fen", "Frost", "ge\u00b7press\u00b7te", "Lufft", "er\u00b7starrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Fluht wird Eisen-Fest. Die Erde Felsen-hart,", "tokens": ["Die", "Fluht", "wird", "Ei\u00b7sen\u00b7Fest", ".", "Die", "Er\u00b7de", "Fel\u00b7sen\u00b7hart", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$.", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ja felsicht in der That. Wenn man wol eh geh\u00f6rt,", "tokens": ["Ja", "fel\u00b7sicht", "in", "der", "That", ".", "Wenn", "man", "wol", "eh", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "APPR", "ART", "NN", "$.", "KOUS", "PIS", "ADV", "KOUS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df gantze St\u00e4dte sich in Stein verkehrt,", "tokens": ["Da\u00df", "gant\u00b7ze", "St\u00e4d\u00b7te", "sich", "in", "Stein", "ver\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PRF", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Erstaunet man darob. Die\u00df ist erstaunens wehrt,", "tokens": ["Er\u00b7stau\u00b7net", "man", "da\u00b7rob", ".", "Die\u00df", "ist", "er\u00b7stau\u00b7nens", "wehrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PAV", "$.", "PDS", "VAFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df nicht nur alles sich fast in der That", "tokens": ["Da\u00df", "nicht", "nur", "al\u00b7les", "sich", "fast", "in", "der", "That"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ADV", "PIS", "PRF", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "In Stein verwandelt hat,", "tokens": ["In", "Stein", "ver\u00b7wan\u00b7delt", "hat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und alles, was man sieht, ein starres Schreck-Bild weiset;", "tokens": ["Und", "al\u00b7les", ",", "was", "man", "sieht", ",", "ein", "star\u00b7res", "Schreck\u00b7Bild", "wei\u00b7set", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "PIS", "VVFIN", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nein, da\u00df, wie durch Erfahrung ja bekannt,", "tokens": ["Nein", ",", "da\u00df", ",", "wie", "durch", "Er\u00b7fah\u00b7rung", "ja", "be\u00b7kannt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "$,", "PWAV", "APPR", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Durch unsers Sch\u00f6pfers Allmachts Hand", "tokens": ["Durch", "un\u00b7sers", "Sch\u00f6p\u00b7fers", "All\u00b7machts", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Sich alles wieder\u00fcm entsteinet und enteiset.", "tokens": ["Sich", "al\u00b7les", "wie\u00b7de\u00b7r\u00fcm", "ent\u00b7stei\u00b7net", "und", "en\u00b7tei\u00b7set", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PIS", "KOKOM", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "\u201enimm doch GOttes weise Macht", "tokens": ["\u201e", "nimm", "doch", "Got\u00b7tes", "wei\u00b7se", "Macht"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VVIMP", "ADV", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "\u201each! so nimm, o Mensch, in Acht,", "tokens": ["\u201e", "ach", "!", "so", "nimm", ",", "o", "Mensch", ",", "in", "Acht", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ITJ", "$.", "ADV", "VVIMP", "$,", "FM", "NN", "$,", "APPR", "CARD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "\u201ewie des weisen Sch\u00f6pfers Macht,", "tokens": ["\u201e", "wie", "des", "wei\u00b7sen", "Sch\u00f6p\u00b7fers", "Macht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u201euns zum Nutz, und Jhm zum Preise,", "tokens": ["\u201e", "uns", "zum", "Nutz", ",", "und", "Jhm", "zum", "Prei\u00b7se", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "APPRART", "NN", "$,", "KON", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u201eauf so wunderbare Weise,", "tokens": ["\u201e", "auf", "so", "wun\u00b7der\u00b7ba\u00b7re", "Wei\u00b7se", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "\u201ein dem Felsen-gleichen Eise", "tokens": ["\u201e", "in", "dem", "Fel\u00b7sen\u00b7glei\u00b7chen", "Ei\u00b7se"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "\u201ewelches Er hervorgebracht,", "tokens": ["\u201e", "wel\u00b7ches", "Er", "her\u00b7vor\u00b7ge\u00b7bracht", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "\u201eden Verband so spr\u00f6d gemacht.", "tokens": ["\u201e", "den", "Ver\u00b7band", "so", "spr\u00f6d", "ge\u00b7macht", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Inde\u00df erzittert ietzt f\u00fcr\u2019s Frostes Grimm, und bebet", "tokens": ["In\u00b7de\u00df", "er\u00b7zit\u00b7tert", "ietzt", "f\u00fcr's", "Fros\u00b7tes", "Grimm", ",", "und", "be\u00b7bet"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "APPRART", "ADJA", "NE", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was in den L\u00fcfften fleucht, und was auf Erden lebet.", "tokens": ["Was", "in", "den", "L\u00fcff\u00b7ten", "fleucht", ",", "und", "was", "auf", "Er\u00b7den", "le\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "$,", "KON", "PWS", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es schneidet recht der Frost, er klemmt, er sticht und dr\u00fc", "tokens": ["Es", "schnei\u00b7det", "recht", "der", "Frost", ",", "er", "klemmt", ",", "er", "sticht", "und", "dr\u00fc"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "KON", "XY"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ja greiffet Haut und Fleisch, so scharff, so hefftig an,", "tokens": ["Ja", "greif\u00b7fet", "Haut", "und", "Fleisch", ",", "so", "scharff", ",", "so", "heff\u00b7tig", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "NN", "KON", "NN", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df es kein C\u00f6rper leicht erdulden kann;", "tokens": ["Da\u00df", "es", "kein", "C\u00f6r\u00b7per", "leicht", "er\u00b7dul\u00b7den", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Indem er ihn offt brennt, offt gar ersticket.", "tokens": ["In\u00b7dem", "er", "ihn", "offt", "brennt", ",", "offt", "gar", "er\u00b7sti\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVFIN", "$,", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Doch setzet GOtt des wilden W\u00fctrichs Wuth,", "tokens": ["Doch", "set\u00b7zet", "Gott", "des", "wil\u00b7den", "W\u00fct\u00b7richs", "Wuth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zu einer Linderung, nicht nur die rege Glut,", "tokens": ["Zu", "ei\u00b7ner", "Lin\u00b7de\u00b7rung", ",", "nicht", "nur", "die", "re\u00b7ge", "Glut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PTKNEG", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und einen warmen Peltz entgegen;", "tokens": ["Und", "ei\u00b7nen", "war\u00b7men", "Peltz", "ent\u00b7ge\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es steckt so gar, blo\u00df im Bewegen,", "tokens": ["Es", "steckt", "so", "gar", ",", "blo\u00df", "im", "Be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Ein wolfeil Mittel, das uns n\u00fctzet,", "tokens": ["Ein", "wol\u00b7feil", "Mit\u00b7tel", ",", "das", "uns", "n\u00fct\u00b7zet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das auch den d\u00fcrftigen besch\u00fctzet,", "tokens": ["Das", "auch", "den", "d\u00fcrf\u00b7ti\u00b7gen", "be\u00b7sch\u00fct\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Jhm die zu hefftige Gewalt des Frostes mindert,", "tokens": ["Jhm", "die", "zu", "heff\u00b7ti\u00b7ge", "Ge\u00b7walt", "des", "Fros\u00b7tes", "min\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "PTKZU", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und die, dadurch ihm sonst erregte, Schmertzen lindert,", "tokens": ["Und", "die", ",", "da\u00b7durch", "ihm", "sonst", "er\u00b7reg\u00b7te", ",", "Schmert\u00b7zen", "lin\u00b7dert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "PAV", "PPER", "ADV", "VVFIN", "$,", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ja gar den kalten Tod,", "tokens": ["Ja", "gar", "den", "kal\u00b7ten", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Den ein zu strenger Frost ihm \u00f6ffters droht,", "tokens": ["Den", "ein", "zu", "stren\u00b7ger", "Frost", "ihm", "\u00f6ff\u00b7ters", "droht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "PTKZU", "ADJA", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Durch offnen Weg der D\u00fcnste, von ihm treibet.", "tokens": ["Durch", "off\u00b7nen", "Weg", "der", "D\u00fcns\u00b7te", ",", "von", "ihm", "trei\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$,", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Wann aber doch die Schmertzen und die Plagen,", "tokens": ["Wann", "a\u00b7ber", "doch", "die", "Schmert\u00b7zen", "und", "die", "Pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die langer Frost erregt, kaum m\u00f6glich zu ertragen;", "tokens": ["Die", "lan\u00b7ger", "Frost", "er\u00b7regt", ",", "kaum", "m\u00f6g\u00b7lich", "zu", "er\u00b7tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So wollest Du, o GOTT, dich dieser Zeit der Armen,", "tokens": ["So", "wol\u00b7lest", "Du", ",", "o", "GoTT", ",", "dich", "die\u00b7ser", "Zeit", "der", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "FM", "NE", "$,", "PRF", "PDAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die sonder Feuer, Kost und Kleid sind, doch erbarmen!", "tokens": ["Die", "son\u00b7der", "Feu\u00b7er", ",", "Kost", "und", "Kleid", "sind", ",", "doch", "er\u00b7bar\u00b7men", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "NN", "KON", "NN", "VAFIN", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "La\u00df aber, lieber Mensch, auch du, so viel an dir,", "tokens": ["La\u00df", "a\u00b7ber", ",", "lie\u00b7ber", "Mensch", ",", "auch", "du", ",", "so", "viel", "an", "dir", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "ADV", "NN", "$,", "ADV", "PPER", "$,", "ADV", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein Hertz zum Mittleid doch bewegen:", "tokens": ["Dein", "Hertz", "zum", "Mitt\u00b7leid", "doch", "be\u00b7we\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Damit dein Liebes-Feur dein armer N\u00e4chster sp\u00fchr:", "tokens": ["Da\u00b7mit", "dein", "Lie\u00b7bes\u00b7Feur", "dein", "ar\u00b7mer", "N\u00e4chs\u00b7ter", "sp\u00fchr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Komm, lindre seine Noth, mit deinem Segen:", "tokens": ["Komm", ",", "lind\u00b7re", "sei\u00b7ne", "Noth", ",", "mit", "dei\u00b7nem", "Se\u00b7gen", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Such ihm in scharffem Frost ein Labsal zu bereiten,", "tokens": ["Such", "ihm", "in", "scharf\u00b7fem", "Frost", "ein", "Lab\u00b7sal", "zu", "be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Damit, wie Hiob spricht, auch seine Seiten,", "tokens": ["Da\u00b7mit", ",", "wie", "Hiob", "spricht", ",", "auch", "sei\u00b7ne", "Sei\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PWAV", "NE", "VVFIN", "$,", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-++-+-+-", "measure": "unknown.measure.penta"}, "line.7": {"text": "Wenn sie durch deine H\u00fclff erw\u00e4rmt, dich preisen,", "tokens": ["Wenn", "sie", "durch", "dei\u00b7ne", "H\u00fclff", "er\u00b7w\u00e4rmt", ",", "dich", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und so, durch dich, dem Sch\u00f6pfer Danck erweisen.", "tokens": ["Und", "so", ",", "durch", "dich", ",", "dem", "Sch\u00f6p\u00b7fer", "Danck", "er\u00b7wei\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPR", "PPER", "$,", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}