{"textgrid.poem.59619": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Der Freudenklang", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch Deutschland flog ein heller Klang", "tokens": ["Durch", "Deutschland", "flog", "ein", "hel\u00b7ler", "Klang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Vom S\u00fcden bis zum Norden,", "tokens": ["Vom", "S\u00fc\u00b7den", "bis", "zum", "Nor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Ehrenklang, ein Freiheitsklang", "tokens": ["Ein", "Eh\u00b7ren\u00b7klang", ",", "ein", "Frei\u00b7heits\u00b7klang"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist laut geklungen worden:", "tokens": ["Ist", "laut", "ge\u00b7klun\u00b7gen", "wor\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVPP", "VAPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Drum auf, ihr M\u00e4nner! Auf ins Feld!", "tokens": ["Drum", "auf", ",", "ihr", "M\u00e4n\u00b7ner", "!", "Auf", "ins", "Feld", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "PTKVZ", "$,", "PPOSAT", "NN", "$.", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drum auf, ihr deutschen Br\u00fcder!", "tokens": ["Drum", "auf", ",", "ihr", "deut\u00b7schen", "Br\u00fc\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die B\u00f6sen hat der Herr gef\u00e4llt,", "tokens": ["Die", "B\u00f6\u00b7sen", "hat", "der", "Herr", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Gl\u00fcck ersteht nicht wieder \u2013", "tokens": ["Ihr", "Gl\u00fcck", "er\u00b7steht", "nicht", "wie\u00b7der", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKNEG", "ADV", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Drum auf mit Jubelschalle!", "tokens": ["Drum", "auf", "mit", "Ju\u00b7bel\u00b7schal\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und ruft und schw\u00f6ret alle:", "tokens": ["Und", "ruft", "und", "schw\u00f6\u00b7ret", "al\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PIS", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nicht Bayern und nicht Sachsen mehr,", "tokens": ["Nicht", "Bay\u00b7ern", "und", "nicht", "Sach\u00b7sen", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NE", "KON", "PTKNEG", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht \u00d6streich und nicht Preu\u00dfen,", "tokens": ["Nicht", "\u00d6s\u00b7treich", "und", "nicht", "Preu\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "KON", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Land, ein Volk, ein Herz, ein Heer,", "tokens": ["Ein", "Land", ",", "ein", "Volk", ",", "ein", "Herz", ",", "ein", "Heer", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir wollen Deutsche hei\u00dfen;", "tokens": ["Wir", "wol\u00b7len", "Deut\u00b7sche", "hei\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Als echte deutsche Br\u00fcder", "tokens": ["Als", "ech\u00b7te", "deut\u00b7sche", "Br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Haun wir die R\u00e4uber nieder,", "tokens": ["Haun", "wir", "die", "R\u00e4u\u00b7ber", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die unsre Ehr' zerrei\u00dfen.", "tokens": ["Die", "uns\u00b7re", "Ehr'", "zer\u00b7rei\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "In gleicher Liebe fest und treu,", "tokens": ["In", "glei\u00b7cher", "Lie\u00b7be", "fest", "und", "treu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In einem Bund geschlossen,", "tokens": ["In", "ei\u00b7nem", "Bund", "ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Welschen, ziehen wir herbei", "tokens": ["Ihr", "Wel\u00b7schen", ",", "zie\u00b7hen", "wir", "her\u00b7bei"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit M\u00e4nnern und mit Rossen:", "tokens": ["Mit", "M\u00e4n\u00b7nern", "und", "mit", "Ros\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie Herbstesst\u00fcrme brausen", "tokens": ["Wie", "Herbs\u00b7tess\u00b7t\u00fcr\u00b7me", "brau\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und wilde Meere sausen,", "tokens": ["Und", "wil\u00b7de", "Mee\u00b7re", "sau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "So kommen wir geflossen.", "tokens": ["So", "kom\u00b7men", "wir", "ge\u00b7flos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "So kommen wir, so brausen wir", "tokens": ["So", "kom\u00b7men", "wir", ",", "so", "brau\u00b7sen", "wir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und schw\u00f6ren rote Rache,", "tokens": ["Und", "schw\u00f6\u00b7ren", "ro\u00b7te", "Ra\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und Gott der Herr ist mit uns hier", "tokens": ["Und", "Gott", "der", "Herr", "ist", "mit", "uns", "hier"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "ART", "NN", "VAFIN", "APPR", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und h\u00e4lt die gute Sache;", "tokens": ["Und", "h\u00e4lt", "die", "gu\u00b7te", "Sa\u00b7che", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Herr der Himmelsscharen", "tokens": ["Der", "Herr", "der", "Him\u00b7mels\u00b7scha\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wird Recht und Licht bewahren,", "tokens": ["Wird", "Recht", "und", "Licht", "be\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Vor ihm erliegt der Drache.", "tokens": ["Vor", "ihm", "er\u00b7liegt", "der", "Dra\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Mit diesem Glauben ziehn wir aus", "tokens": ["Mit", "die\u00b7sem", "Glau\u00b7ben", "ziehn", "wir", "aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPER", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als rechte deutsche Br\u00fcder,", "tokens": ["Als", "rech\u00b7te", "deut\u00b7sche", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In Deutschland stand der Freiheit Haus,", "tokens": ["In", "Deutschland", "stand", "der", "Frei\u00b7heit", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wir baun es tapfer wieder,", "tokens": ["Wir", "baun", "es", "tap\u00b7fer", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In Fahr und Todesflammen", "tokens": ["In", "Fahr", "und", "To\u00b7des\u00b7flam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wir baun es k\u00fchn zusammen,", "tokens": ["Wir", "baun", "es", "k\u00fchn", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Kein Teufel rei\u00dft es nieder.", "tokens": ["Kein", "Teu\u00b7fel", "rei\u00dft", "es", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Durch Deutschland flog ein heller Klang", "tokens": ["Durch", "Deutschland", "flog", "ein", "hel\u00b7ler", "Klang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Vom S\u0