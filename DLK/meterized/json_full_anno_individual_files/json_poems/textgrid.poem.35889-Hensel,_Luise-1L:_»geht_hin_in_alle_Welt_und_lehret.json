{"textgrid.poem.35889": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbgeht hin in alle Welt und lehret", "genre": "verse", "period": "N.A.", "pub_year": 1820, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbgeht hin in alle Welt und lehret", "tokens": ["\u00bb", "geht", "hin", "in", "al\u00b7le", "Welt", "und", "leh\u00b7ret"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "ADV", "APPR", "PIAT", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die V\u00f6lker, die im Dunkeln gehn.", "tokens": ["Die", "V\u00f6l\u00b7ker", ",", "die", "im", "Dun\u00b7keln", "gehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sagt laut, was ihr durch Mich geh\u00f6ret,", "tokens": ["Sagt", "laut", ",", "was", "ihr", "durch", "Mich", "ge\u00b7h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "PWS", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bezeugt, was ihr von Mir gesehn.\u00ab", "tokens": ["Be\u00b7zeugt", ",", "was", "ihr", "von", "Mir", "ge\u00b7sehn", ".", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVPP", "$,", "PWS", "PPER", "APPR", "NE", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbtauft sie in des Dreieinen Namen,", "tokens": ["\u00bb", "tauft", "sie", "in", "des", "Drei\u00b7ei\u00b7nen", "Na\u00b7men", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Macht ihnen Meinen Bund bekannt", "tokens": ["Macht", "ih\u00b7nen", "Mei\u00b7nen", "Bund", "be\u00b7kannt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und streut der Christentugend Samen", "tokens": ["Und", "streut", "der", "Chris\u00b7ten\u00b7tu\u00b7gend", "Sa\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In jedes fruchtbar weiche Land.\u00ab", "tokens": ["In", "je\u00b7des", "frucht\u00b7bar", "wei\u00b7che", "Land", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PIAT", "ADJD", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dies und noch viele hohe Worte", "tokens": ["Dies", "und", "noch", "vie\u00b7le", "ho\u00b7he", "Wor\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "KON", "ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sprach unser Herr zur J\u00fcngerschaar,", "tokens": ["Sprach", "un\u00b7ser", "Herr", "zur", "J\u00fcn\u00b7ger\u00b7schaar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als Er des Todes dunkler Pforte", "tokens": ["Als", "Er", "des", "To\u00b7des", "dunk\u00b7ler", "Pfor\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Licht der Welt erstanden war.", "tokens": ["Zum", "Licht", "der", "Welt", "er\u00b7stan\u00b7den", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Dann lie\u00df der treue Hirt die Heerde", "tokens": ["Dann", "lie\u00df", "der", "treu\u00b7e", "Hirt", "die", "Heer\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und ging hinauf zum ew'gen Licht.", "tokens": ["Und", "ging", "hin\u00b7auf", "zum", "ew'\u00b7gen", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie sah'n nun auf der weiten Erde", "tokens": ["Sie", "sah'n", "nun", "auf", "der", "wei\u00b7ten", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Fortan den treuen F\u00fchrer nicht.", "tokens": ["For\u00b7tan", "den", "treu\u00b7en", "F\u00fch\u00b7rer", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Da war ihr Lieben, ihr Verlangen", "tokens": ["Da", "war", "ihr", "Lie\u00b7ben", ",", "ihr", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von dieser Welt hinweggekehrt;", "tokens": ["Von", "die\u00b7ser", "Welt", "hin\u00b7weg\u00b7ge\u00b7kehrt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da sind sie treulich hingegangen", "tokens": ["Da", "sind", "sie", "treu\u00b7lich", "hin\u00b7ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und haben alle Welt gelehrt.", "tokens": ["Und", "ha\u00b7ben", "al\u00b7le", "Welt", "ge\u00b7lehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und haben ihres Wortes Wahrheit", "tokens": ["Und", "ha\u00b7ben", "ih\u00b7res", "Wor\u00b7tes", "Wahr\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bezeugt durch Wunder, Qual und Tod.", "tokens": ["Be\u00b7zeugt", "durch", "Wun\u00b7der", ",", "Qual", "und", "Tod", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch zeigt den Weg zur ew'gen Klarheit", "tokens": ["Noch", "zeigt", "den", "Weg", "zur", "ew'\u00b7gen", "Klar\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Uns ihres Blutes Morgenroth.", "tokens": ["Uns", "ih\u00b7res", "Blu\u00b7tes", "Mor\u00b7gen\u00b7roth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Drum la\u00dft uns ihren Staub noch ehren", "tokens": ["Drum", "la\u00dft", "uns", "ih\u00b7ren", "Staub", "noch", "eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und achtsam auf ihr Beispiel sehn", "tokens": ["Und", "acht\u00b7sam", "auf", "ihr", "Bei\u00b7spiel", "sehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und treu nach ihren heil'gen Lehren", "tokens": ["Und", "treu", "nach", "ih\u00b7ren", "heil'\u00b7gen", "Leh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den steilen Pfad zur Heimath gehn.", "tokens": ["Den", "stei\u00b7len", "Pfad", "zur", "Hei\u00b7math", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und la\u00dft uns ringen, wie sie rangen,", "tokens": ["Und", "la\u00dft", "uns", "rin\u00b7gen", ",", "wie", "sie", "ran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nur trachtend nach dem ew'gen Licht,", "tokens": ["Nur", "trach\u00b7tend", "nach", "dem", "ew'\u00b7gen", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und nicht mehr an der Erde hangen", "tokens": ["Und", "nicht", "mehr", "an", "der", "Er\u00b7de", "han\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "ADV", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ihre Leiden f\u00fcrchten nicht;", "tokens": ["Und", "ih\u00b7re", "Lei\u00b7den", "f\u00fcrch\u00b7ten", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Und wenn uns winken ihre Freuden,", "tokens": ["Und", "wenn", "uns", "win\u00b7ken", "ih\u00b7re", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nur treu und fest auf Jesum sehn", "tokens": ["Nur", "treu", "und", "fest", "auf", "Je\u00b7sum", "sehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADJD", "APPR", "NE", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und gern f\u00fcr Ihn durch Kampf und Leiden,", "tokens": ["Und", "gern", "f\u00fcr", "Ihn", "durch", "Kampf", "und", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Seine heil'gen Zw\u00f6lfe gehn. \u2013", "tokens": ["Wie", "Sei\u00b7ne", "heil'\u00b7gen", "Zw\u00f6l\u00b7fe", "gehn", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Und Du, mein Kind, das Gott gegeben,", "tokens": ["Und", "Du", ",", "mein", "Kind", ",", "das", "Gott", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPOSAT", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du kleiner Pilger, sei gegr\u00fc\u00dft!", "tokens": ["Du", "klei\u00b7ner", "Pil\u00b7ger", ",", "sei", "ge\u00b7gr\u00fc\u00dft", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Geh' hin durch dieses Pr\u00fcfungsleben,", "tokens": ["Geh'", "hin", "durch", "die\u00b7ses", "Pr\u00fc\u00b7fungs\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Freu' Dich und leide als ein Christ!", "tokens": ["Freu'", "Dich", "und", "lei\u00b7de", "als", "ein", "Christ", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Dich hat der gute Herr gegeben", "tokens": ["Dich", "hat", "der", "gu\u00b7te", "Herr", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Jetzt, wo Er so viel Blumen giebt.", "tokens": ["Jetzt", ",", "wo", "Er", "so", "viel", "Blu\u00b7men", "giebt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Willkommen denn, Du s\u00fc\u00dfes Leben!", "tokens": ["Will\u00b7kom\u00b7men", "denn", ",", "Du", "s\u00fc\u00b7\u00dfes", "Le\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du kommst vom Vater, der uns liebt.", "tokens": ["Du", "kommst", "vom", "Va\u00b7ter", ",", "der", "uns", "liebt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Sei mir gesegnet, zarte Bl\u00fcthe,", "tokens": ["Sei", "mir", "ge\u00b7seg\u00b7net", ",", "zar\u00b7te", "Bl\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die einst \u2013 ich hoffe \u2013 Frucht erwirbt! \u2013", "tokens": ["Die", "einst", "\u2013", "ich", "hof\u00b7fe", "\u2013", "Frucht", "er\u00b7wirbt", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "ADV", "$(", "PPER", "VVFIN", "$(", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df Gott Dich vor dem Wurm beh\u00fcte,", "tokens": ["Da\u00df", "Gott", "Dich", "vor", "dem", "Wurm", "be\u00b7h\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4