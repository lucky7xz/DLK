{"textgrid.poem.39764": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "9. R\u00fcckblick", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit zwanzig leichten Lenzen", "tokens": ["Mit", "zwan\u00b7zig", "leich\u00b7ten", "Len\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "CARD", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Lag ich in diesem Wald,", "tokens": ["Lag", "ich", "in", "die\u00b7sem", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und seh' ihn heute gl\u00e4nzen", "tokens": ["Und", "seh'", "ihn", "heu\u00b7te", "gl\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In gleicher Lichtgestalt!", "tokens": ["In", "glei\u00b7cher", "Licht\u00b7ge\u00b7stalt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es duften seine W\u00fcrzen", "tokens": ["Es", "duf\u00b7ten", "sei\u00b7ne", "W\u00fcr\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und seine B\u00e4che st\u00fcrzen,", "tokens": ["Und", "sei\u00b7ne", "B\u00e4\u00b7che", "st\u00fcr\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ja, nimmer wird er alt.", "tokens": ["Ja", ",", "nim\u00b7mer", "wird", "er", "alt", "."], "toke