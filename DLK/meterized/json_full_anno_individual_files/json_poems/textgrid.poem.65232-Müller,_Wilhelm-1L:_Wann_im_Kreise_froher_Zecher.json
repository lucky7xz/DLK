{"textgrid.poem.65232": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wann im Kreise froher Zecher", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann im Kreise froher Zecher", "tokens": ["Wann", "im", "Krei\u00b7se", "fro\u00b7her", "Ze\u00b7cher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich in meinen vollen Becher", "tokens": ["Ich", "in", "mei\u00b7nen", "vol\u00b7len", "Be\u00b7cher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schaue hellen Blicks hinein,", "tokens": ["Schau\u00b7e", "hel\u00b7len", "Blicks", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wann um mich die Gl\u00e4ser klingen,", "tokens": ["Wann", "um", "mich", "die", "Gl\u00e4\u00b7ser", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "KOUI", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und die Freunde Lieder singen", "tokens": ["Und", "die", "Freun\u00b7de", "Lie\u00b7der", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dir zur Ehren, deutscher Wein!", "tokens": ["Dir", "zur", "Eh\u00b7ren", ",", "deut\u00b7scher", "Wein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dann, dann steht's vor meinen Blicken,", "tokens": ["Dann", ",", "dann", "steht's", "vor", "mei\u00b7nen", "Bli\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie die goldnen Trauben nicken", "tokens": ["Wie", "die", "gold\u00b7nen", "Trau\u00b7ben", "ni\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nieder in den klaren Flu\u00df,", "tokens": ["Nie\u00b7der", "in", "den", "kla\u00b7ren", "Flu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie die Wogen lustig rauschen,", "tokens": ["Wie", "die", "Wo\u00b7gen", "lus\u00b7tig", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und die Winzerinnen lauschen", "tokens": ["Und", "die", "Win\u00b7ze\u00b7rin\u00b7nen", "lau\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf des Fischers Abendgru\u00df.", "tokens": ["Auf", "des", "Fi\u00b7schers", "A\u00b7bend\u00b7gru\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und der Mond am stillen Himmel", "tokens": ["Und", "der", "Mond", "am", "stil\u00b7len", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Freut sich mit an dem Get\u00fcmmel,", "tokens": ["Freut", "sich", "mit", "an", "dem", "Ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Das er auf der Erden sieht,", "tokens": ["Das", "er", "auf", "der", "Er\u00b7den", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "An den F\u00e4ssern mit den Kr\u00e4nzen,", "tokens": ["An", "den", "F\u00e4s\u00b7sern", "mit", "den", "Kr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "An den Liedern und den T\u00e4nzen,", "tokens": ["An", "den", "Lie\u00b7dern", "und", "den", "T\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bis er sacht von dannen zieht.", "tokens": ["Bis", "er", "sacht", "von", "dan\u00b7nen", "zieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Z\u00fcndet an die bunten Lichter,", "tokens": ["Z\u00fcn\u00b7det", "an", "die", "bun\u00b7ten", "Lich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df die seligen Gesichter", "tokens": ["Da\u00df", "die", "se\u00b7li\u00b7gen", "Ge\u00b7sich\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht die finstre Nacht bedeckt!", "tokens": ["Nicht", "die", "finst\u00b7re", "Nacht", "be\u00b7deckt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wer zu selig f\u00fcr das Helle,", "tokens": ["Wer", "zu", "se\u00b7lig", "f\u00fcr", "das", "Hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKA", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sucht sich eine dunkle Stelle,", "tokens": ["Sucht", "sich", "ei\u00b7ne", "dunk\u00b7le", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo kein N\u00fcchterner ihn neckt.", "tokens": ["Wo", "kein", "N\u00fcch\u00b7ter\u00b7ner", "ihn", "neckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "PPER", "VVFIN", "$."], "meter": "--+---+", "measure": "anapaest.init"}}, "stanza.5": {"line.1": {"text": "Auch die Liebe kennt viel Wege", "tokens": ["Auch", "die", "Lie\u00b7be", "kennt", "viel", "We\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem gr\u00fcnen Weingehege,", "tokens": ["In", "dem", "gr\u00fc\u00b7nen", "Wein\u00b7ge\u00