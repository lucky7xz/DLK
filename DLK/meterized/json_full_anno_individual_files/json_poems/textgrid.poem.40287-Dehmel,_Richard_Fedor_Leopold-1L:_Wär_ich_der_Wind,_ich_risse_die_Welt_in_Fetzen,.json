{"textgrid.poem.40287": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: W\u00e4r ich der Wind, ich risse die Welt in Fetzen,", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4r ich der Wind, ich risse die Welt in Fetzen,", "tokens": ["W\u00e4r", "ich", "der", "Wind", ",", "ich", "ris\u00b7se", "die", "Welt", "in", "Fet\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "w\u00e4r ich das Feuer, zerfr\u00e4\u00df ich sie zu Funken,", "tokens": ["w\u00e4r", "ich", "das", "Feu\u00b7er", ",", "zer\u00b7fr\u00e4\u00df", "ich", "sie", "zu", "Fun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "VVFIN", "PPER", "PPER", "APPR", "NN", "$,"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "w\u00e4r ich das Meer, sie l\u00e4ge l\u00e4ngst versunken,", "tokens": ["w\u00e4r", "ich", "das", "Meer", ",", "sie", "l\u00e4\u00b7ge", "l\u00e4ngst", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "PPER", "VVFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "w\u00e4re ich Gott, Spa\u00df: g\u00e4b das ein Entsetzen!", "tokens": ["w\u00e4\u00b7re", "ich", "Gott", ",", "Spa\u00df", ":", "g\u00e4b", "das", "ein", "Ent\u00b7set\u00b7zen", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word",