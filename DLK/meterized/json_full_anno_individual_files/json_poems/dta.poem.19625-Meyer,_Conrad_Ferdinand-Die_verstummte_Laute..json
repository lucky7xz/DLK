{"dta.poem.19625": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Die verstummte Laute.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Sie mochte gern an seiner Schulter lehnen", "tokens": ["Sie", "moch\u00b7te", "gern", "an", "sei\u00b7ner", "Schul\u00b7ter", "leh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In einem weichen Abendd\u00e4mmerlicht,", "tokens": ["In", "ei\u00b7nem", "wei\u00b7chen", "A\u00b7bend\u00b7d\u00e4m\u00b7mer\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie barg vor ihm das Rieseln ihrer Thr\u00e4nen,", "tokens": ["Sie", "barg", "vor", "ihm", "das", "Rie\u00b7seln", "ih\u00b7rer", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den halbenth\u00fcllten Reiz der Seele nicht:", "tokens": ["Den", "hal\u00b7bent\u00b7h\u00fcll\u00b7ten", "Reiz", "der", "See\u00b7le", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "\u201efreund, einz'ger Freund auf diesem d\u00fcstern Eiland,", "tokens": ["\u201e", "freund", ",", "einz'\u00b7ger", "Freund", "auf", "die\u00b7sem", "d\u00fcs\u00b7tern", "Ei\u00b7land", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "ADJA", "NN", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Ich welke! Chastelard, auch du bist bleich!", "tokens": ["Ich", "wel\u00b7ke", "!", "Chas\u00b7te\u00b7lard", ",", "auch", "du", "bist", "bleich", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NE", "$,", "ADV", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Schlag deine Laute! Singe mir von weiland!", "tokens": ["Schlag", "dei\u00b7ne", "Lau\u00b7te", "!", "Sin\u00b7ge", "mir", "von", "wei\u00b7land", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$.", "VVFIN", "PPER", "APPR", "ADV", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.8": {"text": "Von meinem ersten K\u00f6nigreich!\u201c", "tokens": ["Von", "mei\u00b7nem", "ers\u00b7ten", "K\u00f6\u00b7nig\u00b7reich", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er st\u00fcrmte durch die Saiten: \u201eJener Tage", "tokens": ["Er", "st\u00fcrm\u00b7te", "durch", "die", "Sai\u00b7ten", ":", "\u201e", "Je\u00b7ner", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$.", "$(", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ins Meer gesunkne Sonnen sind verbla\u00dft!", "tokens": ["Ins", "Meer", "ge\u00b7sunk\u00b7ne", "Son\u00b7nen", "sind", "ver\u00b7bla\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Maria Stuart! Ich erhebe Klage,", "tokens": ["Ma\u00b7ria", "Stu\u00b7art", "!", "Ich", "er\u00b7he\u00b7be", "Kla\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "PPER", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Da\u00df du geschluchzt an meinem Herzen hast!", "tokens": ["Da\u00df", "du", "ge\u00b7schluchzt", "an", "mei\u00b7nem", "Her\u00b7zen", "hast", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mit deinen Z\u00e4hren bade hier dem reinen,", "tokens": ["Mit", "dei\u00b7nen", "Z\u00e4h\u00b7ren", "ba\u00b7de", "hier", "dem", "rei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADV", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Entseelten Gott die Marmorf\u00fc\u00dfe bleich \u2014", "tokens": ["Ent\u00b7seel\u00b7ten", "Gott", "die", "Mar\u00b7mor\u00b7f\u00fc\u00b7\u00dfe", "bleich"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Weib, s\u00fcndlich ist's vor einem Menschen weinen", "tokens": ["Weib", ",", "s\u00fcnd\u00b7lich", "ist's", "vor", "ei\u00b7nem", "Men\u00b7schen", "wei\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADJD", "VAFIN", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mit diesen Augen warm und weich!", "tokens": ["Mit", "die\u00b7sen", "Au\u00b7gen", "warm", "und", "weich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Was war ich dir? Der nichtige Vertraute!", "tokens": ["Was", "war", "ich", "dir", "?", "Der", "nich\u00b7ti\u00b7ge", "Ver\u00b7trau\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PPER", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Echo, das von deinen Seufzern scholl!", "tokens": ["Ein", "E\u00b7cho", ",", "das", "von", "dei\u00b7nen", "Seuf\u00b7zern", "scholl", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Spiegel, drin sie eitel sich beschaute,", "tokens": ["Ein", "Spie\u00b7gel", ",", "drin", "sie", "ei\u00b7tel", "sich", "be\u00b7schau\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PPER", "ADJD", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Z\u00e4hre, die dir an der Wimper quoll!", "tokens": ["Die", "Z\u00e4h\u00b7re", ",", "die", "dir", "an", "der", "Wim\u00b7per", "quoll", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "War dir die Laute nur, darauf zu breiten", "tokens": ["War", "dir", "die", "Lau\u00b7te", "nur", ",", "da\u00b7rauf", "zu", "brei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "$,", "PAV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Fingerspitzen und ich hallte sch\u00f6n \u2014", "tokens": ["Die", "Fin\u00b7ger\u00b7spit\u00b7zen", "und", "ich", "hall\u00b7te", "sch\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPER", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich hasse dich!\u201c Er ri\u00df entzwei die Saiten", "tokens": ["Ich", "has\u00b7se", "dich", "!", "\u201c", "Er", "ri\u00df", "ent\u00b7zwei", "die", "Sai\u00b7ten"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$.", "$(", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mit einem gellen Mi\u00dfget\u00f6n.", "tokens": ["Mit", "ei\u00b7nem", "gel\u00b7len", "Mi\u00df\u00b7ge\u00b7t\u00f6n", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Er floh davon hinaus in Wald und Wildni\u00df,", "tokens": ["Er", "floh", "da\u00b7von", "hin\u00b7aus", "in", "Wald", "und", "Wild\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "APZR", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch wo er lechzend schl\u00fcrft' aus einem Quell,", "tokens": ["Doch", "wo", "er", "lech\u00b7zend", "schl\u00fcrft'", "aus", "ei\u00b7nem", "Quell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADJD", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sah er im Brunnen ein geliebtes Bildni\u00df", "tokens": ["Sah", "er", "im", "Brun\u00b7nen", "ein", "ge\u00b7lieb\u00b7tes", "Bild\u00b7ni\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus naher Tiefe schimmern dunkel hell,", "tokens": ["Aus", "na\u00b7her", "Tie\u00b7fe", "schim\u00b7mern", "dun\u00b7kel", "hell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sah er ein blasses Angesicht in Z\u00e4hren,", "tokens": ["Sah", "er", "ein", "blas\u00b7ses", "An\u00b7ge\u00b7sicht", "in", "Z\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Es schwand und blickte wiederum empor,", "tokens": ["Es", "schwand", "und", "blick\u00b7te", "wie\u00b7de\u00b7rum", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Von Sehnen und Erf\u00fcllen und Gew\u00e4hren", "tokens": ["Von", "Seh\u00b7nen", "und", "Er\u00b7f\u00fcl\u00b7len", "und", "Ge\u00b7w\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Rauscht's um den Born in Schilf und Rohr.", "tokens": ["Rauscht's", "um", "den", "Born", "in", "Schilf", "und", "Rohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.5": {"line.1": {"text": "\u201emaria!\u201c so beginnt in ihrer Kammer", "tokens": ["\u201e", "ma\u00b7ria", "!", "\u201c", "so", "be\u00b7ginnt", "in", "ih\u00b7rer", "Kam\u00b7mer"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ITJ", "$.", "$(", "ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Am Lager knieend sie das Nachtgebet,", "tokens": ["Am", "La\u00b7ger", "kni\u00b7e\u00b7end", "sie", "das", "Nacht\u00b7ge\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "\u201emaria!\u201c wiederholt voll Glut und Jammer", "tokens": ["\u201e", "ma\u00b7ria", "!", "\u201c", "wie\u00b7der\u00b7holt", "voll", "Glut", "und", "Jam\u00b7mer"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ITJ", "$.", "$(", "VVFIN", "ADJD", "NN", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ein Mund, der neben ihr im Dunkel fleht.", "tokens": ["Ein", "Mund", ",", "der", "ne\u00b7ben", "ihr", "im", "Dun\u00b7kel", "fleht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sie schreit. Man kommt. Von Fackelglut umlodert", "tokens": ["Sie", "schreit", ".", "Man", "kommt", ".", "Von", "Fa\u00b7ckel\u00b7glut", "um\u00b7lo\u00b7dert"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PIS", "VVFIN", "$.", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Bebt sie vor Zorn: \u201eEin M\u00f6rder! fesselt ihn!\u201c", "tokens": ["Bebt", "sie", "vor", "Zorn", ":", "\u201e", "Ein", "M\u00f6r\u00b7der", "!", "fes\u00b7selt", "ihn", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$.", "$(", "ART", "NN", "$.", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Er l\u00e4chelt: \u201eBist du sch\u00f6n!\u201c Unaufgefordert", "tokens": ["Er", "l\u00e4\u00b7chelt", ":", "\u201e", "Bist", "du", "sch\u00f6n", "!", "\u201c", "Un\u00b7auf\u00b7ge\u00b7for\u00b7dert"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word"], "pos": ["PPER", "VVFIN", "$.", "$(", "VAFIN", "PPER", "ADJD", "$.", "$(", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Giebt er den Schergen sich dahin.", "tokens": ["Giebt", "er", "den", "Scher\u00b7gen", "sich", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PRF", "PAV", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.6": {"line.1": {"text": "Er schreitet seinem Blutger\u00fcst entgegen", "tokens": ["Er", "schrei\u00b7tet", "sei\u00b7nem", "Blut\u00b7ge\u00b7r\u00fcst", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In einem klaren k\u00fchlen Morgenrot,", "tokens": ["In", "ei\u00b7nem", "kla\u00b7ren", "k\u00fch\u00b7len", "Mor\u00b7gen\u00b7rot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ins Ohr des S\u00fcnders fl\u00fcstert angelegen", "tokens": ["Ins", "Ohr", "des", "S\u00fcn\u00b7ders", "fl\u00fcs\u00b7tert", "an\u00b7ge\u00b7le\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Capuziner, der vermummte Tod:", "tokens": ["Ein", "Ca\u00b7pu\u00b7zi\u00b7ner", ",", "der", "ver\u00b7mumm\u00b7te", "Tod", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "\u201efreund, du bekommst es gut! Du wirst entlastet!", "tokens": ["\u201e", "freund", ",", "du", "be\u00b7kommst", "es", "gut", "!", "Du", "wirst", "ent\u00b7las\u00b7tet", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$.", "PPER", "VAFIN", "VVPP", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Ich absolvire dich von Lust und Pein!", "tokens": ["Ich", "ab\u00b7sol\u00b7vi\u00b7re", "dich", "von", "Lust", "und", "Pein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Von keiner weichen wei\u00dfen Hand betastet,", "tokens": ["Von", "kei\u00b7ner", "wei\u00b7chen", "wei\u00b7\u00dfen", "Hand", "be\u00b7tas\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wirst du die stumme Laute sein!\u201c", "tokens": ["Wirst", "du", "die", "stum\u00b7me", "Lau\u00b7te", "sein", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "VAINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}