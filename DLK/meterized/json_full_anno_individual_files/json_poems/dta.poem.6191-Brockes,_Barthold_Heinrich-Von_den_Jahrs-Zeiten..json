{"dta.poem.6191": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Jahrs-Zeiten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Lasst uns den Fortgang unsrer Zeiten,", "tokens": ["Lasst", "uns", "den", "Fort\u00b7gang", "uns\u00b7rer", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die so ver\u00e4nderlich, erwegen.", "tokens": ["Die", "so", "ver\u00b7\u00e4n\u00b7der\u00b7lich", ",", "er\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wir wollen erstlich \u00fcberlegen", "tokens": ["Wir", "wol\u00b7len", "erst\u00b7lich", "\u00fc\u00b7berl\u00b7e\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des holden ", "tokens": ["Des", "hol\u00b7den"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Der die Natur verj\u00fcnget und sie schm\u00fcckt,", "tokens": ["Der", "die", "Na\u00b7tur", "ver\u00b7j\u00fcn\u00b7get", "und", "sie", "schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wann uns des Himmels Gunst den Seegens-Regen schickt:", "tokens": ["Wann", "uns", "des", "Him\u00b7mels", "Gunst", "den", "See\u00b7gens\u00b7Re\u00b7gen", "schickt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wann unsres Tages F\u00fcrst, in unsre Welt verliebet,", "tokens": ["Wann", "uns\u00b7res", "Ta\u00b7ges", "F\u00fcrst", ",", "in", "uns\u00b7re", "Welt", "ver\u00b7lie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "NN", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Durch warme Lauigkeit den strengen Frost verjagt,", "tokens": ["Durch", "war\u00b7me", "Lau\u00b7ig\u00b7keit", "den", "stren\u00b7gen", "Frost", "ver\u00b7jagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der sie bisher gefesselt und geplagt,", "tokens": ["Der", "sie", "bis\u00b7her", "ge\u00b7fes\u00b7selt", "und", "ge\u00b7plagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Da sie uns dann so zarte Blumen giebet,", "tokens": ["Da", "sie", "uns", "dann", "so", "zar\u00b7te", "Blu\u00b7men", "gie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Da man so dann, auf Wiesen, fruchtbar Kraut,", "tokens": ["Da", "man", "so", "dann", ",", "auf", "Wie\u00b7sen", ",", "frucht\u00b7bar", "Kraut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADV", "$,", "APPR", "NN", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Auf B\u00e4umen, dichte Bl\u00e4tter schaut,", "tokens": ["Auf", "B\u00e4u\u00b7men", ",", "dich\u00b7te", "Bl\u00e4t\u00b7ter", "schaut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und sie der Fr\u00fccht\u2019 und Pflantzen Nahrungs-Krafft,", "tokens": ["Und", "sie", "der", "Fr\u00fccht'", "und", "Pflant\u00b7zen", "Nah\u00b7rungs\u00b7Krafft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "KON", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Aus ihrem fenchten Schoo\u00df hervor bringt, sanfft beweget,", "tokens": ["Aus", "ih\u00b7rem", "fench\u00b7ten", "Schoo\u00df", "her\u00b7vor", "bringt", ",", "sanfft", "be\u00b7we\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKVZ", "VVFIN", "$,", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Als den so sch\u00e4tzbarn Safft,", "tokens": ["Als", "den", "so", "sch\u00e4tz\u00b7barn", "Safft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Den sie in sich verborgen heget.", "tokens": ["Den", "sie", "in", "sich", "ver\u00b7bor\u00b7gen", "he\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PRF", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Durch solche gl\u00fcckliche Ver\u00e4nderung entsteht,", "tokens": ["Durch", "sol\u00b7che", "gl\u00fcck\u00b7li\u00b7che", "Ver\u00b7\u00e4n\u00b7de\u00b7rung", "ent\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Entspriesst, erh\u00e4lt und bl\u00fcht nun alles, was ihr seht.", "tokens": ["Ent\u00b7spriesst", ",", "er\u00b7h\u00e4lt", "und", "bl\u00fcht", "nun", "al\u00b7les", ",", "was", "ihr", "seht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "KON", "VVFIN", "ADV", "PIS", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Das Feld hat seine Sch\u00e4tz\u2019, es hat der Wald", "tokens": ["Das", "Feld", "hat", "sei\u00b7ne", "Sch\u00e4tz'", ",", "es", "hat", "der", "Wald"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Hiedurch die liebliche Gestallt.", "tokens": ["Hie\u00b7durch", "die", "lieb\u00b7li\u00b7che", "Ge\u00b7stallt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Hierdurch w\u00e4chst alles fort und wird vermehrt,", "tokens": ["Hier\u00b7durch", "w\u00e4chst", "al\u00b7les", "fort", "und", "wird", "ver\u00b7mehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIS", "PTKVZ", "KON", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Hierdurch sind alle Ding\u2019ern\u00e4hrt.", "tokens": ["Hier\u00b7durch", "sind", "al\u00b7le", "Ding'\u00b7er\u00b7n\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}