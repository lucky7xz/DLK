{"dta.poem.15847": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "52.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gott ist ein Geist, und kann des Leibes nicht entbehren;", "tokens": ["Gott", "ist", "ein", "Geist", ",", "und", "kann", "des", "Lei\u00b7bes", "nicht", "ent\u00b7beh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "KON", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Sch\u00f6pfer fassen nicht reingeistiger Sch\u00f6pfung Sf\u00e4ren.", "tokens": ["Den", "Sch\u00f6p\u00b7fer", "fas\u00b7sen", "nicht", "rein\u00b7geis\u00b7ti\u00b7ger", "Sch\u00f6p\u00b7fung", "Sf\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Er schuf, um Halt und Bild der Sch\u00f6pfung zu verleihn,", "tokens": ["Er", "schuf", ",", "um", "Halt", "und", "Bild", "der", "Sch\u00f6p\u00b7fung", "zu", "ver\u00b7leihn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUI", "NN", "KON", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zum Geiste Fleisch und Bein, zum Menschen Pflanz' und Stein.", "tokens": ["Zum", "Geis\u00b7te", "Fleisch", "und", "Bein", ",", "zum", "Men\u00b7schen", "Pflanz'", "und", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "KON", "NN", "$,", "APPRART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Alswie gefangen ist die Rose von dem Strauch,", "tokens": ["Als\u00b7wie", "ge\u00b7fan\u00b7gen", "ist", "die", "Ro\u00b7se", "von", "dem", "Strauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So ist gefangen auch vom Leib des Geistes Hauch.", "tokens": ["So", "ist", "ge\u00b7fan\u00b7gen", "auch", "vom", "Leib", "des", "Geis\u00b7tes", "Hauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "ADV", "APPRART", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dich zu vergeistigen, darfst du dich nicht entleiben;", "tokens": ["Dich", "zu", "ver\u00b7geis\u00b7ti\u00b7gen", ",", "darfst", "du", "dich", "nicht", "ent\u00b7lei\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "VMFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Wenn du den Stock zerst\u00f6rst, wo soll die Rose bleiben?", "tokens": ["Wenn", "du", "den", "Stock", "zer\u00b7st\u00f6rst", ",", "wo", "soll", "die", "Ro\u00b7se", "blei\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "PWAV", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}