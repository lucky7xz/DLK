{"textgrid.poem.45510": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[die Kritiker, will sagen: die neuen]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Kritiker, will sagen: die neuen,", "tokens": ["Die", "Kri\u00b7ti\u00b7ker", ",", "will", "sa\u00b7gen", ":", "die", "neu\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VMFIN", "VVINF", "$.", "ART", "ADJA", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vergleich ich den Papageien,", "tokens": ["Ver\u00b7gleich", "ich", "den", "Pa\u00b7pa\u00b7gei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie haben drei oder vier Worte,", "tokens": ["Sie", "ha\u00b7ben", "drei", "o\u00b7der", "vier", "Wor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "CARD", "KON", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die wiederholen sie an jedem Orte.", "tokens": ["Die", "wie\u00b7der\u00b7ho\u00b7len", "sie", "an", "je\u00b7dem", "Or\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Romantisch, klassisch und modern", "tokens": ["Ro\u00b7man\u00b7tisch", ",", "klas\u00b7sisch", "und", "mo\u00b7dern"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "ADJD", "KON", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Scheint schon ein Urteil diesen Herrn,", "tokens": ["Scheint", "schon", "ein", "Ur\u00b7teil", "die\u00b7sen", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und sie \u00fcbersehen in stolzem Mut", "tokens": ["Und", "sie", "\u00fc\u00b7ber\u00b7se\u00b7hen", "in", "stol\u00b7zem", "Mut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Die wahren Gattungen: schlecht und gut.", "tokens": ["Die", "wah\u00b7ren", "Gat\u00b7tun\u00b7gen", ":", "schlecht", "und", "gut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Kritiker, will sagen: die neuen,", "tokens": ["Die", "Kri\u00b7ti\u00b7ker", ",", "will", "sa\u00b7gen", ":", "die", "neu\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VMFIN", "VVINF", "$.", "ART", "ADJA", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vergleich ich den Papageien,", "tokens": ["Ver\u00b7gleich", "ich", "den", "Pa\u00b7pa\u00b7gei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie haben drei oder vier Worte,", "tokens": ["Sie", "ha\u00b7ben", "drei", "o\u00b7der", "vier", "Wor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "CARD", "KON", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die w