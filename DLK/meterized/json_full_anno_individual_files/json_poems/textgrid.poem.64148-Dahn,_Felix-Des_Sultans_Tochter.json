{"textgrid.poem.64148": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Des Sultans Tochter", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbo Fatime, was verzehret dich, was welken deine Wangen?", "tokens": ["\u00bb", "o", "Fa\u00b7ti\u00b7me", ",", "was", "ver\u00b7zeh\u00b7ret", "dich", ",", "was", "wel\u00b7ken", "dei\u00b7ne", "Wan\u00b7gen", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "FM", "$,", "PWS", "VVFIN", "PPER", "$,", "PWS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Alles was dein Herz begehret, kann dein leiser Wink erlangen.", "tokens": ["Al\u00b7les", "was", "dein", "Herz", "be\u00b7ge\u00b7hret", ",", "kann", "dein", "lei\u00b7ser", "Wink", "er\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PRELS", "PPOSAT", "NN", "VVFIN", "$,", "VMFIN", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Willst du Schmuck und Goldgeschmeide \u2013 so befiehl und unsre Flotten", "tokens": ["Willst", "du", "Schmuck", "und", "Gold\u00b7ge\u00b7schmei\u00b7de", "\u2013", "so", "be\u00b7fiehl", "und", "uns\u00b7re", "Flot\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "NN", "KON", "NN", "$(", "ADV", "ADJD", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Holen Purpur dir und Seide, Perlen dir aus feuchten Grotten.", "tokens": ["Ho\u00b7len", "Pur\u00b7pur", "dir", "und", "Sei\u00b7de", ",", "Per\u00b7len", "dir", "aus", "feuch\u00b7ten", "Grot\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "KON", "NE", "$,", "NN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Willst du Tanz und bunten Reigen \u2013 die Moriskos brennen alle,", "tokens": ["Willst", "du", "Tanz", "und", "bun\u00b7ten", "Rei\u00b7gen", "\u2013", "die", "Mo\u00b7ris\u00b7kos", "bren\u00b7nen", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "KON", "ADJA", "NN", "$(", "ART", "NN", "VVINF", "PIS", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Der Gebieterin zu zeigen ihre Kunst bei Zimbelschalle.", "tokens": ["Der", "Ge\u00b7bie\u00b7te\u00b7rin", "zu", "zei\u00b7gen", "ih\u00b7re", "Kunst", "bei", "Zim\u00b7bel\u00b7schal\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Willst du Blumen \u2013 sieh dein Garten windet schattig sich dahin", "tokens": ["Willst", "du", "Blu\u00b7men", "\u2013", "sieh", "dein", "Gar\u00b7ten", "win\u00b7det", "schat\u00b7tig", "sich", "da\u00b7hin"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "NN", "$(", "VVIMP", "PPOSAT", "NN", "VVFIN", "ADJD", "PRF", "PAV"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Und die schlanken Palmen warten l\u00e4ngst schon ihrer K\u00f6nigin.", "tokens": ["Und", "die", "schlan\u00b7ken", "Pal\u00b7men", "war\u00b7ten", "l\u00e4ngst", "schon", "ih\u00b7rer", "K\u00f6\u00b7ni\u00b7gin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Oder hat die s\u00fc\u00dfe Flamme dir das junge Herz entz\u00fcndet?", "tokens": ["O\u00b7der", "hat", "die", "s\u00fc\u00b7\u00dfe", "Flam\u00b7me", "dir", "das", "jun\u00b7ge", "Herz", "ent\u00b7z\u00fcn\u00b7det", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Freie Wahl aus jedem Stamme hat dein Vater dir verk\u00fcndet:", "tokens": ["Frei\u00b7e", "Wahl", "aus", "je\u00b7dem", "Stam\u00b7me", "hat", "dein", "Va\u00b7ter", "dir", "ver\u00b7k\u00fcn\u00b7det", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PIAT", "NN", "VAFIN", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.11": {"text": "Ist's der dunkle Held Abdallah, ist's der gl\u00fchende Hussein?", "tokens": ["Ist's", "der", "dunk\u00b7le", "Held", "Ab\u00b7dal\u00b7lah", ",", "ist's", "der", "gl\u00fc\u00b7hen\u00b7de", "Hus\u00b7sein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "NE", "$,", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+---+-+---+", "measure": "unknown.measure.hexa"}, "line.12": {"text": "Sprich es aus \u2013 denn gro\u00df ist Allah, ihre Herzen all' sind dein!\u00ab \u2013", "tokens": ["Sprich", "es", "aus", "\u2013", "denn", "gro\u00df", "ist", "Al\u00b7lah", ",", "ih\u00b7re", "Her\u00b7zen", "all'", "sind", "dein", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVIMP", "PPER", "APPR", "$(", "KON", "ADJD", "VAFIN", "NN", "$,", "PPOSAT", "NN", "PIS", "VAFIN", "PPOSAT", "$.", "$(", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.13": {"text": "\u2013 \u00bbAch, Zuleika, mein Geschmeide hat verloren seinen Schimmer,", "tokens": ["\u2013", "\u00bb", "Ach", ",", "Zu\u00b7lei\u00b7ka", ",", "mein", "Ge\u00b7schmei\u00b7de", "hat", "ver\u00b7lo\u00b7ren", "sei\u00b7nen", "Schim\u00b7mer", ","], "token_info": ["punct", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "ITJ", "$,", "NE", "$,", "PPOSAT", "NN", "VAFIN", "VVPP", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.14": {"text": "Und Fatimens Augenweide \u2013 Perlen sind's und Seide nimmer!", "tokens": ["Und", "Fa\u00b7ti\u00b7mens", "Au\u00b7gen\u00b7wei\u00b7de", "\u2013", "Per\u00b7len", "sin\u00b7d's", "und", "Sei\u00b7de", "nim\u00b7mer", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "$(", "NN", "NE", "KON", "NE", "ADV", "$."], "meter": "-+--+-+-+-+--+-+-", "measure": "iambic.septa.relaxed"}, "line.15": {"text": "Zimbeln nicht und Kastagnetten sollen die Moriskos schlagen \u2013", "tokens": ["Zim\u00b7beln", "nicht", "und", "Kas\u00b7tag\u00b7net\u00b7ten", "sol\u00b7len", "die", "Mo\u00b7ris\u00b7kos", "schla\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "KON", "NN", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.16": {"text": "Wenn sie Trauerfl\u00f6ten h\u00e4tten, m\u00f6chte mir das Spiel behagen!", "tokens": ["Wenn", "sie", "Trau\u00b7er\u00b7fl\u00f6\u00b7ten", "h\u00e4t\u00b7ten", ",", "m\u00f6ch\u00b7te", "mir", "das", "Spiel", "be\u00b7ha\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VAFIN", "$,", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.17": {"text": "Nicht Abdallah mir im Herzen, nicht Hussein, der tapfre, steht:", "tokens": ["Nicht", "Ab\u00b7dal\u00b7lah", "mir", "im", "Her\u00b7zen", ",", "nicht", "Hus\u00b7sein", ",", "der", "tapf\u00b7re", ",", "steht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "NN", "PPER", "APPRART", "NN", "$,", "PTKNEG", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "$."], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.18": {"text": "Machtlos ist in meinen Schmerzen Allah selbst und sein Prophet! \u2013", "tokens": ["Macht\u00b7los", "ist", "in", "mei\u00b7nen", "Schmer\u00b7zen", "Al\u00b7lah", "selbst", "und", "sein", "Pro\u00b7phet", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "VAFIN", "APPR", "PPOSAT", "NN", "NN", "ADV", "KON", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.19": {"text": "In der Waffenruhe zogen gegen Bagdad tausend G\u00e4ste,", "tokens": ["In", "der", "Waf\u00b7fen\u00b7ru\u00b7he", "zo\u00b7gen", "ge\u00b7gen", "Bag\u00b7dad", "tau\u00b7send", "G\u00e4s\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "APPR", "NE", "CARD", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.20": {"text": "Helme blitzten, Banner flogen, Kr\u00e4nze schm\u00fcckten die Pal\u00e4ste.", "tokens": ["Hel\u00b7me", "blitz\u00b7ten", ",", "Ban\u00b7ner", "flo\u00b7gen", ",", "Kr\u00e4n\u00b7ze", "schm\u00fcck\u00b7ten", "die", "Pa\u00b7l\u00e4s\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "NN", "VVFIN", "$,", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.21": {"text": "Und ich stand auf der Altane, leise gingen Abendl\u00fcfte,", "tokens": ["Und", "ich", "stand", "auf", "der", "Al\u00b7ta\u00b7ne", ",", "lei\u00b7se", "gin\u00b7gen", "A\u00b7ben\u00b7dl\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$,", "ADJD", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.22": {"text": "Und Jasmin bl\u00fcht' und Banane und die Rose hauchte D\u00fcfte \u2013", "tokens": ["Und", "Jas\u00b7min", "bl\u00fcht'", "und", "Ba\u00b7na\u00b7ne", "und", "die", "Ro\u00b7se", "hauch\u00b7te", "D\u00fcf\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "KON", "NN", "KON", "ART", "NN", "VVFIN", "NN", "$("], "meter": "-+-+--+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.23": {"text": "Da, aus hoher Zedern Mitten, \u2013 o wie ist mein Herz erschrocken! \u2013", "tokens": ["Da", ",", "aus", "ho\u00b7her", "Ze\u00b7dern", "Mit\u00b7ten", ",", "\u2013", "o", "wie", "ist", "mein", "Herz", "er\u00b7schro\u00b7cken", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "$,", "APPR", "ADJA", "NN", "NN", "$,", "$(", "FM", "KOKOM", "VAFIN", "PPOSAT", "NN", "VVINF", "$.", "$("], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.24": {"text": "Kam ein J\u00fcngling ernst geschritten, sch\u00f6n, in lichten, langen Locken.", "tokens": ["Kam", "ein", "J\u00fcng\u00b7ling", "ernst", "ge\u00b7schrit\u00b7ten", ",", "sch\u00f6n", ",", "in", "lich\u00b7ten", ",", "lan\u00b7gen", "Lo\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADJD", "VVPP", "$,", "ADJD", "$,", "APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.25": {"text": "Tr\u00e4umerisch zum Abendsterne schlug er auf die blauen Augen,", "tokens": ["Tr\u00e4u\u00b7me\u00b7risch", "zum", "A\u00b7bends\u00b7ter\u00b7ne", "schlug", "er", "auf", "die", "blau\u00b7en", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.26": {"text": "Als ob er den Himmel gerne wollt' in seine Seele saugen. \u2013", "tokens": ["Als", "ob", "er", "den", "Him\u00b7mel", "ger\u00b7ne", "wollt'", "in", "sei\u00b7ne", "See\u00b7le", "sau\u00b7gen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ART", "NN", "ADV", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$.", "$("], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.27": {"text": "Halb geworfen, halb entglitten fiel mein Strau\u00df mir aus dem Schleier,", "tokens": ["Halb", "ge\u00b7wor\u00b7fen", ",", "halb", "ent\u00b7glit\u00b7ten", "fiel", "mein", "Strau\u00df", "mir", "aus", "dem", "Schlei\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "ADJD", "VVPP", "VVFIN", "PPOSAT", "NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.28": {"text": "Rosen waren's, frisch geschnitten, Rosen von dem Tigrisweiher.", "tokens": ["Ro\u00b7sen", "wa\u00b7ren's", ",", "frisch", "ge\u00b7schnit\u00b7ten", ",", "Ro\u00b7sen", "von", "dem", "Tig\u00b7ris\u00b7wei\u00b7her", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "ADJD", "VVPP", "$,", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-++", "measure": "trochaic.octa.plus"}, "line.29": {"text": "Ihm zu F\u00fc\u00dfen sonder Irren fiel der Strau\u00df in wei\u00dfen Sand,", "tokens": ["Ihm", "zu", "F\u00fc\u00b7\u00dfen", "son\u00b7der", "Ir\u00b7ren", "fiel", "der", "Strau\u00df", "in", "wei\u00b7\u00dfen", "Sand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "KON", "NN", "VVFIN", "ART", "NE", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.30": {"text": "Er sah auf und schlug mit Klirren auf die Brust die rechte Hand;", "tokens": ["Er", "sah", "auf", "und", "schlug", "mit", "Klir\u00b7ren", "auf", "die", "Brust", "die", "rech\u00b7te", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "VVFIN", "APPR", "NN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.31": {"text": "Auf die Brust die Rechte schlug er \u2013 ach, da ward es mir bewu\u00dft:", "tokens": ["Auf", "die", "Brust", "die", "Rech\u00b7te", "schlug", "er", "\u2013", "ach", ",", "da", "ward", "es", "mir", "be\u00b7wu\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "PPER", "$(", "XY", "$,", "ADV", "VAFIN", "PPER", "PPER", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.32": {"text": "Einen wei\u00dfen Mantel trug er, rot bekreuzt die linke Brust!", "tokens": ["Ei\u00b7nen", "wei\u00b7\u00dfen", "Man\u00b7tel", "trug", "er", ",", "rot", "be\u00b7kreuzt", "die", "lin\u00b7ke", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$,", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.33": {"text": "In des wei\u00dfen Mantels Linnen schlug er fest die Eisenglieder,", "tokens": ["In", "des", "wei\u00b7\u00dfen", "Man\u00b7tels", "Lin\u00b7nen", "schlug", "er", "fest", "die", "Ei\u00b7seng\u00b7lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NE", "VVFIN", "PPER", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.34": {"text": "Wandte sich und schritt von hinnen, \u2013 und ich sah ihn niemals wieder.", "tokens": ["Wand\u00b7te", "sich", "und", "schritt", "von", "hin\u00b7nen", ",", "\u2013", "und", "ich", "sah", "ihn", "nie\u00b7mals", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "KON", "VVFIN", "APPR", "ADV", "$,", "$(", "KON", "PPER", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "\u00bbo Fatime, was verzehret dich,