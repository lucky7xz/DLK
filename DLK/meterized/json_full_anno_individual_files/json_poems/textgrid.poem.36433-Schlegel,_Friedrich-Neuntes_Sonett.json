os": ["ART", "NN", "VVFIN", "ADJD", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und fr\u00fch vertaner Jugend R\u00fcckerstattung.", "tokens": ["Und", "fr\u00fch", "ver\u00b7ta\u00b7ner", "Ju\u00b7gend", "R\u00fc\u00b7cker\u00b7stat\u00b7tung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie peinlich w\u00fcrde heute die Ermattung", "tokens": ["Wie", "pein\u00b7lich", "w\u00fcr\u00b7de", "heu\u00b7te", "die", "Er\u00b7mat\u00b7tung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die junge Frau aus der Umarmung schrecken.", "tokens": ["Die", "jun\u00b7ge", "Frau", "aus", "der", "Um\u00b7ar\u00b7mung", "schre\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Er droht des frechen Narren fr\u00f6stelnd Lachen,", "tokens": ["Er", "droht", "des", "fre\u00b7chen", "Nar\u00b7ren", "fr\u00f6s\u00b7telnd", "La\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Flucht seiner Jugend mondbegl\u00e4nzten Nachen", "tokens": ["Flucht", "sei\u00b7ner", "Ju\u00b7gend", "mond\u00b7be\u00b7gl\u00e4nz\u00b7ten", "Na\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "ADJA", "NN"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.3": {"text": "Und glaubt nicht mehr an schwarzer Kr\u00e4uter Sieden.", "to