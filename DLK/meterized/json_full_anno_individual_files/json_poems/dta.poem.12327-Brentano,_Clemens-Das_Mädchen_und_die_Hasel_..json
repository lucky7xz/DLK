{"dta.poem.12327": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Das M\u00e4dchen und die Hasel .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es wollt ein M\u00e4dchen Rosen brechen gehn,               ", "tokens": ["Es", "wollt", "ein", "M\u00e4d\u00b7chen", "Ro\u00b7sen", "bre\u00b7chen", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wohl in die gr\u00fcne Heide,", "tokens": ["Wohl", "in", "die", "gr\u00fc\u00b7ne", "Hei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was fand sie da am Wege stehn?", "tokens": ["Was", "fand", "sie", "da", "am", "We\u00b7ge", "stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Eine Hasel, die war gr\u00fcne.", "tokens": ["Ei\u00b7ne", "Ha\u00b7sel", ",", "die", "war", "gr\u00fc\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VAFIN", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "\u201eguten Tag, guten Tag, liebe Hasel mein,", "tokens": ["\u201e", "gu\u00b7ten", "Tag", ",", "gu\u00b7ten", "Tag", ",", "lie\u00b7be", "Ha\u00b7sel", "mein", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "ADJA", "NN", "$,", "ADJA", "NN", "PPOSAT", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "\u201ewarum bist du so gr\u00fcne?\u201c", "tokens": ["\u201e", "wa\u00b7rum", "bist", "du", "so", "gr\u00fc\u00b7ne", "?", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "VAFIN", "PPER", "ADV", "ADJA", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u201ehab' Dank, hab' Dank, wackres M\u00e4gdelein,", "tokens": ["\u201e", "hab'", "Dank", ",", "hab'", "Dank", ",", "wack\u00b7res", "M\u00e4g\u00b7del\u00b7ein", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VAFIN", "NN", "$,", "VAFIN", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "\u201ewarum bist du so sch\u00f6ne?\u201c", "tokens": ["\u201e", "wa\u00b7rum", "bist", "du", "so", "sch\u00f6\u00b7ne", "?", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "VAFIN", "PPER", "ADV", "ADJA", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u201ewarum da\u00df ich so sch\u00f6ne bin,", "tokens": ["\u201e", "wa\u00b7rum", "da\u00df", "ich", "so", "sch\u00f6\u00b7ne", "bin", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "KOUS", "PPER", "ADV", "ADJA", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201edas will ich dir wohl sagen:", "tokens": ["\u201e", "das", "will", "ich", "dir", "wohl", "sa\u00b7gen", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VMFIN", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u201eich e\u00df' wei\u00df Brod, trink k\u00fchlen Wein,", "tokens": ["\u201e", "ich", "e\u00df'", "wei\u00df", "Brod", ",", "trink", "k\u00fch\u00b7len", "Wein", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADV", "VVFIN", "NN", "$,", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201edavon bin ich so sch\u00f6ne.\u201c", "tokens": ["\u201e", "da\u00b7von", "bin", "ich", "so", "sch\u00f6\u00b7ne", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PAV", "VAFIN", "PPER", "ADV", "ADJA", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u201ei\u00dft du wei\u00df Brod, trinkst k\u00fchlen Wein,", "tokens": ["\u201e", "i\u00dft", "du", "wei\u00df", "Brod", ",", "trinkst", "k\u00fch\u00b7len", "Wein", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "VVFIN", "NN", "$,", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201eund bist davon so sch\u00f6ne:", "tokens": ["\u201e", "und", "bist", "da\u00b7von", "so", "sch\u00f6\u00b7ne", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VAFIN", "PAV", "ADV", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u201eso f\u00e4llt alle Morgen k\u00fchler Thau auf mich,", "tokens": ["\u201e", "so", "f\u00e4llt", "al\u00b7le", "Mor\u00b7gen", "k\u00fch\u00b7ler", "Thau", "auf", "mich", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PIAT", "NN", "ADJA", "NN", "APPR", "PPER", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "\u201edavon bin ich so gr\u00fcne.\u201c", "tokens": ["\u201e", "da\u00b7von", "