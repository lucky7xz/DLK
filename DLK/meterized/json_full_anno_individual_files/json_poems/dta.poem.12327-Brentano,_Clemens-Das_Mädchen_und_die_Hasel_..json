word", "punct"], "pos": ["$(", "KOUS", "PPER", "PPER", "PDS", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u201eh\u00e4tt' mich sonst heut auf'n Narrentanz bereit,", "tokens": ["\u201e", "h\u00e4tt'", "mich", "sonst", "heut", "auf'n", "Nar\u00b7ren\u00b7tanz", "be\u00b7reit", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "ADJD", "$,"], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.4": {"text": "\u201ezu Hause will ich bleiben.\u201c", "tokens": ["\u201e", "zu", "Hau\u00b7se", "will", "ich", "blei\u00b7ben", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "NN", "VMFIN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}