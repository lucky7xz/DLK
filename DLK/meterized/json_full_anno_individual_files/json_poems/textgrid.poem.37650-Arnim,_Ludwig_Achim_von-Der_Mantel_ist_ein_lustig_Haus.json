{"textgrid.poem.37650": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Der Mantel ist ein lustig Haus", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Mantel ist ein lustig Haus,", "tokens": ["Der", "Man\u00b7tel", "ist", "ein", "lus\u00b7tig", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drin ist gew\u00f6lbt ein Keller,", "tokens": ["Drin", "ist", "ge\u00b7w\u00f6lbt", "ein", "Kel\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da giebt es manchen sch\u00f6nen Schmaus,", "tokens": ["Da", "giebt", "es", "man\u00b7chen", "sch\u00f6\u00b7nen", "Schmaus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da geht es stets herein, heraus,", "tokens": ["Da", "geht", "es", "stets", "her\u00b7ein", ",", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und kostet keinen Heller.", "tokens": ["Und", "kos\u00b7tet", "kei\u00b7nen", "Hel\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein Ofen ist in diesem Haus,", "tokens": ["Ein", "O\u00b7fen", "ist", "in", "die\u00b7sem", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das ist die Tabackspfeife,", "tokens": ["Das", "ist", "die", "Ta\u00b7back\u00b7spfei\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die macht mir W\u00f6lklein wei\u00df und kraus,", "tokens": ["Die", "macht", "mir", "W\u00f6lk\u00b7lein", "wei\u00df", "und", "kraus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "NN", "VVFIN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es scheint recht wie ein Blumenstrau\u00df,", "tokens": ["Es", "scheint", "recht", "wie", "ein", "Blu\u00b7men\u00b7strau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Weg ist's, wenn ich nach greife.", "tokens": ["Weg", "ist's", ",", "wenn", "ich", "nach", "grei\u00b7fe", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "KOUS", "PPER", "APPR", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Brenner ist des Teufels Kost,", "tokens": ["Der", "Bren\u00b7ner", "ist", "des", "Teu\u00b7fels", "Kost", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Feuer ich ihn locke,", "tokens": ["Mit", "Feu\u00b7er", "ich", "ihn", "lo\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und f\u00fcr den einzigen H\u00f6llentrost", "tokens": ["Und", "f\u00fcr", "den", "ein\u00b7zi\u00b7gen", "H\u00f6l\u00b7lent\u00b7rost"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Er alle Feinde niedersto\u00dft,", "tokens": ["Er", "al\u00b7le", "Fein\u00b7de", "nie\u00b7der\u00b7sto\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zu Dutzend und im Schocke.", "tokens": ["Zu", "Dut\u00b7zend", "und", "im", "Scho\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Mein Pferdchen, das mit Spr\u00fcngen trabt,", "tokens": ["Mein", "Pferd\u00b7chen", ",", "das", "mit", "Spr\u00fcn\u00b7gen", "trabt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hab' ich durch ihn erbeutet,", "tokens": ["Hab'", "ich", "durch", "ihn", "er\u00b7beu\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie es mir nun das Herze labt,", "tokens": ["Wie", "es", "mir", "nun", "das", "Her\u00b7ze", "labt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als h\u00e4tt' ich es zum Thron gehabt,", "tokens": ["Als", "h\u00e4tt'", "ich", "es", "zum", "Thron", "ge\u00b7habt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "PPER", "APPRART", "NN", "VAPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn es die M\u00e4hne breitet.", "tokens": ["Wenn", "es", "die", "M\u00e4h\u00b7ne", "brei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Es ist ein gro\u00dfer Federkrieg", "tokens": ["Es", "ist", "ein", "gro\u00b7\u00dfer", "Fe\u00b7der\u00b7krieg"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In aller Welt entstanden,", "tokens": ["In", "al\u00b7ler", "Welt", "ent\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die hohe Feder wallt zum Sieg,", "tokens": ["Die", "ho\u00b7he", "Fe\u00b7der", "wallt", "zum", "Sieg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So weit mein Schwert reicht, alles liegt,", "tokens": ["So", "weit", "mein", "Schwert", "reicht", ",", "al\u00b7les", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "VVFIN", "$,", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als w\u00fcchs es auf dem Lande.", "tokens": ["Als", "w\u00fcchs", "es", "auf", "dem", "Lan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wir sitzen ab im St\u00e4dtlein drin,", "tokens": ["Wir", "sit\u00b7zen", "ab", "im", "St\u00e4dt\u00b7lein", "drin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die B\u00fcrgerm\u00e4dchen schauen,", "tokens": ["Die", "B\u00fcr\u00b7ger\u00b7m\u00e4d\u00b7chen", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die erste fass' ich an das Kinn,", "tokens": ["Die", "ers\u00b7te", "fass'", "ich", "an", "das", "Kinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die zweite sieht, da\u00df ich es bin,", "tokens": ["Die", "zwei\u00b7te", "sieht", ",", "da\u00df", "ich", "es", "bin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$,", "KOUS", "PPER", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und thut mich lieblich hauen.", "tokens": ["Und", "thut", "mich", "lieb\u00b7lich", "hau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ich la\u00df mir ein klein Zettelein", "tokens": ["Ich", "la\u00df", "mir", "ein", "klein", "Zet\u00b7tel\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von ihrem Rathsherrn schmieren,", "tokens": ["Von", "ih\u00b7rem", "Raths\u00b7herrn", "schmie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Daf\u00fcr l\u00e4\u00dft mich ein jeder ein,", "tokens": ["Da\u00b7f\u00fcr", "l\u00e4\u00dft", "mich", "ein", "je\u00b7der", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PRF", "ART", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und bringt mir gleich den Krug mit Wein,", "tokens": ["Und", "bringt", "mir", "gleich", "den", "Krug", "mit", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich und mein Pferd regieren.", "tokens": ["Ich", "und", "mein", "Pferd", "re\u00b7gie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Das M\u00e4dchen f\u00fchrt uns in den Stall,", "tokens": ["Das", "M\u00e4d\u00b7chen", "f\u00fchrt", "uns", "in", "den", "Stall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Stall da ist es dunkel,", "tokens": ["Im", "Stall", "da", "ist", "es", "dun\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da leuchtet dann ihr Aug' zumal,", "tokens": ["Da", "leuch\u00b7tet", "dann", "ihr", "Aug'", "zu\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Sonne \u00fcber Berg und Thal,", "tokens": ["Wie", "Son\u00b7ne", "\u00fc\u00b7ber", "Berg", "und", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit lieblichem Gefunkel.", "tokens": ["Mit", "lieb\u00b7li\u00b7chem", "Ge\u00b7fun\u00b7kel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Das sch\u00f6ne Kind klatscht mir mein Pferd,", "tokens": ["Das", "sch\u00f6\u00b7ne", "Kind", "klatscht", "mir", "mein", "Pferd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "M\u00f6cht' ihm zu fressen geben.", "tokens": ["M\u00f6cht'", "ihm", "zu", "fres\u00b7sen", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbnur gl\u00fche Kohle fri\u00dft mein Pferd,", "tokens": ["\u00bb", "nur", "gl\u00fc\u00b7he", "Koh\u00b7le", "fri\u00dft", "mein", "Pferd", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Augen dein, die sind der Heerd;", "tokens": ["Die", "Au\u00b7gen", "dein", ",", "die", "sind", "der", "Heerd", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "$,", "PRELS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dir ist es ganz ergeben.\u00ab", "tokens": ["Dir", "ist", "es", "ganz", "er\u00b7ge\u00b7ben", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Wer das Kommisbrod hat erdacht,", "tokens": ["Wer", "das", "Kom\u00b7mis\u00b7brod", "hat", "er\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das war ein guter Reiter,", "tokens": ["Das", "war", "ein", "gu\u00b7ter", "Rei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das steht uns frei bei Tag und Nacht,", "tokens": ["Das", "steht", "uns", "frei", "bei", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Pferdchen es auch nicht veracht,", "tokens": ["Mein", "Pferd\u00b7chen", "es", "auch", "nicht", "ver\u00b7acht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es macht uns fest und heiter.", "tokens": ["Es", "macht", "uns", "fest", "und", "hei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "KON", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Der Mantel ist ein lustig Haus,", "tokens": ["Der", "Man\u00b7tel", "ist", "ein", "lus\u00b7tig", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drin ist gew\u00f6lbt ein Keller,", "tokens": ["Drin", "ist", "ge\u00b7w\u00f6lbt", "ein", "Kel\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da giebt es manchen sch\u00f6nen Schmaus,", "tokens": ["Da", "giebt", "es", "man\u00b7chen", "sch\u00f6\u00b7nen", "Schmaus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da geht es stets herein, heraus,", "tokens": ["Da", "geht", "es", "stets", "her\u00b7ein", ",", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und kostet keinen Heller.", "tokens": ["Und", "kos\u00b7tet", "kei\u00b7nen", "Hel\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Ein Ofen ist in diesem Haus,", "tokens": ["Ein", "O\u00b7fen", "ist", "in", "die\u00b7sem", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos":