": ["Den", "Schrei", "des", "Vol\u00b7kes", "h\u00f6rst", "du", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Och orro orro ollalu.", "tokens": ["Och", "or\u00b7ro", "or\u00b7ro", "ol\u00b7la\u00b7lu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Und scheiden soll die Mutter, wie,", "tokens": ["Und", "schei\u00b7den", "soll", "die", "Mut\u00b7ter", ",", "wie", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "ART", "NN", "$,", "PWAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von ihrem Liebchen sch\u00f6n und s\u00fc\u00df?", "tokens": ["Von", "ih\u00b7rem", "Lieb\u00b7chen", "sch\u00f6n", "und", "s\u00fc\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Warst du nicht ihres Herzens Herz,", "tokens": ["Warst", "du", "nicht", "ih\u00b7res", "Her\u00b7zens", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Puls, der ihm das Leben gab?", "tokens": ["Der", "Puls", ",", "der", "ihm", "das", "Le\u00b7ben", "gab", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Och orro orro ollalu.", "tokens": ["Och", "or\