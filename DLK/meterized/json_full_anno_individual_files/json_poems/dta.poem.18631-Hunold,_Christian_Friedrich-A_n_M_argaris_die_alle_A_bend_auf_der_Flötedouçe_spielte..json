{"dta.poem.18631": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "A n  M argaris die alle  A bend  \n auf der Fl\u00f6tedou\u00e7e  spielte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Viel wusten nicht warum die art'ge Margaris", "tokens": ["Viel", "wus\u00b7ten", "nicht", "wa\u00b7rum", "die", "art'\u00b7ge", "Mar\u00b7ga\u00b7ris"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PTKNEG", "PWAV", "ART", "ADJA", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bey jeder Abend-Stunde", "tokens": ["Bey", "je\u00b7der", "A\u00b7ben\u00b7dStun\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit ihren s\u00fcssen Munde", "tokens": ["Mit", "ih\u00b7ren", "s\u00fcs\u00b7sen", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf einer Fl\u00f6te blie\u00df:", "tokens": ["Auf", "ei\u00b7ner", "Fl\u00f6\u00b7te", "blie\u00df", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Allein anitzo kan ein jeder Sperling singen/", "tokens": ["Al\u00b7lein", "a\u00b7nit\u00b7zo", "kan", "ein", "je\u00b7der", "Sper\u00b7ling", "sin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "ART", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was doch die Ursach sey/", "tokens": ["Was", "doch", "die", "Ur\u00b7sach", "sey", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VAFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Denn jeden f\u00e4llt nun bey:", "tokens": ["Denn", "je\u00b7den", "f\u00e4llt", "nun", "bey", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Da\u00df man den Hengsten pfeift/ so offt sie sollen springen.", "tokens": ["Da\u00df", "man", "den", "Hengs\u00b7ten", "pfeift", "/", "so", "offt", "sie", "sol\u00b7len", "sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "VVFIN", "$(", "ADV", "ADV", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}