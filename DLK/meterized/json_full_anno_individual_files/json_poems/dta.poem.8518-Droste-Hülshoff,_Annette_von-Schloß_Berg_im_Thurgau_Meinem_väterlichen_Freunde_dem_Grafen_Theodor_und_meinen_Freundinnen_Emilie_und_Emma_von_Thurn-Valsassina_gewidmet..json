{"dta.poem.8518": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Schlo\u00df Berg im Thurgau   Meinem v\u00e4terlichen Freunde dem Grafen Theodor und meinen  \n Freundinnen Emilie und Emma von Thurn-Valsassina gewidmet.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Nebelsee quillt rauchend aus der Aue", "tokens": ["Ein", "Ne\u00b7bel\u00b7see", "quillt", "rau\u00b7chend", "aus", "der", "Au\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und duft\u2019ge W\u00f6lkchen treiben durch den Raum,", "tokens": ["Und", "duft'\u00b7ge", "W\u00f6lk\u00b7chen", "trei\u00b7ben", "durch", "den", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kaum graut ein Punkt im Osten noch, am Thaue", "tokens": ["Kaum", "graut", "ein", "Punkt", "im", "Os\u00b7ten", "noch", ",", "am", "Thau\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "ADV", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verlosch des Gl\u00fchwurms kleine Leuchte kaum;", "tokens": ["Ver\u00b7losch", "des", "Gl\u00fch\u00b7wurms", "klei\u00b7ne", "Leuch\u00b7te", "kaum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Horch, leises, leises Zirpen unter\u2019m Dache", "tokens": ["Horch", ",", "lei\u00b7ses", ",", "lei\u00b7ses", "Zir\u00b7pen", "un\u00b7ter'm", "Da\u00b7che"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADJA", "$,", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Verk\u00fcndet, da\u00df bereits die Schwalbe wache,", "tokens": ["Ver\u00b7k\u00fcn\u00b7det", ",", "da\u00df", "be\u00b7reits", "die", "Schwal\u00b7be", "wa\u00b7che", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "ADV", "ART", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und um manch Lager spielt ein sp\u00e4ter Traum.", "tokens": ["Und", "um", "manch", "La\u00b7ger", "spielt", "ein", "sp\u00e4\u00b7ter", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Stirn gedr\u00fcckt an meines Fensters Scheiben", "tokens": ["Die", "Stirn", "ge\u00b7dr\u00fcckt", "an", "mei\u00b7nes", "Fens\u00b7ters", "Schei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schau\u2019 sinnend ich in\u2019s duft\u2019ge Meer hinein,", "tokens": ["Schau'", "sin\u00b7nend", "ich", "in's", "duft'\u00b7ge", "Meer", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "PPER", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wie die hellen W\u00f6lkchen dr\u00fcber treiben,", "tokens": ["Und", "wie", "die", "hel\u00b7len", "W\u00f6lk\u00b7chen", "dr\u00fc\u00b7ber", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein Blick h\u00e4ngt unverwendet an dem Schein.", "tokens": ["Mein", "Blick", "h\u00e4ngt", "un\u00b7ver\u00b7wen\u00b7det", "an", "dem", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ja, dort, dort mu\u00df nun bald die Sonne steigen,", "tokens": ["Ja", ",", "dort", ",", "dort", "mu\u00df", "nun", "bald", "die", "Son\u00b7ne", "stei\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "$,", "ADV", "VMFIN", "ADV", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mir ungekannte Herrlichkeit zu zeigen;", "tokens": ["Mir", "un\u00b7ge\u00b7kann\u00b7te", "Herr\u00b7lich\u00b7keit", "zu", "zei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Dort ladet mich der Schweizermorgen ein.", "tokens": ["Dort", "la\u00b7det", "mich", "der", "Schwei\u00b7zer\u00b7mor\u00b7gen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So steh\u2019 ich wirklich denn auf deinem Grunde,", "tokens": ["So", "steh'", "ich", "wirk\u00b7lich", "denn", "auf", "dei\u00b7nem", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Besungnes Land, von dem die Fremde schw\u00e4rmt?", "tokens": ["Be\u00b7sung\u00b7nes", "Land", ",", "von", "dem", "die", "Frem\u00b7de", "schw\u00e4rmt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du meines Lebens allerfr\u00fchste Kunde", "tokens": ["Du", "mei\u00b7nes", "Le\u00b7bens", "al\u00b7ler\u00b7fr\u00fchs\u00b7te", "Kun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus einer Zeit, die noch das Herz erw\u00e4rmt,", "tokens": ["Aus", "ei\u00b7ner", "Zeit", ",", "die", "noch", "das", "Herz", "er\u00b7w\u00e4rmt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Als Eine ", "tokens": ["Als", "Ei\u00b7ne"], "token_info": ["word", "word"], "pos": ["KOUS", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "So manche liebe, hingetr\u00e4umte Stunden", "tokens": ["So", "man\u00b7che", "lie\u00b7be", ",", "hin\u00b7ge\u00b7tr\u00e4um\u00b7te", "Stun\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PIS", "VVFIN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "An allzutheuren Bildern sich geh\u00e4rmt.", "tokens": ["An", "all\u00b7zu\u00b7theu\u00b7ren", "Bil\u00b7dern", "sich", "ge\u00b7h\u00e4rmt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}