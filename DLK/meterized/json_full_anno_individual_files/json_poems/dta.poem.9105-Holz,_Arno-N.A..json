{"dta.poem.9105": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1898", "urn": "urn:nbn:de:kobv:b4-200905192685", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aus einem Kornfeld,", "tokens": ["Aus", "ei\u00b7nem", "Korn\u00b7feld", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "schr\u00e4g zum See,", "tokens": ["schr\u00e4g", "zum", "See", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "hob sich die Linde.", "tokens": ["hob", "sich", "die", "Lin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NE", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Auf schmalem Fussweg an ihr vorbei,", "tokens": ["Auf", "schma\u00b7lem", "Fuss\u00b7weg", "an", "ihr", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "jeden Nachmittag durch die Juliglut zum Baden,", "tokens": ["je\u00b7den", "Nach\u00b7mit\u00b7tag", "durch", "die", "Ju\u00b7lig\u00b7lut", "zum", "Ba\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "wir Jungens.", "tokens": ["wir", "Jun\u00b7gens", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Der blaue Himmel, die tausend gelben Bl\u00fcten, das Bienengesumm!", "tokens": ["Der", "blau\u00b7e", "Him\u00b7mel", ",", "die", "tau\u00b7send", "gel\u00b7ben", "Bl\u00fc\u00b7ten", ",", "das", "Bie\u00b7nen\u00b7ge\u00b7summ", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "CARD", "ADJA", "NN", "$,", "PRELS", "ADJD", "$."], "meter": "-+-+--+-+-+--+--+", "measure": "iambic.septa.relaxed"}}, "stanza.4": {"line.1": {"text": "Und noch immer,", "tokens": ["Und", "noch", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "wenn die Andern l\u00e4ngst unten waren,", "tokens": ["wenn", "die", "An\u00b7dern", "l\u00e4ngst", "un\u00b7ten", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "ADV", "ADV", "VAFIN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "\u2014 aus dem Wasser klang ihr Lachen und Geschrei \u2014", "tokens": ["aus", "dem", "Was\u00b7ser", "klang", "ihr", "La\u00b7chen", "und", "Ge\u00b7schrei"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "stand ich.", "tokens": ["stand", "ich", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.5": {"line.1": {"text": "Und sah den Himmel", "tokens": ["Und", "sah", "den", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "und h\u00f6rte die Bienen", "tokens": ["und", "h\u00f6r\u00b7te", "die", "Bie\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "und sog den Duft.", "tokens": ["und", "sog", "den", "Duft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}