{"dta.poem.11361": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Bey gl\u00fccklicher jahr-feyer des cr\u00f6nungs-  \n tages Seiner K\u00f6nigl. M\u00e4jest\u00e4t  \n in Preussen.  \n B. N.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wjr haben nun ein jahr mit freuden hingelegt,", "tokens": ["Wjr", "ha\u00b7ben", "nun", "ein", "jahr", "mit", "freu\u00b7den", "hin\u00b7ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Seit dem der Preussen held die k\u00f6nigs-crone tr\u00e4gt:", "tokens": ["Seit", "dem", "der", "Preus\u00b7sen", "held", "die", "k\u00f6\u00b7nigs\u00b7cro\u00b7ne", "tr\u00e4gt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer Friedrichs thaten sch\u00e4tzet,", "tokens": ["Wer", "Fried\u00b7richs", "tha\u00b7ten", "sch\u00e4t\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der glaubt, es sey bereits ein ", "tokens": ["Der", "glaubt", ",", "es", "sey", "be\u00b7reits", "ein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "PPER", "VAFIN", "ADV", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wen fried und ruh ergetzet,", "tokens": ["Wen", "fried", "und", "ruh", "er\u00b7get\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der meinet, da\u00df es kaum ein tag gewesen sey.", "tokens": ["Der", "mei\u00b7net", ",", "da\u00df", "es", "kaum", "ein", "tag", "ge\u00b7we\u00b7sen", "sey", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "VAPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wer freut sich nicht mit mir ob dieser g\u00fcldnen zeit?", "tokens": ["Wer", "freut", "sich", "nicht", "mit", "mir", "ob", "die\u00b7ser", "g\u00fcld\u00b7nen", "zeit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "PTKNEG", "APPR", "PPER", "KOUS", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mars steht in vollen waffen,", "tokens": ["Mars", "steht", "in", "vol\u00b7len", "waf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Und giebt der gantzen welt mehr, als sie w\u00fcnscht, zu schaffen:", "tokens": ["Und", "giebt", "der", "gant\u00b7zen", "welt", "mehr", ",", "als", "sie", "w\u00fcnscht", ",", "zu", "schaf\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Bey uns r\u00fchrt er sich auch; doch voll gerechtigkeit!", "tokens": ["Bey", "uns", "r\u00fchrt", "er", "sich", "auch", ";", "doch", "voll", "ge\u00b7rech\u00b7tig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "PRF", "ADV", "$.", "ADV", "ADJD", "NN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Da andre reiche schon auf ihr verderben zeigen,", "tokens": ["Da", "and\u00b7re", "rei\u00b7che", "schon", "auf", "ihr", "ver\u00b7der\u00b7ben", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "ADV", "APPR", "PPER", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So sieht man unsers steigen.", "tokens": ["So", "sieht", "man", "un\u00b7sers", "stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Durch wessen krafft, wo nicht durch dich, o Friderich?", "tokens": ["Durch", "wes\u00b7sen", "krafft", ",", "wo", "nicht", "durch", "dich", ",", "o", "Fri\u00b7de\u00b7rich", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PWAV", "PTKNEG", "APPR", "PPER", "$,", "FM", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Indem du fertig stehst, f\u00fcr deinen freund zu schlagen,", "tokens": ["In\u00b7dem", "du", "fer\u00b7tig", "stehst", ",", "f\u00fcr", "dei\u00b7nen", "freund", "zu", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So h\u00f6rest du zugleich auf deiner b\u00fcrger klagen;", "tokens": ["So", "h\u00f6\u00b7rest", "du", "zu\u00b7gleich", "auf", "dei\u00b7ner", "b\u00fcr\u00b7ger", "kla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und ist der tag so kurtz, nimmst du die nacht darzu.", "tokens": ["Und", "ist", "der", "tag", "so", "kurtz", ",", "nimmst", "du", "die", "nacht", "dar\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "ADJD", "$,", "VVFIN", "PPER", "ART", "NN", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Recht schaffen, kirchen bauen,", "tokens": ["Recht", "schaf\u00b7fen", ",", "kir\u00b7chen", "bau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Sind dinge, die wir hier zu allen zeiten schauen.", "tokens": ["Sind", "din\u00b7ge", ",", "die", "wir", "hier", "zu", "al\u00b7len", "zei\u00b7ten", "schau\u00b7en", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "PRELS", "PPER", "ADV", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Du h\u00e4ltst in einer hand Europ\u2019 und Preussens ruh:", "tokens": ["Du", "h\u00e4ltst", "in", "ei\u00b7ner", "hand", "Eu\u00b7rop'", "und", "Preus\u00b7sens", "ruh", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NE", "KON", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Nur ist der unterscheid,", "tokens": ["Nur", "ist", "der", "un\u00b7ter\u00b7scheid", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Da\u00df du den grund zur ersten mit dem degen,", "tokens": ["Da\u00df", "du", "den", "grund", "zur", "ers\u00b7ten", "mit", "dem", "de\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPRART", "ADJA", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "Zur andern, wie bekannt, mit liebe pflegst zu legen.", "tokens": ["Zur", "an\u00b7dern", ",", "wie", "be\u00b7kannt", ",", "mit", "lie\u00b7be", "pflegst", "zu", "le\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "PWAV", "ADJD", "$,", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wiewohl du w\u00fcrdest nie mit krieg und waffen dr\u00e4un,", "tokens": ["Wie\u00b7wohl", "du", "w\u00fcr\u00b7dest", "nie", "mit", "krieg", "und", "waf\u00b7fen", "dr\u00e4un", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "ADV", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "K\u00f6nt alle welt, wie du, nur klug und friedlich seyn.", "tokens": ["K\u00f6nt", "al\u00b7le", "welt", ",", "wie", "du", ",", "nur", "klug", "und", "fried\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$,", "PWAV", "PPER", "$,", "ADV", "ADJD", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "O himmel! der du uns so hoch und theur begl\u00fcckt,", "tokens": ["O", "him\u00b7mel", "!", "der", "du", "uns", "so", "hoch", "und", "theur", "be\u00b7gl\u00fcckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ART", "PPER", "PRF", "ADV", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Der du in einen held di\u00df alles eingedr\u00fcckt,", "tokens": ["Der", "du", "in", "ei\u00b7nen", "held", "di\u00df", "al\u00b7les", "ein\u00b7ge\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "VVFIN", "PDS", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Was seiner v\u00e4ter geist nur eintzeln hat besessen;", "tokens": ["Was", "sei\u00b7ner", "v\u00e4\u00b7ter", "geist", "nur", "eint\u00b7zeln", "hat", "be\u00b7ses\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "ADV", "VVINF", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "La\u00df seiner jahre ziel nach der natur nicht messen!", "tokens": ["La\u00df", "sei\u00b7ner", "jah\u00b7re", "ziel", "nach", "der", "na\u00b7tur", "nicht", "mes\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Wir schaun, was uns vor dem in eilfen war gegeben:", "tokens": ["Wir", "schaun", ",", "was", "uns", "vor", "dem", "in", "eil\u00b7fen", "war", "ge\u00b7ge\u00b7ben", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "PPER", "APPR", "ART", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Ach! la\u00df ihn auch so lang, als ihrer eilfe, leben!", "tokens": ["Ach", "!", "la\u00df", "ihn", "auch", "so", "lang", ",", "als", "ih\u00b7rer", "eil\u00b7fe", ",", "le\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "$.", "VVIMP", "PPER", "ADV", "ADV", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}