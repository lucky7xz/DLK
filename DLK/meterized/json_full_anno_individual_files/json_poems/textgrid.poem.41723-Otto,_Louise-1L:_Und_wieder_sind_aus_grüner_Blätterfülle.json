{"textgrid.poem.41723": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und wieder sind aus gr\u00fcner Bl\u00e4tterf\u00fclle", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und wieder sind aus gr\u00fcner Bl\u00e4tterf\u00fclle", "tokens": ["Und", "wie\u00b7der", "sind", "aus", "gr\u00fc\u00b7ner", "Bl\u00e4t\u00b7ter\u00b7f\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Viel Blumen zauberhaft hervorgeeilt,", "tokens": ["Viel", "Blu\u00b7men", "zau\u00b7ber\u00b7haft", "her\u00b7vor\u00b7ge\u00b7eilt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie dr\u00e4ngen sich heraus in Pracht und F\u00fclle", "tokens": ["Sie", "dr\u00e4n\u00b7gen", "sich", "he\u00b7raus", "in", "Pracht", "und", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APZR", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als h\u00e4tten sie zu lang versteckt geweilt,", "tokens": ["Als", "h\u00e4t\u00b7ten", "sie", "zu", "lang", "ver\u00b7steckt", "ge\u00b7weilt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "PTKA", "ADJD", "VVPP", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und schauen auf, so wie vom Meeresgrund,", "tokens": ["Und", "schau\u00b7en", "auf", ",", "so", "wie", "vom", "Mee\u00b7res\u00b7grund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$,", "ADV", "KOKOM", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Dem gr\u00fcnen, holde Feen sich erheben", "tokens": ["Dem", "gr\u00fc\u00b7nen", ",", "hol\u00b7de", "Feen", "sich", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "PRF", "VVINF"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und lockend gr\u00fc\u00dfen, gr\u00fc\u00dft der Blumenmund", "tokens": ["Und", "lo\u00b7ckend", "gr\u00fc\u00b7\u00dfen", ",", "gr\u00fc\u00dft", "der", "Blu\u00b7men\u00b7mund"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "VVINF", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und l\u00e4\u00dft statt Seufzer s\u00fc\u00dfe D\u00fcfte schweben.", "tokens": ["Und", "l\u00e4\u00dft", "statt", "Seuf\u00b7zer", "s\u00fc\u00b7\u00dfe", "D\u00fcf\u00b7te", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch wie sie auch um Sch\u00f6nheitspreise ringen,", "tokens": ["Doch", "wie", "sie", "auch", "um", "Sch\u00f6n\u00b7heits\u00b7prei\u00b7se", "rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Rose nur scheint mir des Liedes wert", "tokens": ["Die", "Ro\u00b7se", "nur", "scheint", "mir", "des", "Lie\u00b7des", "wert"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER", "ART", "NN", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Wie wir es jetzt in Kriegerweisen singen:", "tokens": ["Wie", "wir", "es", "jetzt", "in", "Krie\u00b7ger\u00b7wei\u00b7sen", "sin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie tr\u00e4gt den Dorn als drohend rotes Schwert,", "tokens": ["Sie", "tr\u00e4gt", "den", "Dorn", "als", "dro\u00b7hend", "ro\u00b7tes", "Schwert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KOKOM", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein Tropfen Tau in ihrem Angesicht", "tokens": ["Ein", "Trop\u00b7fen", "Tau", "in", "ih\u00b7rem", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Das feurig strahlt im hohen Purpurglanze", "tokens": ["Das", "feu\u00b7rig", "strahlt", "im", "ho\u00b7hen", "Pur\u00b7pur\u00b7glan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADJD", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Blickt sie wie tr\u00e4umend nach dem Himmelslicht,", "tokens": ["Blickt", "sie", "wie", "tr\u00e4u\u00b7mend", "nach", "dem", "Him\u00b7mels\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wohl ohne Schild, doch nimmer ohne Lanze.", "tokens": ["Wohl", "oh\u00b7ne", "Schild", ",", "doch", "nim\u00b7mer", "oh\u00b7ne", "Lan\u00b7ze", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "ADV", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nicht mit der Liebe mag ich sie vergleichen,", "tokens": ["Nicht", "mit", "der", "Lie\u00b7be", "mag", "ich", "sie", "ver\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie ihr vordem im Minnelied gethan.", "tokens": ["Wie", "ihr", "vor\u00b7dem", "im", "Min\u00b7ne\u00b7lied", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie sei f\u00fcr mich der Dichtung heilig Zeichen", "tokens": ["Sie", "sei", "f\u00fcr", "mich", "der", "Dich\u00b7tung", "hei\u00b7lig", "Zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie ich ihr folgt auf meiner Lebensbahn.", "tokens": ["Wie", "ich", "ihr", "folgt", "auf", "mei\u00b7ner", "Le\u00b7bens\u00b7bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Rose hei\u00dft der Blumen K\u00f6nigin,", "tokens": ["Die", "Ro\u00b7se", "hei\u00dft", "der", "Blu\u00b7men", "K\u00f6\u00b7ni\u00b7gin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ihr will man stets den ersten Preis gew\u00e4hren,", "tokens": ["Ihr", "will", "man", "stets", "den", "ers\u00b7ten", "Preis", "ge\u00b7w\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Im kindschen Spiel liegt oft ein hoher Sinn,", "tokens": ["Im", "kind\u00b7schen", "Spiel", "liegt", "oft", "ein", "ho\u00b7her", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der Dichter sagt's, ich will's Euch jetzt erkl\u00e4ren.", "tokens": ["Der", "Dich\u00b7ter", "sagt's", ",", "ich", "will's", "Euch", "jetzt", "er\u00b7kl\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich hab verlernt ein Minnelied zu singen,", "tokens": ["Ich", "hab", "ver\u00b7lernt", "ein", "Min\u00b7ne\u00b7lied", "zu", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den alten Reim von Herz und Schmerz verlernt.", "tokens": ["Den", "al\u00b7ten", "Reim", "von", "Herz", "und", "Schmerz", "ver\u00b7lernt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich kann der Muse nie ein Opfer bringen,", "tokens": ["Ich", "kann", "der", "Mu\u00b7se", "nie", "ein", "Op\u00b7fer", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das von dem Hochaltare mich entfernt", "tokens": ["Das", "von", "dem", "Hoc\u00b7hal\u00b7ta\u00b7re", "mich", "ent\u00b7fernt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "NN", "PPER", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Auf dem der Freiheit heilig Feuer flammt,", "tokens": ["Auf", "dem", "der", "Frei\u00b7heit", "hei\u00b7lig", "Feu\u00b7er", "flammt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In dessen Dienst ich mutig mich begeben,", "tokens": ["In", "des\u00b7sen", "Dienst", "ich", "mu\u00b7tig", "mich", "be\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PPER", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Es ist ein kriegerisches Priesteramt", "tokens": ["Es", "ist", "ein", "krie\u00b7ge\u00b7ri\u00b7sches", "Pries\u00b7ter\u00b7amt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und Kampfeslieder nur kann ich erheben.", "tokens": ["Und", "Kamp\u00b7fes\u00b7lie\u00b7der", "nur", "kann", "ich", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und diese Lieder wolltet Ihr verwehren,", "tokens": ["Und", "die\u00b7se", "Lie\u00b7der", "woll\u00b7tet", "Ihr", "ver\u00b7weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verrat sie nennen an der Poesie?", "tokens": ["Ver\u00b7rat", "sie", "nen\u00b7nen", "an", "der", "Poe\u00b7sie", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}, "line.3": {"text": "Ihr nennts die Kunst die himmlische entehren", "tokens": ["Ihr", "nennts", "die", "Kunst", "die", "himm\u00b7li\u00b7sche", "en\u00b7teh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn unser Ringen Waffen ihr verlieh?", "tokens": ["Wenn", "un\u00b7ser", "Rin\u00b7gen", "Waf\u00b7fen", "ihr", "ver\u00b7lieh", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein Lied das k\u00e4mpfen will im Dienst der Zeit,", "tokens": ["Ein", "Lied", "das", "k\u00e4mp\u00b7fen", "will", "im", "Dienst", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VVINF", "VMFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der holden Rose ist es zu vergleichen:", "tok