{"dta.poem.11024": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Unbillige  \n freywillige Blindheit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Menschen, welche die Vernunft vom Dienste Gottes\ng\u00e4nzlich trennen,", "tokens": ["Die", "Men\u00b7schen", ",", "wel\u00b7che", "die", "Ver\u00b7nunft", "vom", "Diens\u00b7te", "Got\u00b7tes", "g\u00e4nz\u00b7lich", "tren\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "APPRART", "ADJA", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.2": {"text": "Die reissen sich die Augen aus, damit sie besser sehen k\u00f6nnen.", "tokens": ["Die", "reis\u00b7sen", "sich", "die", "Au\u00b7gen", "aus", ",", "da\u00b7mit", "sie", "bes\u00b7ser", "se\u00b7hen", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$,", "KOUS", "PPER", "ADJD", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}}}}}