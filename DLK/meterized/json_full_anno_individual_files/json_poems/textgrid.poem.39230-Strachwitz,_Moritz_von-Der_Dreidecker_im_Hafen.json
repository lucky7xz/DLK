{"textgrid.poem.39230": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "Der Dreidecker im Hafen", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im wellenlosen Hafensumpf,", "tokens": ["Im", "wel\u00b7len\u00b7lo\u00b7sen", "Ha\u00b7fen\u00b7sumpf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den nie die Brise frisch durchpfiff,", "tokens": ["Den", "nie", "die", "Bri\u00b7se", "frisch", "durch\u00b7pfiff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Liegt abgetakelt, tr\u00e4g' und stumpf", "tokens": ["Liegt", "ab\u00b7ge\u00b7ta\u00b7kelt", ",", "tr\u00e4g'", "und", "stumpf"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVPP", "$,", "VVFIN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Hundertzehn-Kanonen-Schiff.", "tokens": ["Das", "Hun\u00b7dert\u00b7zehn\u00b7Ka\u00b7no\u00b7nen\u00b7Schiff", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es liegt ein schwertberaubter Held,", "tokens": ["Es", "liegt", "ein", "schwert\u00b7be\u00b7raub\u00b7ter", "Held", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Kerker fault sein stolz' Gebein,", "tokens": ["Im", "Ker\u00b7ker", "fault", "sein", "stolz'", "Ge\u00b7bein", ","], "token_info": ["word", "word", "word", "word", "word", "word