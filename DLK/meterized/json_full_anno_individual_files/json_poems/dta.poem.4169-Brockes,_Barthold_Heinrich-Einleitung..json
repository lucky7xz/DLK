{"dta.poem.4169": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Einleitung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zufrieden mit mir selbst, verbringe,", "tokens": ["Zu\u00b7frie\u00b7den", "mit", "mir", "selbst", ",", "ver\u00b7brin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ADV", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein selbst verfertigt Liedchen singe,", "tokens": ["Ein", "selbst", "ver\u00b7fer\u00b7tigt", "Lied\u00b7chen", "sin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vergangner Widerw\u00e4rtigkeit,", "tokens": ["Ver\u00b7gang\u00b7ner", "Wi\u00b7der\u00b7w\u00e4r\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach \u00fcberstandnem Sturm, vergesse,", "tokens": ["Nach", "\u00fc\u00b7bers\u00b7tand\u00b7nem", "Sturm", ",", "ver\u00b7ges\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und, in vern\u00fcnftiger Gelassenheit,", "tokens": ["Und", ",", "in", "ver\u00b7n\u00fcnf\u00b7ti\u00b7ger", "Ge\u00b7las\u00b7sen\u00b7heit", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mein gegenw\u00e4rtigs Gl\u00fcck ermesse.", "tokens": ["Mein", "ge\u00b7gen\u00b7w\u00e4r\u00b7tigs", "Gl\u00fcck", "er\u00b7mes\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Jetzt will ich hier, in meines Sch\u00f6pfers Werken,", "tokens": ["Jetzt", "will", "ich", "hier", ",", "in", "mei\u00b7nes", "Sch\u00f6p\u00b7fers", "Wer\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Sein\u2019 Allmacht, Lieb und Weisheit merken.", "tokens": ["Sein'", "All\u00b7macht", ",", "Lieb", "und", "Weis\u00b7heit", "mer\u00b7ken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Jetzt will ich, halb entz\u00fcckt, bald von der Sonnen Gluht,", "tokens": ["Jetzt", "will", "ich", ",", "halb", "ent\u00b7z\u00fcckt", ",", "bald", "von", "der", "Son\u00b7nen", "Gluht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "ADJD", "VVPP", "$,", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Bald von der tiefen Meeres-Fluht,", "tokens": ["Bald", "von", "der", "tie\u00b7fen", "Mee\u00b7res\u00b7Fluht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Bald von der Erden Pracht und Zier,", "tokens": ["Bald", "von", "der", "Er\u00b7den", "Pracht", "und", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Jetzt dort, dann wieder hier,", "tokens": ["Jetzt", "dort", ",", "dann", "wie\u00b7der", "hier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Zu aller Dinge Quell, dem grossen All zu Ehren,", "tokens": ["Zu", "al\u00b7ler", "Din\u00b7ge", "Quell", ",", "dem", "gros\u00b7sen", "All", "zu", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "$,", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Beseelte Lieder lassen h\u00f6ren.", "tokens": ["Be\u00b7seel\u00b7te", "Lie\u00b7der", "las\u00b7sen", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Zu diesem Endzweck st\u00e4rk, o Sch\u00f6pfer, meine", "tokens": ["Zu", "die\u00b7sem", "End\u00b7zweck", "st\u00e4rk", ",", "o", "Sch\u00f6p\u00b7fer", ",", "mei\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "$,", "FM", "NN", "$,", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Sinnen,", "tokens": ["Sin\u00b7nen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "Und segne, fernerhin, wie vormahls, mein Be-", "tokens": ["Und", "seg\u00b7ne", ",", "fer\u00b7ner\u00b7hin", ",", "wie", "vor\u00b7mahls", ",", "mein", "Be"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PTKVZ", "$,", "PWAV", "ADV", "$,", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.18": {"text": "ginnen!", "tokens": ["gin\u00b7nen", "!"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}