{"textgrid.poem.23923": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sch\u00f6nes Kind, ich denk an dich,", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6nes Kind, ich denk an dich,", "tokens": ["Sch\u00f6\u00b7nes", "Kind", ",", "ich", "denk", "an", "dich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil die Geigen klingen", "tokens": ["Weil", "die", "Gei\u00b7gen", "klin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und im Herzen wunderlich", "tokens": ["Und", "im", "Her\u00b7zen", "wun\u00b7der\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Stille Stimmen singen.", "tokens": ["Stil\u00b7le", "Stim\u00b7men", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Sch\u00f6nes Kind, die Geige wei\u00df,", "tokens": ["Sch\u00f6\u00b7nes", "Kind", ",", "die", "Gei\u00b7ge", "wei\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie ich dich ersehne,", "tokens": ["Wie", "ich", "dich", "er\u00b7seh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "