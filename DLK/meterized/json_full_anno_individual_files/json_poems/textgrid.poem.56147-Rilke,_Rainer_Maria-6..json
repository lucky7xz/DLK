{"textgrid.poem.56147": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rose, du thronende, denen im Altertume", "tokens": ["Ro\u00b7se", ",", "du", "thro\u00b7nen\u00b7de", ",", "de\u00b7nen", "im", "Al\u00b7ter\u00b7tu\u00b7me"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PPER", "VVFIN", "$,", "PRELS", "APPRART", "NN"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.2": {"text": "warst du ein Kelch mit einfachem Rand.", "tokens": ["warst", "du", "ein", "Kelch", "mit", "ein\u00b7fa\u00b7chem", "Rand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "der unersch\u00f6pfliche Gegenstand.", "tokens": ["der", "un\u00b7er\u00b7sch\u00f6pf\u00b7li\u00b7che", "Ge\u00b7gen\u00b7stand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "In deinem Reichtum scheinst du wie Kleidung um Kleidung", "tokens": ["In", "dei\u00b7nem", "Reich\u00b7tum", "scheinst", "du", "wie", "Klei\u00b7dung", "um", "Klei\u00b7dung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "KOKOM", "NN", "APPR", "NN"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "um einen Leib aus nichts als Glanz;", "tokens": ["um", "ei\u00b7nen", "Leib", "aus", "nichts", "als", "Glanz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PIS", "KOKOM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "aber dein einzelnes Blatt ist zugleich die Vermeidung", "tokens": ["a\u00b7ber", "dein", "ein\u00b7zel\u00b7nes", "Blatt", "ist", "zu\u00b7gleich", "die", "Ver\u00b7mei\u00b7dung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VAFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "und die Verleugnung jedes Gewands.", "tokens": ["und", "die", "Ver\u00b7leug\u00b7nung", "je\u00b7des", "Ge\u00b7wands", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Seit Jahrhunderten ruft uns dein Duft", "tokens": ["Seit", "Jahr\u00b7hun\u00b7der\u00b7ten", "ruft", "uns", "dein", "Duft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "seine s\u00fc\u00dfesten Namen her\u00fcber;", "tokens": ["sei\u00b7ne", "s\u00fc\u00b7\u00dfes\u00b7ten", "Na\u00b7men", "her\u00b7\u00fc\u00b7ber", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "pl\u00f6tzlich liegt er wie Ruhm in der Luft.", "tokens": ["pl\u00f6tz\u00b7lich", "liegt", "er", "wie", "Ruhm", "in", "der", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "KOKOM", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Dennoch, wir wissen ihn nicht zu nennen, wir raten...", "tokens": ["Den\u00b7noch", ",", "