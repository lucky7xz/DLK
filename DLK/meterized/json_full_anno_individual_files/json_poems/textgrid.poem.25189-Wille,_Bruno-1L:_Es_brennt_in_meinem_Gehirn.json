{"textgrid.poem.25189": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es brennt in meinem Gehirn", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es brennt in meinem Gehirn", "tokens": ["Es", "brennt", "in", "mei\u00b7nem", "Ge\u00b7hirn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Ein Traum mit g\u00e4hrender Glut,", "tokens": ["Ein", "Traum", "mit", "g\u00e4h\u00b7ren\u00b7der", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wie hinter Vesuvius' Felsenstirn", "tokens": ["Wie", "hin\u00b7ter", "Ve\u00b7su\u00b7vi\u00b7us'", "Fel\u00b7sen\u00b7stirn"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NE", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Erde fieberndes Feuerblut. \u2013", "tokens": ["Der", "Er\u00b7de", "fie\u00b7bern\u00b7des", "Feu\u00b7er\u00b7blut", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ich tr\u00e4ume die kommende Sonne.", "tokens": ["Ich", "tr\u00e4u\u00b7me", "die", "kom\u00b7men\u00b7de", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Und wie des Meeres Fluth empor", "tokens": ["Und", "wie", "des", "Mee\u00b7res", "Fluth", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum lockenden Monde schwillt,", "tokens": ["Zum", "lo\u00b7cken\u00b7den", "Mon\u00b7de", "schwillt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wallt meine Seele schmachtend", "tokens": ["Wallt", "mei\u00b7ne", "See\u00b7le", "schmach\u00b7tend"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dem angebeteten Traumgebild", "tokens": ["Dem", "an\u00b7ge\u00b7be\u00b7te\u00b7ten", "Traum\u00b7ge\u00b7bild"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Entgegen \u2013 der kommenden Sonne.", "tokens": ["Ent\u00b7ge\u00b7gen", "\u2013", "der", "kom\u00b7men\u00b7den", "Son\u00b7ne", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "In stummer Nacht, dem weichen Arm", "tokens": ["In", "stum\u00b7mer", "Nacht", ",", "dem", "wei\u00b7chen", "Arm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schl\u00e4fernder Ruh entwunden,", "tokens": ["Schl\u00e4\u00b7fern\u00b7der", "Ruh", "ent\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "W\u00e4lz ich mich mit hei\u00dfem Sehnen,", "tokens": ["W\u00e4lz", "ich", "mich", "mit", "hei\u00b7\u00dfem", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fclle mit Gr\u00fcbeln z\u00f6gernde Stunden", "tokens": ["F\u00fcl\u00b7le", "mit", "Gr\u00fc\u00b7beln", "z\u00f6\u00b7gern\u00b7de", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "ADJA", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Und harre der kommenden Sonne.", "tokens": ["Und", "har\u00b7re", "der", "kom\u00b7men\u00b7den", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Vom Lager fahr' ich wild empor,", "tokens": ["Vom", "La\u00b7ger", "fahr'", "ich", "wild", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wissende B\u00fccher aufzuschlagen;", "tokens": ["Wis\u00b7sen\u00b7de", "B\u00fc\u00b7cher", "auf\u00b7zu\u00b7schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVIZU", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Ihr starren Z\u00fcge, la\u00dft mich lesen:", "tokens": ["Ihr", "star\u00b7ren", "Z\u00fc\u00b7ge", ",", "la\u00dft", "mich", "le\u00b7sen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VVIMP", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wann wird umnachteten V\u00f6lkern tagen", "tokens": ["Wann", "wird", "um\u00b7nach\u00b7te\u00b7ten", "V\u00f6l\u00b7kern", "ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ADJA", "NN", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Die selig machende Sonne?", "tokens": ["Die", "se\u00b7lig", "ma\u00b7chen\u00b7de", "Son\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Es treibt mich auf die Gassen hinaus;", "tokens": ["Es", "treibt", "mich", "auf", "die", "Gas\u00b7sen", "hin\u00b7aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Da athmen die Gassen Moderluft;", "tokens": ["Da", "ath\u00b7men", "die", "Gas\u00b7sen", "Mo\u00b7der\u00b7luft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ein steinerner Sarg jedwedes Haus,", "tokens": ["Ein", "stei\u00b7ner\u00b7ner", "Sarg", "jed\u00b7we\u00b7des", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PIAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Stadt eine riesige Gruft. \u2013", "tokens": ["Die", "Stadt", "ei\u00b7ne", "rie\u00b7si\u00b7ge", "Gruft", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Erbarme dich, kommende Sonne!", "tokens": ["Er\u00b7bar\u00b7me", "dich", ",", "kom\u00b7men\u00b7de", "Son\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.6": {"line.1": {"text": "Und schaudernd durch das Thor der Gruft", "tokens": ["Und", "schau\u00b7dernd", "durch", "das", "Thor", "der", "Gruft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Fl\u00fccht' ich hinaus auf offnes Feld,", "tokens": ["Fl\u00fccht'", "ich", "hin\u00b7aus", "auf", "off\u00b7nes", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu sp\u00e4hen, ob die finstre Luft", "tokens": ["Zu", "sp\u00e4\u00b7hen", ",", "ob", "die", "finst\u00b7re", "Luft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Morgenschimmer nicht erhellt.", "tokens": ["Ein", "Mor\u00b7gen\u00b7schim\u00b7mer", "nicht", "er\u00b7hellt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich ahne die kommende Sonne.", "tokens": ["Ich", "ah\u00b7ne", "die", "kom\u00b7men\u00b7de", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.7": {"line.1": {"text": "Und sieh, des Lichtes Halme schie\u00dfen", "tokens": ["Und", "sieh", ",", "des", "Lich\u00b7tes", "Hal\u00b7me", "schie\u00b7\u00dfen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Empor vom grauen Himmelsstrande,", "tokens": ["Em\u00b7por", "vom", "grau\u00b7en", "Him\u00b7mels\u00b7stran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie hinter schwarzem Schildesrande", "tokens": ["Wie", "hin\u00b7ter", "schwar\u00b7zem", "Schil\u00b7des\u00b7ran\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Blutige Speere sprie\u00dfen.", "tokens": ["Blu\u00b7ti\u00b7ge", "Spee\u00b7re", "sprie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Das sind die Speere der Sonne!", "tokens": ["Das", "sind", "die", "Spee\u00b7re", "der", "Son\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Da weicht der Drache der Verwesung", "tokens": ["Da", "weicht", "der", "Dra\u00b7che", "der", "Ver\u00b7we\u00b7sung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von seinem Nest, der V\u00f6lkergruft;", "tokens": ["Von", "sei\u00b7nem", "Nest", ",", "der", "V\u00f6l\u00b7ker\u00b7gruft", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er faltet die zackigen Fl\u00fcgel", "tokens": ["Er", "fal\u00b7tet", "die", "za\u00b7cki\u00b7gen", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und kriecht entsetzt in eine Schluft. \u2013", "tokens": ["Und", "kriecht", "ent\u00b7setzt", "in", "ei\u00b7ne", "Schluft", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Preis dir, siegende Sonne!", "tokens": ["Preis", "dir", ",", "sie\u00b7gen\u00b7de", "Son\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.9": {"line.1": {"text": "Nun taucht am froh err\u00f6thenden Himmel", "tokens": ["Nun", "taucht", "am", "froh", "er\u00b7r\u00f6\u00b7then\u00b7den", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PTKA", "ADJD", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Empor der rollende Feuerball.", "tokens": ["Em\u00b7por", "der", "rol\u00b7len\u00b7de", "Feu\u00b7er\u00b7ball", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da zittert die Erde, da bersten", "tokens": ["Da", "zit\u00b7tert", "die", "Er\u00b7de", ",", "da", "bers\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die Riesens\u00e4rge mit Donnerschall. \u2013", "tokens": ["Die", "Rie\u00b7sen\u00b7s\u00e4r\u00b7ge", "mit", "Don\u00b7ner\u00b7schall", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Preis dir, erl\u00f6sende Sonne!", "tokens": ["Preis", "dir", ",", "er\u00b7l\u00f6\u00b7sen\u00b7de", "Son\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.10": {"line.1": {"text": "Die toten V\u00f6lker stehen auf", "tokens": ["Die", "to\u00b7ten", "V\u00f6l\u00b7ker", "ste\u00b7hen", "auf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und baden im goldig str\u00f6menden Licht;", "tokens": ["Und", "ba\u00b7den", "im", "gol\u00b7dig", "str\u00f6\u00b7men\u00b7den", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJD", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Leiber bl\u00fchen sch\u00f6n und stark,", "tokens": ["Die", "Lei\u00b7ber", "bl\u00fc\u00b7hen", "sch\u00f6n", "und", "stark", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und geistig strahlt das Angesicht. \u2013", "tokens": ["Und", "geis\u00b7tig", "strahlt", "das", "An\u00b7ge\u00b7sicht", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Preis dir, erweckende Sonne!", "tokens": ["Preis", "dir", ",", "er\u00b7we\u00b7cken\u00b7de", "Son\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.11": {"line.1": {"text": "Die Erde schimmert wie eine Braut", "tokens": ["Die", "Er\u00b7de", "schim\u00b7mert", "wie", "ei\u00b7ne", "Braut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Im Schmuck der Blumen und Seen;", "tokens": ["Im", "Schmuck", "der", "Blu\u00b7men", "und", "Seen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Hinter \u00fcppig gr\u00fcnenden Hainen", "tokens": ["Hin\u00b7ter", "\u00fcp\u00b7pig", "gr\u00fc\u00b7nen\u00b7den", "Hai\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Marmorh\u00e4user erstehen. \u2013", "tokens": ["Mar\u00b7mor\u00b7h\u00e4u\u00b7ser", "er\u00b7ste\u00b7hen", ".", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["NE", "VVINF", "$.", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Preis dir, verkl\u00e4rende Sonne!", "tokens": ["Preis", "dir", ",", "ver\u00b7kl\u00e4\u00b7ren\u00b7de", "Son\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.12": {"line.1": {"text": "Und aus den Thoren der Marmorstadt", "tokens": ["Und", "aus", "den", "Tho\u00b7ren", "der", "Mar\u00b7mor\u00b7stadt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wallt des Volkes festliche Schaar,", "tokens": ["Wallt", "