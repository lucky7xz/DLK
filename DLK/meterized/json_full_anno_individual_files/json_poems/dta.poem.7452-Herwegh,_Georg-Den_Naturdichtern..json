{"dta.poem.7452": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Den Naturdichtern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Titan und Zwerg, das Gro\u00dfe, wie das Kleine,", "tokens": ["Ti\u00b7tan", "und", "Zwerg", ",", "das", "Gro\u00b7\u00dfe", ",", "wie", "das", "Klei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "ART", "ADJA", "$,", "PWAV", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist Poesie, und Poesie im Halme,", "tokens": ["Ist", "Poe\u00b7sie", ",", "und", "Poe\u00b7sie", "im", "Hal\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "KON", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Wie in des Orientes stolzer Palme,", "tokens": ["Wie", "in", "des", "O\u00b7ri\u00b7en\u00b7tes", "stol\u00b7zer", "Pal\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Poesie noch in der Weisen Steine;", "tokens": ["Und", "Poe\u00b7sie", "noch", "in", "der", "Wei\u00b7sen", "Stei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Und Poesie die M\u00fcck' im Sonnenscheine,", "tokens": ["Und", "Poe\u00b7sie", "die", "M\u00fcck", "im", "Son\u00b7nen\u00b7schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und Poesie in eines Dampfschiffs Qualme,", "tokens": ["Und", "Poe\u00b7sie", "in", "ei\u00b7nes", "Dampf\u00b7schiffs", "Qual\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und Poesie auf einer Schweizeralme,", "tokens": ["Und", "Poe\u00b7sie", "auf", "ei\u00b7ner", "Schwei\u00b7ze\u00b7ral\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und Poesie vor Allem auch im Weine.", "tokens": ["Und", "Poe\u00b7sie", "vor", "Al\u00b7lem", "auch", "im", "Wei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PIS", "ADV", "APPRART", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Wo Euch des Himmels heil'ge Luft umweht,", "tokens": ["Wo", "Euch", "des", "Him\u00b7mels", "heil'\u00b7ge", "Luft", "um\u00b7weht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da rauscht die Poesie mit ihren Schwingen;", "tokens": ["Da", "rauscht", "die", "Poe\u00b7sie", "mit", "ih\u00b7ren", "Schwin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Wie Gott, ist sie zuletzt in allen Dingen:", "tokens": ["Wie", "Gott", ",", "ist", "sie", "zu\u00b7letzt", "in", "al\u00b7len", "Din\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "VAFIN", "PPER", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch wenn einmal ein L\u00f6we vor Euch steht,", "tokens": ["Doch", "wenn", "ein\u00b7mal", "ein", "L\u00f6\u00b7we", "vor", "Euch", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ART", "NE", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sollt Ihr nicht das Insekt auf ihm besingen.", "tokens": ["Sollt", "Ihr", "nicht", "das", "In\u00b7sekt", "auf", "ihm", "be\u00b7sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ART", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "+----+-+-+-", "measure": "dactylic.init"}}}}}