{"dta.poem.9619": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Nacht-lied.  \n J. v. M.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr stillen l\u00fcffte dieser nacht/", "tokens": ["Ihr", "stil\u00b7len", "l\u00fcff\u00b7te", "die\u00b7ser", "nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVFIN", "PDAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit denen ich zum \u00f6fftern schwatze/", "tokens": ["Mit", "de\u00b7nen", "ich", "zum", "\u00f6ff\u00b7tern", "schwat\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "APPRART", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Fangt auff den thon/ den meine rede macht/", "tokens": ["Fangt", "auff", "den", "thon", "/", "den", "mei\u00b7ne", "re\u00b7de", "macht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$(", "ART", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und tragt ihn hin nach jenem platze/", "tokens": ["Und", "tragt", "ihn", "hin", "nach", "je\u00b7nem", "plat\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "PDAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da wo mein engel liegt/", "tokens": ["Da", "wo", "mein", "en\u00b7gel", "liegt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und in der hut der sch\u00f6nsten Amoretten", "tokens": ["Und", "in", "der", "hut", "der", "sch\u00f6ns\u00b7ten", "A\u00b7mo\u00b7ret\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Auff schwanen-brust und feder-betten", "tokens": ["Auff", "schwa\u00b7nen\u00b7brust", "und", "fe\u00b7der\u00b7bet\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wird eingewiegt.", "tokens": ["Wird", "ein\u00b7ge\u00b7wiegt", "."], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Eilt hin und seht an meiner statt", "tokens": ["Eilt", "hin", "und", "seht", "an", "mei\u00b7ner", "statt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das grab der edlen sch\u00f6nheit stehen/", "tokens": ["Das", "grab", "der", "ed\u00b7len", "sch\u00f6n\u00b7heit", "ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Was zeit und gl\u00fcck mir abgesaget hat/", "tokens": ["Was", "zeit", "und", "gl\u00fcck", "mir", "ab\u00b7ge\u00b7sa\u00b7get", "hat", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "VVIMP", "PPER", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das k\u00f6nt ihr unverwehrt durchwehen/", "tokens": ["Das", "k\u00f6nt", "ihr", "un\u00b7ver\u00b7wehrt", "durch\u00b7we\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Jhr solt der spiegel seyn/", "tokens": ["Ihr", "solt", "der", "spie\u00b7gel", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VAINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Darinnen ich di\u00df himmels-bild betrachte/", "tokens": ["Da\u00b7rin\u00b7nen", "ich", "di\u00df", "him\u00b7mels\u00b7bild", "be\u00b7trach\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PDS", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Was ich verehr und g\u00f6ttlich achte/", "tokens": ["Was", "ich", "ver\u00b7ehr", "und", "g\u00f6tt\u00b7lich", "ach\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "KON", "ADJD", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wist ihr allein.", "tokens": ["Wist", "ihr", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Ich wei\u00df/ da\u00df dort der h\u00f6chste prei\u00df", "tokens": ["Ich", "wei\u00df", "/", "da\u00df", "dort", "der", "h\u00f6chs\u00b7te", "prei\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der sch\u00f6nheit ausgekramet lieget/", "tokens": ["Der", "sch\u00f6n\u00b7heit", "aus\u00b7ge\u00b7kra\u00b7met", "lie\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dran die natur mit ihrem gr\u00f6sten flei\u00df", "tokens": ["Dran", "die", "na\u00b7tur", "mit", "ih\u00b7rem", "gr\u00f6s\u00b7ten", "flei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein wunder an das andre f\u00fcget.", "tokens": ["Ein", "wun\u00b7der", "an", "das", "and\u00b7re", "f\u00fc\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wer doch so selig w\u00e4r/", "tokens": ["Wer", "doch", "so", "se\u00b7lig", "w\u00e4r", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "ADJD", "VAFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Da\u00df nur ein blick so k\u00fchn/ so hoch d\u00fcrfft steigen/", "tokens": ["Da\u00df", "nur", "ein", "blick", "so", "k\u00fchn", "/", "so", "hoch", "d\u00fcrfft", "stei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADV", "ADJD", "$(", "ADV", "ADJD", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Solt er auch gleich sich wieder neigen", "tokens": ["Solt", "er", "auch", "gleich", "sich", "wie\u00b7der", "nei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "PRF", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zur wiederkehr.", "tokens": ["Zur", "wie\u00b7der\u00b7kehr", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Sind gleich die augen zugethan;", "tokens": ["Sind", "gleich", "die", "au\u00b7gen", "zu\u00b7ge\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die sonnen sind nur untergangen/", "tokens": ["Die", "son\u00b7nen", "sind", "nur", "un\u00b7ter\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Um wenn der tag wird wieder brechen an/", "tokens": ["Um", "wenn", "der", "tag", "wird", "wie\u00b7der", "bre\u00b7chen", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "KOUS", "ART", "NN", "VAFIN", "ADV", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit mehrer glut und glantz zu prangen.", "tokens": ["Mit", "meh\u00b7rer", "glut", "und", "glantz", "zu", "pran\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die sch\u00f6nheit wird bey nacht", "tokens": ["Die", "sch\u00f6n\u00b7heit", "wird", "bey", "nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Verstohlen/ (und w\u00e4r es gleich nicht ihr wille/)", "tokens": ["Ver\u00b7stoh\u00b7len", "/", "(", "und", "w\u00e4r", "es", "gleich", "nicht", "ihr", "wil\u00b7le", "/", ")"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$(", "$(", "KON", "VAFIN", "PPER", "ADV", "PTKNEG", "PPOSAT", "NN", "$(", "$("], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Viel sichrer und mit mehrer stille", "tokens": ["Viel", "sich\u00b7rer", "und", "mit", "meh\u00b7rer", "stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "KON", "APPR", "PIAT", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Als tags betracht.", "tokens": ["Als", "tags", "be\u00b7tracht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Des munds rubin bleibt ohne licht", "tokens": ["Des", "munds", "ru\u00b7bin", "bleibt", "oh\u00b7ne", "licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und in dem schlaff gleich hoch ger\u00f6thet;", "tokens": ["Und", "in", "dem", "schlaff", "gleich", "hoch", "ge\u00b7r\u00f6\u00b7thet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "VVFIN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch dienet er zum k\u00fcssen ietzo nicht/", "tokens": ["Doch", "die\u00b7net", "er", "zum", "k\u00fcs\u00b7sen", "iet\u00b7zo", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "VVFIN", "ADV", "PTKNEG", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wer schl\u00e4fft/ der scheinet halb get\u00f6dtet.", "tokens": ["Wer", "schl\u00e4fft", "/", "der", "schei\u00b7net", "halb", "ge\u00b7t\u00f6d\u00b7tet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$(", "ART", "VVFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Drum kan die seele nicht", "tokens": ["Drum", "kan", "die", "see\u00b7le", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "ART", "VVFIN", "PTKNEG"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Zum k\u00fcssen sich auff ihre lippen setzen/", "tokens": ["Zum", "k\u00fcs\u00b7sen", "sich", "auff", "ih\u00b7re", "lip\u00b7pen", "set\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und jene seele recht ergetzen/", "tokens": ["Und", "je\u00b7ne", "see\u00b7le", "recht", "er\u00b7get\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die k\u00fcsse bricht.", "tokens": ["Die", "k\u00fcs\u00b7se", "bricht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Schlaff sanffte/ g\u00f6ttin/ in der pracht/", "tokens": ["Schlaff", "sanff\u00b7te", "/", "g\u00f6t\u00b7tin", "/", "in", "der", "pracht", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$(", "NE", "$(", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der wunder deines leibes gaben/", "tokens": ["Der", "wun\u00b7der", "dei\u00b7nes", "lei\u00b7bes", "ga\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der k\u00fchlen lufft in dieser stillen nacht", "tokens": ["Der", "k\u00fch\u00b7len", "lufft", "in", "die\u00b7ser", "stil\u00b7len", "nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sey die verwundrung eingegraben/", "tokens": ["Sey", "die", "ver\u00b7wund\u00b7rung", "ein\u00b7ge\u00b7gra\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die aus dem hertzen quillt/", "tokens": ["Die", "aus", "dem", "hert\u00b7zen", "quillt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Das sich verwirrt in deiner sch\u00f6nheit netze", "tokens": ["Das", "sich", "ver\u00b7wirrt", "in", "dei\u00b7ner", "sch\u00f6n\u00b7heit", "net\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PRF", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und gantz mit liebe deiner sch\u00e4tze", "tokens": ["Und", "gantz", "mit", "lie\u00b7be", "dei\u00b7ner", "sch\u00e4t\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "VVFIN", "PPOSAT", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ist angef\u00fcllt.", "tokens": ["Ist", "an\u00b7ge\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}