{"textgrid.poem.58569": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott ist willig und will mir sich und alles Gute schenken,", "tokens": ["Gott", "ist", "wil\u00b7lig", "und", "will", "mir", "sich", "und", "al\u00b7les", "Gu\u00b7te", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "KON", "VMFIN", "PPER", "PRF", "KON", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Sollt' ich Wurm mich weigern noch, sollt' ich lange mich bedenken?", "tokens": ["Sollt'", "ich", "Wurm", "mich", "wei\u00b7gern", "noch", ",", "sollt'", "ich", "lan\u00b7ge", "mich", "be\u00b7den\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "PPER", "VVFIN", "ADV", "$,", "VMFIN", "PPER", "ADV", "PPER", "VVINF", "$."], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Willig lass' ich alles da, willig schenk' ich ihm das Herz,", "tokens": ["Wil\u00b7lig", "lass'", "ich", "al\u00b7les", "da", ",", "wil\u00b7lig", "schenk'", "ich", "ihm", "das", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PIS", "ADV", "$,", "ADJD", "VVFIN", "PPER", "PPER", "ART", "NN", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Willig folg' ich seinem Ruf, bringt es gleich dem Fleische Schmerz.", "tokens": ["Wil\u00b7lig", "fol\u00b7g'", "ich", "sei\u00b7nem", "Ruf", ",", "bringt", "es", "gleich", "dem", "Flei\u00b7sche", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+----+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Sprich in meiner Seele Grund, zieh mich, da hinein zu kehren,", "tokens": ["Sprich", "in", "mei\u00b7ner", "See\u00b7le", "Grund", ",", "zieh", "mich", ",", "da", "hin\u00b7ein", "zu", "keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "PPOSAT", "NN", "NN", "$,", "VVFIN", "PPER", "$,", "ADV", "APZR", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Setze mit Maria mich, rede, Herr, dein Knecht soll h\u00f6ren,", "tokens": ["Set\u00b7ze", "mit", "Ma\u00b7ria", "mich", ",", "re\u00b7de", ",", "Herr", ",", "dein", "Knecht", "soll", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "PPER", "$,", "VVFIN", "$,", "NN", "$,", "PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "+--+--+-+-+-+-", "measure": "elegiambus"}, "line.7": {"text": "La\u00df Gedanken und Vernunft, Sinn und Willen schweigen still,", "tokens": ["La\u00df", "Ge\u00b7dan\u00b7ken", "und", "Ver\u00b7nunft", ",", "Sinn", "und", "Wil\u00b7len", "schwei\u00b7gen", "still", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Haue nieder durch dein Schwert, was in mir sich weigern will!", "tokens": ["Hau\u00b7e", "nie\u00b7der", "durch", "dein", "Schwert", ",", "was", "in", "mir", "sich", "wei\u00b7gern", "will", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "APPR", "PPOSAT", "NN", "$,", "PRELS", "APPR", "PPER", "PRF", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Gott ist willig und will mir sich und alles Gute schenken,", "tokens": ["Gott", "ist", "wil\u00b7lig", "und", "will", "mir", "sich", "und", "al\u00b7les", "Gu\u00b7te", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "KON", "VMFIN", "PPER", "PRF", "KON", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Sollt' ich Wurm mich weigern noch, sollt' ich lange mich bedenken?", "tokens": ["Sollt'", "ich", "Wurm", "mich", "wei\u00b7gern", "noch", ",", "sollt'", "ich", "lan\u00b7ge", "mich", "be\u00b7den\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "PPER", "VVFIN", "ADV", "$,", "VMFIN", "PPER", "ADV", "PPER", "VVINF", "$."], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Willig lass' ich alles da, willig schenk' ich ihm das Herz,", "tokens": ["Wil\u00b7lig", "lass'", "ich", "al\u00b7les", "da", ",", "wil\u00b7lig", "schenk'", "ich", "ihm", "das", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PIS", "ADV", "$,", "ADJD", "VVFIN", "PPER", "PPER", "ART", "NN", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Willig folg' ich seinem Ruf, brin