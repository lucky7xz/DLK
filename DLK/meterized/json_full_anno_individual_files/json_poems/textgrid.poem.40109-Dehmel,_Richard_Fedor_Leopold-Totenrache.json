{"textgrid.poem.40109": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Totenrache", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch die schlafende Lagune", "tokens": ["Durch", "die", "schla\u00b7fen\u00b7de", "La\u00b7gu\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "seufzend zieht der lange Kahn", "tokens": ["seuf\u00b7zend", "zieht", "der", "lan\u00b7ge", "Kahn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "seine Bahn,", "tokens": ["sei\u00b7ne", "Bahn", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "einsam zieht er durch der dunkeln", "tokens": ["ein\u00b7sam", "zieht", "er", "durch", "der", "dun\u00b7keln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "langen Wellen glattes Funkeln", "tokens": ["lan\u00b7gen", "Wel\u00b7len", "glat\u00b7tes", "Fun\u00b7keln"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "wie ein gro\u00dfer schwarzer Schwan ...", "tokens": ["wie", "ein", "gro\u00b7\u00dfer", "schwar\u00b7zer", "Schwan", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nun im Zelt der Barke fl\u00fcsternd", "tokens": ["Nun", "im", "Zelt", "der", "Bar\u00b7ke", "fl\u00fcs\u00b7ternd"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "regt sich eines Mundes Glut,", "tokens": ["regt", "sich", "ei\u00b7nes", "Mun\u00b7des", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "und die Flut", "tokens": ["und", "die", "Flut"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "ebnet sich in weiten Kreisen:", "tokens": ["eb\u00b7net", "sich", "in", "wei\u00b7ten", "Krei\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "hei\u00dfer wird der Strom der leisen", "tokens": ["hei\u00b7\u00dfer", "wird", "der", "Strom", "der", "lei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Laute, \u2013 still! das Ruder ruht.", "tokens": ["Lau\u00b7te", ",", "\u2013", "still", "!", "das", "Ru\u00b7der", "ruht", "."], "token_info": ["word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "$(", "PTKVZ", "$.", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbdonna Anna, Deine Schw\u00fcre", "tokens": ["\u00bb", "don\u00b7na", "An\u00b7na", ",", "Dei\u00b7ne", "Schw\u00fc\u00b7re"], "token_info": ["punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "NE", "NE", "$,", "PPOSAT", "NN"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "sind viel dunkler als die Nacht!", "tokens": ["sind", "viel", "dunk\u00b7ler", "als", "die", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ADJA", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Stolz verlacht", "tokens": ["Stolz", "ver\u00b7lacht"], "token_info": ["word", "word"], "pos": ["NN", "VVPP"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "hab' ich all die L\u00e4sterzungen,", "tokens": ["hab'", "ich", "all", "die", "L\u00e4s\u00b7ter\u00b7zun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbliebster, willst du mich betr\u00fcben?!", "tokens": ["\u00bb", "liebs\u00b7ter", ",", "willst", "du", "mich", "be\u00b7tr\u00fc\u00b7ben", "?!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "f\u00fchlst du nicht, da\u00df nie von Lust", "tokens": ["f\u00fchlst", "du", "nicht", ",", "da\u00df", "nie", "von", "Lust"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "ADV", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "je gewu\u00dft", "tokens": ["je", "ge\u00b7wu\u00dft"], "token_info": ["word", "word"], "pos": ["ADV", "VVPP"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "meine K\u00fcsse, bis sie Deinen", "tokens": ["mei\u00b7ne", "K\u00fcs\u00b7se", ",", "bis", "sie", "Dei\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "PPER", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "bang und s\u00fc\u00df sich durften einen?\u00ab", "tokens": ["bang", "und", "s\u00fc\u00df", "sich", "durf\u00b7ten", "ei\u00b7nen", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "KON", "VVFIN", "PRF", "VMFIN", "PIS", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "und sie sinkt ihm an die Brust.", "tokens": ["und", "sie", "sinkt", "ihm", "an", "die", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbschw\u00f6re \u2013\u00ab will er pr\u00fcfend wehren,", "tokens": ["\u00bb", "schw\u00f6\u00b7re", "\u2013", "\u00ab", "will", "er", "pr\u00fc\u00b7fend", "weh\u00b7ren", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$(", "$(", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "aber an ihm liegt sie dicht:", "tokens": ["a\u00b7ber", "an", "ihm", "liegt", "sie", "dicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bb", "tokens": ["\u00bb"], "token_info": ["punct"], "pos": ["$("]}, "line.4": {"text": "wie der Vogel in die Weiten,", "tokens": ["wie", "der", "Vo\u00b7gel", "in", "die", "Wei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "sehn' ich mich nach Seligkeiten!\u00ab", "tokens": ["sehn'", "ich", "mich", "nach", "Se\u00b7lig\u00b7kei\u00b7ten", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "hebt sie schmachtend ihr Gesicht.", "tokens": ["hebt", "sie", "schmach\u00b7tend", "ihr", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und er sieht und f\u00fchlt bezwungen", "tokens": ["Und", "er", "sieht", "und", "f\u00fchlt", "be\u00b7zwun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "ihres Leibes weiche Pracht,", "tokens": ["ih\u00b7res", "Lei\u00b7bes", "wei\u00b7che", "Pracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "warme Macht;", "tokens": ["war\u00b7me", "Macht", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "seine jungen Wangen bl\u00fchen,", "tokens": ["sei\u00b7ne", "jun\u00b7gen", "Wan\u00b7gen", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "r\u00f6tlich schwankt der Ampel Gl\u00fchen,", "tokens": ["r\u00f6t\u00b7lich", "schwankt", "der", "Am\u00b7pel", "Gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "K\u00fcsse st\u00f6hnen durch die Nacht.", "tokens": ["K\u00fcs\u00b7se", "st\u00f6h\u00b7nen", "durch", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und den Mund umzuckt von Schlangen", "tokens": ["Und", "den", "Mund", "um\u00b7zuckt", "von", "Schlan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "sieht sie, wie er trunken ist,", "tokens": ["sieht", "sie", ",", "wie", "er", "trun\u00b7ken", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PPER", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "sich vergi\u00dft, \u2013", "tokens": ["sich", "ver\u00b7gi\u00dft", ",", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["PRF", "VVFIN", "$,", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "doch ihr Spott ist kaum verflogen:", "tokens": ["doch", "ihr", "Spott", "ist", "kaum", "ver\u00b7flo\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "w\u00fctend \u00fcber sie gebogen", "tokens": ["w\u00fc\u00b7tend", "\u00fc\u00b7ber", "sie", "ge\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "f\u00fchlt er ihrer Seele List, \u2013", "tokens": ["f\u00fchlt", "er", "ih\u00b7rer", "See\u00b7le", "List", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "NE", "$,", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "und ein Ringen \u2013 und ein Keuchen, \u2013", "tokens": ["und", "ein", "Rin\u00b7gen", "\u2013", "und", "ein", "Keu\u00b7chen", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "$(", "KON", "ART", "NN", "$,", "$("], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "\u00bbgott, Erbarmen \u2013\u00ab stickt ein Schrei", "tokens": ["\u00bb", "gott", ",", "Er\u00b7bar\u00b7men", "\u2013", "\u00ab", "stickt", "ein", "Schrei"], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "word", "word", "word"], "pos": ["$(", "ADJD", "$,", "NN", "$(", "$(", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "dumpf entzwei, \u2013", "tokens": ["dumpf", "ent\u00b7zwei", ",", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "hohl ein Brodeln im Canale, \u2013", "tokens": ["hohl", "ein", "Bro\u00b7deln", "im", "Ca\u00b7na\u00b7le", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "ART", "NN", "APPRART", "NN", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "stille wird's mit Einem Male, \u2013 \u2013", "tokens": ["stil\u00b7le", "wird's", "mit", "Ei\u00b7nem", "Ma\u00b7le", ",", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "VAFIN", "APPR", "ART", "NN", "$,", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "fern vom Turme sch\u00fcttert's: Drei ...", "tokens": ["fern", "vom", "Tur\u00b7me", "sch\u00fct\u00b7tert's", ":", "Drei", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVFIN", "$.", "CARD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Wochen fliehen, \u2013 wachend, tr\u00e4umend", "tokens": ["Wo\u00b7chen", "flie\u00b7hen", ",", "\u2013", "wa\u00b7chend", ",", "tr\u00e4u\u00b7mend"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word"], "pos": ["NN", "VVINF", "$,", "$(", "VVPP", "$,", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "sehnt er sich nach ihrem Mund", "tokens": ["sehnt", "er", "sich", "nach", "ih\u00b7rem", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "m\u00fcd und wund;", "tokens": ["m\u00fcd", "und", "wund", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "immer um die dritte Stunde", "tokens": ["im\u00b7mer", "um", "die", "drit\u00b7te", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "macht er n\u00e4chtlich dort die Runde,", "tokens": ["macht", "er", "n\u00e4cht\u00b7lich", "dort", "die", "Run\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "starrt er in den blauen Schlund.", "tokens": ["starrt", "er", "in", "den", "blau\u00b7en", "Schlund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "In der dunklen Wasserschale", "tokens": ["In", "der", "dunk\u00b7len", "Was\u00b7ser\u00b7scha\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "sieht er ruhn den bleichen Mond,", "tokens": ["sieht", "er", "ruhn", "den", "blei\u00b7chen", "Mond", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "ruhn den Mond,", "tokens": ["ruhn", "den", "Mond", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "h\u00f6rt er seufzen die versunknen", "tokens": ["h\u00f6rt", "er", "seuf\u00b7zen", "die", "ver\u00b7sun\u00b7knen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "VVFIN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "bleichen Lippen und die trunknen", "tokens": ["blei\u00b7chen", "Lip\u00b7pen", "und", "die", "trunk\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "K\u00fcsse, die er so belohnt \u2013!", "tokens": ["K\u00fcs\u00b7se", ",", "die", "er", "so", "be\u00b7lohnt", "\u2013", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$(", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "und ihn lockt ein banges R\u00fchren,", "tokens": ["und", "ihn", "lockt", "ein", "ban\u00b7ges", "R\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "und von tiefer banger Macht", "tokens": 