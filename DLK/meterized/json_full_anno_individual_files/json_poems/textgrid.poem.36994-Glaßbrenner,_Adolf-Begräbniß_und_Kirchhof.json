{"textgrid.poem.36994": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "Begr\u00e4bni\u00df und Kirchhof", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Gel\u00e4chter, ein Jauchzen! Es lockt mich hinaus,", "tokens": ["Ein", "Ge\u00b7l\u00e4ch\u00b7ter", ",", "ein", "Jauch\u00b7zen", "!", "Es", "lockt", "mich", "hin\u00b7aus", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$.", "PPER", "VVFIN", "PRF", "APZR", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Was seh' ich? Ein Leichenbeg\u00e4ngni\u00df!", "tokens": ["Was", "seh'", "ich", "?", "Ein", "Lei\u00b7chen\u00b7be\u00b7g\u00e4ng\u00b7ni\u00df", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "\u00bber hat's \u00fcberstanden,\u00ab so rufen sie aus,", "tokens": ["\u00bb", "er", "hat's", "\u00fc\u00b7bers\u00b7tan\u00b7den", ",", "\u00ab", "so", "ru\u00b7fen", "sie", "aus", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "VVPP", "$,", "$(", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "\u00bbdas traurige Daseinsverh\u00e4ngni\u00df!", "tokens": ["\u00bb", "das", "trau\u00b7ri\u00b7ge", "Da\u00b7seins\u00b7ver\u00b7h\u00e4ng\u00b7ni\u00df", "!"], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Sklav, der den Kerker des Lebens verlie\u00df,", "tokens": ["Der", "Sklav", ",", "der", "den", "Ker\u00b7ker", "des", "Le\u00b7bens", "ver\u00b7lie\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Geht ein nun in's ewige Lustparadies,", "tokens": ["Geht", "ein", "nun", "in's", "e\u00b7wi\u00b7ge", "Lust\u00b7pa\u00b7ra\u00b7dies", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Juchhe! wo die Engel, die holden,", "tokens": ["Juch\u00b7he", "!", "wo", "die", "En\u00b7gel", ",", "die", "hol\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "PWAV", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.8": {"text": "Uns von unten bis oben vergolden!", "tokens": ["Uns", "von", "un\u00b7ten", "bis", "o\u00b7ben", "ver\u00b7gol\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADV", "APPR", "ADV", "VVINF", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Wo die H\u00e4user aus funkelndem Edelgestein;", "tokens": ["Wo", "die", "H\u00e4u\u00b7ser", "aus", "fun\u00b7keln\u00b7dem", "E\u00b7del\u00b7ge\u00b7stein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Wo gebraten die V\u00f6gel 'rumfliegen;", "tokens": ["Wo", "ge\u00b7bra\u00b7ten", "die", "V\u00f6\u00b7gel", "'rum\u00b7flie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "NE", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Wo in Str\u00f6men von k\u00f6stlichem, feurigem Wein", "tokens": ["Wo", "in", "Str\u00f6\u00b7men", "von", "k\u00f6st\u00b7li\u00b7chem", ",", "feu\u00b7ri\u00b7gem", "Wein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "APPR", "NN", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Wei\u00dfzuckerne Schw\u00e4ne sich wiegen;", "tokens": ["Wei\u00df\u00b7zu\u00b7cker\u00b7ne", "Schw\u00e4\u00b7ne", "sich", "wie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Wo die seligen Sultane all von Konfekt,", "tokens": ["Wo", "die", "se\u00b7li\u00b7gen", "Sul\u00b7ta\u00b7ne", "all", "von", "Kon\u00b7fekt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "PIAT", "APPR", "NN", "$,"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.6": {"text": "Und das selige Volk sie beliebig beleckt;", "tokens": ["Und", "das", "se\u00b7li\u00b7ge", "Volk", "sie", "be\u00b7lie\u00b7big", "be\u00b7leckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PPER", "ADJD", "VVPP", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.7": {"text": "Wo die Mufti's uns waschen, und Nixen", "tokens": ["Wo", "die", "Muf\u00b7ti's", "uns", "wa\u00b7schen", ",", "und", "Ni\u00b7xen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ART", "NN", "PPER", "VVINF", "$,", "KON", "NN"], "meter": "--+--+-++-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Und Feeen die Stiefel uns wichsen!", "tokens": ["Und", "Fe\u00b7e\u00b7en", "die", "Stie\u00b7fel", "uns", "wich\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Wo als adlige Ritter man wieder begr\u00fc\u00dft", "tokens": ["Wo", "als", "ad\u00b7li\u00b7ge", "Rit\u00b7ter", "man", "wie\u00b7der", "be\u00b7gr\u00fc\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "KOKOM", "ADJA", "NN", "PIS", "ADV", "VVPP"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Unsere kl\u00fcgsten, plebegischsten T\u00f6ffel;", "tokens": ["Un\u00b7se\u00b7re", "kl\u00fcgs\u00b7ten", ",", "ple\u00b7be\u00b7gischs\u00b7ten", "T\u00f6f\u00b7fel", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Wo man Alles, die Luft selbst, die freie, genie\u00dft", "tokens": ["Wo", "man", "Al\u00b7les", ",", "die", "Luft", "selbst", ",", "die", "frei\u00b7e", ",", "ge\u00b7nie\u00dft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PWAV", "PIS", "PIS", "$,", "ART", "NN", "ADV", "$,", "ART", "ADJA", "$,", "VVFIN"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "H\u00f6chst vornehm mit silbernem L\u00f6ffel!", "tokens": ["H\u00f6chst", "vor\u00b7nehm", "mit", "sil\u00b7ber\u00b7nem", "L\u00f6f\u00b7fel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "----+--+-", "measure": "iambic.di.relaxed"}, "line.5": {"text": "Wo man Nektar ", "tokens": ["Wo", "man", "Nek\u00b7tar"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PIS", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Und Perlen, die k\u00f6stlichsten, schwitzt, wenn man l\u00e4uft;", "tokens": ["Und", "Per\u00b7len", ",", "die", "k\u00f6st\u00b7lichs\u00b7ten", ",", "schwitzt", ",", "wenn", "man", "l\u00e4uft", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "$,", "KOUS", "PIS", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Wo man Arien singt, wenn man prustet,", "tokens": ["Wo", "man", "A\u00b7rien", "singt", ",", "wenn", "man", "prus\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "NE", "VVFIN", "$,", "KOUS", "PIS", "VVFIN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Und Volkshymnen reimt, wenn man hustet!", "tokens": ["Und", "Volks\u00b7hym\u00b7nen", "reimt", ",", "wenn", "man", "hus\u00b7tet", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,", "KOUS", "PIS", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Wo die Dirne Moral uns den Becher kredenzt,", "tokens": ["Wo", "die", "Dir\u00b7ne", "Mo\u00b7ral", "uns", "den", "Be\u00b7cher", "kre\u00b7denzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Kein Genu\u00df von der Lust'gen bedroht ist;", "tokens": ["Kein", "Ge\u00b7nu\u00df", "von", "der", "Lust'\u00b7gen", "be\u00b7droht", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Wo den ewigen Lenz man durchfaullenzt,", "tokens": ["Wo", "den", "e\u00b7wi\u00b7gen", "Lenz", "man", "durch\u00b7faul\u00b7lenzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "PIS", "VVPP", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und die Arbeit das einz'ge Verbot ist!", "tokens": ["Und", "die", "Ar\u00b7beit", "das", "einz'\u00b7ge", "Ver\u00b7bot", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Wo zwei Magen man, also in einem stets hat", "tokens": ["Wo", "zwei", "Ma\u00b7gen", "man", ",", "al\u00b7so", "in", "ei\u00b7nem", "stets", "hat"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "CARD", "NN", "PIS", "$,", "ADV", "APPR", "ART", "ADV", "VAFIN"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.6": {"text": "Appetit, wenn man just in dem anderen satt;", "tokens": ["Ap\u00b7pe\u00b7tit", ",", "wenn", "man", "just", "in", "dem", "an\u00b7de\u00b7ren", "satt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PIS", "ADV", "APPR", "ART", "ADJA", "ADJD", "$."], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Wo der einzige Doctor der Schankwirth,", "tokens": ["Wo", "der", "ein\u00b7zi\u00b7ge", "Doc\u00b7tor", "der", "Schank\u00b7wirth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "--+--+---+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Und Keiner der Seligen krank wird!\u00ab", "tokens": ["Und", "Kei\u00b7ner", "der", "Se\u00b7li\u00b7gen", "krank", "wird", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PIS", "ART", "NN", "ADJD", "VAFIN", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Das war's, was vom Jubelgeschrei ich verstand. \u2013", "tokens": ["Das", "wa\u00b7r's", ",", "was", "vom", "Ju\u00b7bel\u00b7ge\u00b7schrei", "ich", "ver\u00b7stand", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "$,", "PRELS", "APPRART", "NN", "PPER", "VVFIN", "$.", "$("], "meter": "-+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Auf dem Kirchhof begruben die Leiche", "tokens": ["Auf", "dem", "Kirch\u00b7hof", "be\u00b7gru\u00b7ben", "die", "Lei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Viel Mufti's und machten dabei allerhand", "tokens": ["Viel", "Muf\u00b7ti's", "und", "mach\u00b7ten", "da\u00b7bei", "al\u00b7ler\u00b7hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "VVFIN", "PAV", "PIAT"], "meter": "-++-+-+-+-+", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Narrethei'n und possierliche Streiche.", "tokens": ["Nar\u00b7rethei'n", "und", "pos\u00b7sier\u00b7li\u00b7che", "Strei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJA", "NN", "$."], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Sie heulten und blafften, umtanzten das Grab,", "tokens": ["Sie", "heul\u00b7ten", "und", "blaff\u00b7ten", ",", "um\u00b7tanz\u00b7ten", "das", "Grab", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "ADJA", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Ueberhopsten's und warfen Kartoffeln hinab,", "tokens": ["Ue\u00b7ber\u00b7hops\u00b7ten's", "und", "war\u00b7fen", "Kar\u00b7tof\u00b7feln", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "NN", "PTKVZ", "$,"], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Und sammelten dann auf 'nem Teller", "tokens": ["Und", "sam\u00b7mel\u00b7ten", "dann", "auf", "'nem", "Tel\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Sich Scudi's ein, Groschen und Heller.", "tokens": ["Sich", "Scu\u00b7di's", "ein", ",", "Gro\u00b7schen", "und", "Hel\u00b7ler", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "NE", "PTKVZ", "$,", "NN", "KON", "NE", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.6": {"line.1": {"text": "Und zum Schlusse verlief die Beerdigungsschaar", "tokens": ["Und", "zum", "Schlus\u00b7se", "ver\u00b7lief", "die", "Be\u00b7er\u00b7di\u00b7gungs\u00b7schaar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVFIN", "ART", "NN"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Sich, den lachenden Erben noch bringend", "tokens": ["Sich", ",", "den", "la\u00b7chen\u00b7den", "Er\u00b7ben", "noch", "brin\u00b7gend"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PRF", "$,", "ART", "ADJA", "NN", "ADV", "ADJD"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Ein Hoch, ganz wie sie gekommen war,", "tokens": ["Ein", "Hoch", ",", "ganz", "wie", "sie", "ge\u00b7kom\u00b7men", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "KOKOM", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-++-+-+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Aufjauchzend, l\u00e4rmend und singend.", "tokens": ["Auf\u00b7jauch\u00b7zend", ",", "l\u00e4r\u00b7mend", "und", "sin\u00b7gend", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ich aber, ich ging durch den friedlichen Hain \u2013", "tokens": ["Ich", "a\u00b7ber", ",", "ich", "ging", "durch", "den", "fried\u00b7li\u00b7chen", "Hain", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Beleuchtet von wunderbar magischem Schein \u2013", "tokens": ["Be\u00b7leuch\u00b7tet", "von", "wun\u00b7der\u00b7bar", "ma\u00b7gi\u00b7schem", "Schein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJD", "ADJA", "NN", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Und las, was da meldeten Tafel und Stein", "tokens": ["Und", "las", ",", "was", "da", "mel\u00b7de\u00b7ten", "Ta\u00b7fel", "und", "Stein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PRELS", "ADV", "VVFIN", "NN", "KON", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Von den Ruhenden allen, den Lieben,", "tokens": ["Von", "den", "Ru\u00b7hen\u00b7den", "al\u00b7len", ",", "den", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "