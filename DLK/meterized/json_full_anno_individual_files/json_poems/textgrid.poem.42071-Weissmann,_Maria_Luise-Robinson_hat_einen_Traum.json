{"textgrid.poem.42071": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "Robinson hat einen Traum", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was war der Traum? Ich kann mich nicht besinnen", "tokens": ["Was", "war", "der", "Traum", "?", "Ich", "kann", "mich", "nicht", "be\u00b7sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "NN", "$.", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und seh ihn rings: Er bl\u00fcht von jedem Strauch,", "tokens": ["Und", "seh", "ihn", "rings", ":", "Er", "bl\u00fcht", "von", "je\u00b7dem", "Strauch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$.", "PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die milde Kokosnu\u00df verschlie\u00dft ihn innen", "tokens": ["Die", "mil\u00b7de", "Ko\u00b7kos\u00b7nu\u00df", "ver\u00b7schlie\u00dft", "ihn", "in\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Duft der Milch, ihn wei\u00df die Schlange auch,", "tokens": ["Im", "Duft", "der", "Milch", ",", "ihn", "wei\u00df", "die", "Schlan\u00b7ge", "auch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die mich vorhin mit gr\u00fcnem Auge traf.", "tokens": ["Die", "mich", "vor\u00b7hin", "mit", "gr\u00fc\u00b7nem", "Au\u00b7ge", "traf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wie seltsam ist die alte H\u00fctte neu!", "tokens": ["Wie", "selt\u00b7sam", "ist", "die", "al\u00b7te", "H\u00fct\u00b7te", "neu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich f\u00fcrchte mich vor dir, o fremder Schlaf,", "tokens": ["Ich", "f\u00fcrch\u00b7te", "mich", "vor", "dir", ",", "o", "frem\u00b7der", "Schlaf", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPER", "$,", "FM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Du Gast von weither, m\u00e4chtig und nicht treu:", "tokens": ["Du", "Gast", "von", "weit\u00b7her", ",", "m\u00e4ch\u00b7tig", "und", "nicht", "treu", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "ADJA", "$,", "ADJD", "KON", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Du hast die Tat vollbracht, die ich nicht wei\u00df,", "tokens": ["Du", "hast", "die", "Tat", "voll\u00b7bracht", ",", "die", "ich", "nicht", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,", "PRELS", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Als w\u00e4rst du ich. Nun um den Schritt zieht Scheu", "tokens": ["Als", "w\u00e4rs