{"textgrid.poem.54003": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Unerledigte Konten", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Kind \u2013 so um 95 rum \u2013", "tokens": ["Als", "Kind", "\u2013", "so", "um", "95", "rum", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "number", "word", "punct"], "pos": ["KOUS", "NN", "$(", "ADV", "APPR", "CARD", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "da war ich bei Tante Jenny", "tokens": ["da", "war", "ich", "bei", "Tan\u00b7te", "Jen\u00b7ny"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "NN", "NE"], "meter": "-+--+---", "measure": "iambic.di.relaxed"}, "line.3": {"text": "zur Kindergesellschaft eingeladen,", "tokens": ["zur", "Kin\u00b7der\u00b7ge\u00b7sell\u00b7schaft", "ein\u00b7ge\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "mit Fritz und Ellen und M\u00e4nni.", "tokens": ["mit", "Fritz", "und", "El\u00b7len", "und", "M\u00e4n\u00b7ni", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ja.", "tokens": ["Ja", "."], "token_info": ["word", "punct"], "pos": ["PTKANT", "$."], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Und da hats Sahnenbaisers gegeben,", "tokens": ["Und", "da", "hats", "Sah\u00b7nen\u00b7bai\u00b7sers", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "jeder hat eins bekommen;", "tokens": ["je\u00b7der", "hat", "eins", "be\u00b7kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PIS", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "und dann wurde noch mal herumgereicht \u2013", "tokens": ["und", "dann", "wur\u00b7de", "noch", "mal", "her\u00b7um\u00b7ge\u00b7reicht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADV", "ADV", "VVPP", "$("], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.9": {"text": "und ich hab keins mehr genommen!", "tokens": ["und", "ich", "hab", "keins", "mehr", "ge\u00b7nom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PIAT", "PIS", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Das hat mich noch jahrelang geplagt . . . !", "tokens": ["Das", "hat", "mich", "noch", "jah\u00b7re\u00b7lang", "ge\u00b7plagt", ".", ".", ".", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$.", "$.", "$.", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Ich h\u00e4tte sollen . . . und hab Nein gesagt.", "tokens": ["Ich", "h\u00e4t\u00b7te", "sol\u00b7len", ".", ".", ".", "und", "hab", "Nein", "ge\u00b7sagt", "."], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VMFIN", "$.", "$.", "$.", "KON", "VAFIN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da hab ich noch eine Braut zu stehn", "tokens": ["Da", "hab", "ich", "noch", "ei\u00b7ne", "Braut", "zu", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "in Neu-Globsow \u2013 die Dame hie\u00df K\u00e4tchen;", "tokens": ["in", "Neu\u00b7Glob\u00b7sow", "\u2013", "die", "Da\u00b7me", "hie\u00df", "K\u00e4t\u00b7chen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$(", "ART", "NN", "VVFIN", "NE", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "irgendwas war da . . . die hat so geguckt . . .", "tokens": ["ir\u00b7gend\u00b7was", "war", "da", ".", ".", ".", "die", "hat", "so", "ge\u00b7guckt", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "VAFIN", "ADV", "$.", "$.", "$.", "PDS", "VAFIN", "ADV", "VVPP", "$.", "$.", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "doch ich hatte genug der M\u00e4dchen.", "tokens": ["doch", "ich", "hat\u00b7te", "ge\u00b7nug", "der", "M\u00e4d\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Ja.", "tokens": ["Ja", "."], "token_info": ["word", "punct"], "pos": ["PTKANT", "$."], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Und dann hat sie mir noch mal geschrieben,", "tokens": ["Und", "dann", "hat", "sie", "mir", "noch", "mal", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Briefe? Wie? Ist das sch\u00f6n?", "tokens": ["Brie\u00b7fe", "?", "Wie", "?", "Ist", "das", "sch\u00f6n", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWAV", "$.", "VAFIN", "PDS", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.8": {"text": "Und dann war ich zu faul, und Neu-Globsow ist weit,", "tokens": ["Und", "dann", "war", "ich", "zu", "faul", ",", "und", "Neu\u00b7Glob\u00b7sow", "ist", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PTKA", "ADJD", "$,", "KON", "NE", "VAFIN", "ADJD", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.9": {"text": "und jetzt m\u00f6cht ich sie wiedersehn.", "tokens": ["und", "jetzt", "m\u00f6cht", "ich", "sie", "wie\u00b7der\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wie mich das in leeren N\u00e4chten plagt . . .", "tokens": ["Wie", "mich", "das", "in", "lee\u00b7ren", "N\u00e4ch\u00b7ten", "plagt", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PWAV", "PPER", "PDS", "APPR", "ADJA", "NN", "VVFIN", "$.", "$.", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.11": {"text": "Ich h\u00e4tte sollen . . . und hab Nein gesagt.", "tokens": ["Ich", "h\u00e4t\u00b7te", "sol\u00b7len", ".", ".", ".", "und", "hab", "Nein", "ge\u00b7sagt", "."], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VMFIN", "$.", "$.", "$.", "KON", "VAFIN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da stand ich vor Jahren in Moabit", "tokens": ["Da", "stand", "ich", "vor", "Jah\u00b7ren", "in", "Mo\u00b7a\u00b7bit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "APPR", "NE"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "vor einem Talar, den das freute;", "tokens": ["vor", "ei\u00b7nem", "Ta\u00b7lar", ",", "den", "das", "freu\u00b7te", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ART", "ADJA", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "er redete, redete, quatschte und schrie", "tokens": ["er", "re\u00b7de\u00b7te", ",", "re\u00b7de\u00b7te", ",", "quatschte", "und", "schrie"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "und redet gewi\u00df noch heute.", "tokens": ["und", "re\u00b7det", "ge\u00b7wi\u00df", "noch", "heu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ADV", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ja.", "tokens": ["Ja", "."], "token_info": ["word", "punct"], "pos": ["PTKANT", "$."], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Und aus einem hier nicht zu erz\u00e4hlenden Grund", "tokens": ["Und", "aus", "ei\u00b7nem", "hier", "nicht", "zu", "er\u00b7z\u00e4h\u00b7len\u00b7den", "Grund"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADV", "PTKNEG", "PTKZU", "ADJA", "NN"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.7": {"text": "hielt ich die ganze Zeit meinen Mund.", "tokens": ["hielt", "ich", "die", "gan\u00b7ze", "Zeit", "mei\u00b7nen", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Ich mu\u00dfte. Und habe nichts gesagt.", "tokens": ["Ich", "mu\u00df\u00b7te", ".", "Und", "ha\u00b7be", "nichts", "ge\u00b7sagt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$.", "KON", "VAFIN", "PIS", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Aber das hat mich noch oft geplagt!", "tokens": ["A\u00b7ber", "das", "hat", "mich", "noch", "oft", "ge\u00b7plagt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Mit dem Jungen tret ich gern noch mal an \u2013", "tokens": ["Mit", "dem", "Jun\u00b7gen", "tret", "ich", "gern", "noch", "mal", "an", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADV", "ADV", "ADV", "APPR", "$("], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.11": {"text": "nur ein einziges Mal!", "tokens": ["nur", "ein", "ein\u00b7zi\u00b7ges", "Mal", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "aber dann \u2013 aber dann \u2013", "tokens": ["a\u00b7ber", "dann", "\u2013", "a\u00b7ber", "dann", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$(", "ADV", "ADV", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Ist ja gar nicht wahr.", "tokens": ["Ist", "ja", "gar", "nicht", "wahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PTKNEG", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Wenn heut K\u00e4tchen da steht,", "tokens": ["Wenn", "heut", "K\u00e4t\u00b7chen", "da", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "ADV", "VVFIN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "das Baiser und der Kerl aus Moabit \u2013:", "tokens": ["das", "Bai\u00b7ser", "und", "der", "Kerl", "aus", "Mo\u00b7a\u00b7bit", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "APPR", "NE", "$(", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "es ist ja leider alles zu sp\u00e4t!", "tokens": ["es", "ist", "ja", "lei\u00b7der", "al\u00b7les", "zu", "sp\u00e4t", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PIS", "PTKA", "ADJD", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Es ist immer das gleiche Lied:", "tokens": ["Es", "ist", "im\u00b7mer", "das", "glei\u00b7che", "Lied", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn wir was brauchen, dann haben wirs nicht;", "tokens": ["Wenn", "wir", "was", "brau\u00b7chen", ",", "dann", "ha\u00b7ben", "wirs", "nicht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VVFIN", "$,", "ADV", "VAFIN", "PIS", "PTKNEG", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "und wenn wir es kriegen, dann wollen wirs nicht.", "tokens": ["und", "wenn", "wir", "es", "krie\u00b7gen", ",", "dann", "wol\u00b7len", "wirs", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "VVINF", "$,", "ADV", "VMFIN", "PIS", "PTKNEG", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Lieber Gott! sei doch nur einmal gescheit", "tokens": ["Lie\u00b7ber", "Gott", "!", "sei", "doch", "nur", "ein\u00b7mal", "ge\u00b7scheit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "$.", "VAFIN", "ADV", "ADV", "ADV", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.9": {"text": "und gib uns die Dinge zu ihrer Zeit \u2013!", "tokens": ["und", "gib", "uns", "die", "Din\u00b7ge", "zu", "ih\u00b7rer", "Zeit", "\u2013", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVIMP", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "$(", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Amen.", "tokens": ["A\u00b7men", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.5": {"line.1": {"text": "Als Kind \u2013 so um 95 rum \u2013", "tokens": ["Als", "Kind", "\u2013", "so", "um", "95", "rum", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "number", "word", "punct"], "pos": ["KOUS", "NN", "$(", "ADV", "APPR", "CARD", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "da war ich bei Tante Jenny", "tokens": ["da", "war", "ich", "bei", "Tan\u00b7te", "Jen\u00b7ny"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "NN", "NE"], "meter": "-+--+---", "measure": "iambic.di.relaxed"}, "line.3": {"text": "zur Kindergesellschaft eingeladen,", "tokens": ["zur", "Kin\u00b7der\u00b7ge\u00b7sell\u00b7schaft", "ein\u00b7ge\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "mit Fritz und Ellen und M\u00e4nni.", "tokens": ["mit", "Fritz", "und", "El\u00b7len", "und", "M\u00e4n\u00b7ni", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ja.", "tokens": ["Ja", "."], "token_info": ["word", "punct"], "pos": ["PTKANT", "$."], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Und da hats Sahnenbaisers gegeben,", "tokens": ["Und", "da", "hats", "Sah\u00b7nen\u00b7bai\u00b7sers", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "jeder hat eins bekommen;", "tokens": ["je\u00b7der", "hat", "eins", "be\u00b7kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PIS", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "und dann wurde noch mal herumgereicht \u2013", "tokens": ["und", "dann", "wur\u00b7de", "noch", "mal", "her\u00b7um\u00b7ge\u00b7reicht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADV", "ADV", "VVPP", "$("], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.9": {"text": "und ich hab keins mehr genommen!", "tokens": ["und", "ich", "hab", "keins", "mehr", "ge\u00b7nom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PIAT", "PIS", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Das hat mich noch jahrelang geplagt . . . !", "tokens": ["Das", "hat", "mich", "noch", "jah\u00b7re\u00b7lang", "ge\u0