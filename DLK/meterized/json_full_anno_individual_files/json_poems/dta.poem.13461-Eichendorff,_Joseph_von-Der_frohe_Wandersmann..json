{"dta.poem.13461": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der frohe Wandersmann.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wem Gott will rechte Gunst erweisen,", "tokens": ["Wem", "Gott", "will", "rech\u00b7te", "Gunst", "er\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VMFIN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den schickt er in die weite Welt,", "tokens": ["Den", "schickt", "er", "in", "die", "wei\u00b7te", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem will er seine Wunder weisen", "tokens": ["Dem", "will", "er", "sei\u00b7ne", "Wun\u00b7der", "wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In Feld und Wald und Strom und Feld.", "tokens": ["In", "Feld", "und", "Wald", "und", "Strom", "und", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Tr\u00e4gen, die zu Hause liegen,", "tokens": ["Die", "Tr\u00e4\u00b7gen", ",", "die", "zu", "Hau\u00b7se", "lie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erquicket nicht das Morgenroth,", "tokens": ["Er\u00b7quic\u00b7ket", "nicht", "das", "Mor\u00b7gen\u00b7roth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie wissen nur vom Kinderwiegen", "tokens": ["Sie", "wis\u00b7sen", "nur", "vom", "Kin\u00b7der\u00b7wie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von Sorgen, Last und Noth um Brodt.", "tokens": ["Von", "Sor\u00b7gen", ",", "Last", "und", "Noth", "um", "Brodt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die B\u00e4chlein von den Bergen springen,", "tokens": ["Die", "B\u00e4ch\u00b7lein", "von", "den", "Ber\u00b7gen", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Lerchen schwirren hoch vor Lust,", "tokens": ["Die", "Ler\u00b7chen", "schwir\u00b7ren", "hoch", "vor", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was sollt' ich nicht mit ihnen singen", "tokens": ["Was", "sollt'", "ich", "nicht", "mit", "ih\u00b7nen", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "PTKNEG", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus voller Kehl' und frischer Brust?", "tokens": ["Aus", "vol\u00b7ler", "Kehl'", "und", "fri\u00b7scher", "Brust", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Den lieben Gott la\u00df ich nur walten;", "tokens": ["Den", "lie\u00b7ben", "Gott", "la\u00df", "ich", "nur", "wal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVIMP", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der B\u00e4chlein, Lerchen, Wald und Feld", "tokens": ["Der", "B\u00e4ch\u00b7lein", ",", "Ler\u00b7chen", ",", "Wald", "und", "Feld"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Erd' und Himmel will erhalten,", "tokens": ["Und", "Erd'", "und", "Him\u00b7mel", "will", "er\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hat auch mein' Sach' aufs Best' bestellt!", "tokens": ["Hat", "auch", "mein'", "Sach'", "aufs", "Best'", "be\u00b7stellt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}