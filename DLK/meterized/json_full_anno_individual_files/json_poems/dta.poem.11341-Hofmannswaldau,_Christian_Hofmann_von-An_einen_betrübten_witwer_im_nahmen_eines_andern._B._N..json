{"dta.poem.11341": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An einen betr\u00fcbten witwer im nahmen  \n eines andern.  \n  B. N.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Benug, genug geweint! die tugend fordert nicht,", "tokens": ["Be\u00b7nug", ",", "ge\u00b7nug", "ge\u00b7weint", "!", "die", "tu\u00b7gend", "for\u00b7dert", "nicht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVPP", "$.", "ART", "NN", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df man so lange zeit von tod und grabe spricht.", "tokens": ["Da\u00df", "man", "so", "lan\u00b7ge", "zeit", "von", "tod", "und", "gra\u00b7be", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADJA", "NN", "APPR", "NN", "KON", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dein eh-gemahl, o freund! hat freylich viel besessen,", "tokens": ["Dein", "eh\u00b7ge\u00b7mahl", ",", "o", "freund", "!", "hat", "frey\u00b7lich", "viel", "be\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "FM", "NN", "$.", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was GOtt viel anderen nur sparsam zugemessen:", "tokens": ["Was", "Gott", "viel", "an\u00b7de\u00b7ren", "nur", "spar\u00b7sam", "zu\u00b7ge\u00b7mes\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "PIAT", "ADJA", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie war von hertzen fromm, und sah\u2019 schon in der welt,", "tokens": ["Sie", "war", "von", "hert\u00b7zen", "fromm", ",", "und", "sah'", "schon", "in", "der", "welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ADJD", "$,", "KON", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was sie nun ewig sieht: Was uns gefangen h\u00e4lt,", "tokens": ["Was", "sie", "nun", "e\u00b7wig", "sieht", ":", "Was", "uns", "ge\u00b7fan\u00b7gen", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "VVFIN", "$.", "PWS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und manchem, wenn er stirbt, kaum m\u00f6glich ist zu hassen,", "tokens": ["Und", "man\u00b7chem", ",", "wenn", "er", "stirbt", ",", "kaum", "m\u00f6g\u00b7lich", "ist", "zu", "has\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "$,", "KOUS", "PPER", "VVFIN", "$,", "ADV", "ADJD", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Das alles sah\u2019 man sie schon lebend hier verlassen:", "tokens": ["Das", "al\u00b7les", "sah'", "man", "sie", "schon", "le\u00b7bend", "hier", "ver\u00b7las\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "PIS", "PPER", "ADV", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sie war dein ander hertz, des hauses freuden-licht,", "tokens": ["Sie", "war", "dein", "an\u00b7der", "hertz", ",", "des", "hau\u00b7ses", "freu\u00b7den\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJD", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der kinder ehren-bild, des n\u00e4chsten zuversicht.", "tokens": ["Der", "kin\u00b7der", "eh\u00b7ren\u00b7bild", ",", "des", "n\u00e4chs\u00b7ten", "zu\u00b7ver\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und also scheint es zwar, ob m\u00fcst du sie beklagen;", "tokens": ["Und", "al\u00b7so", "scheint", "es", "zwar", ",", "ob", "m\u00fcst", "du", "sie", "be\u00b7kla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Jedoch was dir dein hertz, dein kranckes hertz zerschlagen,", "tokens": ["Je\u00b7doch", "was", "dir", "dein", "hertz", ",", "dein", "kran\u00b7ckes", "hertz", "zer\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "PPER", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Das eben solte ja zum troste dir gedeyn.", "tokens": ["Das", "e\u00b7ben", "sol\u00b7te", "ja", "zum", "tros\u00b7te", "dir", "ge\u00b7deyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VMFIN", "ADV", "APPRART", "ADJA", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Was klagst du? Einen leib, der aller angst und pein", "tokens": ["Was", "klagst", "du", "?", "Ei\u00b7nen", "leib", ",", "der", "al\u00b7ler", "angst", "und", "pein"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$.", "ART", "NN", "$,", "PRELS", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Nunmehr entrissen ist? Was klagst du? Eine seele,", "tokens": ["Nun\u00b7mehr", "ent\u00b7ris\u00b7sen", "ist", "?", "Was", "klagst", "du", "?", "Ei\u00b7ne", "see\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "$.", "PWS", "VVFIN", "PPER", "$.", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die ihr gef\u00e4ngni\u00df flieht, und aus der s\u00fcnden-h\u00f6le", "tokens": ["Die", "ihr", "ge\u00b7f\u00e4ng\u00b7ni\u00df", "flieht", ",", "und", "aus", "der", "s\u00fcn\u00b7den\u00b7h\u00f6le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$,", "KON", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Sich nach dem himmel schwingt? Wie? oder klagst du dich?", "tokens": ["Sich", "nach", "dem", "him\u00b7mel", "schwingt", "?", "Wie", "?", "o\u00b7der", "klagst", "du", "dich", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVFIN", "$.", "PWAV", "$.", "KON", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Klagst du dein armes haus? Ach freylich, sprichst du, mich!", "tokens": ["Klagst", "du", "dein", "ar\u00b7mes", "haus", "?", "Ach", "frey\u00b7lich", ",", "sprichst", "du", ",", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$.", "ITJ", "ADV", "$,", "VVFIN", "PPER", "$,", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ach freylich, kind und haus! La\u00df ab von deinen thr\u00e4nen!", "tokens": ["Ach", "frey\u00b7lich", ",", "kind", "und", "haus", "!", "La\u00df", "ab", "von", "dei\u00b7nen", "thr\u00e4\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$,", "NN", "KON", "NN", "$.", "VVIMP", "PTKVZ", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wer liebt, der mu\u00df sich stets nach solchen dingen sehnen,", "tokens": ["Wer", "liebt", ",", "der", "mu\u00df", "sich", "stets", "nach", "sol\u00b7chen", "din\u00b7gen", "seh\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "ART", "VMFIN", "PRF", "ADV", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die das geliebt\u2019 erfreun. Dein treues eh-gemahl", "tokens": ["Die", "das", "ge\u00b7liebt'", "er\u00b7freun", ".", "Dein", "treu\u00b7es", "eh\u00b7ge\u00b7mahl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "VVINF", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Hat f\u00fcnff und zwantzig jahr im haus und \u00fcberall,", "tokens": ["Hat", "f\u00fcnff", "und", "zwant\u00b7zig", "jahr", "im", "haus", "und", "\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "CARD", "KON", "CARD", "NN", "APPRART", "NN", "KON", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "N\u00e4chst GOtt und ihrer seel, auf nichts als dich gesehen:", "tokens": ["N\u00e4chst", "Gott", "und", "ih\u00b7rer", "seel", ",", "auf", "nichts", "als", "dich", "ge\u00b7se\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "PPOSAT", "NN", "$,", "APPR", "PIS", "KOKOM", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Das gl\u00fccke, das dir schien, war auch ihr wohl-ergehen:", "tokens": ["Das", "gl\u00fc\u00b7cke", ",", "das", "dir", "schien", ",", "war", "auch", "ihr", "wohl\u00b7er\u00b7ge\u00b7hen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VAFIN", "ADV", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Der kummer, der dich traff, war auch ihr trauer-brod;", "tokens": ["Der", "kum\u00b7mer", ",", "der", "dich", "traff", ",", "war", "auch", "ihr", "trau\u00b7e\u00b7rbrod", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Liebst du nun so, wie sie, was klagst du ihren tod,", "tokens": ["Liebst", "du", "nun", "so", ",", "wie", "sie", ",", "was", "klagst", "du", "ih\u00b7ren", "tod", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$,", "PWAV", "PPER", "$,", "PWS", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Der sie so gl\u00fccklich macht? Was weinst du, da\u00df sie stirbet,", "tokens": ["Der", "sie", "so", "gl\u00fcck\u00b7lich", "macht", "?", "Was", "weinst", "du", ",", "da\u00df", "sie", "stir\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVFIN", "$.", "PWS", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Da sie zwar viel verl\u00e4st, jedoch weit mehr erwirbet?", "tokens": ["Da", "sie", "zwar", "viel", "ver\u00b7l\u00e4st", ",", "je\u00b7doch", "weit", "mehr", "er\u00b7wir\u00b7bet", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVFIN", "$,", "ADV", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.29": {"text": "Ja da sie, stirbt sie gleich, dennoch lebendig bleibt,", "tokens": ["Ja", "da", "sie", ",", "stirbt", "sie", "gleich", ",", "den\u00b7noch", "le\u00b7ben\u00b7dig", "bleibt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "KOUS", "PPER", "$,", "VVFIN", "PPER", "ADV", "$,", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Weil sie ihr wesen l\u00e4ngst den kindern einverleibt.", "tokens": ["Weil", "sie", "ihr", "we\u00b7sen", "l\u00e4ngst", "den", "kin\u00b7dern", "ein\u00b7ver\u00b7leibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "La\u00df andre traurig seyn, wo alles mu\u00df verschwinden;", "tokens": ["La\u00df", "and\u00b7re", "trau\u00b7rig", "seyn", ",", "wo", "al\u00b7les", "mu\u00df", "ver\u00b7schwin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PIS", "ADJD", "VAINF", "$,", "PWAV", "PIS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Du kanst der mutter bild in jeder tochter finden.", "tokens": ["Du", "kanst", "der", "mut\u00b7ter", "bild", "in", "je\u00b7der", "toch\u00b7ter", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}