{"dta.poem.19040": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "XxI.  An seine Gemahlin an ihrem 22sten  \n Jahr-Tage.   Am 7. November.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Gesegnete des HErrn! gedenck an unsern Bund,", "tokens": ["Ge\u00b7se\u00b7gne\u00b7te", "des", "Herrn", "!", "ge\u00b7denck", "an", "un\u00b7sern", "Bund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und komm, den Lebens GOtt gantz kindlich anzubeten,", "tokens": ["Und", "komm", ",", "den", "Le\u00b7bens", "Gott", "gantz", "kind\u00b7lich", "an\u00b7zu\u00b7be\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "NN", "ADV", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Versencke dich gantz tief in seinen Liebes-Grund,", "tokens": ["Ver\u00b7sen\u00b7cke", "dich", "gantz", "tief", "in", "sei\u00b7nen", "Lie\u00b7bes\u00b7Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der ehmals auch vor dich so H\u00f6ll als Tod zertreten!", "tokens": ["Der", "eh\u00b7mals", "auch", "vor", "dich", "so", "H\u00f6ll", "als", "Tod", "zer\u00b7tre\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "APPR", "PPER", "ADV", "NN", "KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er hat an Leib und Geist dich seeliglich gef\u00fchrt;", "tokens": ["Er", "hat", "an", "Leib", "und", "Geist", "dich", "see\u00b7lig\u00b7lich", "ge\u00b7f\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KON", "NN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Er hat dich vor der Bahn der L\u00e4sterer beh\u00fctet;", "tokens": ["Er", "hat", "dich", "vor", "der", "Bahn", "der", "L\u00e4s\u00b7te\u00b7rer", "be\u00b7h\u00fc\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dein annoch zartes Hertz hat er mit Ernst ger\u00fchrt;", "tokens": ["Dein", "an\u00b7noch", "zar\u00b7tes", "Hertz", "hat", "er", "mit", "Ernst", "ge\u00b7r\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "ADJA", "NN", "VAFIN", "PPER", "APPR", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Noch eh in deiner Brust was feindliches gew\u00fctet.", "tokens": ["Noch", "eh", "in", "dei\u00b7ner", "Brust", "was", "feind\u00b7li\u00b7ches", "ge\u00b7w\u00fc\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "APPR", "PPOSAT", "NN", "PWS", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du f\u00fchltest, Wertheste! von deiner Jugend auf", "tokens": ["Du", "f\u00fchl\u00b7test", ",", "Wert\u00b7hes\u00b7te", "!", "von", "dei\u00b7ner", "Ju\u00b7gend", "auf"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "NN", "$.", "APPR", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ein Treiben zu der Welt; ein Licht von falschen Scheine;", "tokens": ["Ein", "Trei\u00b7ben", "zu", "der", "Welt", ";", "ein", "Licht", "von", "fal\u00b7schen", "Schei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$.", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Erhellete die Bahn von deinem ersten Lauff,", "tokens": ["Er\u00b7hel\u00b7le\u00b7te", "die", "Bahn", "von", "dei\u00b7nem", "ers\u00b7ten", "Lauff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und deines Willens Trieb war eben nicht so reine.", "tokens": ["Und", "dei\u00b7nes", "Wil\u00b7lens", "Trieb", "war", "e\u00b7ben", "nicht", "so", "rei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VAFIN", "ADV", "PTKNEG", "ADV", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Insonderheit bewarb sich eitele Vernunfft,", "tokens": ["In\u00b7son\u00b7der\u00b7heit", "be\u00b7warb", "sich", "ei\u00b7te\u00b7le", "Ver\u00b7nunfft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die mit der sch\u00f6nsten Art sich heilig wei\u00df zu brennen,", "tokens": ["Die", "mit", "der", "sch\u00f6ns\u00b7ten", "Art", "sich", "hei\u00b7lig", "wei\u00df", "zu", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "PRF", "ADJD", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Um deine gantze Gunst, und lockte in die Zunft", "tokens": ["Um", "dei\u00b7ne", "gant\u00b7ze", "Gunst", ",", "und", "lock\u00b7te", "in", "die", "Zunft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "$,", "KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Derjenigen, die sich die weisen Christen nennen;", "tokens": ["Der\u00b7je\u00b7ni\u00b7gen", ",", "die", "sich", "die", "wei\u00b7sen", "Chris\u00b7ten", "nen\u00b7nen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PRF", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die Zunft, die \u00fcberall den besten Prei\u00df erjagt,", "tokens": ["Die", "Zunft", ",", "die", "\u00fc\u00b7be\u00b7rall", "den", "bes\u00b7ten", "Prei\u00df", "er\u00b7jagt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die Eitelkeit verschm\u00e4ht, davon kein Ruhm zu hoffen.", "tokens": ["Die", "Ei\u00b7tel\u00b7keit", "ver\u00b7schm\u00e4ht", ",", "da\u00b7von", "kein", "Ruhm", "zu", "hof\u00b7fen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PAV", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Der Hauffe, welcher viel von JEsu Liebe sagt,", "tokens": ["Der", "Hauf\u00b7fe", ",", "wel\u00b7cher", "viel", "von", "Je\u00b7su", "Lie\u00b7be", "sagt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPR", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und der den rechten Punct des Glaubens nie getroffen:", "tokens": ["Und", "der", "den", "rech\u00b7ten", "Punct", "des", "Glau\u00b7bens", "nie", "ge\u00b7trof\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "ADJA", "NN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die Zunft, davon ich selbst bey nah ein Mitglied war,", "tokens": ["Die", "Zunft", ",", "da\u00b7von", "ich", "selbst", "bey", "nah", "ein", "Mit\u00b7glied", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PAV", "PPER", "ADV", "APPR", "ADJD", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die Einfalt JEsu wohl vor eine Tummheit hielte,", "tokens": ["Die", "Ein\u00b7falt", "Je\u00b7su", "wohl", "vor", "ei\u00b7ne", "Tumm\u00b7heit", "hiel\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und ihr gesegnet Creutz vor furchtbare Gefahr,", "tokens": ["Und", "ihr", "ge\u00b7seg\u00b7net", "Creutz", "vor", "furcht\u00b7ba\u00b7re", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVPP", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Dem Tantzen fluchete, und ohne Vortheil spielte:", "tokens": ["Dem", "Tant\u00b7zen", "flu\u00b7che\u00b7te", ",", "und", "oh\u00b7ne", "Vor\u00b7theil", "spiel\u00b7te", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Die Schaar, die ohne Scheu der armen Christen lacht,", "tokens": ["Die", "Schaar", ",", "die", "oh\u00b7ne", "Scheu", "der", "ar\u00b7men", "Chris\u00b7ten", "lacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und ihres Helden-Fahn zu einer Jrr-Standarte,", "tokens": ["Und", "ih\u00b7res", "Hel\u00b7den\u00b7Fahn", "zu", "ei\u00b7ner", "Jrr\u00b7Stan\u00b7dar\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Den Ruhm der Niedrigkeit zu eignem Geiste macht,", "tokens": ["Den", "Ruhm", "der", "Nied\u00b7rig\u00b7keit", "zu", "eig\u00b7nem", "Geis\u00b7te", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und l\u00e4stert, da\u00df man nur auf Wunder-Z\u00fcge warte;", "tokens": ["Und", "l\u00e4s\u00b7tert", ",", "da\u00df", "man", "nur", "auf", "Wun\u00b7der\u00b7Z\u00fc\u00b7ge", "war\u00b7te", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PIS", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Die aber alles das so reiflich \u00fcberlegt,", "tokens": ["Die", "a\u00b7ber", "al\u00b7les", "das", "so", "reif\u00b7lich", "\u00fc\u00b7ber\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIS", "ART", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Da\u00df man gar oftermahl ihr Bitteres vor S\u00fc\u00dfe,", "tokens": ["Da\u00df", "man", "gar", "of\u00b7ter\u00b7mahl", "ihr", "Bit\u00b7te\u00b7res", "vor", "S\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADV", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Jhr Spotten freundlich h\u00e4lt, und was sie b\u00f6ses hegt,", "tokens": ["Ihr", "Spot\u00b7ten", "freund\u00b7lich", "h\u00e4lt", ",", "und", "was", "sie", "b\u00f6\u00b7ses", "hegt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVFIN", "$,", "KON", "PWS", "PPER", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und was uns st\u00fcrtzen will, sich immer tr\u00e4umen lie\u00dfe.", "tokens": ["Und", "was", "uns", "st\u00fcrt\u00b7zen", "will", ",", "sich", "im\u00b7mer", "tr\u00e4u\u00b7men", "lie\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVINF", "VMFIN", "$,", "PRF", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Die ist es, die dich bald, Geliebte! angelockt,", "tokens": ["Die", "ist", "es", ",", "die", "dich", "bald", ",", "Ge\u00b7lieb\u00b7te", "!", "an\u00b7ge\u00b7lockt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "$,", "PRELS", "PPER", "ADV", "$,", "NN", "$.", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und die das Christenthum der Kraft verleiden wollen;", "tokens": ["Und", "die", "das", "Chris\u00b7ten\u00b7thum", "der", "Kraft", "ver\u00b7lei\u00b7den", "wol\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "NN", "ART", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "So, da\u00df du in dem Ernst bald hie, bald da gestockt,", "tokens": ["So", ",", "da\u00df", "du", "in", "dem", "Ernst", "bald", "hie", ",", "bald", "da", "ge\u00b7stockt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "ADV", "ADV", "$,", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "An statt, da\u00df sich dein Flei\u00df und Eifer mehren sollen,", "tokens": ["An", "statt", ",", "da\u00df", "sich", "dein", "Flei\u00df", "und", "Ei\u00b7fer", "meh\u00b7ren", "sol\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "$,", "KOUS", "PRF", "PPOSAT", "NN", "KON", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Das wei\u00df ich, liebstes Kind! aus dem, was deine Treu", "tokens": ["Das", "wei\u00df", "ich", ",", "liebs\u00b7tes", "Kind", "!", "aus", "dem", ",", "was", "dei\u00b7ne", "Treu"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "$,", "ADJA", "NN", "$.", "APPR", "ART", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Mir als dem N\u00e4hesten in Liebe selbst vertrauet.", "tokens": ["Mir", "als", "dem", "N\u00e4\u00b7hes\u00b7ten", "in", "Lie\u00b7be", "selbst", "ver\u00b7trau\u00b7et", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "ART", "NN", "APPR", "NN", "ADV", "VVFIN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.39": {"text": "Allein, wie preise ich den guten GOt", "tokens": ["Al\u00b7lein", ",", "wie", "prei\u00b7se", "ich", "den", "gu\u00b7ten", "Got"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Da\u00df er bey alle dem dich gn\u00e4dig angeschauet;", "tokens": ["Da\u00df", "er", "bey", "al\u00b7le", "dem", "dich", "gn\u00e4\u00b7dig", "an\u00b7ge\u00b7schau\u00b7et", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIS", "ART", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "In solchem Stande bin ich von der guten Hand", "tokens": ["In", "sol\u00b7chem", "Stan\u00b7de", "bin", "ich", "von", "der", "gu\u00b7ten", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Des lieben Vaters selbst hieher geleitet worden;", "tokens": ["Des", "lie\u00b7ben", "Va\u00b7ters", "selbst", "hie\u00b7her", "ge\u00b7lei\u00b7tet", "wor\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "PAV", "VVPP", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Da kn\u00fcpfte GOtt zuerst das innerliche Band,", "tokens": ["Da", "kn\u00fcpf\u00b7te", "Gott", "zu\u00b7erst", "das", "in\u00b7ner\u00b7li\u00b7che", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Da ward der Heyraths-Schlu\u00df gefa\u00dft im W\u00e4chter-Orden.", "tokens": ["Da", "ward", "der", "Hey\u00b7raths\u00b7Schlu\u00df", "ge\u00b7fa\u00dft", "im", "W\u00e4ch\u00b7ter\u00b7Or\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Doch wurde die Gedult und die Gelassenheit", "tokens": ["Doch", "wur\u00b7de", "die", "Ge\u00b7dult", "und", "die", "Ge\u00b7las\u00b7sen\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Nach jedes Nothdurft erst absonderlich probiret;", "