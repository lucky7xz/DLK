Wegen scheun,", "tokens": ["Wenn", "and\u00b7re", "Men\u00b7schen", "sich", "vor", "sei\u00b7nen", "We\u00b7gen", "scheun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "So windet sie ihr Freund aus den verwirrt\u2019sten Sachen.", "tokens": ["So", "win\u00b7det", "sie", "ihr", "Freund", "aus", "den", "ver\u00b7wirrt'\u00b7sten", "Sa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Sein Seegen breitet sich auf Kindes-Kinder aus,", "tokens": ["Sein", "See\u00b7gen", "brei\u00b7tet", "sich", "auf", "Kin\u00b7des\u00b7Kin\u00b7der", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Ins weit entfernt\u2019ste Glied verdoppelt sich die Gnade,", "tokens": ["Ins", "weit", "ent\u00b7fernt'\u00b7ste", "Glied", "ver\u00b7dop\u00b7pelt", "sich", "die", "Gna\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJD", "ADJA", "NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Und endlich bringt Er die in ein best\u00e4ndigs Hau\u00df,", "tokens": ["Und", "end\u00b7lich", "bringt", "Er", "die", "in", "ein", "be\u00b7st\u00e4n\u00b7digs", "Hau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Die hier nicht wohneten. Denn Welt war ihnen Schade. Phil. 3.", "tokens": ["Die", "hier", "nicht", "woh\u00b7ne\u00b7ten", ".", "Denn", "Welt", "war", "ih\u00b7nen", "Scha\u00b7de", ".", "Phil", ".", "3."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "ordinal"], "pos": ["ART", "ADV", "PTKNEG", "VVFIN", "$.", "KON", "NN", "VAFIN", "PPER", "ADJD", "$.", "NE", "$.", "ADJA"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.57": {"text": "Wohlan, die Zeit ist kurtz, die Gnade sey mit dir!", "tokens": ["Wo\u00b7hlan", ",", "die", "Zeit", "ist", "kurtz", ",", "die", "Gna\u00b7de", "sey", "mit", "dir", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Ich wolte dir wohl sonst, mein Hertz! genauer sagen;", "tokens": ["Ich", "wol\u00b7te", "dir", "wohl", "sonst", ",", "mein", "Hertz", "!", "ge\u00b7nau\u00b7er", "sa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADV", "$,", "PPOSAT", "NN", "$.", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Allein dis sey genug: Geh\u00fclfin! tragen wir", "tokens": ["Al\u00b7lein", "dis", "sey", "ge\u00b7nug", ":", "Ge\u00b7h\u00fcl\u00b7fin", "!", "tra\u00b7gen", "wir"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "PDS", "VAFIN", "ADV", "$.", "NN", "$.", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.h