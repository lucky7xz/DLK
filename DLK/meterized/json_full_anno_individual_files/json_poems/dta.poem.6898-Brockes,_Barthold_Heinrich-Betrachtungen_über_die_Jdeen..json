{"dta.poem.6898": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtungen \u00fcber die Jdeen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Lasst uns die zarte Kindheit sehen,", "tokens": ["Lasst", "uns", "die", "zar\u00b7te", "Kind\u00b7heit", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wir treffen sie nicht ohn Erkenntni\u00df an,", "tokens": ["Wir", "tref\u00b7fen", "sie", "nicht", "ohn", "Er\u00b7kennt\u00b7ni\u00df", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es scheint ja ihr ein Licht noch eher zu entstehen,", "tokens": ["Es", "scheint", "ja", "ihr", "ein", "Licht", "noch", "e\u00b7her", "zu", "ent\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "ART", "NN", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als die Erfahrung ihr und Lehr was zeigen kan.", "tokens": ["Als", "die", "Er\u00b7fah\u00b7rung", "ihr", "und", "Lehr", "was", "zei\u00b7gen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "KON", "NN", "PWS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein Kind, wenn es ein Vorwurff r\u00fchret,", "tokens": ["Ein", "Kind", ",", "wenn", "es", "ein", "Vor\u00b7wurff", "r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Erwehlt, und folget seinem Willen,", "tokens": ["Er\u00b7wehlt", ",", "und", "fol\u00b7get", "sei\u00b7nem", "Wil\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Es l\u00e4sst, als wenn in ihm man ein Erinnern sp\u00fcret,", "tokens": ["Es", "l\u00e4sst", ",", "als", "wenn", "in", "ihm", "man", "ein", "E\u00b7rin\u00b7nern", "sp\u00fc\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOKOM", "KOUS", "APPR", "PPER", "PIS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Man siehts sich Staffelweis entwickeln und enth\u00fcllen.", "tokens": ["Man", "siehts", "sich", "Staf\u00b7fel\u00b7weis", "ent\u00b7wi\u00b7ckeln", "und", "ent\u00b7h\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "NN", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}