{"dta.poem.17617": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "239.  Gott hat kein Muster als sich selbst.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Fragstu warumb mich Gott nach seinem Bildn\u00fc\u00df", "tokens": ["Frags\u00b7tu", "wa\u00b7rumb", "mich", "Gott", "nach", "sei\u00b7nem", "Bild\u00b7n\u00fc\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PWAV", "PPER", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Machte?", "tokens": ["Mach\u00b7te", "?"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Ich sag\u2019 es war niemands der jhm ein anders brachte.", "tokens": ["Ich", "sag'", "es", "war", "nie\u00b7mands", "der", "jhm", "ein", "an\u00b7ders", "brach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VAFIN", "PIS", "ART", "PPER", "ART", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}