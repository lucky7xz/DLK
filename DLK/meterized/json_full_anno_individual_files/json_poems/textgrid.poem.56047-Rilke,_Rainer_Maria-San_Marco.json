{"textgrid.poem.56047": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "San Marco", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In diesem Innern, das wie ausgeh\u00f6hlt", "tokens": ["In", "die\u00b7sem", "In\u00b7nern", ",", "das", "wie", "aus\u00b7ge\u00b7h\u00f6hlt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "KOKOM", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "sich w\u00f6lbt und wendet in den goldnen Smalten,", "tokens": ["sich", "w\u00f6lbt", "und", "wen\u00b7det", "in", "den", "gold\u00b7nen", "Smal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "rundkantig, glatt, mit K\u00f6stlichkeit ge\u00f6lt,", "tokens": ["rund\u00b7kan\u00b7tig", ",", "glatt", ",", "mit", "K\u00f6st\u00b7lich\u00b7keit", "ge\u00b7\u00f6lt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "ward dieses Staates Dunkelheit gehalten", "tokens": ["ward", "die\u00b7ses", "Staa\u00b7tes", "Dun\u00b7kel\u00b7heit", "ge\u00b7hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PDAT", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "und heimlich aufgeh\u00e4uft, als Gleichgewicht", "tokens": ["und", "heim\u00b7lich", "auf\u00b7ge\u00b7h\u00e4uft", ",", "als", "Gleich\u00b7ge\u00b7wicht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJD", "VVPP", "$,", "KOUS", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "des Lichtes, das in allen seinen Dingen", "tokens": ["des", "Lich\u00b7tes", ",", "das", "in", "al\u00b7len", "sei\u00b7nen", "Din\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "sich so vermehrte, da\u00df sie fast vergingen \u2013.", "tokens": ["sich", "so", "ver\u00b7mehr\u00b7te", ",", "da\u00df", "sie", "fast", "ver\u00b7gin\u00b7gen", "\u2013", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PRF", "ADV", "ADJA", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$(", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und pl\u00f6tzlich zweifelst du: vergehn sie nicht?", "tokens": ["Und", "pl\u00f6tz\u00b7lich", "zwei\u00b7felst", "du", ":", "ver\u00b7gehn", "sie", "nicht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "$.", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "und dr\u00e4ngst zur\u00fcck die harte Galerie,", "tokens": ["und", "dr\u00e4ngst", "zu\u00b7r\u00fcck", "die", "har\u00b7te", "Ga\u00b7le\u00b7rie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "die, wie ein Gang im Bergwerk, nah am Glanz", "tokens": ["die", ",", "wie", "ein", "Gang", "im", "Berg\u00b7werk", ",", "nah", "am", "Glanz"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "$,", "PWAV", "A