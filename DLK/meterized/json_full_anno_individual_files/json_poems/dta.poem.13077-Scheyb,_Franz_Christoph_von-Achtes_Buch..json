{"dta.poem.13077": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Achtes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eso bist du das Geb\u00e4u zu f\u00fchren unvonn\u00f6then,", "tokens": ["\u201e", "so", "bist", "du", "das", "Ge\u00b7b\u00e4u", "zu", "f\u00fch\u00b7ren", "un\u00b7von\u00b7n\u00f6\u00b7then", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "ART", "NN", "PTKZU", "VVINF", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "40\u201dWeil es die Winde leicht \u201d, erhohlt der Grei\u00df, verwehten.", "tokens": ["\"", "Weil", "es", "die", "Win\u00b7de", "leicht", "\"", ",", "er\u00b7hohlt", "der", "Grei\u00df", ",", "ver\u00b7weh\u00b7ten", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ART", "NN", "ADJD", "$(", "$,", "VVFIN", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201ewir brauchen Kalch und Sand, Stahl, Marmel und Metall,", "tokens": ["\u201e", "wir", "brau\u00b7chen", "Kalch", "und", "Sand", ",", "Stahl", ",", "Mar\u00b7mel", "und", "Me\u00b7tall", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "NN", "KON", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eso taugt dein Wort-Gepr\u00e4ng sehr schlecht zu diesem Fall.", "tokens": ["\u201e", "so", "taugt", "dein", "Wor\u00b7tGe\u00b7pr\u00e4ng", "sehr", "schlecht", "zu", "die\u00b7sem", "Fall", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201ees m\u00fcssen Wercke seyn; man will den F\u00fcrsten ehren,", "tokens": ["\u201e", "es", "m\u00fcs\u00b7sen", "Wer\u00b7cke", "seyn", ";", "man", "will", "den", "F\u00fcrs\u00b7ten", "eh\u00b7ren", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "NN", "VAINF", "$.", "PIS", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201eda\u00df auch die sp\u00e4tste Welt soll dessen Nachruhm h\u00f6ren.", "tokens": ["\u201e", "da\u00df", "auch", "die", "sp\u00e4ts\u00b7te", "Welt", "soll", "des\u00b7sen", "Nach\u00b7ruhm", "h\u00f6\u00b7ren", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "ADV", "ART", "ADJA", "NN", "VMFIN", "PDS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "45\u201dDer eitle W\u00f6rter-Klang verliehrt sich in der Luft,", "tokens": ["\"", "Der", "eit\u00b7le", "W\u00f6r\u00b7ter\u00b7Klang", "ver\u00b7liehrt", "sich", "in", "der", "Luft", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201enimmt die Vergessenheit ihn nicht in ihre Gruft?", "tokens": ["\u201e", "nimmt", "die", "Ver\u00b7ges\u00b7sen\u00b7heit", "ihn", "nicht", "in", "ih\u00b7re", "Gruft", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ART", "NN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.9": {"text": "\u201ees mu\u00df Unsterblichkeit die Meisterst\u00fccke sehen,", "tokens": ["\u201e", "es", "mu\u00df", "U\u00b7nsterb\u00b7lich\u00b7keit", "die", "Meis\u00b7ter\u00b7st\u00fc\u00b7cke", "se\u00b7hen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-++--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "\u201eauf welchen Ruhm und Lob dergleichen F\u00fcrsten stehen.", "tokens": ["\u201e", "auf", "wel\u00b7chen", "Ruhm", "und", "Lob", "derg\u00b7lei\u00b7chen", "F\u00fcrs\u00b7ten", "ste\u00b7hen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PWAT", "NN", "KON", "NN", "PIS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201eja! die Unsterblichkeit ..... Hier regte sich ein Weib", "tokens": ["\u201e", "ja", "!", "die", "U\u00b7nsterb\u00b7lich\u00b7keit", ".....", "Hier", "reg\u00b7te", "sich", "ein", "Weib"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PTKANT", "$.", "ART", "NN", "$.", "ADV", "VVFIN", "PRF", "ART", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.12": {"text": "50Und unterbrach das Wort: sie war am ganzen Leib", "tokens": ["un\u00b7ter\u00b7brach", "das", "Wort", ":", "sie", "war", "am", "gan\u00b7zen", "Leib"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$.", "PPER", "VAFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.13": {"text": "Mit goldnem Stoff bedeckt; ihr Schild wies Amaranten,", "tokens": ["Mit", "gold\u00b7nem", "Stoff", "be\u00b7deckt", ";", "ihr", "Schild", "wies", "A\u00b7ma\u00b7ran\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$.", "PPOSAT", "NN", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Nebst eines Vogels Bild, den sie den Ph\u00f6nix nannten.", "tokens": ["Nebst", "ei\u00b7nes", "Vo\u00b7gels", "Bild", ",", "den", "sie", "den", "Ph\u00f6\u00b7nix", "nann\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So fieng ihr Vortrag an: \u201eIch habe bis daher", "tokens": ["So", "fi\u00b7eng", "ihr", "Vor\u00b7trag", "an", ":", "\u201e", "Ich", "ha\u00b7be", "bis", "da\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$.", "$(", "PPER", "VAFIN", "ADV", "PAV"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "\u201eaufmercksam zugeh\u00f6rt, was man zum Schlu\u00df begehr;", "tokens": ["\u201e", "auf\u00b7merck\u00b7sam", "zu\u00b7ge\u00b7h\u00f6rt", ",", "was", "man", "zum", "Schlu\u00df", "be\u00b7gehr", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVPP", "$,", "PRELS", "PIS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "55\u201dIch, die Unsterblichkeit, von der du wolltest sprechen,", "tokens": ["\"", "Ich", ",", "die", "U\u00b7nsterb\u00b7lich\u00b7keit", ",", "von", "der", "du", "woll\u00b7test", "spre\u00b7chen", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "$,", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VMFIN", "VVINF", "$,"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.18": {"text": "\u201emu\u00df hier, verzeih es mir, dein Reden unterbrechen:", "tokens": ["\u201e", "mu\u00df", "hier", ",", "ver\u00b7zeih", "es", "mir", ",", "dein", "Re\u00b7den", "un\u00b7ter\u00b7bre\u00b7chen", ":"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "ADV", "$,", "VVIMP", "PPER", "PPER", "$,", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "\u201eja! die Unsterblichkeit, womit ", "tokens": ["\u201e", "ja", "!", "die", "U\u00b7nsterb\u00b7lich\u00b7keit", ",", "wo\u00b7mit"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word"], "pos": ["$(", "PTKANT", "$.", "ART", "NN", "$,", "PWAV"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.20": {"text": "\u201evon mir beschencket ward, als ich sie streiten sah,", "tokens": ["\u201e", "von", "mir", "be\u00b7schen\u00b7cket", "ward", ",", "als", "ich", "sie", "strei\u00b7ten", "sah", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPER", "VVFIN", "VAFIN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "\u201eist jenes, was ich auch f\u00fcr diesen F\u00fcrsten w\u00e4hle,", "tokens": ["\u201e", "ist", "je\u00b7nes", ",", "was", "ich", "auch", "f\u00fcr", "die\u00b7sen", "F\u00fcrs\u00b7ten", "w\u00e4h\u00b7le", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PDS", "$,", "PWS", "PPER", "ADV", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "60\u201dWeil ich zu denen ihn, die niemahls sterben, z\u00e4hle.", "tokens": ["\"", "Weil", "ich", "zu", "de\u00b7nen", "ihn", ",", "die", "nie\u00b7mahls", "ster\u00b7ben", ",", "z\u00e4h\u00b7le", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "APPR", "PRELS", "PPER", "$,", "PRELS", "ADV", "VVINF", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "\u201eseht diesen Vogel an! seht diesen gr\u00fcnen Strau\u00df!", "tokens": ["\u201e", "seht", "die\u00b7sen", "Vo\u00b7gel", "an", "!", "seht", "die\u00b7sen", "gr\u00fc\u00b7nen", "Strau\u00df", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PDAT", "NN", "PTKVZ", "$.", "VVFIN", "PDAT", "ADJA", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "(sie hob den Schild empor) \u201edie leben niemahl aus.", "tokens": ["(", "sie", "hob", "den", "Schild", "em\u00b7por", ")", "\u201e", "die", "le\u00b7ben", "nie\u00b7mahl", "aus", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$(", "$(", "PDS", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "\u201edas ligt an meiner Macht. Jhr kennt des F\u00fcrstens Ahnen;", "tokens": ["\u201e", "das", "ligt", "an", "mei\u00b7ner", "Macht", ".", "Ihr", "kennt", "des", "F\u00fcrs\u00b7tens", "Ah\u00b7nen", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VVFIN", "APPR", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "\u201esie seynd in meinem Reich; Er ist auf ihren Bahnen.", "tokens": ["\u201e", "sie", "seynd", "in", "mei\u00b7nem", "Reich", ";", "Er", "ist", "auf", "ih\u00b7ren", "Bah\u00b7nen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "APPR", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "65\u201dSo geht ihm nichts mehr ab, als meiner Tugend Kraft,", "tokens": ["\"", "So", "geht", "ihm", "nichts", "mehr", "ab", ",", "als", "mei\u00b7ner", "Tu\u00b7gend", "Kraft", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "PIS", "ADV", "PTKVZ", "$,", "KOUS", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "\u201edie dessen Majest\u00e4t der Welt Nachkommenschaft,", "tokens": ["\u201e", "die", "des\u00b7sen", "Ma\u00b7jes\u00b7t\u00e4t", "der", "Welt", "Nach\u00b7kom\u00b7men\u00b7schaft", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "PRELAT", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "\u201eja dessen Ruhm, Verdienst und h\u00f6chsten Nahmens-Preise", "tokens": ["\u201e", "ja", "des\u00b7sen", "Ruhm", ",", "Ver\u00b7dienst", "und", "h\u00f6chs\u00b7ten", "Nah\u00b7mens\u00b7Prei\u00b7se"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ADV", "ART", "NN", "$,", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "\u201ein Ceder, Marmel, Erz, in Gold gepr\u00e4get weise.", "tokens": ["\u201e", "in", "Ce\u00b7der", ",", "Mar\u00b7mel", ",", "Erz", ",", "in", "Gold", "ge\u00b7pr\u00e4\u00b7get", "wei\u00b7se", "."], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "$,", "NN", "$,", "NN", "$,", "APPR", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}