{"dta.poem.3781": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1892", "urn": "urn:nbn:de:kobv:b4-200905191524", "language": ["de:0.85", "fr:0.14"], "booktitle": "George, Stefan: Algabal. Paris u. a., 1892."}, "poem": {"stanza.1": {"line.1": {"text": "Und durch grause fluren jagen", "tokens": ["Und", "durch", "grau\u00b7se", "flu\u00b7ren", "ja\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVINF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bis wir uns im moor verirren", "tokens": ["Bis", "wir", "uns", "im", "moor", "ver\u00b7ir\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPRART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Oder blitze mich erschlagen", "tokens": ["O\u00b7der", "blit\u00b7ze", "mich", "er\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Auf dem samenlosen acker", "tokens": ["Auf", "dem", "sa\u00b7men\u00b7lo\u00b7sen", "ac\u00b7ker"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Viele helden stumm verbleichen", "tokens": ["Vie\u00b7le", "hel\u00b7den", "stumm", "ver\u00b7blei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur das russende geflacker", "tokens": ["Nur", "das", "rus\u00b7sen\u00b7de", "ge\u00b7fla\u00b7cker"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Loher fichten ehrt die leichen", "tokens": ["Lo\u00b7her", "fich\u00b7ten", "ehrt", "die", "lei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Schmal in regelgraden ketten", "tokens": ["Schmal", "in", "re\u00b7gel\u00b7gra\u00b7den", "ket\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rinnen ziegelrote b\u00e4che", "tokens": ["Rin\u00b7nen", "zie\u00b7gel\u00b7ro\u00b7te", "b\u00e4\u00b7che"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seufzen singt aus ihren betten", "tokens": ["Seuf\u00b7zen", "singt", "aus", "ih\u00b7ren", "bet\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hahler wind umkreist die fl\u00e4che", "tokens": ["Hah\u00b7ler", "wind", "um\u00b7kreist", "die", "fl\u00e4\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VVFIN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aufgel\u00f6st im sande w\u00fchlend", "tokens": ["Auf\u00b7ge\u00b7l\u00f6st", "im", "san\u00b7de", "w\u00fch\u00b7lend"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Frauenhaare dichte str\u00e4hnen", "tokens": ["Frau\u00b7en\u00b7haa\u00b7re", "dich\u00b7te", "str\u00e4h\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Frauenthr\u00e4nen wunden k\u00fchlend", "tokens": ["Frau\u00b7ent\u00b7hr\u00e4\u00b7nen", "wun\u00b7den", "k\u00fch\u00b7lend"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "ADJA", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Reiche thr\u00e4nen \u00b7 wahre thr\u00e4nen?", "tokens": ["Rei\u00b7che", "thr\u00e4\u00b7nen", "wah\u00b7re", "thr\u00e4\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "XY", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}