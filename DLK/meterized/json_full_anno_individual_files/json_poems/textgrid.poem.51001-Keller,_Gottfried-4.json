{"textgrid.poem.51001": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "4", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sah ich eine junge Welle,", "tokens": ["Sah", "ich", "ei\u00b7ne", "jun\u00b7ge", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die durch Alpenrosen flo\u00df", "tokens": ["Die", "durch", "Al\u00b7pen\u00b7ro\u00b7sen", "flo\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sich rauschend mit der Quelle,", "tokens": ["Und", "sich", "rau\u00b7schend", "mit", "der", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit dem Strom ins Tal ergo\u00df.", "tokens": ["Mit", "dem", "Strom", "ins", "Tal", "er\u00b7go\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Schien der Himmel drin versunken,", "tokens": ["Schien", "der", "Him\u00b7mel", "drin", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und war doch so leicht und klar,", "tokens": ["Und", "war", "doch", "so", "leicht", "und", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und ich hab davon getrunken,", "tokens": ["Und", "ich", "hab", "da\u00b7von", "ge\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PAV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie so frisch und rein sie war!", "tokens": ["Wie", "so", "frisch", "und", "rein", "sie", "war", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "KON", "ADJD", "PPER", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Bin dann auf dem Meer gelegen,", "tokens": ["Bin", "dann", "auf", "dem", "Meer", "ge\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo das Kreuz am Himmel steht;", "tokens": ["Wo", "das", "Kreuz", "am", "Him\u00b7mel", "steht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht konnt unser Schiff sich regen,", "tokens": ["Nicht", "konnt", "un\u00b7ser", "Schiff", "sich", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "PPOSAT", "NN", "PRF", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In der Glut kein L\u00fcftchen weht'!", "tokens": ["In", "der", "Glut", "kein", "L\u00fcft\u00b7chen", "weht'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Schaut ich in die Wasser nieder,", "tokens": ["Schaut", "ich", "in", "die", "Was\u00b7ser", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In die Tiefen unverwandt", "tokens": ["In", "die", "Tie\u00b7fen", "un\u00b7ver\u00b7wandt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sah meine Welle wieder", "tokens": ["Und", "sah", "mei\u00b7ne", "Wel\u00b7le", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ADV"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Aus den Bergen, wohlbekannt.", "tokens": ["Aus", "den", "Ber\u00b7gen", ",", "wohl\u00b7be\u00b7kannt", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Von dem hei\u00dfen Strahl durchzittert,", "tokens": ["Von", "dem", "hei\u00b7\u00dfen", "Strahl", "durch\u00b7zit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ja, sie war es, deutlich, nah!", "tokens": ["Ja", ",", "sie", "war", "es", ",", "deut\u00b7lich", ",", "nah", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "PPER", "$,", "ADJD", "$,", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch versalzen und verbittert,", "tokens": ["Doch", "ver\u00b7sal\u00b7zen", "und", "ve