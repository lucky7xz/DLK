{"dta.poem.5820": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Gesetzen der Bewegung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie es unstreitig wahr", "tokens": ["Wie", "es", "un\u00b7strei\u00b7tig", "wahr"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADJD", "ADJD"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Da\u00df alle C\u00f6rper stets, so viel sie k\u00f6nnen,", "tokens": ["Da\u00df", "al\u00b7le", "C\u00f6r\u00b7per", "stets", ",", "so", "viel", "sie", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADV", "$,", "ADV", "ADV", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In einer Linie gerade rennen;", "tokens": ["In", "ei\u00b7ner", "Li\u00b7nie", "ge\u00b7ra\u00b7de", "ren\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So ist es ebenfalls nicht minder klar,", "tokens": ["So", "ist", "es", "e\u00b7ben\u00b7falls", "nicht", "min\u00b7der", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da\u00df die, so wir im Kreyse drehen,", "tokens": ["Da\u00df", "die", ",", "so", "wir", "im", "Krey\u00b7se", "dre\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "$,", "ADV", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und die da in der R\u00fcnde gehen,", "tokens": ["Und", "die", "da", "in", "der", "R\u00fcn\u00b7de", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "In ihren Fliehen, ohn Verweilen,", "tokens": ["In", "ih\u00b7ren", "Flie\u00b7hen", ",", "ohn", "Ver\u00b7wei\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KOUI", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Von ihrem Mittelpunet best\u00e4ndig abw\u00e4rts eilen.", "tokens": ["Von", "ih\u00b7rem", "Mit\u00b7tel\u00b7pu\u00b7net", "be\u00b7st\u00e4n\u00b7dig", "ab\u00b7w\u00e4rts", "ei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Wenn sie gerade vor sich fliegen", "tokens": ["Wenn", "sie", "ge\u00b7ra\u00b7de", "vor", "sich", "flie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Sieht man all Augenblick die Linien sich biegen", "tokens": ["Sieht", "man", "all", "Au\u00b7gen\u00b7blick", "die", "Li\u00b7ni\u00b7en", "sich", "bie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "PIAT", "NN", "ART", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und da\u00df ihr Lauff sich lenckt. Daher ist gantz gewi\u00df,", "tokens": ["Und", "da\u00df", "ihr", "Lauff", "sich", "lenckt", ".", "Da\u00b7her", "ist", "gantz", "ge\u00b7wi\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "PRF", "VVFIN", "$.", "PAV", "VAFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da\u00df eine Hinderni\u00df", "tokens": ["Da\u00df", "ei\u00b7ne", "Hin\u00b7der\u00b7ni\u00df"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Sie st\u00f6sset, aus dem Wege dringet,", "tokens": ["Sie", "st\u00f6s\u00b7set", ",", "aus", "dem", "We\u00b7ge", "drin\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Und sie zu dieser Aendrung zwinget.", "tokens": ["Und", "sie", "zu", "die\u00b7ser", "A\u00b7en\u00b7drung", "zwin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}}}}