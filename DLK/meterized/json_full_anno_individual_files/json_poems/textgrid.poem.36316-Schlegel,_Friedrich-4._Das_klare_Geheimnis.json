{"textgrid.poem.36316": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "4. Das klare Geheimnis", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vielseitigkeit wird auf des Lebens Gipfeln nur", "tokens": ["Viel\u00b7sei\u00b7tig\u00b7keit", "wird", "auf", "des", "Le\u00b7bens", "Gip\u00b7feln", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "APPR", "ART", "NN", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gefunden, wo des reinen Daseins heitres Nichts,", "tokens": ["Ge\u00b7fun\u00b7den", ",", "wo", "des", "rei\u00b7nen", "Da\u00b7seins", "heit\u00b7res", "Nichts", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}, "line.3": {"text": "In zarter Redensarten klaren Schein verh\u00fcllt,", "tokens": ["In", "zar\u00b7ter", "Re\u00b7dens\u00b7ar\u00b7ten", "kla\u00b7ren", "Schein", "ver\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auf schwankem Seil der Bildung hin und wieder spielt.", "tokens": ["Auf", "schwan\u00b7kem", "Seil", "der", "Bil\u00b7dung", "hin", "und", "wie\u00b7der", "spielt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "PTKVZ", "KON", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So red' und bilde dich, gebildet rede fort;", "tokens": ["So", "red'",