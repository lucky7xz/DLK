{"dta.poem.2711": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Trauer-Ode/  \n  Uber dem fr\u00fchen Absterben Jungf. A. D. B.  \n den 6. April 1678.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dein Freund ist wie ein P\u00fcschel Myrrhen", "tokens": ["Dein", "Freund", "ist", "wie", "ein", "P\u00fc\u00b7schel", "Myr\u00b7rhen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "KOKOM", "ART", "NN", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Das zwischen deinen Br\u00fcsten liegt.", "tokens": ["Das", "zwi\u00b7schen", "dei\u00b7nen", "Br\u00fcs\u00b7ten", "liegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du hast satt au\u00df den Angst-Geschirren", "tokens": ["Du", "hast", "satt", "au\u00df", "den", "Angst\u00b7Ge\u00b7schir\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verblichnes Jungfer-Bild gekriegt;", "tokens": ["Ver\u00b7blich\u00b7nes", "Jung\u00b7fer\u00b7Bild", "ge\u00b7kriegt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Aloe war nur zu bitter", "tokens": ["Die", "A\u00b7loe", "war", "nur", "zu", "bit\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKA", "ADJD"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Die dir dein Leben abgezehrt/", "tokens": ["Die", "dir", "dein", "Le\u00b7ben", "ab\u00b7ge\u00b7zehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und durch der Kranckheit Ungewitter", "tokens": ["Und", "durch", "der", "Kran\u00b7ck\u00b7heit", "Un\u00b7ge\u00b7wit\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "In eine Leiche dich verkehrt.", "tokens": ["In", "ei\u00b7ne", "Lei\u00b7che", "dich", "ver\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Allein da alle dich verlassen/", "tokens": ["Al\u00b7lein", "da", "al\u00b7le", "dich", "ver\u00b7las\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PIS", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und niemand mehr mit H\u00fclff erscheint/", "tokens": ["Und", "nie\u00b7mand", "mehr", "mit", "H\u00fclff", "er\u00b7scheint", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So will dein JEsus dich umbfassen", "tokens": ["So", "will", "dein", "Je\u00b7sus", "dich", "umb\u00b7fas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPOSAT", "NE", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der allerbest\u2019 und treuste Freund.", "tokens": ["Der", "al\u00b7ler\u00b7best'", "und", "treus\u00b7te", "Freund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der will dich in den Rosen weiden/", "tokens": ["Der", "will", "dich", "in", "den", "Ro\u00b7sen", "wei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PRF", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bi\u00df da\u00df de\u00df Todes Schatten k\u00f6mmt/", "tokens": ["Bi\u00df", "da\u00df", "de\u00df", "To\u00b7des", "Schat\u00b7ten", "k\u00f6mmt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und er durch seinen Tod und Leiden", "tokens": ["Und", "er", "durch", "sei\u00b7nen", "Tod", "und", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Nacht der S\u00fcnden von dir nimmt.", "tokens": ["Die", "Nacht", "der", "S\u00fcn\u00b7den", "von", "dir", "nimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Kan eine Liebe dieser gleichen", "tokens": ["Kan", "ei\u00b7ne", "Lie\u00b7be", "die\u00b7ser", "glei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "PDAT", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die \u00fcber Erd\u2019 und Himmel geht?", "tokens": ["Die", "\u00fc\u00b7ber", "Erd'", "und", "Him\u00b7mel", "geht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach Liebe du thust Wunder-Zeichen!", "tokens": ["Ach", "Lie\u00b7be", "du", "thust", "Wun\u00b7der\u00b7Zei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du bist der seeligste Magnet!", "tokens": ["Du", "bist", "der", "see\u00b7ligs\u00b7te", "Mag\u00b7net", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Der unsern Staub mit GOtt verm\u00e4hlet", "tokens": ["Der", "un\u00b7sern", "Staub", "mit", "Gott", "ver\u00b7m\u00e4h\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und uns durch desseu Wunden f\u00fchrt.", "tokens": ["Und", "uns", "durch", "des\u00b7seu", "Wun\u00b7den", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ja da\u00df uns ewig nichts mehr fehlet/", "tokens": ["Ja", "da\u00df", "uns", "e\u00b7wig", "nichts", "mehr", "feh\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "KOUS", "PPER", "ADJD", "PIS", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Gut machet/ was wir selbst verb\u00fchrt.", "tokens": ["Gut", "ma\u00b7chet", "/", "was", "wir", "selbst", "ver\u00b7b\u00fchrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$(", "PWS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du wilst mit deinem Heyland sterben", "tokens": ["Du", "wilst", "mit", "dei\u00b7nem", "Hey\u00b7land", "ster\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Seel und Hertz zerschmeltzt in ihm.", "tokens": ["Und", "Seel", "und", "Hertz", "zer\u00b7schmeltzt", "in", "ihm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVPP", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Tod mag Lipp und Wang entferben", "tokens": ["Der", "Tod", "mag", "Lipp", "und", "Wang", "ent\u00b7fer\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "NE", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du weist da\u00df dieses ein ", "tokens": ["Du", "weist", "da\u00df", "die\u00b7ses", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOUS", "PDAT", "ART"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "An dem dein JEsus sich ergetzet/", "tokens": ["An", "dem", "dein", "Je\u00b7sus", "sich", "er\u00b7get\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "NE", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df man aus Dornen Kronen flicht/", "tokens": ["Da\u00df", "man", "aus", "Dor\u00b7nen", "Kro\u00b7nen", "flicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie sehr die F\u00e4uln\u00fc\u00df dich verletzet/", "tokens": ["Wie", "sehr", "die", "F\u00e4ul\u00b7n\u00fc\u00df", "dich", "ver\u00b7let\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Je sch\u00f6ner bist du zugericht.", "tokens": ["Je", "sch\u00f6\u00b7ner", "bist", "du", "zu\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Er will gekeltert dir gefallen/", "tokens": ["Er", "will", "ge\u00b7kel\u00b7tert", "dir", "ge\u00b7fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVPP", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du kennst\u2019 es da\u00df sein Kleid blut-roth.", "tokens": ["Du", "kennst'", "es", "da\u00df", "sein", "Kleid", "blut\u00b7roth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KOUS", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er ist der Sch\u00f6nste unter allen", "tokens": ["Er", "ist", "der", "Sch\u00f6ns\u00b7te", "un\u00b7ter", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Retter in der letzten Noth.", "tokens": ["Dein", "Ret\u00b7ter", "in", "der", "letz\u00b7ten", "Noth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach geh ach geh/ mit ihm zu grabe", "tokens": ["Ach", "geh", "ach", "geh", "/", "mit", "ihm", "zu", "gra\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "ADV", "VVFIN", "$(", "APPR", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie pr\u00e4chtig wirstu aufferstehn!", "tokens": ["Wie", "pr\u00e4ch\u00b7tig", "wirs\u00b7tu", "auf\u00b7fer\u00b7stehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn di\u00df/ was itzt frist Wurm und Schabe/", "tokens": ["Wenn", "di\u00df", "/", "was", "itzt", "frist", "Wurm", "und", "Scha\u00b7be", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "$(", "PWS", "ADV", "ADJD", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wird \u00fcber Stern und Sonn gehn.", "tokens": ["Wird", "\u00fc\u00b7ber", "Stern", "und", "Sonn", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Braut eile/ la\u00df die Myrrhen-H\u00fcgel", "tokens": ["Braut", "ei\u00b7le", "/", "la\u00df", "die", "Myr\u00b7rhen\u00b7H\u00fc\u00b7gel"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$(", "VVIMP", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Seelen Trost und Labsal seyn.", "tokens": ["Der", "See\u00b7len", "Trost", "und", "Lab\u00b7sal", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3":