{"textgrid.poem.64296": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "15. Selbstverleugnung", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Thr\u00e4nen der Sehnsucht tr\u00fcben Doris' Augen;", "tokens": ["Thr\u00e4\u00b7nen", "der", "Sehn\u00b7sucht", "tr\u00fc\u00b7ben", "Do\u00b7ris'", "Au\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ihren seufzenden Busen hebt die Treue:", "tokens": ["Ih\u00b7ren", "seuf\u00b7zen\u00b7den", "Bu\u00b7sen", "hebt", "die", "Treu\u00b7e", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Sturm und Woge fernen von ihren K\u00fcssen,", "tokens": ["Sturm", "und", "Wo\u00b7ge", "fer\u00b7nen", "von", "ih\u00b7ren", "K\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADJA", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Welchen sie liebet.", "tokens": ["Wel\u00b7chen", "sie", "lie\u00b7bet", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Wehende Weste, bringet ihn den K\u00fcssen", "tokens": ["We\u00b7hen\u00b7de", "Wes\u00b7te", ",", "brin\u00b7get", "ihn", "den", "K\u00fcs\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "ART", "NN"], "meter": "+