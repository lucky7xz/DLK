{"dta.poem.529": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Uber den Feld - Zug der hohen  Alliir ten in  \n Flandern/ im Jahr 1708. und 1709. und folgen-  \n den andern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Du stoltzer Ludewig, doch jetzo mehr als klein/", "tokens": ["Du", "stolt\u00b7zer", "Lu\u00b7de\u00b7wig", ",", "doch", "jet\u00b7zo", "mehr", "als", "klein", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NE", "$,", "ADV", "ADV", "PIAT", "KOKOM", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sieh wie dein Hochmuth f\u00e4llt/ und deine Gr\u00f6sse ein;", "tokens": ["Sieh", "wie", "dein", "Hoch\u00b7muth", "f\u00e4llt", "/", "und", "dei\u00b7ne", "Gr\u00f6s\u00b7se", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "PPOSAT", "NN", "VVFIN", "$(", "KON", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn die verkn\u00fcpffte Macht dich ihre St\u00e4rcke zeiget/", "tokens": ["Wenn", "die", "ver\u00b7kn\u00fcpff\u00b7te", "Macht", "dich", "ih\u00b7re", "St\u00e4r\u00b7cke", "zei\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPER", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So wird bey ", "tokens": ["So", "wird", "bey"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Halt deine Krone vest/ sie wackelt ", "tokens": ["Halt", "dei\u00b7ne", "Kro\u00b7ne", "vest", "/", "sie", "wa\u00b7ckelt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "VVFIN", "$(", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Kleinod ist schon fort durch ", "tokens": ["Ein", "Klei\u00b7nod", "ist", "schon", "fort", "durch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKVZ", "APPR"], "meter": "-+-++--", "measure": "unknown.measure.tri"}, "line.7": {"text": "Die beste Lilje hat ", "tokens": ["Die", "bes\u00b7te", "Lil\u00b7je", "hat"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und vor der Teutschen Macht sich nun dein ", "tokens": ["Und", "vor", "der", "Teut\u00b7schen", "Macht", "sich", "nun", "dein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "PRF", "ADV", "PPOSAT"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Es suchte zwar ", "tokens": ["Es", "such\u00b7te", "zwar"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Allein/ was hilfft der Witz wenn man kein Gl\u00fccke hat?", "tokens": ["Al\u00b7lein", "/", "was", "hilfft", "der", "Witz", "wenn", "man", "kein", "Gl\u00fc\u00b7cke", "hat", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "PWS", "VVFIN", "ART", "NN", "KOUS", "PIS", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ob sich viel Dornen gleich um ", "tokens": ["Ob", "sich", "viel", "Dor\u00b7nen", "gleich", "um"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "PIAT", "NN", "ADV", "APPR"], "meter": "++-+--+", "measure": "iambic.tetra.chol"}, "line.12": {"text": "So ist es doch hinweg ohn\u2019 Hand Schuh dir gerissen.", "tokens": ["So", "ist", "es", "doch", "hin\u00b7weg", "ohn'", "Hand", "Schuh", "dir", "ge\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "APPR", "NN", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Der Hand-Schuh ging auch fort als man dir ", "tokens": ["Der", "Han\u00b7dSchuh", "ging", "auch", "fort", "als", "man", "dir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKVZ", "KOKOM", "PIS", "PPER"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.14": {"text": "Da eben die Gewalt des kalten Winters kahm.", "tokens": ["Da", "e\u00b7ben", "die", "Ge\u00b7walt", "des", "kal\u00b7ten", "Win\u00b7ters", "kahm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So wie dir ", "tokens": ["So", "wie", "dir"], "token_info": ["word", "word", "word"], "pos": ["ADV", "KOKOM", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.16": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.17": {"text": "Ja selbst dein ", "tokens": ["Ja", "selbst", "dein"], "token_info": ["word", "word", "word"], "pos": ["PTKANT", "ADV", "PPOSAT"], "meter": "+-+", "measure": "trochaic.di"}, "line.18": {"text": "Wenn man durch Arbeit nimmt dein ", "tokens": ["Wenn", "man", "durch", "Ar\u00b7beit", "nimmt", "dein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "NN", "VVFIN", "PPOSAT"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.19": {"text": "Dem mancher Dornen-Strauch zu seinen Schutze n\u00fctzte/", "tokens": ["Dem", "man\u00b7cher", "Dor\u00b7nen\u00b7Strauch", "zu", "sei\u00b7nen", "Schut\u00b7ze", "n\u00fctz\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und das wie ", "tokens": ["Und", "das", "wie"], "token_info": ["word", "word", "word"], "pos": ["KON", "PDS", "KOKOM"], "meter": "+-+", "measure": "trochaic.di"}, "line.21": {"text": "Soll denn ein Felsen Loch dir eine Frey-Stadt seyn/", "tokens": ["Soll", "denn", "ein", "Fel\u00b7sen", "Loch", "dir", "ei\u00b7ne", "Frey\u00b7Stadt", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "NN", "PPER", "ART", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "So f\u00e4llt ein tapffer Arm auch in die Schantzen ein/", "tokens": ["So", "f\u00e4llt", "ein", "tapf\u00b7fer", "Arm", "auch", "in", "die", "Schant\u00b7zen", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Schl\u00e4gt deine V\u00f6lcker weg/ und mu\u00df das Feld besiegen/", "tokens": ["Schl\u00e4gt", "dei\u00b7ne", "V\u00f6l\u00b7cker", "weg", "/", "und", "mu\u00df", "das", "Feld", "be\u00b7sie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$(", "KON", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und auch zum Sieges-Lohn den Berg und Felsen kriegen.", "tokens": ["Und", "auch", "zum", "Sie\u00b7ges\u00b7Lohn", "den", "Berg", "und", "Fel\u00b7sen", "krie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "ART", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}