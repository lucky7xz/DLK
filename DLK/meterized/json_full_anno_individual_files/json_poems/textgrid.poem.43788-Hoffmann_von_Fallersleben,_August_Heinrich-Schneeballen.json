{"textgrid.poem.43788": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Schneeballen", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist wieder frischer Schnee gefallen,", "tokens": ["Ist", "wie\u00b7der", "fri\u00b7scher", "Schnee", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und l\u00e4\u00dft er sich wieder trefflich ballen,", "tokens": ["Und", "l\u00e4\u00dft", "er", "sich", "wie\u00b7der", "treff\u00b7lich", "bal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dann ziehen wir, wenn die Schule vorbei,", "tokens": ["Dann", "zie\u00b7hen", "wir", ",", "wenn", "die", "Schu\u00b7le", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hinaus mit Trommeln, mit Sang und Juchhei", "tokens": ["Hin\u00b7aus", "mit", "Trom\u00b7meln", ",", "mit", "Sang", "und", "Juch\u00b7hei"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Das Schneefeld wird zum Schlachtfeld dann,", "tokens": ["Das", "Schnee\u00b7feld", "wird", "zum", "Schlacht\u00b7feld", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir k\u00e4mpfen muthig Mann gegen Mann,", "tokens": ["Wir", "k\u00e4mp\u00b7fen", "mut\u00b7hig", "Mann", "ge\u00b7gen", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Wie Freunde wider Feinde streiten,", "tokens": ["Wie", "Freun\u00b7de", "wi\u00b7der", "Fein\u00b7de", "strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und werfen die B\u00e4lle nach allen Seiten.", "tokens": ["Und", "wer\u00b7fen", "die", "B\u00e4l\u00b7le", "nach", "al\u00b7len", "Sei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Wir r\u00fccken vor, wir zieh'n uns zur\u00fcck;", "tokens": ["Wir", "r\u00fc\u00b7cken", "vor", ",", "wir", "zieh'n", "uns", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Es schwankt her\u00fcber, hin\u00fcber das Gl\u00fcck.", "tokens": ["Es", "schwankt", "her\u00b7\u00fc\u00b7ber", ",", "hin\u00b7\u00fc\u00b7ber", "das", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ADV", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und wer zuletzt behauptet das Feld,", "tokens": ["Und", "wer", "zu\u00b7letzt", "be\u00b7haup\u00b7tet", "das", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Der hat gesiegt, der ist der Held!", "tokens": ["Der", "hat", "ge\u00b7siegt", ",", "der", "ist", "der", "Held", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVPP", "$,", "PRELS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ist wieder frischer Schnee gefallen,", "tokens": ["Ist", "wie\u00b7der", "fri\u00b7scher", "Schnee", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und l\u00e4\u00dft er sich wieder trefflich ballen,", "tokens": ["Und", "l\u00e4\u00dft", "er", "sich", "wie\u00b7der", "treff\u00b7lich", "bal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dann ziehen wir, wenn die Schule vorbei,", "tokens": ["Dann", "zie\u00b7hen", "wir", ",", "wenn", "die", "Schu\u00b7le", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hinaus mit Trommeln, mit Sang und Juchhei", "tokens": ["Hin\u00b7aus", "mit", "Trom\u00b7meln", ",", "mit", "Sang", "und", "Juch\u00b7hei"], "token_info": ["word", "word", "word", "punct", "word", "word", "