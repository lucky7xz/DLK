{"textgrid.poem.55906": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Grabmal", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir gedenkens noch. Das ist, als m\u00fc\u00dfte", "tokens": ["Wir", "ge\u00b7den\u00b7kens", "noch", ".", "Das", "ist", ",", "als", "m\u00fc\u00df\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "ADV", "$.", "PDS", "VAFIN", "$,", "KOUS", "VMFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "alles dieses einmal wieder sein.", "tokens": ["al\u00b7les", "die\u00b7ses", "ein\u00b7mal", "wie\u00b7der", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PDAT", "ADV", "ADV", "VAINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wie ein Baum an der Limonenk\u00fcste", "tokens": ["Wie", "ein", "Baum", "an", "der", "Li\u00b7mo\u00b7nen\u00b7k\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "trugst du deine kleinen leichten Br\u00fcste", "tokens": ["trugst", "du", "dei\u00b7ne", "klei\u00b7nen", "leich\u00b7ten", "Br\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "in das Rauschen seines Bluts hinein:", "tokens": ["in", "das", "Rau\u00b7schen", "sei\u00b7nes", "Bluts", "hin\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "\u2013 jenes Gottes.", "tokens": ["\u2013", "je\u00b7nes", "Got\u00b7tes", "."], "token_info": ["punct", "word", "word", "punct"], "pos": ["$(", "PDAT", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Und es war der schlanke", "tokens": ["Und", "es", "war", "der", "schlan\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ART", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Fl\u00fcchtling, der Verw\u00f6hnende der Fraun.", "tokens": ["Fl\u00fccht\u00b7ling", ",", "der", "Ver\u00b7w\u00f6h\u00b7nen\u00b7de", "der", "Fraun", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "S\u00fc\u00df und gl\u00fchend, warm wie dein Gedanke,", "tokens": ["S\u00fc\u00df", "und", "gl\u00fc\u00b7hend", ",", "warm", "wie", "dein", "Ge\u00b7dan\u00b7ke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$,", "ADJD", "KOKOM", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "\u00fcberschattend deine fr\u00fche Flanke", "tokens": ["\u00fc\u00b7bersc\u00b7hat\u00b7tend", "dei\u00b7ne", "fr\u00fc\u00b7he", "Flan\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "und geneigt wie deine Augenbraun.", "tokens": ["und", "ge\u00b7neigt", "wie", "dei\u00b7ne", "Au\u00b7gen\u00b7braun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Wir gedenkens noch. Das ist, als m\u00fc\u00dfte", "tokens": ["Wir", "ge\u00b7den