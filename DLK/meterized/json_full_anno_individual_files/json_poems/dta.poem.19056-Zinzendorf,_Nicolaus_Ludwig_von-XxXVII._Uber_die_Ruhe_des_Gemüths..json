{"dta.poem.19056": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "XxXVII.  Uber die Ruhe des Gem\u00fcths.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Wjewol ist einer solchen Seele!", "tokens": ["Wie\u00b7wol", "ist", "ei\u00b7ner", "sol\u00b7chen", "See\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die JEsum Christum in sich hat!", "tokens": ["Die", "Je\u00b7sum", "Chris\u00b7tum", "in", "sich", "hat", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NE", "APPR", "PRF", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wird gleich die \u00e4u\u00dfre Liebes-H\u00f6hle", "tokens": ["Wird", "gleich", "die", "\u00e4u\u00df\u00b7re", "Lie\u00b7bes\u00b7H\u00f6h\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von mancher Arbeit m\u00fcd und matt;", "tokens": ["Von", "man\u00b7cher", "Ar\u00b7beit", "m\u00fcd", "und", "matt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So steht der Geist doch ungebunden,", "tokens": ["So", "steht", "der", "Geist", "doch", "un\u00b7ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und hat den Quell der Freude funden:", "tokens": ["Und", "hat", "den", "Quell", "der", "Freu\u00b7de", "fun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und zwar die Freud in s\u00fcsser Still;", "tokens": ["Und", "zwar", "die", "Freud", "in", "s\u00fcs\u00b7ser", "Still", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Denn das ist eine schlechte Freude,", "tokens": ["Denn", "das", "ist", "ei\u00b7ne", "schlech\u00b7te", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "So lange man die Seelen-Weyde", "tokens": ["So", "lan\u00b7ge", "man", "die", "See\u00b7len\u00b7Wey\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PIS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "In lauter Unruh suchen will.", "tokens": ["In", "lau\u00b7ter", "Un\u00b7ruh", "su\u00b7chen", "will", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Drum kan kein Menschen-Kind ergr\u00fcnden,", "tokens": ["Drum", "kan", "kein", "Men\u00b7schen\u00b7Kind", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie gut mans erst bey JEsu trift.", "tokens": ["Wie", "gut", "mans", "erst", "bey", "Je\u00b7su", "trift", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PIS", "ADV", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man schleppet sich mit seinen S\u00fcnden,", "tokens": ["Man", "schlep\u00b7pet", "sich", "mit", "sei\u00b7nen", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Man isset \u00fcberzuckert Gift,", "tokens": ["Man", "is\u00b7set", "\u00fc\u00b7ber\u00b7zu\u00b7ckert", "Gift", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "VVPP", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und meynt, man hab es wohl getroffen,", "tokens": ["Und", "meynt", ",", "man", "hab", "es", "wohl", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PIS", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn man sich endlich was erloffen,", "tokens": ["Wenn", "man", "sich", "end\u00b7lich", "was", "er\u00b7lof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "ADV", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Das einer vor ein Gl\u00fcck erkennt.", "tokens": ["Das", "ei\u00b7ner", "vor", "ein", "Gl\u00fcck", "er\u00b7kennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Allein, wer will uns glauben machen,", "tokens": ["Al\u00b7lein", ",", "wer", "will", "uns", "glau\u00b7ben", "ma\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWS", "VMFIN", "PPER", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Da\u00df man auf Erden alle Sachen", "tokens": ["Da\u00df", "man", "auf", "Er\u00b7den", "al\u00b7le", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Bey ihrem rechten Nahmen nennt.", "tokens": ["Bey", "ih\u00b7rem", "rech\u00b7ten", "Nah\u00b7men", "nennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Das wei\u00df ich wol, wenn ein Studente,", "tokens": ["Das", "wei\u00df", "ich", "wol", ",", "wenn", "ein", "Stu\u00b7den\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der im Register noch zur\u00fcck,", "tokens": ["Der", "im", "Re\u00b7gis\u00b7ter", "noch", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein gutes Amt errennen k\u00f6nte,", "tokens": ["Ein", "gu\u00b7tes", "Amt", "er\u00b7ren\u00b7nen", "k\u00f6n\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das hielt er vor ein grosses Gl\u00fcck.", "tokens": ["Das", "hielt", "er", "vor", "ein", "gros\u00b7ses", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch ist die Weyhe kaum empfangen,", "tokens": ["Doch", "ist", "die", "Wey\u00b7he", "kaum", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Wirthschaft folglich angefangen,", "tokens": ["Die", "Wirth\u00b7schaft", "folg\u00b7lich", "an\u00b7ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Das erste Amts-Jahr bald vorbey;", "tokens": ["Das", "ers\u00b7te", "Amts\u00b7Jahr", "bald", "vor\u00b7bey", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So kan ein zeitlichs Lamentiren,", "tokens": ["So", "kan", "ein", "zeit\u00b7lichs", "La\u00b7men\u00b7ti\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die Expectanten \u00fcberf\u00fchren,", "tokens": ["Die", "Ex\u00b7pect\u00b7an\u00b7ten", "\u00fc\u00b7berf\u00b7\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Da\u00df dieses Gl\u00fcck verg\u00e4nglich sey.", "tokens": ["Da\u00df", "die\u00b7ses", "Gl\u00fcck", "ver\u00b7g\u00e4ng\u00b7lich", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Vom Lehr-Amt auf den Stand zu kommen,", "tokens": ["Vom", "Lehr\u00b7Amt", "auf", "den", "Stand", "zu", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der das Regierungs-Ruder f\u00fchrt:", "tokens": ["Der", "das", "Re\u00b7gie\u00b7rungs\u00b7Ru\u00b7der", "f\u00fchrt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was wird nicht oftmals vorgenommen,", "tokens": ["Was", "wird", "nicht", "oft\u00b7mals", "vor\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Damit man auch einmal regiert.", "tokens": ["Da\u00b7mit", "man", "auch", "ein\u00b7mal", "re\u00b7giert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Warum? Es ist ein Gl\u00fcck zu nennen.", "tokens": ["Wa\u00b7rum", "?", "Es", "ist", "ein", "Gl\u00fcck", "zu", "nen\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PPER", "VAFIN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wo wenige das Ziel errennen.", "tokens": ["Wo", "we\u00b7ni\u00b7ge", "das", "Ziel", "er\u00b7ren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Trift einer nur zum Ziele zu;", "tokens": ["Trift", "ei\u00b7ner", "nur", "zum", "Zie\u00b7le", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So sucht er etwas anzukauffen,", "tokens": ["So", "sucht", "er", "et\u00b7was", "an\u00b7zu\u00b7kauf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Daselbst zuweilen zu verschnauffen,", "tokens": ["Da\u00b7selbst", "zu\u00b7wei\u00b7len", "zu", "ver\u00b7schnauf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und setzt sich endlich gar zur Ruh.", "tokens": ["Und", "setzt", "sich", "end\u00b7lich", "gar", "zur", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der N\u00e4hr-Stand hat das Recht bekommen,", "tokens": ["Der", "N\u00e4hr\u00b7Stand", "hat", "das", "Recht", "be\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den nennt man eigentlich ein Gl\u00fcck,", "tokens": ["Den", "nennt", "man", "ei\u00b7gent\u00b7lich", "ein", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIS", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wann eine einen Mann genommen.", "tokens": ["Wann", "ei\u00b7ne", "ei\u00b7nen", "Mann", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Allein man rechne nur zur\u00fcck;", "tokens": ["Al\u00b7lein", "man", "rech\u00b7ne", "nur", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wo ist wol eine unter allen,", "tokens": ["Wo", "ist", "wol", "ei\u00b7ne", "un\u00b7ter", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "ART", "APPR", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die in das Ehe-Netz gefallen,", "tokens": ["Die", "in", "das", "E\u00b7he\u00b7Netz", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Darein so manche Hofnung kirrt,", "tokens": ["Da\u00b7rein", "so", "man\u00b7che", "Hof\u00b7nung", "kirrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Von ihrem Gl\u00fccke je beth\u00f6ret,", "tokens": ["Von", "ih\u00b7rem", "Gl\u00fc\u00b7cke", "je", "be\u00b7th\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die, wenn man sie frey sprechen h\u00f6ret,", "tokens": ["Die", ",", "wenn", "man", "sie", "frey", "spre\u00b7chen", "h\u00f6\u00b7ret", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PIS", "PPER", "ADJD", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Nicht andre Leute warnen wird?", "tokens": ["Nicht", "and\u00b7re", "Leu\u00b7te", "war\u00b7nen", "wird", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wie kommts denn, da\u00df man Leute siehet,", "tokens": ["Wie", "kommts", "denn", ",", "da\u00df", "man", "Leu\u00b7te", "sie\u00b7het", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "$,", "KOUS", "PIS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-++", "measure": "unknown.measure.penta"}, "line.2": {"text": "Nur, da\u00df man sie gar selten findt,", "tokens": ["Nur", ",", "da\u00df", "man", "sie", "gar", "sel\u00b7ten", "findt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PIS", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die weder sich ums Amt bem\u00fchet,", "tokens": ["Die", "we\u00b7der", "sich", "ums", "Amt", "be\u00b7m\u00fc\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KON", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch, wenn sies haben, schw\u00fcrig sind?", "tokens": ["Noch", ",", "wenn", "sies", "ha\u00b7ben", ",", "schw\u00fc\u00b7rig", "sind", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "VAFIN", "$,", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das macht, weil sie im Lehrer-Orden", "tokens": ["Das", "macht", ",", "weil", "sie", "im", "Leh\u00b7rer\u00b7Or\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "KOUS", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht erstlich JEsu J\u00fcnger worden,", "tokens": ["Nicht", "erst\u00b7lich", "Je\u00b7su", "J\u00fcn\u00b7ger", "wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "NE", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und nun von ihm geruffen seyn,", "tokens": ["Und", "nun", "von", "ihm", "ge\u00b7ruf\u00b7fen", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPER", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ist ihnen wenig dran gelegen,", "tokens": ["Ist", "ih\u00b7nen", "we\u00b7nig", "dran", "ge\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIS", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Wie starck der Beicht- und Decem-Segen,", "tokens": ["Wie", "starck", "der", "Beicht", "und", "De\u00b7cem\u00b7Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Sie sammlen sich nur Seelen ein.", "tokens": ["Sie", "samm\u00b7len", "sich", "nur", "See\u00b7len", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "sta