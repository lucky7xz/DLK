{"dta.poem.19076": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "LvII.   An einen wanckelm\u00fcthigen Sch\u00fcler  \n der Gnade.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn, vielgeliebter Freund! dein Diener wissen k\u00f6nt',", "tokens": ["Wenn", ",", "viel\u00b7ge\u00b7lieb\u00b7ter", "Freund", "!", "dein", "Die\u00b7ner", "wis\u00b7sen", "k\u00f6nt'", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "ADJA", "NN", "$.", "PPOSAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie lange dir der HErr noch Lebens-Zeit verg\u00f6nnt\u2019:", "tokens": ["Wie", "lan\u00b7ge", "dir", "der", "Herr", "noch", "Le\u00b7bens\u00b7Zeit", "ver\u00b7g\u00f6nnt'", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "ART", "NN", "ADV", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So k\u00f6nt er eher noch mit gleichen Augen sehen,", "tokens": ["So", "k\u00f6nt", "er", "e\u00b7her", "noch", "mit", "glei\u00b7chen", "Au\u00b7gen", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie wenig bi\u00df daher zu deinem Heyl geschehen.", "tokens": ["Wie", "we\u00b7nig", "bi\u00df", "da\u00b7her", "zu", "dei\u00b7nem", "Heyl", "ge\u00b7sche\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "PAV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Weil aber, der die H\u00f6ll und Tod in H\u00e4nden h\u00e4lt,", "tokens": ["Weil", "a\u00b7ber", ",", "der", "die", "H\u00f6ll", "und", "Tod", "in", "H\u00e4n\u00b7den", "h\u00e4lt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "$,", "PRELS", "ART", "NN", "KON", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Uns eine kurtze Frist zur Gnaden-Zeit bestellt,", "tokens": ["Uns", "ei\u00b7ne", "kurt\u00b7ze", "Frist", "zur", "Gna\u00b7den\u00b7Zeit", "be\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und will, da\u00df unser Geist nach seinem Reiche ringe:", "tokens": ["Und", "will", ",", "da\u00df", "un\u00b7ser", "Geist", "nach", "sei\u00b7nem", "Rei\u00b7che", "rin\u00b7ge", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "$,", "KOUS", "PPOSAT", "NN", "APPR", "PPOSAT", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So halte mir zu gut, da\u00df ich auf Eifer dringe.", "tokens": ["So", "hal\u00b7te", "mir", "zu", "gut", ",", "da\u00df", "ich", "auf", "Ei\u00b7fer", "drin\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKA", "ADJD", "$,", "KOUS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du hast das ewige unwandelbare Wort,", "tokens": ["Du", "hast", "das", "e\u00b7wi\u00b7ge", "un\u00b7wan\u00b7del\u00b7ba\u00b7re", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Das weiset schlechterdings auf eine enge Pfort:", "tokens": ["Das", "wei\u00b7set", "schlech\u00b7ter\u00b7dings", "auf", "ei\u00b7ne", "en\u00b7ge", "Pfort", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Das redet \u00fcberall von einem schmalen Wege,", "tokens": ["Das", "re\u00b7det", "\u00fc\u00b7be\u00b7rall", "von", "ei\u00b7nem", "schma\u00b7len", "We\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und da\u00df man seines Heyls mit Furcht und Zittern pflege.", "tokens": ["Und", "da\u00df", "man", "sei\u00b7nes", "Heyls", "mit", "Furcht", "und", "Zit\u00b7tern", "pfle\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die Menschen hindern uns! allein, wo ist der Mann?", "tokens": ["Die", "Men\u00b7schen", "hin\u00b7dern", "uns", "!", "al\u00b7lein", ",", "wo", "ist", "der", "Mann", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$.", "ADV", "$,", "PWAV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der unsern armen Geist dereinst vertreten kan:", "tokens": ["Der", "un\u00b7sern", "ar\u00b7men", "Geist", "de\u00b7reinst", "ver\u00b7tre\u00b7ten", "kan", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wenn JEsus auf dem Stuhl nach unsern Ernste fragen,", "tokens": ["Wenn", "Je\u00b7sus", "auf", "dem", "Stuhl", "nach", "un\u00b7sern", "Erns\u00b7te", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und unser eigen Hertz nichts wissen wird zu sagen?", "tokens": ["Und", "un\u00b7ser", "ei\u00b7gen", "Hertz", "nichts", "wis\u00b7sen", "wird", "zu", "sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJD", "NN", "PIS", "VVINF", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Gelobet sey der HErr, der dich ger\u00fchret hat!", "tokens": ["Ge\u00b7lo\u00b7bet", "sey", "der", "Herr", ",", "der", "dich", "ge\u00b7r\u00fch\u00b7ret", "hat", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Er gebe keiner Ruh in deiner Seele statt,", "tokens": ["Er", "ge\u00b7be", "kei\u00b7ner", "Ruh", "in", "dei\u00b7ner", "See\u00b7le", "statt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Bi\u00df du das richtige und grosse Werck vollendet,", "tokens": ["Bi\u00df", "du", "das", "rich\u00b7ti\u00b7ge", "und", "gros\u00b7se", "Werck", "voll\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "KON", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Um welches willen er sich selbst bey GOtt verpf\u00e4ndet.", "tokens": ["Um", "wel\u00b7ches", "wil\u00b7len", "er", "sich", "selbst", "bey", "Gott", "ver\u00b7pf\u00e4n\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PWAT", "NN", "PPER", "PRF", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wohl dem, der diese Welt vor lauter Spiel-Zeug h\u00e4lt.", "tokens": ["Wohl", "dem", ",", "der", "die\u00b7se", "Welt", "vor", "lau\u00b7ter", "Spiel\u00b7Zeug", "h\u00e4lt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$,", "PRELS", "PDAT", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Allein, wer kan doch das? wer \u00fcberwindt die Welt?", "tokens": ["Al\u00b7lein", ",", "wer", "kan", "doch", "das", "?", "wer", "\u00fc\u00b7berw\u00b7indt", "die", "Welt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWS", "VMFIN", "ADV", "PDS", "$.", "PWS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Nur einer, welchen GOtt zu Christo hin gezogen,", "tokens": ["Nur", "ei\u00b7ner", ",", "wel\u00b7chen", "Gott", "zu", "Chris\u00b7to", "hin", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "$,", "PWAT", "NN", "APPR", "NE", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und der sein theures Heyl mit Redlichkeit erwogen.", "tokens": ["Und", "der", "sein", "theu\u00b7res", "Heyl", "mit", "Red\u00b7lich\u00b7keit", "er\u00b7wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPOSAT", "ADJA", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Derselbe wirfft sich bald vor JEsu F\u00fcsse hin,", "tokens": ["Der\u00b7sel\u00b7be", "wirfft", "sich", "bald", "vor", "Je\u00b7su", "F\u00fcs\u00b7se", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADV", "APPR", "NE", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und spricht: ", "tokens": ["Und", "spricht", ":"], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.27": {"text": "Ach! mache mich doch recht zu deinem Unterthanen,", "tokens": ["Ach", "!", "ma\u00b7che", "mich", "doch", "recht", "zu", "dei\u00b7nem", "Un\u00b7ter\u00b7tha\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und solt'st du mir den Weg durch Dorn und Hecken bahnen.", "tokens": ["Und", "solt'st", "du", "mir", "den", "Weg", "durch", "Dorn", "und", "He\u00b7cken", "bah\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Der Vorsatz aber mu\u00df nicht ohne Nachsatz seyn:", "tokens": ["Der", "Vor\u00b7satz", "a\u00b7ber", "mu\u00df", "nicht", "oh\u00b7ne", "Nach\u00b7satz", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VMFIN", "PTKNEG", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Es treffe Wort und Sinn recht redlich \u00fcberein;", "tokens": ["Es", "tref\u00b7fe", "Wort", "und", "Sinn", "recht", "red\u00b7lich", "\u00fc\u00b7be\u00b7re\u00b7in", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "ADJD", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.31": {"text": "So wird man allererst die Kraft des HErrn empfinden,", "tokens": ["So", "wird", "man", "al\u00b7le\u00b7rerst", "die", "Kraft", "des", "Herrn", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Da stutzt der Seelen-Feind und das Gesetz der S\u00fcnden.", "tokens": ["Da", "stutzt", "der", "See\u00b7len\u00b7Feind", "und", "das", "Ge\u00b7setz", "der", "S\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Die Menschen dieser Welt sind nur ein blo\u00df Gespenst,", "tokens": ["Die", "Men\u00b7schen", "die\u00b7ser", "Welt", "sind", "nur", "ein", "blo\u00df", "Ge\u00b7spenst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "VAFIN", "ADV", "ART", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Wenn du sie, werther Freund! nach ihren Wesen kennst;", "tokens": ["Wenn", "du", "sie", ",", "wert\u00b7her", "Freund", "!", "nach", "ih\u00b7ren", "We\u00b7sen", "kennst", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "$,", "ADJA", "NN", "$.", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "So h\u00e4ltst du sie gewi\u00df vor ungl\u00fcckselge Narren,", "tokens": ["So", "h\u00e4ltst", "du", "sie", "ge\u00b7wi\u00df", "vor", "un\u00b7gl\u00fcck\u00b7sel\u00b7ge", "Nar\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Mit allen heuchelnden und Lohn-begiergen Pfarren.", "tokens": ["Mit", "al\u00b7len", "heu\u00b7cheln\u00b7den", "und", "Lohn\u00b7be\u00b7gier\u00b7gen", "Pfar\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "KON", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Indessen weil du selbst von N. N. angezeigt,", "tokens": ["In\u00b7des\u00b7sen", "weil", "du", "selbst", "von", "N.", "N.", "an\u00b7ge\u00b7zeigt", ","], "token_info": ["word", "word", "word", "word", "word", "abbreviation", "abbreviation", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "APPR", "NE", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Da\u00df er sein Hertze GOtt und JEsu zugeneigt:", "tokens": ["Da\u00df", "er", "sein", "Hert\u00b7ze", "Gott", "und", "Je\u00b7su", "zu\u00b7gen\u00b7eigt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAINF", "VVFIN", "NN", "KON", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "So wolle neben ihm ja keine Zeit vers\u00e4umen,", "tokens": ["So", "wol\u00b7le", "ne\u00b7ben", "ihm", "ja", "kei\u00b7ne", "Zeit", "ver\u00b7s\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "APPR", "PPER", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Dich eben so, wie er, dem Heyland einzur\u00e4umen.", "tokens": ["Dich", "e\u00b7ben", "so", ",", "wie", "er", ",", "dem", "Hey\u00b7land", "ein\u00b7zu\u00b7r\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "$,", "PWAV", "PPER", "$,", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Die Br\u00fcder dieses Orts sind allemal erfreut,", "tokens": ["Die", "Br\u00fc\u00b7der", "die\u00b7ses", "Orts", "sind", "al\u00b7le\u00b7mal", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "VAFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure"