punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "KOUI", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "aber du \u2013 du konntest es sagen.", "tokens": ["a\u00b7ber", "du", "\u2013", "du", "konn\u00b7test", "es", "sa\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$(", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Kanntest die B\u00fccher und die Brosch\u00fcren,", "tokens": ["Kann\u00b7test", "die", "B\u00fc\u00b7cher", "und", "die", "Bro\u00b7sch\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "wu\u00dftest besser die Feder zu f\u00fchren.", "tokens": ["wu\u00df\u00b7test", "bes\u00b7ser", "die", "Fe\u00b7der", "zu", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Treue um Treue \u2013 wir glaubten dir doch!", "tokens": ["Treu\u00b7e", "um", "Treu\u00b7e", "\u2013", "wir", "glaub\u00b7ten", "dir", "doch", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$(", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.8": {"text": "Genosse, erinnerst du dich noch?", "tokens": ["Ge\u00b7nos\u00b7se", ",", "e\u00b7rin\u00b7nerst", "du", "dich", "noch", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "PRF", "ADV", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Heute