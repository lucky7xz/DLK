{"textgrid.poem.53721": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "An einen Bonzen", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einmal waren wir beide gleich.", "tokens": ["Ein\u00b7mal", "wa\u00b7ren", "wir", "bei\u00b7de", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Beide: Proleten im deutschen Kaiserreich.", "tokens": ["Bei\u00b7de", ":", "Pro\u00b7le\u00b7ten", "im", "deut\u00b7schen", "Kai\u00b7ser\u00b7reich", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$.", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Beide in derselben Luft,", "tokens": ["Bei\u00b7de", "in", "der\u00b7sel\u00b7ben", "Luft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "beide in gleicher verschwitzter Kluft;", "tokens": ["bei\u00b7de", "in", "glei\u00b7cher", "ver\u00b7schwitz\u00b7ter", "Kluft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ADJA", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "dieselbe Werkstatt \u2013 derselbe Lohn \u2013", "tokens": ["die\u00b7sel\u00b7be", "Werk\u00b7statt", "\u2013", "der\u00b7sel\u00b7be", "Lohn", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$(", "PDAT", "NN", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "derselbe Meister \u2013 dieselbe Fron \u2013", "tokens": ["der\u00b7sel\u00b7be", 