{"textgrid.poem.32490": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "Auf ein Karussell", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freund, gestern war ich \u2013 wo? \u2013 Wo alle Menschen waren.", "tokens": ["Freund", ",", "ge\u00b7stern", "war", "ich", "\u2013", "wo", "?", "\u2013", "Wo", "al\u00b7le", "Men\u00b7schen", "wa\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VAFIN", "PPER", "$(", "PWAV", "$.", "$(", "PWAV", "PIAT", "NN", "VAFIN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Da sah ich f\u00fcr mein bares Geld", "tokens": ["Da", "sah", "ich", "f\u00fcr", "mein", "ba\u00b7res", "Geld"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "So manchen Prinz, so manchen Held,", "tokens": ["So", "man\u00b7chen", "Prinz", ",", "so", "man\u00b7chen", "Held", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach Opernart geputzt, als F\u00fchrer fremder Scharen,", "tokens": ["Nach", "O\u00b7per\u00b7nart", "ge\u00b7putzt", ",", "als", "F\u00fch\u00b7rer", "frem\u00b7der", "Scha\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "KOUS", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da sah ich manche flinke Speere", "tokens": ["Da", "sah", "ich", "man\u00b7che", "flin\u00b7ke", "Spee\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Auf mancher zugerittnen M\u00e4hre", "tokens": ["Auf", "man\u00b7cher", "zu\u00b7ge\u00b7ritt\u00b7nen", "M\u00e4h\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Durch eben nicht den kleinsten Ring,", "tokens": ["Durch", "e\u00b7ben", "nicht", "den", "kleins\u00b7ten", "Ring", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der unter tausend Sonnen hing,", "tokens": ["Der", "un\u00b7ter", "tau\u00b7send", "Son\u00b7nen", "hing", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "(o Schade, da\u00df es Lampen waren!)", "tokens": ["(", "o", "Scha\u00b7de", ",", "da\u00df", "es", "Lam\u00b7pen", "wa\u00b7ren", "!", ")"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "ADJD", "$,", "KOUS", "PPER", "NN", "VAFIN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Oft, sag ich, durch den Ring", "tokens": ["Oft", ",", "sag", "ich", ",", "durch", "den", "Ring"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "VVFIN", "PPER", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Und \u00f6fter noch darneben fahren.", "tokens": ["Und", "\u00f6f\u00b7ter", "noch", "dar\u00b7ne\u00b7ben", "fah\u00b7ren", "."], "toke