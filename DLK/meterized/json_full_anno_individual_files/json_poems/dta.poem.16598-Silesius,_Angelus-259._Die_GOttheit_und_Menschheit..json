{"dta.poem.16598": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "259.  Die GOttheit und Menschheit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Die Ewge GOttheit ist der Menschheit so verpflicht!", "tokens": ["Die", "Ew\u00b7ge", "Got\u00b7theit", "ist", "der", "Menschheit", "so", "ver\u00b7pflicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Da\u00df Jhr auch ohne sie Hertz/ Muth und Sinn gebricht.", "tokens": ["Da\u00df", "Ihr", "auch", "oh\u00b7ne", "sie", "Hertz", "/", "Muth", "und", "Sinn", "ge\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPER", "NN", "$(", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}