{"dta.poem.21167": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "An das Schiff ,  \n welches Klopstocken nach D\u00e4nnemark f\u00fchrte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O! der g\u00fcnstigste Wind schwelle dein Seegel auf,", "tokens": ["O", "!", "der", "g\u00fcns\u00b7tigs\u00b7te", "Wind", "schwel\u00b7le", "dein", "See\u00b7gel", "auf", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Leichtes Fahrzeug, das ietzt \u00fcber die Wogen hin", "tokens": ["Leich\u00b7tes", "Fahr\u00b7zeug", ",", "das", "ietzt", "\u00fc\u00b7ber", "die", "Wo\u00b7gen", "hin"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "ADV", "APPR", "ART", "NN", "PTKVZ"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Mit dem Dichter und Freund, jeder Bewundrung werth,", "tokens": ["Mit", "dem", "Dich\u00b7ter", "und", "Freund", ",", "je\u00b7der", "Be\u00b7wund\u00b7rung", "werth", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "$,", "PIAT", "NN", "ADJD", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Zu den d\u00e4nischen Ufern fliegt.", "tokens": ["Zu", "den", "d\u00e4\u00b7ni\u00b7schen", "U\u00b7fern", "fliegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Leuchte, silberner Mond, in der gestirnten Nacht", "tokens": ["Leuch\u00b7te", ",", "sil\u00b7ber\u00b7ner", "Mond", ",", "in", "der", "ge\u00b7stirn\u00b7ten", "Nacht"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Seinem einsamen Pfad, \u00fcber die stille Fluth!", "tokens": ["Sei\u00b7nem", "ein\u00b7sa\u00b7men", "Pfad", ",", "\u00fc\u00b7ber", "die", "stil\u00b7le", "Fluth", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und du sch\u00fctzender Geist, ihm vom Olympus geschickt,", "tokens": ["Und", "du", "sch\u00fct\u00b7zen\u00b7der", "Geist", ",", "ihm", "vom", "O\u00b7lym\u00b7pus", "ge\u00b7schickt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJA", "NN", "$,", "PPER", "APPRART", "NE", "VVPP", "$,"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Bring ihn sicher ans treue Land!", "tokens": ["Bring", "ihn", "si\u00b7cher", "ans", "treu\u00b7e", "Land", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPRART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}