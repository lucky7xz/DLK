{"dta.poem.13566": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Das Bilderbuch .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Von der Poesie sucht Kunde", "tokens": ["Von", "der", "Poe\u00b7sie", "sucht", "Kun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Mancher im gelehrten Buch,", "tokens": ["Man\u00b7cher", "im", "ge\u00b7lehr\u00b7ten", "Buch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur des Lebens sch\u00f6ne Runde", "tokens": ["Nur", "des", "Le\u00b7bens", "sch\u00f6\u00b7ne", "Run\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lehret Dich den Zauberspruch,", "tokens": ["Leh\u00b7ret", "Dich", "den", "Zau\u00b7ber\u00b7spruch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch in stillgeweihter Stunde", "tokens": ["Doch", "in", "still\u00b7ge\u00b7weih\u00b7ter", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Will das Buch erschlossen sein;", "tokens": ["Will", "das", "Buch", "er\u00b7schlos\u00b7sen", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und so blick' ich heut hinein,", "tokens": ["Und", "so", "blick'", "ich", "heut", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie ein Kind im Fr\u00fchlingswetter", "tokens": ["Wie", "ein", "Kind", "im", "Fr\u00fch\u00b7lings\u00b7wet\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Fr\u00f6hlich Bilderb\u00fccher bl\u00e4ttert,", "tokens": ["Fr\u00f6h\u00b7lich", "Bil\u00b7der\u00b7b\u00fc\u00b7cher", "bl\u00e4t\u00b7tert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und es schweift der Sonnenschein", "tokens": ["Und", "es", "schweift", "der", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Auf den buntgemalten Lettern,", "tokens": ["Auf", "den", "bunt\u00b7ge\u00b7mal\u00b7ten", "Let\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Und gelinde weht der Wind", "tokens": ["Und", "ge\u00b7lin\u00b7de", "weht", "der", "Wind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Durch die Blumen, durch das Herz", "tokens": ["Durch", "die", "Blu\u00b7men", ",", "durch", "das", "Herz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Alte Freuden, alten Schmerz \u2014", "tokens": ["Al\u00b7te", "Freu\u00b7den", ",", "al\u00b7ten", "Schmerz"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Weinen m\u00f6cht' ich, wie ein Kind!", "tokens": ["Wei\u00b7nen", "m\u00f6cht'", "ich", ",", "wie", "ein", "Kind", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "$,", "PWAV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}