{"dta.poem.167": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er kan sich in ihre Weise nicht finden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df Rubelle mich mit dir noch nicht zu stallen/", "tokens": ["Ich", "wei\u00df", "Ru\u00b7bel\u00b7le", "mich", "mit", "dir", "noch", "nicht", "zu", "stal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "PRF", "APPR", "PPER", "ADV", "PTKNEG", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich kenne dich nicht recht. Bald bist du mir zu gut/", "tokens": ["Ich", "ken\u00b7ne", "dich", "nicht", "recht", ".", "Bald", "bist", "du", "mir", "zu", "gut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADJD", "$.", "ADV", "VAFIN", "PPER", "PPER", "PTKA", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bald wenn ein falscher Wahn beth\u00f6ret deinen Muth.", "tokens": ["Bald", "wenn", "ein", "fal\u00b7scher", "Wahn", "be\u00b7th\u00f6\u00b7ret", "dei\u00b7nen", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So mu\u00df ich aus dem Schoo\u00df des sch\u00f6nsten Gl\u00fcckes fallen.", "tokens": ["So", "mu\u00df", "ich", "aus", "dem", "Schoo\u00df", "des", "sch\u00f6ns\u00b7ten", "Gl\u00fc\u00b7ckes", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du spielest so mit mir als wie mit einem Ballen/", "tokens": ["Du", "spie\u00b7lest", "so", "mit", "mir", "als", "wie", "mit", "ei\u00b7nem", "Bal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "KOUS", "KOKOM", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sag ob mein Jammer dir vielleichte sanffte thut?", "tokens": ["Sag", "ob", "mein", "Jam\u00b7mer", "dir", "viel\u00b7leich\u00b7te", "sanff\u00b7te", "thut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PPOSAT", "NN", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Schmertzt oder dich die Lust/ die kitzelt Seel und Blut?", "tokens": ["Schmertzt", "o\u00b7der", "dich", "die", "Lust", "/", "die", "kit\u00b7zelt", "Seel", "und", "Blut", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "PPER", "ART", "NN", "$(", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Er\u00f6ffne es mein Kind/ so halt ich Maa\u00df in allen.", "tokens": ["Er\u00b7\u00f6ff\u00b7ne", "es", "mein", "Kind", "/", "so", "halt", "ich", "Maa\u00df", "in", "al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$(", "ADV", "VVFIN", "PPER", "NN", "APPR", "PIAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wie? wenn ich vorwerths geh/ so eilest du zur\u00fccke/", "tokens": ["Wie", "?", "wenn", "ich", "vor\u00b7werths", "geh", "/", "so", "ei\u00b7lest", "du", "zu\u00b7r\u00fc\u00b7cke", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "KOUS", "PPER", "ADV", "VVFIN", "$(", "ADV", "VVFIN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wenn ich dich froh anschau so krieg ich Donner-Blicke/", "tokens": ["Wenn", "ich", "dich", "froh", "an\u00b7schau", "so", "krieg", "ich", "Don\u00b7ner\u00b7Bli\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVFIN", "ADV", "VVFIN", "PPER", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wenn ich nun traurig bin so lachest du mich an/", "tokens": ["Wenn", "ich", "nun", "trau\u00b7rig", "bin", "so", "la\u00b7chest", "du", "mich", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "ADV", "VVFIN", "PPER", "PRF", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wenn ich dich lieben will/ so kennst du nichts als Hassen/", "tokens": ["Wenn", "ich", "dich", "lie\u00b7ben", "will", "/", "so", "kennst", "du", "nichts", "als", "Has\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "VMFIN", "$(", "ADV", "VVFIN", "PPER", "PIS", "KOKOM", "NE", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So da\u00df mein treuer Sinn dich nicht bezwingen kan/", "tokens": ["So", "da\u00df", "mein", "treu\u00b7er", "Sinn", "dich", "nicht", "be\u00b7zwin\u00b7gen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPOSAT", "ADJA", "NN", "PPER", "PTKNEG", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und fall ich von dir ab/ so wilt du mich nicht lassen.", "tokens": ["Und", "fall", "ich", "von", "dir", "ab", "/", "so", "wilt", "du", "mich", "nicht", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "APPR", "PPER", "PTKVZ", "$(", "ADV", "VMFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}