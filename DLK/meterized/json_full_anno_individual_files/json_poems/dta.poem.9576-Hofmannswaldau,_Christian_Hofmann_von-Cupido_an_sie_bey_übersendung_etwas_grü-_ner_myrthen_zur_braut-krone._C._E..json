{"dta.poem.9576": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Cupido an sie/ bey \u00fcbersendung etwas gr\u00fc-  \n ner myrthen zur braut-krone.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dj\u00df frische myrthen-laub/ das ewig pflegt zu gr\u00fcnen/", "tokens": ["Dj\u00df", "fri\u00b7sche", "myr\u00b7then\u00b7laub", "/", "das", "e\u00b7wig", "pflegt", "zu", "gr\u00fc\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$(", "ART", "ADJD", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Womit die Venus gieng h\u00f6chst-pr\u00e4chtig aufgeschm\u00fcckt/", "tokens": ["Wo\u00b7mit", "die", "Ve\u00b7nus", "gieng", "h\u00f6chst\u00b7pr\u00e4ch\u00b7tig", "auf\u00b7ge\u00b7schm\u00fcckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als sie zum g\u00f6tter-fest sich neulichst angeschickt/", "tokens": ["Als", "sie", "zum", "g\u00f6t\u00b7ter\u00b7fest", "sich", "neu\u00b7lichst", "an\u00b7ge\u00b7schickt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PRF", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Will ich dir/ sch\u00f6nsten/ hier zu senden mich erk\u00fchnen.", "tokens": ["Will", "ich", "dir", "/", "sch\u00f6ns\u00b7ten", "/", "hier", "zu", "sen\u00b7den", "mich", "er\u00b7k\u00fch\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "$(", "VVFIN", "$(", "ADV", "PTKZU", "VVINF", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Denn da du mir so sch\u00f6n/ wie Venus selbst/ geschienen/", "tokens": ["Denn", "da", "du", "mir", "so", "sch\u00f6n", "/", "wie", "Ve\u00b7nus", "selbst", "/", "ge\u00b7schie\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "ADV", "ADJD", "$(", "KOKOM", "NN", "ADV", "$(", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Als ich vor kurtzem noch dein helles aug\u2019 erblickt/", "tokens": ["Als", "ich", "vor", "kurt\u00b7zem", "noch", "dein", "hel\u00b7les", "aug'", "er\u00b7blickt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "ADV", "PPOSAT", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Das durch sein freundlich seyn fast alle welt entz\u00fcckt;", "tokens": ["Das", "durch", "sein", "freund\u00b7lich", "seyn", "fast", "al\u00b7le", "welt", "ent\u00b7z\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPOSAT", "ADJD", "VAINF", "ADV", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So mu\u00df ihr schmuck mit recht auch dir zum schmucke dienen.", "tokens": ["So", "mu\u00df", "ihr", "schmuck", "mit", "recht", "auch", "dir", "zum", "schmu\u00b7cke", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "APPR", "ADV", "ADV", "PPER", "APPRART", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du aber/ heil\u2019ges laub/ das solch ein haupt bekr\u00f6nt/", "tokens": ["Du", "a\u00b7ber", "/", "heil'\u00b7ges", "laub", "/", "das", "solch", "ein", "haupt", "be\u00b7kr\u00f6nt", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$(", "ADJA", "NN", "$(", "ART", "PIAT", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Von dem du allen glantz und alle pracht entlehnt/", "tokens": ["Von", "dem", "du", "al\u00b7len", "glantz", "und", "al\u00b7le", "pracht", "ent\u00b7lehnt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PIAT", "NN", "KON", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Kein gold noch demant ist dir ferner zu vergleichen;", "tokens": ["Kein", "gold", "noch", "de\u00b7mant", "ist", "dir", "fer\u00b7ner", "zu", "ver\u00b7glei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "ADJD", "VAFIN", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.12": {"text": "Wie gl\u00fccklich bist du nicht in deinem gr\u00fcnen schein?", "tokens": ["Wie", "gl\u00fcck\u00b7lich", "bist", "du", "nicht", "in", "dei\u00b7nem", "gr\u00fc\u00b7nen", "schein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Von nun an wird dein stamm den cedern selbst nicht weichen/", "tokens": ["Von", "nun", "an", "wird", "dein", "stamm", "den", "ce\u00b7dern", "selbst", "nicht", "wei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APZR", "VAFIN", "PPOSAT", "VVFIN", "ART", "NN", "ADV", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und eine k\u00f6nigin der sch\u00f6nsten b\u00e4ume seyn.", "tokens": ["Und", "ei\u00b7ne", "k\u00f6\u00b7ni\u00b7gin", "der", "sch\u00f6ns\u00b7ten", "b\u00e4u\u00b7me", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}