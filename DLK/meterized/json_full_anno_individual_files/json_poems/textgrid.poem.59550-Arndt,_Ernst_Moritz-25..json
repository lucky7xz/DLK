ure": "iambic.tri"}, "line.7": {"text": "Baun weite Himmelsbr\u00fccken", "tokens": ["Baun", "wei\u00b7te", "Him\u00b7mels\u00b7br\u00fc\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Aus jedem schmalsten Steg.", "tokens": ["Aus", "je\u00b7dem", "schmals\u00b7ten", "Steg", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Verwaiset sind die Kinder", "tokens": ["Ver\u00b7wai\u00b7set", "sind", "die", "Kin\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nicht mehr und vaterlos,", "tokens": ["Nicht", "mehr", "und", "va\u00b7ter\u00b7los", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gott rufet selbst die S\u00fcnder", "tokens": ["Gott", "ru\u00b7fet", "selbst", "die", "S\u00fcn\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In seinen Gnadenscho\u00df,", "tokens": ["In", "sei\u00b7nen", "Gna\u00b7den\u00b7scho\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er will, da\u00df alle, rein", "tokens": ["Er", "will", ",", "da\u00df", "al\u00b7le", ",", "rein"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "PIS", "$,", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Von ihren alten Schulden,", "tokens": ["Von", "ih\u00b7ren", "al\u00b7ten", "Schul\u00b7den", ","]