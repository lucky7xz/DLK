{"textgrid.poem.59550": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "25.", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Heil'ge Christ ist kommen,", "tokens": ["Der", "Heil'\u00b7ge", "Christ", "ist", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der s\u00fc\u00dfe Gottessohn,", "tokens": ["Der", "s\u00fc\u00b7\u00dfe", "Got\u00b7tes\u00b7sohn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des freun sich alle Frommen", "tokens": ["Des", "freun", "sich", "al\u00b7le", "From\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "PRF", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Am h\u00f6chsten Himmelsthron,", "tokens": ["Am", "h\u00f6chs\u00b7ten", "Him\u00b7melst\u00b7hron", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Auch was auf Erden ist,", "tokens": ["Auch", "was", "auf", "Er\u00b7den", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "APPR", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mu\u00df preisen hoch und loben", "tokens": ["Mu\u00df", "prei\u00b7sen", "hoch", "und", "lo\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "VVFIN", "ADJD", "KON", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mit allen Engeln droben", "tokens": ["Mit", "al\u00b7len", "En\u00b7geln", "