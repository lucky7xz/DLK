{"textgrid.poem.60909": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Floridiana", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Diese Pal\u00e4ste mit hangenden G\u00e4rten, es hat sie ein K\u00f6nig,", "tokens": ["Die\u00b7se", "Pa\u00b7l\u00e4s\u00b7te", "mit", "han\u00b7gen\u00b7den", "G\u00e4r\u00b7ten", ",", "es", "hat", "sie", "ein", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "ADJA", "NN", "$,", "PPER", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Auf des Gebirgs Felsblock, seiner Geliebten erbaut,", "tokens": ["Auf", "des", "Ge\u00b7birgs", "Fels\u00b7block", ",", "sei\u00b7ner", "Ge\u00b7lieb\u00b7ten", "er\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+---+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Grotten vertieft und Rotunden erh\u00f6ht in der lachenden Wildnis,", "tokens": ["Grot\u00b7ten", "ver\u00b7tieft", "und", "Ro\u00b7tun\u00b7den", "er\u00b7h\u00f6ht", "in", "der", "la\u00b7chen\u00b7den", "Wild\u00b7nis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "KON", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+---+--+--+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "\u00dcber die Schluchten zugleich magische Br\u00fccken gew\u00f6lbt.", "tokens": ["\u00dc\u00b7ber", "die", "Schluch\u00b7ten", "zu\u00b7gleich", "ma\u00b7gi\u00b7sche", "Br\u00fc\u00b7cken", "ge\u00b7w\u00f6lbt", "."], "token_info": ["word", "wor