{"textgrid.poem.63882": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "18.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So lasset uns suchen und finden das Gl\u00fcck,", "tokens": ["So", "las\u00b7set", "uns", "su\u00b7chen", "und", "fin\u00b7den", "das", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVINF", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und fanden wir's, nimmer entschwinden das Gl\u00fcck.", "tokens": ["Und", "fan\u00b7den", "wir's", ",", "nim\u00b7mer", "ent\u00b7schwin\u00b7den", "das", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "$,", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Nicht aber die Hexe, am Stirnhaar erfa\u00dft;", "tokens": ["Nicht", "a\u00b7ber", "die", "He\u00b7xe", ",", "am", "Stirn\u00b7haar", "er\u00b7fa\u00dft", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "$,", "APPRART", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Bald flattert davon mit den Winden das Gl\u00fcck.", "tokens": ["Bald", "flat\u00b7tert", "da\u00b7von", "mit", "den", "Win\u00b7den", "das", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PAV", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Das echte, das rechte sitzt traulich am Herd,", "tokens": ["Das", "ech\u00b7te", ",", "das", "rech\u00b7te", "sitzt", "trau\u00b7lich", "am", "Herd", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+--+-++--+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Da mu\u00dft du mit Rosen umwinden das Gl\u00fcck.", "tokens": ["Da", "mu\u00dft", "du", "mit", "Ro\u00b7sen", "um\u00b7win\u00b7den", "das", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Hausfr\u00e4ulich gesch\u00e4ftig, den Wirten zum Lohn,", "tokens": ["Haus\u00b7fr\u00e4u\u00b7lich", "ge\u00b7sch\u00e4f\u00b7tig", ",", "den", "Wir\u00b7ten", "zum", "Lohn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Wird ewige Glut dir entz\u00fcnden das Gl\u00fcck.", "tokens": ["Wird", "e\u00b7wi\u00b7ge", "Glut", "dir", "ent\u00b7z\u00fcn\u00b7den", "das", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.9": {"text": "Es fragt nicht nach Sch\u00e4tzen, nach weltlicher Lust;", "tokens": ["Es", "fragt", "nicht", "nach", "Sch\u00e4t\u00b7zen", ",", "nach", "welt\u00b7li\u00b7cher", "Lust", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "NN", "$,", "APPR", "PWAT", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.10": {"text": "Treu bleibt auch den Armen und Blind