{"dta.poem.18062": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "XiX.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198462", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Krebs fuhr seinen Sohn mit diesen Worten an:", "tokens": ["Ein", "Krebs", "fuhr", "sei\u00b7nen", "Sohn", "mit", "die\u00b7sen", "Wor\u00b7ten", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "APPR", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie gehst du so verkehrt, Sohn, stehst du in dem Wahn", "tokens": ["Wie", "gehst", "du", "so", "ver\u00b7kehrt", ",", "Sohn", ",", "stehst", "du", "in", "dem", "Wahn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "VVPP", "$,", "NN", "$,", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du gehest hinter sich ganz recht?", "tokens": ["Du", "ge\u00b7hest", "hin\u00b7ter", "sich", "ganz", "recht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So bist du Herr, und ich bin Knecht.", "tokens": ["So", "bist", "du", "Herr", ",", "und", "ich", "bin", "Knecht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,", "KON", "PPER", "VAFIN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du solltest lernen vor sich gehn,", "tokens": ["Du", "soll\u00b7test", "ler\u00b7nen", "vor", "sich", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie auch dein Vater geht; das wird dir besser stehn.", "tokens": ["Wie", "auch", "dein", "Va\u00b7ter", "geht", ";", "das", "wird", "dir", "bes\u00b7ser", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "VVFIN", "$.", "PDS", "VAFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ein Sohn, der wie sein Vater thut,", "tokens": ["Ein", "Sohn", ",", "der", "wie", "sein", "Va\u00b7ter", "thut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "KOKOM", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der wird gelobt, und das ist recht und gut.", "tokens": ["Der", "wird", "ge\u00b7lobt", ",", "und", "das", "ist", "recht", "und", "gut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVPP", "$,", "KON", "PDS", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Der Sohn sprach: Vater, du sagst wohl.", "tokens": ["Der", "Sohn", "sprach", ":", "Va\u00b7ter", ",", "du", "sagst", "wohl", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "NN", "$,", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ich wei\u00df es, da\u00df ein Sohn dem Vater folgen soll.", "tokens": ["Ich", "wei\u00df", "es", ",", "da\u00df", "ein", "Sohn", "dem", "Va\u00b7ter", "fol\u00b7gen", "soll", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "De\u00dfwegen geh nur vor; la\u00df sehen, wie du gehest,", "tokens": ["De\u00df\u00b7we\u00b7gen", "geh", "nur", "vor", ";", "la\u00df", "se\u00b7hen", ",", "wie", "du", "ge\u00b7hest", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "PTKVZ", "$.", "VVIMP", "VVINF", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So geh ich auch, wie du; und wie du stehest,", "tokens": ["So", "geh", "ich", "auch", ",", "wie", "du", ";", "und", "wie", "du", "ste\u00b7hest", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "$.", "KON", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "So steh ich auch; und wie du dich", "tokens": ["So", "steh", "ich", "auch", ";", "und", "wie", "du", "dich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$.", "KON", "PWAV", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Dann stellest, stell ich gleichfalls mich.", "tokens": ["Dann", "stel\u00b7lest", ",", "stell", "ich", "gleich\u00b7falls", "mich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Da nun der Vater seinen Weg", "tokens": ["Da", "nun", "der", "Va\u00b7ter", "sei\u00b7nen", "Weg"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Zum Beyspiel sollte vor sich gehen,", "tokens": ["Zum", "Bey\u00b7spiel", "soll\u00b7te", "vor", "sich", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "War er dazu ganz ungeschickt und tr\u00e4g;", "tokens": ["War", "er", "da\u00b7zu", "ganz", "un\u00b7ge\u00b7schickt", "und", "tr\u00e4g", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PAV", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Hingegen war er schnell sich r\u00fcckwerts fortzudrehen.", "tokens": ["Hin\u00b7ge\u00b7gen", "war", "er", "schnell", "sich", "r\u00fcck\u00b7werts", "fort\u00b7zu\u00b7dre\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "PRF", "ADV", "VVINF", "$."], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.19": {"text": "Da sprach der Sohn zum Vater: Mein!", "tokens": ["Da", "sprach", "der", "Sohn", "zum", "Va\u00b7ter", ":", "Mein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$.", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Was soll das vor ein Beyspiel seyn?", "tokens": ["Was", "soll", "das", "vor", "ein", "Bey\u00b7spiel", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PDS", "APPR", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Du hast denselben Gang, wie ich,", "tokens": ["Du", "hast", "den\u00b7sel\u00b7ben", "Gang", ",", "wie", "ich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PDAT", "NN", "$,", "PWAV", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Und gehst geschwinde hintersich,", "tokens": ["Und", "gehst", "ge\u00b7schwin\u00b7de", "hin\u00b7ter\u00b7sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Indem du solltest vor sich gehn.", "tokens": ["In\u00b7dem", "du", "soll\u00b7test", "vor", "sich", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Drum la\u00df dein Strafen lieber stehn.", "tokens": ["Drum", "la\u00df", "dein", "Stra\u00b7fen", "lie\u00b7ber", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}