{"dta.poem.697": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "IiI.  \n Verleugnung der Welt.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Was frag ich nach der welt! sie wird in flam&#769;en stehn: ", "tokens": ["Was", "frag", "ich", "nach", "der", "welt", "!", "sie", "wird", "in", "flam", "&#769;", "en", "stehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "XML_entity", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "PPER", "VAFIN", "APPR", "FM", "FM", "FM", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was acht ich reiche pracht: der Todt rei\u00dft alles hin!", "tokens": ["Was", "acht", "ich", "rei\u00b7che", "pracht", ":", "der", "Todt", "rei\u00dft", "al\u00b7les", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "CARD", "PPER", "ADJA", "NN", "$.", "ART", "NN", "VVFIN", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was hilfft die wissenschafft/ der mehr denn falsche dunst:", "tokens": ["Was", "hilfft", "die", "wis\u00b7sen\u00b7schafft", "/", "der", "mehr", "denn", "fal\u00b7sche", "dunst", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$(", "ART", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der liebe Zauberwerck ist tolle Phantasie:", "tokens": ["Der", "lie\u00b7be", "Zau\u00b7ber\u00b7werck", "ist", "tol\u00b7le", "Phan\u00b7ta\u00b7sie", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die wollust ist f\u00fcrwar nichts al\u00df ein schneller Traum; ", "tokens": ["Die", "wol\u00b7lust", "ist", "f\u00fcr\u00b7war", "nichts", "al\u00df", "ein", "schnel\u00b7ler", "Traum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PIS", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Sch\u00f6nheit ist wie Schnee\u2019/ di\u00df Leben ist der Todt.", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "ist", "wie", "Schnee'", "/", "di\u00df", "Le\u00b7ben", "ist", "der", "Todt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "KOKOM", "NN", "$(", "PDS", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "2. Di\u00df alles stinckt mich an/ drumb w\u00fcndsch ich mir den", "tokens": ["Di\u00df", "al\u00b7les", "stinckt", "mich", "an", "/", "drumb", "w\u00fcndsch", "ich", "mir", "den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "PIS", "VVFIN", "PPER", "PTKVZ", "$(", "PAV", "VVFIN", "PPER", "PPER", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Todt! ", "tokens": ["Todt", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Weil nichts wie sch\u00f6n vnd starck/ wie reich es sey/ kan stehn", "tokens": ["Weil", "nichts", "wie", "sch\u00f6n", "vnd", "starck", "/", "wie", "reich", "es", "sey", "/", "kan", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PIS", "KOKOM", "ADJD", "KON", "NN", "$(", "PWAV", "ADJD", "PPER", "VAFIN", "$(", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Offt/ eh man leben wil/ ist schon di\u00df Leben hin.", "tokens": ["Offt", "/", "eh", "man", "le\u00b7ben", "wil", "/", "ist", "schon", "di\u00df", "Le\u00b7ben", "hin", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "KOUS", "PIS", "VVINF", "VMFIN", "$(", "VAFIN", "ADV", "PDS", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer Sch\u00e4tz' vnd Reichthumb sucht: was sucht er mehr al\u00df ", "tokens": ["Wer", "Sch\u00e4tz'", "vnd", "Reicht\u00b7humb", "sucht", ":", "was", "sucht", "er", "mehr", "al\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "NN", "KON", "NN", "VVFIN", "$.", "PWS", "VVFIN", "PPER", "PIAT", "KOKOM"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "dunst. ", "tokens": ["dunst", "."], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Wenn dem/ der Ehrenrauch entsteckt die Phantasie. ", "tokens": ["Wenn", "dem", "/", "der", "Eh\u00b7ren\u00b7rauch", "ent\u00b7steckt", "die", "Phan\u00b7ta\u00b7sie", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "$(", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So traumt jhm wenn er wacht/ er wacht v\u00f1 sorgt im traum.", "tokens": ["So", "traumt", "jhm", "wenn", "er", "wacht", "/", "er", "wacht", "v\u00f1", "sorgt", "im", "traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOUS", "PPER", "VVFIN", "$(", "PPER", "VVFIN", "NE", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "3. Auff meine Seel/ auf/ auf/ entwach au\u00df diesem", "tokens": ["Auff", "mei\u00b7ne", "Seel", "/", "auf", "/", "auf", "/", "ent\u00b7wach", "au\u00df", "die\u00b7sem"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$(", "APPR", "$(", "APPR", "$(", "VVFIN", "APPR", "PDAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "traum/ ", "tokens": ["traum", "/"], "token_info": ["word", "punct"], "pos": ["XY", "$("], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Verwirff was jrrdisch ist/ vnd trotze Noth vnd Todt!", "tokens": ["Ver\u00b7wirff", "was", "jrr\u00b7disch", "ist", "/", "vnd", "trot\u00b7ze", "Noth", "vnd", "Todt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWS", "PPER", "VAFIN", "$(", "KON", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was wird dir/ wenn du wirst f\u00fcr jenem throne stehn/", "tokens": ["Was", "wird", "dir", "/", "wenn", "du", "wirst", "f\u00fcr", "je\u00b7nem", "thro\u00b7ne", "stehn", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$(", "KOUS", "PPER", "VAFIN", "APPR", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die welt beh\u00fclfflich seyn? wo dencken wir doch hin?", "tokens": ["Die", "welt", "be\u00b7h\u00fclf\u00b7flich", "seyn", "?", "wo", "den\u00b7cken", "wir", "doch", "hin", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VAINF", "$.", "PWAV", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was blendet den verstandt? soll dieser leichte dunst", "tokens": ["Was", "blen\u00b7det", "den", "ver\u00b7standt", "?", "soll", "die\u00b7ser", "leich\u00b7te", "dunst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "VVPP", "$.", "VMFIN", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Bezaubern mein gem\u00fcth mit solcher Phantasie?", "tokens": ["Be\u00b7zau\u00b7bern", "mein", "ge\u00b7m\u00fcth", "mit", "sol\u00b7cher", "Phan\u00b7ta\u00b7sie", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "4. Bi\u00df her! vnd weiter nicht! verfluchte Phantasie!", "tokens": ["Bi\u00df", "her", "!", "vnd", "wei\u00b7ter", "nicht", "!", "ver\u00b7fluch\u00b7te", "Phan\u00b7ta\u00b7sie", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PTKVZ", "$.", "KON", "ADV", "PTKNEG", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nichts werthes Gauckelwerck. Verblendung-voller traum/", "tokens": ["Nichts", "wert\u00b7hes", "Gau\u00b7ckel\u00b7werck", ".", "Ver\u00b7blen\u00b7dung\u00b7vol\u00b7ler", "traum", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "ADJA", "NN", "$.", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du schmertzen-reiche Lust du folter-hartter Todt! ", "tokens": ["Du", "schmert\u00b7zen\u00b7rei\u00b7che", "Lust", "du", "fol\u00b7ter\u00b7hart\u00b7ter", "Todt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ade! ich wil nunmehr auf freyen ", "tokens": ["A\u00b7de", "!", "ich", "wil", "nun\u00b7mehr", "auf", "frey\u00b7en"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "PPER", "VMFIN", "ADV", "APPR", "ADJA"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Vnd tretten was mich tratt! Ich eyle schon dahin;", "tokens": ["Vnd", "tret\u00b7ten", "was", "mich", "tratt", "!", "Ich", "ey\u00b7le", "schon", "da\u00b7hin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PWS", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "ADV", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wo nichts als warheit ist. Kein bald verschwindent dunst.", "tokens": ["Wo", "nichts", "als", "war\u00b7heit", "ist", ".", "Kein", "bald", "ver\u00b7schwin\u00b7dent", "dunst", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "KOKOM", "NN", "VAFIN", "$.", "PIAT", "ADV", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "5. Treib ewig helles Licht der dicken Nebel dunst", "tokens": ["Treib", "e\u00b7wig", "hel\u00b7les", "Licht", "der", "di\u00b7cken", "Ne\u00b7bel", "dunst"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die blinde Lust der welt: die tolle Phantasie ", "tokens": ["Die", "blin\u00b7de", "Lust", "der", "welt", ":", "die", "tol\u00b7le", "Phan\u00b7ta\u00b7sie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die fl\u00fcchtige begierd\u2019vnd dieser g\u00fctter traum", "tokens": ["Die", "fl\u00fcch\u00b7ti\u00b7ge", "be\u00b7gierd'v\u00b7nd", "die\u00b7ser", "g\u00fct\u00b7ter", "traum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hinweg vnd lehre mich recht sterben vor dem Todt.", "tokens": ["Hin\u00b7weg", "vnd", "leh\u00b7re", "mich", "recht", "ster\u00b7ben", "vor", "dem", "Todt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "VVFIN", "PPER", "ADV", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "La\u00df mich die eitelkeit der Erden recht verstehn", "tokens": ["La\u00df", "mich", "die", "ei\u00b7tel\u00b7keit", "der", "Er\u00b7den", "recht", "ver\u00b7stehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "NN", "ART", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Entbinde mein gem\u00fcth/ vnd nimb die Ketten hin.", "tokens": ["Ent\u00b7bin\u00b7de", "mein", "ge\u00b7m\u00fcth", "/", "vnd", "nimb", "die", "Ket\u00b7ten", "hin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$(", "KON", "VVIMP", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "6. Nimb was mich vnd die welt verkuppelt! nimb doch", "tokens": ["Nimb", "was", "mich", "vnd", "die", "welt", "ver\u00b7kup\u00b7pelt", "!", "nimb", "doch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "KON", "ART", "NN", "VVPP", "$.", "VVIMP", "ADV"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "hin", "tokens": ["hin"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Verh\u00fcllen mein Gem\u00fctt/ vnd alle Phantasie", "tokens": ["Ver\u00b7h\u00fcl\u00b7len", "mein", "Ge\u00b7m\u00fctt", "/", "vnd", "al\u00b7le", "Phan\u00b7ta\u00b7sie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "$(", "KON", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Eitel-leren welt sey f\u00fcr mir al\u00df ein traum/ ", "tokens": ["Der", "Ei\u00b7tel\u00b7le\u00b7ren", "welt", "sey", "f\u00fcr", "mir", "al\u00df", "ein", "traum", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAFIN", "APPR", "PPER", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Von dem ich nun erwacht! vnd la\u00df nach diesem tod", "tokens": ["Von", "dem", "ich", "nun", "er\u00b7wacht", "!", "vnd", "la\u00df", "nach", "die\u00b7sem", "tod"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "ADV", "VVPP", "$.", "KON", "VVIMP", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mich vnerschrocken ", "tokens": ["Mich", "vner\u00b7schro\u00b7cken"], "token_info": ["word", "word"], "pos": ["PPER", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}}}}}