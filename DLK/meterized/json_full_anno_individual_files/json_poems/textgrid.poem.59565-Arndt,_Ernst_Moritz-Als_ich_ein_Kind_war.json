{"textgrid.poem.59565": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Als ich ein Kind war", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich ein Kind war,", "tokens": ["Als", "ich", "ein", "Kind", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Was sah ich f\u00fcr Farben!", "tokens": ["Was", "sah", "ich", "f\u00fcr", "Far\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Himmlische Schimmer", "tokens": ["Himm\u00b7li\u00b7sche", "Schim\u00b7mer"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Gl\u00e4nzten im Abendschein,", "tokens": ["Gl\u00e4nz\u00b7ten", "im", "A\u00b7bend\u00b7schein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Gl\u00e4nzten im Morgenrot,", "tokens": ["Gl\u00e4nz\u00b7ten", "im", "Mor\u00b7gen\u00b7rot", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Und wann der Schlaf sanft", "tokens": ["Und", "wann", "der", "Schlaf", "sanft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "ADJD"], "meter": "-+-++", "measure": "unknown.measure.tri"}, "line.7": {"text": "Einwiegte die \u00c4uglein,", "tokens": ["Ein\u00b7wieg\u00b7te", "die", "\u00c4ug\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Gingen nicht Sonnen und Sterne", "tokens": ["Gin\u00b7gen", "nicht", "Son\u00b7nen", "und", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "NN", "KON", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.9": {"text": "Dem tr\u00e4umenden Seelchen", "tokens": ["Dem", "tr\u00e4u\u00b7men\u00b7den", "Seel\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Auf? G\u00f6tterlichter,", "tokens": ["Auf", "?", "G\u00f6t\u00b7ter\u00b7lich\u00b7ter", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["APPR", "$.", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Ach! der himmlischen Heimat", "tokens": ["Ach", "!", "der", "himm\u00b7li\u00b7schen", "Hei\u00b7mat"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.12": {"text": "Selige Spiegel?", "tokens": ["Se\u00b7li\u00b7ge", "Spie\u00b7gel", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Als ich ein Kind war,", "tokens": ["Als", "ich", "ein", "Kind", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Was fand ich f\u00fcr Blumen!", "tokens": ["Was", "fand", "ich", "f\u00fcr", "Blu\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Nicht blo\u00df die blauen", "tokens": ["Nicht", "blo\u00df", "die", "blau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "ART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Lieblichen Veilchen,", "tokens": ["Lieb\u00b7li\u00b7chen", "Veil\u00b7chen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Nicht dich, rote Rose,", "tokens": ["Nicht", "dich", ",", "ro\u00b7te", "Ro\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Blumenk\u00f6nigin allein,", "tokens": ["Blu\u00b7men\u00b7k\u00f6\u00b7ni\u00b7gin", "al\u00b7lein", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nicht euch, ihr schneewei\u00dfen", "tokens": ["Nicht", "euch", ",", "ihr", "schnee\u00b7wei\u00b7\u00dfen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "PPER", "$,", "PPER", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Unschuldskinder, Lilien, allein \u2013", "tokens": ["Un\u00b7schulds\u00b7kin\u00b7der", ",", "Li\u00b7li\u00b7en", ",", "al\u00b7lein", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "ADV", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Ach! noch zehntausend", "tokens": ["Ach", "!", "noch", "zehn\u00b7tau\u00b7send"], "token_info": ["word", "punct", "word", "word"], "pos": ["ITJ", "$.", "ADV", "CARD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Andere und andere", "tokens": ["An\u00b7de\u00b7re", "und", "an\u00b7de\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["PIS", "KON", "ADJA"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Sch\u00f6ner und duftender", "tokens": ["Sch\u00f6\u00b7ner", "und", "duf\u00b7ten\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "ADJA"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.12": {"text": "Bl\u00fchten da auch hier unten.", "tokens": ["Bl\u00fch\u00b7ten", "da", "auch", "hier", "un\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADV", "ADV", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.13": {"text": "Wo sind sie blieben?", "tokens": ["Wo", "sind", "sie", "blie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Als ich ein Kind war,", "tokens": ["Als", "ich", "ein", "Kind", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Was hatt' ich f\u00fcr Gespielen!", "tokens": ["Was", "hatt'", "ich", "f\u00fcr", "Ge\u00b7spie\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "War nie allein", "tokens": ["War", "nie", "al\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ADV", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Einsam im gr\u00fcnen Wald,", "tokens": ["Ein\u00b7sam", "im", "gr\u00fc\u00b7nen", "Wald", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Einsam im Felde.", "tokens": ["Ein\u00b7sam", "im", "Fel\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Wer warst du, bunte Blume?", "tokens": ["Wer", "warst", "du", ",", "bun\u00b7te", "Blu\u00b7me", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wer du, kleines B\u00e4umchen?", "tokens": ["Wer", "du", ",", "klei\u00b7nes", "B\u00e4um\u00b7chen", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Und du, in den Zweigen", "tokens": ["Und", "du", ",", "in", "den", "Zwei\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "APPR", "ART", "NN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.9": {"text": "Singendes V\u00f6glein?", "tokens": ["Sin\u00b7gen\u00b7des", "V\u00f6\u00b7glein", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Waret ihr nicht Engel", "tokens": ["Wa\u00b7ret", "ihr", "nicht", "En\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PTKNEG", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Freundliche Engel Gottes,", "tokens": ["Freund\u00b7li\u00b7che", "En\u00b7gel", "Got\u00b7tes", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.12": {"text": "Mitf\u00fchlend, mitspielend?", "tokens": ["Mit\u00b7f\u00fch\u00b7lend", ",", "mit\u00b7spie\u00b7lend", "?"], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Ach! du, die so sch\u00f6n war,", "tokens": ["Ach", "!", "du", ",", "die", "so", "sch\u00f6n", "war", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "$,", "PRELS", "ADV", "ADJD", "VAFIN", "$,"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.14": {"text": "Junge lebendige Welt,", "tokens": ["Jun\u00b7ge", "le\u00b7ben\u00b7di\u00b7ge", "Welt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.15": {"text": "Wo gingst du hin?", "tokens": ["Wo", "gingst", "du", "hin", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Als ich ein Kind war,", "tokens": ["Als", "ich", "ein", "Kind", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Was hatt' ich f\u00fcr Tr\u00e4ume!", "tokens": ["Was", "hatt'", "ich", "f\u00fcr", "Tr\u00e4u\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Kann ich es nennen,", "tokens": ["Kann", "ich", "es", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Was Namen nicht hat?", "tokens": ["Was", "Na\u00b7men", "nicht", "hat", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "PTKNEG", "VAFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Kann ich euch zeigen,", "tokens": ["Kann", "ich", "euch", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Unverg\u00e4ngliche Bilder", "tokens": ["Un\u00b7ver\u00b7g\u00e4ng\u00b7li\u00b7che", "Bil\u00b7der"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Himmlischer Sch\u00f6nheit?", "tokens": ["Himm\u00b7li\u00b7scher", "Sch\u00f6n\u00b7heit", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "O meine Sehnsucht", "tokens": ["O", "mei\u00b7ne", "Sehn\u00b7sucht"], "token_info": ["word", "word", "word"], "pos": ["NE", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Kennet euch noch und die nimmer", "tokens": ["Ken\u00b7net", "euch", "noch", "und", "die", "nim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "KON", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.10": {"text": "Rastende Liebe.", "tokens": ["Ras\u00b7ten\u00b7de", "Lie\u00b7be", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Himmlischer Vater,", "tokens": ["Himm\u00b7li\u00b7scher", "Va\u00b7ter", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Du, der uns alle", "tokens": ["Du", ",", "der", "uns", "al\u00b7le"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "PPER", "PIAT"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Seine Kinder nennet,", "tokens": ["Sei\u00b7ne", "Kin\u00b7der", "nen\u00b7net", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Dessen Geisteratems", "tokens": ["Des\u00b7sen", "Geis\u00b7te\u00b7ra\u00b7tems"], "token_info": ["word", "word"], "pos": ["PDAT", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Gebilde wir sind,", "tokens": ["Ge\u00b7bil\u00b7de", "wir", "sind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VAFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "O mache mich wieder", "tokens": ["O", "ma\u00b7che", "mich", "wie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ADV"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Wie ein unschuldiges Kind!", "tokens": ["Wie", "ein", "un\u00b7schul\u00b7di\u00b7ges", "Kind", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "---+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Ach! nur ein Lallen,", "tokens": ["Ach", "!", "nur", "ein", "Lal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Ein leises Stammeln", "tokens": ["Ein", "lei\u00b7ses", "Stam\u00b7meln"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Jener Gef\u00fchle!", "tokens": ["Je\u00b7ner", "Ge\u00b7f\u00fch\u00b7le", "!"], "token_info": ["word", "word", "punct"], "pos": ["PDAT", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Jener Kinderspiele!", "tokens": ["Je\u00b7ner", "Kin\u00b7der\u00b7spie\u00b7le", "!"], "token_info": ["word", "word", "punct"], "pos": ["PDAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.12": {"text": "Nur einen Schimmer", "tokens": ["Nur", "ei\u00b7nen", "Schim\u00b7mer"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Jener Gestalten!", "tokens": ["Je\u00b7ner", "Ge\u00b7stal\u00b7ten", "!"], "token_info": ["word", "word", "punct"], "pos": ["PDAT", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Einen Ton jener Kl\u00e4nge!", "tokens": ["Ei\u00b7nen", "Ton", "je\u00b7ner", "Kl\u00e4n\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.15": {"text": "O warum blieb ich", "tokens": ["O", "wa\u00b7rum", "blieb", "ich"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "PWAV", "VVFIN", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.16": {"text": "Nicht ewig ein Kind?", "tokens": ["Nicht", "e\u00b7wig", "ein", "Kind", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Als ich ein Kind war,", "tokens": ["Als", "ich", "ein", "Kind", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Was sah ich f\u00fcr Farben!", "tokens": ["Was", "sah", "ich", "f\u00fcr", "Far\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Himmlische Schimmer", "tokens": ["Himm\u00b7li\u00b7sche", "Schim\u00b7mer"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Gl\u00e4nzten im Abendschein,", "tokens": ["Gl\u00e4nz\u00b7ten", "im", "A\u00b7bend\u00b7schein", ","], "token_info": ["word", "word", "word", "punct