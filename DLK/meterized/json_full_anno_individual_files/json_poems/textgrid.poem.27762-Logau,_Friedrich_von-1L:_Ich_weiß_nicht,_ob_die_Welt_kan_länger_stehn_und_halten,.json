{"textgrid.poem.27762": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich wei\u00df nicht, ob die Welt kan l\u00e4nger stehn und halten,", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df nicht, ob die Welt kan l\u00e4nger stehn und halten,", "tokens": ["Ich", "wei\u00df", "nicht", ",", "ob", "die", "Welt", "kan", "l\u00e4n\u00b7ger", "stehn", "und", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "ART", "NN", "VMFIN", "ADJD", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil da und dort ihr Bau nimmt Br\u00fcche, Risse, Spalten.", "tokens": ["Weil", "da", "und", "dort", "ihr", "Bau", "nimmt", "Br\u00fc\u00b7che", ",", "Ris\u00b7se", ",", "Spal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "ADV", "KON", "ADV", "PPOSAT", "NN", "VVFIN", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gott scheidet sich von uns; wir scheiden uns von Gott.", "tokens": ["Gott", "schei\u00b7det", "sich", "von", "uns", ";", "wir", "schei\u00b7den", "uns", "von", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPR", "PPER", "$.", "PPER", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Wolfahrt reumt das Land, und bleibt uns nichts als Not;", "tokens": ["Die", "Wol\u00b7fahrt", "reumt", "das", "Land", ",", "und", "bleibt", "uns", "nichts", "als", "Not", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "PPER", "PIS", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Tugend fleucht seitab; die alten Laster weichen", "tokens": ["Die", "Tu\u00b7gend", "fleucht", "seit\u00b7ab", ";", "die", "al\u00b7ten", "Las\u00b7ter", "wei\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "$.", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der neuen Teuffeley. Es k\u00fcnnen sich nicht gleichen", "tokens": ["Der", "neu\u00b7en", "Teuf\u00b7fe\u00b7ley", ".", "Es", "k\u00fcn\u00b7nen", "sich", "nicht", "glei\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "PRF", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Unterthan und Herr, der Herr und Unterthan;", "tokens": ["Der", "Un\u00b7ter\u00b7than", "und", "Herr", ",", "der", "Herr", "und", "Un\u00b7ter\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der Mann sucht fremdes Weib; das Weib sucht fremden Mann.", "tokens": ["Der", "Mann", "sucht", "frem\u00b7des", "Weib", ";", "das", "Weib", "sucht", "frem\u00b7den", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$.", "ART", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Himmel wil nicht mehr der Erde Saamen g\u00fcnnen;", "tokens": ["Der", "Him\u00b7mel", "wil", "nicht", "mehr", "der", "Er\u00b7de", "Saa\u00b7men", "g\u00fcn\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "ADV", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die Erde wil nicht mehr wie vor geb\u00e4ren k\u00fcnnen.", "tokens": ["Die", "Er\u00b7de", "wil", "nicht", "mehr", "wie", "vor", "ge\u00b7b\u00e4\u00b7ren", "k\u00fcn\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "ADV", "KOKOM", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Das macht, da\u00df man zum Theil dem Epikurus gl\u00e4ubt:", "tokens": ["Das", "macht", ",", "da\u00df", "man", "zum", "Theil", "dem", "E\u00b7pi\u00b7ku\u00b7rus", "gl\u00e4ubt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "KOUS", "PIS", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die Welt werd ehstes das, was in der Sonne st\u00e4ubt.", "tokens": ["Die", "Welt", "werd", "ehs\u00b7tes", "das", ",", "was", "in", "der", "Son\u00b7ne", "st\u00e4ubt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJA", "PDS", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich wei\u00df nicht, ob die Welt kan l\u00e4n