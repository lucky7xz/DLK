{"dta.poem.12440": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "3.  Variazion .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aus hartem Weh, klagt sich ein Held,                 ", "tokens": ["Aus", "har\u00b7tem", "Weh", ",", "klagt", "sich", "ein", "Held", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In strenger Hut verborgen:", "tokens": ["In", "stren\u00b7ger", "Hut", "ver\u00b7bor\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u201eich w\u00fcnsch ihr Heil, die mir gef\u00e4llt,", "tokens": ["\u201e", "ich", "w\u00fcnsch", "ihr", "Heil", ",", "die", "mir", "ge\u00b7f\u00e4llt", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADJD", "PPOSAT", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201ekomm schier l\u00f6\u00df mich aus Sorgen,", "tokens": ["\u201e", "komm", "schier", "l\u00f6\u00df", "mich", "aus", "Sor\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADJD", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u201eo weiblich Bild, wie schl\u00e4fst so lang,", "tokens": ["\u201e", "o", "weib\u00b7lich", "Bild", ",", "wie", "schl\u00e4fst", "so", "lang", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "ADJD", "NN", "$,", "PWAV", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u201ewillst du die Klag nicht h\u00f6ren,", "tokens": ["\u201e", "willst", "du", "die", "Klag", "nicht", "h\u00f6\u00b7ren", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "\u201ela\u00df dich erwecken mein Gesang,", "tokens": ["\u201e", "la\u00df", "dich", "er\u00b7we\u00b7cken", "mein", "Ge\u00b7sang", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "\u201edein Lieb will mich beth\u00f6ren.\u201c", "tokens": ["\u201e", "dein", "Lieb", "will", "mich", "be\u00b7th\u00f6\u00b7ren", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "VMFIN", "PRF", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein freier W\u00e4chter h\u00f6rt die Mehr,", "tokens": ["Ein", "frei\u00b7er", "W\u00e4ch\u00b7ter", "h\u00f6rt", "die", "Mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lag still an seiner Zinnen,", "tokens": ["Lag", "still", "an", "sei\u00b7ner", "Zin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er fragt, wer hier verborgen w\u00e4r,", "tokens": ["Er", "fragt", ",", "wer", "hier", "ver\u00b7bor\u00b7gen", "w\u00e4r", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So hart nach Lieb th\u00e4t ringen:", "tokens": ["So", "hart", "nach", "Lieb", "th\u00e4t", "rin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u201eey komm her Held, willt mir vertraun,", "tokens": ["\u201e", "ey", "komm", "her", "Held", ",", "willt", "mir", "ver\u00b7traun", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "NE", "VVFIN", "ADV", "NN", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u201edein Klag hilf ich dir decken,", "tokens": ["\u201e", "dein", "Klag", "hilf", "ich", "dir", "de\u00b7cken", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VVFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "\u201esehnst dich so hart nach meiner Frau,", "tokens": ["\u201e", "sehnst", "dich", "so", "hart", "nach", "mei\u00b7ner", "Frau", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PPER", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "\u201eohn Zweifel sollst du auf mich baun,", "tokens": ["\u201e", "ohn", "Zwei\u00b7fel", "sollst", "du", "auf", "mich", "baun", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "\u201efreundlich will ichs auferwecken.\u201c", "tokens": ["\u201e", "freund\u00b7lich", "will", "ichs", "au\u00b7fer\u00b7we\u00b7cken", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "VMFIN", "PIS", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "\u201emein Trauen g\u00e4nzlich zu dir setz,", "tokens": ["\u201e", "mein", "Trau\u00b7en", "g\u00e4nz\u00b7lich", "zu", "dir", "setz", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201ew\u00e4chter, o freyer Geselle!", "tokens": ["\u201e", "w\u00e4ch\u00b7ter", ",", "o", "frey\u00b7er", "Ge\u00b7sel\u00b7le", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "$,", "FM", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "\u201emein Kleid la\u00df ich dir hie zuletz,", "tokens": ["\u201e", "mein", "Kleid", "la\u00df", "ich", "dir", "hie", "zu\u00b7letz", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VVIMP", "PPER", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201emach uns kein Ungef\u00e4lle:", "tokens": ["\u201e", "mach", "uns", "kein", "Un\u00b7ge\u00b7f\u00e4l\u00b7le", ":"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u201egeh h\u00fcbschlich dar, nimm dir der Weil,", "tokens": ["\u201e", "geh", "h\u00fcb\u00b7schlich", "dar", ",", "nimm", "dir", "der", "Weil", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADJD", "PTKVZ", "$,", "VVIMP", "PPER", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "\u201ela\u00df auch dein Gespan nicht merken,", "tokens": ["\u201e", "la\u00df", "auch", "dein", "Ge\u00b7span", "nicht", "mer\u00b7ken", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "ADV", "PPOSAT", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "\u201edie Th\u00fcrmer sehn aus Langeweil,", "tokens": ["\u201e", "die", "Th\u00fcr\u00b7mer", "sehn", "aus", "Lan\u00b7ge\u00b7weil", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "\u201eschau da\u00df dich keiner \u00fcbereil.\u201c", "tokens": ["\u201e", "schau", "da\u00df", "dich", "kei\u00b7ner", "\u00fc\u00b7be\u00b7reil", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "KOUS", "PPER", "PIS", "PTKVZ", "$.", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "\u201ezu Hoffnung thu mich st\u00e4rken.\u201c", "tokens": ["\u201e", "zu", "Hoff\u00b7nung", "thu", "mich", "st\u00e4r\u00b7ken", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "NN", "VVFIN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u201ewach auf, herzallerliebste Frau,", "tokens": ["\u201e", "wach", "auf", ",", "her\u00b7zal\u00b7ler\u00b7liebs\u00b7te", "Frau", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADJD", "PTKVZ", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201eh\u00f6rt j\u00e4mmerliche Schmerzen,", "tokens": ["\u201e", "h\u00f6rt", "j\u00e4m\u00b7mer\u00b7li\u00b7che", "Schmer\u00b7zen", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADJA", "NN", "$,"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u201ees singt ein Held vor gr\u00fcner Au,", "tokens": ["\u201e", "es", "singt", "ein", "Held", "vor", "gr\u00fc\u00b7ner", "Au", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201ef\u00fcrwahr thu ich nicht scherzen.", "tokens": ["\u201e", "f\u00fcr\u00b7wahr", "thu", "ich", "nicht", "scher\u00b7zen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u201elegt an euer Wad, besorgt euch nicht,", "tokens": ["\u201e", "legt", "an", "eu\u00b7er", "Wad", ",", "be\u00b7sorgt", "euch", "nicht", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "\u201eeuch soll nichts wiederfahren,", "tokens": ["\u201e", "euch", "soll", "nichts", "wie\u00b7der\u00b7fah\u00b7ren", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "PIS", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "\u201emerkt eben dem zu sein Gedicht,", "tokens": ["\u201e", "merkt", "e\u00b7ben", "dem", "zu", "sein", "Ge\u00b7dicht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADV", "ART", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "\u201ewie ihn ein Liebe aneficht,", "tokens": ["\u201e", "wie", "ihn", "ein", "Lie\u00b7be", "a\u00b7ne\u00b7ficht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "\u201eeuer Liebe thut selbst bewahren.\u201c", "tokens": ["\u201e", "eu\u00b7er", "Lie\u00b7be", "thut", "selbst", "be\u00b7wah\u00b7ren", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "VVFIN", "ADV", "VVINF", "$.", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Der Held hub an zum drittenmal,", "tokens": ["Der", "Held", "hub", "an", "zum", "drit\u00b7ten\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "APPRART", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gro\u00df Freud th\u00e4t er da nehmen,", "tokens": ["Gro\u00df", "Freud", "th\u00e4t", "er", "da", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er nahet zu des Herren Saal,", "tokens": ["Er", "na\u00b7het", "zu", "des", "Her\u00b7ren", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dabey sie sollt erkennen,", "tokens": ["Da\u00b7bey", "sie", "sollt", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df er ihr treuer Diener w\u00e4r,", "tokens": ["Da\u00df", "er", "ihr", "treu\u00b7er", "Die\u00b7ner", "w\u00e4r", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sollt Gesellschaft mit ihm pflegen:", "tokens": ["Sollt", "Ge\u00b7sell\u00b7schaft", "mit", "ihm", "pfle\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "\u201each W\u00e4chter, ich h\u00f6r gute M\u00e4hr!", "tokens": ["\u201e", "ach", "W\u00e4ch\u00b7ter", ",", "ich", "h\u00f6r", "gu\u00b7te", "M\u00e4hr", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "XY", "NE", "$,", "PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "\u201ean deiner Red sp\u00fcr ich kein Gef\u00e4hr,", "tokens": ["\u201e", "an", "dei\u00b7ner", "Red", "sp\u00fcr", "ich", "kein", "Ge\u00b7f\u00e4hr", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "\u201eschweig still, h\u00fct uns vor Sorgen.\u201c", "tokens": ["\u201e", "schweig", "still", ",", "h\u00fct", "uns", "vor", "Sor\u00b7gen", ".", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "PTKVZ", "$,", "VVFIN", "PPER", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die Frau den Held gar sch\u00f6n empfing,", "tokens": ["Die", "Frau", "den", "Held", "gar", "sch\u00f6n", "emp\u00b7fing", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "K\u00fc\u00dft ihn an seinem Munde,", "tokens": ["K\u00fc\u00dft", "ihn", "an", "sei\u00b7nem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zu rechter Lieb er mit ihr gunt,", "tokens": ["Zu", "rech\u00b7ter", "Lieb", "er", "mit", "ihr", "gunt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "APPR", "PPOSAT", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Macht ihr viel Freud und Wonne,", "tokens": ["Macht", "ihr", "viel", "Freud", "und", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PIAT", "NN", "KON", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Der W\u00e4chter sprach: \u201eNun lieget still,", "tokens": ["Der", "W\u00e4ch\u00b7ter", "sprach", ":", "\u201e", "Nun", "lie\u00b7get", "still", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u201ekein Sorgen thut euch nahen,", "tokens": ["\u201e", "kein", "Sor\u00b7gen", "thut", "euch", "na\u00b7hen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIAT", "NN", "VVFIN", "PPER", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "\u201ef\u00fcrwahr ich euch des Tages Ziel,", "tokens": ["\u201e", "f\u00fcr\u00b7wahr", "ich", "euch", "des", "Ta\u00b7ges", "Ziel", ","], "token_info": ["punct", "word", "w