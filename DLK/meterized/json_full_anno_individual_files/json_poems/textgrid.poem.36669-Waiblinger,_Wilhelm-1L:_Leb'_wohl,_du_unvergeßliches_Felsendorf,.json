, "pos": ["VVIMP", "ADV", "$.", "APPR", "ADJD", "ADJA", "NN", "PTKNEG", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Will ich dich preisen, wie's den Kindern,", "tokens": ["Will", "ich", "dich", "prei\u00b7sen", ",", "wie's", "den", "Kin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVFIN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "G\u00f6ttern und Gl\u00fccklichen ist gegeben.", "tokens": ["G\u00f6t\u00b7tern", "und", "Gl\u00fcck\u00b7li\u00b7chen", "ist", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "VVPP", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.17": {"line.1": {"text": "Der leichte Scherz, der fl\u00fcchtig im Sommertag", "tokens": ["Der", "leich\u00b7te", "Scherz", ",", "der", "fl\u00fcch\u00b7tig", "im", "Som\u00b7mer\u00b7tag"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "APPRART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dem Schmetterling vergleichbar die Blumen neckt,", "tokens": ["Dem", "Schmet\u00b7ter\u00b7ling", "ver\u00b7gleich\u00b7bar", "die", "Blu\u00b7men", "neckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ist nicht mein Erbtheil, anders lenkt' es", "tokens": ["Ist", "nicht", "mein", "E\u00b7rbtheil", ",", "an\u00b7ders", "lenkt'", "es"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.