{"textgrid.poem.36669": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Leb' wohl, du unverge\u00dfliches Felsendorf,", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Leb' wohl, du unverge\u00dfliches Felsendorf,", "tokens": ["Leb'", "wohl", ",", "du", "un\u00b7ver\u00b7ge\u00df\u00b7li\u00b7ches", "Fel\u00b7sen\u00b7dorf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Leb' wohl! Mit heiter scherzendem Lied nicht mehr", "tokens": ["Leb'", "wohl", "!", "Mit", "hei\u00b7ter", "scher\u00b7zen\u00b7dem", "Lied", "nicht", "mehr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "$.", "APPR", "ADJD", "ADJA", "NN", "PTKNEG", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Will ich dich preisen, wie's den Kindern,", "tokens": ["Will", "ich", "dich", "prei\u00b7sen", ",", "wie's", "den", "Kin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVFIN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "G\u00f6ttern und Gl\u00fccklichen ist gegeben.", "tokens": ["G\u00f6t\u00b7tern", "und", "Gl\u00fcck\u00b7li\u00b7chen", "ist", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "VVPP", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Der leichte Scherz, der fl\u00fcchtig im Sommertag", "tokens": ["Der", "leich\u00b7te", "Scherz", ",", "der", "fl\u00fcch\u00b7tig", "im", "Som\u00b7mer\u00b7tag"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "APPRART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dem Schmetterling vergleichbar die Blumen neckt,", "tokens": ["Dem", "Schmet\u00b7ter\u00b7ling", "ver\u00b7gleich\u00b7bar", "die", "Blu\u00b7men", "neckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ist nicht mein Erbtheil, anders lenkt' es", "tokens": ["Ist", "nicht", "mein", "E\u00b7rbtheil", ",", "an\u00b7ders", "lenkt'", "es"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jener zerst\u00f6rende Geist, den schauernd", "tokens": ["Je\u00b7ner", "zer\u00b7st\u00f6\u00b7ren\u00b7de", "Geist", ",", "den", "schau\u00b7ernd"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PDAT", "ADJA", "NN", "$,", "PRELS", "ADJD"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Im Lebenskampf mein gl\u00fchendes Herz erpr\u00fcft.", "tokens": ["Im", "Le\u00b7bens\u00b7kampf", "mein", "gl\u00fc\u00b7hen\u00b7des", "Herz", "er\u00b7pr\u00fcft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Gef\u00e4hrlich ist's zu spielen; die Nemesis", "tokens": ["Ge\u00b7f\u00e4hr\u00b7lich", "ist's", "zu", "spie\u00b7len", ";", "die", "Ne\u00b7me\u00b7sis"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VAFIN", "PTKZU", "VVINF", "$.", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ist eine ernste Macht, die Charis", "tokens": ["Ist", "ei\u00b7ne", "erns\u00b7te", "Macht", ",", "die", "Cha\u00b7ris"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Fliehet vor ihr in das Reich der Kindheit.", "tokens": ["Flie\u00b7het", "vor", "ihr", "in", "das", "Reich", "der", "Kind\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Was dein Beginnen, armes get\u00e4uschtes Herz?", "tokens": ["Was", "dein", "Be\u00b7gin\u00b7nen", ",", "ar\u00b7mes", "ge\u00b7t\u00e4uschtes", "Herz", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "$,", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ziemt es dem Krieger mitten im Graun der Schlacht,", "tokens": ["Ziemt", "es", "dem", "Krie\u00b7ger", "mit\u00b7ten", "im", "Graun", "der", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dem Schiffer in des Meers Orkanen,", "tokens": ["Dem", "Schif\u00b7fer", "in", "des", "Meers", "Or\u00b7ka\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bilder der Heimat, der Ruh' zu n\u00e4hren?", "tokens": ["Bil\u00b7der", "der", "Hei\u00b7mat", ",", "der", "Ruh'", "zu", "n\u00e4h\u00b7ren", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Den aus des Paradieses verlorner Lust", "tokens": ["Den", "aus", "des", "Pa\u00b7ra\u00b7die\u00b7ses", "ver\u00b7lor\u00b7ner", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der unvers\u00f6hnte z\u00fcrnende Gott gejagt,", "tokens": ["Der", "un\u00b7ver\u00b7s\u00f6hn\u00b7te", "z\u00fcr\u00b7nen\u00b7de", "Gott", "ge\u00b7jagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ziemt's dem, die s\u00fc\u00dfe Frucht zu w\u00fcnschen,", "tokens": ["Ziemt's", "dem", ",", "die", "s\u00fc\u00b7\u00dfe", "Frucht", "zu", "w\u00fcn\u00b7schen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "$,", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Deren Genu\u00df ihm den Tod bereitet?", "tokens": ["De\u00b7ren", "Ge\u00b7nu\u00df", "ihm", "den", "Tod", "be\u00b7rei\u00b7tet", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Still, Herz, dein wartet Rom! noch empf\u00e4ngt dich heut", "tokens": ["Still", ",", "Herz", ",", "dein", "war\u00b7tet", "Rom", "!", "noch", "emp\u00b7f\u00e4ngt", "dich", "heut"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "NN", "$,", "PPOSAT", "VVFIN", "NE", "$.", "ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sein uralt Thor, und gr\u00f6\u00dferer Herrlichkeit", "tokens": ["Sein", "ur\u00b7alt", "Thor", ",", "und", "gr\u00f6\u00b7\u00dfe\u00b7rer", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "$,", "KON", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Schwerm\u00fcth'ge Reste wirst du schauen,", "tokens": ["Schwer\u00b7m\u00fcth'\u00b7ge", "Res\u00b7te", "wirst", "du", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Sch\u00e4m' dich des wen'gen, das du beweinest!", "tokens": ["Sch\u00e4m'", "dich", "des", "wen'\u00b7gen", ",", "das", "du", "be\u00b7wei\u00b7nest", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Und dennoch einmal, einmal noch kehrt mein Blick", "tokens": ["Und", "den\u00b7noch", "ein\u00b7mal", ",", "ein\u00b7mal", "noch", "kehrt", "mein", "Blick"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "$,", "ADV", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Sich r\u00fcckw\u00e4rts, wo der wallende Nebeldunst", "tokens": ["Sich", "r\u00fcck\u00b7w\u00e4rts", ",", "wo", "der", "wal\u00b7len\u00b7de", "Ne\u00b7bel\u00b7dunst"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PRF", "ADV", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und milde Morgenwolken r\u00f6thlich", "tokens": ["Und", "mil\u00b7de", "Mor\u00b7gen\u00b7wol\u00b7ken", "r\u00f6th\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mir mein Olevano schon umziehen.", "tokens": ["Mir", "mein", "O\u00b7le\u00b7va\u00b7no", "schon", "um\u00b7zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NE", "ADV", "VVIZU", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Ist's nicht, als w\u00e4r's der dampfenden Erd' entr\u00fcckt?", "tokens": ["Ist's", "nicht", ",", "als", "w\u00e4r's", "der", "damp\u00b7fen\u00b7den", "Erd'", "ent\u00b7r\u00fcckt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "$,", "KOKOM", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Versteh' ich dich, o Geist der Natur, hinfort", "tokens": ["Ver\u00b7steh'", "ich", "dich", ",", "o", "Geist", "der", "Na\u00b7tur", ",", "hin\u00b7fort"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["VVIMP", "PPER", "PRF", "$,", "FM", "NN", "ART", "NN", "$,", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "W\u00e4r's nimmer m\u00f6glich, w\u00e4r's vor\u00fcber,", "tokens": ["W\u00e4r's", "nim\u00b7mer", "m\u00f6g\u00b7lich", ",", "w\u00e4r's", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "$,", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "W\u00e4re verschwunden f\u00fcr mich auf ewig?", "tokens": ["W\u00e4\u00b7re", "ver\u00b7schwun\u00b7den", "f\u00fcr", "mich", "auf", "e\u00b7wig", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "PPER", "APPR", "ADJD", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Und was auch hofft' ich, gl\u00fccklich zu sein, und es", "tokens": ["Und", "was", "auch", "hofft'", "ich", ",", "gl\u00fcck\u00b7lich", "zu", "sein", ",", "und", "es"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PWS", "ADV", "VVFIN", "PPER", "$,", "ADJD", "PTKZU", "VAINF", "$,", "KON", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zu bleiben f\u00fcr und f\u00fcr, o verwegner Wahn!", "tokens": ["Zu", "blei\u00b7ben", "f\u00fcr", "und", "f\u00fcr", ",", "o", "ver\u00b7weg\u00b7ner", "Wahn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "KON", "APPR", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mir reifen keine Fr\u00fcchte; Bl\u00fcten,", "tokens": ["Mir", "rei\u00b7fen", "kei\u00b7ne", "Fr\u00fcch\u00b7te", ";", "Bl\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$.", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aber hesperische, sind mein Alles.", "tokens": ["A\u00b7ber", "hes\u00b7pe\u00b7ri\u00b7sche", ",", "sind", "mein", "Al\u00b7les", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "$,", "VAFIN", "PPOSAT", "PIS", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.10": {"line.1": {"text": "Ach freilich s\u00fc\u00df war's, menschlicher Irrthum nur,", "tokens": ["Ach", "frei\u00b7lich", "s\u00fc\u00df", "wa\u00b7r's", ",", "menschli\u00b7cher", "Irr\u00b7thum", "nur", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "ADJD", "VAFIN", "$,", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Was ich getr\u00e4umt. Noch tief in der Schattenwelt", "tokens": ["Was", "ich", "ge\u00b7tr\u00e4umt", ".", "Noch", "tief", "in", "der", "Schat\u00b7ten\u00b7welt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "VVPP", "$.", "ADV", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Hofft ja der Todte, seine Qualen", "tokens": ["Hofft", "ja", "der", "Tod\u00b7te", ",", "sei\u00b7ne", "Qua\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,", "PPOSAT", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Mit der Erinn'rung der Freude n\u00e4hrend.", "tokens": ["Mit", "der", "Er\u00b7inn'\u00b7rung", "der", "Freu\u00b7de", "n\u00e4h\u00b7rend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "ADJD", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.11": {"line.1": {"text": "Nach finstern Tagen bricht aus dem Nachtgew\u00f6lk", "tokens": ["Nach", "fins\u00b7tern", "Ta\u00b7gen", "bricht", "aus", "dem", "Nacht\u00b7ge\u00b7w\u00f6lk"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Oft noch ein hold wehm\u00fcthiges Abendlicht,", "tokens": ["Oft", "noch", "ein", "hold", "weh\u00b7m\u00fct\u00b7hi\u00b7ges", "A\u00b7bend\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-++-+-+", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Und mancher schon am Rand des Grabes", "tokens": ["Und", "man\u00b7cher", "schon", "am", "Rand", "des", "Gra\u00b7bes"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "ADV", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "L\u00e4chelt und spricht noch vom Gl\u00fcck der Jugend.", "tokens": ["L\u00e4\u00b7chelt", "und", "spricht", "noch", "vom", "Gl\u00fcck", "der", "Ju\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.12": {"line.1": {"text": "O wer nur einmal irrte! Zu sch\u00f6n, zu tief,", "tokens": ["O", "wer", "nur", "ein\u00b7mal", "irr\u00b7te", "!", "Zu", "sch\u00f6n", ",", "zu", "tief", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "PWS", "ADV", "ADV", "VVFIN", "$.", "PTKA", "ADJD", "$,", "PTKA", "ADJD", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Zu wahr ist doch die T\u00e4uschung, zu herb und leer", "tokens": ["Zu", "wahr", "ist", "doch", "die", "T\u00e4u\u00b7schung", ",", "zu", "herb", "und", "leer"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "VAFIN", "ADV", "ART", "NN", "$,", "PTKA", "ADJD", "KON", "ADJD"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Wahrheit, und