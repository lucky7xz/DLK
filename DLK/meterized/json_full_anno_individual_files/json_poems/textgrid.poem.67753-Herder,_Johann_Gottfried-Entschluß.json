{"textgrid.poem.67753": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Entschlu\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Knabe liebt' ich B\u00fccher,", "tokens": ["Als", "Kna\u00b7be", "liebt'", "ich", "B\u00fc\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die ich jetzt, J\u00fcngling, hasse;", "tokens": ["Die", "ich", "jetzt", ",", "J\u00fcng\u00b7ling", ",", "has\u00b7se", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "ADV", "$,", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Im Winter sucht' ich Freunde,", "tokens": ["Im", "Win\u00b7ter", "sucht'", "ich", "Freun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die mich im Fr\u00fchling fliehen,", "tokens": ["Die", "mich", "im", "Fr\u00fch\u00b7ling", "flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie Andre sie verga\u00dfen;", "tokens": ["Wie", "And\u00b7re", "sie", "ver\u00b7ga\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und ich soll Fr\u00fchlingsfreunde,", "tokens": ["Und", "ich", "soll", "Fr\u00fch\u00b7lings\u00b7freun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die ich im Winter misse,", "tokens": ["Die", "ich", "im", "Win\u00b7ter", "mis\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und Rosenm\u00e4dchen suchen,", "tokens": ["Und", "Ro\u00b7sen\u00b7m\u00e4d\u00b7chen", "su\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Die mir der Sommer raubet?", "tokens": ["Die", "mir", "der", "Som\u00b7mer", "rau\u00b7bet", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Nein, ", "tokens": ["Nein", ","], "token_info": ["word", "punct"], "pos": ["PTKANT", "$,"], "meter": "+", "measure": "single.up"}, "line.11": {"text": "Da\u00df ich mich endlich finde", "tokens": ["Da\u00df", "ich", "mich", "end\u00b7lich", "fin\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Und dann mich nie verliere", "tokens": ["Und", "dann", "mich", "nie", "ver\u00b7lie\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Und dann mich Freunden schenke,", "tokens": ["Und", "dann", "mich", "Freun\u00b7den", "schen\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Bis ich ein M\u00e4dchen finde.", "tokens": ["Bis", "ich", "ein", "M\u00e4d\u00b7chen", "fin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Komm, sei mein F\u00fchrer, ", "tokens": ["Komm", ",", "sei", "mein", "F\u00fch\u00b7rer", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Als Knabe liebt' ich B\u00fccher,", "tokens": ["Als", "Kna\u00b7be", "liebt'", "ich", "B\u00fc\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die ich jetzt, J\u00fcngling, hasse;", "tokens": ["Die", "ich", "jetzt", ",", "J\u00fcng\u00b7ling", ",", "has\u00b7se", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "ADV", "$,", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Im Winter sucht' ich Freunde,", "tokens": ["Im", "Win\u00b7ter", "sucht'", "ich", "Freun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die mich im Fr\u00fchling fliehen,", "tokens": ["Die", "mich", "im", "Fr\u00fch\u00b7ling", "flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie Andre sie verga\u00dfen;", "tokens": ["Wie", "And\u00b7re", "sie", "ver\u00b7ga\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "ia