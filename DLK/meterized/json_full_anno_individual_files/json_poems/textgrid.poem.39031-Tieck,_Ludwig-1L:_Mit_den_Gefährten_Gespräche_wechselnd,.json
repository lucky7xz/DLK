re": "iambic.tri.relaxed"}, "line.2": {"text": "Ein starker, alter Mann,", "tokens": ["Ein", "star\u00b7ker", ",", "al\u00b7ter", "Mann", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er \u00f6ffnet die Th\u00fcr", "tokens": ["Er", "\u00f6ff\u00b7net", "die", "Th\u00fcr"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Und stellt uns hin die Bank und den Tisch,", "tokens": ["Und", "stellt", "uns", "hin", "die", "Bank", "und", "den", "Tisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Giebt Wein und Brod,", "tokens": ["Giebt", "Wein", "und", "Brod", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Und dankt f\u00fcr Bezahlung.", "tokens": ["Und", "dankt", "f\u00fcr", "Be\u00b7zah\u00b7lung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Wie wir uns erquicken", "tokens": ["Wie", "wir", "uns", "er\u00b7qui\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Schaun aus der Ferne", "tokens": ["Schaun", "aus", "der", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Jung und Alt", "tokens": ["Jung", "und", "Alt"], "token_info": ["word", "word", "word"], "pos": ["NE", "KON", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "Dem Wunder zu,", "tokens": ["Dem", "Wun\u00b7der", "zu", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Kaum wagt ein dreistes Kind", "tokens": ["Kaum", "wagt", "ein", "dreis\u00b7tes", "Kind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Heranzutreten, Geschenk zu empfahn,", "tokens": ["Her\u00b7an\u00b7zu\u00b7tre\u00b7ten", ",", "Ge\u00b7schenk", "zu", "em\u00b7pfahn", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Doch wie es die M\u00fcnze", "tokens": ["Doch", "wie", "es", "die", "M\u00fcn\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Nur f\u00fchlt in der Hand,", "tokens": ["Nur", "f\u00fchlt", "in", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.15": {"text": "Rennt es zur\u00fcck", "tokens": ["Rennt", "es", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.16": {"text": "Und zeigt den Gespielen", "tokens": ["Und", "zeigt", "den", "Ge\u00b7spie\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.17": {"text": "Die Gabe und zittert noch.", "tokens": ["Die", "Ga\u00b7be", "un