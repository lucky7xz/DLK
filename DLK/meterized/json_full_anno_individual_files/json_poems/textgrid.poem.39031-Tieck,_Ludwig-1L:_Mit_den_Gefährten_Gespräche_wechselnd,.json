{"textgrid.poem.39031": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mit den Gef\u00e4hrten Gespr\u00e4che wechselnd,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit den Gef\u00e4hrten Gespr\u00e4che wechselnd,", "tokens": ["Mit", "den", "Ge\u00b7f\u00e4hr\u00b7ten", "Ge\u00b7spr\u00e4\u00b7che", "wech\u00b7selnd", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "ADJD", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Wandeln wir den steilen Pfad,", "tokens": ["Wan\u00b7deln", "wir", "den", "stei\u00b7len", "Pfad", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Den wenig betretenen", "tokens": ["Den", "we\u00b7nig", "be\u00b7tre\u00b7te\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ART", "PIS", "VVINF"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.4": {"text": "Hinauf zum einsamen St\u00e4dtchen des Felsens.", "tokens": ["Hin\u00b7auf", "zum", "ein\u00b7sa\u00b7men", "St\u00e4dt\u00b7chen", "des", "Fel\u00b7sens", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Durch das enge Thor geschritten,", "tokens": ["Durch", "das", "en\u00b7ge", "Thor", "ge\u00b7schrit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Stehn wir auf der einzigen Gasse der Stadt,", "tokens": ["Stehn", "wir", "auf", "der", "ein\u00b7zi\u00b7gen", "Gas\u00b7se", "der", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Und Kinder, die hier spielen,", "tokens": ["Und", "Kin\u00b7der", ",", "die", "hier", "spie\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So wie ihr Blick uns trifft,", "tokens": ["So", "wie", "ihr", "Blick", "uns", "trifft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Rennen mit Geschrei in die H\u00e4user", "tokens": ["Ren\u00b7nen", "mit", "Ge\u00b7schrei", "in", "die", "H\u00e4u\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "APPR", "ART", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "Die sie schnell verriegeln.", "tokens": ["Die", "sie", "schnell", "ver\u00b7rie\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "Die Eltern, aufgeschreckt,", "tokens": ["Die", "El\u00b7tern", ",", "auf\u00b7ge\u00b7schreckt", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Schaun mit Mi\u00dftraun", "tokens": ["Schaun", "mit", "Mi\u00df\u00b7traun"], "token_info": ["word", "word", "word"], "pos": ["NE", "APPR", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.13": {"text": "Aus den kleinen zerbro