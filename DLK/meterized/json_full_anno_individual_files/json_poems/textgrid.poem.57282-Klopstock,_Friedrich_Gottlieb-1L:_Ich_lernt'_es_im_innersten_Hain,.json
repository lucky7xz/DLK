{"textgrid.poem.57282": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich lernt' es im innersten Hain,", "genre": "verse", "period": "N.A.", "pub_year": 1766, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich lernt' es im innersten Hain,", "tokens": ["Ich", "lernt'", "es", "im", "in\u00b7ners\u00b7ten", "Hain", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Welche Lieder der Barden ah!", "tokens": ["Wel\u00b7che", "Lie\u00b7der", "der", "Bar\u00b7den", "ah", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "In die Nacht deines Thals sinken, Untergang", "tokens": ["In", "die", "Nacht", "dei\u00b7nes", "Thals", "sin\u00b7ken", ",", "Un\u00b7ter\u00b7gang"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "VVINF", "$,", "NN"], "meter": "+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Welch' auf den H\u00f6hen der Tag bleibend umstrahlt.", "tokens": ["Welch'", "auf", "den", "H\u00f6\u00b7hen", "der", "Tag", "blei\u00b7bend", "um\u00b7strahlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "APPR", "ART", "NN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "+--+--++--+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Ich sahe, noch beb' ich vor ihr!", "tokens": ["Ich", "sa\u00b7he", ",", "noch", "beb'", "ich", "vor", "ihr", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Sah der richtenden Norne Wink!", "tokens": ["Sah", "der", "rich\u00b7ten\u00b7den", "Nor\u00b7ne", "Wink", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Ich vernahm, h\u00f6r' ihn noch! ihres Fluges Schlag,", "tokens": ["Ich", "ver\u00b7nahm", ",", "h\u00f6r'", "ihn", "noch", "!", "ih\u00b7res", "Flu\u00b7ges", "Schlag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "$.", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Dass bis hinauf in des Hains Wipfel es scholl!", "tokens": ["Dass", "bis", "hin\u00b7auf", "in", "des", "Hains", "Wip\u00b7fel", "es", "scholl", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADV", "APPR", "ART", "NN", "NN", "PPER", "ADJD", "$."], "meter": "---+--++--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Gek\u00fchlt von dem wehenden Quell,", "tokens": ["Ge\u00b7k\u00fchlt", "von", "dem", "we\u00b7hen\u00b7den", "Quell", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Sass und hatt' auf die Telyn sanft", "tokens": ["Sass", "und", "hatt'", "auf", "die", "Te\u00b7lyn", "sanft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "VAFIN", "APPR", "ART", "NN", "ADJD"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Sich gelehnt Braga. Jetzt brachte Geister ihm,", "tokens": ["Sich", "ge\u00b7lehnt", "Bra\u00b7ga", ".", "Jetzt", "brach\u00b7te", "Geis\u00b7ter", "ihm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVPP", "NE", "$.", "ADV", "VVFIN", "NN", "PPER", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Die sie, in N\u00e4chten des Monds, Liedern entlockt,", "tokens": ["Die", "sie", ",", "in", "N\u00e4ch\u00b7ten", "des", "Monds", ",", "Lie\u00b7dern", "ent\u00b7lockt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "APPR", "NN", "ART", "NN", "$,", "NN", "VVPP", "$,"], "meter": "-+-+--++--+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Die Norne Werandi, und sie", "tokens": ["Die", "Nor\u00b7ne", "We\u00b7ran\u00b7di", ",", "und", "sie"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NE", "$,", "KON", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hatt' in Leiber geh\u00fcllt, die ganz", "tokens": ["Hatt'", "in", "Lei\u00b7ber", "ge\u00b7h\u00fcllt", ",", "die", "ganz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "APPR", "NN", "VVPP", "$,", "PRELS", "ADV"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "F\u00fcr den Geist waren, ganz jeden leisen Zug", "tokens": ["F\u00fcr", "den", "Geist", "wa\u00b7ren", ",", "ganz", "je\u00b7den", "lei\u00b7sen", "Zug"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAFIN", "$,", "ADV", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Sprachen, Gebilder, als w\u00e4rs wahre Gestalt;", "tokens": ["Spra\u00b7chen", ",", "Ge\u00b7bil\u00b7der", ",", "als", "w\u00e4rs", "wah\u00b7re", "Ge\u00b7stalt", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "KOKOM", "VAFIN", "ADJA", "NN", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}}, "stanza.5": {"line.1": {"text": "Zehn neue. Sie nahten. Nur Eins", "tokens": ["Zehn", "neu\u00b7e", ".", "Sie", "nah\u00b7ten", ".", "Nur", "Eins"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["CARD", "ADJA", "$.", "PPER", "VVFIN", "$.", "ADV", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Hatte Minen der Ewigkeit!", "tokens": ["Hat\u00b7te", "Mi\u00b7nen", "der", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Vom Gef\u00fchl seines Werths sch\u00f6n err\u00f6thend! voll", "tokens": ["Vom", "Ge\u00b7f\u00fchl", "sei\u00b7nes", "Werths", "sch\u00f6n", "er\u00b7r\u00f6\u00b7thend", "!", "voll"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "ADJD", "VVPP", "$.", "ADJD"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Reize des J\u00fcnglings, und voll St\u00e4rke des Manns!", "tokens": ["Rei\u00b7ze", "des", "J\u00fcng\u00b7lings", ",", "und", "voll", "St\u00e4r\u00b7ke", "des", "Manns", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "KON", "ADJD", "NN", "ART", "NN", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}}, "stanza.6": {"line.1": {"text": "Mit Furchtsamkeit trat es herzu,", "tokens": ["Mit", "Furcht\u00b7sam\u00b7keit", "trat", "es", "her\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Als es stehen die Norne sah,", "tokens": ["Als", "es", "ste\u00b7hen", "die", "Nor\u00b7ne", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Die allein nach des Tags fernen H\u00fcgeln f\u00fchrt,", "tokens": ["Die", "al\u00b7lein", "nach", "des", "Tags", "fer\u00b7nen", "H\u00fc\u00b7geln", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Oder hinab, wo die Nacht ewig bew\u00f6lkt.", "tokens": ["O\u00b7der", "hin\u00b7ab", ",", "wo", "die", "Nacht", "e\u00b7wig", "be\u00b7w\u00f6lkt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "$,", "PWAV", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "+--+--++--+", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Nachdenkender breitete schon", "tokens": ["Nach\u00b7den\u00b7ken\u00b7der", "brei\u00b7te\u00b7te", "schon"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Skulda schattende Fl\u00fcgel aus;", "tokens": ["Skul\u00b7da", "schat\u00b7ten\u00b7de", "Fl\u00fc\u00b7gel", "aus", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Doch es sank nieder noch ihr der Eichenstab,", "tokens": ["Doch", "es", "sank", "nie\u00b7der", "noch", "ihr", "der", "Ei\u00b7chen\u00b7stab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "KON", "PPER", "ART", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Dessen entscheidender Wink Thoren nicht warnt.", "tokens": ["Des\u00b7sen", "ent\u00b7schei\u00b7den\u00b7der", "Wink", "Tho\u00b7ren", "nicht", "warnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "NN", "PTKNEG", "VVFIN", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}}, "stanza.8": {"line.1": {"text": "Die Neune betraten den Hain", "tokens": ["Die", "Neu\u00b7ne", "be\u00b7tra\u00b7ten", "den", "Hain"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Stolz, und horchten mit trunknem Ohr", "tokens": ["Stolz", ",", "und", "horch\u00b7ten", "mit", "trunk\u00b7nem", "Ohr"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Dem Geschw\u00e4tz, welches laut Stimmenschw\u00e4rme schrien,", "tokens": ["Dem", "Ge\u00b7schw\u00e4tz", ",", "wel\u00b7ches", "laut", "Stim\u00b7men\u00b7schw\u00e4r\u00b7me", "schri\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "+-++--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Und von dem wankenden Stuhl Richter am Thal.", "tokens": ["Und", "von", "dem", "wan\u00b7ken\u00b7den", "Stuhl", "Rich\u00b7ter", "am", "Thal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "NN", "APPRART", "NN", "$."], "meter": "-+-+---+--+", "measure": "iambic.tetra.chol"}}, "stanza.9": {"line.1": {"text": "Sie schreckte das L\u00e4cheln im Blick", "tokens": ["Sie", "schreck\u00b7te", "das", "L\u00e4\u00b7cheln", "im", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Skulda's nicht, und sie schlummerten", "tokens": ["Skulda's", "nicht", ",", "und", "sie", "schlum\u00b7mer\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PTKNEG", "$,", "KON", "PPER", "VVFIN"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.3": {"text": "Noch get\u00e4uscht, ahndungsfrey auf den Kr\u00e4nzen ein,", "tokens": ["Noch", "ge\u00b7t\u00e4uscht", ",", "ahn\u00b7dungs\u00b7frey", "auf", "den", "Kr\u00e4n\u00b7zen", "ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Welche jetzt gr\u00fcnen ihr Traum, welken nicht sah.", "tokens": ["Wel\u00b7che", "jetzt", "gr\u00fc\u00b7nen", "ihr", "Traum", ",", "wel\u00b7ken", "nicht", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "PPOSAT", "NN", "$,", "PRELS", "PTKNEG", "VVFIN", "$."], "meter": "+--+--+---+", "measure": "dactylic.tri.plus"}}, "stanza.10": {"line.1": {"text": "Ah Norne! ... Sie hub sich im Flug,", "tokens": ["Ah", "Nor\u00b7ne", "!", "...", "Sie", "hub", "sich", "im", "Flug", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "$(", "PPER", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Schwebt', und wies mit dem ernsten Stab'", "tokens": ["Schwebt'", ",", "und", "wies", "mit", "dem", "erns\u00b7ten", "Stab'"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "In das Thal! Taumellos endlich, schlichen sie", "tokens": ["In", "das", "Thal", "!", "Tau\u00b7mel\u00b7los", "end\u00b7lich", ",", "schli\u00b7chen", "sie"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$.", "NE", "ADV", "$,", "VVFIN", "PPER"], "meter": "+-++--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "K\u00fcrzeren, l\u00e4ngeren Weg, aber hinab!", "tokens": ["K\u00fcr\u00b7ze\u00b7ren", ",", "l\u00e4n\u00b7ge\u00b7ren", "Weg", ",", "a\u00b7ber", "hin\u00b7ab", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "ADV", "PTKVZ", "$."], "meter": "+--+---+--+", "measure": "dactylic.di.plus"}}, "stanza.11": {"line.1": {"text": "Dem Einen nur wandte sie sich", "tokens": ["Dem", "Ei\u00b7nen", "nur", "wand\u00b7te", "sie", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADV", "VVFIN", "PPER", "PRF"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Nach den schimmernden H\u00fcgeln hin!", "tokens": ["Nach", "den", "schim\u00b7mern\u00b7den", "H\u00fc\u00b7geln", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Es entfloss Lautenklang ihrer Fl\u00fcgel Schwung,", "tokens": ["Es", "ent\u00b7floss", "Lau\u00b7ten\u00b7klang", "ih\u00b7rer", "Fl\u00fc\u00b7gel", "Schwung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PPOSAT", "NN", "NN", "$,"], "meter": "---+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da sie sich wandt', und der Stab Ewigkeit wies!", "tokens": ["Da", "sie", "sich", "wandt'", ",", "und", "der", "Stab", "E\u00b7wig\u00b7keit", "wies", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$,", "KON", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.12": {"line.1": {"text": "Ich lernt' es im innersten Hain,", "tokens": ["Ich", "lernt'", "es", "im", "in\u00b7ners\u00b7ten", "Hain", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Welche Lieder der Barden ah!", "tokens": ["Wel\u00b7che", "Lie\u00b7der", "der", "Bar\u00b7den", "ah", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "In die Nacht deines Thals sinken, Untergang", "tokens": ["In", "die", "Nacht", "dei\u00b7nes", "Thals", "sin\u00b7ken", ",", "Un\u00b7ter\u00b7gang"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "VVINF", "$,", "NN"], "meter": "+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Welch' auf den H\u00f6hen der Tag bleibend umstrahlt.", "tokens": ["Welch'", "auf", "den", "H\u00f6\u00b7hen", "der", "Tag", "blei\u00b7bend", "um\u00b7strahlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "APPR", "ART", "NN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "+--+--++--+", "measure": "dactylic.di.plus"}}, "stanza.13": {"line.1": {"text": "Ich sahe, noch beb' ich vor ihr!", "tokens": ["Ich", "sa\u00b7he", ",", "noch", "beb'", "ich", "vor", "ihr", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "$."], "