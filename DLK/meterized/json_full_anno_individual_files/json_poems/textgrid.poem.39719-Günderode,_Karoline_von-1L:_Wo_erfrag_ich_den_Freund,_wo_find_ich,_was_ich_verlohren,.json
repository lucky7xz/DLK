{"textgrid.poem.39719": {"metadata": {"author": {"name": "G\u00fcnderode, Karoline von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wo erfrag ich den Freund, wo find ich, was ich verlohren,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo erfrag ich den Freund, wo find ich, was ich verlohren,", "tokens": ["Wo", "er\u00b7frag", "ich", "den", "Freund", ",", "wo", "find", "ich", ",", "was", "ich", "ver\u00b7loh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "$,", "PWAV", "VVFIN", "PPER", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Sage es Morgenroth mir, wo mein Geliebter verweilt!", "tokens": ["Sa\u00b7ge", "es", "Mor\u00b7gen\u00b7roth", "mir", ",", "wo", "mein", "Ge\u00b7lieb\u00b7ter", "ver\u00b7weilt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "PPER", "$,", "PWAV", "PPOSAT", "NN", "VVPP", "$."], "meter": "+--+----+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Weihet der Priester den Schleier, der den, dich mir o Lieber vereinigt,", "tokens": ["Wei\u00b7het", "der", "Pries\u00b7ter", "den", "Schlei\u00b7er", ",", "der", "den", ",", "dich", "mir", "o", "Lie\u00b7ber", "ver\u00b7ei\u00b7nigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,", "PRELS", "ART", "$,", "PPER", "PPER", "FM", "ADJD", "VVPP", "$,"], "meter": "+--+--+-+-+--+--+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "H\u00e4lt ein fremdes Gesez stets dich entfernet von mir?", "tokens": ["H\u00e4lt", "ein", "frem\u00b7des", "Ge\u00b7sez", "stets", "dich", "ent\u00b7fer\u00b7net", "von", "mir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "VVFIN", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Aber der Morgen verstumt, verschlungen vom gl\u00fchenden Tage;", "tokens": ["A\u00b7ber", "der", "Mor\u00b7gen", "ver\u00b7stumt", ",", "ver\u00b7schlun\u00b7gen", "vom", "gl\u00fc\u00b7hen\u00b7den", "Ta\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$,", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Abendroth, sage es mir, freundlicher milderer Schein!", "tokens": ["A\u00b7ben\u00b7droth", ",", "sa\u00b7ge", "es", "mir", ",", "freund\u00b7li\u00b7cher", "mil\u00b7de\u00b7rer", "Schein", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "PPER", "$,", "ADJD", "ADJA", "NN", "$."], "meter": "+-+----+--+--+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Aber es f\u00e4rbt sich die Wange des Abendroths bl\u00e4sser und bl\u00e4sser,", "tokens": ["A\u00b7ber", "es", "f\u00e4rbt", "sich", "die", "Wan\u00b7ge", "des", "A\u00b7ben\u00b7droths", "bl\u00e4s\u00b7ser", "und", "bl\u00e4s\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ART", "NN", "ART", "NN", "ADJA", "KON", "ADJA", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Und es streuet auf mich wehmutsvoll perlenden Thau hin.", "tokens": ["Und", "es", "streu\u00b7et", "auf", "mich", "weh\u00b7muts\u00b7voll", "per\u00b7len\u00b7den", "Tha