{"dta.poem.4059": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Feiern m\u00f6cht' ich, aber wof\u00fcr? und singen mit", "tokens": ["Fei\u00b7ern", "m\u00f6cht'", "ich", ",", "a\u00b7ber", "wo\u00b7f\u00fcr", "?", "und", "sin\u00b7gen", "mit"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "$,", "KON", "PWAV", "$.", "KON", "VVFIN", "APPR"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Andern,", "tokens": ["An\u00b7dern", ","], "token_info": ["word", "punct"], "pos": ["ADJA", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Aber so einsam fehlt jegliches G\u00f6ttliche mir.", "tokens": ["A\u00b7ber", "so", "ein\u00b7sam", "fehlt", "jeg\u00b7li\u00b7ches", "G\u00f6tt\u00b7li\u00b7che", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "PIAT", "NN", "PPER", "$."], "meter": "+--+-+---+--+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Die\u00df ist's, die\u00df mein Gebrechen, ich wei\u00df, es", "tokens": ["Die\u00df", "ist's", ",", "die\u00df", "mein", "Ge\u00b7bre\u00b7chen", ",", "ich", "wei\u00df", ",", "es"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PDS", "VAFIN", "$,", "PDS", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "$,", "PPER"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "l\u00e4hmet ein Fluch mir", "tokens": ["l\u00e4h\u00b7met", "ein", "Fluch", "mir"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PPER"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Darum die Sehnen, und wirft, wo ich beginne,", "tokens": ["Da\u00b7rum", "die", "Seh\u00b7nen", ",", "und", "wirft", ",", "wo", "ich", "be\u00b7gin\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "$,", "KON", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "mich hin,", "tokens": ["mich", "hin", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Da\u00df ich f\u00fchllos sitze den Tag und stumm, wie die", "tokens": ["Da\u00df", "ich", "f\u00fchl\u00b7los", "sit\u00b7ze", "den", "Tag", "und", "stumm", ",", "wie", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "ART", "NN", "KON", "ADJD", "$,", "PWAV", "ART"], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Kinder,", "tokens": ["Kin\u00b7der", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.10": {"text": "Nur vom Auge mir kalt \u00f6fters die Thr\u00e4ne noch", "tokens": ["Nur", "vom", "Au\u00b7ge", "mir", "kalt", "\u00f6f\u00b7ters", "die", "Thr\u00e4\u00b7ne", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "PPER", "ADJD", "ADV", "ART", "NN", "ADV"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.11": {"text": "schleicht,", "tokens": ["schleicht", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.12": {"text": "Und die Pflanze des Felds, und der V\u00f6gel Singen", "tokens": ["Und", "die", "Pflan\u00b7ze", "des", "Felds", ",", "und", "der", "V\u00f6\u00b7gel", "Sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,", "KON", "ART", "NN", "NN"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.13": {"text": "mich tr\u00fcb macht,", "tokens": ["mich", "tr\u00fcb", "macht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "$,"], "meter": "+++", "measure": "unknown.measure.tri"}, "line.14": {"text": "Weil mit Freuden auch sie Boten des Himmli-", "tokens": ["Weil", "mit", "Freu\u00b7den", "auch", "sie", "Bo\u00b7ten", "des", "Himm\u00b7li"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "ADV", "PPER", "NN", "ART", "TRUNC"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "schen sind,", "tokens": ["schen", "sind", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "VAFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.16": {"text": "Aber mir in schaudernder Brust die beseelende Sonne,", "tokens": ["A\u00b7ber", "mir", "in", "schau\u00b7dern\u00b7der", "Brust", "die", "be\u00b7see\u00b7len\u00b7de", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.17": {"text": "K\u00fchl und fruchtlos mir d\u00e4mmert, wie Stralen", "tokens": ["K\u00fchl", "und", "frucht\u00b7los", "mir", "d\u00e4m\u00b7mert", ",", "wie", "Stra\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "KON", "ADJD", "PPER", "VVFIN", "$,", "PWAV", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.18": {"text": "der Nacht,", "tokens": ["der", "Nacht", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.19": {"text": "Ach! und nichtig und leer, wie Gef\u00e4ngni\u00dfw\u00e4nde,", "tokens": ["Ach", "!", "und", "nich\u00b7tig", "und", "leer", ",", "wie", "Ge\u00b7f\u00e4ng\u00b7ni\u00df\u00b7w\u00e4n\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "KON", "ADJD", "KON", "ADJD", "$,", "PWAV", "NN", "$,"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.20": {"text": "der Himmel,", "tokens": ["der", "Him\u00b7mel", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.21": {"text": "Eine beugende Last, \u00fcber dem Haupte mir h\u00e4ngt!", "tokens": ["Ei\u00b7ne", "beu\u00b7gen\u00b7de", "Last", ",", "\u00fc\u00b7ber", "dem", "Haup\u00b7te", "mir", "h\u00e4ngt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}}}}}