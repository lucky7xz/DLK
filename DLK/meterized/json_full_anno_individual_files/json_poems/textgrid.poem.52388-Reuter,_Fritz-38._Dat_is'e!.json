 "measure": "iambic.tetra"}, "line.12": {"text": "Dat wull wat Richtiges bed\u00fcden,", "tokens": ["Dat", "wull", "wat", "Rich\u00b7ti\u00b7ges", "be\u00b7d\u00fc\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Dor danzten s' bet an hellig S\u00fcnn,", "tokens": ["Dor", "danz\u00b7ten", "s'", "bet", "an", "hel\u00b7lig", "S\u00fcnn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NE", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Un wer denn nich mihr stahen k\u00fcnn,", "tokens": ["Un", "wer", "denn", "nich", "mihr", "sta\u00b7hen", "k\u00fcnn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWS", "ADV", "PTKNEG", "ADV", "VVFIN", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.15": {"text": "De danzte up den Kopp her\u00fcm.", "tokens": ["De", "danz\u00b7te", "up", "den", "Kopp", "he\u00b7r\u00fcm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NE", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Na, as dat kamm so gegen Morgen,", "tokens": ["Na", ",", "as", "dat", "kamm", "so", "ge\u00b7gen", "Mor\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NE", "ART", "NN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Lett Korl den Kutscher ran besorgen.", "tokens": ["Lett", "Korl", "den", "Kut\u00b7scher", "ran", "be\u00b7sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "NN", "NE", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "De Kutscher h\u00f6llt twei lang, twei breid,", "tokens": ["De", "Kut\u00b7scher", "h\u00f6llt", "twei", "lang", ",", "twei", "breid", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "NE", "ADJD", "$,", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "De Kutscher h\u00f6llt, hei weit Bescheid.", "tokens": ["De", "Kut\u00b7scher", "h\u00f6llt", ",", "hei", "weit", "Be\u00b7scheid", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "$,", "VAFIN", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Nu k\u00fcmmt ein stramm de Trepp hendal,", "tokens": ["Nu", "k\u00fcmmt", "ein", "stramm", "de", "Trepp", "hen\u00b7dal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NE", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "De Husknecht seggt: \u00bbDat is din Herr.\u00ab", "tokens": ["De", "Hus\u00b7knecht", "seggt", ":", "\u00bb", "Dat", "is", "din", "Herr", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NE", "VVFIN", "$.", "$(", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "\u00bbne\u00ab, seggt Jehann, \u00bbdat's anners wer,", "tokens": ["\u00bb", "ne", "\u00ab", ",", "seggt", "Je\u00b7hann", ",", "\u00bb", "dat's", "an\u00b7ners", "wer", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "NE", "$(", "$,", "VVFIN", "NE", "$,", "$(", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Min Herr, de sitt un drinkt noch mal.\u00ab", "tokens": ["Min", "Herr", ",", "de", "sitt", "un", "drinkt", "noch", "mal", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$,", "FM", "FM", "FM", "VVFIN", "ADV", "ADV", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.24": {"text": "En anner k\u00fcmmt in lichten Draww,", "tokens": ["En", "an\u00b7ner", "k\u00fcmmt", "in", "lich\u00b7ten", "Draww", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "So recht behen'n de Trepp heraf,", "tokens": ["So", "recht", "be\u00b7hen'n", "de", "Trepp", "he\u00b7raf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "NE", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "De Husknecht seggt: \u00bbPa\u00df up, Jehann,", "tokens": ["De", "Hus\u00b7knecht", "seggt", ":", "\u00bb", "Pa\u00df", "up", ",", "Je\u00b7hann", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "$.", "$(", "NN", "NE", "$,", "NE", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.27": {"text": "Dit is din Herr!\u00ab \u2013 \u00bbNee\u00ab, seggt de Kutscher,", "tokens": ["Dit", "is", "din", "Herr", "!", "\u00ab", "\u2013", "\u00bb", "Nee", "\u00ab", ",", "seggt", "de", "Kut\u00b7scher", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["FM", "FM", "PPOSAT", "NN", "$.", "$(", "$(", "$(", "NN", "$(", "$,", "VVFIN", "NE", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "\u00bbdat 