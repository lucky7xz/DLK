{"dta.poem.7463": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Xx.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Freiheit Priester, der Vasall des Sch\u00f6nen,", "tokens": ["Der", "Frei\u00b7heit", "Pries\u00b7ter", ",", "der", "Va\u00b7sall", "des", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So wird der Dichter in die Welt gesandt;", "tokens": ["So", "wird", "der", "Dich\u00b7ter", "in", "die", "Welt", "ge\u00b7sandt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Troubadour zieh' er von Land zu Land,", "tokens": ["Ein", "Trou\u00b7ba\u00b7dour", "zieh'", "er", "von", "Land", "zu", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das Herrlichste mit seinem Lied zu kr\u00f6nen.", "tokens": ["Das", "Herr\u00b7lichs\u00b7te", "mit", "sei\u00b7nem", "Lied", "zu", "kr\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Heldenthat gewinn' in seinen T\u00f6nen", "tokens": ["Die", "Hel\u00b7den\u00b7that", "ge\u00b7winn'", "in", "sei\u00b7nen", "T\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fcr alle Zeiten sicheren Bestand,", "tokens": ["F\u00fcr", "al\u00b7le", "Zei\u00b7ten", "si\u00b7che\u00b7ren", "Be\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den eignen Kummer schreib' er in den Sand,", "tokens": ["Den", "eig\u00b7nen", "Kum\u00b7mer", "schreib'", "er", "in", "den", "Sand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Des eignen Herzens m\u00f6g' er sich entw\u00f6hnen.", "tokens": ["Des", "eig\u00b7nen", "Her\u00b7zens", "m\u00f6g'", "er", "sich", "ent\u00b7w\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein G\u00e4rtner, dem der Garten nur gegeben,", "tokens": ["Ein", "G\u00e4rt\u00b7ner", ",", "dem", "der", "Gar\u00b7ten", "nur", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fcr fremde Busen Blumen draus zu pfl\u00fccken,", "tokens": ["F\u00fcr", "frem\u00b7de", "Bu\u00b7sen", "Blu\u00b7men", "draus", "zu", "pfl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "PAV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Winzer, der f\u00fcr Fremde baut die Reben \u2014", "tokens": ["Ein", "Win\u00b7zer", ",", "der", "f\u00fcr", "Frem\u00b7de", "baut", "die", "Re\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sei all sein Trost, nur And're zu begl\u00fccken;", "tokens": ["Sei", "all", "sein", "Trost", ",", "nur", "An\u00b7d'\u00b7re", "zu", "be\u00b7gl\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "PPOSAT", "NN", "$,", "ADV", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dem armen Taucher gleich, wag' er das Leben,", "tokens": ["Dem", "ar\u00b7men", "Tau\u00b7cher", "gleich", ",", "wag'", "er", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Mit seltnen Perlen seine Zeit zu schm\u00fccken.", "tokens": ["Mit", "selt\u00b7nen", "Per\u00b7len", "sei\u00b7ne", "Zeit", "zu", "schm\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}