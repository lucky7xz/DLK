{"textgrid.poem.25067": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Morgen in Athen", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bist du's und bringst vom Lande des Homer,", "tokens": ["Bist", "du's", "und", "bringst", "vom", "Lan\u00b7de", "des", "Ho\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "KON", "VVFIN", "APPRART", "NN", "ART", "NE", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "O Eos, uns den neuen Morgen her,", "tokens": ["O", "E\u00b7os", ",", "uns", "den", "neu\u00b7en", "Mor\u00b7gen", "her", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auf den wir lang vergebens harrten?", "tokens": ["Auf", "den", "wir", "lang", "ver\u00b7ge\u00b7bens", "harr\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPER", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schon auf den Wellen spr\u00fcht vom Himmelsrand", "tokens": ["Schon", "auf", "den", "Wel\u00b7len", "spr\u00fcht", "vom", "Him\u00b7mels\u00b7rand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein ros'ger Schimmer hin und l\u00e4\u00dft am Strand", "tokens": ["Ein", "ros'\u00b7ger", "Schim\u00b7mer", "hin", "und", "l\u00e4\u00dft", "am", "Strand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "KON", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die Bergh\u00f6hn gl\u00fchn, die Felsenwarten.", "tokens": ["Die", "Berg\u00b7h\u00f6hn", "gl\u00fchn", ",", "die", "Fel\u00b7sen\u00b7war\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Erde, lang wie Dantes Trauerstadt", "tokens": ["Die", "Er\u00b7de", ",", "lang", "wie", "Dan\u00b7tes", "Trau\u00b7er\u00b7stadt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADJD", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Sitz des Wehes, ist der Bu\u00dfe satt,", "tokens": ["Ein", "Sitz", "des", "We\u00b7hes", ",", "ist", "der", "Bu\u00b7\u00dfe", "satt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Kreuze und der Hochgerichte;", "tokens": ["Der", "Kreu\u00b7ze", "und", "der", "Hoch\u00b7ge\u00b7rich\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und scheuchen soll die Sonne Griechenlands", "tokens": ["Und", "scheu\u00b7chen", "soll", "die", "Son\u00b7ne", "Grie\u00b7chen\u00b7lands"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "VMFIN", "ART", "NN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Des Mittelalters grausen Totentanz", "tokens": ["Des", "Mit\u00b7tel\u00b7al\u00b7ters", "grau\u00b7sen", "To\u00b7ten\u00b7tanz"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mit ihrem reinen Himmelslichte.", "tokens": ["Mit", "ih\u00b7rem", "rei\u00b7nen", "Him\u00b7mels\u00b7lich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Im Fr\u00fchglanz, siehe, der sich vom Hymett", "tokens": ["Im", "Fr\u00fch\u00b7glanz", ",", "sie\u00b7he", ",", "der", "sich", "vom", "Hy\u00b7mett"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "VVIMP", "$,", "PRELS", "PRF", "APPRART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Herniedersenkt zu des Ilyssus Bett,", "tokens": ["Her\u00b7nie\u00b7der\u00b7senkt", "zu", "des", "I\u00b7lys\u00b7sus", "Bett", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NE", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Aufleuchtet schon dem G\u00f6ttervater", "tokens": ["Auf\u00b7leuch\u00b7tet", "schon", "dem", "G\u00f6t\u00b7ter\u00b7va\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Tempels\u00e4ulenwald, und ersten Blicks", "tokens": ["Der", "Tem\u00b7pel\u00b7s\u00e4u\u00b7len\u00b7wald", ",", "und", "ers\u00b7ten", "Blicks"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Gr\u00fc\u00dft Helios der Athene Bild, die Pnyx", "tokens": ["Gr\u00fc\u00dft", "He\u00b7li\u00b7os", "der", "A\u00b7the\u00b7ne", "Bild", ",", "die", "Pnyx"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "NE", "ART", "ADJA", "NN", "$,", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Und Erechtheum und Theater.", "tokens": ["Und", "E\u00b7recht\u00b7heum", "und", "The\u00b7a\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Und sanfter Schauer geht durch die Natur;", "tokens": ["Und", "sanf\u00b7ter", "Schau\u00b7er", "geht", "durch", "die", "Na\u00b7tur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NE", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Aus Grotten durch den d\u00e4mmernden Azur", "tokens": ["Aus", "Grot\u00b7ten", "durch", "den", "d\u00e4m\u00b7mern\u00b7den", "A\u00b7zur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wei\u00df schimmern der Najaden Glieder;", "tokens": ["Wei\u00df", "schim\u00b7mern", "der", "Na\u00b7ja\u00b7den", "Glie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Pinienhain am Quell Kalirrho\u00eb", "tokens": ["Im", "Pi\u00b7ni\u00b7en\u00b7hain", "am", "Quell", "Ka\u00b7lir\u00b7rho\u00eb"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPRART", "NN", "NE"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Anhebt die Nachtigall in s\u00fc\u00dfem Weh", "tokens": ["An\u00b7hebt", "die", "Nach\u00b7ti\u00b7gall", "in", "s\u00fc\u00b7\u00dfem", "Weh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ihr Klagelied um Itys wieder.", "tokens": ["Ihr", "Kla\u00b7ge\u00b7lied", "um", "I\u00b7tys", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NE", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Neu schlie\u00dfe nun sich das Gymnasium auf,", "tokens": ["Neu", "schlie\u00b7\u00dfe", "nun", "sich", "das", "Gym\u00b7na\u00b7si\u00b7um", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "PRF", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Da\u00df sich im Diskuswurf, im Kampf, im Lauf", "tokens": ["Da\u00df", "sich", "im", "Dis\u00b7kus\u00b7wurf", ",", "im", "Kampf", ",", "im", "Lauf"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PRF", "APPRART", "NN", "$,", "APPRART", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zu J\u00fcnglingskraft der Knabe st\u00e4hle,", "tokens": ["Zu", "J\u00fcng\u00b7lings\u00b7kraft", "der", "Kna\u00b7be", "st\u00e4h\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Am Marmorbild, das auf ihn niedersieht,", "tokens": ["Am", "Mar\u00b7mor\u00b7bild", ",", "das", "auf", "ihn", "nie\u00b7der\u00b7sieht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "U