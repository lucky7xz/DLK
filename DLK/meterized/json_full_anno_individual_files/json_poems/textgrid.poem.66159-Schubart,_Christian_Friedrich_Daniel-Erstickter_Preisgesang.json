{"textgrid.poem.66159": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Erstickter Preisgesang", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Singen will ich, Sch\u00f6pfer, singen", "tokens": ["Sin\u00b7gen", "will", "ich", ",", "Sch\u00f6p\u00b7fer", ",", "sin\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["NN", "VMFIN", "PPER", "$,", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dir mit heiterem Gem\u00fcth;", "tokens": ["Dir", "mit", "hei\u00b7te\u00b7rem", "Ge\u00b7m\u00fcth", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hell, wie Waldgesang, erklingen", "tokens": ["Hell", ",", "wie", "Wald\u00b7ge\u00b7sang", ",", "er\u00b7klin\u00b7gen"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["NE", "$,", "PWAV", "NN", "$,", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Soll vor dir, o Gott! mein Lied.", "tokens": ["Soll", "vor", "dir", ",", "o", "Gott", "!", "mein", "Lied", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPER", "$,", "FM", "NN", "$.", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Woge, Geist, in mir, frohlocke,", "tokens": ["Wo\u00b7ge", ",", "Geist", ",", "in", "mir", ",", "froh\u00b7lo\u00b7cke", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "APPR", "PPER", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und zerflie\u00df' in Lobgesang;", "tokens": ["Und", "zer\u00b7flie\u00df'", "in", "Lob\u00b7ge\u00b7sang", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "T\u00f6ne wie die Silberglocke,", "tokens": ["T\u00f6\u00b7ne", "wie", "die", "Sil\u00b7ber\u00b7glo\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Brause wie der Orgel Klang.", "tokens": ["Brau\u00b7se", "wie", "der", "Or\u00b7gel", "Klang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Geister, die wie Feuerflammen", "tokens": ["Geis\u00b7ter", ",", "die", "wie", "Feu\u00b7er\u00b7flam\u00b7men"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "KOKOM", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um den Thron des H\u00f6chsten stehn,", "tokens": ["Um", "den", "Thron", "des", "H\u00f6chs\u00b7ten", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Engel, Menschen, singt zusammen;", "tokens": ["En\u00b7gel", ",", "Men\u00b7schen", ",", "singt", "zu\u00b7sam\u00b7men", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Helft mir meinen Gott erh\u00f6hn!", "tokens": ["Helft", "mir", "mei\u00b7nen", "Gott", "er\u00b7h\u00f6hn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hallt Posaunen, Davids Psalter,", "tokens": ["Hallt", "Po\u00b7sau\u00b7nen", ",", "Da\u00b7vids", "Psal\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "NN", "$,", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Harfe, die Eloa schlug,", "tokens": ["Har\u00b7fe", ",", "die", "E\u00b7loa", "schlug", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "NE", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "T\u00f6nt dem Sch\u00f6pfer, dem Erhalter!", "tokens": ["T\u00f6nt", "dem", "Sch\u00f6p\u00b7fer", ",", "dem", "Er\u00b7hal\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Doch ihr t\u00f6nt nicht laut genug.", "tokens": ["Doch", "ihr", "t\u00f6nt", "nicht", "laut", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "ADJD", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Thier' in W\u00e4ldern und in Meeren,", "tokens": ["Thier'", "in", "W\u00e4l\u00b7dern", "und", "in", "Mee\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "V\u00f6gel in der Luft, im Hain,", "tokens": ["V\u00f6\u00b7gel", "in", "der", "Luft", ",", "im", "Hain", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Preist ihn all'; ihr Christenz\u00e4hren,", "tokens": ["Preist", "ihn", "all'", ";", "ihr", "Chris\u00b7ten\u00b7z\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "PIS", "$.", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Str\u00f6mt voll Dank und Wonne drein.", "tokens": ["Str\u00f6mt", "voll", "Dank", "und", "Won\u00b7ne", "drein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber \u2013 Weh'! wie schmerzt die Wunde \u2013", "tokens": ["A\u00b7ber", "\u2013", "Weh'", "!", "wie", "schmerzt", "die", "Wun\u00b7de", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "NN", "$.", "PWAV", "ADJD", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ach! mich Armen traf ein Pfeil;", "tokens": ["Ach", "!", "mich", "Ar\u00b7men", "traf", "ein", "Pfeil", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Der Gesang erstickt im Munde,", "tokens": ["Der", "Ge\u00b7sang", "er\u00b7stickt", "im", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wandelt sich und wird Geheul.", "tokens": ["Wan\u00b7delt", "sich", "und", "wird", "Ge\u00b7heul", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "KON", "VAFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sieh dich um, du bist gefangen \u2013", "tokens": ["Sieh", "dich", "um", ",", "du", "bist", "ge\u00b7fan\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "$,", "PPER", "VAFIN", "ADJD", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der Gedanke st\u00fcrzt auf mich;", "tokens": ["Der", "Ge\u00b7dan\u00b7ke", "st\u00fcrzt", "auf", "mich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieh am Arm die Fessel hangen,", "tokens": ["Sieh", "am", "Arm", "die", "Fes\u00b7sel", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sieh die braune Wand um dich!", "tokens": ["Sieh", "die", "brau\u00b7ne", "Wand", "um", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ha! ich seh' das Nachtgefieder", "tokens": ["Ha", "!", "ich", "seh'", "das", "Nacht\u00b7ge\u00b7fie\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ausgebreitet \u00fcber mir;", "tokens": ["Aus\u00b7ge\u00b7brei\u00b7tet", "\u00fc\u00b7ber", "mir", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Gott! ach Gott! ich st\u00fcrze nieder,", "tokens": ["Gott", "!", "ach", "Gott", "!", "ich", "st\u00fcr\u00b7ze", "nie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "XY", "NN", "$.", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und mein Lied verstummt vor dir!", "tokens": ["Und", "mein", "Lied", "ver\u00b7stummt", "vor", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "So beginnt im Morgenstrahle", "tokens": ["So", "be\u00b7ginnt", "im", "Mor\u00b7gen\u00b7strah\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Oft des Finken Lobgesang;", "tokens": ["Oft", "des", "Fin\u00b7ken", "Lob\u00b7ge\u00b7sang", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach! er sieht im nahen Thale", "tokens": ["Ach", "!", "er", "sieht", "im", "na\u00b7hen", "Tha\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht des Vogelm\u00f6rders Gang!", "tokens": ["Nicht", "des", "Vo\u00b7gel\u00b7m\u00f6r\u00b7ders", "Gang", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Pl\u00f6tzlich aus dem ehrnen Schlunde", "tokens": ["Pl\u00f6tz\u00b7lich", "aus", "dem", "ehr\u00b7nen", "Schlun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Fliegt der m\u00f6rderische Schrot;", "tokens": ["Fliegt", "der", "m\u00f6r\u00b7de\u00b7ri\u00b7sche", "Schrot", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Blutig, mit geschlo\u00dfnem Munde,", "tokens": ["Blu\u00b7tig", ",", "mit", "ge\u00b7schlo\u00df\u00b7nem", "Mun\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Liegt der arme Vogel todt.", "tokens": ["Liegt", "der", "ar\u00b7me", "Vo\u00b7gel", "todt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NE", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Singen will ich, Sch\u00f6pfer, singen", "tokens": ["Sin\u00b7gen", "will", "ich", ",", "Sch\u00f6p\u00b7fer", ",", "sin\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["NN", "VMFIN", "PPER", "$,", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dir mit heiterem Gem\u00fcth;", "tokens": ["Dir", "mit", "hei\u00b7te\u00b7rem", "Ge\u00b7m\u00fcth", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hell, wie Waldgesang, erklingen", "tokens": ["Hell", ",", "wie", "Wald\u00b7ge\u00b7sang", ",", "er\u00b7klin\u00b7gen"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["NE", "$,", "PWAV", "NN", "$,", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Soll vor dir, o Gott! mein Lied.", "tokens": ["Soll", "vor", "dir", ",", "o", "Gott", "!", "mein", "Lied", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "wo