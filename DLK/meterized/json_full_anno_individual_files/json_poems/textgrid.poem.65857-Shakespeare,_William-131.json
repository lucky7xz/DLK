{"textgrid.poem.65857": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "131", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist so herrisch, Herz! f\u00fcr deine Gaben,", "tokens": ["Du", "bist", "so", "her\u00b7risch", ",", "Herz", "!", "f\u00fcr", "dei\u00b7ne", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "NN", "$.", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie andre, die ihr Liebreiz grausam macht:", "tokens": ["Wie", "and\u00b7re", ",", "die", "ihr", "Lieb\u00b7reiz", "grau\u00b7sam", "macht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "$,", "PRELS", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denn wohl wei\u00dft du, mir liebekrankem Knaben", "tokens": ["Denn", "wohl", "wei\u00dft", "du", ",", "mir", "lie\u00b7be\u00b7kran\u00b7kem", "Kna\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bist du mein sch\u00f6nstes Kleinod, mein Smaragd.", "tokens": ["Bist", "du", "mein", "sch\u00f6ns\u00b7tes", "Klei\u00b7nod", ",", "mein", "Sma\u00b7ragd", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und doch, traun, mancher sagt, der dein Gesicht", "tokens": ["Und", "doch", ",", "tr