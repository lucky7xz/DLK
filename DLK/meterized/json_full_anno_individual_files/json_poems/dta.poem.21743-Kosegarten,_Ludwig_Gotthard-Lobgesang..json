{"dta.poem.21743": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Lobgesang.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Schwebt empor,", "tokens": ["Schwebt", "em\u00b7por", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Lispel meiner Lieder!", "tokens": ["Lis\u00b7pel", "mei\u00b7ner", "Lie\u00b7der", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Hallt sie wieder,", "tokens": ["Hallt", "sie", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Nacht und Sternenchor!", "tokens": ["Nacht", "und", "Ster\u00b7nen\u00b7chor", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Wer hat die Bange, Bl\u00f6de", "tokens": ["Wer", "hat", "die", "Ban\u00b7ge", ",", "Bl\u00f6\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PWS", "VAFIN", "ART", "NN", "$,", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In schauerlicher \u00d6de", "tokens": ["In", "schau\u00b7er\u00b7li\u00b7cher", "\u00d6\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Geschirmet und gewahrt?", "tokens": ["Ge\u00b7schir\u00b7met", "und", "ge\u00b7wahrt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wer in den Felsgerippen,", "tokens": ["Wer", "in", "den", "Fels\u00b7ge\u00b7rip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Riss geborstner Klippen", "tokens": ["Im", "Riss", "ge\u00b7borst\u00b7ner", "Klip\u00b7pen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr manchen Trunk gespart?", "tokens": ["Ihr", "man\u00b7chen", "Trunk", "ge\u00b7spart", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wer sparte, sie zu n\u00e4hren,", "tokens": ["Wer", "spar\u00b7te", ",", "sie", "zu", "n\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Des Strauches rothe Beeren,", "tokens": ["Des", "Strau\u00b7ches", "ro\u00b7the", "Bee\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das Ey im Adlerhorst?", "tokens": ["Das", "Ey", "im", "Ad\u00b7ler\u00b7horst", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wer liess, f\u00fcr mich zu quellen,", "tokens": ["Wer", "liess", ",", "f\u00fcr", "mich", "zu", "quel\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Des Isards Euter schwellen,", "tokens": ["Des", "I\u00b7sards", "Eu\u00b7ter", "schwel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NE", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Als meine Lippe borst?", "tokens": ["Als", "mei\u00b7ne", "Lip\u00b7pe", "borst", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}