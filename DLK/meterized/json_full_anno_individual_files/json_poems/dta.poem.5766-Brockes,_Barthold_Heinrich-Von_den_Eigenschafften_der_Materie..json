{"dta.poem.5766": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Eigenschafften der  \n Materie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nimm ein so kleines Theil, das unsre Augen", "tokens": ["Nimm", "ein", "so", "klei\u00b7nes", "Theil", ",", "das", "uns\u00b7re", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ART", "ADV", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es nimmer zu erblicken taugen;", "tokens": ["Es", "nim\u00b7mer", "zu", "er\u00b7bli\u00b7cken", "tau\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So mercket man von einiger Figur", "tokens": ["So", "mer\u00b7cket", "man", "von", "ei\u00b7ni\u00b7ger", "Fi\u00b7gur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch eine Spur.", "tokens": ["Doch", "ei\u00b7ne", "Spur", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Dieselbige Figur hat ihre Seiten,", "tokens": ["Die\u00b7sel\u00b7bi\u00b7ge", "Fi\u00b7gur", "hat", "ih\u00b7re", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Hat Ober-Unter-Theil, hat Ecken, L\u00e4ng\u2019 und Breiten.", "tokens": ["Hat", "O\u00b7ber\u00b7Un\u00b7ter\u00b7T\u00b7heil", ",", "hat", "E\u00b7cken", ",", "L\u00e4ng'", "und", "Brei\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "VAFIN", "NN", "$,", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Durch die verschiedne Gr\u00f6ss\u2019, aus welcher sie besteht", "tokens": ["Durch", "die", "ver\u00b7schied\u00b7ne", "Gr\u00f6ss'", ",", "aus", "wel\u00b7cher", "sie", "be\u00b7steht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und welche wir verschiedlich messen k\u00f6nnen,", "tokens": ["Und", "wel\u00b7che", "wir", "ver\u00b7schied\u00b7lich", "mes\u00b7sen", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADJD", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Kan man dieselbe wieder trennen.", "tokens": ["Kan", "man", "die\u00b7sel\u00b7be", "wie\u00b7der", "tren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PDAT", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}