gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schmetterling' und V\u00f6gel jagen,", "tokens": ["Schmet\u00b7ter\u00b7ling'", "und", "V\u00f6\u00b7gel", "ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo wir irgend einen seh'n.", "tokens": ["Wo", "wir", "ir\u00b7gend", "ei\u00b7nen", "seh'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["PWAV", "PPER", "ADV", "ART", "XY", "XY"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alles soll vor uns erschrecken,", "tokens": ["Al\u00b7les", "soll", "vor", "uns", "er\u00b7schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Soll vor uns die Waffen strecken,", "tokens": ["Soll", "vor", "uns", "die", "Waf\u00b7fen", "stre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Alles soll es seh'n geschwind,", "tokens": ["Al\u00b7les", "soll", "es", "seh'n", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df wir tapfre Helden sind.", "tokens": ["Da\u00df", "wir", "tapf\u00b7re", "Hel\u00b7den", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}