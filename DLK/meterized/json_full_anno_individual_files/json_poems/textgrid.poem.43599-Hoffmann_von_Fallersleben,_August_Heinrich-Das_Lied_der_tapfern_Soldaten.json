 "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "NN", "APPR", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Jeder mu\u00df ja sein ein Held,", "tokens": ["Je\u00b7der", "mu\u00df", "ja", "sein", "ein", "Held", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "PPOSAT", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Weil wir jetzo zieh'n ins Feld.", "tokens": ["Weil", "wir", "jet\u00b7zo", "zieh'n", "ins", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Seht, wie sind wir ausstaffiret!", "tokens": ["Seht", ",", "wie", "sind", "wir", "aus\u00b7staf\u00b7fi\u00b7ret", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Jeder ist ein Offizier.", "tokens": ["Je\u00b7der", "ist", "ein", "Of\u00b7fi\u00b7zier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Helm und Harnisch sind gezieret", "tokens": ["Helm", "und", "Har\u00b7nisch", "sind", "ge\u00b7zie\u00b7ret"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wundersch\u00f6n mit Goldpapier.", "tokens": ["Wun\u00b7der\u00b7sch\u00f6n", "mit", "Gold\u00b7pa\u00b7pier", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Vorw\u00e4rts geht es recht verwegen", "tokens": ["Vor\u00b7w\u00e4rts", "geht", "es", "recht", "ver\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit der Flinte, mit dem Degen", "tokens": ["Mit", "der", "Flin\u00b7te", ",", "mit", "dem", "De\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Auf den Feind, Feld aus Feld ein,", "tokens": ["Auf", "den", "Feind", ",", "Feld", "aus", "Feld", "ein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "NN", "APPR", "NN", "PTKVZ", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "In die weite Welt hinein.", "tokens": ["In", "die", "wei\u00b7te", "Welt", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Schie\u00dfen kann ich, da\u00df die Spatzen", "tokens": ["Schie\u00b7\u00dfen", "kann", "ich", ",", "da\u00df", "die", "Spat\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fliegen fort wie Spreu und Stroh,", "tokens": ["Flie\u00b7gen", "fort", "wie", "Spreu", "und", "Stroh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KOKOM", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es laufen Hund' und Katzen,", "tokens": ["Und", "es", "lau\u00b7fen", "H