{"textgrid.poem.35478": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Marie Louise", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du fragst, was mir so herbe, tiefe Falten", "tokens": ["Du", "fragst", ",", "was", "mir", "so", "her\u00b7be", ",", "tie\u00b7fe", "Fal\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "ADV", "VVFIN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In meine junge Stirne gr\u00e4bt?", "tokens": ["In", "mei\u00b7ne", "jun\u00b7ge", "Stir\u00b7ne", "gr\u00e4bt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was mich so pl\u00f6tzlich macht erkalten?", "tokens": ["Was", "mich", "so", "pl\u00f6tz\u00b7lich", "macht", "er\u00b7kal\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was mich durchbebt,", "tokens": ["Was", "mich", "durch\u00b7bebt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Da\u00df ich dich an mich rei\u00dfen will \u2013", "tokens": ["Da\u00df", "ich", "dich", "an", "mich", "rei\u00b7\u00dfen", "will", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPER", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In hei\u00dfer Leidenschaft dich an mich press