.", "tokens": ["Ist", "war\u00b7lich", "Thie\u00b7ren", "gleich", "zu", "sch\u00e4z\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.89": {"text": "Drum sieh des H\u00f6chsten Allmachts-Hand", "tokens": ["Drum", "sieh", "des", "H\u00f6chs\u00b7ten", "All\u00b7machts\u00b7Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.90": {"text": "Beschaue die geschafnen Wunder,", "tokens": ["Be\u00b7schau\u00b7e", "die", "ge\u00b7schaf\u00b7nen", "Wun\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.91": {"text": "Entz\u00fcnd daran den Andachts-Zunder;", "tokens": ["Ent\u00b7z\u00fcnd", "da\u00b7ran", "den", "An\u00b7dachts\u00b7Zun\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.92": {"text": "Und wenn des Herzens heilger Brand", "tokens": ["Und", "wenn", "des", "Her\u00b7zens", "heil\u00b7ger", "Brand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.93": {"text": "Auch deine Zunge feurig r\u00fchret,", "tokens": ["Auch", "dei\u00b7ne", "Zun\u00b7ge", "feu\u00b7rig", "r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.94": {"text": "So preise ", "tokens": ["So", "prei\u00b7se"], "token_info": ["word", "word"], "pos": ["ADV", "NN"], "meter": "-+-", "measure": "amphibrach.single"}}}}}