{"dta.poem.18181": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Woher es komme, da\u00df so weni-  \n ge die herrlichen Gesch\u00f6pfe GOttes be-  \n trachten, und dadurch ger\u00fchret  \n werden?", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198805", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Menschen m\u00fcssen eingestehn,", "tokens": ["Die", "Men\u00b7schen", "m\u00fcs\u00b7sen", "ein\u00b7ge\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df in dem sch\u00f6nen Kreaturen", "tokens": ["Da\u00df", "in", "dem", "sch\u00f6\u00b7nen", "Kre\u00b7a\u00b7tu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Weisheit, G\u00fct und Allmachts", "tokens": ["Der", "Weis\u00b7heit", ",", "G\u00fct", "und", "All\u00b7machts"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Spuren,", "tokens": ["Spu\u00b7ren", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Vom grossen Sch\u00f6pfer anzusehn:", "tokens": ["Vom", "gros\u00b7sen", "Sch\u00f6p\u00b7fer", "an\u00b7zu\u00b7sehn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVIZU", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch da\u00df man sie gar wenig achtet,", "tokens": ["Doch", "da\u00df", "man", "sie", "gar", "we\u00b7nig", "ach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "PPER", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und nicht wie sichs geb\u00fchrt betrachtet,", "tokens": ["Und", "nicht", "wie", "sichs", "ge\u00b7b\u00fchrt", "be\u00b7trach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "KOKOM", "PIS", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das ist auch mehr, als allzu wahr;", "tokens": ["Das", "ist", "auch", "mehr", ",", "als", "all\u00b7zu", "wahr", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "$,", "KOUS", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Will man davon die Ursach finden,", "tokens": ["Will", "man", "da\u00b7von", "die", "Ur\u00b7sach", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Der Tr\u00e4gheit wahre Quell ergr\u00fcnden;", "tokens": ["Der", "Tr\u00e4g\u00b7heit", "wah\u00b7re", "Quell", "er\u00b7gr\u00fcn\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "So ist es, wie mir deucht ganz klar,", "tokens": ["So", "ist", "es", ",", "wie", "mir", "deucht", "ganz", "klar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "PWAV", "PPER", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Da\u00df aus Gewohnheit unsre Augen,", "tokens": ["Da\u00df", "aus", "Ge\u00b7wohn\u00b7heit", "uns\u00b7re", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Die Sch\u00f6nheit nicht zu sehen taugen,", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "nicht", "zu", "se\u00b7hen", "tau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Die man doch allenthalben sieht;", "tokens": ["Die", "man", "doch", "al\u00b7len\u00b7thal\u00b7ben", "sieht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Da\u00df der Gewohnheits Schlummer bleibet,", "tokens": ["Da\u00df", "der", "Ge\u00b7wohn\u00b7heits", "Schlum\u00b7mer", "blei\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Man ihn nicht aus dem Augen reibet;", "tokens": ["Man", "ihn", "nicht", "aus", "dem", "Au\u00b7gen", "rei\u00b7bet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PPER", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.17": {"text": "Da\u00df man nicht Vorurtheile flieht:", "tokens": ["Da\u00df", "man", "nicht", "Vor\u00b7urt\u00b7hei\u00b7le", "flieht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKNEG", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Da\u00df macht, weil wir die Welt nicht kennen,", "tokens": ["Da\u00df", "macht", ",", "weil", "wir", "die", "Welt", "nicht", "ken\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Das schlecht und woll gar B\u00f6se nennen", "tokens": ["Das", "schlecht", "und", "woll", "gar", "B\u00f6\u00b7se", "nen\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "KON", "ADV", "ADV", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Wie die Unwissenheit sonst spricht;", "tokens": ["Wie", "die", "Un\u00b7wis\u00b7sen\u00b7heit", "sonst", "spricht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Davon wir, weil wirs nicht verstehen,", "tokens": ["Da\u00b7von", "wir", ",", "weil", "wirs", "nicht", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "$,", "KOUS", "PIS", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Nicht alsobald den Nuzzen sehen.", "tokens": ["Nicht", "al\u00b7so\u00b7bald", "den", "Nuz\u00b7zen", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Man sieht die Sonn, des Himmels Licht,", "tokens": ["Man", "sieht", "die", "Sonn", ",", "des", "Him\u00b7mels", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Den schreklich grossen Feuerballen,", "tokens": ["Den", "schrek\u00b7lich", "gros\u00b7sen", "Feu\u00b7er\u00b7bal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Daraus die Strahlen zu uns prallen,", "tokens": ["Da\u00b7raus", "die", "Strah\u00b7len", "zu", "uns", "pral\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Als einen runden Teller an;", "tokens": ["Als", "ei\u00b7nen", "run\u00b7den", "Tel\u00b7ler", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Da man mit vielen tausend Meilen,", "tokens": ["Da", "man", "mit", "vie\u00b7len", "tau\u00b7send", "Mei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PIAT", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Wenn wir sie nach dem Durchschnitt theilen,", "tokens": ["Wenn", "wir", "sie", "nach", "dem", "Durch\u00b7schnitt", "thei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Nicht deren Gr\u00f6\u00df ermessen kann;", "tokens": ["Nicht", "de\u00b7ren", "Gr\u00f6\u00df", "er\u00b7mes\u00b7sen", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PRELAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Dagegen unser Kreis der Erden,", "tokens": ["Da\u00b7ge\u00b7gen", "un\u00b7ser", "Kreis", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Nicht einmahl kan verglichen werden.", "tokens": ["Nicht", "ein\u00b7mahl", "kan", "ver\u00b7gli\u00b7chen", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Die Einfalt gl\u00e4ubt der Hinimel sei,", "tokens": ["Die", "Ein\u00b7falt", "gl\u00e4ubt", "der", "Hi\u00b7ni\u00b7mel", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.33": {"text": "Als wie ein aufgesezter Bogen,", "tokens": ["Als", "wie", "ein", "auf\u00b7ge\u00b7sez\u00b7ter", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Nur \u00fcber unsre Erd gezogen;", "tokens": ["Nur", "\u00fc\u00b7ber", "uns\u00b7re", "Erd", "ge\u00b7zo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Die Sterne w\u00e4ren einerlei,", "tokens": ["Die", "Ster\u00b7ne", "w\u00e4\u00b7ren", "ei\u00b7ner\u00b7lei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "So wie der Augenschein uns weiset,", "tokens": ["So", "wie", "der", "Au\u00b7gen\u00b7schein", "uns", "wei\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Da jeder um den Himmel reiset,", "tokens": ["Da", "je\u00b7der", "um", "den", "Him\u00b7mel", "rei\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Bis er den vorgen Plaz einnimt:", "tokens": ["Bis", "er", "den", "vor\u00b7gen", "Plaz", "ein\u00b7nimt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Sie gl\u00e4ubt, da\u00df jeder dieser Sterne,", "tokens": ["Sie", "gl\u00e4ubt", ",", "da\u00df", "je\u00b7der", "die\u00b7ser", "Ster\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PIS", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "An jenes Himmels blauer Ferne,", "tokens": ["An", "je\u00b7nes", "Him\u00b7mels", "blau\u00b7er", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Als wie ein kleiner Lampen glimmt,", "tokens": ["Als", "wie", "ein", "klei\u00b7ner", "Lam\u00b7pen", "glimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Der wie ein Pechlicht bei den Rennen,", "tokens": ["Der", "wie", "ein", "Pech\u00b7licht", "bei", "den", "Ren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Dennoch in steten Schein und Brennen,", "tokens": ["Den\u00b7noch", "in", "ste\u00b7ten", "Schein", "und", "Bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "Sich bis er ausgebrandt, erh\u00e4lt.", "tokens": ["Sich", "bis", "er", "aus\u00b7ge\u00b7brandt", ",", "er\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PRF", "APPR", "PPER", "VVPP", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Die Erde ist nach den Gedanken,", "tokens": ["Die", "Er\u00b7de", "ist", "nach", "den", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.46": {"text": "Ein Kreis der in gar engen Schranken,", "tokens": ["Ein", "Kreis", "der", "in", "gar", "en\u00b7gen", "Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.47": {"text": "Als eine Fl\u00e4che eingespannt;", "tokens": ["Als", "ei\u00b7ne", "Fl\u00e4\u00b7che", "ein\u00b7ge\u00b7spannt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Die Fr\u00fcchte die darauf entstehen,", "tokens": ["Die", "Fr\u00fcch\u00b7te", "die", "da\u00b7rauf", "ent\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.49": {"text": "Zur Reiffe kommen und vergehen,", "tokens": ["Zur", "Reif\u00b7fe", "kom\u00b7men", "und", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.50": {"text": "Die Werke der Natur genannt,", "tokens": ["Die", "Wer\u00b7ke", "der", "Na\u00b7tur", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Die sind so wie die Einfalt denket,", "tokens": ["Die", "sind", "so", "wie", "die", "Ein\u00b7falt", "den\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.52": {"text": "Wie sie die Zeit des Jahres schenket", "tokens": ["Wie", "sie", "die", "Zeit", "des", "Jah\u00b7res", "schen\u00b7ket"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.53": {"text": "Und dabei ist man schon vergn\u00fcgt;", "tokens": ["Und", "da\u00b7bei", "ist", "man", "schon", "ver\u00b7gn\u00fcgt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VAFIN", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.54": {"text": "Wer aber nach der Warheit ringet,", "tokens": ["Wer", "a\u00b7ber", "nach", "der", "War\u00b7heit", "rin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.55": {"text": "Und in der Dinge Wesen dringet,", "tokens": ["Und", "in", "der", "Din\u00b7ge", "We\u00b7sen", "drin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.56": {"text": "Den Wahn des P\u00f6bels nur besiegt,", "tokens": ["Den", "Wahn", "des", "P\u00f6\u00b7bels", "nur", "be\u00b7siegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.57": {"text": "Dem werden an den Himmels Spheren,", "tokens": ["Dem", "wer\u00b7den", "an", "den", "Him\u00b7mels", "Sphe\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.58": {"text": "So viele tausend Wunder lehren,", "tokens": ["So", "vie\u00b7le", "tau\u00b7send", "Wun\u00b7der", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.59": {"text": "Was ", "tokens": ["Was"], "token_info": ["word"], "pos": ["PWS"], "meter": "-", "measure": "single.down"}, "line.60": {"text": "Der wird erstaunt ob diesen Kreisen,", "tokens": ["Der", "wird", "er\u00b7staunt", "ob", "die\u00b7sen", "Krei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "KOUS", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.61": {"text": "Daran er in georndten Gleisen,", "tokens": ["Da\u00b7ran", "er", "in", "ge\u00b7ornd\u00b7ten", "Glei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.62": {"text": "Sieht grosse Himmels K\u00f6rper an.", "tokens": ["Sieht", "gros\u00b7se", "Him\u00b7mels", "K\u00f6r\u00b7per", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.63": {"text": "Wer so mit Andacht \u00fcberleget,", "tokens": ["Wer", "so", "mit", "An\u00b7dacht", "\u00fc\u00b7ber\u00b7le\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.64": {"text": "Was unser Ball der Erde tr\u00e4get", "tokens": ["Was", "un\u00b7ser", "Ball", "der", "Er\u00b7de", "tr\u00e4\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPOSAT", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.65": {"text": "Und ihre Wunder Dinge z\u00e4hlt,", "tokens": ["Und", "ih\u00b7re", "Wun\u00b7der", "Din\u00b7ge", "z\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.66": {"text": "Der wird mit Ehrfurchts vollen Grauen,", "tokens": ["Der", "wird", "mit", "Ehr\u00b7furchts", "vol\u00b7len", "Grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.67": {"text": "Jm Kleinen grosse Dinge schauen,", "tokens": ["Jm", "Klei\u00b7nen", "gros\u00b7se", "Din\u00b7ge", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.68": {"text": "Die der zu seinem Ruhm erw\u00e4hlt,", "tokens": ["Die", "der", "zu", "sei\u00b7nem", "Ruhm", "er\u00b7w\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.69": {"text": "Der alles, was wir sehn und h\u00f6ren", "tokens": ["Der", "al\u00b7les", ",", "was", "wir", "sehn", "und", "h\u00f6\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "$,", "PRELS", "PPER", "VVINF", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.70": {"text": "Die ewge GOttheit zu verehren,", "tokens": ["Die", "ew\u00b7ge", "Got\u00b7theit", "zu", "ver\u00b7eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.71": {"text": "Uns sichtbar vor die Augen stellt:", "tokens": ["Uns", "sicht\u00b7bar", "vor", "die", "Au\u00b7gen", "stellt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.72": {"text": "Der wird mit Lust das alles sehen,", "tokens": ["Der", "wird", "mit", "Lust", "das", "al\u00b7les", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "NN", "ART", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.73": {"text": "Und \u00fcberzeugend eingestehen,", "tokens": ["Und", "\u00fc\u00b7berz\u00b7eu\u00b7gend", "ein\u00b7ge\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.74": {"text": "Da\u00df alles herrlich in der Welt,", "tokens": ["Da\u00df", "al\u00b7les", "herr\u00b7lich", "in", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.75": {"text": "Da\u00df alle Dinge zu erwegen,", "tokens": ["Da\u00df", "al\u00b7le", "Din\u00b7ge", "zu", "er\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.76": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.77": {"text": "Nun lieber Mensch! erwege das,", "tokens": ["Nun", "lie\u00b7ber", "Mensch", "!", "er\u00b7we\u00b7ge", "das", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "$.", "VVFIN", "PDS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.78": {"text": "Was unser Sch\u00f6pfer uns gewiesen,", "tokens": ["Was", "un\u00b7ser", "Sch\u00f6p\u00b7fer", "uns", "ge\u00b7wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.79": {"text": "Denn seine Hoheit wird gepriesen,", "tokens": ["Denn", "sei\u00b7ne", "Ho\u00b7heit", "wird", "ge\u00b7prie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.80": {"text": "Jm kleinsten Kraut, im feinsten Gra\u00df:", "tokens": ["Jm", "kleins\u00b7ten", "Kraut", ",", "im", "feins\u00b7ten", "Gra\u00df", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.81": {"text": "Du lebst im Inbegrif der Erden,", "tokens": ["Du", "lebst", "im", "In\u00b7b