{"textgrid.poem.67043": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf einer Heide geschrieben", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl mir! da\u00df ich den Schwarm der Toren nimmer erblicke,", "tokens": ["Wohl", "mir", "!", "da\u00df", "ich", "den", "Schwarm", "der", "To\u00b7ren", "nim\u00b7mer", "er\u00b7bli\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$.", "KOUS", "PPER", "ART", "NN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da\u00df jetzt unumw\u00f6lkter der Blick zu den L\u00fcften emporschaut,", "tokens": ["Da\u00df", "jetzt", "un\u00b7um\u00b7w\u00f6lk\u00b7ter", "der", "Blick", "zu", "den", "L\u00fcf\u00b7ten", "em\u00b7por\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+--+--+---+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Freier atmet die Brust dann in den Mauren des Elends,", "tokens": ["Frei\u00b7er", "at\u00b7met", "die", "Brust", "dann", "in", "den", "Mau\u00b7ren", "des", "E\u00b7lends", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Und den Winkeln des Trugs. O! sch\u00f6ne, selige Stunde!", "tokens": ["Und", "den", "Win\u00b7keln", "des", "Trugs", ".", "O", "!", "sch\u00f6\u00b7ne", ",", "se\u00b7li\u00b7ge", "Stun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$.", "NE", "$.", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Wie getrennte Geliebte nach langentbehrter Umarmung", "tokens": ["Wie", "ge\u00b7trenn\u00b7te", "Ge\u00b7lieb\u00b7te", "nach", "lan\u00b7gent\u00b7behr\u00b7ter", "Um\u00b7ar\u00b7mung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "+-+--+----+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "In die Arme sich st\u00fcrzen, so eilt ich herauf auf die Heide,", "tokens": ["In", "die", "Ar\u00b7me", "sich", "st\u00fcr\u00b7zen", ",", "so", "eilt", "ich", "her\u00b7auf", "auf", "die", "Hei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "VVINF", "$,", "ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "--+--+--+---+-+-", "measure": "anapaest.tri.plus"}, "line.7": {"text": "Mir ein Fest zu bereiten auf meiner einsamen Heide.", "tokens": ["Mir", "ein", "Fest", "zu", "be\u00b7rei\u00b7ten", "auf", "mei\u00b7ner", "ein\u00b7sa\u00b7men", "Hei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PTKZU", "VVINF", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "--+--+--+--+-+-", "measure": "anapaest.tetra.plus"}, "line.8": {"text": "Und ich habe sie wieder gefunden, die stille Freuden", "tokens": ["Und", "ich", "ha\u00b7be", "sie", "wie\u00b7der", "ge\u00b7fun\u00b7den", ",", "die", "stil\u00b7le", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,", "ART", "ADJA", "NN"], "meter": "--+--+--+--+-+-", "measure": "anapaest.tetra.plus"}, "line.9": {"text": "Alle wieder gefunden, und meine schattigten Eichen", "tokens": ["Al\u00b7le", "wie\u00b7der", "ge\u00b7fun\u00b7den", ",", "und", "mei\u00b7ne", "schat\u00b7tig\u00b7ten", "Ei\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "ADV", "VVPP", "$,", "KON", "PPOSAT", "ADJA", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Stehn noch eben so k\u00f6niglich da, umd\u00e4mmern die Heide", "tokens": ["Stehn", "noch", "e\u00b7ben", "so", "k\u00f6\u00b7nig\u00b7lich", "da", ",", "um\u00b7d\u00e4m\u00b7mern", "die", "Hei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADV", "ADV", "ADV", "ADJD", "PTKVZ", "$,", "VVFIN", "ART", "NN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.11": {"text": "Noch in alten stattlichen Reihn, die schattigten Eichen.", "tokens": ["Noch", "in", "al\u00b7ten", "statt\u00b7li\u00b7chen", "Reihn", ",", "die", "schat\u00b7tig\u00b7ten", "Ei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.12": {"text": "Jedesmal wandelt an meinen tausendj\u00e4hrigen Eichen", "tokens": ["Je\u00b7des\u00b7mal", "wan\u00b7delt", "an", "mei\u00b7nen", "tau\u00b7send\u00b7j\u00e4h\u00b7ri\u00b7gen", "Ei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.13": {"text": "Mit entbl\u00f6\u00dftem Haupt der J\u00e4ger vor\u00fcber, dann also", "tokens": ["Mit", "ent\u00b7bl\u00f6\u00df\u00b7tem", "Haupt", "der", "J\u00e4\u00b7ger", "vor\u00b7\u00fc\u00b7ber", ",", "dann", "al\u00b7so"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "PTKVZ", "$,", "ADV", "ADV"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "Heischet die l\u00e4ndliche Sage; denn unter den stattlichen Reihen", "tokens": ["Hei\u00b7schet", "die", "l\u00e4nd\u00b7li\u00b7che", "Sa\u00b7ge", ";", "denn", "un\u00b7ter", "den", "statt\u00b7li\u00b7chen", "Rei\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$.", "KON", "APPR", "ART", "ADJA", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.15": {"text": "Schlummern schon lange gefallene Helden der eisernen Vorzeit.", "tokens": ["Schlum\u00b7mern", "schon", "lan\u00b7ge", "ge\u00b7fal\u00b7le\u00b7ne", "Hel\u00b7den", "der", "ei\u00b7ser\u00b7nen", "Vor\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.16": {"text": "Aber horch! was rauschet herauf im schwarzen Geb\u00fcsche?", "tokens": ["A\u00b7ber", "horch", "!", "was", "rau\u00b7schet", "her\u00b7auf", "im", "schwar\u00b7zen", "Ge\u00b7b\u00fc\u00b7sche", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$.", "PWS", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.17": {"text": "Bleibe ferne! St\u00f6rer des S\u00e4ngers! \u2013 aber siehe,", "tokens": ["Blei\u00b7be", "fer\u00b7ne", "!", "St\u00f6\u00b7rer", "des", "S\u00e4n\u00b7gers", "!", "\u2013", "a\u00b7ber", "sie\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$.", "NN", "ART", "NN", "$.", "$(", "ADV", "VVFIN", "$,"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.18": {"text": "Siehe! \u2013 wie herrlich! wie gro\u00df! ein hochgeweihetes Hirschheer", "tokens": ["Sie\u00b7he", "!", "\u2013", "wie", "herr\u00b7lich", "!", "wie", "gro\u00df", "!", "ein", "hoch\u00b7ge\u00b7wei\u00b7he\u00b7tes", "Hirschheer"], "token_info": ["word", "punct", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "$.", "$(", "PWAV", "ADJD", "$.", "PWAV", "ADJD", "$.", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Wandelt langsam vor\u00fcber \u2013 hinab nach der Quelle des Tales. \u2013", "tokens": ["Wan\u00b7delt", "lang\u00b7sam", "vor\u00b7\u00fc\u00b7ber", "\u2013", "hin\u00b7ab", "nach", "der", "Quel\u00b7le", "des", "Ta\u00b7les", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "$(", "ADV", "APPR", "ART", "NN", "ART", "NN", "$.", "$("], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.20": {"text": "O! jetzt kenn ich mich wieder, der menschenhassende Tr\u00fcbsinn", "tokens": ["O", "!", "jetzt", "kenn", "ich", "mich", "wie\u00b7der", ",", "der", "men\u00b7schen\u00b7has\u00b7sen\u00b7de", "Tr\u00fcb\u00b7sinn"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "ADV", "VVFIN", "PPER", "PRF", "ADV", "$,", "ART", "ADJA", "NN"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.21": {"text": "Ist so ganz, so ganz aus meinem Herzen verschwunden.", "tokens": ["Ist", "so", "ganz", ",", "so", "ganz", "aus", "mei\u00b7nem", "Her\u00b7zen", "ver\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "$,", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.22": {"text": "W\u00e4r ich doch ewig fern von diesen Mauren des Elends,", "tokens": ["W\u00e4r", "ich", "doch", "e\u00b7wig", "fern", "von", "die\u00b7sen", "Mau\u00b7ren", "des", "E\u00b7lends", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "ADJD", "APPR", "PDAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "Diesen Mauren des Trugs! \u2013 Es blinken der Riesenpal\u00e4ste", "tokens": ["Die\u00b7sen", "Mau\u00b7ren", "des", "Trugs", "!", "\u2013", "Es", "blin\u00b7ken", "der", "Rie\u00b7sen\u00b7pa\u00b7l\u00e4s\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "ART", "NN", "$.", "$(", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.24": {"text": "Schimmernde D\u00e4cher herauf, und die Spitzen der alternden T\u00fcrme,", "tokens": ["Schim\u00b7mern\u00b7de", "D\u00e4\u00b7cher", "her\u00b7auf", ",", "und", "die", "Spit\u00b7zen", "der", "al\u00b7tern\u00b7den", "T\u00fcr\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$,", "KON", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.25": {"text": "Wo so einzeln stehn die Buchen und Eichen; es t\u00f6net", "tokens": ["Wo", "so", "ein\u00b7zeln", "stehn", "die", "Bu\u00b7chen", "und", "Ei\u00b7chen", ";", "es", "t\u00f6\u00b7net"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADV", "ADJD", "VVFIN", "ART", "NN", "KON", "NN", "$.", "PPER", "VVFIN"], "meter": "--+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "Dumpf vom Tale herauf das h\u00f6fische Wagengerassel", "tokens": ["Dumpf", "vom", "Ta\u00b7le", "her\u00b7auf", "das", "h\u00f6\u00b7fi\u00b7sche", "Wa\u00b7gen\u00b7ge\u00b7ras\u00b7sel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "ADV", "ART", "ADJA", "NN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.27": {"text": "Und der Huf der prangenden Rosse \u2013 \u2013 H\u00f6flinge! bleibet,", "tokens": ["Und", "der", "Huf", "der", "pran\u00b7gen\u00b7den", "Ros\u00b7se", "\u2013", "\u2013", "H\u00f6f\u00b7lin\u00b7ge", "!", "blei\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "ADJA", "NN", "$(", "$(", "NN", "$.", "VVFIN", "$,"], "meter": "--+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.28": {"text": "Bleibet immerhin in eurem Wagengerassel,", "tokens": ["Blei\u00b7bet", "im\u00b7mer\u00b7hin", "in", "eu\u00b7rem", "Wa\u00b7gen\u00b7ge\u00b7ras\u00b7sel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.29": {"text": "B\u00fcckt euch tief auf den Narrenb\u00fchnen der Riesenpal\u00e4ste,", "tokens": ["B\u00fcckt", "euch", "tief", "auf", "den", "Nar\u00b7ren\u00b7b\u00fch\u00b7nen", "der", "Rie\u00b7sen\u00b7pa\u00b7l\u00e4s\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.30": {"text": "Bleibet immerhin! \u2013 Und ihr, ihr Edlere, kommet!", "tokens": ["Blei\u00b7bet", "im\u00b7mer\u00b7hin", "!", "\u2013", "Und", "ihr", ",", "ihr", "Ed\u00b7le\u00b7re", ",", "kom\u00b7met", "!"], "token_info": ["word", 