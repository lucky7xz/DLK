{"dta.poem.16007": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "61.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Reich des Friedens ist, der Unschuld einst gewesen,", "tokens": ["Ein", "Reich", "des", "Frie\u00b7dens", "ist", ",", "der", "Un\u00b7schuld", "einst", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "$,", "ART", "NN", "ADV", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wieder wird vom Weh die Menschheit einst genesen.", "tokens": ["Und", "wie\u00b7der", "wird", "vom", "Weh", "die", "Menschheit", "einst", "ge\u00b7ne\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "APPRART", "NN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Fern in der Zukunft steht und in Vergangenheit", "tokens": ["Fern", "in", "der", "Zu\u00b7kunft", "steht", "und", "in", "Ver\u00b7gan\u00b7gen\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Heil, und tr\u00f6stet uns im Unheil dieser Zeit.", "tokens": ["Das", "Heil", ",", "und", "tr\u00f6s\u00b7tet", "uns", "im", "Un\u00b7heil", "die\u00b7ser", "Zeit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "VVFIN", "PPER", "APPRART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Gewis, es war einmal, und wird auch einmal werden,", "tokens": ["Ge\u00b7wis", ",", "es", "war", "ein\u00b7mal", ",", "und", "wird", "auch", "ein\u00b7mal", "wer\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "ADV", "$,", "KON", "VAFIN", "ADV", "ADV", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur fragen l\u00e4\u00dft sich, ob im Himmel, ob auf Erden?", "tokens": ["Nur", "fra\u00b7gen", "l\u00e4\u00dft", "sich", ",", "ob", "im", "Him\u00b7mel", ",", "ob", "auf", "Er\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VVFIN", "PRF", "$,", "KOUS", "APPRART", "NN", "$,", "KOUS", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dort gn\u00fcgt' es selber mir zu meinem eignen Frommen,", "tokens": ["Dort", "gn\u00fcgt'", "es", "sel\u00b7ber", "mir", "zu", "mei\u00b7nem", "eig\u00b7nen", "From\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Allein ich w\u00fcnscht' es hier f\u00fcr die so nach mir kommen.", "tokens": ["Al\u00b7lein", "ich", "w\u00fcnscht'", "es", "hier", "f\u00fcr", "die", "so", "nach", "mir", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}