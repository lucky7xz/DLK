{"textgrid.poem.23984": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Winter", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der alte S\u00e4emann geht \u00fcbers Land;", "tokens": ["Der", "al\u00b7te", "S\u00e4e\u00b7mann", "geht", "\u00fc\u00b7bers", "Land", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sein grauer Sack ist voll und wird nicht leer,", "tokens": ["Sein", "grau\u00b7er", "Sack", "ist", "voll", "und", "wird", "nicht", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADJD", "KON", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So viele Hampfeln auch die Hand verstreut.", "tokens": ["So", "vie\u00b7le", "Hamp\u00b7feln", "auch", "die", "Hand", "ver\u00b7streut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und alles ist ihm Feld: Wald, Wiese, Berg;", "tokens": ["Und", "al\u00b7les", "ist", "ihm", "Feld", ":", "Wald", ",", "Wie\u00b7se", ",", "Berg", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "PPER", "NN", "$.", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "All\u00fcberallhin s\u00e4t er seine Saat,", "tokens": ["Al\u00b7l\u00fc\u00b7be\u00b7rall\u00b7hin", "s\u00e4t", "er", "sei\u00b7ne", "Saat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die niemals aufgeht. Schweigend thut er so.", "tokens": ["Die", "nie\u00b7mals", "auf\u00b7geht", ".", "Schwei\u00b7gend", "thut", "er", "so", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$.", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich seh ihm zu. Mich \u00fcbersch\u00fcttet wei\u00df", "tokens": ["Ich", "seh", "ihm", "zu", ".", "Mich", "\u00fc\u00b7ber\u00b7sch\u00fct\u00b7tet", "wei\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VVPP", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der kalte Segen seiner toten Saat.", "tokens": ["Der", "kal\u00b7te", "Se\u00b7gen", "sei\u00b7ner", "to\u00b7ten", "Saat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und wie ein Baum, aus dem der Lebenssaft", "tokens": ["Und", "wie", "ein", "Baum", ",", "aus", "dem", "der", "Le\u00b7bens\u00b7saft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "$,", "APPR", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sich in die Erde schlug, so steh ich starr", "tokens": ["Sich", "in", "die", "Er\u00b7de", "schlug", ",", "so", "steh", "ich", "starr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und f\u00fchle innerlichst mich selbst vergehn.", "tokens": ["Und", "f\u00fch\u00b7le", "in\u00b7ner\u00b7lichst", "mich", "selbst", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und Schlaf und Tod ist mir nur noch ein Gott.", "tokens": ["Und", "Schlaf", "und", "Tod", "ist", "mir", "nur", "noch", "ein", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VAFIN", "PPER", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Der alte S\u00e4emann geht \u00fcbers Land;", "tokens": ["Der", "al\u00b7te", "S\u00e4e\u00b7mann", "geht", "\u00fc\u00b7bers", "Land", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sein grauer Sack ist voll und wird nicht leer,", "tokens": ["Sein", "grau\u00b7er", "Sack", "ist", "voll", "und", "wird", "nicht", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADJD", "KON", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So viele Hampfeln auch die Hand verstreut.", "tokens": ["So", "vie\u00b7le", "Hamp\u00b7feln", "auch", "die", "Hand", "ver\u00b7streut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7"