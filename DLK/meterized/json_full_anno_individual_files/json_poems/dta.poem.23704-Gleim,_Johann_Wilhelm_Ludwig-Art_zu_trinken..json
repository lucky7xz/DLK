{"dta.poem.23704": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Art zu trinken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-200905197682", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich mein Glas geleeret,", "tokens": ["Wenn", "ich", "mein", "Glas", "ge\u00b7lee\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So f\u00fcll ichs hurtig wieder,", "tokens": ["So", "f\u00fcll", "ichs", "hur\u00b7tig", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADJD", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und bring es meinem M\u00e4dchen.", "tokens": ["Und", "bring", "es", "mei\u00b7nem", "M\u00e4d\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein M\u00e4dchen nimts und trinket,", "tokens": ["Mein", "M\u00e4d\u00b7chen", "nimts", "und", "trin\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und klopft mich auf die Bakken,", "tokens": ["Und", "klopft", "mich", "auf", "die", "Bak\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und f\u00fcllt es gleich von neuem,", "tokens": ["Und", "f\u00fcllt", "es", "gleich", "von", "neu\u00b7em", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und lobt den Sekt im Glase,", "tokens": ["Und", "lobt", "den", "Sekt", "im", "Gla\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und bringt mirs selbst zum Munde,", "tokens": ["Und", "bringt", "mirs", "selbst", "zum", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Ich k\u00fc\u00df es, eh ich trinke,", "tokens": ["Ich", "k\u00fc\u00df", "es", ",", "eh", "ich", "trin\u00b7ke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Es trinkt, und k\u00fc\u00dft mich wieder.", "tokens": ["Es", "trinkt", ",", "und", "k\u00fc\u00dft", "mich", "wie\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Freund, dort im Trauerkleide", "tokens": ["Freund", ",", "dort", "im", "Trau\u00b7er\u00b7klei\u00b7de"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "APPRART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.12": {"text": "Soll es dir auch eins bringen?", "tokens": ["Soll", "es", "dir", "auch", "eins", "brin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "PIS", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}}}}