{"dta.poem.4265": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "La Solitaire.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein vergn\u00fcglich holder Schauer, den man meist in", "tokens": ["Ein", "ver\u00b7gn\u00fcg\u00b7lich", "hol\u00b7der", "Schau\u00b7er", ",", "den", "man", "meist", "in"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,", "PRELS", "PIS", "ADV", "APPR"], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Grotten kennet,", "tokens": ["Grot\u00b7ten", "ken\u00b7net", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Dessen k\u00fchlen Druck man f\u00fchlen, aber nicht beschreiben", "tokens": ["Des\u00b7sen", "k\u00fch\u00b7len", "Druck", "man", "f\u00fch\u00b7len", ",", "a\u00b7ber", "nicht", "be\u00b7schrei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDAT", "ADJA", "NN", "PIS", "VVINF", "$,", "ADV", "PTKNEG", "VVINF"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "kann,", "tokens": ["kann", ","], "token_info": ["word", "punct"], "pos": ["VMFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Tritt, beym Eintritt in die Laube, die man ", "tokens": ["Tritt", ",", "beym", "Ein\u00b7tritt", "in", "die", "Lau\u00b7be", ",", "die", "man"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "APPRART", "NN", "APPR", "ART", "NN", "$,", "PRELS", "PIS"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und an dunkler Einsamkeit sich den Grotten gleicht, uns", "tokens": ["Und", "an", "dunk\u00b7ler", "Ein\u00b7sam\u00b7keit", "sich", "den", "Grot\u00b7ten", "gleicht", ",", "uns"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "PRF", "ART", "NN", "VVFIN", "$,", "PPER"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "an.", "tokens": ["an", "."], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$."], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Der verwachs\u2019nen Zweige Menge zeugt hier eine gr\u00fcne", "tokens": ["Der", "ver\u00b7wachs'\u00b7nen", "Zwei\u00b7ge", "Men\u00b7ge", "zeugt", "hier", "ei\u00b7ne", "gr\u00fc\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "ADV", "ART", "ADJA"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.9": {"text": "Nacht,", "tokens": ["Nacht", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.10": {"text": "Welche der geschlo\u00dfne Bogen dunkler und doch sch\u00f6ner", "tokens": ["Wel\u00b7che", "der", "ge\u00b7schlo\u00df\u00b7ne", "Bo\u00b7gen", "dunk\u00b7ler", "und", "doch", "sch\u00f6\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "ADJA", "NN", "ADJA", "KON", "ADV", "ADJA"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.11": {"text": "macht.", "tokens": ["macht", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.12": {"text": "Wenn wir hier mit, durch die Schatten recht gest\u00e4rkten,", "tokens": ["Wenn", "wir", "hier", "mit", ",", "durch", "die", "Schat\u00b7ten", "recht", "ge\u00b7st\u00e4rk\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKVZ", "$,", "APPR", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.13": {"text": "Augen sitzen,", "tokens": ["Au\u00b7gen", "sit\u00b7zen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.14": {"text": "Und durch die verschr\u00e4nkte Dicke der belaubten B\u00e4ume", "tokens": ["Und", "durch", "die", "ver\u00b7schr\u00e4nk\u00b7te", "Di\u00b7cke", "der", "be\u00b7laub\u00b7ten", "B\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "seh'n,", "tokens": ["seh'n", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.16": {"text": "Wie zuweilen kleine Strahlen an bemoo\u00dfte St\u00e4mme blitzen,", "tokens": ["Wie", "zu\u00b7wei\u00b7len", "klei\u00b7ne", "Strah\u00b7len", "an", "be\u00b7moo\u00df\u00b7te", "St\u00e4m\u00b7me", "blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.17": {"text": "Alle dunkle Schatten trennen, und, durch sie, noch einst", "tokens": ["Al\u00b7le", "dunk\u00b7le", "Schat\u00b7ten", "tren\u00b7nen", ",", "und", ",", "durch", "sie", ",", "noch", "einst"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VVINF", "$,", "KON", "$,", "APPR", "PPER", "$,", "ADV", "ADV"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.18": {"text": "so sch\u00f6n,", "tokens": ["so", "sch\u00f6n", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.19": {"text": "Durch den Gegensatz erhaben, recht wie kleine Lichter", "tokens": ["Durch", "den", "Ge\u00b7gen\u00b7satz", "er\u00b7ha\u00b7ben", ",", "recht", "wie", "klei\u00b7ne", "Lich\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD", "$,", "ADJD", "KOKOM", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.20": {"text": "steh'n;", "tokens": ["steh'n", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.21": {"text": "Wird es in der Seelen helle. Dieser sch\u00f6nen Lichter", "tokens": ["Wird", "es", "in", "der", "See\u00b7len", "hel\u00b7le", ".", "Die\u00b7ser", "sch\u00f6\u00b7nen", "Lich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "ADJA", "$.", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.22": {"text": "Schein", "tokens": ["Schein"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.23": {"text": "Senkt sich in das Innerste der ger\u00fchrten Brust hinein,", "tokens": ["Senkt", "sich", "in", "das", "In\u00b7ners\u00b7te", "der", "ge\u00b7r\u00fchr\u00b7ten", "Brust", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.24": {"text": "Und erleuchtet so den Geist, da\u00df er sich zum Sch\u00f6pfer", "tokens": ["Und", "er\u00b7leuch\u00b7tet", "so", "den", "Geist", ",", "da\u00df", "er", "sich", "zum", "Sch\u00f6p\u00b7fer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$,", "KOUS", "PPER", "PRF", "APPRART", "NN"], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "lenket,", "tokens": ["len\u00b7ket", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.26": {"text": "In nur Jhm ergebner Ehrfurcht: Da\u00df nur Er uns alles", "tokens": ["In", "nur", "Jhm", "er\u00b7geb\u00b7ner", "Ehr\u00b7furcht", ":", "Da\u00df", "nur", "Er", "uns", "al\u00b7les"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "PPER", "ADJA", "NN", "$.", "KOUS", "ADV", "PPER", "PRF", "PIS"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.27": {"text": "schenket,", "tokens": ["schen\u00b7ket", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.28": {"text": "Da\u00df Er aller Sch\u00f6nheit Quelle, voller Dank-Begierde,", "tokens": ["Da\u00df", "Er", "al\u00b7ler", "Sch\u00f6n\u00b7heit", "Quel\u00b7le", ",", "vol\u00b7ler", "Dank\u00b7Be\u00b7gier\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.29": {"text": "denket,", "tokens": ["den\u00b7ket", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.30": {"text": "Welches denn aus seinem Herzen manchen frohen Seufzer", "tokens": ["Wel\u00b7ches", "denn", "aus", "sei\u00b7nem", "Her\u00b7zen", "man\u00b7chen", "fro\u00b7hen", "Seuf\u00b7zer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPR", "PPOSAT", "NN", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.31": {"text": "pre\u00dft,", "tokens": ["pre\u00dft", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.32": {"text": "So, da\u00df er, voll innrer Freude, manch GOtt Lob! erschal-", "tokens": ["So", ",", "da\u00df", "er", ",", "voll", "inn\u00b7rer", "Freu\u00b7de", ",", "manch", "Gott", "Lob", "!", "er\u00b7scha\u00b7l"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "$,", "ADJD", "ADJA", "NN", "$,", "PIAT", "NN", "NN", "$.", "TRUNC"], "meter": "-+--+-+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.33": {"text": "len l\u00e4\u00dft.", "tokens": ["len", "l\u00e4\u00dft", "."], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}}}}}