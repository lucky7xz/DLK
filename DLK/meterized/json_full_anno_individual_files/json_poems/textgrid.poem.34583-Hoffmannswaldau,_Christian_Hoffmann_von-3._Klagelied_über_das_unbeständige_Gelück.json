c.penta"}}, "stanza.16": {"line.1": {"text": "Mein weinen sehnt sich nicht nach Freudenzehren;", "tokens": ["Mein", "wei\u00b7nen", "sehnt", "sich", "nicht", "nach", "Freu\u00b7den\u00b7zeh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVINF", "VVFIN", "PRF", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn sie vertrocknen allzubald.", "tokens": ["Denn", "sie", "ver\u00b7trock\u00b7nen", "all\u00b7zu\u00b7bald", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Sinn ist nicht viel Wehmuth zubegehren;", "tokens": ["Mein", "Sinn", "ist", "nicht", "viel", "Weh\u00b7muth", "zu\u00b7be\u00b7geh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Denn Wehmuth wird zu leichtlich kalt.", "tokens": ["Denn", "Weh\u00b7muth", "wird", "zu", "leicht\u00b7lich", "kalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PTKA", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hat mir einer wol gewollt,", "tokens": ["Hat", "mir", "ei\u00b7ner", "wol", "ge\u00b7wollt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADV", "VMPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Der sage nur: Er leidet ohne Schuld.", "tokens": ["Der", "sa\u00b7ge", "nur", ":", "Er", "lei\u00b7det", "oh\u00b7ne", "Schuld", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "$.", "PPER", "VVFIN", "AP