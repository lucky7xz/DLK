{"textgrid.poem.34583": {"metadata": {"author": {"name": "Hoffmannswaldau, Christian Hoffmann von", "birth": "N.A.", "death": "N.A."}, "title": "3. Klagelied \u00fcber das unbest\u00e4ndige Gel\u00fcck", "genre": "verse", "period": "N.A.", "pub_year": 1647, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Worzu hat mich der Himmel doch erkohren?", "tokens": ["Wor\u00b7zu", "hat", "mich", "der", "Him\u00b7mel", "doch", "er\u00b7koh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bin ich der Sternen Gauckelspiel?", "tokens": ["Bin", "ich", "der", "Ster\u00b7nen", "Gau\u00b7ckel\u00b7spiel", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hab ich denn nun Verstand und Witz verlohren?", "tokens": ["Hab", "ich", "denn", "nun", "Ver\u00b7stand", "und", "Witz", "ver\u00b7loh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich wei\u00df nicht, was ich sagen will;", "tokens": ["Ich", "wei\u00df", "nicht", ",", "was", "ich", "sa\u00b7gen", "will", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch mein' Ungeduld die spricht:", "tokens": ["Doch", "mein'", "Un\u00b7ge\u00b7duld", "die", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Der ist nicht klug, dem itzund Witz gebricht.", "tokens": ["Der", "ist", "nicht", "klug", ",", "dem", "it\u00b7zund", "Witz", "ge\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKNEG", "ADJD", "$,", "PRELS", "ADV", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich bin ein Ball, den das Verh\u00e4ngnis schl\u00e4get;", "tokens": ["Ich", "bin", "ein", "Ball", ",", "den", "das", "Ver\u00b7h\u00e4ng\u00b7nis", "schl\u00e4\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Zufalls Spiel; ein Schertz der Zeit;", "tokens": ["Des", "Zu\u00b7falls", "Spiel", ";", "ein", "Schertz", "der", "Zeit", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$.", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Kummers Zweck; ein Rohr durch Angst beweget;", "tokens": ["Des", "Kum\u00b7mers", "Zweck", ";", "ein", "Rohr", "durch", "Angst", "be\u00b7we\u00b7get", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$.", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Zeughau\u00df voller Angst und Leid.", "tokens": ["Ein", "Zeug\u00b7hau\u00df", "vol\u00b7ler", "Angst", "und", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Meine Seele lieget kranck;", "tokens": ["Mei\u00b7ne", "See\u00b7le", "lie\u00b7get", "kranck", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mein Hencker lacht; die Lieb ist Folterbanck.", "tokens": ["Mein", "Hen\u00b7cker", "lacht", ";", "die", "Lieb", "ist", "Fol\u00b7ter\u00b7banck", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$.", "ART", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Des Ungl\u00fccks Garn will mich nun gantz umschl\u00fcssen,", "tokens": ["Des", "Un\u00b7gl\u00fccks", "Garn", "will", "mich", "nun", "gantz", "um\u00b7schl\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein Leben ist ein langer Tod,", "tokens": ["Mein", "Le\u00b7ben", "ist", "ein", "lan\u00b7ger", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich bin ein Brunn, aus welchem Thr\u00e4nen fl\u00fcssen,", "tokens": ["Ich", "bin", "ein", "Brunn", ",", "aus", "wel\u00b7chem", "Thr\u00e4\u00b7nen", "fl\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "APPR", "PWAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als nasse Zeugen meiner Noth;", "tokens": ["Als", "nas\u00b7se", "Zeu\u00b7gen", "mei\u00b7ner", "Noth", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Weil der Jammer dieser Welt", "tokens": ["Weil", "der", "Jam\u00b7mer", "die\u00b7ser", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Den Sammelplatz in meinem Hertzen h\u00e4lt.", "tokens": ["Den", "Sam\u00b7mel\u00b7platz", "in", "mei\u00b7nem", "Hert\u00b7zen", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der Pfeil, damit Cupido mich getroffen,", "tokens": ["Der", "Pfeil", ",", "da\u00b7mit", "Cu\u00b7pi\u00b7do", "mich", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "NE", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der ist mit Wermuth angesteckt,", "tokens": ["Der", "ist", "mit", "Wer\u00b7muth", "an\u00b7ge\u00b7steckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Venus selbsten heist mich wenig hoffen,", "tokens": ["Die", "Ve\u00b7nus", "selbs\u00b7ten", "heist", "mich", "we\u00b7nig", "hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was nicht nach Gall und Essig schmeckt.", "tokens": ["Was", "nicht", "nach", "Gall", "und", "Es\u00b7sig", "schmeckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "APPR", "NE", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ja der klare Sonnenschein", "tokens": ["Ja", "der", "kla\u00b7re", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKANT", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Bem\u00fchet sich mir itzt ein Blitz zu seyn.", "tokens": ["Be\u00b7m\u00fc\u00b7het", "sich", "mir", "itzt", "ein", "Blitz", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPER", "ADV", "ART", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Das falsche Nichts, Best\u00e4ndigkeit genennet,", "tokens": ["Das", "fal\u00b7sche", "Nichts", ",", "Be\u00b7st\u00e4n\u00b7dig\u00b7keit", "ge\u00b7nen\u00b7net", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Darauf ich manchen Schlu\u00df gebaut,", "tokens": ["Da\u00b7rauf", "ich", "man\u00b7chen", "Schlu\u00df", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das hab ich allzulangsam recht erkennet,", "tokens": ["Das", "hab", "ich", "all\u00b7zu\u00b7lang\u00b7sam", "recht", "er\u00b7ken\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und allzu sicher angeschaut.", "tokens": ["Und", "all\u00b7zu", "si\u00b7cher", "an\u00b7ge\u00b7schaut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PTKA", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hett' ich doch zuvor bedacht:", "tokens": ["Hett'", "ich", "doch", "zu\u00b7vor", "be\u00b7dacht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Das Irrthum klug, doch nicht gel\u00fccklich macht.", "tokens": ["Das", "Irr\u00b7thum", "klug", ",", "doch", "nicht", "ge\u00b7l\u00fcck\u00b7lich", "macht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ADV", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wer aber kan den Schlu\u00df des Himmels st\u00f6ren?", "tokens": ["Wer", "a\u00b7ber", "kan", "den", "Schlu\u00df", "des", "Him\u00b7mels", "st\u00f6\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VMFIN", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wer hebt sein strenges Urtheil auf?", "tokens": ["Wer", "hebt", "sein", "stren\u00b7ges", "Ur\u00b7theil", "auf", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man mus es nur mit gleichen Ohren h\u00f6ren,", "tokens": ["Man", "mus", "es", "nur", "mit", "glei\u00b7chen", "Oh\u00b7ren", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und ihm verg\u00f6nnen seinen Lauf;", "tokens": ["Und", "ihm", "ver\u00b7g\u00f6n\u00b7nen", "sei\u00b7nen", "Lauf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Geduld mus hier allein,", "tokens": ["Die", "Ge\u00b7duld", "mus", "hier", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Der Sinnen Trost, der Wunden Pflaster seyn.", "tokens": ["Der", "Sin\u00b7nen", "Trost", ",", "der", "Wun\u00b7den", "Pflas\u00b7ter", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Doch wil ich nicht mein Ungl\u00fcck selbst besingen;", "tokens": ["Doch", "wil", "ich", "nicht", "mein", "Un\u00b7gl\u00fcck", "selbst", "be\u00b7sin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wohl dem, der stille leiden kan.", "tokens": ["Wohl", "dem", ",", "der", "stil\u00b7le", "lei\u00b7den", "kan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$,", "ART", "ADJA", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Sklav' erschrickt, wenn seine Fessel klingen,", "tokens": ["Ein", "Skla\u00b7v'", "er\u00b7schrickt", ",", "wenn", "sei\u00b7ne", "Fes\u00b7sel", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "KOUS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Er r\u00fchrt sie nicht mit Willen an.", "tokens": ["Er", "r\u00fchrt", "sie", "nicht", "mit", "Wil\u00b7len", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wer sein Ungl\u00fcck recht bedeckt,", "tokens": ["Wer", "sein", "Un\u00b7gl\u00fcck", "recht", "be\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Hat oftermals des Kummers Kraft ersteckt.", "tokens": ["Hat", "of\u00b7ter\u00b7mals", "des", "Kum\u00b7mers", "Kraft", "er\u00b7steckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Mein weinen sehnt sich nicht nach Freudenzehren;", "tokens": ["Mein", "wei\u00b7nen", "sehnt", "sich", "nicht", "nach", "Freu\u00b7den\u00b7zeh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVINF", "VVFIN", "PRF", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn sie vertrocknen allzubald.", "tokens": ["Denn", "sie", "ver\u00b7trock\u00b7nen", "all\u00b7zu\u00b7bald", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Sinn ist nicht viel Wehmuth zubegehren;", "tokens": ["Mein", "Sinn", "ist", "nicht", "viel", "Weh\u00b7muth", "zu\u00b7be\u00b7geh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Denn Wehmuth wird zu leichtlich kalt.", "tokens": ["Denn", "Weh\u00b7muth", "wird", "zu", "leicht\u00b7lich", "kalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PTKA", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hat mir einer wol gewollt,", "tokens": ["Hat", "mir", "ei\u00b7ner", "wol", "ge\u00b7wollt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADV", "VMPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Der sage nur: Er leidet ohne Schuld.", "tokens": ["Der", "sa\u00b7ge", "nur", ":", "Er", "lei\u00b7det", "oh\u00b7ne", "Schuld", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "$.", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Worzu hat mich der Himmel doch erkohren?", "tokens": ["Wor\u00b7zu", "hat", "mich", "der", "Him\u00b7mel", "doch", "er\u00b7koh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bin ich der Sternen Gauckelspiel?", "tokens": ["Bin", "ich", "der", "Ster\u00b7nen", "Gau\u00b7ckel\u00b7spiel", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hab ich denn nun Verstand und Witz verlohren?", "tokens": ["Hab", "ich", "denn", "nun", "Ver\