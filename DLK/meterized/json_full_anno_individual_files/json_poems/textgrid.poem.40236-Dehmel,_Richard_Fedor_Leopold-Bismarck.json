{"textgrid.poem.40236": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Bismarck", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.85", "sv:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Glocken \u2013 Glocken \u2013", "tokens": ["Glo\u00b7cken", "\u2013", "Glo\u00b7cken", "\u2013"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$(", "NN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "sonst den Donner klagen wir,", "tokens": ["sonst", "den", "Don\u00b7ner", "kla\u00b7gen", "wir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "sonst den Flammensturm frohlocken wir:", "tokens": ["sonst", "den", "Flam\u00b7mens\u00b7turm", "froh\u00b7lo\u00b7cken", "wir", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "heut um ", "tokens": ["heut", "um"], "token_info": ["word", "word"], "pos": ["ADV", "APPR"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "aber immer", "tokens": ["a\u00b7ber", "im\u00b7mer"], "token_info": ["word", "word"], "pos": ["ADV", "ADV"], "meter": "+-++", "measure": "unknown.measure.tri"}, "line.6": {"text": "widerhallt aus unserm", "tokens": ["wi\u00b7der\u00b7hallt", "aus", "un\u00b7serm"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Mund die ", "tokens": ["Mund", "die"], "token_info": ["word", "word"], "pos": ["NN", "ART"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.2": {"line.1": {"text": "Immer hungernd,", "tokens": ["Im\u00b7mer", "hun\u00b7gernd", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "dumpf, bang nach Opfern", "tokens": ["dumpf", ",", "bang", "nach", "Op\u00b7fern"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "ADJD", "APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "ruft der Mund der Kraft.", "tokens": ["ruft", "der", "Mund", "der", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Doch auch immer", "tokens": ["Doch", "auch", "im\u00b7mer"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "auf zu dumpfen ", "tokens": ["auf", "zu", "dum\u00b7pfen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PTKZU", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "thut den Mund die dunkle", "tokens": ["thut", "den", "Mund", "die", "dunk\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Mutter dann;", "tokens": ["Mut\u00b7ter", "dann", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADV", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "denn auch immer", "tokens": ["denn", "auch", "im\u00b7mer"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "ADV"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "zeuget, ", "tokens": ["zeu\u00b7get", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.10": {"text": "jung", "tokens": ["jung"], "token_info": ["word"], "pos": ["ADJD"], "meter": "+", "measure": "single.up"}, "line.11": {"text": "immer jung der Schoo\u00df der Kraft ...", "tokens": ["im\u00b7mer", "jung", "der", "Schoo\u00df", "der", "Kraft", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Nur ein Hauch", "tokens": ["Nur", "ein", "Hauch"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "kommt und r\u00fchret das Gebot der gro\u00dfen", "tokens": ["kommt", "und", "r\u00fch\u00b7ret", "das", "Ge\u00b7bot", "der", "gro\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "ART", "NN", "ART", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Mutter die Erkornen;", "tokens": ["Mut\u00b7ter", "die", "Er\u00b7kor\u00b7nen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.15": {"text": "doch dahingezogen", "tokens": ["doch", "da\u00b7hin\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word"], "pos": ["ADV", "PAV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.16": {"text": "folgen sie gebannt und wachsen", "tokens": ["fol\u00b7gen", "sie", "ge\u00b7bannt", "und", "wach\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "VVPP", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "zu den Wolken, \u2013", "tokens": ["zu", "den", "Wol\u00b7ken", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "$,", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.18": {"text": "folgen sie und wankend", "tokens": ["fol\u00b7gen", "sie", "und", "wan\u00b7kend"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KON", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.19": {"text": "bebt der Boden, \u2013", "tokens": ["bebt", "der", "Bo\u00b7den", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.20": {"text": "folgen sie", "tokens": ["fol\u00b7gen", "sie"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.21": {"text": "und", "tokens": ["und"], "token_info": ["word"], "pos": ["KON"], "meter": "-", "measure": "single.down"}, "line.22": {"text": "fallen.", "tokens": ["fal\u00b7len", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "rollt der Strom", "tokens": ["rollt", "der", "Strom"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "quoll der Glut-", "tokens": ["quoll", "der", "Glut"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "TRUNC"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Block,", "tokens": ["Block", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "der nun \u2013 kalt \u2013 die Wogen", "tokens": ["der", "nun", "\u2013", "kalt", "\u2013", "die", "Wo\u00b7gen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "ADV", "$(", "ADJD", "$(", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "staut empor.", "tokens": ["staut", "em\u00b7por", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Hinget\u00fcrmt, stolz,", "tokens": ["Hin\u00b7ge\u00b7t\u00fcrmt", ",", "stolz", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "hochenthoben dem Gebrause,", "tokens": ["ho\u00b7chent\u00b7ho\u00b7ben", "dem", "Ge\u00b7brau\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "starr thront das Lavahaupt,", "tokens": ["starr", "thront", "das", "La\u00b7va\u00b7haupt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "ruhet die gewalt'ge Sohle, \u2013", "tokens": ["ru\u00b7het", "die", "ge\u00b7walt'\u00b7ge", "Soh\u00b7le", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "schaut: starrer immer,", "tokens": ["schaut", ":", "star\u00b7rer", "im\u00b7mer", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$.", "ADJA", "ADV", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "nur gewaltiger noch", "tokens": ["nur", "ge\u00b7wal\u00b7ti\u00b7ger", "noch"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "ADV"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.12": {"text": "von der Wucht der Brandung", "tokens": ["von", "der", "Wucht", "der", "Bran\u00b7dung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.13": {"text": "eingebohrt dem Boden, ", "tokens": ["ein\u00b7ge\u00b7bohrt", "dem", "Bo\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.14": {"text": "Aber aufgeb\u00e4umt nun,", "tokens": ["A\u00b7ber", "auf\u00b7ge\u00b7b\u00e4umt", "nun", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.15": {"text": "voller prallt und w\u00fchlt und kocht die hohe Flut,", "tokens": ["vol\u00b7ler", "prallt", "und", "w\u00fchlt", "und", "kocht", "die", "ho\u00b7he", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.16": {"text": "schaut: voller immer,", "tokens": ["schaut", ":", "vol\u00b7ler", "im\u00b7mer", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$.", "ADJA", "ADV", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.17": {"text": "und es ", "tokens": ["und", "es"], "token_info": ["word", "word"], "pos": ["KON", "PPER"], "meter": "-+", "measure": "iambic.single"}, "line.18": {"text": "stolze Haupt,", "tokens": ["stol\u00b7ze", "Haupt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.19": {"text": "das ", "tokens": ["das"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.20": {"text": "Horcht: grollend krachen,", "tokens": ["Horcht", ":", "grol\u00b7lend", "kra\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$.", "ADJD", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.21": {"text": "rauschend drohen", "tokens": ["rau\u00b7schend", "dro\u00b7hen"], "token_info": ["word", "word"], "pos": ["ADJD", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.22": {"text": "ringsum dunkle Jubelklagelaute, \u2013", "tokens": ["ring\u00b7sum", "dunk\u00b7le", "Ju\u00b7bel\u00b7kla\u00b7ge\u00b7lau\u00b7te", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.23": {"text": "horcht in Ehrfurcht:", "tokens": ["horcht", "in", "Ehr\u00b7furcht", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.24": {"text": "heut der Kraft gefallen", "tokens": ["heut", "der", "Kraft", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.25": {"text": "ist ein Opferzeuge! \u2013", "tokens": ["ist", "ein", "Op\u00b7fer\u00b7zeu\u00b7ge", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ART", "NN", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Ruhe, ruhe,", "tokens": ["Ru\u00b7he", ",", "ru\u00b7he", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Bismarck, graue Klippe du \u2013", "tokens": ["Bis\u00b7marck", ",", "grau\u00b7e", "Klip\u00b7pe", "du", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "rolle, rolle,", "tokens": ["rol\u00b7le", ",", "rol\u00b7le", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Volk, du auferwachte junge Stromflut \u2013", "tokens": ["Volk", ",", "du", "au\u00b7fer\u00b7wach\u00b7te", "jun\u00b7ge", "Strom\u00b7flut", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "hohl verhallet", "tokens": ["hohl", "ver\u00b7hal\u00b7let"], "token_info": ["word", "word"], "pos": [