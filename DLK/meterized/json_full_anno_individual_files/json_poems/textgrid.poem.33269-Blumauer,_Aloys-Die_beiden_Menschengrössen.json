{"textgrid.poem.33269": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Die beiden Menschengr\u00f6ssen", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Menschengr\u00f6ssen gibt es zwei hienieden,", "tokens": ["Men\u00b7schen\u00b7gr\u00f6s\u00b7sen", "gibt", "es", "zwei", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "CARD", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Eine jede kleidet ihren Mann.", "tokens": ["Ei\u00b7ne", "je\u00b7de", "klei\u00b7det", "ih\u00b7ren", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Das Verdienst webt beide, doch verschieden", "tokens": ["Das", "Ver\u00b7dienst", "webt", "bei\u00b7de", ",", "doch", "ver\u00b7schie\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PIS", "$,", "ADV", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sind die F\u00e4den und die Farben d'ran.", "tokens": ["Sind", "die", "F\u00e4\u00b7den", "und", "die", "Far\u00b7ben", "d'\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "KON", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Eine h\u00fcllet sich in eitel Licht,", "tokens": ["Ei\u00b7ne", "h\u00fcl\u00b7let", "sich", "in", "ei\u00b7tel", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "APPR", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Wo die and're sanfte Farben bricht.", "tokens": ["Wo", "die", "an\u00b7d'\u00b7re", "sanf\u00b7te", "Far\u00b7ben", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie die Sonne gl\u00e4ntz und strahlt die eine,", "tokens": ["Wie", "die", "Son\u00b7ne", "gl\u00e4ntz", "und", "strahlt", "die", "ei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NE", "KON", "VVFIN", "ART", "ART", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Welten w\u00e4rmt und brennet ihre Glut;", "tokens": ["Wel\u00b7ten", "w\u00e4rmt", "und", "bren\u00b7net", "ih\u00b7re", "Glut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und die and're gleicht dem Mondenscheine,", "tokens": ["Und", "die", "an\u00b7d'\u00b7re", "gleicht", "dem", "Mon\u00b7den\u00b7schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der nur Nachts im Stillen Gutes thut.", "tokens": ["Der", "nur", "Nachts", "im", "Stil\u00b7len", "Gu\u00b7tes", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "APPRART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Jene blendet mit zu vielem Licht,", "tokens": ["Je\u00b7ne", "blen\u00b7det", "mit", "zu", "vie\u00b7lem", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "APPR", "PIS", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Diese leuchtet, aber blendet nicht.", "tokens": ["Die\u00b7se", "leuch\u00b7tet", ",", "a\u00b7ber", "blen\u00b7det", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ADV", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Wie ein Bergstrom \u00fcber Felsenst\u00fccke,", "tokens": ["Wie", "ein", "Berg\u00b7strom", "\u00fc\u00b7ber", "Fel\u00b7sen\u00b7st\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Rauschet jene, laut und f\u00fcrchterlich;", "tokens": ["Rau\u00b7schet", "je\u00b7ne", ",", "laut", "und", "f\u00fcrch\u00b7ter\u00b7lich", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Diese windet, unbemerkt dem Blicke,", "tokens": ["Die\u00b7se", "win\u00b7det", ",", "un\u00b7be\u00b7merkt", "dem", "Bli\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wie ein Bach durch die Gestr\u00e4uche sich.", "tokens": ["Wie", "ein", "Bach", "durch", "die", "Ge\u00b7str\u00e4u\u00b7che", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "PRF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Jene brauset und verheert die Flur,", "tokens": ["Je\u00b7ne", "brau\u00b7set", "und", "ver\u00b7heert", "die", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Diese tr\u00e4nket und erquickt sie nur.", "tokens": ["Die\u00b7se", "tr\u00e4n\u00b7ket", "und", "er\u00b7quickt", "sie", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Jene baut sich Ehrenmausol\u00e4en", "tokens": ["Je\u00b7ne", "baut", "sich", "Eh\u00b7ren\u00b7mau\u00b7so\u00b7l\u00e4\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PRF", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Aus den Tr\u00fcmmern einer halben Welt;", "tokens": ["Aus", "den", "Tr\u00fcm\u00b7mern", "ei\u00b7ner", "hal\u00b7ben", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Diese f\u00fchlt sich reicher an Troph\u00e4en,", "tokens": ["Die\u00b7se", "f\u00fchlt", "sich", "rei\u00b7cher", "an", "Trop\u00b7h\u00e4\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+++-", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Wenn sie Thr\u00e4nen regen Dankes z\u00e4hlt.", "tokens": ["Wenn", "sie", "Thr\u00e4\u00b7nen", "re\u00b7gen", "Dan\u00b7kes", "z\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Jene bauet ihren Ruhm in Stein,", "tokens": ["Je\u00b7ne", "bau\u00b7et", "ih\u00b7ren", "Ruhm", "in", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Diese gr\u00e4bt ihn in die Herzen ein.", "tokens": ["Die\u00b7se", "gr\u00e4bt", "ihn", "in", "die", "Her\u00b7zen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Jene l\u00e4\u00dft mit lautem Ruhm sich lohnen,", "tokens": ["Je\u00b7ne", "l\u00e4\u00dft", "mit", "lau\u00b7tem", "Ruhm", "sich", "loh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und ihr Aufenthalt sind Thronen nur;", "tokens": ["Und", "ihr", "Auf\u00b7ent\u00b7halt", "sind", "Thro\u00b7nen", "nur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "NN", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Diese sieht man auch in H\u00fctten wohnen,", "tokens": ["Die\u00b7se", "sieht", "man", "auch", "in", "H\u00fct\u00b7ten", "woh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und ihr Lohn ist Segen der Natur.", "tokens": ["Und", "ihr", "Lohn", "ist", "Se\u00b7gen", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Jene kann ein Kind des Gl\u00fcckes sein.", "tokens": ["Je\u00b7ne", "kann", "ein", "Kind", "des", "Gl\u00fc\u00b7ckes", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "ART", "NN", "VAINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Diese dankt ihr Dasein sich allein.", "tokens": ["Die\u00b7se", "dankt", "ihr", "Da\u00b7sein", "sich", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "PRF", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Gr\u00f6sse lauten Ruhmes! deiner Schwingen", "tokens": ["Gr\u00f6s\u00b7se", "lau\u00b7ten", "Ruh\u00b7mes", "!", "dei\u00b7ner", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ADJA", "NN", "$.", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Breite gleicht dem Himmelsfirmament;", "tokens": ["Brei\u00b7te", "gleicht", "dem", "Him\u00b7mels\u00b7fir\u00b7ma\u00b7ment", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Aber deinen Standort zu erringen", "tokens": ["A\u00b7ber", "dei\u00b7nen", "Stand\u00b7ort", "zu", "er\u00b7rin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ist nur wenig Sterblichen verg\u00f6nnt.", "tokens": ["Ist", "nur", "we\u00b7nig", "Sterb\u00b7li\u00b7chen", "ver\u00b7g\u00f6nnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "l