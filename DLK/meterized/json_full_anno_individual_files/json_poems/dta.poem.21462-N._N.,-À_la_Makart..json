{"dta.poem.21462": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "\u00c0 la  Makart.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Mit d\u00e4monischen Reizen", "tokens": ["Mit", "d\u00e4\u00b7mo\u00b7ni\u00b7schen", "Rei\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schm\u00fcckte dich Venus,", "tokens": ["Schm\u00fcck\u00b7te", "dich", "Ve\u00b7nus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Die G\u00f6ttin der Liebe:", "tokens": ["Die", "G\u00f6t\u00b7tin", "der", "Lie\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Du woll\u00fcstig blasse,", "tokens": ["Du", "wol\u00b7l\u00fcs\u00b7tig", "blas\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Lustheischende Dirne.", "tokens": ["Lust\u00b7hei\u00b7schen\u00b7de", "Dir\u00b7ne", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Wie sch\u00f6n bist du!", "tokens": ["Wie", "sch\u00f6n", "bist", "du", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Leise heben sich", "tokens": ["Lei\u00b7se", "he\u00b7ben", "sich"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "In zitternden Wogen", "tokens": ["In", "zit\u00b7tern\u00b7den", "Wo\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Deiner \u00fcppigen Br\u00fcste", "tokens": ["Dei\u00b7ner", "\u00fcp\u00b7pi\u00b7gen", "Br\u00fcs\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.10": {"text": "Zartknospende Rosen.", "tokens": ["Zart\u00b7knos\u00b7pen\u00b7de", "Ro\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Phantastisch flutet", "tokens": ["Phan\u00b7tas\u00b7tisch", "flu\u00b7tet"], "token_info": ["word", "word"], "pos": ["ADJD", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Deines Seidenhaars", "tokens": ["Dei\u00b7nes", "Sei\u00b7den\u00b7haars"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.13": {"text": "Duftige Lockenf\u00fclle", "tokens": ["Duf\u00b7ti\u00b7ge", "Lo\u00b7cken\u00b7f\u00fcl\u00b7le"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.14": {"text": "Auf den bl\u00fcthenwei\u00dfen", "tokens": ["Auf", "den", "bl\u00fc\u00b7then\u00b7wei\u00b7\u00dfen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.15": {"text": "Nacken hernieder,", "tokens": ["Na\u00b7cken", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.16": {"text": "Der so lieblich gerundet \u2026", "tokens": ["Der", "so", "lieb\u00b7lich", "ge\u00b7run\u00b7det", "\u2026"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VVPP", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.17": {"text": "Immer hei\u00dfer zehrt", "tokens": ["Im\u00b7mer", "hei\u00b7\u00dfer", "zehrt"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.18": {"text": "Am innersten Mark mir", "tokens": ["Am", "in\u00b7ners\u00b7ten", "Mark", "mir"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "PPER"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.19": {"text": "Deiner nachtschwarzen Augen", "tokens": ["Dei\u00b7ner", "nacht\u00b7schwar\u00b7zen", "Au\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-++-+-", "measure": "unknown.measure.tetra"}, "line.20": {"text": "Wildlodernde Glut.", "tokens": ["Wild\u00b7lo\u00b7dern\u00b7de", "Glut", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+---+", "measure": "dactylic.init"}, "line.21": {"text": "Wollustathmend,", "tokens": ["Wol\u00b7lus\u00b7tath\u00b7mend", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.22": {"text": "Fieberhei\u00df,", "tokens": ["Fie\u00b7ber\u00b7hei\u00df", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.23": {"text": "Bl\u00fcht mir entgegen", "tokens": ["Bl\u00fcht", "mir", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.24": {"text": "Deines schwellenden Leibes", "tokens": ["Dei\u00b7nes", "schwel\u00b7len\u00b7den", "Lei\u00b7bes"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.25": {"text": "Nacktschimmernde Pracht;", "tokens": ["Nackt\u00b7schim\u00b7mern\u00b7de", "Pracht", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.26": {"text": "Und wonnig umschlungen", "tokens": ["Und", "won\u00b7nig", "um\u00b7schlun\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.27": {"text": "Von dem sammetweichen Fleische", "tokens": ["Von", "dem", "sam\u00b7met\u00b7wei\u00b7chen", "Flei\u00b7sche"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Deiner wei\u00dfkosigen Arme", "tokens": ["Dei\u00b7ner", "wei\u00df\u00b7ko\u00b7si\u00b7gen", "Ar\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Sinke ich liebe\u00e4chzend", "tokens": ["Sin\u00b7ke", "ich", "lie\u00b7be\u00b7\u00e4ch\u00b7zend"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.30": {"text": "In deines feuchten", "tokens": ["In", "dei\u00b7nes", "feuch\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.31": {"text": "Br\u00fcnstigen Schoo\u00dfes", "tokens": ["Br\u00fcns\u00b7ti\u00b7gen", "Schoo\u00b7\u00dfes"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.32": {"text": "Thauspendende Tiefen.", "tokens": ["Thaus\u00b7pen\u00b7den\u00b7de", "Tie\u00b7fen", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.33": {"text": "Voll s\u00fc\u00dfer Gier,", "tokens": ["Voll", "s\u00fc\u00b7\u00dfer", "Gier", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.34": {"text": "In wahnsinniger Trunkenheit", "tokens": ["In", "wahn\u00b7sin\u00b7ni\u00b7ger", "Trun\u00b7ken\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.35": {"text": "Pre\u00df ich dich an mich;", "tokens": ["Pre\u00df", "ich", "dich", "an", "mich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PRF", "APPR", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.36": {"text": "Lippe brennt auf Lippe,", "tokens": ["Lip\u00b7pe", "brennt", "auf", "Lip\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NE", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.37": {"text": "Leib schwelgt an Leib,", "tokens": ["Leib", "schwelgt", "an", "Leib", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.38": {"text": "In seligen Schauern", "tokens": ["In", "se\u00b7li\u00b7gen", "Schau\u00b7ern"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+---+-", "measure": "dactylic.init"}, "line.39": {"text": "Rinnt in einander", "tokens": ["Rinnt", "in", "ein\u00b7an\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "PRF"], "meter": "++-+-", "measure": "iambic.di"}, "line.40": {"text": "Der Seelen Geheimni\u00df \u2026", "tokens": ["Der", "See\u00b7len", "Ge\u00b7heim\u00b7ni\u00df", "\u2026"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}