{"textgrid.poem.67138": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Geh unter, sch\u00f6ne Sonne ...", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geh unter, sch\u00f6ne Sonne, sie achteten", "tokens": ["Geh", "un\u00b7ter", ",", "sch\u00f6\u00b7ne", "Son\u00b7ne", ",", "sie", "ach\u00b7te\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "APPR", "$,", "ADJA", "NN", "$,", "PPER", "VVINF"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nur wenig dein, sie kannten dich, Heilge, nicht,", "tokens": ["Nur", "we\u00b7nig", "dein", ",", "sie", "kann\u00b7ten", "dich", ",", "Heil\u00b7ge", ",", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "PIS", "PPOSAT", "$,", "PPER", "VVFIN", "PPER", "$,", "NN", "$,", "PTKNEG", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Denn m\u00fchelos und stille bist du", "tokens": ["Denn", "m\u00fc\u00b7he\u00b7los", "und", "stil\u00b7le", "bist", "du"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "ADJA", "VAFIN", "PPER"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "\u00dcber den M\u00fchsamen aufgegangen.", "tokens": ["\u00dc\u00b7ber", "den", "M\u00fch\u00b7sa\u00b7men", "auf\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Mir gehst du freundlich unter und auf, o Licht!", "tokens": ["Mir", "gehst", "du", "freund\u00b7lich", "un\u00b7ter", "und", "auf", ",", "o", "Licht", "!"], 