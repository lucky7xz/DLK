{"textgrid.poem.54313": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Belache nicht, mein Freund, mein matt und heischer Singen,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Belache nicht, mein Freund, mein matt und heischer Singen,", "tokens": ["Be\u00b7la\u00b7che", "nicht", ",", "mein", "Freund", ",", "mein", "matt", "und", "hei\u00b7scher", "Sin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Apollo, schw\u00f6r ich dir, hat meiner Leyer satt;", "tokens": ["A\u00b7pol\u00b7lo", ",", "schw\u00f6r", "ich", "dir", ",", "hat", "mei\u00b7ner", "Le\u00b7yer", "satt", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "PPER", "$,", "VAFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Drum kan ich keinen Vers geschickt zu Marckte bringen.", "tokens": ["Drum", "kan", "ich", "kei\u00b7nen", "Vers", "ge\u00b7schickt", "zu", "Marck\u00b7te", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PIAT", "NN", "VVPP", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich bin, du glaubst es nicht, zu dichten la\u00df und matt.", "tokens": ["Ich", "bin", ",", "du", "glaubst", "es", "nicht", ",", "zu", "dich\u00b7ten", "la\u00df", "und", "matt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$,", "APPR", "ADJA", "NN", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dem allen ungeacht mu\u00df meine Muse singen,", "tokens": ["Dem", "al\u00b7len", "un\u00b7ge\u00b7acht", "mu\u00df", "mei\u00b7ne", "Mu\u00b7se", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Gesetzt, es kostet ihr die allerletzte Krafft,", "tokens": ["Ge\u00b7setzt", ",", "es", "kos\u00b7tet", "ihr", "die", "al\u00b7ler\u00b7letz\u00b7te", "Krafft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So wird sie doch vor dich noch was zu Marckte bringen.", "tokens": ["So", "wird", "sie", "doch", "vor", "dich", "noch", "was", "zu", "Marck\u00b7te", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PPER", "ADV", "PIS", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der Tag, der deinen Hau\u00df und uns Vergn\u00fcgen schafft,", "tokens": ["Der", "Tag", ",", "der", "dei\u00b7nen", "Hau\u00df", "und", "uns", "Ver\u00b7gn\u00fc\u00b7gen", "schafft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "KON", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Weckt meine Geister auf, die Muse mu\u00df nicht sterben,", "tokens": ["Weckt", "mei\u00b7ne", "Geis\u00b7ter", "auf", ",", "die", "Mu\u00b7se", "mu\u00df", "nicht", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$,", "ART", "NN", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Sie baut auf deine Kunst, die alle Welt verehrt,", "tokens": ["Sie", "baut", "auf", "dei\u00b7ne", "Kunst", ",", "die", "al\u00b7le", "Welt", "ver\u00b7ehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "PRELS", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Du andrer Podalir l\u00e4st sie noch nicht verderben,", "tokens": ["Du", "an\u00b7drer", "Po\u00b7da\u00b7lir", "l\u00e4st", "sie", "noch", "nicht", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VVFIN", "PPER", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Weil dir dergleichen Cur vor anderen geh\u00f6rt.", "tokens": ["Weil", "dir", "derg\u00b7lei\u00b7chen", "Cur", "vor", "an\u00b7de\u00b7ren", "ge\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "NN", "APPR", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Drum will ich selbige dir g\u00e4ntzlich \u00fcbergeben,", "tokens": ["Drum", "will", "ich", "sel\u00b7bi\u00b7ge", "dir", "g\u00e4ntz\u00b7lich", "\u00fc\u00b7ber\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "VVFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Verschreibe was du wilst, es ist ihr einerley.", "tokens": ["Ver\u00b7schrei\u00b7be", "was", "du", "wilst", ",", "es", "ist", "ihr", "ei\u00b7ner\u00b7ley", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWS", "PPER", "VMFIN", "$,", "PPER", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Gib durch dein Recipe ihr wieder frisches Leben,", "tokens": ["Gib", "durch", "dein", "Re\u00b7ci\u00b7pe", "ihr", "wie\u00b7der", "fri\u00b7sches", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "PPOSAT", "NN", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und bring ihr St\u00e4rck und Krafft durch Quint-Essenzen bey.", "tokens": ["Und", "bring", "ihr", "St\u00e4rck", "und", "Krafft", "durch", "Quint\u00b7Es\u00b7sen\u00b7zen", "bey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "KON", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Statt Sostri will ich dich mit einen Wunsch beschencken,", "tokens": ["Statt", "Sost\u00b7ri", "will", "ich", "dich", "mit", "ei\u00b7nen", "Wunsch", "be\u00b7schen\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VMFIN", "PPER", "PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "O k\u00f6nt ich in voraus schon die Erf\u00fcllung sehn!", "tokens": ["O", "k\u00f6nt", "ich", "in", "vo\u00b7raus", "schon", "die", "Er\u00b7f\u00fcl\u00b7lung", "sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "APPR", "ADV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Jedoch der Himmel wird schon alles gl\u00fccklich lencken,", "tokens": ["Je\u00b7doch", "der", "Him\u00b7mel", "wird", "schon", "al\u00b7les", "gl\u00fcck\u00b7lich", "len\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ADV", "PIS", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ich wette, da\u00df es wird noch dieses Jahr geschehn:", "tokens": ["Ich", "wet\u00b7te", ",", "da\u00df", "es", "wird", "noch", "die\u00b7ses", "Jahr", "ge\u00b7schehn", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "VAFIN", "ADV", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ein Vetter Michelgen soll in der Wiegen lachen,", "tokens": ["Ein", "Vet\u00b7ter", "Mi\u00b7chel\u00b7gen", "soll", "in", "der", "Wie\u00b7gen", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Damit die Bade-Cur recht ihre W\u00fcrckung zeigt.", "tokens": ["Da\u00b7mit", "die", "Ba\u00b7de\u00b7Cur", "recht", "ih\u00b7re", "W\u00fcr\u00b7ckung", "zeigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Von f\u00fcnff und zwanzigsten will ich die Rechnung machen,", "tokens": ["Von", "f\u00fcnff", "und", "zwan\u00b7zigs\u00b7ten", "will", "ich", "die", "Rech\u00b7nung", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "KON", "VVINF", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Wann der Calender nicht in seiner Zeichnung treugt.", "tokens": ["Wann", "der", "Ca\u00b7len\u00b7der", "nicht", "in", "sei\u00b7ner", "Zeich\u00b7nung", "treugt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PTKNEG", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.25": {"text": "Ich w\u00fcnsche, liebster Freund, und di\u00df von gantzen Hertzen,", "tokens": ["Ich", "w\u00fcn\u00b7sche", ",", "liebs\u00b7ter", "Freund", ",", "und", "di\u00df", "von", "gant\u00b7zen", "Hert\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "$,", "KON", "PDS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Da\u00df man das heutge Fest noch \u00f6ffters feyern mag,", "tokens": ["Da\u00df", "man", "das", "heut\u00b7ge", "Fest", "noch", "\u00f6ff\u00b7ters", "fey\u00b7ern", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "ADJA", "NN", "ADV", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und zwar sans Podagra, sans Reissen, sonder Schmertzen,", "tokens": ["Und", "zwar", "sans", "Po\u00b7da\u00b7gra", ",", "sans", "Reis\u00b7sen", ",", "son\u00b7der", "Schmert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "$,", "ADJA", "NN", "$,", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Auch niemahls so beschmutzt als er in Windeln lag.", "tokens": ["Auch", "nie\u00b7mahls", "so", "be\u00b7schmutzt", "als", "er", "in", "Win\u00b7deln", "lag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADJD", "KOKOM", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Belache nicht, mein Freund, mein matt und heischer Singen,", "tokens": ["Be\u00b7la\u00b7che", "nicht", ",", "mein", "Freund", ",", "mein", "matt", "und", "hei\u00b7scher", "Sin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Apollo, schw\u00f6r ich dir, hat meiner Leyer satt;", "tokens": ["A\u00b7pol\u00b7lo", ",", "schw\u00f6r", "ich", "dir", ",", "hat", "mei\u00b7ner", "Le\u00b7yer", "satt", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "PPER", "$,", "VAFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Drum kan ich keinen Vers geschickt zu Marckte bringen.", "tokens": ["Drum", "kan", "ich", "kei\u00b7nen", "Vers", "ge\u00b7schickt", "zu", "Marck\u0