{"textgrid.poem.27595": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kenn ein h\u00f6llisch Volck, die Br\u00fcder der Erinnen,", "tokens": ["Ich", "kenn", "ein", "h\u00f6l\u00b7lisch", "Volck", ",", "die", "Br\u00fc\u00b7der", "der", "E\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Volck von s\u00fcsser Zung' und von vergifften Sinnen,", "tokens": ["Ein", "Volck", "von", "s\u00fcs\u00b7ser", "Zung'", "und", "von", "ver\u00b7giff\u00b7ten", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "KON", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das zwischen Mund und Hertz, das zwischen Wort und That", "tokens": ["Das", "zwi\u00b7schen", "Mund", "und", "Hertz", ",", "das", "zwi\u00b7schen", "Wort", "und", "That"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$,", "PRELS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hat einen engen Raum, wie Ost und Westen hat.", "tokens": ["Hat", "ei\u00b7nen", "en\u00b7gen", "Raum", ",", "wie", "Ost", "und", "Wes\u00b7ten", "hat", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,", "PWAV", "NN", "KON", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es lobt mich im Gesicht, es sch\u00e4ndet mich im R\u00fccken,", "tokens": ["Es", "lobt", "mich", "im", "Ge\u00b7sicht", ",", "es", "sch\u00e4n\u00b7det", "mich", "im", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$,", "PPER", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es wil durch meine Schmach sein eignes Laster schm\u00fccken;", "tokens": ["Es", "wil", "durch", "mei\u00b7ne", "Schmach", "sein", "eig\u00b7nes", "Las\u00b7ter", "schm\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es sehnet sich empor, verachtet alle Welt", "tokens": ["Es", "seh\u00b7net", "sich", "em\u00b7por", ",", "ver\u00b7ach\u00b7tet", "al\u00b7le", "Welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "PTKVZ", "$,", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und hat genug an dem, da\u00df ihm es selbst gef\u00e4llt.", "tokens": ["Und", "hat", "ge\u00b7nug", "an", "dem", ",", "da\u00df", "ihm", "es", "selbst", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "APPR", "ART", "$,", "KOUS", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Was ist mir denn zu thun? Sonst wil ich nichts ihm g\u00f6nnen,", "tokens": ["Was", "ist", "mir", "denn", "zu", "thun", "?", "Sonst", "wil", "ich", "nichts", "ihm", "g\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "PTKZU", "VVINF", "$.", "ADV", "VMFIN", "PPER", "PIS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Als da\u00df sein falsches Maul m\u00f6g' einen Stand gewinnen,", "tokens": ["Als", "da\u00df", "sein", "fal\u00b7sches", "Maul", "m\u00f6g'", "ei\u00b7nen", "Stand", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPOSAT", "ADJA", "NN", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wo sonst durch holen Grund ein stinkend Athem zeucht,", "tokens": ["Wo", "sonst", "durch", "ho\u00b7len", "Grund", "ein", "stin\u00b7kend", "A\u00b7them", "zeucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "ADJA", "NN", "ART", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der auff die Fersen zielt und in die Nasen kreucht.", "tokens": ["Der", "auff", "die", "Fer\u00b7sen", "zielt", "und", "in", "die", "Na\u00b7sen", "kreucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "KON", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich kenn ein h\u00f6llisch Volck, die Br\u00fcder der Erinnen,", "tokens": ["Ich", "kenn", "ein", "h\u00f6l\u00b7lisch", "Volck", ",", "die", "Br\u00fc\u00b7der", "der", "E\u00b7