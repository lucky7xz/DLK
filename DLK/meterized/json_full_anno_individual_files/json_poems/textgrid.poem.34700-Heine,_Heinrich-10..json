{"textgrid.poem.34700": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Wind zieht seine Hosen an,", "tokens": ["Der", "Wind", "zieht", "sei\u00b7ne", "Ho\u00b7sen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die wei\u00dfen Wasserhosen!", "tokens": ["Die", "wei\u00b7\u00dfen", "Was\u00b7ser\u00b7ho\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er peitscht die Wellen, so stark er kann,", "tokens": ["Er", "peitscht", "die", "Wel\u00b7len", ",", "so", "stark", "er", "kann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ADV", "ADJD", "PPER", "VMFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die heulen und brausen und tosen.", "tokens": ["Die", "heu\u00b7len", "und", "brau\u00b7sen", "und", "to\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "VVFIN", "KON", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Aus dunkler H\u00f6h', mit wilder Macht,", "tokens": ["Aus", "dunk\u00b7ler", "H\u00f6h'", ",", "mit", "wil\u00b7der", "Macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Regeng\u00fcsse tr\u00e4ufen;", "to