{"textgrid.poem.24387": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Uber die Vergn\u00fcgung", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vergn\u00fcgung klinget sch\u00f6n in aller Menschen Munde.", "tokens": ["Ver\u00b7gn\u00fc\u00b7gung", "klin\u00b7get", "sch\u00f6n", "in", "al\u00b7ler", "Men\u00b7schen", "Mun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Man h\u00f6rt di\u00df s\u00fcsse Wort theils zur begl\u00fcckten Stunde.", "tokens": ["Man", "h\u00f6rt", "di\u00df", "s\u00fcs\u00b7se", "Wort", "theils", "zur", "be\u00b7gl\u00fcck\u00b7ten", "Stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PDS", "ADJA", "NN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Theils wenn man nicht erlangt/ was man so embsig sucht/", "tokens": ["Theils", "wenn", "man", "nicht", "er\u00b7langt", "/", "was", "man", "so", "emb\u00b7sig", "sucht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PIS", "PTKNEG", "VVPP", "$(", "PWS", "PIS", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und gro\u00df von aussen thut/ wenn man von innen flucht.", "tokens": ["Und", "gro\u00df", "von", "aus\u00b7sen", "thut", "/", "wenn", "man", "von", "in\u00b7nen", "flucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "NE", "VVFIN", "$(", "KOUS", "PIS", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der ist allein vergn\u00fcgt/ der Gottes Schickung liebet/", "tokens": ["Der", "ist", "al\u00b7lein", "ver\u00b7gn\u00fcgt", "/", "der", "Got\u00b7tes", "Schi\u00b7ckung", "lie\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "VVPP", "$(", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Den F\u00e4llen dieser Welt sich aber nicht ergiebet.", "tokens": ["Den", "F\u00e4l\u00b7len", "die\u00b7ser", "Welt", "sich", "a\u00b7ber", "nicht", "er\u00b7gie\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "PRF", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Vergn\u00fcgung klinget sch\u00f6n in aller Menschen Munde.", "tokens": ["Ver\u00b7gn\u00fc\u00b7gung", "klin\u00b7get", "sch\u00f6n", "in", "al\u00b7ler", "Men\u00b7schen", "Mun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Man h\u00f6rt di\u00df s\u00fcsse Wort theils zur begl\u00fcckten Stunde.", "tokens": ["Man", "h\u00f6rt", "di\u00df", "s\u00fcs\u00b7se", "Wort", "theils", "zur", "be\u00b7gl\u00fcck\u00b7ten", "Stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PDS", "ADJA", "NN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": 