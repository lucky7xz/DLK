{"dta.poem.8177": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Wer je gelebt in Liebesarmen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Der kann im Leben nie verarmen;", "tokens": ["Der", "kann", "im", "Le\u00b7ben", "nie", "ver\u00b7ar\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPRART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und m\u00fc\u00dft' er sterben fern, allein,", "tokens": ["Und", "m\u00fc\u00dft'", "er", "ster\u00b7ben", "fern", ",", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "VVFIN", "PTKVZ", "$,", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er f\u00fchlte noch die sel'ge Stunde,", "tokens": ["Er", "f\u00fchl\u00b7te", "noch", "die", "sel'\u00b7ge", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo er gelebt an ihrem Munde,", "tokens": ["Wo", "er", "ge\u00b7lebt", "an", "ih\u00b7rem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und noch im Tode ist sie sein.", "tokens": ["Und", "noch", "im", "To\u00b7de", "ist", "sie", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "VAFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}