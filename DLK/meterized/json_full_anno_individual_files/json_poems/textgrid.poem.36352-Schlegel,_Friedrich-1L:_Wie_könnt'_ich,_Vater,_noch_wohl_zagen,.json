{"textgrid.poem.36352": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie k\u00f6nnt' ich, Vater, noch wohl zagen,", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie k\u00f6nnt' ich, Vater, noch wohl zagen,", "tokens": ["Wie", "k\u00f6nnt'", "ich", ",", "Va\u00b7ter", ",", "noch", "wohl", "za\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "$,", "NN", "$,", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da deine Hand mich sichtbar f\u00fchrt?", "tokens": ["Da", "dei\u00b7ne", "Hand", "mich", "sicht\u00b7bar", "f\u00fchrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Ungl\u00fcck soll mich nicht zerschlagen,", "tokens": ["Das", "Un\u00b7gl\u00fcck", "soll", "mich", "nicht", "zer\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dankbar hab' ich es oft gesp\u00fcrt.", "tokens": ["Dank\u00b7bar", "hab'", "ich", "es", "oft", "ge\u00b7sp\u00fcrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nun f\u00fchl' ich recht ein fest Vertrauen,", "tokens": ["Nun", "f\u00fchl'", "ich", "recht", "ein", "fest", "Ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ruhig auf was da kommt zu schauen;", "tokens": ["Ru\u00b7hig", "auf", "was", "da", "kommt", "zu", "schau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PRELS", "ADV", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Dein Wink ist was mich trifft, dein Ruf,", "tokens": ["Dein", "Wink", "ist", "was", "mich", "trifft", ",", "dein", "Ruf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PWS", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dir hab' ich ganz mich \u00fcbergeben,", "tokens": ["Dir", "hab'", "ich", "ganz", "mich", "\u00fc\u00b7ber\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Vollf\u00fchrend treu, was mein Beruf,", "tokens": ["Voll\u00b7f\u00fch\u00b7rend", "treu", ",", "was", "mein", "Be\u00b7ruf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "$,", "PRELS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und darf nach anderm Gut nicht streben.", "tokens": ["Und", "darf", "nach", "an\u00b7derm", "Gut", "nicht", "stre\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "PIS", "ADJD", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So lange der Natur in Armen,", "tokens": ["So", "lan\u00b7ge", "der", "Na\u00b7tur", "in", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der erstgeborne Erdensohn", "tokens": ["Der", "erst\u00b7ge\u00b7bor\u00b7ne", "Er\u00b7den\u00b7sohn"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An ihrer Brust nur will erwarmen,", "tokens": ["An", "ih\u00b7rer", "Brust", "nur", "will", "er\u00b7war\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo f\u00e4nde wohl sein Herz den Lohn?", "tokens": ["Wo", "f\u00e4n\u00b7de", "wohl", "sein", "Herz", "den", "Lohn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da mu\u00df er bald sich freun, bald weinen,", "tokens": ["Da", "mu\u00df", "er", "bald", "sich", "freun", ",", "bald", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PRF", "VVINF", "$,", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der fl\u00fccht'gen Sch\u00f6nheit Kranz vereinen,", "tokens": ["Der", "fl\u00fccht'\u00b7gen", "Sch\u00f6n\u00b7heit", "Kranz", "ver\u00b7ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und Herz von Herzen dann getrennt,", "tokens": ["Und", "Herz", "von", "Her\u00b7zen", "dann", "ge\u00b7trennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In w\u00fcster Leere umgetrieben,", "tokens": ["In", "w\u00fcs\u00b7ter", "Lee\u00b7re", "um\u00b7ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Flieht ihn das Leben, wie er's kennt,", "tokens": ["Flieht", "ihn", "das", "Le\u00b7ben", ",", "wie", "er's", "kennt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Er f\u00fchlt den Tod in seinem Lieben.", "tokens": ["Er", "f\u00fchlt", "den", "Tod", "in", "sei\u00b7nem", "Lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nun aber wei\u00df ich, wie du leitest,", "tokens": ["Nun", "a\u00b7ber", "wei\u00df", "ich", ",", "wie", "du", "lei\u00b7test", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die selber sich dir anvertraut,", "tokens": ["Die", "sel\u00b7ber", "sich", "dir", "an\u00b7ver\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PRF", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie allen du den Weg bereitest,", "tokens": ["Wie", "al\u00b7len", "du", "den", "Weg", "be\u00b7rei\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die du als Kinder hast geschaut.", "tokens": ["Die", "du", "als", "Kin\u00b7der", "hast", "ge\u00b7schaut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "KOUS", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Endlich erwacht vom ird'schen Schlummer,", "tokens": ["End\u00b7lich", "er\u00b7wacht", "vom", "ird'\u00b7schen", "Schlum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Gewaffnet gegen Sorg' und Kummer,", "tokens": ["Ge\u00b7waff\u00b7net", "ge\u00b7gen", "Sor\u00b7g'", "und", "Kum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "F\u00fchl' ich mit Beben die Gewalt", "tokens": ["F\u00fchl'", "ich", "mit", "Be\u00b7ben", "die", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Des hohen Bundes im Gebete,", "tokens": ["Des", "ho\u00b7hen", "Bun\u00b7des", "im", "Ge\u00b7be\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Wo dieser Erd' der Geist entwallt", "tokens": ["Wo", "die\u00b7ser", "Erd'", "der", "Geist", "ent\u00b7wallt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PDAT", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Zu Gott als seiner Ruhest\u00e4tte.", "tokens": ["Zu", "Gott", "als", "sei\u00b7ner", "Ru\u00b7he\u00b7st\u00e4t\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KOKOM", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wer einmal, Herr! dich angerufen,", "tokens": ["Wer", "ein\u00b7mal", ",", "Herr", "!", "dich", "an\u00b7ge\u00b7ru\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADV", "$,", "NN", "$.", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Tritt ein in fremde Geisteswelt,", "tokens": ["Tritt", "ein", "in", "frem\u00b7de", "Geis\u00b7tes\u00b7welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "K\u00fchn wandelt er die Himmelsstufen,", "tokens": ["K\u00fchn", "wan\u00b7delt", "er", "die", "Him\u00b7mels\u00b7stu\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo deiner Liebe Hauch ihn h\u00e4lt;", "tokens": ["Wo", "dei\u00b7ner", "Lie\u00b7be", "Hauch", "ihn", "h\u00e4lt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Leuchten aus des Herzens Grunde", "tokens": ["Ein", "Leuch\u00b7ten", "aus", "des", "Her\u00b7zens", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Kn\u00fcpft ihn an dich zu ew'gem Bunde.", "tokens": ["Kn\u00fcpft", "ihn", "an", "dich", "zu", "ew'\u00b7gem", "Bun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Frei von der ird'schen Fessel Band,", "tokens": ["Frei", "von", "der", "ird'\u00b7schen", "Fes\u00b7sel", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Dem weltlichen Geschick entzogen,", "tokens": ["Dem", "welt\u00b7li\u00b7chen", "Ge\u00b7schick", "ent\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Leitet fortan ihn deine Hand", "tokens": ["Lei\u00b7tet", "for\u00b7tan", "ihn", "dei\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPER", "PPOSAT", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Durch dieses Daseins wilde Wogen.", "tokens": ["Durch", "die\u00b7ses", "Da\u00b7seins", "wil\u00b7de", "Wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "So la\u00df auch mich nicht untersinken,", "tokens": ["So", "la\u00df", "auch", "mich", "nicht", "un\u00b7ter\u00b7sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ADV", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verdopple mir noch Kraft und Mut,", "tokens": ["Ver\u00b7dopp\u00b7le", "mir", "noch", "Kraft", "und", "Mut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gehorsam folg ich deinen Winken,", "tokens": ["Ge\u00b7hor\u00b7sam", "folg", "ich", "dei\u00b7nen", "Win\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dein ist mein Trachten, dein mein Blut.", "tokens": ["Dein", "ist", "mein", "Trach\u00b7ten", ",", "dein", "mein", "Blut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und woll'n mich Schmerzen wild ergreifen,", "tokens": ["Und", "woll'n", "mich", "Schmer\u00b7zen", "wild", "er\u00b7grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Trauer Sturmwind unst\u00e4t schweifen,", "tokens": ["Der", "Trau\u00b7er", "Sturm\u00b7wind", "un\u00b7st\u00e4t", "schwei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So f\u00fchre du mir Freunde zu,", "tokens": ["So", "f\u00fch\u00b7re", "du", "mir", "Freun\u00b7de", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die dir getreu in gleicher Liebe", "tokens": ["Die", "dir", "ge\u00b7treu", "in", "glei\u00b7cher", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Ins Herz mir hauchen sanfte Ruh,", "tokens": ["Ins", "Herz", "mir", "hau\u00b7chen", "sanf\u00b7te", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "M\u00e4nnlich vereint mit starkem Triebe.", "tokens": ["M\u00e4nn\u00b7lich", "ver\u00b7eint", "mit", "star\u00b7kem", "Trie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.6": {"line.1": {"text": "Mit Mut soll sich der Mann umkleiden", "tokens": ["Mit", "Mut", "soll", "sich", "der", "Mann", "um\u00b7klei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VMFIN", "PRF", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In dieser wilden Zeiten Sturm,", "tokens": ["In", "die\u00b7ser", "wil\u00b7den", "Zei\u00b7ten", "Sturm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Standhaft dastehn in allen Leiden,", "tokens": ["Stand\u00b7haft", "das\u00b7tehn", "in", "al\u00b7len", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im w\u00fcsten Meer ein Felsenturm;", "tokens": ["Im", "w\u00fcs\u00b7ten", "Meer", "ein", "Fel\u00b7sen\u00b7turm", ";"], "token_info":