{"textgrid.poem.37899": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Weine nur nicht", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weine, weine, weine nur nicht,", "tokens": ["Wei\u00b7ne", ",", "wei\u00b7ne", ",", "wei\u00b7ne", "nur", "nicht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "ADV", "PTKNEG", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ich will dich lieben, doch heute nicht,", "tokens": ["Ich", "will", "dich", "lie\u00b7ben", ",", "doch", "heu\u00b7te", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "$,", "ADV", "ADV", "PTKNEG", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich will dich ehren so viel ich kann,", "tokens": ["Ich", "will", "dich", "eh\u00b7ren", "so", "viel", "ich", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "ADV", "ADV", "PPER", "VMFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Aber's Nehmen, 's Nehmen,", "tokens": ["A\u00b7ber's", "Neh\u00b7men", ",", "'s", "Neh\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PPER", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Aber's Nehmen steht mir nicht an.", "tokens": ["A\u00b7ber's", "Neh\u00b7men", "steht", "mir", "nicht", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "+-+-+--+", "measure": "iambi