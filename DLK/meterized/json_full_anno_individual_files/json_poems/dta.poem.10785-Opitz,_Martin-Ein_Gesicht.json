{"dta.poem.10785": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Ein Gesicht/", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Ich seh' ein F\u00fcncklein klein noch glimmendt in der Eschen/", "tokens": ["Ich", "seh'", "ein", "F\u00fcnc\u00b7klein", "klein", "noch", "glim\u00b7mendt", "in", "der", "E\u00b7schen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "ADV", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das kan allein/ die solchs hat angez\u00fcndt/ erleschen/", "tokens": ["Das", "kan", "al\u00b7lein", "/", "die", "solchs", "hat", "an\u00b7ge\u00b7z\u00fcndt", "/", "er\u00b7le\u00b7schen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "$(", "ART", "PIS", "VAFIN", "VVPP", "$(", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es glimt/ vnd glimt so lang/ bi\u00df endtlich gar au\u00dfbricht.", "tokens": ["Es", "glimt", "/", "vnd", "glimt", "so", "lang", "/", "bi\u00df", "endt\u00b7lich", "gar", "au\u00df\u00b7bricht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KON", "VVFIN", "ADV", "ADJD", "$(", "APPR", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vnd ein gro\u00df mechtig Fewr im Hertzen mein anricht.", "tokens": ["Vnd", "ein", "gro\u00df", "mech\u00b7tig", "Fewr", "im", "Hert\u00b7zen", "mein", "an\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJD", "ADJD", "NN", "APPRART", "NN", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich seh\u2019/ ich seh\u2019 ein Blum in jhrer gr\u00fcnen Jugendt/", "tokens": ["Ich", "seh'", "/", "ich", "seh'", "ein", "Blum", "in", "jhrer", "gr\u00fc\u00b7nen", "Ju\u00b7gendt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VVFIN", "ART", "NE", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "In jhrer besten bl\u00fcth/ begabt mit vieler Tugendt/", "tokens": ["In", "jhrer", "bes\u00b7ten", "bl\u00fcth", "/", "be\u00b7gabt", "mit", "vie\u00b7ler", "Tu\u00b7gendt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$(", "ADJD", "APPR", "PIAT", "NN", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.7": {"text": "Mit w\u00fcrckung mancherley/ mit s\u00fcsser lebens Krafft/", "tokens": ["Mit", "w\u00fcr\u00b7ckung", "man\u00b7cher\u00b7ley", "/", "mit", "s\u00fcs\u00b7ser", "le\u00b7bens", "Krafft", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIAT", "$(", "APPR", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mit k\u00f6stlichem Geruch vnd zartem Honigsafft/", "tokens": ["Mit", "k\u00f6st\u00b7li\u00b7chem", "Ge\u00b7ruch", "vnd", "zar\u00b7tem", "Ho\u00b7nigs\u00b7afft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Narcissus Sch\u00f6nheit jhr gantz ehrerbietig weichet/", "tokens": ["Nar\u00b7cis\u00b7sus", "Sch\u00f6n\u00b7heit", "jhr", "gantz", "ehr\u00b7er\u00b7bie\u00b7tig", "wei\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PPER", "ADV", "ADJD", "VVFIN", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.10": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.11": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.12": {"text": "Ist nichts/ wie auch die Ros der ", "tokens": ["Ist", "nichts", "/", "wie", "auch", "die", "Ros", "der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIS", "$(", "KOKOM", "ADV", "ART", "NN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Dann sie dieselbige vorlengst her schamrot machet/", "tokens": ["Dann", "sie", "die\u00b7sel\u00b7bi\u00b7ge", "vor\u00b7lengst", "her", "scham\u00b7rot", "ma\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PDS", "VVFIN", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "So/ da\u00df auch ", "tokens": ["So", "/", "da\u00df", "auch"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADV", "$(", "KOUS", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.15": {"text": "Doch ist er drauff bedacht/ wie er sie rechen m\u00f6g/", "tokens": ["Doch", "ist", "er", "drauff", "be\u00b7dacht", "/", "wie", "er", "sie", "re\u00b7chen", "m\u00f6g", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PAV", "VVPP", "$(", "PWAV", "PPER", "PPER", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Vnd seiner Mutter Ehr vnd guten Namens pfleg.", "tokens": ["Vnd", "sei\u00b7ner", "Mut\u00b7ter", "Ehr", "vnd", "gu\u00b7ten", "Na\u00b7mens", "pfleg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "KON", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Er r\u00fchrt ohn vnderla\u00df mein eingeeschert Hertze/", "tokens": ["Er", "r\u00fchrt", "ohn", "vn\u00b7der\u00b7la\u00df", "mein", "ein\u00b7ge\u00b7e\u00b7schert", "Hert\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PPOSAT", "VVPP", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Vnd macht jhm grosse m\u00fch/ vnd mir noch gr\u00f6ssern schmertze/", "tokens": ["Vnd", "macht", "jhm", "gros\u00b7se", "m\u00fch", "/", "vnd", "mir", "noch", "gr\u00f6s\u00b7sern", "schmert\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "ADJD", "$(", "KON", "PPER", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Er bleset immer auff/ er sch\u00fcret immer zu/", "tokens": ["Er", "ble\u00b7set", "im\u00b7mer", "auff", "/", "er", "sch\u00fc\u00b7ret", "im\u00b7mer", "zu", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "$(", "PPER", "VVFIN", "ADV", "PTKZU", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "BndVnd lest mir Tag vnd Nacht kein einig rast noch ruh.", "tokens": ["Bnd", "Vnd", "lest", "mir", "Tag", "vnd", "Nacht", "kein", "ei\u00b7nig", "rast", "noch", "ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVIMP", "PPER", "NN", "KON", "NN", "PIAT", "ADJD", "VVFIN", "ADV", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.21": {"text": "Jetzt brennet schon daher die helle liechte Flamme/", "tokens": ["Jetzt", "bren\u00b7net", "schon", "da\u00b7her", "die", "hel\u00b7le", "liech\u00b7te", "Flam\u00b7me", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PAV", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Vnd macht/ da\u00df ich mir werd jhrt wegen selber gramme/", "tokens": ["Vnd", "macht", "/", "da\u00df", "ich", "mir", "werd", "jhrt", "we\u00b7gen", "sel\u00b7ber", "gram\u00b7me", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KOUS", "PPER", "PPER", "VAFIN", "ADJD", "APPR", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Der Blum/ der sch\u00f6nen Blum mich dauret also sehr/", "tokens": ["Der", "Blum", "/", "der", "sch\u00f6\u00b7nen", "Blum", "mich", "dau\u00b7ret", "al\u00b7so", "sehr", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$(", "ART", "ADJA", "NE", "PPER", "VVFIN", "ADV", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Dann er sie will kartzumb nitlassen stehen mehr/", "tokens": ["Dann", "er", "sie", "will", "kart\u00b7zumb", "nit\u00b7las\u00b7sen", "ste\u00b7hen", "mehr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PPER", "VMFIN", "ADJD", "VVINF", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ich seh\u2019 ich seh\u2019 ich seh\u2019: Ach/ ach er will sie brechen/", "tokens": ["Ich", "seh'", "ich", "seh'", "ich", "seh'", ":", "Ach", "/", "ach", "er", "will", "sie", "bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "PPER", "VVFIN", "$.", "ITJ", "$(", "XY", "PPER", "VMFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Die Blum/ die sch\u00f6ne Blum! So/ spricht er/ mu\u00df ich rechen", "tokens": ["Die", "Blum", "/", "die", "sch\u00f6\u00b7ne", "Blum", "!", "So", "/", "spricht", "er", "/", "mu\u00df", "ich", "re\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NE", "$(", "ART", "ADJA", "NE", "$.", "ADV", "$(", "VVFIN", "PPER", "$(", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Venus die G\u00f6ttin gro\u00df/ erweisen meine Sterck/", "tokens": ["Ve\u00b7nus", "die", "G\u00f6t\u00b7tin", "gro\u00df", "/", "er\u00b7wei\u00b7sen", "mei\u00b7ne", "Sterck", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "$(", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Ihr zum Exempel/ mir zu Lob ein nutzlich werck.", "tokens": ["Ihr", "zum", "Ex\u00b7em\u00b7pel", "/", "mir", "zu", "Lob", "ein", "nutz\u00b7lich", "werck", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "$(", "PPER", "APPR", "NN", "ART", "ADJD", "PTKVZ", "$."], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.29": {"text": "Vffseinen Fl\u00fcgeln er sie eilendts her thut f\u00fchren/", "tokens": ["Vffsei\u00b7nen", "Fl\u00fc\u00b7geln", "er", "sie", "ei\u00b7lendts", "her", "thut", "f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "PPER", "ADV", "APZR", "VVFIN", "VVINF", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.30": {"text": "Nicht willens einig Stundt noch Zeit mehr zuverlieren/", "tokens": ["Nicht", "wil\u00b7lens", "ei\u00b7nig", "Stundt", "noch", "Zeit", "mehr", "zu\u00b7ver\u00b7lie\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "ADJD", "NN", "ADV", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Bi\u00df er sie in dem Fewr vff meinem Hertzaltar", "tokens": ["Bi\u00df", "er", "sie", "in", "dem", "Fewr", "vff", "mei\u00b7nem", "Hert\u00b7zal\u00b7tar"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}}}}