{"dta.poem.7164": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung des Schlafs,  \n als  \n eine G\u00f6ttliche Wohlthat,  \n bey dem 1728sten Jahres-Wechsel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mu\u00df ich nun auch in diesem Jahr,", "tokens": ["Mu\u00df", "ich", "nun", "auch", "in", "die\u00b7sem", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach unsrer Stadt Verfassung, mich bequemen,", "tokens": ["Nach", "uns\u00b7rer", "Stadt", "Ver\u00b7fas\u00b7sung", ",", "mich", "be\u00b7que\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,", "PPER", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das schwere Richter-Amt zu \u00fcbernehmen;", "tokens": ["Das", "schwe\u00b7re", "Rich\u00b7ter\u00b7Amt", "zu", "\u00fc\u00b7ber\u00b7neh\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So sch\u00fctze Du mich, HERR, f\u00fcr allerley Gefahr,", "tokens": ["So", "sch\u00fct\u00b7ze", "Du", "mich", ",", "HeRR", ",", "f\u00fcr", "al\u00b7ler\u00b7ley", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "NN", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und r\u00fcste Du mich aus, mit solcher Krafft,", "tokens": ["Und", "r\u00fcs\u00b7te", "Du", "mich", "aus", ",", "mit", "sol\u00b7cher", "Krafft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "PTKVZ", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mit solchen Tugenden, mit solcher Eigenschafft,", "tokens": ["Mit", "sol\u00b7chen", "Tu\u00b7gen\u00b7den", ",", "mit", "sol\u00b7cher", "Ei\u00b7gen\u00b7schafft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die ein so wichtig Amt erfodert!", "tokens": ["Die", "ein", "so", "wich\u00b7tig", "Amt", "er\u00b7fo\u00b7dert", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADV", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn etwan, ohne Noth, ein schneller Eifer lodert;", "tokens": ["Wenn", "et\u00b7wan", ",", "oh\u00b7ne", "Noth", ",", "ein", "schnel\u00b7ler", "Ei\u00b7fer", "lo\u00b7dert", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "$,", "KOUI", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "La\u00df mich ihn mit Vernunfft bes\u00e4nfftigen und d\u00e4mpffen!", "tokens": ["La\u00df", "mich", "ihn", "mit", "Ver\u00b7nunfft", "be\u00b7s\u00e4nff\u00b7ti\u00b7gen", "und", "d\u00e4mpf\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "APPR", "NN", "VVPP", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "La\u00df mich jedoch mit Ernst der Laster Brut bek\u00e4mpffen!", "tokens": ["La\u00df", "mich", "je\u00b7doch", "mit", "Ernst", "der", "Las\u00b7ter", "Brut", "be\u00b7k\u00e4mpf\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "NE", "ART", "NN", "NN", "VVINF", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.11": {"text": "Ach sende mir dazu aus Deiner heil\u2019gen H\u00f6he", "tokens": ["Ach", "sen\u00b7de", "mir", "da\u00b7zu", "aus", "Dei\u00b7ner", "heil'\u00b7gen", "H\u00f6\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "PPER", "PAV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der Weisheit Licht, voll Vorsicht und voll Klarheit,", "tokens": ["Der", "Weis\u00b7heit", "Licht", ",", "voll", "Vor\u00b7sicht", "und", "voll", "Klar\u00b7heit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ADJD", "NN", "KON", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Da\u00df ich die offtermahls so sehr versteckte Wahrheit", "tokens": ["Da\u00df", "ich", "die", "off\u00b7ter\u00b7mahls", "so", "sehr", "ver\u00b7steck\u00b7te", "Wahr\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADV", "ADV", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Kann durch den schwartzen Dunst der schn\u00f6den L\u00fcgen", "tokens": ["Kann", "durch", "den", "schwart\u00b7zen", "Dunst", "der", "schn\u00f6\u00b7den", "L\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "sehn!", "tokens": ["sehn", "!"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+", "measure": "single.up"}, "line.16": {"text": "Gieb, da\u00df ich offt den Zweck von meinem Amt ermesse,", "tokens": ["Gieb", ",", "da\u00df", "ich", "offt", "den", "Zweck", "von", "mei\u00b7nem", "Amt", "er\u00b7mes\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und nie der Billigkeit und meiner Pflicht vergesse;", "tokens": ["Und", "nie", "der", "Bil\u00b7lig\u00b7keit", "und", "mei\u00b7ner", "Pflicht", "ver\u00b7ges\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die Frommen nemlich zu besch\u00fctzen,", "tokens": ["Die", "From\u00b7men", "nem\u00b7lich", "zu", "be\u00b7sch\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.19": {"text": "Die B\u00f6sen, kann es seyn, zur Besserung zu leiten,", "tokens": ["Die", "B\u00f6\u00b7sen", ",", "kann", "es", "seyn", ",", "zur", "Bes\u00b7se\u00b7rung", "zu", "lei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VMFIN", "PPER", "VAINF", "$,", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und die Grund-B\u00f6sen auszureuten!", "tokens": ["Und", "die", "Grun\u00b7dB\u00f6\u00b7sen", "aus\u00b7zu\u00b7reu\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$."], "meter": "-+++-+-+-", "measure": "unknown.measure.penta"}}}}}