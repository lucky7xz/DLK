{"textgrid.poem.53266": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "[was hat ein frommer Christ doch Noth]", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was hat ein frommer Christ doch Noth", "tokens": ["Was", "hat", "ein", "from\u00b7mer", "Christ", "doch", "Noth"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So heydnisch sich zu halten,", "tokens": ["So", "heyd\u00b7nisch", "sich", "zu", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn Gott jhm selig durch den Todt", "tokens": ["Wenn", "Gott", "jhm", "se\u00b7lig", "durch", "den", "Todt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Seinen l\u00e4st erkalten?", "tokens": ["Die", "Sei\u00b7nen", "l\u00e4st", "er\u00b7kal\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSS", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ihm ist ja au\u00df der Schrifft bekant", "tokens": ["Ihm", "ist", "ja", "au\u00df", "der", "Schrifft", "be\u00b7kant"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df, die auff Christum sterben,", "tokens": ["Da\u00df", ",", "die", "auff", "Chris\u00b7tum", "ster\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PRELS", "APPR", "NE", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Den Himmel vnser Vaterland", "tokens": ["Den", "Him\u00b7mel", "vn\u00b7ser", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Vnwiedersprechlich erben,", "tokens": ["Vn\u00b7wie\u00b7der\u00b7sprech\u00b7lich", "er\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da Gott den wahren Reichthum Sich", "tokens": ["Da", "Gott", "den", "wah\u00b7ren", "Reicht\u00b7hum", "Sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ART", "ADJA", "NN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Giebt herrlich zu geniessen,", "tokens": ["Giebt", "herr\u00b7lich", "zu", "ge\u00b7nies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da Frewden sind, vnd mildiglich", "tokens": ["Da", "Frew\u00b7den", "sind", ",", "vnd", "mil\u00b7dig\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "NN", "VAFIN", "$,", "KON", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Lebens Str\u00f6me fliessen:", "tokens": ["Des", "Le\u00b7bens", "Str\u00f6\u00b7me", "flies\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da weder Hertzleid noch Gefahr", "tokens": ["Da", "we\u00b7der", "Hertz\u00b7leid", "noch", "Ge\u00b7fahr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KON", "NN", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mag ewig hin gelangen,", "tokens": ["Mag", "e\u00b7wig", "hin", "ge\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Vnd da der lieben Engel Schar", "tokens": ["Vnd", "da", "der", "lie\u00b7ben", "En\u00b7gel", "Schar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Frommen stets vmbfangen.", "tokens": ["Die", "From\u00b7men", "stets", "vmb\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Au\u00dferwehlten Frewd vnd Lust", "tokens": ["Der", "Au\u00b7\u00dfer\u00b7wehl\u00b7ten", "Frewd", "vnd", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geht vber alle Zungen,", "tokens": ["Geht", "vber", "al\u00b7le", "Zun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Sie ist noch keinem je bewust,", "tokens": ["Sie", "ist", "noch", "kei\u00b7nem", "je", "be\u00b7wust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist keines Hertz durchdrungen,", "tokens": ["Ist", "kei\u00b7nes", "Hertz", "durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Kein Aug hat jemahls angesehn,", "tokens": ["Kein", "Aug", "hat", "je\u00b7mahls", "an\u00b7ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kein Ohr hat je geh\u00f6ret,", "tokens": ["Kein", "Ohr", "hat", "je", "ge\u00b7h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Was dem dort gutes sol geschehn", "tokens": ["Was", "dem", "dort", "gu\u00b7tes", "sol", "ge\u00b7schehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "ADV", "ADJA", "VMFIN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Gott hie hertzlich ehret.", "tokens": ["Der", "Gott", "hie", "hertz\u00b7lich", "eh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wer diese Sachen allzumahl", "tokens": ["Wer", "die\u00b7se", "Sa\u00b7chen", "all\u00b7zu\u00b7mahl"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PDAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich Christlich l\u00e4st bedeuten,", "tokens": ["Sich", "Christ\u00b7lich", "l\u00e4st", "be\u00b7deu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "NE", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wird lachend au\u00df dem Jammerthal", "tokens": ["Wird", "la\u00b7chend", "au\u00df", "dem", "Jam\u00b7mer\u00b7thal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Seinen hin begleiten,", "tokens": ["Die", "Sei\u00b7nen", "hin", "be\u00b7glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Vnd w\u00fcnschen, da\u00df auch er der Pein", "tokens": ["Vnd", "w\u00fcn\u00b7schen", ",", "da\u00df", "auch", "er", "der", "Pein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ADV", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des Kummers dieser Erden", "tokens": ["Des", "Kum\u00b7mers", "die\u00b7ser", "Er\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Durch ein gew\u00fcnschtes St\u00fcndelein", "tokens": ["Durch", "ein", "ge\u00b7w\u00fcnschtes", "St\u00fcn\u00b7del\u00b7ein"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Bald mag befreyet werden.", "tokens": ["Bald", "mag", "be\u00b7fre\u00b7yet", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wir wollen, die sich fort gemacht,", "tokens": ["Wir", "wol\u00b7len", ",", "die", "sich", "fort", "ge\u00b7macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PRELS", "PRF", "PTKVZ", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Friede schlaffen lassen,", "tokens": ["Im", "Frie\u00b7de", "schlaf\u00b7fen", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vnd blo\u00df nur seyn auff vns bedacht,", "tokens": ["Vnd", "blo\u00df", "nur", "seyn", "auff", "vns", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PPOSAT", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der S\u00fcnden wege hassen,", "tokens": ["Der", "S\u00fcn\u00b7den", "we\u00b7ge", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df wir, weil sie nun ewig nicht", "tokens": ["Da\u00df", "wir", ",", "weil", "sie", "nun", "e\u00b7wig", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "KOUS", "PPER", "ADV", "ADJD", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zu vns zur\u00fccke kommen,", "tokens": ["Zu", "vns", "zu\u00b7r\u00fc\u00b7cke", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Zu jhnen in das wahre Liecht", "tokens": ["Zu", "jh\u00b7nen", "in", "das", "wah\u00b7re", "Liecht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bald werden auffgenommen.", "tokens": ["Bald", "wer\u00b7den", "auff\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text"