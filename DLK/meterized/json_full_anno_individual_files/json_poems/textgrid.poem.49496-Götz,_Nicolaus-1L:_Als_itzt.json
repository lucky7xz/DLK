{"textgrid.poem.49496": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als itzt ", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als itzt ", "tokens": ["Als", "itzt"], "token_info": ["word", "word"], "pos": ["KOUS", "ADV"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Auf entspriesenden Blumen gebohren.", "tokens": ["Auf", "ent\u00b7sprie\u00b7sen\u00b7den", "Blu\u00b7men", "ge\u00b7boh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Und seine niedliche Bildung zu sehn, lies Venus ihn holen.", "tokens": ["Und", "sei\u00b7ne", "nied\u00b7li\u00b7che", "Bil\u00b7dung", "zu", "sehn", ",", "lies", "Ve\u00b7nus", "ihn", "ho\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$,", "VVFIN", "NN", "PPER", "VVINF", "$."], "meter": "-+-+--+-++--+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Schnell sprang Amor herbey, und sprach mit zornigem Muthe:", "tokens": ["Schnell", "sprang", "A\u00b7mor", "her\u00b7bey", ",", "und", "sprach", "mit", "zor\u00b7ni\u00b7gem", "Mu\u00b7the", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "NE", "PTKVZ", "$,", "KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "\u00bbwarum schenkte dann nicht das sonst so altkluge Schicksal", "tokens": ["\u00bb", "wa\u00b7rum", "schenk\u00b7te", "dann", "nicht", "das", "sonst", "so", "alt\u00b7klu\u00b7ge", "Schick\u00b7sal"], "token_info": ["pu