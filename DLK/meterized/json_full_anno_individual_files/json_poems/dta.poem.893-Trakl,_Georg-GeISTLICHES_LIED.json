{"dta.poem.893": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "GeISTLICHES LIED", "genre": "Lyrik", "period": "N.A.", "pub_year": "1913", "urn": "urn:nbn:de:kobv:b4-30357-9", "language": ["de:0.99"], "booktitle": "Trakl, Georg: Gedichte. Leipzig, 1913."}, "poem": {"stanza.1": {"line.1": {"text": "Zeichen, seltne Stickerein", "tokens": ["Zei\u00b7chen", ",", "selt\u00b7ne", "Sti\u00b7cke\u00b7rein"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Malt ein flatternd Blumenbeet.", "tokens": ["Malt", "ein", "flat\u00b7ternd", "Blu\u00b7men\u00b7beet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gottes blauer Odem weht", "tokens": ["Got\u00b7tes", "blau\u00b7er", "O\u00b7dem", "weht"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In den Gartensaal herein,", "tokens": ["In", "den", "Gar\u00b7ten\u00b7saal", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Heiter ein.", "tokens": ["Hei\u00b7ter", "ein", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Ragt ein Kreuz im wilden Wein.", "tokens": ["Ragt", "ein", "Kreuz", "im", "wil\u00b7den", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "H\u00f6r\u2019 im Dorf sich viele freun,", "tokens": ["H\u00f6r'", "im", "Dorf", "sich", "vie\u00b7le", "freun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "PRF", "PIS", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "G\u00e4rtner an der Mauer m\u00e4ht,", "tokens": ["G\u00e4rt\u00b7ner", "an", "der", "Mau\u00b7er", "m\u00e4ht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Leise eine Orgel geht,", "tokens": ["Lei\u00b7se", "ei\u00b7ne", "Or\u00b7gel", "geht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mischet Klang und goldenen Schein,", "tokens": ["Mi\u00b7schet", "Klang", "und", "gol\u00b7de\u00b7nen", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Klang und Schein.", "tokens": ["Klang", "und", "Schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Liebe segnet Brot und Wein.", "tokens": ["Lie\u00b7be", "seg\u00b7net", "Brot", "und", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "M\u00e4dchen kommen auch herein", "tokens": ["M\u00e4d\u00b7chen", "kom\u00b7men", "auch", "her\u00b7ein"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der Hahn zum letzten kr\u00e4ht.", "tokens": ["Und", "der", "Hahn", "zum", "letz\u00b7ten", "kr\u00e4ht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "ADJA", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Sacht ein morsches Gitter geht", "tokens": ["Sacht", "ein", "mor\u00b7sches", "Git\u00b7ter", "geht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und in Rosen Kranz und Reihn,", "tokens": ["Und", "in", "Ro\u00b7sen", "Kranz", "und", "Reihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "NN", "KON", "NE", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Rosenreihn", "tokens": ["Ro\u00b7sen\u00b7reihn"], "token_info": ["word"], "pos": ["NE"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Ruht Maria wei\u00df und fein.", "tokens": ["Ruht", "Ma\u00b7ria", "wei\u00df", "und", "fein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Bettler dort am alten Stein", "tokens": ["Bett\u00b7ler", "dort", "am", "al\u00b7ten", "Stein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Scheint verstorben im Gebet,", "tokens": ["Scheint", "ver\u00b7stor\u00b7ben", "im", "Ge\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVPP", "APPRART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Sanft ein Hirt vom H\u00fcgel geht", "tokens": ["Sanft", "ein", "Hirt", "vom", "H\u00fc\u00b7gel", "geht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ein Engel singt im Hain,", "tokens": ["Und", "ein", "En\u00b7gel", "singt", "im", "Hain", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nah am Hain", "tokens": ["Nah", "am", "Hain"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Kinder in den Schlaf hinein.", "tokens": ["Kin\u00b7der", "in", "den", "Schlaf", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}