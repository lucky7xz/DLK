{"dta.poem.14874": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die stille Gemeine.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Da taumelt' er und sank in's Gras", "tokens": ["Da", "tau\u00b7melt'", "er", "und", "sank", "in's", "Gras"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Betend im stillen Grunde,", "tokens": ["Be\u00b7tend", "im", "stil\u00b7len", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und wie Felsenquellen im Fr\u00fchling brach", "tokens": ["Und", "wie", "Fel\u00b7sen\u00b7quel\u00b7len", "im", "Fr\u00fch\u00b7ling", "brach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "NN", "APPRART", "NN", "VVFIN"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sein Herzblut aus allen Wunden.", "tokens": ["Sein", "Herz\u00b7blut", "aus", "al\u00b7len", "Wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}}}}}