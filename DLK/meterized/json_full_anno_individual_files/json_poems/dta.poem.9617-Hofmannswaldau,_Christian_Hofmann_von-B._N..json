{"dta.poem.9617": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "B. N.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Florette was umflort dein himmlisch angesicht?", "tokens": ["Flo\u00b7ret\u00b7te", "was", "um\u00b7flort", "dein", "himm\u00b7lisch", "an\u00b7ge\u00b7sicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "VVFIN", "PPOSAT", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kan auch ein blitz au\u00df lichten strahlen schiessen?", "tokens": ["Kan", "auch", "ein", "blitz", "au\u00df", "lich\u00b7ten", "strah\u00b7len", "schies\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "L\u00e4st jaspis und saphier auch kr\u00fcben regen fliessen?", "tokens": ["L\u00e4st", "jas\u00b7pis", "und", "sa\u00b7phier", "auch", "kr\u00fc\u00b7ben", "re\u00b7gen", "flies\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "KON", "ADV", "ADV", "ADJA", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Schont denn die sinsterni\u00df der sonnen nicht/", "tokens": ["Schont", "denn", "die", "sins\u00b7ter\u00b7ni\u00df", "der", "son\u00b7nen", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "ADJA", "PTKNEG", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und zeigen die entflammte kertzen/", "tokens": ["Und", "zei\u00b7gen", "die", "ent\u00b7flamm\u00b7te", "kert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df nichts als nebel und comet", "tokens": ["Da\u00df", "nichts", "als", "ne\u00b7bel", "und", "co\u00b7met"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "KOKOM", "ADJD", "KON", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Um deinen liebes-himmel steht/", "tokens": ["Um", "dei\u00b7nen", "lie\u00b7bes\u00b7him\u00b7mel", "steht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und nach entbrandter glut stets donnerkeilen schertzen?", "tokens": ["Und", "nach", "ent\u00b7brand\u00b7ter", "glut", "stets", "don\u00b7ner\u00b7kei\u00b7len", "schert\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So ist es: aber ach! was brauchstu doch f\u00fcr macht/", "tokens": ["So", "ist", "es", ":", "a\u00b7ber", "ach", "!", "was", "brauchs\u00b7tu", "doch", "f\u00fcr", "macht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$.", "ADV", "ADV", "$.", "PWS", "VVFIN", "ADV", "APPR", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mich schwachen sclaven v\u00f6llig zu besiegen?", "tokens": ["Mich", "schwa\u00b7chen", "scla\u00b7ven", "v\u00f6l\u00b7lig", "zu", "be\u00b7sie\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sprich nur/ so will ich dir zu deinen f\u00fcssen liegen/", "tokens": ["Sprich", "nur", "/", "so", "will", "ich", "dir", "zu", "dei\u00b7nen", "f\u00fcs\u00b7sen", "lie\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$(", "ADV", "VMFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die wunder und natur zu marmel macht.", "tokens": ["Die", "wun\u00b7der", "und", "na\u00b7tur", "zu", "mar\u00b7mel", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich will die schuld mit thr\u00e4nen b\u00fcssen/", "tokens": ["Ich", "will", "die", "schuld", "mit", "thr\u00e4\u00b7nen", "b\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJD", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Als zeugen meiner liebes-pein/", "tokens": ["Als", "zeu\u00b7gen", "mei\u00b7ner", "lie\u00b7bes\u00b7pein", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und solte die\u00df zu wenig seyn/", "tokens": ["Und", "sol\u00b7te", "die\u00df", "zu", "we\u00b7nig", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PDS", "PTKA", "PIS", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So soll mein warmes blut zu deinen fersen fliessen.", "tokens": ["So", "soll", "mein", "war\u00b7mes", "blut", "zu", "dei\u00b7nen", "fer\u00b7sen", "flies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich l\u00e4ugne/ werthe/ nicht/ da\u00df Myrto fehlen kan;", "tokens": ["Ich", "l\u00e4ug\u00b7ne", "/", "wert\u00b7he", "/", "nicht", "/", "da\u00df", "Myr\u00b7to", "feh\u00b7len", "kan", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "VVFIN", "$(", "PTKNEG", "$(", "KOUS", "NE", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der fall ist mir als menschen angebohren/", "tokens": ["Der", "fall", "ist", "mir", "als", "men\u00b7schen", "an\u00b7ge\u00b7boh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "KOUS", "NN", "VVPP", "$("], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Du bist als engel mir zum richter au\u00dferkohren/", "tokens": ["Du", "bist", "als", "en\u00b7gel", "mir", "zum", "rich\u00b7ter", "au\u00b7\u00dfer\u00b7koh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "NN", "PPER", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Drum nehm ich mit gedult die martern an.", "tokens": ["Drum", "nehm", "ich", "mit", "ge\u00b7dult", "die", "mar\u00b7tern", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Bestraffe nur mein bittres leiden", "tokens": ["Be\u00b7straf\u00b7fe", "nur", "mein", "bitt\u00b7res", "lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "ADJA", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Durch allzu hei\u00dfen eyfer nicht;", "tokens": ["Durch", "all\u00b7zu", "hei\u00b7\u00dfen", "ey\u00b7fer", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PTKA", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Denn was mir noch das hertze bricht/", "tokens": ["Denn", "was", "mir", "noch", "das", "hert\u00b7ze", "bricht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ist/ da\u00df ich armer soll von meiner seelen scheiden.", "tokens": ["Ist", "/", "da\u00df", "ich", "ar\u00b7mer", "soll", "von", "mei\u00b7ner", "see\u00b7len", "schei\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$(", "KOUS", "PPER", "ADJA", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Florette la\u00df dir doch den schmertz zu hertzen gehn;", "tokens": ["Flo\u00b7ret\u00b7te", "la\u00df", "dir", "doch", "den", "schmertz", "zu", "hert\u00b7zen", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVIMP", "PPER", "ADV", "ART", "ADJD", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein treuer knecht kan F\u00fcrsten ja bewegen;", "tokens": ["Ein", "treu\u00b7er", "knecht", "kan", "F\u00fcrs\u00b7ten", "ja", "be\u00b7we\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der himmel selber mu\u00df die ruthe niederlegen/", "tokens": ["Der", "him\u00b7mel", "sel\u00b7ber", "mu\u00df", "die", "ru\u00b7the", "nie\u00b7der\u00b7le\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wann wir vor ihm in hei\u00dfer andacht stehn.", "tokens": ["Wann", "wir", "vor", "ihm", "in", "hei\u00b7\u00dfer", "an\u00b7dacht", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPER", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich falle dir zu deinen f\u00fcssen/", "tokens": ["Ich", "fal\u00b7le", "dir", "zu", "dei\u00b7nen", "f\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mein hertze brennt dir f\u00fcr und f\u00fcr/", "tokens": ["Mein", "hert\u00b7ze", "brennt", "dir", "f\u00fcr", "und", "f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "KON", "APPR", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Warum wiltu denn l\u00e4nger mir", "tokens": ["Wa\u00b7rum", "wil\u00b7tu", "denn", "l\u00e4n\u00b7ger", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "ADV", "ADJD", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Vor einen apfel-griff dein Paradie\u00df verschliessen?", "tokens": ["Vor", "ei\u00b7nen", "ap\u00b7fel\u00b7griff", "dein", "Pa\u00b7ra\u00b7die\u00df", "ver\u00b7schlies\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Verzeihe/ sch\u00f6nstes bild/ ich rede fast zu viel:", "tokens": ["Ver\u00b7zei\u00b7he", "/", "sch\u00f6ns\u00b7tes", "bild", "/", "ich", "re\u00b7de", "fast", "zu", "viel", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "ADJA", "NN", "$(", "PPER", "VVFIN", "ADV", "PTKA", "PIS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein reiner mund kan alle fehler decken;", "tokens": ["Ein", "rei\u00b7ner", "mund", "kan", "al\u00b7le", "feh\u00b7ler", "de\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der menschen liebe kan die g\u00f6tter nicht beflecken/", "tokens": ["Der", "men\u00b7schen", "lie\u00b7be", "kan", "die", "g\u00f6t\u00b7ter", "nicht", "be\u00b7fle\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "VMFIN", "ART", "ADJA", "PTKNEG", "VVINF", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Man f\u00e4llt nicht/ wann man gleich hier fallen will.", "tokens": ["Man", "f\u00e4llt", "nicht", "/", "wann", "man", "gleich", "hier", "fal\u00b7len", "will", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKNEG", "$(", "PWAV", "PIS", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Was hab ich armer denn entzieret/", "tokens": ["Was", "hab", "ich", "ar\u00b7mer", "denn", "ent\u00b7zie\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADJA", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Indem ich deine liljen-brust", "tokens": ["In\u00b7dem", "ich", "dei\u00b7ne", "lil\u00b7jen\u00b7brust"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Vor zucker-s\u00fc\u00dfer liebes-lust", "tokens": ["Vor", "zu\u00b7cke\u00b7rs\u00fc\u00b7\u00dfer", "lie\u00b7bes\u00b7lust"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Durch einen feuchten ku\u00df auf erden anger\u00fchret?", "tokens": ["Durch", "ei\u00b7nen", "feuch\u00b7ten", "ku\u00df", "auf", "er\u00b7den", "an\u00b7ge\u00b7r\u00fch\u00b7ret", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Wiltu der tugend nach ein rechter Engel seyn/", "tokens": ["Wil\u00b7tu", "der", "tu\u00b7gend", "nach", "ein", "rech\u00b7ter", "En\u00b7gel", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So mustu auch/ was menschlich ist/ ver