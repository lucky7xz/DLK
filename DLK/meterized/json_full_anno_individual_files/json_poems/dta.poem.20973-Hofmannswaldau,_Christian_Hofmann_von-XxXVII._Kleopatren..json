{"dta.poem.20973": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "XxXVII.  \n Kleopatren.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Hier liegt Kleopatra das Wunder ihrer Zeit/", "tokens": ["Hier", "liegt", "Kleo\u00b7pat\u00b7ra", "das", "Wun\u00b7der", "ih\u00b7rer", "Zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wer sie gewesen ist das wei\u00df man weit und breit.", "tokens": ["Wer", "sie", "ge\u00b7we\u00b7sen", "ist", "das", "wei\u00df", "man", "weit", "und", "breit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAPP", "VAFIN", "PDS", "VVFIN", "PIS", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein ieder h\u00fctte sich viel Perlen herzubringen/", "tokens": ["Ein", "ie\u00b7der", "h\u00fct\u00b7te", "sich", "viel", "Per\u00b7len", "her\u00b7zu\u00b7brin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PRF", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil sie gewohnet ist dieselben zu verschlingen.", "tokens": ["Weil", "sie", "ge\u00b7woh\u00b7net", "ist", "die\u00b7sel\u00b7ben", "zu", "ver\u00b7schlin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "PDS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}