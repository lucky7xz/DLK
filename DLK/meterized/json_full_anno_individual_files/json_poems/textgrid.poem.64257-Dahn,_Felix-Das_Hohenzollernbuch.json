{"textgrid.poem.64257": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Das Hohenzollernbuch", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dies Buch will wandern in die deutschen H\u00e4user!", "tokens": ["Dies", "Buch", "will", "wan\u00b7dern", "in", "die", "deut\u00b7schen", "H\u00e4u\u00b7ser", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VMFIN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein St\u00fcck des deutschen Hausrats will es werden,", "tokens": ["Ein", "St\u00fcck", "des", "deut\u00b7schen", "Haus\u00b7rats", "will", "es", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VMFIN", "PPER", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch nicht im Prunksaal stehn bei anderm Prunk, \u2013", "tokens": ["Doch", "nicht", "im", "Prunk\u00b7saal", "stehn", "bei", "an\u00b7derm", "Prunk", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PTKNEG", "APPRART", "NN", "VVFIN", "APPR", "PIS", "NN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nein, auf dem Tisch, darum allabendlich", "tokens": ["Nein", ",", "auf", "dem", "Tisch", ",", "da\u00b7rum", "al\u00b7la\u00b7bend\u00b7lich"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKANT", "$,", "APPR", "ART", "NN", "$,", "PAV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Lampe Schein die Eltern und die Kinder", "tokens": ["Der", "Lam\u00b7pe", "Schein", "die", "El\u00b7tern", "und", "die", "Kin\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Versammelt nach vollbrachtem Tagewerk, \u2013", "tokens": ["Ver\u00b7sam\u00b7melt", "nach", "voll\u00b7brach\u00b7tem", "Ta\u00b7ge\u00b7werk", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dort will es ruhn, vertraulich, handgerecht. \u2013", "tokens": ["Dort", "will", "es", "ruhn", ",", "ver\u00b7trau\u00b7lich", ",", "hand\u00b7ge\u00b7recht", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,", "ADJD", "$,", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Aufschlagen soll's der Vater hie und da", "tokens": ["Auf\u00b7schla\u00b7gen", "soll's", "der", "Va\u00b7ter", "hie", "und", "da"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "ART", "NN", "ADV", "KON", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und soll daraus den trotzgemuten Knaben,", "tokens": ["Und", "soll", "da\u00b7raus", "den", "trotz\u00b7ge\u00b7mu\u00b7ten", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Den blondgez\u00f6pften M\u00e4dchen kurz und schlicht", "tokens": ["Den", "blond\u00b7ge\u00b7z\u00f6pf\u00b7ten", "M\u00e4d\u00b7chen", "kurz", "und", "schlicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Berichten von den Freuden und den Leiden", "tokens": ["Be\u00b7rich\u00b7ten", "von", "den", "Freu\u00b7den", "und", "den", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Des Hauses Hohenzollern;", "tokens": ["Des", "Hau\u00b7ses", "Ho\u00b7hen\u00b7zol\u00b7lern", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Wie sie, entspro\u00dft dem Stamm der Alamannen,", "tokens": ["Wie", "sie", ",", "ent\u00b7spro\u00dft", "dem", "Stamm", "der", "A\u00b7la\u00b7man\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Bei ihrem Flug vom Schwabenland zur See", "tokens": ["Bei", "ih\u00b7rem", "Flug", "vom", "Schwa\u00b7ben\u00b7land", "zur", "See"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Auf N\u00fcrnbergs Feste kurz den Fittich ruhten,", "tokens": ["Auf", "N\u00fcrn\u00b7bergs", "Fes\u00b7te", "kurz", "den", "Fit\u00b7tich", "ruh\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Bis immer weiter sie der Adlerschwung", "tokens": ["Bis", "im\u00b7mer", "wei\u00b7ter", "sie", "der", "Ad\u00b7ler\u00b7schwung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADV", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Bis an das Bernsteinhaff nach Osten trug.", "tokens": ["Bis", "an", "das", "Bern\u00b7stein\u00b7haff", "nach", "Os\u00b7ten", "trug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Und wie zuletzt, zur\u00fcck zum S\u00fcden greifend,", "tokens": ["Und", "wie", "zu\u00b7letzt", ",", "zu\u00b7r\u00fcck", "zum", "S\u00fc\u00b7den", "grei\u00b7fend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "$,", "ADV", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Sie alle deutschen St\u00e4mme fest vereint,", "tokens": ["Sie", "al\u00b7le", "deut\u00b7schen", "St\u00e4m\u00b7me", "fest", "ver\u00b7eint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Vereint im Glanz der kaiserlichen Krone,", "tokens": ["Ver\u00b7eint", "im", "Glanz", "der", "kai\u00b7ser\u00b7li\u00b7chen", "Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Die ihnen dort auf Sedans H\u00fcgelrund", "tokens": ["Die", "ih\u00b7nen", "dort", "auf", "Se\u00b7dans", "H\u00fc\u00b7gel\u00b7rund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "APPR", "NE", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Der Gott des Siegs geschmiedet in der Schlacht! \u2013", "tokens": ["Der", "Gott", "des", "Siegs", "ge\u00b7schmie\u00b7det", "in", "der", "Schlacht", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch nicht von Siegen und von Freuden nur,", "tokens": ["Doch", "nicht", "von", "Sie\u00b7gen", "und", "von", "Freu\u00b7den", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "NN", "KON", "APPR", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von b\u00f6ser Zeit auch meldet dieses Buch:", "tokens": ["Von", "b\u00f6\u00b7ser", "Zeit", "auch", "mel\u00b7det", "die\u00b7ses", "Buch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ja, auch dem Schmerz geb\u00fchret sein Gedenktag.", "tokens": ["Ja", ",", "auch", "dem", "Schmerz", "ge\u00b7b\u00fch\u00b7ret", "sein", "Ge\u00b7denk\u00b7tag", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er mahnt uns erst, wie selbst verschuldet Unheil", "tokens": ["Er", "mahnt", "uns", "erst", ",", "wie", "selbst", "ver\u00b7schul\u00b7det", "Un\u00b7heil"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PWAV", "ADV", "VVFIN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Am schwersten dr\u00fcckt: jedoch er lehrt uns auch,", "tokens": ["Am", "schwers\u00b7ten", "dr\u00fcckt", ":", "je\u00b7doch", "er", "lehrt", "uns", "auch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVFIN", "$.", "ADV", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wie aus dem Abgrund Mannesmut sich hebt", "tokens": ["Wie", "aus", "dem", "Ab\u00b7grund", "Man\u00b7nes\u00b7mut", "sich", "hebt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Am starken, treuen Eichenstab der Pflicht,", "tokens": ["Am", "star\u00b7ken", ",", "treu\u00b7en", "Ei\u00b7chen\u00b7stab", "der", "Pflicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Er lehrt, wie Arbeit, unerm\u00fcdbar z\u00e4h,", "tokens": ["Er", "lehrt", ",", "wie", "Ar\u00b7beit", ",", "un\u00b7er\u00b7m\u00fcd\u00b7bar", "z\u00e4h", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "NN", "$,", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Aus jenem kargen, vielgeschm\u00e4hten Nordland,", "tokens": ["Aus", "je\u00b7nem", "kar\u00b7gen", ",", "viel\u00b7ge\u00b7schm\u00e4h\u00b7ten", "Nord\u00b7land", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Dem Sand der Mark, den S\u00fcmpfen von Masuren", "tokens": ["Dem", "Sand", "der", "Mark", ",", "den", "S\u00fcmp\u00b7fen", "von", "Ma\u00b7su\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Dies Preu\u00dfen schuf, dem als dem Schild des Friedens", "tokens": ["Dies", "Preu\u00b7\u00dfen", "schuf", ",", "dem", "als", "dem", "Schild", "des", "Frie\u00b7dens"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "NN", "VVFIN", "$,", "PRELS", "KOUS", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Jetzt ganz Europa dankt. \u2013 Das tat die Zucht,", "tokens": ["Jetzt", "ganz", "Eu\u00b7ro\u00b7pa", "dankt", ".", "\u2013", "Das", "tat", "die", "Zucht", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NE", "VVFIN", "$.", "$(", "PDS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Das tat die harte Zucht der Hohenzollern,", "tokens": ["Das", "tat", "die", "har\u00b7te", "Zucht", "der", "Ho\u00b7hen\u00b7zol\u00b7lern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Die strenge Zucht der Arbeit und der Pflicht:", "tokens": ["Die", "stren\u00b7ge", "Zucht", "der", "Ar\u00b7beit", "und", "der", "Pflicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Der Arbeit mit dem Pflug und mit dem Schwert,", "tokens": ["Der", "Ar\u00b7beit", "mit", "dem", "Pflug", "und", "mit", "dem", "Schwert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Der Pflicht wie auf der Schulbank, auf dem Schlachtfeld:", "tokens": ["Der", "Pflicht", "wie", "auf", "der", "Schul\u00b7bank", ",", "auf", "dem", "Schlacht\u00b7feld", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Sie hielt in Preu\u00dfen F\u00fcrst und Volk vereint", "tokens": ["Sie", "hielt", "in", "Preu\u00b7\u00dfen", "F\u00fcrst", "und", "Volk", "ver\u00b7eint"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NE", "NN", "KON", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Und Gl\u00fcck wie Ungl\u00fcck teilten sie getreu.", "tokens": ["Und", "Gl\u00fcck", "wie", "Un\u00b7gl\u00fcck", "teil\u00b7ten", "sie", "ge\u00b7treu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KOKOM", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Jedoch dies Buch ist nicht ein totes Buch,", "tokens": ["Je\u00b7doch", "dies", "Buch", "ist", "nicht", "ein", "to\u00b7tes", "Buch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "NN", "VAFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nicht abgeschlossen ist's und abgetan:", "tokens": ["Nicht", "ab\u00b7ge\u00b7schlos\u00b7sen", "ist's", "und", "ab\u00b7ge\u00b7tan", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVPP", "VAFIN", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es lebt! Es wird noch stets daran geschrieben,", "tokens": ["Es", "lebt", "!", "Es", "wird", "noch", "stets", "da\u00b7ran", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VAFIN", "ADV", "ADV", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So lang am Stamm des Kaiserhauses noch", "tokens": ["So", "lang", "am", "Stamm", "des", "Kai\u00b7ser\u00b7hau\u00b7ses", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPRART", "NN", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein Reis nur gr\u00fcnt, nur Eine Wurzel treibt.", "tokens": ["Ein", "Reis", "nur", "gr\u00fcnt", ",", "nur", "Ei\u00b7ne", "Wur\u00b7zel", "treibt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Allein nicht die Gelehrten setzen's fort!", "tokens": ["Al\u00b7lein", "nicht", "die", "Ge\u00b7lehr\u00b7ten", "set\u00b7zen's", "fort", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "NN", "NE", "PTKVZ", "$."], "meter": "-----+-+-+", "measure": "unknown.measure.tri"}, "line.7": {"text": "In jedem Haus f\u00fchr' es der Vater selbst:", "tokens": ["In", "je\u00b7dem", "Haus", "f\u00fchr'", "es", "der", "Va\u00b7ter", "selbst", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Raum findet hier noch manc