{"dta.poem.21521": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Anathem !", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.71", "sv:0.14", "da:0.14"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "In flammender Emp\u00f6rung", "tokens": ["In", "flam\u00b7men\u00b7der", "Em\u00b7p\u00f6\u00b7rung"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sprech\u2019 ich der ", "tokens": ["Sprech'", "ich", "der"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "PPER", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Und wenn du tausend Nacken beugst", "tokens": ["Und", "wenn", "du", "tau\u00b7send", "Na\u00b7cken", "beugst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "CARD", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und tausend Sclavenseelen s\u00e4ugst", "tokens": ["Und", "tau\u00b7send", "Scla\u00b7ven\u00b7see\u00b7len", "s\u00e4ugst"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "CARD", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit feilem Judaslohn:", "tokens": ["Mit", "fei\u00b7lem", "Ju\u00b7das\u00b7lohn", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ich trotze deinen Jochen!               ", "tokens": ["Ich", "trot\u00b7ze", "dei\u00b7nen", "Jo\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich hab' den Bann zerbrochen &#8212;               ", "tokens": ["Ich", "hab'", "den", "Bann", "zer\u00b7bro\u00b7chen", "&#8212;"], "token_info": ["word", "word", "word", "word", "word", "XML_entity"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ich hab' mich freigesprochen:               ", "tokens": ["Ich", "hab'", "mich", "frei\u00b7ge\u00b7spro\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Ich bin der Freiheit Sohn!             ", "tokens": ["Ich", "bin", "der", "Frei\u00b7heit", "Sohn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}