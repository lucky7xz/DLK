{"textgrid.poem.60656": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wir d\u00fcrfen uns zu solchen Dingen,", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir d\u00fcrfen uns zu solchen Dingen,", "tokens": ["Wir", "d\u00fcr\u00b7fen", "uns", "zu", "sol\u00b7chen", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die uns nicht anstehn, niemals zwingen;", "tokens": ["Die", "uns", "nicht", "an\u00b7stehn", ",", "nie\u00b7mals", "zwin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "VVINF", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Denn einem T\u00f6lpel wird es nie gelingen,", "tokens": ["Denn", "ei\u00b7nem", "T\u00f6l\u00b7pel", "wird", "es", "nie", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als zierlicher Galan einherzuspringen.", "tokens": ["Als", "zier\u00b7li\u00b7cher", "Ga\u00b7lan", "ein\u00b7her\u00b7zu\u00b7sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Nur wenig Leute sind es, die der Himmel liebt,", "tokens": ["Nur", "we\u00b7nig", "Leu\u00b7te", "sind", "es", ",", "die", "der", "Him\u00b7mel", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VAFIN", "PPER", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nur wenig, denen schon Natur die Gabe gibt,", "tokens": ["Nur", "we\u00b7nig", ",", "de\u00b7nen", "schon", "Na\u00b7tur", "die", "Ga\u00b7be", "gibt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "$,", "PRELS", "ADV", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die anderen durch Anmut zu erfreuen;", "tokens": ["Die", "an\u00b7de\u00b7ren", "durch", "An\u00b7mut", "zu", "er\u00b7freu\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Man neide ihnen nicht die sch\u00f6ne Gabe", "tokens": ["Man", "nei\u00b7de", "ih\u00b7nen", "nicht", "die", "sch\u00f6\u00b7ne", "Ga\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und suche nicht, es ihnen gleichzutun,", "tokens": ["Und", "su\u00b7che", "nicht", ",", "es", "ih\u00b7nen", "gleich\u00b7zu\u00b7tun", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Sonst wird man's bald bereuen,", "tokens": ["Sonst", "wird", "man's", "bald", "be\u00b7reu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Wof\u00fcr ich hier ein sch\u00f6nes Beispiel habe.", "tokens": ["Wo\u00b7f\u00fcr", "ich", "hier", "ein", "sch\u00f6\u00b7nes", "Bei\u00b7spiel", "ha\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Den plumpen Esel lie\u00df die Lust nicht ruhn,", "tokens": ["Den", "plum\u00b7pen", "E\u00b7sel", "lie\u00df", "die", "Lust", "nicht", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit seinem Herrn recht sch\u00f6n zu tun", "tokens": ["Mit", "sei\u00b7nem", "Herrn", "recht", "sch\u00f6n", "zu", "tun"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sich bei ihm in Gunst zu setzen.", "tokens": ["Und", "sich", "bei", "ihm", "in", "Gunst", "zu", "set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "PPER", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbwie?\u00ab sprach er, \u00bbdieser kleine Hund", "tokens": ["\u00bb", "wie", "?", "\u00ab", "sprach", "er", ",", "\u00bb", "die\u00b7ser", "klei\u00b7ne", "Hund"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["$(", "PWAV", "$.", "$(", "VVFIN", "PPER", "$,", "$(", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Lebt mit dem Herrn im Freundschaftsbund,", "tokens": ["Lebt", "mit", "dem", "Herrn", "im", "Freund\u00b7schafts\u00b7bund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Leckt ihm die Hand, wohl auch den Mund,", "tokens": ["Leckt", "ihm", "die", "Hand", ",", "wohl", "auch", "den", "Mund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "ADV", "ADV", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Und ich soll nur am Stock mich wetzen?", "tokens": ["Und", "ich", "soll", "nur", "am", "Stock", "mich", "wet\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und was denn bringt ihn so in Gunst?", "tokens": ["Und", "was", "denn", "bringt", "ihn", "so", "in", "Gunst", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Er gibt die Pfote \u2013 sch\u00f6ne Kunst! \u2013", "tokens": ["Er", "gibt", "die", "Pfo\u00b7te", "\u2013", "sch\u00f6\u00b7ne", "Kunst", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Dann heimst er Lob und K\u00fcsse ein.", "tokens": ["Dann", "heimst", "er", "Lob", "und", "K\u00fcs\u00b7se", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Wenn weiter nichts dazu geh\u00f6rt,", "tokens": ["Wenn", "wei\u00b7ter", "nichts", "da\u00b7zu", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIS", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "So kann auch ich beliebter sein.\u00ab", "tokens": ["So", "kann", "auch", "ich", "be\u00b7lieb\u00b7ter", "sein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PPER", "ADJD", "VAINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und da ihn der Gedanke ganz bet\u00f6rt", "tokens": ["Und", "da", "ihn", "der", "Ge\u00b7dan\u00b7ke", "ganz", "be\u00b7t\u00f6rt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Und er nicht fern des Herren Stimme h\u00f6rt,", "tokens": ["Und", "er", "nicht", "fern", "des", "Her\u00b7ren", "Stim\u00b7me", "h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKNEG", "ADJD", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "L\u00e4uft er in plumpem Trab zu diesem hin", "tokens": ["L\u00e4uft", "er", "in", "plum\u00b7pem", "Trab", "zu", "die\u00b7sem", "hin"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Und hebt den Huf, der ganz voll Erde ist,", "tokens": ["Und", "hebt", "den", "Huf", ",", "der", "ganz", "voll", "Er\u00b7de", "ist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PRELS", "ADV", "ADJD", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "In k\u00fchnem Schwung dem Herrn ans Kinn,", "tokens": ["In", "k\u00fch\u00b7nem", "Schwung", "dem", "Herrn", "ans", "Kinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Wobei er nicht vergi\u00dft,", "tokens": ["Wo\u00b7bei", "er", "nicht", "ver\u00b7gi\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Um seinem Kosen Wirkung zu verleihn,", "tokens": ["Um", "sei\u00b7nem", "Ko\u00b7sen", "Wir\u00b7kung", "zu", "ver\u00b7leihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Aus vollem Halse sein \u00bbI-ah!\u00ab zu schrein.", "tokens": ["Aus", "vol\u00b7lem", "Hal\u00b7se", "sein", "\u00bb", "I\u00b7\u00b7ah", "!", "\u00ab", "zu", "schrein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "$(", "NE", "$.", "$(", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "\u00bbo welcher Sang und welche Schmeichelei!\u00ab", "tokens": ["\u00bb", "o", "wel\u00b7cher", "Sang", "und", "wel\u00b7che", "Schmei\u00b7che\u00b7lei", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "PWAT", "NN", "KON", "PWAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Ruft da der Herr. \u00bbHe, Martin mit dem Stock!\u00ab", "tokens": ["Ruft", "da", "der", "Herr", ".", "\u00bb", "He", ",", "Mar\u00b7tin", "mit", "dem", "Stock", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$.", "$(", "ITJ", "$,", "NE", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Und Martin mit dem Stock l\u00e4uft schnell herbei", "tokens": ["Und", "Mar\u00b7tin", "mit", "dem", "Stock", "l\u00e4uft", "schnell", "her\u00b7bei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "APPR", "ART", "NN", "VVFIN", "ADJD", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Und klopft dem Esel t\u00fcchtig seinen Rock.", "tokens": ["Und", "klopft", "dem", "E\u00b7sel", "t\u00fcch\u00b7tig", "sei\u00b7nen", "Rock", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Der stimmt jetzt an ein ander Lied.", "tokens": ["Der", "stimmt", "jetzt", "an", "ein", "an\u00b7der", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Ich aber finde, da\u00df ihm recht geschieht.", "tokens": ["Ich", "a\u00b7ber", "fin\u00b7de", ",", "da\u00df", "ihm", "recht", "ge\u00b7schieht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wir d\u00fcrfen uns zu solchen Dinge