{"textgrid.poem.41936": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hier, wo der Wald mich tr\u00e4umerisch umh\u00fcllt,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier, wo der Wald mich tr\u00e4umerisch umh\u00fcllt,", "tokens": ["Hier", ",", "wo", "der", "Wald", "mich", "tr\u00e4u\u00b7me\u00b7risch", "um\u00b7h\u00fcllt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Soll zwiefach Laub die Stirne mir umwinden;", "tokens": ["Soll", "zwie\u00b7fach", "Laub", "die", "Stir\u00b7ne", "mir", "um\u00b7win\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das welke soll mein Leben dir verk\u00fcnden,", "tokens": ["Das", "wel\u00b7ke", "soll", "mein", "Le\u00b7ben", "dir", "ver\u00b7k\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das gr\u00fcnende sey meiner Liebe Bild.", "tokens": ["Das", "gr\u00fc\u00b7nen\u00b7de", "sey", "mei\u00b7ner", "Lie\u00b7be", "Bild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+---+-+-+", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Denn so wie oft auf herbstlichem Gefild", "tokens": ["Denn", "so", "wie", "oft", "auf", "herbst\u00b7li\u00b7chem", "Ge\u00b7fild"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "KOKOM", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "An ", "tokens": ["An"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "So mu\u00df auch ich ein doppelt Seyn empfinden,", "tokens": ["So", "mu\u00df", "auch", "ich", "ein", "dop\u00b7pelt", "Seyn", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PPER", "ART", "ADJD", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da Schmerz und Lust aus einem Quell mir quillt.", "tokens": ["Da", "Schmerz", "und", "Lust", "aus", "ei\u00b7nem", "Quell", "mir", "quillt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Was mir des Todes hei\u00dfen Wunsch gegeben,", "tokens": ["Was", "mir", "des", "To\u00b7des", "hei\u00b7\u00dfen", "Wunsch", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das schm\u00fcckt allein, nur das erh\u00e4lt mein Leben,", "tokens": ["Das", "schm\u00fcckt", "al\u00b7lein", ",", "nur", "das", "er\u00b7h\u00e4lt", "mein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "$,", "ADV", "PDS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und was mich flieht, das wohnt in meiner Brust.", "tokens"