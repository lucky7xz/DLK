{"textgrid.poem.60975": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Aufruf an die Deutschen", "genre": "verse", "period": "N.A.", "pub_year": 1830, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eilt, o S\u00f6hne Teuts, herbei,", "tokens": ["Eilt", ",", "o", "S\u00f6h\u00b7ne", "Teuts", ",", "her\u00b7bei", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "FM", "NN", "NE", "$,", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "St\u00fcrzt die kleine Tyrannei!", "tokens": ["St\u00fcrzt", "die", "klei\u00b7ne", "Ty\u00b7ran\u00b7nei", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Metternich und Nesselrode", "tokens": ["Met\u00b7ter\u00b7nich", "und", "Nes\u00b7sel\u00b7ro\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["NE", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind so ziemlich aus der Mode:", "tokens": ["Sind", "so", "ziem\u00b7lich", "aus", "der", "Mo\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wollen sie zur\u00fcck euch weisen", "tokens": ["Wol\u00b7len", "sie", "zu\u00b7r\u00fcck", "euch", "wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Greift zum Eisen!", "tokens": ["Greift", "zum", "Ei\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Mit den gro\u00dfen Franken eins,", "tokens": ["Mit", "den", "gro\u00b7\u00dfen", "Fran\u00b7ken", "eins", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PIS", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Machet neu den Bund des Rheins!", "tokens": ["Ma\u00b7chet", "neu", "den", "Bund", "des", "Rheins", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "ART", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch den Handschlag wird er wachsen", "tokens": ["Durch", "den", "Hand\u00b7schlag", "wird", "er", "wach\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Urverwandter Angelsachsen:", "tokens": ["Ur\u00b7ver\u00b7wand\u00b7ter", "An\u00b7gel\u00b7sach\u00b7sen", ":"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Alle seid ihr ja Germanen:", "tokens": ["Al\u00b7le", "seid", "ihr", "ja", "Ger\u00b7ma\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADV", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Schwenkt die Fahnen!", "tokens": ["Schwenkt", "die", "Fah\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Wenn der blut'ge Strau\u00df beginnt,", "tokens": ["Wenn", "der", "blut'\u00b7ge", "Strau\u00df", "be\u00b7ginnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NE", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wei\u00df ich, wer den Kranz gewinnt.", "tokens": ["Wei\u00df", "ich", ",", "wer", "den", "Kranz", "ge\u00b7winnt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWS", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo Germane gegen Slave", "tokens": ["Wo", "Ger\u00b7ma\u00b7ne", "ge\u00b7gen", "Sla\u00b7ve"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NE", "APPR", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo den Knecht bek\u00e4mpft der Brave,", "tokens": ["Wo", "den", "Knecht", "be\u00b7k\u00e4mpft", "der", "Bra\u00b7ve", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Sollte Freiheit unterliegen?", "tokens": ["Soll\u00b7te", "Frei\u00b7heit", "un\u00b7ter\u00b7lie\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Deutsche siegen!", "tokens": ["Deut\u00b7sche", "sie\u00b7gen", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.7": {"line.1": {"text": "Will sich aus Borussia nie", "tokens": ["Will", "sich", "aus", "Bo\u00b7rus\u00b7sia", "nie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PRF", "APPR", "NE", "ADV"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Trollen Dame Despotie,", "tokens": ["Trol\u00b7len", "Da\u00b7me", "Des\u00b7po\u00b7tie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Spannen wir, sie wegzutragen", "tokens": ["Span\u00b7nen", "wir", ",", "sie", "weg\u00b7zu\u00b7tra\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Selber uns an ihren Wagen,", "tokens": ["Sel\u00b7ber", "uns", "an", "ih\u00b7ren", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ziehn sie fort an goldner Deichsel", "tokens": ["Ziehn", "sie", "fort", "an", "gold\u00b7ner", "Deich\u00b7sel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "\u00dcber die Weichsel.", "tokens": ["\u00dc\u00b7ber", "die", "Weich\u00b7sel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.9": {"line.1": {"text": "Hohenzollerns edler Stamm", "tokens": ["Ho\u00b7hen\u00b7zol\u00b7lerns", "ed\u00b7ler", "Stamm"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Werde Deutschlands Wehr und Damm!", "tokens": ["Wer\u00b7de", "Deutschlands", "Wehr", "und", "Damm", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "NN", "KON", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Aller Knechtschaft Widersacher", "tokens": ["Al\u00b7ler", "Knecht\u00b7schaft", "Wi\u00b7der\u00b7sa\u00b7cher"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind die gro\u00dfen Witte