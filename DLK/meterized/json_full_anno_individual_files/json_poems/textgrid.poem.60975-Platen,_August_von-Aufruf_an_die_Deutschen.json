 "pos": ["VVFIN", "APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Asiaten!", "tokens": ["A\u00b7sia\u00b7ten", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.13": {"line.1": {"text": "Trotz der heiligen Allianz", "tokens": ["Trotz", "der", "hei\u00b7li\u00b7gen", "Al\u00b7li\u00b7anz"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Wagen wir den Schwertertanz!", "tokens": ["Wa\u00b7gen", "wir", "den", "Schwer\u00b7ter\u00b7tanz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn du dich den Bundsgenossen,", "tokens": ["Wenn", "du", "dich", "den", "Bunds\u00b7ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Cholera Morbus angeschlossen,", "tokens": ["Cho\u00b7le\u00b7ra", "Mor\u00b7bus", "an\u00b7ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "VVPP", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "O so schone freie Nacken,", "tokens": ["O", "so", "scho\u00b7ne", "frei\u00b7e", "Na\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Fri\u00df Kosaken!", "tokens": ["Fri\u00df", "Ko\u00b7sa\u00b7ken", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVIMP", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.15": {"line.1": {"text": "Eilt, o S\u00f6hne Teuts,