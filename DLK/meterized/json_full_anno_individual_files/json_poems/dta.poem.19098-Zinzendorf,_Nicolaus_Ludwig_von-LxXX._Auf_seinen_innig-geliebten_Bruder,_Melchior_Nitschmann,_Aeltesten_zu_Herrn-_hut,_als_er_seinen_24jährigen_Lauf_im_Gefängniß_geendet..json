{"dta.poem.19098": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "LxXX.   Auf seinen innig-geliebten Bruder,  \n Melchior Nitschmann, Aeltesten zu Herrn-  \n hut, als er seinen 24j\u00e4hrigen Lauf im  \n Gef\u00e4ngni\u00df geendet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Wo giengt ihr hin, wo kamt ihr her,\nJhr gr\u00fcnende Gebeine?", "tokens": ["Wo", "giengt", "ihr", "hin", ",", "wo", "kamt", "ihr", "her", ",", "Ihr", "gr\u00fc\u00b7nen\u00b7de", "Ge\u00b7bei\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PTKVZ", "$,", "PWAV", "VVFIN", "PPER", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Dir nach, ", "tokens": ["Dir", "nach", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Der heiligen Gemeine;", "tokens": ["Der", "hei\u00b7li\u00b7gen", "Ge\u00b7mei\u00b7ne", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie kamen aus der Friedens-Stadt,", "tokens": ["Sie", "ka\u00b7men", "aus", "der", "Frie\u00b7dens\u00b7Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von Seelen-Hunger m\u00fcd und matt.", "tokens": ["Von", "See\u00b7len\u00b7Hun\u00b7ger", "m\u00fcd", "und", "matt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Gelobet sey dein muntrer Gang,", "tokens": ["Ge\u00b7lo\u00b7bet", "sey", "dein", "mun\u00b7trer", "Gang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und deiner F\u00fcsse Rauschen:", "tokens": ["Und", "dei\u00b7ner", "F\u00fcs\u00b7se", "Rau\u00b7schen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du wilst die Freyheit gegen Zwang,", "tokens": ["Du", "wilst", "die", "Frey\u00b7heit", "ge\u00b7gen", "Zwang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor Unruh, Ruhe tauschen.", "tokens": ["Vor", "Un\u00b7ruh", ",", "Ru\u00b7he", "tau\u00b7schen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es ist der Welt die h\u00f6chste Noth:", "tokens": ["Es", "ist", "der", "Welt", "die", "h\u00f6chs\u00b7te", "Noth", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Geh hin, du bist ein guter Both.", "tokens": ["Geh", "hin", ",", "du", "bist", "ein", "gu\u00b7ter", "Both", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du weist, wohin der Eifer reicht", "tokens": ["Du", "weist", ",", "wo\u00b7hin", "der", "Ei\u00b7fer", "reicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo man um Land und Wasser zeucht,", "tokens": ["Wo", "man", "um", "Land", "und", "Was\u00b7ser", "zeucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und treibt das Werck nicht h\u00f6her,", "tokens": ["Und", "treibt", "das", "Werck", "nicht", "h\u00f6\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als da\u00df ein Thor den andern macht,", "tokens": ["Als", "da\u00df", "ein", "Thor", "den", "an\u00b7dern", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein ", "tokens": ["Ein"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}}, "stanza.4": {"line.1": {"text": "Die Pharis\u00e4er wurden alt", "tokens": ["Die", "Pha\u00b7ri\u00b7s\u00e4\u00b7er", "wur\u00b7den", "alt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bey ihren Lasten binden:", "tokens": ["Bey", "ih\u00b7ren", "Las\u00b7ten", "bin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Heyland sagte nicht so bald:", "tokens": ["Der", "Hey\u00b7land", "sag\u00b7te", "nicht", "so", "bald", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kommt Menschen! Ruh zu finden,", "tokens": ["Kommt", "Men\u00b7schen", "!", "Ruh", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "$.", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So war die gantze H\u00f6lle auf,", "tokens": ["So", "war", "die", "gant\u00b7ze", "H\u00f6l\u00b7le", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und hemmete des Lehrers Lauf.", "tokens": ["Und", "hem\u00b7me\u00b7te", "des", "Leh\u00b7rers", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Mein Bruder! kennst du deinen Weg,", "tokens": ["Mein", "Bru\u00b7der", "!", "kennst", "du", "dei\u00b7nen", "Weg", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er geht ins Todes Rachen,", "tokens": ["Er", "geht", "ins", "To\u00b7des", "Ra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das ist der allgemeine Steg", "tokens": ["Das", "ist", "der", "all\u00b7ge\u00b7mei\u00b7ne", "Steg"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor die, so Friede machen:", "tokens": ["Vor", "die", ",", "so", "Frie\u00b7de", "ma\u00b7chen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Bleib da; Du kanst nicht; Ey so geh!", "tokens": ["Bleib", "da", ";", "Du", "kanst", "nicht", ";", "Ey", "so", "geh", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "PPER", "VMFIN", "PTKNEG", "$.", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Durchs Todes-Thal zur Lebens H\u00f6h.", "tokens": ["Durchs", "To\u00b7des\u00b7Thal", "zur", "Le\u00b7bens", "H\u00f6h", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Nur fliehe die Gelegenheit,", "tokens": ["Nur", "flie\u00b7he", "die", "Ge\u00b7le\u00b7gen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die deine Ehre sch\u00e4ndet:", "tokens": ["Die", "dei\u00b7ne", "Eh\u00b7re", "sch\u00e4n\u00b7det", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Feind bem\u00fcht sich allezeit,", "tokens": ["Der", "Feind", "be\u00b7m\u00fcht", "sich", "al\u00b7le\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Damit ers also wendet,", "tokens": ["Da\u00b7mit", "ers", "al\u00b7so", "wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df, wers mit Christo treulich meynt,", "tokens": ["Da\u00df", ",", "wers", "mit", "Chris\u00b7to", "treu\u00b7lich", "meynt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWS", "APPR", "NE", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Um Ubelthat zu leiden scheint.", "tokens": ["Um", "U\u00b7belt\u00b7hat", "zu", "lei\u00b7den", "scheint", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wie wir gedacht, so ists geschehn.", "tokens": ["Wie", "wir", "ge\u00b7dacht", ",", "so", "ists", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "$,", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du bist dahin gegangen:", "tokens": ["Du", "bist", "da\u00b7hin", "ge\u00b7gan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Feind hat sich die Zeit ersehn,", "tokens": ["Der", "Feind", "hat", "sich", "die", "Zeit", "er\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und hat dich aufgefangen,", "tokens": ["Und", "hat", "dich", "auf\u00b7ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Noch eh\u2019 du das Gebiet erreicht,", "tokens": ["Noch", "eh'", "du", "das", "Ge\u00b7biet", "er\u00b7reicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wohin dich Trieb und Zug geneigt.", "tokens": ["Wo\u00b7hin", "dich", "Trieb", "und", "Zug", "ge\u00b7neigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Was hat die Schlange f\u00fcr Gewinn,", "tokens": ["Was", "hat", "die", "Schlan\u00b7ge", "f\u00fcr", "Ge\u00b7winn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die alte Th\u00f6rin lachte:", "tokens": ["Die", "al\u00b7te", "Th\u00f6\u00b7rin", "lach\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie wuste nicht, da\u00df ohne hin", "tokens": ["Sie", "wus\u00b7te", "nicht", ",", "da\u00df", "oh\u00b7ne", "hin"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "APPR", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die morsche H\u00fctte krachte,", "tokens": ["Die", "mor\u00b7sche", "H\u00fct\u00b7te", "krach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und da\u00df der Geist ihr Ehren-Bett", "tokens": ["Und", "da\u00df", "der", "Geist", "ihr", "Eh\u00b7ren\u00b7Bett"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit diesem Lauf bestellet h\u00e4tt.", "tokens": ["Mit", "die\u00b7sem", "Lauf", "be\u00b7stel\u00b7let", "h\u00e4tt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Dein ", "tokens": ["Dein"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Da\u00df ", "tokens": ["Da\u00df"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "So l\u00e4\u00dft er seiner Regung Raum,", "tokens": ["So", "l\u00e4\u00dft", "er", "sei\u00b7ner", "Re\u00b7gung", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und \u00fcbersieht die Ritzen,", "tokens": ["Und", "\u00fc\u00b7ber\u00b7sieht", "die", "Rit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Durch die ein Geist zur Ruh gestreckt,", "tokens": ["Durch", "die", "ein", "Geist", "zur", "Ruh", "ge\u00b7streckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Weg des HErrn gar bald entdeckt.", "tokens": ["Den", "Weg", "des", "Herrn", "gar", "bald", "ent\u00b7deckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Dein Mitt-Knecht eilt, um Christi Ruhm", "tokens": ["Dein", "Mit\u00b7tKnecht", "eilt", ",", "um", "Chris\u00b7ti", "Ruhm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "KOUI", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In deinen Leidens-Ketten,", "tokens": ["In", "dei\u00b7nen", "Lei\u00b7dens\u00b7Ket\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vor ", "tokens": ["Vor"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Bald \u00f6ffentlich zu retten,", "tokens": ["Bald", "\u00f6f\u00b7fent\u00b7lich", "zu", "ret\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Bezeuget seinen Bund mit dir,", "tokens": ["Be\u00b7zeu\u00b7get", "sei\u00b7nen", "Bund", "mit", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und bringet deine Unschuld f\u00fcr.", "tokens": ["Und", "brin\u00b7get", "dei\u00b7ne", "Un\u00b7schuld", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Allein in kurtzem sahe man,", "tokens": ["Al\u00b7lein", "in", "kurt\u00b7zem", "sa\u00b7he", "man", ","], 