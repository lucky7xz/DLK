{"dta.poem.4277": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das  \n vom Wiederschein geschm\u00fcckte klare  \n Wasser.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hier, wo helle Sittig- gr\u00fcne mit viel dunkel- gr\u00fcnen", "tokens": ["Hier", ",", "wo", "hel\u00b7le", "Sit\u00b7tig", "gr\u00fc\u00b7ne", "mit", "viel", "dun\u00b7kel", "gr\u00fc\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "ADJA", "TRUNC", "ADJA", "APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Schatten,", "tokens": ["Schat\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Zum Vergn\u00fcgen unsrer Augen, im polierten Glanz sich", "tokens": ["Zum", "Ver\u00b7gn\u00fc\u00b7gen", "uns\u00b7rer", "Au\u00b7gen", ",", "im", "po\u00b7lier\u00b7ten", "Glanz", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$,", "APPRART", "ADJA", "NN", "PRF"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "gatten,", "tokens": ["gat\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Sieht man von den weissen Weyden, von dem Schilf und", "tokens": ["Sieht", "man", "von", "den", "weis\u00b7sen", "Wey\u00b7den", ",", "von", "dem", "Schilf", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "APPR", "ART", "ADJA", "NN", "$,", "APPR", "ART", "NN", "KON"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "niedern B\u00fcschen", "tokens": ["nie\u00b7dern", "B\u00fc\u00b7schen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Eine deutliche Verdopplung in den Wiederschein sich", "tokens": ["Ei\u00b7ne", "deut\u00b7li\u00b7che", "Ver\u00b7dopp\u00b7lung", "in", "den", "Wie\u00b7der\u00b7schein", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "PRF"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.8": {"text": "mischen,", "tokens": ["mi\u00b7schen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Da\u00df ein es bemerkend Aug\u2019, inniglich dadurch ger\u00fchrt,", "tokens": ["Da\u00df", "ein", "es", "be\u00b7mer\u00b7kend", "Aug'", ",", "in\u00b7nig\u00b7lich", "da\u00b7durch", "ge\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PPER", "ADJD", "NN", "$,", "ADJD", "PAV", "VVPP", "$,"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "In dem gr\u00fcnen klaren Glanz, in dem Wasser-Spiegel hie,", "tokens": ["In", "dem", "gr\u00fc\u00b7nen", "kla\u00b7ren", "Glanz", ",", "in", "dem", "Was\u00b7ser\u00b7Spie\u00b7gel", "hie", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,", "APPR", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.11": {"text": "Den so sch\u00f6ne Bilder f\u00fcllen von der schwimmenden Copie,", "tokens": ["Den", "so", "sch\u00f6\u00b7ne", "Bil\u00b7der", "f\u00fcl\u00b7len", "von", "der", "schwim\u00b7men\u00b7den", "Co\u00b7pie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Bald ein ja so gro\u00df Vergn\u00fcgen, als von ihrem Urbild,", "tokens": ["Bald", "ein", "ja", "so", "gro\u00df", "Ver\u00b7gn\u00fc\u00b7gen", ",", "als", "von", "ih\u00b7rem", "Ur\u00b7bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADV", "ADV", "ADJD", "NN", "$,", "KOUS", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.13": {"text": "sp\u00fchrt.", "tokens": ["sp\u00fchrt", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.14": {"text": "Aber, wenn man alle beyde, \u00f6fters wechsels-weis\u2019, erblicket,", "tokens": ["A\u00b7ber", ",", "wenn", "man", "al\u00b7le", "bey\u00b7de", ",", "\u00f6f\u00b7ters", "wech\u00b7sels\u00b7weis'", ",", "er\u00b7bli\u00b7cket", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PIS", "PIAT", "PIS", "$,", "ADV", "ADJD", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.15": {"text": "Wird man durch die dopple Pracht doppelt auch dadurch", "tokens": ["Wird", "man", "durch", "die", "dopp\u00b7le", "Pracht", "dop\u00b7pelt", "auch", "da\u00b7durch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIS", "APPR", "ART", "ADJA", "NN", "ADJD", "ADV", "PAV"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.16": {"text": "erquicket.", "tokens": ["er\u00b7quic\u00b7ket", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Oft, wenn man, in sanfter Stille, sich an beyder Pracht", "tokens": ["Oft", ",", "wenn", "man", ",", "in", "sanf\u00b7ter", "Stil\u00b7le", ",", "sich", "an", "bey\u00b7der", "Pracht"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PIS", "$,", "APPR", "ADJA", "NN", "$,", "PRF", "APPR", "PIAT", "NN"], "meter": "-+--+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "ergetzt,", "tokens": ["er\u00b7getzt", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Wird man, durch ein schnell Ger\u00e4usche, fast in kleine Furcht", "tokens": ["Wird", "man", ",", "durch", "ein", "schnell", "Ge\u00b7r\u00e4u\u00b7sche", ",", "fast", "in", "klei\u00b7ne", "Furcht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PIS", "$,", "APPR", "ART", "ADJD", "NN", "$,", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.4": {"text": "gesetzt,", "tokens": ["ge\u00b7setzt", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Wenn, aus gr\u00fcnen nassen B\u00e4umen, Karpen hie und da sich", "tokens": ["Wenn", ",", "aus", "gr\u00fc\u00b7nen", "nas\u00b7sen", "B\u00e4u\u00b7men", ",", "Kar\u00b7pen", "hie", "und", "da", "sich"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "APPR", "ADJA", "ADJA", "NN", "$,", "NN", "ADV", "KON", "KOUS", "PRF"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.6": {"text": "heben,", "tokens": ["he\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Und, mit klatschendem Gezisch, in begr\u00fcnten L\u00fcften", "tokens": ["Und", ",", "mit", "klat\u00b7schen\u00b7dem", "Ge\u00b7zisch", ",", "in", "be\u00b7gr\u00fcn\u00b7ten", "L\u00fcf\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "schweben,", "tokens": ["schwe\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Bald beschuppte blaue R\u00fccken, bald die gelben B\u00e4uche", "tokens": ["Bald", "be\u00b7schupp\u00b7te", "blau\u00b7e", "R\u00fc\u00b7cken", ",", "bald", "die", "gel\u00b7ben", "B\u00e4u\u00b7che"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "$,", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.10": {"text": "zeigen,", "tokens": ["zei\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "Und, mit schwankender Bewegung, wieder schnell sich ab-", "tokens": ["Und", ",", "mit", "schwan\u00b7ken\u00b7der", "Be\u00b7we\u00b7gung", ",", "wie\u00b7der", "schnell", "sich", "ab"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "APPR", "ADJA", "NN", "$,", "ADV", "ADJD", "PRF", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "werts neigen.", "tokens": ["werts", "nei\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Da sie denn, mit st\u00e4rkerm Klatschen, und vergr\u00f6ssertem", "tokens": ["Da", "sie", "denn", ",", "mit", "st\u00e4r\u00b7kerm", "Klat\u00b7schen", ",", "und", "ver\u00b7gr\u00f6s\u00b7ser\u00b7tem"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "$,", "APPR", "ADJA", "NN", "$,", "KON", "ADJA"], "meter": "+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Bewegen,", "tokens": ["Be\u00b7we\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Tausend Silber-farbne Cirkel auf der gr\u00fcnen Fl\u00e4ch\u2019 erregen.", "tokens": ["Tau\u00b7send", "Sil\u00b7ber\u00b7fa\u00b7rb\u00b7ne", "Cir\u00b7kel", "auf", "der", "gr\u00fc\u00b7nen", "Fl\u00e4ch'", "er\u00b7re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "NN", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.6": {"text": "Hat sich nun die vor\u2019ge Stille m\u00e4hlich wieder eingefunden,", "tokens": ["Hat", "sich", "nun", "die", "vor'\u00b7ge", "Stil\u00b7le", "m\u00e4h\u00b7lich", "wie\u00b7der", "ein\u00b7ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ADV", "ART", "ADJA", "NN", "ADJD", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Siehet man die gr\u00fcne Sch\u00f6nheit wieder, wie zuvor, ver-", "tokens": ["Sie\u00b7het", "man", "die", "gr\u00fc\u00b7ne", "Sch\u00f6n\u00b7heit", "wie\u00b7der", ",", "wie", "zu\u00b7vor", ",", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VVFIN", "PIS", "ART", "ADJA", "NN", "ADV", "$,", "PWAV", "ADV", "$,", "TRUNC"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "bunden.", "tokens": ["bun\u00b7den", "."], "token_info": ["word", "punct"], "pos": ["ADJA", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Tausend zierliche Figuren stellen in dem gr\u00fcnen Schein,", "tokens": ["Tau\u00b7send", "zier\u00b7li\u00b7che", "Fi\u00b7gu\u00b7ren", "stel\u00b7len", "in", "dem", "gr\u00fc\u00b7nen", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Der so mannigfaltig gr\u00fcn, wie auf Glas gemahlt, sich ein.", "tokens": ["Der", "so", "man\u00b7nig\u00b7fal\u00b7tig", "gr\u00fcn", ",", "wie", "auf", "Glas", "ge\u00b7mahlt", ",", "sich", "ein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "ADJD", "$,", "PWAV", "APPR", "NN", "VVPP", "$,", "PRF", "PTKVZ", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Wie ich eben dieses schrieb, und ich auf des Wassers", "tokens": ["Wie", "ich", "e\u00b7ben", "die\u00b7ses", "schrieb", ",", "und", "ich", "auf", "des", "Was\u00b7sers"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "PDS", "VVFIN", "$,", "KON", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.12": {"text": "Spiegel", "tokens": ["Spie\u00b7gel"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Einen nah dabey gelegnen Gras- und Kr\u00e4uter-reichen", "tokens": ["Ei\u00b7nen", "nah", "da\u00b7bey", "ge\u00b7leg\u00b7nen", "Gras", "und", "Kr\u00e4u\u00b7ter\u00b7rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "PAV", "ADJA", "TRUNC", "KON", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.14": {"text": "H\u00fcgel,", "tokens": ["H\u00fc\u00b7gel", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "Auch ein auf ihm weidend Schaf sich im Wasser bilden", "tokens": ["Auch", "ein", "auf", "ihm", "wei\u00b7dend", "Schaf", "sich", "im", "Was\u00b7ser", "bil\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "APPR", "PPER", "ADJD", "NN", "PRF", "APPRART", "NN", "VVINF"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "sah;", "tokens": ["sah", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.17": {"text": "Machte mir, nebst andern Bildern, welche auf dem Was-", "tokens": ["Mach\u00b7te", "mir", ",", "nebst", "an\u00b7dern", "Bil\u00b7dern", ",", "wel\u00b7che", "auf", "dem", "Was"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.18": {"text": "ser liegen,", "tokens": ["ser", "lie\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.19": {"text": "Durch der Farb- und Formen Schmuck, dieses Bild auch", "tokens": ["Durch", "der", "Fa\u00b7rb", "und", "For\u00b7men", "Schmuck", ",", "die\u00b7ses", "Bild", "auch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "TRUNC", "KON", "NN", "NN", "$,", "PDAT", "NN", "ADV"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.20": {"text": "ein Vergn\u00fcgen.", "tokens": ["ein", "Ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.21": {"text": "Aber h\u00f6ret, was geschah!", "tokens": ["A\u00b7ber", "h\u00f6\u00b7ret", ",", "was", "ge\u00b7schah", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Eben auf derselben Stelle, wo das Bild vom Schafe", "tokens": ["E\u00b7ben", "auf", "der\u00b7sel\u00b7ben", "Stel\u00b7le", ",", "wo", "das", "Bild", "vom", "Scha\u00b7fe"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PDAT", "NN", "$,", "PWAV", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.23": {"text": "schwamm,", "tokens": ["schwamm", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.24": {"text": "Nah an einer Weiden Stamm,", "tokens": ["Nah", "an", "ei\u00b7ner", "Wei\u00b7den", "Stamm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Warf von ungefehr ein Fisch aus der Fluht sich in die", "tokens": ["Warf", "von", "un\u00b7ge\u00b7fehr", "ein", "Fisch", "aus", "der", "Fluht", "sich", "in", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ADJD", "ART", "NN", "APPR", "ART", "NN", "PRF", "APPR", "ART"], "meter": "+-+-+-+--+-++", "measure": "trochaic.septa.relaxed"}, "line.26": {"text": "H\u00f6he,", "tokens": ["H\u00f6\u00b7he", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.27": {"text": "Welches mich zum Lachen brachte,", "tokens": ["Wel\u00b7ches", "mich", "zum", "La\u00b7chen", "brach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Weil ich bey dem Zufall dachte:", "tokens": ["Weil", "ich", "bey", "dem", "Zu\u00b7fall", "dach\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Dieses dient\u2019, ein gutes R\u00e4htsel etwan folgends abzuge-", "tokens": ["Die\u00b7ses", "dient'", ",", "ein", "gu\u00b7tes", "R\u00e4ht\u00b7sel", "et\u00b7wan", "fol\u00b7gends", "ab\u00b7zu\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "ART", "ADJA", "NN", "ADV", "PIS", "TRUNC"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.30": {"text": "ben:", "tokens": ["ben", ":"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-", "measure": "single.down"}, "line.31": {"text": "(mancher d\u00fcrft\u2019, es aufzul\u00f6sen, sich vielleicht umsonst", "tokens": ["(", "man\u00b7cher", "d\u00fcrft'", ",", "es", "auf\u00b7zu\u00b7l\u00f6\u00b7sen", ",", "sich", "viel\u00b7leicht", "um\u00b7sonst"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PIS", "VMFIN", "$,", "PPER", "VVPP", "$,", "PRF", "ADV", "ADV"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.32": {"text": "bestreben)", "tokens": ["be\u00b7stre\u00b7ben", ")"], "token_info": ["word", "punct"], "pos": ["VVPP", "$("], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "\u201eoerter w\u00e4ren in der Welt, wo es dann und wann gesch\u00e4he,", "tokens": ["\u201e", "o\u00b7er\u00b7ter", "w\u00e4\u00b7ren", "in", "der", "Welt", ",", "wo", "es", "dann", "und", "wann", "ge\u0