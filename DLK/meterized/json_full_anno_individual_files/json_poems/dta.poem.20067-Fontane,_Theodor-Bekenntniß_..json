{"dta.poem.20067": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Bekenntni\u00df .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin ein ungl\u00fcckselig Rohr:", "tokens": ["Ich", "bin", "ein", "un\u00b7gl\u00fcck\u00b7se\u00b7lig", "Rohr", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Gef\u00fchle und Gedanken", "tokens": ["Ge\u00b7f\u00fch\u00b7le", "und", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Seh rechts und links, zur\u00fcck und vor,", "tokens": ["Seh", "rechts", "und", "links", ",", "zu\u00b7r\u00fcck", "und", "vor", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KON", "ADV", "$,", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "In jedem Wind, ich schwanken.", "tokens": ["In", "je\u00b7dem", "Wind", ",", "ich", "schwan\u00b7ken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Bald ist\u2019s im Herzen kirchenstill,", "tokens": ["Bald", "ist's", "im", "Her\u00b7zen", "kir\u00b7chen\u00b7still", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bald sch\u00e4umt\u2019s wie Saft der Reben,", "tokens": ["Bald", "sch\u00e4umt's", "wie", "Saft", "der", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOKOM", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich wei\u00df nicht, was ich soll und will; \u2014", "tokens": ["Ich", "wei\u00df", "nicht", ",", "was", "ich", "soll", "und", "will", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VMFIN", "KON", "VMFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Es ist ein kl\u00e4glich Leben!", "tokens": ["Es", "ist", "ein", "kl\u00e4g\u00b7lich", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da liegt nichts zwischen Sein und Tod,", "tokens": ["Da", "liegt", "nichts", "zwi\u00b7schen", "Sein", "und", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "PPOSAT", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was ich nicht schon erflehte:", "tokens": ["Was", "ich", "nicht", "schon", "er\u00b7fleh\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Heut bitt\u2019 ich um des Glaubens Brod,", "tokens": ["Heut", "bitt'", "ich", "um", "des", "Glau\u00b7bens", "Brod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df morgen ich\u2019s zertrete;", "tokens": ["Da\u00df", "mor\u00b7gen", "ich's", "zer\u00b7tre\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Was heut ich segne, segn\u2019 ich nicht,", "tokens": ["Was", "heut", "ich", "seg\u00b7ne", ",", "segn'", "ich", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich werde sein Verflucher,", "tokens": ["Ich", "wer\u00b7de", "sein", "Ver\u00b7flu\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Nach Wahrheit ring\u2019 ich und nach Licht,", "tokens": ["Nach", "Wahr\u00b7heit", "ring'", "ich", "und", "nach", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und schelt es dann \u2014 Versucher.", "tokens": ["Und", "schelt", "es", "dann", "Ver\u00b7su\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$(", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Dich ruf ich, der das Kleinste Du", "tokens": ["Dich", "ruf", "ich", ",", "der", "das", "Kleins\u00b7te", "Du"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PRELS", "ART", "ADJA", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Deinen Schutz genommen,", "tokens": ["In", "Dei\u00b7nen", "Schutz", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "G\u00f6nn meinem Herzen Halt und Ruh,", "tokens": ["G\u00f6nn", "mei\u00b7nem", "Her\u00b7zen", "Halt", "und", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gott, la\u00df mich nicht verkommen;", "tokens": ["Gott", ",", "la\u00df", "mich", "nicht", "ver\u00b7kom\u00b7men", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Leih mir die Kraft, die mir gebricht,", "tokens": ["Leih", "mir", "die", "Kraft", ",", "die", "mir", "ge\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Nimm weg, was mich verwirret,", "tokens": ["Nimm", "weg", ",", "was", "mich", "ver\u00b7wir\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sonst l\u00f6sch es aus dies Flackerlicht,", "tokens": ["Sonst", "l\u00f6sch", "es", "aus", "dies", "Fla\u00b7cker\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "APPR", "PDS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das \u00fcber S\u00fcmpfe irret!", "tokens": ["Das", "\u00fc\u00b7ber", "S\u00fcmp\u00b7fe", "ir\u00b7ret", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}