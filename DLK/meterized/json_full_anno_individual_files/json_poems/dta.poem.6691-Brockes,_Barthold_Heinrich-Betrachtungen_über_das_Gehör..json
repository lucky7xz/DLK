{"dta.poem.6691": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtungen \u00fcber das Geh\u00f6r.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was dis noch mehr bew\u00e4hret,", "tokens": ["Was", "dis", "noch", "mehr", "be\u00b7w\u00e4h\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDS", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df uns\u2019re Seel allein", "tokens": ["Da\u00df", "un\u00b7s'\u00b7re", "Seel", "al\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADV"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "In sich die W\u00fcrckung blo\u00df empfindet und erf\u00e4hret,", "tokens": ["In", "sich", "die", "W\u00fcr\u00b7ckung", "blo\u00df", "emp\u00b7fin\u00b7det", "und", "er\u00b7f\u00e4h\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "NN", "ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die nach dem Augen-Schein", "tokens": ["Die", "nach", "dem", "Au\u00b7gen\u00b7Schein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sich in die Sinne dr\u00fcckt: Ist, da\u00df von allen T\u00f6nen", "tokens": ["Sich", "in", "die", "Sin\u00b7ne", "dr\u00fcckt", ":", "Ist", ",", "da\u00df", "von", "al\u00b7len", "T\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "ART", "NN", "VVFIN", "$.", "VAFIN", "$,", "KOUS", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Stimme der Person, die jemand liebt,", "tokens": ["Die", "Stim\u00b7me", "der", "Per\u00b7son", ",", "die", "je\u00b7mand", "liebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Jhm das empfindlichste Vergn\u00fcgen giebt,", "tokens": ["Jhm", "das", "emp\u00b7find\u00b7lichs\u00b7te", "Ver\u00b7gn\u00fc\u00b7gen", "giebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.8": {"text": "Da einer, der vom Reitz derselben Sch\u00f6nen", "tokens": ["Da", "ei\u00b7ner", ",", "der", "vom", "Reitz", "der\u00b7sel\u00b7ben", "Sch\u00f6\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "$,", "PRELS", "APPRART", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Nicht angeflammet, nicht ger\u00fchrt,", "tokens": ["Nicht", "an\u00b7ge\u00b7flam\u00b7met", ",", "nicht", "ge\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "VVPP", "$,", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Dergleichen Anmuth nicht versp\u00fcrt.", "tokens": ["Derg\u00b7lei\u00b7chen", "An\u00b7muth", "nicht", "ver\u00b7sp\u00fcrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Man wird vom Schall der W\u00f6rter blo\u00df entz\u00fccket.", "tokens": ["Man", "wird", "vom", "Schall", "der", "W\u00f6r\u00b7ter", "blo\u00df", "ent\u00b7z\u00fc\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPRART", "NN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Der Stimme, die man liebt, mu\u00df alles weichen,", "tokens": ["Der", "Stim\u00b7me", ",", "die", "man", "liebt", ",", "mu\u00df", "al\u00b7les", "wei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Die herlichste Musie ist ihr nicht zu vergleichen.", "tokens": ["Die", "her\u00b7lichs\u00b7te", "Mu\u00b7sie", "ist", "ihr", "nicht", "zu", "ver\u00b7glei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}}}}}