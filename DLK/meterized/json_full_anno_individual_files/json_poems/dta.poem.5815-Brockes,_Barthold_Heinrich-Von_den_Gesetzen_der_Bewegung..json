{"dta.poem.5815": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Gesetzen der Bewegung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dies kan uns die Erfahrung lehren:", "tokens": ["Dies", "kan", "uns", "die", "Er\u00b7fah\u00b7rung", "leh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was uns umgiebt, ohn\u2019 da\u00df mans f\u00fchlt, die Lufft,", "tokens": ["Was", "uns", "um\u00b7giebt", ",", "ohn'", "da\u00df", "mans", "f\u00fchlt", ",", "die", "Lufft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "KOUI", "KOUS", "PIS", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein feuchter Dufft,", "tokens": ["Ein", "feuch\u00b7ter", "Dufft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Die Fluht, die dichter noch, als jene zwey, verwehren", "tokens": ["Die", "Fluht", ",", "die", "dich\u00b7ter", "noch", ",", "als", "je\u00b7ne", "zwey", ",", "ver\u00b7weh\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "$,", "KOUS", "PDAT", "CARD", "$,", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der fortgestossnen C\u00f6rper regen,", "tokens": ["Der", "fort\u00b7ges\u00b7toss\u00b7nen", "C\u00f6r\u00b7per", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und machen, da\u00df nach kurtzer Zeit,", "tokens": ["Und", "ma\u00b7chen", ",", "da\u00df", "nach", "kurt\u00b7zer", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "KOUS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da sie sich ferner nicht bewegen,", "tokens": ["Da", "sie", "sich", "fer\u00b7ner", "nicht", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Man sie in Ruh erblickt", "tokens": ["Man", "sie", "in", "Ruh", "er\u00b7blickt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "PPER", "APPR", "NN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Und zwar, nachdem mit mehr und mindrer Hefftigkeit,", "tokens": ["Und", "zwar", ",", "nach\u00b7dem", "mit", "mehr", "und", "mind\u00b7rer", "Heff\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "APPR", "PIAT", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Das Mittelste sie hemmet und sie dr\u00fcckt.", "tokens": ["Das", "Mit\u00b7tels\u00b7te", "sie", "hem\u00b7met", "und", "sie", "dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "KON", "PPER", "VVFIN", "$."], "meter": "-++--+-+-+", "measure": "iambic.penta.relaxed"}}}}}