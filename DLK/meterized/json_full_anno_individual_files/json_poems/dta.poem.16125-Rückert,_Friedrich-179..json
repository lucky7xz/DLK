{"dta.poem.16125": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "179.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wol ist das Gegentheil von der Gelegenheit", "tokens": ["Wol", "ist", "das", "Ge\u00b7gen\u00b7theil", "von", "der", "Ge\u00b7le\u00b7gen\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Das Alter, denn es kommt zur ungelegnen Zeit.", "tokens": ["Das", "Al\u00b7ter", ",", "denn", "es", "kommt", "zur", "un\u00b7ge\u00b7leg\u00b7nen", "Zeit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Gelegenheit ist kahl von hinten, vorn behaart,", "tokens": ["Ge\u00b7le\u00b7gen\u00b7heit", "ist", "kahl", "von", "hin\u00b7ten", ",", "vorn", "be\u00b7haart", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "APPR", "ADV", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Davon das Gegentheil ist meist des Alters Art.", "tokens": ["Da\u00b7von", "das", "Ge\u00b7gen\u00b7theil", "ist", "meist", "des", "Al\u00b7ters", "Art", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VAFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Gelegenheit ist uns entflohn mit schnellem Schritt,", "tokens": ["Ge\u00b7le\u00b7gen\u00b7heit", "ist", "uns", "ent\u00b7flohn", "mit", "schnel\u00b7lem", "Schritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Alter aber geht gemach und nimmt uns mit.", "tokens": ["Das", "Al\u00b7ter", "a\u00b7ber", "geht", "ge\u00b7mach", "und", "nimmt", "uns", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ADV", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}