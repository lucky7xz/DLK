{"dta.poem.908": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "De PROFUNDIS", "genre": "Lyrik", "period": "N.A.", "pub_year": "1913", "urn": "urn:nbn:de:kobv:b4-30357-9", "language": ["de:0.99"], "booktitle": "Trakl, Georg: Gedichte. Leipzig, 1913."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein Stoppelfeld, in das ein schwarzer Regen f\u00e4llt.", "tokens": ["Es", "ist", "ein", "Stop\u00b7pel\u00b7feld", ",", "in", "das", "ein", "schwar\u00b7zer", "Re\u00b7gen", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "APPR", "PRELS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Es ist ein brauner Baum, der einsam dasteht.", "tokens": ["Es", "ist", "ein", "brau\u00b7ner", "Baum", ",", "der", "ein\u00b7sam", "das\u00b7teht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Es ist ein Zischelwind, der leere H\u00fctten umkreist", "tokens": ["Es", "ist", "ein", "Zi\u00b7schel\u00b7wind", ",", "der", "lee\u00b7re", "H\u00fct\u00b7ten", "um\u00b7kreist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.4": {"text": "Wie traurig dieser Abend.", "tokens": ["Wie", "trau\u00b7rig", "die\u00b7ser", "A\u00b7bend", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PDAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Am Weiler vorbei", "tokens": ["Am", "Wei\u00b7ler", "vor\u00b7bei"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "PTKVZ"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Sammelt die sanfte Waise noch sp\u00e4rliche \u00c4hren ein.", "tokens": ["Sam\u00b7melt", "die", "sanf\u00b7te", "Wai\u00b7se", "noch", "sp\u00e4r\u00b7li\u00b7che", "\u00c4h\u00b7ren", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADV", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+-+--+--+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Ihre Augen weiden rund und goldig in der D\u00e4mmerung", "tokens": ["Ih\u00b7re", "Au\u00b7gen", "wei\u00b7den", "rund", "und", "gol\u00b7dig", "in", "der", "D\u00e4m\u00b7me\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "KON", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-+--", "measure": "unknown.measure.septa"}, "line.4": {"text": "Und ihr Scho\u00df harrt des himmlischen Br\u00e4utigams.", "tokens": ["Und", "ihr", "Scho\u00df", "harrt", "des", "himm\u00b7li\u00b7schen", "Br\u00e4u\u00b7ti\u00b7gams", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NE", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}}, "stanza.3": {"line.1": {"text": "Bei der Heimkehr", "tokens": ["Bei", "der", "Heim\u00b7kehr"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Fanden die Hirten den s\u00fc\u00dfen Leib", "tokens": ["Fan\u00b7den", "die", "Hir\u00b7ten", "den", "s\u00fc\u00b7\u00dfen", "Leib"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Verwest im Dornenbusch.", "tokens": ["Ver\u00b7west", "im", "Dor\u00b7nen\u00b7busch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ein Schatten bin ich ferne finsteren D\u00f6rfern.", "tokens": ["Ein", "Schat\u00b7ten", "bin", "ich", "fer\u00b7ne", "fins\u00b7te\u00b7ren", "D\u00f6r\u00b7fern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVFIN", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Gottes Schweigen", "tokens": ["Got\u00b7tes", "Schwei\u00b7gen"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Trank ich aus dem Brunnen des Hains.", "tokens": ["Trank", "ich", "aus", "dem", "Brun\u00b7nen", "des", "Hains", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+---+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Auf meine Stirne tritt kaltes Metall", "tokens": ["Auf", "mei\u00b7ne", "Stir\u00b7ne", "tritt", "kal\u00b7tes", "Me\u00b7tall"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Spinnen suchen mein Herz.", "tokens": ["Spin\u00b7nen", "su\u00b7chen", "mein", "Herz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Es ist ein Licht, das in meinem Mund erl\u00f6scht.", "tokens": ["Es", "ist", "ein", "Licht", ",", "das", "in", "mei\u00b7nem", "Mund", "er\u00b7l\u00f6scht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Nachts fand ich mich auf einer Heide,", "tokens": ["Nachts", "fand", "ich", "mich", "auf", "ei\u00b7ner", "Hei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Starrend von Unrat und Staub der Sterne.", "tokens": ["Star\u00b7rend", "von", "Un\u00b7rat", "und", "Staub", "der", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Im Haselgeb\u00fcsch", "tokens": ["Im", "Ha\u00b7sel\u00b7ge\u00b7b\u00fcsch"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Klangen wieder kristallne Engel.", "tokens": ["Klan\u00b7gen", "wie\u00b7der", "kris\u00b7tall\u00b7ne", "En\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJA", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}}}}