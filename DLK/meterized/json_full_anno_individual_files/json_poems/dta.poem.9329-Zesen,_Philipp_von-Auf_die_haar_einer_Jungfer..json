{"dta.poem.9329": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Auf die haar einer Jungfer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.71", "af:0.14", "cy:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Seyn das die g\u00fcldnen haar? ach gold/ sie k\u00f6nnen", "tokens": ["Seyn", "das", "die", "g\u00fcld\u00b7nen", "haar", "?", "ach", "gold", "/", "sie", "k\u00f6n\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ART", "ART", "ADJA", "NN", "$.", "XY", "NN", "$(", "PPER", "VMFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "zwingen", "tokens": ["zwin\u00b7gen"], "token_info": ["word"], "pos": ["VVINF"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "und binden meinen muth mit ihrem glantz an sich;", "tokens": ["und", "bin\u00b7den", "mei\u00b7nen", "muth", "mit", "ih\u00b7rem", "glantz", "an", "sich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "APPR", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nicht bande; straalen seyns; damit sie bl\u00e4ndet mich", "tokens": ["Nicht", "ban\u00b7de", ";", "straa\u00b7len", "seyns", ";", "da\u00b7mit", "sie", "bl\u00e4n\u00b7det", "mich"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "NN", "$.", "VVFIN", "NE", "$.", "KOUS", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "die Sonne meiner zeit; Nicht straalen; blitze dringen", "tokens": ["die", "Son\u00b7ne", "mei\u00b7ner", "zeit", ";", "Nicht", "straa\u00b7len", ";", "blit\u00b7ze", "drin\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "$.", "PTKNEG", "VVINF", "$.", "VVFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "mit eingemischt herzu/ und in den L\u00fcfften ringen;", "tokens": ["mit", "ein\u00b7ge\u00b7mischt", "her\u00b7zu", "/", "und", "in", "den", "L\u00fcff\u00b7ten", "rin\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ADV", "$(", "KON", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Nicht blitze; Seenen seyns/ davon so seuberlich", "tokens": ["Nicht", "blit\u00b7ze", ";", "See\u00b7nen", "seyns", "/", "da\u00b7von", "so", "seu\u00b7ber\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "$.", "NN", "VAFIN", "$(", "PAV", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "die g\u00fcldnen pfeile schiest der kleine W\u00fctherich.", "tokens": ["die", "g\u00fcld\u00b7nen", "pfei\u00b7le", "schiest", "der", "klei\u00b7ne", "W\u00fct\u00b7he\u00b7rich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nicht Seenen; was denn sonst so unter vielen dingen?", "tokens": ["Nicht", "See\u00b7nen", ";", "was", "denn", "sonst", "so", "un\u00b7ter", "vie\u00b7len", "din\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$.", "PWS", "ADV", "ADV", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dan\u0303 g\u00fclden seyn sie nicht/ weil gold nit halb so theuer/", "tokens": ["Da\u00f1", "g\u00fcl\u00b7den", "seyn", "sie", "nicht", "/", "weil", "gold", "nit", "halb", "so", "theu\u00b7er", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAINF", "PPER", "PTKNEG", "$(", "KOUS", "NN", "PTKNEG", "ADJD", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "auch bande seyn sie nicht/ weil bande fester seyn;", "tokens": ["auch", "ban\u00b7de", "seyn", "sie", "nicht", "/", "weil", "ban\u00b7de", "fes\u00b7ter", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAINF", "PPER", "PTKNEG", "$(", "KOUS", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Auch Sonnenstrahlen nicht/ weil nur ein Sonnen-", "tokens": ["Auch", "Son\u00b7nen\u00b7strah\u00b7len", "nicht", "/", "weil", "nur", "ein", "Son\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "NN", "PTKNEG", "$(", "KOUS", "ADV", "ART", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "schein;", "tokens": ["schein", ";"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+", "measure": "single.up"}, "line.14": {"text": "Nicht blitze; weil der blitz ein augenblicklich feuer/", "tokens": ["Nicht", "blit\u00b7ze", ";", "weil", "der", "blitz", "ein", "au\u00b7gen\u00b7blick\u00b7lich", "feu\u00b7er", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$.", "KOUS", "ART", "NN", "ART", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "auch seyn sie Seenen nicht. Noch werden sie mit", "tokens": ["auch", "seyn", "sie", "See\u00b7nen", "nicht", ".", "Noch", "wer\u00b7den", "sie", "mit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAINF", "PPER", "NN", "PTKNEG", "$.", "ADV", "VAFIN", "PPER", "APPR"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.16": {"text": "macht", "tokens": ["macht"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.17": {"text": "gold/ straalen/ banden/ blitz und seenen gleich geacht.", "tokens": ["gold", "/", "straa\u00b7len", "/", "ban\u00b7den", "/", "blitz", "und", "see\u00b7nen", "gleich", "ge\u00b7acht", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$(", "VVFIN", "$(", "VVINF", "$(", "VVIMP", "KON", "VVFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}