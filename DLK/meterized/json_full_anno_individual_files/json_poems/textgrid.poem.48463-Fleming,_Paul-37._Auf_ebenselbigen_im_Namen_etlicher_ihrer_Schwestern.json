{"textgrid.poem.48463": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "37. Auf ebenselbigen im Namen etlicher ihrer Schwestern", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein s\u00fc\u00dfer Namenstag, geliebte Schwester, will,", "tokens": ["Dein", "s\u00fc\u00b7\u00dfer", "Na\u00b7mens\u00b7tag", ",", "ge\u00b7lieb\u00b7te", "Schwes\u00b7ter", ",", "will", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ADJA", "NN", "$,", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "da\u00df du auf Lustigkeit und manches sch\u00f6nes Spiel", "tokens": ["da\u00df", "du", "auf", "Lus\u00b7tig\u00b7keit", "und", "man\u00b7ches", "sch\u00f6\u00b7nes", "Spiel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "KON", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "auf heute seist bedacht. La\u00df alles Trauren fahren!", "tokens": ["auf", "heu\u00b7te", "seist", "be\u00b7dacht", ".", "La\u00df", "al\u00b7les", "Trau\u00b7ren", "fah\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VAFIN", "VVPP", "$.", "VVIMP", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zu Lande soll man sonst ganz keine Freude sparen.", "tokens": ["Zu", "Lan\u00b7de", "soll", "man", "sonst", "ganz", "kei\u00b7ne", "Freu\u00b7de", "spa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PIS", "ADV", "ADV", "PIAT", "NN", "VVINF",