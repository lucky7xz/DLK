{"dta.poem.22460": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "6. Die GOtts gew\u00fcrdigte Einfalt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.71", "nl:0.28"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Denkt doch/ was Demut ist! seht doch was Einfalt", "tokens": ["Denkt", "doch", "/", "was", "De\u00b7mut", "ist", "!", "seht", "doch", "was", "Ein\u00b7falt"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$(", "PWS", "NN", "VAFIN", "$.", "VVFIN", "ADV", "PWS", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Hirten schauen GOtt am allerersten an. (kan!)", "tokens": ["Die", "Hir\u00b7ten", "schau\u00b7en", "Gott", "am", "al\u00b7le\u00b7rers\u00b7ten", "an", ".", "(", "kan", "!", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "APPRART", "ADJA", "PTKVZ", "$.", "$(", "VMFIN", "$.", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "Der siht Gott nim\u0303er mehr/ noch dort noch hier auf Erde\u0303/", "tokens": ["Der", "siht", "Gott", "nim\u0303er", "mehr", "/", "noch", "dort", "noch", "hier", "auf", "Erd\u1ebd", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "ADV", "ADV", "$(", "ADV", "ADV", "ADV", "ADV", "APPR", "NN", "$("], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Der nicht gantz jnniglich begehrt ein Hirt zu werden.", "tokens": ["Der", "nicht", "gantz", "jn\u00b7nig\u00b7lich", "be\u00b7gehrt", "ein", "Hirt", "zu", "wer\u00b7den", "."], "tok