{"textgrid.poem.64930": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "19.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O sch\u00f6ner, bleicher Tod, du winkst mir mild und licht!", "tokens": ["O", "sch\u00f6\u00b7ner", ",", "blei\u00b7cher", "Tod", ",", "du", "winkst", "mir", "mild", "und", "licht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "$,", "ADJA", "NN", "$,", "PPER", "VVFIN", "PPER", "ADJD", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch, wie mich's sehnt nach dir, ich darf dir folgen nicht.", "tokens": ["Doch", ",", "wie", "mich's", "sehnt", "nach", "dir", ",", "ich", "darf", "dir", "fol\u00b7gen", "nicht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PIS", "VVFIN", "APPR", "PPER", "$,", "PPER", "VMFIN", "PPER", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Denn noch hab' ich allhier mein Tagwerk nicht vollendet,", "tokens": ["Denn", "noch", "hab'", "ich", "all\u00b7hier", "mein", "Tag\u00b7werk", "nicht", "voll\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "PPOSAT", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Auftrag nicht erf\u00fcllt, mit dem ich ausgesendet.", "tokens": ["Den", "Auf\u00b7trag", "nicht", "er\u00b7f\u00fcllt", ",", "mit", "dem", "ich", "aus\u00b7ge\u00b7sen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVPP", "$,", "APPR", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Verlassen nicht darf ich den Keim, den ich ges\u00e4't,", "tokens": ["Ver\u00b7las\u00b7sen", "nicht", "darf", "ich", "den", "Keim", ",", "den", "ich", "ge\u00b7s\u00e4't", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "VMFIN", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als bis er frei und stark in kr\u00e4ft'ger Bl\u00fcthe steht.", "tokens": ["Als", "bis", "er", "frei", "und", "stark", "in", "kr\u00e4ft'\u00b7ger", "Bl\u00fc\u00b7the", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "ADJD", "KON", "ADJD", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch ist die\u00df einst gescheh'n, und schwillt er reich an Segen:", "tokens": ["Doch", "ist", "die\u00df", "einst", "ge\u00b7scheh'n", ",", "und", "schwillt", "er", "reich", "an", "Se\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PDS", "ADV", "VVPP", "$,", "KON", "VVFIN", "PPER", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dann will mein wundes Haupt zur Ruh ich niederlegen.", "tokens": ["Dann", "will", "mein", "wun\u00b7des", "Haupt", "zur", "Ruh", "ich", "nie\u00b7der\u00b7le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "So wie der Schnitter nach dem Tage hei\u00df und schw\u00fcl,", "tokens": ["So", "wie", "der", "Schnit\u00b7ter", "nach", "dem", "Ta\u00b7ge", "hei\u00df", "und", "schw\u00fcl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "APPR", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ersch\u00f6pft, entschlummernd sinkt auf seiner Garben Pf\u00fchl.", "tokens": ["Er\u00b7sch\u00f6pft", ",", "ent\u00b7schlum\u00b7mernd", "sinkt", "auf", "sei\u00b7ner", "Gar\u00b7ben", "Pf\u00fchl", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Doch wie er schlafend liegt in Abendsonnenstrahlen,", "tokens": ["Doch", "wie", "er", "schla\u00b7fend", "liegt", "in", "A\u00b7bend\u00b7son\u00b7nen\u00b7strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADJD", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weckt ihn der Ernte Herr, ihm seine M\u00fch zu zahlen.", "tokens": ["Weckt", "ihn", "der", "Ern\u00b7te", "Herr", ",", "ihm", "sei\u00b7ne", "M\u00fch", "zu", "zah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "PPER", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"