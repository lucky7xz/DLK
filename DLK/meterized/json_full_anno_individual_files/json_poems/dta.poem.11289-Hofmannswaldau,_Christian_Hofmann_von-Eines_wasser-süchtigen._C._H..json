{"dta.poem.11289": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Eines wasser-s\u00fcchtigen.  \n C. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Eh ich die welt erblickt, so lag ich im gew\u00e4sser;", "tokens": ["Eh", "ich", "die", "welt", "er\u00b7blickt", ",", "so", "lag", "ich", "im", "ge\u00b7w\u00e4s\u00b7ser", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,", "ADV", "VVFIN", "PPER", "APPRART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als ich die welt erblickt, so hatt\u2019 ichs nicht viel besser.", "tokens": ["Als", "ich", "die", "welt", "er\u00b7blickt", ",", "so", "hatt'", "ichs", "nicht", "viel", "bes\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,", "ADV", "VAFIN", "PIS", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jtzt mu\u00df ich nun, (das kan auf meinem grabe stehn:)", "tokens": ["Jtzt", "mu\u00df", "ich", "nun", ",", "(", "das", "kan", "auf", "mei\u00b7nem", "gra\u00b7be", "stehn", ":)"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "emoticon"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "$,", "$(", "PDS", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auch, wie die erste welt, durchs wasser untergehn.", "tokens": ["Auch", ",", "wie", "die", "ers\u00b7te", "welt", ",", "durchs", "was\u00b7ser", "un\u00b7ter\u00b7gehn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "ADJA", "NN", "$,", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}