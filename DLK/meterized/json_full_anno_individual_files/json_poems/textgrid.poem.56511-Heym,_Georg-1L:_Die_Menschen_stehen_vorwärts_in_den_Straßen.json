{"textgrid.poem.56511": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Menschen stehen vorw\u00e4rts in den Stra\u00dfen", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Menschen stehen vorw\u00e4rts in den Stra\u00dfen", "tokens": ["Die", "Men\u00b7schen", "ste\u00b7hen", "vor\u00b7w\u00e4rts", "in", "den", "Stra\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und sehen auf die gro\u00dfen Himmelszeichen,", "tokens": ["Und", "se\u00b7hen", "auf", "die", "gro\u00b7\u00dfen", "Him\u00b7mels\u00b7zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo die Kometen mit den Feuernasen", "tokens": ["Wo", "die", "Ko\u00b7me\u00b7ten", "mit", "den", "Feu\u00b7er\u00b7na\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um die gezackten T\u00fcrme drohend schleichen.", "tokens": ["Um", "die", "ge\u00b7zack\u00b7ten", "T\u00fcr\u00b7me", "dro\u00b7hend", "schlei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und alle D\u00e4cher sind voll Sternedeuter,", "tokens": ["Und", "al\u00b7le", "D\u00e4\u00b7cher", "sind", "voll", "Ster\u00b7ne\u00b7deu\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die in den Himmel stecken gro\u00dfe R\u00f6hren.", "tokens": ["Die", "in", "den", "Him\u00b7mel", "ste\u00b7cken", "gro\u00b7\u00dfe", "R\u00f6h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Zaubrer, wachsend aus den Bodenl\u00f6chern,", "tokens": ["Und", "Zaub\u00b7rer", ",", "wach\u00b7send", "aus", "den", "Bo\u00b7den\u00b7l\u00f6\u00b7chern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In Dunkel schr\u00e4g, die einen Stern beschw\u00f6ren.", "tokens": ["In", "Dun\u00b7kel", "schr\u00e4g", ",", "die", "ei\u00b7nen", "Stern", "be\u00b7schw\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$,", "PRELS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Krankheit und Mi\u00dfwachs durch die Tore kriechen", "tokens": ["Krank\u00b7heit", "und", "Mi\u00df\u00b7wachs", "durch", "die", "To\u00b7re", "krie\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "APPR", "ART", "NN", "VVINF"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "In schwarzen T\u00fcchern. Und die Betten tragen", "tokens": ["In", "schwar\u00b7zen", "T\u00fc\u00b7chern", ".", "Und", "die", "Bet\u00b7ten", "tra\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$.", "KON", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das W\u00e4lzen und das Jammern vieler Siechen,", "tokens": ["Das", "W\u00e4l\u00b7zen", "und", "das", "Jam\u00b7mern", "vie\u00b7ler", "Sie\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und welche rennen mit den Totenschragen.", "tokens": ["Und", "wel\u00b7che", "ren\u00b7nen", "mit", "den", "To\u00b7ten\u00b7schra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Selbstm\u00f6rder gehen nachts in gro\u00dfen Horden,", "tokens": ["Selbst\u00b7m\u00f6r\u00b7der", "ge\u00b7hen", "nachts", "in", "gro\u00b7\u00dfen", "Hor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Die suchen vor sich ihr verlornes Wesen,", "tokens": ["Die", "su\u00b7chen", "vor", "sich", "ihr", "ver\u00b7lor\u00b7nes", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PRF", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Geb\u00fcckt in S\u00fcd und West, und Ost und Norden,", "tokens": ["Ge\u00b7b\u00fcckt", "in", "S\u00fcd", "und", "West", ",", "und", "Ost", "und", "Nor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "KON", "NN", "$,", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Staub zerfegend mit den Armen-Besen.", "tokens": ["Den", "Staub", "zer\u00b7fe\u00b7gend", "mit", "den", "Ar\u00b7men\u00b7Be\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Sie sind wie Staub, der h\u00e4lt noch eine Weile,", "tokens": ["Sie", "sind", "wie", "Staub", ",", "der", "h\u00e4lt", "noch", "ei\u00b7ne", "Wei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "NN", "$,", "PRELS", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Haare fallen schon auf ihren Wegen,", "tokens": ["Die", "Haa\u00b7re", "fal\u00b7len", "schon", "auf", "ih\u00b7ren", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie springen, da\u00df sie sterben, \u203anun\u2039 in Eile,", "tokens": ["Sie", "sprin\u00b7gen", ",", "da\u00df", "sie", "ster\u00b7ben", ",", "\u203a", "nun", "\u2039", "in", "Ei\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "VVINF", "$,", "$(", "ADV", "$(", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und sind mit totem Haupt im Feld gelegen.", "tokens": ["Und", "sind", "mit", "to\u00b7tem", "Haupt", "im", "Feld", "ge\u00b7le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "ADJA", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Noch manchmal zappelnd. Und der Felder Tiere", "tokens": ["Noch", "manch\u00b7mal", "zap\u00b7pelnd", ".", "Und", "der", "Fel\u00b7der", "Tie\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "$.", "KON", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Stehn um sie blind, und sto\u00dfen mit dem Horne", "tokens": ["Stehn", "um", "sie", "blind", ",", "und", "sto\u00b7\u00dfen", "mit", "dem", "Hor\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "ADJD", "$,", "KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In ihren Bauch. Sie strecken alle viere", "tokens": ["In", "ih\u00b7ren", "Bauch", ".", "Sie", "stre\u00b7cken", "al\u00b7le", "vie\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "PIAT", "PIS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Begraben unter Salbei und dem Dorne.", "tokens": ["Be\u00b7gra\u00b7ben", "un\u00b7ter", "Sal\u00b7bei", "und", "dem", "Dor\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "[das Jahr ist tot und leer von seinen Winden,", "tokens": ["das", "Jahr", "ist", "tot", "und", "leer", "von", "sei\u00b7nen", "Win\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Das wie ein Mantel h\u00e4ngt voll Wassertriefen,", "tokens": ["Das", "wie", "ein", "Man\u00b7tel", "h\u00e4ngt", "voll", "Was\u00b7ser\u00b7trie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "KOKOM", "ART", "NN", "VVFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und ewig Wetter, die sich klagend winden", "tokens": ["Und", "e\u00b7wig", "Wet\u00b7ter", ",", "die", "sich", "kla\u00b7gend", "win\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "NN", "$,", "PRELS", "PRF", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus Tiefen wolkig wieder zu den Tiefen.]", "tokens": ["Aus", "Tie\u00b7fen", "wol\u00b7kig", "wie\u00b7der", "zu", "den", "Tie\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "ADJD", "ADV", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Die Meere aber stocken. In den Wogen", "tokens": ["Die", "Mee\u00b7re", "a\u00b7ber", "sto\u00b7cken", ".", "In", "den", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVINF", "$.", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Schiffe h\u00e4ngen modernd und verdrossen,", "tokens": ["Die", "Schif\u00b7fe", "h\u00e4n\u00b7gen", "mo\u00b7dernd", "und", "ver\u00b7dros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zerstreut, und keine Str\u00f6mung wird gezogen", "tokens": ["Zer\u00b7streut", ",", "und", "kei\u00b7ne", "Str\u00f6\u00b7mung", "wird", "ge\u00b7zo\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KON", "PIAT", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und aller Himmel H\u00f6fe sind verschlossen.", "tokens": ["Und", "al\u00b7ler", "Him\u00b7mel", "H\u00f6\u00b7fe", "sind", "ver\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Die B\u00e4ume wechseln nicht die Zeiten", "tokens": ["Die", "B\u00e4u\u00b7me", "wech\u00b7seln", "nicht", "die", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und bleiben ewig tot in ihrem Ende", "tokens": ["Und", "blei\u00b7ben", "e\u00b7wig", "tot", "in", "ih\u00b7rem", "En\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und \u00fcber die verfallnen Wege spreiten", "tokens": ["Und", "\u00fc\u00b7ber", "die", "ver\u00b7fall\u00b7nen", "We\u00b7ge", "sprei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie h\u00f6lzern ihre langen Finger-H\u00e4nde.", "tokens": ["Sie", "h\u00f6l\u00b7zern", "ih\u00b7re", "lan\u00b7gen", "Fin\u00b7ger\u00b7H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Wer stirbt, der setzt sich auf, sich zu er