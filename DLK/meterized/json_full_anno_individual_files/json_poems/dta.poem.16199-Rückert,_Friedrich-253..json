{"dta.poem.16199": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "253.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ist in dir etwas noch, das du dich sch\u00e4mst zu zeigen,", "tokens": ["Ist", "in", "dir", "et\u00b7was", "noch", ",", "das", "du", "dich", "sch\u00e4mst", "zu", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "ADV", "ADV", "$,", "PRELS", "PPER", "PRF", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu deiner Ehre was du andern mu\u00dft verschweigen?", "tokens": ["Zu", "dei\u00b7ner", "Eh\u00b7re", "was", "du", "an\u00b7dern", "mu\u00dft", "ver\u00b7schwei\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PWS", "PPER", "PIS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was hilft es dir, wenn du's dem Blick der Welt entziehst,", "tokens": ["Was", "hilft", "es", "dir", ",", "wenn", "du's", "dem", "Blick", "der", "Welt", "ent\u00b7ziehst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "$,", "KOUS", "PIS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da wider Willen doch du es vor Augen siehst?", "tokens": ["Da", "wi\u00b7der", "Wil\u00b7len", "doch", "du", "es", "vor", "Au\u00b7gen", "siehst", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "PPER", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das wirf aus dir heraus, wenn du dir willst ersparen", "tokens": ["Das", "wirf", "aus", "dir", "he\u00b7raus", ",", "wenn", "du", "dir", "willst", "er\u00b7spa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "PPER", "PTKVZ", "$,", "KOUS", "PPER", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Anblicks Unlust sammt der M\u00fch es zu verwahren.", "tokens": ["Des", "An\u00b7blicks", "Un\u00b7lust", "sammt", "der", "M\u00fch", "es", "zu", "ver\u00b7wah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ART", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}