{"dta.poem.5232": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Der weinende Amor,  \n bey Betrachtung einer Bilds\u00e4ule zu Char-  \n lottenburg im Garten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-200905197778", "language": ["de:0.99"], "booktitle": "Karsch, Anna Luise: Auserlesene Gedichte. Berlin, 1764."}, "poem": {"stanza.1": {"line.1": {"text": "Ja, spricht sie, schweig, du nutzenloser Knabe,", "tokens": ["Ja", ",", "spricht", "sie", ",", "schweig", ",", "du", "nut\u00b7zen\u00b7lo\u00b7ser", "Kna\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "PPER", "$,", "VVFIN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was hilft mir Hercul, was Achill?", "tokens": ["Was", "hilft", "mir", "Her\u00b7cul", ",", "was", "A\u00b7chill", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "NE", "$,", "PRELS", "NE", "$."], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Wann ich den Helden nicht zu meinen F\u00fcssen habe,", "tokens": ["Wann", "ich", "den", "Hel\u00b7den", "nicht", "zu", "mei\u00b7nen", "F\u00fcs\u00b7sen", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "PTKNEG", "APPR", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der nicht empfinden will?", "tokens": ["Der", "nicht", "emp\u00b7fin\u00b7den", "will", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}