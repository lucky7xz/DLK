{"textgrid.poem.64864": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Oftmals glaubt' ich, da\u00df nicht mir zum Frommen", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oftmals glaubt' ich, da\u00df nicht mir zum Frommen", "tokens": ["Oft\u00b7mals", "glaubt'", "ich", ",", "da\u00df", "nicht", "mir", "zum", "From\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PTKNEG", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mir geworden sei des Liedes Gabe;", "tokens": ["Mir", "ge\u00b7wor\u00b7den", "sei", "des", "Lie\u00b7des", "Ga\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAPP", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Aber jetzo hab' ich wahrgenommen,", "tokens": ["A\u00b7ber", "jet\u00b7zo", "hab'", "ich", "wahr\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Da\u00df sie meines Lebens beste Labe.", "tokens": ["Da\u00df", "sie", "mei\u00b7nes", "Le\u00b7bens", "bes\u00b7te", "La\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "K\u00f6nnt' ich singen nicht, m\u00fc\u00dft ich zur\u00fcck", "tokens": ["K\u00f6nnt'", "ich", "sin\u00b7gen", "nicht", ",", "m\u00fc\u00dft", "ich", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "VVFIN", "PTKNEG", "$,", "VMFIN", "PPER", "PTKVZ"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Pressen nun mein schmerzvoll Liebesgl\u00fcck,", "tokens": ["Pres\u00b7sen", "nun", "mein", "schmerz\u00b7voll", "Lie\u00b7bes\u00b7gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPOSAT", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "D\u00fcrfte nicht mit gl\u00fchendem Bekennen", "tokens": ["D\u00fcrf\u00b7te", "nicht", "mit", "gl\u00fc\u00b7hen\u00b7dem", "Be\u00b7ken\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PTKNEG", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Dein mich nennen:", "tokens": ["Dein", "mich", "nen\u00b7nen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "PPER", "VVINF", "$."], "meter": "+---", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Also trennt die Welt vereinte Seelen,", "tokens": ["Al\u00b7so", "trennt", "die", "Welt", "ver\u00b7ein\u00b7te", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df nur im geheimni\u00dfvollen Sange", "tokens": ["Da\u00df", "nur", "im", "ge\u00b7heim\u00b7ni\u00df\u00b7vol\u00b7len", "San\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ich dir lust- und leidvoll darf erz\u00e4hlen", "tokens": ["Ich", "dir", "lust", "und", "leid\u00b7voll", "darf", "er\u00b7z\u00e4h\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PPER", "TRUNC", "KON", "ADJD", "VMFIN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Von des Busens ewig regem Drange.", "tokens": ["Von", "des", "Bu\u00b7sens", "e\u00b7wig", "re\u00b7gem", "Dran\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Da\u00df ich meiner Liebe Flammengru\u00df", "tokens": ["Da\u00df", "ich", "mei\u00b7ner", "Lie\u00b7be", "Flam\u00b7men\u00b7gru\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Erst in Reim und Verse bringen mu\u00df,", "tokens": ["Erst", "in", "Reim", "und", "Ver\u00b7se", "brin\u00b7gen", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Um ihn vor der Menschen b\u00f6sen Willen", "tokens": ["Um", "ihn", "vor", "der", "Men\u00b7schen", "b\u00f6\u00b7sen", "Wil\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PPER", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Zu verh\u00fcllen!", "tokens": ["Zu", "ver\u00b7h\u00fcl\u00b7len", "!"], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Da\u00df ich dich, der herrlich steht im Leben", "tokens": ["Da\u00df", "ich", "dich", ",", "der", "herr\u00b7lich", "steht", "im", "Le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "$,", "PRELS", "ADJD", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wie ein Halbgott aus versunknen Zeiten,", "tokens": ["Wie", "ein", "Halb\u00b7gott", "aus", "ver\u00b7sun\u00b7knen", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Mit den Nebelfl\u00f6ren mu\u00df umgeben,", "tokens": ["Mit", "den", "Ne\u00b7bel\u00b7fl\u00f6\u00b7ren", "mu\u00df", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Die sich rings um Ideale breiten,", "tokens": ["Die", "sich", "rings", "um", "I\u00b7dea\u00b7le", "brei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "APPR", "NN", "ADJA", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Da\u00df ich wie von leerem Traume nur", "tokens": ["Da\u00df", "ich", "wie", "von", "lee\u00b7rem", "Trau\u00b7me", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "KOKOM", "APPR", "ADJA", "NN", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Sprechen darf von unserm heil'gen Schwur,", "tokens": ["Spre\u00b7chen", "darf", "von", "un\u00b7serm", "heil'\u00b7gen", "Schwur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPR", "PPOSAT", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Von der festgeschlossnen Geistesehe", "tokens": ["Von", "der", "fest\u00b7ge\u00b7schloss\u00b7nen", "Geis\u00b7te\u00b7se\u00b7he"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Gl\u00fcck und Wehe!", "tokens": ["Gl\u00fcck", "und", "We\u00b7he", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Und so stelle ich die Welt zufrieden,", "tokens": ["Und", "so", "stel\u00b7le", "ich", "die", "Welt", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die nur in der Dichtung Lieb' gestattet,", "tokens": ["Die", "nur", "in", "der", "Dich\u00b7tung", "Lieb'", "ge\u00b7stat\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "----+-+-+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Die sich von Begeistrung ausgeschieden,", "tokens": ["Die", "sich", "von", "Be\u00b7geis\u00b7trung", "aus\u00b7ge\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Weil sie f\u00fcr den Flug viel zu ermattet;", "tokens": ["Weil", "sie", "f\u00fcr", "den", "Flug", "viel", "zu", "er\u00b7mat\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "ADV", "PTKA", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Die im Liede sch\u00f6n und herrlich nennt,", "tokens": ["Die", "im", "Lie\u00b7de", "sch\u00f6n", "und", "herr\u00b7lich", "nennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Was sie in der Wirklichkeit verkennt,", "tokens": ["Was", "sie", "in", "der", "Wirk\u00b7lich\u00b7keit", "ver\u00b7kennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Nur in ihm des Herzens sehnend Streifen", "tokens": ["Nur", "in", "ihm", "des", "Her\u00b7zens", "seh\u00b7nend", "Strei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPER", "ART", "NN", "VVPP", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Mag begreifen.", "tokens": ["Mag", "be\u00b7grei\u00b7fen", "."], "token_info": ["word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Nimmer werd' der Schleier ihr zerrissen;", "tokens": ["Nim\u00b7mer", "werd'", "der", "Schlei\u00b7er", "ihr", "zer\u00b7ris\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ihrem Irrthum will ich nichts erwidern,", "tokens": ["Ih\u00b7rem", "Irr\u00b7thum", "will", "ich", "nichts", "er\u00b7wi\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Du allein, mein Freund, nur du sollst wissen,", "tokens": ["Du", "al\u00b7lein", ",", "mein", "Freund", ",", "nur", "du", "sollst", "wis\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PPOSAT", "NN", "$,", "ADV", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Da\u00df du Quell von allen meinen Liedern;", "tokens": ["Da\u00df", "du", "Quell", "von", "al\u00b7len", "mei\u00b7nen", "Lie\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "APPR", "PIAT", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Da\u00df jedweder Laut von meinem Mund", "tokens": ["Da\u00df", "jed\u00b7we\u00b7der", "Laut", "von", "mei\u00b7nem", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "APPR", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Einer ew'gen Wahrheit treue Kund',", "tokens": ["Ei\u00b7ner", "ew'\u00b7gen", "Wahr\u00b7heit", "treu\u00b7e", "Kund'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Bis mein Sang, dir liebend zugewendet,", "tokens": ["Bis", "mein", "Sang", ",", "dir", "lie\u00b7bend", "zu\u00b7ge\u00b7wen\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Jenseits endet!", "tokens": ["Jen\u00b7seits", "en\u00b7det", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.6": {"line.1": {"text": "Oftmals glaubt' ich, da\u00df nicht mir zum Frommen", "tokens": ["Oft\u00b7mals", "glaubt'", "ich", ",", "da\u00df", "nicht", "mir", "zum", "From\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PTKNEG", "PPER", "APPRART", "NN"], "meter":