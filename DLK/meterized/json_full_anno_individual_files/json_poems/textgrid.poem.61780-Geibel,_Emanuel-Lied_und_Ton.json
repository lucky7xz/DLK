{"textgrid.poem.61780": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Lied und Ton", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verzaubert lag, verschollen,", "tokens": ["Ver\u00b7zau\u00b7bert", "lag", ",", "ver\u00b7schol\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVPP", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dornr\u00f6schen gleich im Walde tief,", "tokens": ["Dorn\u00b7r\u00f6\u00b7schen", "gleich", "im", "Wal\u00b7de", "tief", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Lied auf staub'gen Rollen,", "tokens": ["Das", "Lied", "auf", "staub'\u00b7gen", "Rol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Musenkind, und schlief.", "tokens": ["Das", "Mu\u00b7sen\u00b7kind", ",", "und", "schlief", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da bricht durchs Dorngestrippe", "tokens": ["Da", "bricht", "durchs", "Dorn\u00b7ge\u00b7strip\u00b7pe"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit hellem Ruf ein K\u00f6nigssohn,", "tokens": ["Mit", "hel\u00b7lem", "Ruf", "ein", "K\u00f6\u00b7nigs\u00b7sohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da k\u00fc\u00dft mit warmer Lippe", "tokens": ["Da", "k\u00fc\u00dft", "mit", "war\u00b7mer", "Lip\u00b7pe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Schl\u00e4ferin der Ton.", "tokens": ["Die", "Schl\u00e4\u00b7fe\u00b7rin", "der", "Ton", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und sieh, zu raschen Schl\u00e4gen", "tokens": ["Und", "sieh", ",", "zu", "ra\u00b7schen", "Schl\u00e4\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Urpl\u00f6tzlich ist ihr Herz erwacht;", "tokens": ["Ur\u00b7pl\u00f6tz\u00b7lich", "ist", "ihr", "Herz", "er\u00b7wacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie hebt sich ihm entgegen,", "tokens": ["Sie", "hebt", "sich", "ihm", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihr Auge weint und lacht.", "tokens": ["Ihr", "Au\u00b7ge", "weint", "und", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Vom Lager aufgesprungen", "tokens": ["Vom", "La\u00b7ger", "auf\u00b7ge\u00b7sprun\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVINF"], "meter": "-+-+