{"dta.poem.23812": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Sultan.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1700", "urn": "urn:nbn:de:kobv:b4-200905197532", "language": ["de:0.99"], "booktitle": "[Canitz, Friedrich Rudolph Ludwig von]: Neben-Stunden Unterschiedener Gedichte. [Hrsg. v. Joachim Lange]. Berlin, 1700."}, "poem": {"stanza.1": {"line.1": {"text": "Kein Ottomanner Printz mit allen seinen Reichen/", "tokens": ["Kein", "Ot\u00b7to\u00b7man\u00b7ner", "Printz", "mit", "al\u00b7len", "sei\u00b7nen", "Rei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "APPR", "PIAT", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist mir an Tapfferkeit und Ansehn zu vergleichen.", "tokens": ["Ist", "mir", "an", "Tapf\u00b7fer\u00b7keit", "und", "An\u00b7sehn", "zu", "ver\u00b7glei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nur eins macht da\u00df ich nicht un\u00fcberwindlich bin:", "tokens": ["Nur", "eins", "macht", "da\u00df", "ich", "nicht", "un\u00b7\u00fc\u00b7berw\u00b7ind\u00b7lich", "bin", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "KOUS", "PPER", "PTKNEG", "ADJD", "VAFIN", "$."], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.4": {"text": "Die ungemeine Zier der holden K\u00e4yserin.", "tokens": ["Die", "un\u00b7ge\u00b7mei\u00b7ne", "Zier", "der", "hol\u00b7den", "K\u00e4y\u00b7se\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "m