{"textgrid.poem.62502": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Himmels Botschaft ist erklungen;", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Himmels Botschaft ist erklungen;", "tokens": ["Him\u00b7mels", "Bot\u00b7schaft", "ist", "er\u00b7klun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach, ein wunderbarer Klang!", "tokens": ["Ach", ",", "ein", "wun\u00b7der\u00b7ba\u00b7rer", "Klang", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Engel haben uns gesungen", "tokens": ["En\u00b7gel", "ha\u00b7ben", "uns", "ge\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einen seligen Gesang:", "tokens": ["Ei\u00b7nen", "se\u00b7li\u00b7gen", "Ge\u00b7sang", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Heute sei das Kind erschienen,", "tokens": ["Heu\u00b7te", "sei", "das", "Kind", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dem die Himmel ewig dienen.", "tokens": ["Dem", "die", "Him\u00b7mel", "e\u00b7wig", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nun zu suchen seine Spuren", "tokens": ["Nun", "zu", "su\u00b7chen", "sei\u00b7ne", "Spu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PTKZU", "VVINF", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und zu schaun das Licht der Welt,", "tokens": ["Und", "zu", "schaun", "das", "Licht", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKZU", "VVINF", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fchrt uns Liebe durch die Fluren,", "tokens": ["F\u00fchrt", "uns", "Lie\u00b7be", "durch", "die", "Flu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Liebe zieht uns \u00fcber's Feld.", "tokens": ["Lie\u00b7be", "zieht", "uns", "\u00fc\u00b7ber's", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sprecht, wo seid ihr, liebe Boten,", "tokens": ["Sprecht", ",", "wo", "seid", "ihr", ",", "lie\u00b7be", "Bo\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "VAFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die uns jenen Gru\u00df entboten?", "tokens": ["Die", "uns", "je\u00b7nen", "Gru\u00df", "ent\u00b7bo\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PDAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Seitw\u00e4rts lenken sich die Schritte,", "tokens": ["Seit\u00b7w\u00e4rts", "len\u00b7ken", "sich", "die", "Schrit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seitw\u00e4rts, wo das helle Licht", "tokens": ["Seit\u00b7w\u00e4rts", ",", "wo", "das", "hel\u00b7le", "Licht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus der alten kleinen H\u00fctte", "tokens": ["Aus", "der", "al\u00b7ten", "klei\u00b7nen", "H\u00fct\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gar zu lieb und fr\u00f6hlich bricht.", "tokens": ["Gar", "zu", "lieb", "und", "fr\u00f6h\u00b7lich", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKA", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo sich unsre St\u00e4be neigen", "tokens": ["Wo", "sich", "uns\u00b7re", "St\u00e4\u00b7be", "nei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Scheinet Alles hinzuzeigen.", "tokens": ["Schei\u00b7net", "Al\u00b7les", "hin\u00b7zu\u00b7zei\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Kommt ihr endlich in das Leben,", "tokens": ["Kommt", "ihr", "end\u00b7lich", "in", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alte Sehnsucht, alter Traum?", "tokens": ["Al\u00b7te", "Sehn\u00b7sucht", ",", "al\u00b7ter", "Traum", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kann die Erde dir nicht geben", "tokens": ["Kann", "die", "Er\u00b7de", "dir", "nicht", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "PPER", "PTKNEG", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Be\u00dfre Ruh, und bessern Raum?", "tokens": ["Be\u00df\u00b7re", "Ruh", ",", "und", "bes\u00b7sern", "Raum", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo die Thiere friedlich schlafen,", "tokens": ["Wo", "die", "Thie\u00b7re", "fried\u00b7lich", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Liegt der Hirt bei seinen Schafen.", "tokens": ["Liegt", "der", "Hirt", "bei", "sei\u00b7nen", "Scha\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sei gegr\u00fc\u00dft, o holder Knabe,", "tokens": ["Sei", "ge\u00b7gr\u00fc\u00dft", ",", "o", "hol\u00b7der", "Kna\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "FM", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unsrer Hoffnung Morgenroth,", "tokens": ["Uns\u00b7rer", "Hoff\u00b7nung", "Mor\u00b7gen\u00b7roth", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aller Himmel h\u00f6chste Gabe,", "tokens": ["Al\u00b7ler", "Him\u00b7mel", "h\u00f6chs\u00b7te", "Ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aller Welten Lebensbrod,", "tokens": ["Al\u00b7ler", "Wel\u00b7ten", "Le\u00b7bens\u00b7brod", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Angesagt von alter Kunde,", "tokens": ["An\u00b7ge\u00b7sagt", "von", "al\u00b7ter", "Kun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Meister in dem neuen Bunde!", "tokens": ["Meis\u00b7ter", "in", "dem", "neu\u00b7en", "Bun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Nimm den Stab mit zarten H\u00e4nden,", "tokens": ["Nimm", "den", "Stab", "mit", "zar\u00b7ten", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deinen sanften Hirtenstab,", "tokens": ["Dei\u00b7nen", "sanf\u00b7ten", "Hir\u00b7ten\u00b7stab", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fchre treu von allen Enden", "tokens": ["F\u00