}, "line.4": {"text": "Gott im hellen Sternenklang!", "tokens": ["Gott", "im", "hel\u00b7len", "Ster\u00b7nen\u00b7klang", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Friede soll nun auf der Erden", "tokens": ["Frie\u00b7de", "soll", "nun", "auf", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aller Menschen Freude werden!", "tokens": ["Al\u00b7ler", "Men\u00b7schen", "Freu\u00b7de", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Himmels Botschaft ist erklungen;", "tokens": ["Him\u00b7mels", "Bot\u00b7schaft", "ist", "er\u00b7klun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach, ein wunderbarer Klang!", "tokens": ["Ach", ",", "ein", "wun\u00b7der\u00b7ba\u00b7rer", "Klang", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Engel haben uns gesungen", "tokens": ["En\u00b7gel", "ha\u00b7ben", "uns", "ge\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einen seligen Gesang:", "tokens": ["Ei\u00b7nen", "se\u00b7li\u00b7gen", "Ge\u00b7sang", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Heute sei das