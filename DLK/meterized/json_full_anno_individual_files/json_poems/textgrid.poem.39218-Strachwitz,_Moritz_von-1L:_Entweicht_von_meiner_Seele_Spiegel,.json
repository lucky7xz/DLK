{"textgrid.poem.39218": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Entweicht von meiner Seele Spiegel,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Entweicht von meiner Seele Spiegel,", "tokens": ["Ent\u00b7weicht", "von", "mei\u00b7ner", "See\u00b7le", "Spie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Nebel, die ihr ihn umzogt,", "tokens": ["Ihr", "Ne\u00b7bel", ",", "die", "ihr", "ihn", "um\u00b7zogt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es ist der Liebe Schwanenfl\u00fcgel,", "tokens": ["Es", "ist", "der", "Lie\u00b7be", "Schwa\u00b7nen\u00b7fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der \u00fcber meinem Haupte wogt.", "tokens": ["Der", "\u00fc\u00b7ber", "mei\u00b7nem", "Haup\u00b7te", "wogt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und sieh: Du kommst dahergefahren,", "tokens": ["Und", "sieh", ":", "Du", "kommst", "da\u00b7her\u00b7ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$.", "PPER", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Frau Minne, durch des \u00c4thers See;", "tokens": ["Frau", "Min\u00b7ne", ",", "durch", "des", "\u00c4\u00b7thers", "See", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch anders bist Du als vor Jahren,", "tokens": ["Doch", "an\u00b7ders", "bist", "Du", "als", "vor", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "KOKOM", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und strahlender, allm\u00e4cht'ge Fee!", "tokens": ["Und", "strah\u00b7len\u00b7der", ",", "all\u00b7m\u00e4cht'\u00b7ge", "Fee", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du tr\u00e4uftest sonst mir als Armide", "tokens": ["Du", "tr\u00e4uf\u00b7test", "sonst", "mir", "als", "Ar\u00b7mi\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "KOUS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Zauberschlaf ins beste Mark,", "tokens": ["Den", "Zau\u00b7ber\u00b7schlaf", "ins", "bes\u00b7te", "Mark", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun kommt Dein Ku\u00df, wie Gottesfriede,", "tokens": ["Nun", "kommt", "Dein", "Ku\u00df", ",", "wie", "Got\u00b7tes\u00b7frie\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "PWAV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und macht mich freudig, fromm und stark.", "tokens": ["Und", "macht", "mich", "freu\u00b7dig", ",", "fromm", "und", "stark", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und kamst Du sonst gesch\u00e4umt, geschossen,", "tokens": ["Und", "kamst", "Du", "sonst", "ge\u00b7sch\u00e4umt", ",", "ge\u00b7schos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Strom, der vom Gebirge rollt,", "tokens": ["Ein", "Strom", ",", "der", "vom", "Ge\u00b7bir\u00b7ge", "rollt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So liegst Du jetzo mild ergossen,", "tokens": ["So", "liegst", "Du", "jet\u00b7zo", "mild", "er\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein See im keuschen Sonnengold.", "tokens": ["Ein", "See", "im", "keu\u00b7schen", "Son\u00b7nen\u00b7gold", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du bist kein Feuer farbenfl\u00fcchtig,", "tokens": ["Du", "bist", "kein", "Feu\u00b7er", "far\u00b7ben\u00b7fl\u00fcch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das pr\u00e4cht'ge Funkengarben s\u00e4t,", "tokens": ["Das", "pr\u00e4cht'\u00b7ge", "Fun\u00b7ken\u00b7gar\u00b7ben", "s\u00e4t", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nein, eine Flamme alldurchsichtig", "tokens": ["Nein", ",", "ei\u00b7ne", "Flam\u00b7me", "all\u00b7durch\u00b7sich\u00b7tig"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und loderst still in Majest\u00e4t.", "tokens": ["Und", "lo\u00b7derst", "still", "in", "Ma\u00b7jes\u00b7t\u00e4t", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du bist kein ungest\u00fcmes Regen,", "tokens": ["Du", "bist", "kein", "un\u00b7ge\u00b7st\u00fc\u00b7mes", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das hei\u00dfe Herzen blutig gr\u00e4bt,", "tokens": ["Das", "hei\u00b7\u00dfe", "Her\u00b7zen", "blu\u00b7tig", "gr\u00e4bt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du bist der reiche Gottessegen,", "tokens": ["Du", "bist", "der", "rei\u00b7che", "Got\u00b7tes\u00b7se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der \u00fcber meinem Liede schwebt.", "tokens": ["Der", "\u00fc\u00b7ber", "mei\u00b7nem", "Lie\u00b7de", "schwebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Es war mein Geist ein sehnsuchtskranker", "tokens": ["Es", "war", "mein", "Geist", "ein", "sehn\u00b7suchts\u00b7kran\u00b7ker"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nach reiner Liebe, frischem Blut;", "tokens": ["Nach", "rei\u00b7ner", "Lie\u00b7be", ",", "fri\u00b7schem", "Blut", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Sanges Schiff lag matt vor Anker,", "tokens": ["Des", "San\u00b7ges", "Schiff", "lag", "matt", "vor", "An\u00b7ker", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es schliefen Segel, Lust und Flut.", "tokens": ["Es", "schlie\u00b7fen", "Se\u00b7gel", ",", "Lust", "und", "Flut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da kommt Dein Sturm und schwellt die Linnen,", "tokens": ["Da", "kommt", "Dein", "Sturm", "und", "schwellt", "die", "Lin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Purpurwimpel fliegt zur H\u00f6h',", "tokens": ["Der", "Pur\u00b7pur\u00b7wim\u00b7pel", "fliegt", "zur", "H\u00f6h'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Segler jagt mit Klang von hinnen,", "tokens": ["Der", "Seg\u00b7ler", "jagt", "mit", "Klang", "von", "hin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und vor dem Kiele jauchzt die See.", "tokens": ["Und", "vor", "dem", "Kie\u00b7le", "jauchzt", "die", "See", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und immer tiefer werd' ich's inne,", "tokens": ["Und", "im\u00b7mer", "tie\u00b7fer", "werd'", "ich's", "in\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was vor Dir war, ist Farbendunst,", "tokens": ["Was", "vor", "Dir", "war", ",", "ist", "Far\u00b7ben\u00b7dunst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "VAFIN", "$,", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du bist die wahre, h\u00f6chste Minne,", "tokens": ["Du", "bist", "die", "wah\u00b7re", ",", "h\u00f6chs\u00b7te", "Min\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du bist des Himmels beste Gunst!", "tokens": ["Du", "bist", "des", "Him\u00b7mels", "bes\u00b7te", "Gunst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Heil jedem Munde, der Dich feiert,", "tokens": ["Heil", "je\u00b7dem", "Mun\u00b7de", ",", "der", "Dich", "fei\u00b7ert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Aus dem Dein Bl\u00fctenodem geht;", "tokens": ["Aus", "dem", "Dein", "Bl\u00fc\u00b7te\u00b7no\u00b7dem", "geht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wem Du Dich einmal ganz entschleiert,", "tokens": ["Wem", "Du", "Dich", "ein\u00b7mal", "ganz", "ent\u00b7schlei\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PRF", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der ist wahrhaftig ein Poet.", "tokens": ["Der", "ist", "wahr\u00b7haf\u00b7tig", "ein", "Po\u00b7et", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Es zieht die Nacht den heil'gen Bogen,", "tokens": ["Es", "zieht", "die", "Nacht", "den", "heil'\u00b7gen", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Liebe wogt ob Land und Meer;", "tokens": ["Und", "Lie\u00b7be", "wogt", "ob", "Land", "und", "Meer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "KOUS", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es tr\u00e4gt auf lauen Zitterwogen", "tokens": ["Es", "tr\u00e4gt", "auf", "lau\u00b7en", "Zit\u00b7ter\u00b7wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die linde Luft dies Lied daher,", "tokens": ["Die", "lin\u00b7de", "Luft", "dies", "Lied", "da\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PDS", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und z\u00fcrnst Du mir in keuschem Sinne,", "tokens": ["Und", "z\u00fcrnst", "Du", "mir", "in", "keu\u00b7schem", "Sin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "O Herrin, wenn es trifft Dein Ohr,", "tokens": ["O", "Her\u00b7rin", ",", "wenn", "es", "trifft", "Dein", "Ohr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KOUS", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So denk': Es ist die reinste Minne!", "tokens": ["So", "denk'", ":", "Es", "ist", "die", "reins\u00b7te", "Min\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und schlafe furchtlos, wie zuvor.", "tokens": ["Und", "schla\u00b7fe", "furcht\u00b7los", ",", "wie", "zu\u00b7vor", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,", "PWAV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Entweicht von meiner Seele Spiegel,", "tokens": ["Ent\u00b7weicht", "von", "mei\u00b7ner", "See\u00b7le", "Spie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Nebel, die ihr ihn umzogt,", "tokens": ["Ihr", "Ne\u00b7bel", ",", "die", "ihr", "ihn", "um\u00b7zogt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es ist der Liebe Schwanenfl\u00fcgel,", "tokens": ["Es", "ist", "der", "Lie\u00b7be", "Schwa\u00b7nen\u00b7fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der \u00fcber meinem Haupte wogt.", "tokens": ["Der", "\u00fc\u00b7ber", "mei\u00b7nem", "Haup\u00b7te", "wogt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und sieh: Du kommst dahergefahren,", "tokens": ["Und", "sieh", ":", "Du", "kommst", "da\u00b7her\u00b7ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$.", "PPER", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Frau Minne, durch des \u00c4thers See;", "tokens": ["Frau", "Min\u00b7ne", ",", "durch", "des", "\u00c4\u00b7thers", "See", ";"], "token_info": ["word", "w