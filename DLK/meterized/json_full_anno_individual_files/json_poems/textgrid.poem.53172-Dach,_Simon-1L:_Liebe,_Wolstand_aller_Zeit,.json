{"textgrid.poem.53172": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Liebe, Wolstand aller Zeit,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebe, Wolstand aller Zeit,", "tokens": ["Lie\u00b7be", ",", "Wol\u00b7stand", "al\u00b7ler", "Zeit", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mutter s\u00fcsser Freundlicheit,", "tokens": ["Mut\u00b7ter", "s\u00fcs\u00b7ser", "Freund\u00b7li\u00b7cheit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kron' und Glantz der h\u00f6chsten Gaben,", "tokens": ["Kron'", "und", "Glantz", "der", "h\u00f6chs\u00b7ten", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Du gew\u00fcnschtes Himmel-Kind,", "tokens": ["Du", "ge\u00b7w\u00fcnschtes", "Him\u00b7mel\u00b7Kind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Schatz der Sch\u00e4tz', erb\u00e4rmlich sind", "tokens": ["Schatz", "der", "Sch\u00e4tz'", ",", "er\u00b7b\u00e4rm\u00b7lich", "sind"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "ADJD", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Die erst, welche dich nicht haben.", "tokens": ["Die", "erst", ",", "wel\u00b7che", "dich", "nicht", "ha\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "PRELS", "PPER", "PTKNEG", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Stell dich ein auff diesen Sal,", "tokens": ["Stell", "dich", "ein", "auff", "die\u00b7sen", "Sal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ART", "APPR", "PDAT", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Kr\u00f6n die\u00df wehrte Hochzeit-Mahl,", "tokens": ["Kr\u00f6n", "die\u00df", "wehr\u00b7te", "Hoch\u00b7zeit\u00b7Mahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PDS", "VVFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df dein Liecht f\u00fcr andern blinken,", "tokens": ["La\u00df", "dein", "Liecht", "f\u00fcr", "an\u00b7dern", "blin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "APPR", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Halt die G\u00e4st in guter Acht,", "tokens": ["Halt", "die", "G\u00e4st", "in", "gu\u00b7ter", "Acht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sey die Wirtschaft und die Tracht,", "tokens": ["Sey", "die", "Wirt\u00b7schaft", "und", "die", "Tracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "La\u00df dich essen, la\u00df dich trinken.", "tokens": ["La\u00df", "dich", "es\u00b7sen", ",", "la\u00df", "dich", "trin\u00b7ken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$,", "VVIMP", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Thu dein Nectar in den Wein,", "tokens": ["Thu", "dein", "Nec\u00b7tar", "in", "den", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df man schmecke dich allein,", "tokens": ["Da\u00df", "man", "schme\u00b7cke", "dich", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gieb jhm newe Krafft vnd Leben,", "tokens": ["Gieb", "jhm", "ne\u00b7we", "Krafft", "vnd", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Thu was sonst der Zucker sol,", "tokens": ["Thu", "was", "sonst", "der", "Zu\u00b7cker", "sol", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ART", "NN", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mach uns mehr von Liebe vol", "tokens": ["Mach", "uns", "mehr", "von", "Lie\u00b7be", "vol"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Als vom Safft der s\u00fcssen Reben.", "tokens": ["Als", "vom", "Safft", "der", "s\u00fcs\u00b7sen", "Re\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Thu den Argwohn in den Bann,", "tokens": ["Thu", "den", "Arg\u00b7wohn", "in", "den", "Bann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Steh im Reyen oben an,", "tokens": ["Steh", "im", "Re\u00b7yen", "o\u00b7ben", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hei\u00df die Seiten lieblich klingen,", "tokens": ["Hei\u00df", "die", "Sei\u00b7ten", "lieb\u00b7lich", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schaff, da\u00df alles richtig geh'", "tokens": ["Schaff", ",", "da\u00df", "al\u00b7les", "rich\u00b7tig", "geh'"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "PIS", "ADJD", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Vnd sich keiner untersteh", "tokens": ["Vnd", "sich", "kei\u00b7ner", "un\u00b7ter\u00b7steh"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PRF", "PIS", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Einem andern vor-zu-springen.", "tokens": ["Ei\u00b7nem", "an\u00b7dern", "vor\u00b7zu\u00b7sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Macht jetzt manchen Heyraht-Schlu\u00df,", "tokens": ["Macht", "jetzt", "man\u00b7chen", "Hey\u00b7raht\u00b7Schlu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil durch dich sich lieben mu\u00df,", "tokens": ["Weil", "durch", "dich", "sich", "lie\u00b7ben", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "PRF", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was sich vor offt nie gesehen,", "tokens": ["Was", "sich", "vor", "offt", "nie", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPR", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was von manchem wird belacht,", "tokens": ["Was", "von", "man\u00b7chem", "wird", "be\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PIAT", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nicht gegl\u00e4ubt wird, nicht bedacht,", "tokens": ["Nicht", "ge\u00b7gl\u00e4ubt", "wird", ",", "nicht", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "VVPP", "VAFIN", "$,", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Kan gar leicht durch dich geschehen.", "tokens": ["Kan", "gar", "leicht", "durch", "dich", "ge\u00b7sche\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADJD", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Sey f\u00fcr allen diesem Paar,", "tokens": ["Sey", "f\u00fcr", "al\u00b7len", "die\u00b7sem", "Paar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PIAT", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebe, g\u00fcnstig immerdar,", "tokens": ["Lie\u00b7be", ",", "g\u00fcns\u00b7tig", "im\u00b7mer\u00b7dar", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gieb sie selbs anjetzt zusammen:", "tokens": ["Gieb", "sie", "selbs", "an\u00b7jetzt", "zu\u00b7sam\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Senck in aller Lust und Pein", "tokens": ["Senck", "in", "al\u00b7ler", "Lust", "und", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PIAT", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dich in jhre Hertzen ein,", "tokens": ["Dich", "in", "jhre", "Hert\u00b7zen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mehr in jhnen deine Flammen.", "tokens": ["Mehr", "in", "jh\u00b7nen", "dei\u00b7ne", "Flam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Sieh, was Kunst den Br\u00e4utgam ziert,", "tokens": ["Sieh", ",", "was", "Kunst", "den", "Br\u00e4ut\u00b7gam", "ziert", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "NN", "ART", "NE", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Was die Braut f\u00fcr Vnschuld f\u00fchrt,", "tokens": ["Was", "die", "Braut", "f\u00fcr", "Vn\u00b7schuld", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], 