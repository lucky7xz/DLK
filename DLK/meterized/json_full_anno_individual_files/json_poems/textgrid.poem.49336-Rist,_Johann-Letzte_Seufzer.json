ord", "word", "punct", "word", "word"], "pos": ["NN", "NE", "NE", "$.", "PPER", "VVFIN", "$,", "PPER", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.55": {"text": "Mein Bruder von Gem\u00fcte.", "tokens": ["Mein", "Bru\u00b7der", "von", "Ge\u00b7m\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.56": {"text": "Dein herber Tod hat mich befreit,", "tokens": ["Dein", "her\u00b7ber", "Tod", "hat", "mich", "be\u00b7freit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.57": {"text": "Dein Blut gibt mir die Seligkeit,", "tokens": ["Dein", "Blut", "gibt", "mir", "die", "Se\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.58": {"text": "Du hast mich angenommen", "tokens": ["Du", "hast", "mich", "an\u00b7ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.59": {"text": "Im Gnadenbund; ich bin gesund", "tokens": ["Im", "Gna\u00b7den\u00b7bund", ";", "ich", "bin", "ge\u00b7sund"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$.", "PPER", "VAFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.60": {"text": "Aus diesem Bande kommen.", "tokens": ["Aus", "die\u00b7sem", "Ban\u00b7de", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.61": {"text": "Ach du mein allerh\u00f6chstes Gut", "tokens": ["Ach", "du", "mein", "al\u00b7ler\u00b7h\u00f6chs\u00b7tes", "Gut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.62": {"text": "Hast ja dein rosinfarbes Blut", "tokens": ["Hast", "ja", "dein", "ro\u00b7sin\u00b7far\u00b7bes", "Blut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.63": {"text": "Auf Golgatha vergossen", "tokens": ["Auf", "Gol\u00b7ga\u00b7tha", "ver\u00b7gos\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NE", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.64": {"text": "Ganz mildiglich, das machet mich", "tokens": ["Ganz", "mil\u00b7dig\u00b7lich", ",", "das", "ma\u00b7chet", "mich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "PDS", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.65": {"text": "Zu deines Reichs Genossen.", "tokens": ["Zu", "dei\u00b7nes", "Reichs", "Ge\u00b7nos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.66": {"text": "Wolan, so la\u00df mich das Gesicht", "tokens": ["Wo\u00b7lan", ",", "so", "la\u00df", "mich", "das", "Ge\u00b7sicht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "VVIMP", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.67": {"text": "Des Menschenw\u00fcrgers schrecken nicht;", "tokens": ["Des", "Men\u00b7schen\u00b7w\u00fcr\u00b7gers", "schre\u00b7cken", "nicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.68": {"text": "Wenn mein Gesicht verschwindet,", "tokens": ["Wenn", "mein", "Ge\u00b7sicht", "ver\u00b7schwin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.69": {"text": "La\u00df sehend sein mein Herz allein,", "tokens": ["La\u00df", "se\u00b7hend", "sein", "mein", "Herz", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVPP", "VAINF", "PPOSAT", "NN", "ADV", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.70": {"text": "Das dich im Glauben findet.", "tokens": ["Das", "dich", "im", "Glau\u00b7ben", "fin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.71": {"text": "O Jesu, nimm dich meiner an,", "tokens": ["O", "Je\u00b7su", ",", "nimm", "dich", "mei\u00b7ner", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "VVIMP", "PPER", "PPOSAT", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.72": {"text": "La\u00df, wenn ich nicht mehr reden kan,", "tokens": ["La\u00df", ",", "wenn", "ich", "nicht", "mehr", "re\u00b7den", "kan", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-----+-+", "measure": "unknown.measure.di"}, "line.73": {"text": "Mein Seelichen noch schreien;", "tokens": ["Mein", "See\u00b7li\u00b7chen", "noch", "schrei\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.74": {"text": "Du kanst f\u00fcrwahr mich aus Gefahr", "tokens": ["Du", "kanst", "f\u00fcr\u00b7wahr", "mich", "aus", "Ge\u00b7fahr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "PRF", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.75": {"text": "Des Todes schnell befreien.", "tokens": ["Des", "To\u00b7des", "schnell", "be\u00b7frei\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.76": {"text": "Nim diese Seel' in deine H\u00e4nd'", "tokens": ["Nim", "die\u00b7se", "Seel'", "in", "dei\u00b7ne", "H\u00e4nd'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "PDAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.77": {"text": "Und gib mir bald ein seligs End',", "tokens": ["Und", "gib", "mir", "bald", "ein", "se\u00b7ligs", "End'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.78": {"text": "Auf da\u00df ich deinen Namen,", "tokens": ["Auf", "da\u00df", "ich", "dei\u00b7nen", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.79": {"text": "Mit Cherubim und Seraphim,", "tokens": ["Mit", "Che\u00b7ru\u00b7bim", "und", "Se\u00b7ra\u00b7phim", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.80": {"text": "M\u00f6g' ewig preisen. Amen.", "tokens": ["M\u00f6g'", "e\u00b7wig", "prei\u00b7sen", ".", "A\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "ADJD", "VVFIN", "$.", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}