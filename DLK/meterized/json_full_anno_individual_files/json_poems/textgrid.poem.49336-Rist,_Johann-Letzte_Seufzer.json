{"textgrid.poem.49336": {"metadata": {"author": {"name": "Rist, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Letzte Seufzer", "genre": "verse", "period": "N.A.", "pub_year": 1637, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr Jesu Christ, mein Trost und Licht,", "tokens": ["Herr", "Je\u00b7su", "Christ", ",", "mein", "Trost", "und", "Licht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "NE", "$,", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu dir heb' ich mein Angesicht", "tokens": ["Zu", "dir", "heb'", "ich", "mein", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "In meiner Not und Leiden,", "tokens": ["In", "mei\u00b7ner", "Not", "und", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ach Gott! Dein Kind sol itz geschwind", "tokens": ["Ach", "Gott", "!", "Dein", "Kind", "sol", "itz", "ge\u00b7schwind"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "NN", "$.", "PPOSAT", "NN", "VMFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von dieser Welt abscheiden.", "tokens": ["Von", "die\u00b7ser", "Welt", "ab\u00b7schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hie lig' ich, Herr, in deiner Hand", "tokens": ["Hie", "lig'", "ich", ",", "Herr", ",", "in", "dei\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und warte, wenn des Lebens Band", "tokens": ["Und", "war\u00b7te", ",", "wenn", "des", "Le\u00b7bens", "Band"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sol abgeschnitten werden,", "tokens": ["Sol", "ab\u00b7ge\u00b7schnit\u00b7ten", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Da\u00df mein Gebein im h\u00f6lzern Schrein", "tokens": ["Da\u00df", "mein", "Ge\u00b7bein", "im", "h\u00f6l\u00b7zern", "Schrein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Werd' hingebracht zur Erden.", "tokens": ["Werd'", "hin\u00b7ge\u00b7bracht", "zur", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "In deinen Willen geb' ich mich,", "tokens": ["In", "dei\u00b7nen", "Wil\u00b7len", "geb'", "ich", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Herr Jesu, hilf mir gn\u00e4diglich", "tokens": ["Herr", "Je\u00b7su", ",", "hilf", "mir", "gn\u00e4\u00b7di\u00b7glich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NE", "$,", "VVIMP", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Dies St\u00fcndlein \u00fcberwinden;", "tokens": ["Dies", "St\u00fcnd\u00b7lein", "\u00fc\u00b7berw\u00b7in\u00b7den", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Bei dir ist Rat, bei dir ist That,", "tokens": ["Bei", "dir", "ist", "Rat", ",", "bei", "dir", "ist", "That", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "NN", "$,", "APPR", "PPER", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Bei dir ist Trost zu finden.", "tokens": ["Bei", "dir", "ist", "Trost", "zu", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Herr, gib mir doch zu dieser Frist,", "tokens": ["Herr", ",", "gib", "mir", "doch", "zu", "die\u00b7ser", "Frist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Was meiner Seelen n\u00fctzlich ist", "tokens": ["Was", "mei\u00b7ner", "See\u00b7len", "n\u00fctz\u00b7lich", "ist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPOSAT", "NN", "ADJD", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Zum Leben oder Sterben;", "tokens": ["Zum", "Le\u00b7ben", "o\u00b7der", "Ster\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Sol leben ich, so la\u00df du mich", "tokens": ["Sol", "le\u00b7ben", "ich", ",", "so", "la\u00df", "du", "mich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "In S\u00fcnden nicht verderben.", "tokens": ["In", "S\u00fcn\u00b7den", "nicht", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Und sol ich denn von hinnen gehn,", "tokens": ["Und", "sol", "ich", "denn", "von", "hin\u00b7nen", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "APPR", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "In jennem Leben dich zu sehn,", "tokens": ["In", "jen\u00b7nem", "Le\u00b7ben", "dich", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Daselbst dir Lob zu singen,", "tokens": ["Da\u00b7selbst", "dir", "Lob", "zu", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Bin ich bereit, aus dieser Zeit", "tokens": ["Bin", "ich", "be\u00b7reit", ",", "aus", "die\u00b7ser", "Zeit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Mich in dein Reich zu schwingen.", "tokens": ["Mich", "in", "dein", "Reich", "zu", "schwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Zwar diesen Leib und die\u00df Gebein", "tokens": ["Zwar", "die\u00b7sen", "Leib", "und", "die\u00df", "Ge\u00b7bein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PDAT", "NN", "KON", "PDS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "La\u00df ich der W\u00fcrmer Speise sein,", "tokens": ["La\u00df", "ich", "der", "W\u00fcr\u00b7mer", "Spei\u00b7se", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Die\u00df ist der Lohn der S\u00fcnden;", "tokens": ["Die\u00df", "ist", "der", "Lohn", "der", "S\u00fcn\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.29": {"text": "Ich aber wei\u00df, im Paradeis", "tokens": ["Ich", "a\u00b7ber", "wei\u00df", ",", "im", "Pa\u00b7ra\u00b7deis"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Werd' ich was Bessers finden.", "tokens": ["Werd'", "ich", "was", "Bes\u00b7sers", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIS", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "So bitt' ich nun von Herzen Grund:", "tokens": ["So", "bitt'", "ich", "nun", "von", "Her\u00b7zen", "Grund", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "La\u00df meine Seel' in dieser Stund',", "tokens": ["La\u00df", "mei\u00b7ne", "Seel'", "in", "die\u00b7ser", "Stund'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "O Herr, dir sein befohlen;", "tokens": ["O", "Herr", ",", "dir", "sein", "be\u00b7foh\u00b7len", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Von dir allein, mein Jesulein,", "tokens": ["Von", "dir", "al\u00b7lein", ",", "mein", "Je\u00b7su\u00b7lein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Kan ich Erquickung holen.", "tokens": ["Kan", "ich", "Er\u00b7quic\u00b7kung", "ho\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.36": {"text": "Besch\u00fctze mich mit deinem Schild',", "tokens": ["Be\u00b7sch\u00fct\u00b7ze", "mich", "mit", "dei\u00b7nem", "Schild'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Indem der Satan, frech und wild,", "tokens": ["In\u00b7dem", "der", "Sa\u00b7tan", ",", "frech", "und", "wild", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Mich Armen wil erschrecken;", "tokens": ["Mich", "Ar\u00b7men", "wil", "er\u00b7schre\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.39": {"text": "Sonst wei\u00df ich nicht, o du mein Licht,", "tokens": ["Sonst", "wei\u00df", "ich", "nicht", ",", "o", "du", "mein", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "$,", "FM", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Womit ich sol mich decken.", "tokens": ["Wo\u00b7mit", "ich", "sol", "mich", "de\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.41": {"text": "Hier ist kein Rat, denn Menschengunst", "tokens": ["Hier", "ist", "kein", "Rat", ",", "denn", "Men\u00b7schen\u00b7gunst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "$,", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Samt aller H\u00fclf' ist gar \u00fcmsunst;", "tokens": ["Samt", "al\u00b7ler", "H\u00fcl\u00b7f'", "ist", "gar", "\u00fcm\u00b7sunst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "VAFIN", "ADV", "ADV", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.43": {"text": "Wer hilft doch denn mir Armen?", "tokens": ["Wer", "hilft", "doch", "denn", "mir", "Ar\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "KON", "PPER", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.44": {"text": "Wer h\u00e4lt mich nun? Gott mu\u00df es thun,", "tokens": ["Wer", "h\u00e4lt", "mich", "nun", "?", "Gott", "mu\u00df", "es", "thun", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$.", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Bei dem ist viel Erbarmen.", "tokens": ["Bei", "dem", "ist", "viel", "Er\u00b7bar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.46": {"text": "Ich darf nicht kommen f\u00fcr Gericht,", "tokens": ["Ich", "darf", "nicht", "kom\u00b7men", "f\u00fcr", "Ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Denn meine Werke taugen nicht,", "tokens": ["Denn", "mei\u00b7ne", "Wer\u00b7ke", "tau\u00b7gen", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Ich kan mich gar nichts r\u00fchmen:", "tokens": ["Ich", "kan", "mich", "gar", "nichts", "r\u00fch\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.49": {"text": "Was ich gethan, steht auf dem Plan,", "tokens": ["Was", "ich", "ge\u00b7than", ",", "steht", "auf", "dem", "Plan", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Es l\u00e4\u00dft sich nicht verbl\u00fcmen.", "tokens": ["Es", "l\u00e4\u00dft", "sich", "nicht", "ver\u00b7bl\u00fc\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.51": {"text": "Wo sol ich denn nun fliehen hin,", "tokens": ["Wo", "sol", "ich", "denn", "nun", "flie\u00b7hen", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.52": {"text": "Der ich ein solcher S\u00fcnder bin?", "tokens": ["Der", "ich", "ein", "sol\u00b7cher", "S\u00fcn\u00b7der", "bin", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.53": {"text": "Allein zu deiner G\u00fcte,", "tokens": ["Al\u00b7lein", "zu", "dei\u00b7ner", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.54": {"text": "Herr Jesu Christ; ich wei\u00df, du bist", "tokens": ["Herr", "Je\u00b7su", "Christ", ";", "ich", "wei\u00df", ",", "du", "bist"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "NE", "NE", "$.", "PPER", "VVFIN", "$,", "PPER", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.55": {"text": "Mein Bruder von Gem\u00fcte.", "tokens": ["Mein", "Bru\u00b7der", "von", "Ge\u00b7m\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.56": {"text": "Dein herber Tod hat mich befreit,", "tokens": ["Dein", "her\u00b7ber", "Tod", "hat", "mich", "be\u00b7freit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.57": {"text": "Dein Blut gibt mir die Seligkeit