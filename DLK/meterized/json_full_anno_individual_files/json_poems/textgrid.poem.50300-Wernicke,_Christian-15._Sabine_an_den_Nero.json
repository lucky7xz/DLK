{"textgrid.poem.50300": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "15. Sabine an den Nero", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weil ", "tokens": ["Weil"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Und dich mit mir l\u00e4sst schalt- und walten;", "tokens": ["Und", "dich", "mit", "mir", "l\u00e4sst", "schal\u00b7t", "und", "wal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPER", "VVFIN", "TRUNC", "KON", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Weil du so gutes Muhts ihn fandst,", "tokens": ["Weil", "du", "so", "gu\u00b7tes", "Muhts", "ihn", "fandst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So thu, was du nicht lassen kanst.", "tokens": ["So", "thu", ",", "was", "du", "nicht", "las\u00b7sen", "kanst", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Glaub' aber, dass er, ungelogen,", "tokens": ["Glaub'", "a\u00b7ber", ",", "dass", "er", ",", "un\u00b7ge\u00b7lo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "KOUS", "PPER", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dich auf die Helfft' und mehr betrogen;", "tokens": ["Dich", "auf", "die", "Helfft'", "und", "mehr", "be\u00b7tro\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "KON", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und dass du ", "tokens": ["Und", "dass", "du"], "token_info": ["word", "word", "word"], "pos": ["KON", "KOUS", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Sonst bin ich wol des Dings gewohnet;", "tokens": ["Sonst", "bin", "ich", "wol", "des", "Dings", "ge\u00b7woh\u00b7net", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Mein ", "tokens": ["Mein"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "-", "measure": "single.down"}, "line.10": {"text": "Und weil er selbst dich drum anspricht,", "tokens": ["Und", "weil", "er", "selbst", "dich", "drum", "an\u00b7spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PRF", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "So bitt' ich dich, ", "tokens": ["So", "bitt'", "ich", "dich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Weil ", "tokens": ["Weil"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Und dich mit mir l\u00e4sst schalt- und walten;", "tokens": ["Und", "dich", "mit", "mir", "l\u00e4sst", "schal\u00b7t", "und", "wal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPER", "VVFIN", "TRUNC", "KON", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"