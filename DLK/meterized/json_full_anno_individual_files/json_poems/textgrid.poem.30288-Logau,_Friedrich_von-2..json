{"textgrid.poem.30288": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df wol, da\u00df man glaubt, da\u00df einer gerne thu", "tokens": ["Ich", "wei\u00df", "wol", ",", "da\u00df", "man", "glaubt", ",", "da\u00df", "ei\u00b7ner", "ger\u00b7ne", "thu"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PIS", "VVFIN", "$,", "KOUS", "PIS", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das, was er gerne sagt; allein es trifft nicht zu;", "tokens": ["Das", ",", "was", "er", "ger\u00b7ne", "sagt", ";", "al\u00b7lein", "es", "trifft", "nicht", "zu", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWS", "PPER", "ADV", "VVFIN", "$.", "ADV", "PPER", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Welt ist umgewand. Ich kenne manchen Mann:", "tokens": ["Die", "Welt", "ist", "um\u00b7ge\u00b7wand", ".", "Ich", "ken\u00b7ne", "man\u00b7chen", "Mann", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$.", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "An Worten ist er M\u00f6nch; an Thaten ist er Hahn.", "tokens": ["An", "Wor\u00b7ten", "ist", "er", "M\u00f6nch", ";", "an", "Tha\u00b7ten", "ist", "er", "Hahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["A