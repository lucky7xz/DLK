{"textgrid.poem.57552": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "3. In Greifswald", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das war ein kampfesfroher Tag,", "tokens": ["Das", "war", "ein", "kamp\u00b7fes\u00b7fro\u00b7her", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ger\u00f6tet ist des Saales Gediel,", "tokens": ["Ge\u00b7r\u00f6\u00b7tet", "ist", "des", "Saa\u00b7les", "Ge\u00b7diel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Rot spritzte es bei manchem Schlag,", "tokens": ["Rot", "spritz\u00b7te", "es", "bei", "man\u00b7chem", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der scharf und schwer herniederfiel,", "tokens": ["Der", "scharf", "und", "schwer", "her\u00b7nie\u00b7der\u00b7fiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So manche Wange, manche Stirn,", "tokens": ["So", "man\u00b7che", "Wan\u00b7ge", ",", "man\u00b7che", "Stirn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bis heute noch glatt und narbenleer,", "tokens": ["Bis", "heu\u00b7te", "noch", "glatt", "und", "nar\u00b7ben\u00b7leer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Sprang klaffend auf beim Gl\u00e4serklang.", "tokens": ["Sprang", "klaf\u00b7fend", "auf", "beim", "Gl\u00e4\u00b7ser\u00b7klang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Heut sind es gerad zehn Tage her,", "tokens": ["Heut", "sind", "es", "ge\u00b7rad", "zehn", "Ta\u00b7ge", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "CARD", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Als ich in ihrer Stube stand,", "tokens": ["Als", "ich", "in", "ih\u00b7rer", "Stu\u00b7be", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da gab sie mir die wei\u00dfe Hand", "tokens": ["Da", "gab", "sie", "mir", "die", "wei\u00b7\u00dfe", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sprach die wenigen Worte nur:", "tokens": ["Und", "sprach", "die", "we\u00b7ni\u00b7gen", "Wor\u00b7te", "nur", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "PIAT", "NN", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "\u00bbviel Gl\u00fcck im Leben, Herr Studiosus,", "tokens": ["\u00bb", "viel", "Gl\u00fcck", "im", "Le\u00b7ben", ",", "Herr", "Stu\u00b7di\u00b7o\u00b7sus", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PIAT", "NN", "APPRART", "NN", "$,", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Im Dichten und auf der Mensur!\u00ab", "tokens": ["Im", "Dich\u00b7ten", "und", "auf", "der", "Men\u00b7sur", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "KON", "APPR", "ART", "NN", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Erinnrung, kennst du keine Gnade!", "tokens": ["E\u00b7rinn\u00b7rung", ",", "kennst", "du", "kei\u00b7ne", "Gna\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Selbst bei dem frohen Burschenstrau\u00df", "tokens": ["Selbst", "bei", "dem", "fro\u00b7hen", "Bur\u00b7schen\u00b7strau\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00dcbst du, erbarmungslose Herrin,", "tokens": ["\u00dcbst", "du", ",", "er\u00b7bar\u00b7mungs\u00b7lo\u00b7se", "Her\u00b7rin", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Herrschaft unerbittlich aus!", "tokens": ["Die", "Herr\u00b7schaft", "un\u00b7er\u00b7bitt\u00b7lich", "aus", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich h\u00f6re es beim Schl\u00e4gerklange,", "tokens": ["Ich", "h\u00f6\u00b7re", "es", "beim", "Schl\u00e4\u00b7ger\u00b7