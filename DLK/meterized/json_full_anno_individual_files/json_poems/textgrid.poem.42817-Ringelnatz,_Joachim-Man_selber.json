{"textgrid.poem.42817": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Man selber", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn wir \u00fcber uns selber springen,", "tokens": ["Wenn", "wir", "\u00fc\u00b7ber", "uns", "sel\u00b7ber", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ADV", "VVINF", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Werden uns alle Pl\u00e4ne gelingen. \u2013", "tokens": ["Wer\u00b7den", "uns", "al\u00b7le", "Pl\u00e4\u00b7ne", "ge\u00b7lin\u00b7gen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Hopla! \u2013 Das werfe ich nur so hin,", "tokens": ["Ho\u00b7pla", "!", "\u2013", "Das", "wer\u00b7fe", "ich", "nur", "so", "hin", ","], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "$(", "PDS", "VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wei\u00df ich doch gar nicht, wer ich bin.", "tokens": ["Wei\u00df", "ich", "doch", "gar", "nicht", ",", "wer", "ich", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PTKNEG", "$,", "PWS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Man sollte rechtzeitig den Mut haben,", "tokens": ["Man", "soll\u00b7te", "recht\u00b7zei\u00b7tig", "den", "Mut", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADJD", "ART", "NN", "VAFIN", "$,"], "meter": "-+--+--+--", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Selbst zu beginnen, sich selbst zu begraben.", "tokens": ["Selbst", "zu", "be\u00b7gin\u00b7nen", ",", "sich", "selbst", "zu", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "$,", "PRF", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Diese und \u00e4hnliche (f\u00fcr die Jungen", "tokens": ["Die\u00b7se", "und", "\u00e4hn\u00b7li\u00b7che", "(", "f\u00fcr", "die", "Jun\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDAT", "KON", "ADJA", "$(", "APPR", "ART", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Ganz unwichtigen) Besch\u00e4ftigungen", "tokens": ["Ganz", "un\u00b7wich\u00b7ti\u00b7gen", ")", "Be\u00b7sch\u00e4f\u00b7ti\u00b7gun\u00b7gen"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADV", "ADJD", "$(", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Sind mir noch nie so ganz ehrlich gelungen.", "tokens": ["Sind", "mir", "noch", "nie", "so", "ganz", "ehr\u00b7lich", "ge\u00b7lun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "Und doch sind wir selber das Wichtigste", "tokens": ["Und", "doch", "sind", "wir", "sel\u00b7ber", "das", "Wich\u00b7tigs\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und die Mitmenschen das Nichtigste.", "tokens": ["Und", "die", "Mit\u00b7men\u00b7schen", "das", "Nich\u00b7tigs\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Man wird \u00fcber solchen Bekenntnissen", "tokens": ["Man", "wird", "\u00fc\u00b7ber", "sol\u00b7chen", "Be\u00b7kennt\u00b7nis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+--++-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Leicht in die Philosophie gerissen.", "tokens": ["Leicht", "in", "die", "Phi\u00b7lo\u00b7so\u00b7phie", "ge\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und dann sollte man umdrehen,", "tokens": ["Und", "dann", "soll\u00b7te", "man", "um\u00b7dre\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Irgendwo anders hingehen,", "tokens": ["Ir\u00b7gend\u00b7wo", "an\u00b7ders", "hin\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "Spiegel zertr\u00fcmmern und sich umsehen.", "tokens": ["Spie\u00b7gel", "zer\u00b7tr\u00fcm\u00b7mern", "und", "sich", "um\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "KON", "PRF", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Wenn wir \u00fcber uns selber springen,", "tokens": ["Wenn", "wir", "\u00fc\u00b7ber", "uns", "sel\u00b7ber", "sprin\u00b7gen", ","], "token_info":