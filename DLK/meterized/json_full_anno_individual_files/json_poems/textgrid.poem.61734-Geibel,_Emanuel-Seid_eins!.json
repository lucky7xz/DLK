{"textgrid.poem.61734": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Seid eins!", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie lang noch eifers\u00fccht'gen Mutes", "tokens": ["Wie", "lang", "noch", "ei\u00b7fer\u00b7s\u00fccht'\u00b7gen", "Mu\u00b7tes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verzehrt ihr euch in Streit und Neid?", "tokens": ["Ver\u00b7zehrt", "ihr", "euch", "in", "Streit", "und", "Neid", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr Volksgeschlechter deutschen Blutes,", "tokens": ["Ihr", "Volks\u00b7ge\u00b7schlech\u00b7ter", "deut\u00b7schen", "Blu\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Besinnt euch endlich, wer ihr seid!", "tokens": ["Be\u00b7sinnt", "euch", "end\u00b7lich", ",", "wer", "ihr", "seid", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PWS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Schon donnert's \u00fcberm Eidergrunde,", "tokens": ["Schon", "don\u00b7nert's", "\u00fc\u00b7berm", "Ei\u00b7der\u00b7grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schon w\u00f6lkt sich's am Gestad' des Rheins;", "tokens": ["Schon", "w\u00f6lkt", "sich's", "am", "Ge\u00b7stad'", "des", "Rheins", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPRART", "NN", "ART", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es rinnt der Sand der elften Stunde,", "tokens": ["Es", "rinnt", "der", "Sand", "der", "elf\u00b7ten", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und jedes Sandkorn mahnt: Seid eins!", "tokens": ["Und", "je\u00b7des", "Sand\u00b7korn", "mahnt", ":", "Seid", "eins", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$.", "VAIMP", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Seid eins! Von Gau zu Gau verk\u00fcndigt", "tokens": ["Seid", "eins", "!", "Von", "Gau", "zu", "Gau", "ver\u00b7k\u00fcn\u00b7digt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAIMP", "PIS", "$.", "APPR", "NN", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Fest der S\u00fchnung insgemein!", "tokens": ["Ein", "Fest", "der", "S\u00fch\u00b7nung", "ins\u00b7ge\u00b7mein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo all in gleicher Schuld ges\u00fcndigt,", "tokens": ["Wo", "all", "in", "glei\u00b7cher", "Schuld", "ge\u00b7s\u00fcn\u00b7digt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist's da so