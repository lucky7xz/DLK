{"textgrid.poem.64229": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Volk, ja du hast dir in jeglicher Kunst,", "tokens": ["Mein", "Volk", ",", "ja", "du", "hast", "dir", "in", "jeg\u00b7li\u00b7cher", "Kunst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "PPER", "VAFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "In jeglichem Wissen errungen den Preis:", "tokens": ["In", "jeg\u00b7li\u00b7chem", "Wis\u00b7sen", "er\u00b7run\u00b7gen", "den", "Preis", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Es g\u00f6nnte die Palme der Himmlischen Gunst", "tokens": ["Es", "g\u00f6nn\u00b7te", "die", "Pal\u00b7me", "der", "Himm\u00b7li\u00b7schen", "Gunst"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Der innigen Kraft und dem dauernden Flei\u00df:", "tokens": ["Der", "in\u00b7ni\u00b7gen", "Kraft", "und", "dem", "dau\u00b7ern\u00b7den", "Flei\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Du hast an dem Himmel die Sterne gez\u00e4hlet,", "tokens": ["Du", "hast", "an", "dem", "Him\u00b7mel", "die", "Ster\u00b7ne", "ge\u00b7z\u00e4h\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Hast tief in den Gr\u00fcnden durchforschet den Schacht,", "tokens": ["Hast", "tief", "in", "den", "Gr\u00fcn\u00b7den", "durch\u00b7for\u00b7schet", "den", "Schacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Hast Steine zu atmendem Leben beseelet,", "tokens": ["Hast", "Stei\u00b7ne", "zu", "at\u00b7men\u00b7dem", "Le\u00b7ben", "be\u00b7see\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.8": {"text": "Hast Lieder von ewiger Sch\u00f6nheit erdacht,", "tokens": ["Hast", "Lie\u00b7der", "von", "e\u00b7wi\u00b7ger", "Sch\u00f6n\u00b7heit", "er\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.9": {"text": "Du hast dir die Pforten des Geistes entriegelt,", "tokens": ["Du", "hast", "dir", "die", "Pfor\u00b7ten", "des", "Geis\u00b7tes", "ent\u00b7rie\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.10": {"text": "Die heiligsten Rollen des Ahnens entsiegelt: \u2013", "tokens": ["Die", "hei\u00b7ligs\u00b7ten", "Rol\u00b7len", "des", "Ah\u00b7nens", "ent\u00b7sie\u00b7gelt", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.11": {"text": "Leg alles dahin, sei zu anderm bereit,", "tokens": ["Leg", "al\u00b7les", "da\u00b7hin", ",", "sei", "zu", "an\u00b7derm", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "PAV", "$,", "VAFIN", "APPR", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.12": {"text": "Nach Eisen verlanget die eiserne Zeit:", "tokens": ["Nach", "Ei\u00b7sen", "ver\u00b7lan\u00b7get", "die", "ei\u00b7ser\u00b7ne", "Zeit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.13": {"text": "Zu den Waffen, mein Volk!", "tokens": ["Zu", "den", "Waf\u00b7fen", ",", "mein", "Volk", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Es hat die Olive kein Haupt noch gesch\u00fctzt,", "tokens": ["Es", "hat", "die", "O\u00b7li\u00b7ve", "kein", "Haupt", "noch", "ge\u00b7sch\u00fctzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PIAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Dem ruchlos das Schwert sich des Feindes genaht:", "tokens": ["Dem", "ruch\u00b7los", "das", "Schwert", "sich", "des", "Fein\u00b7des", "ge\u00b7naht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "PRF", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Hat Hellas die Liebe der Musen gen\u00fctzt,", "tokens": ["Hat", "Hel\u00b7las", "die", "Lie\u00b7be", "der", "Mu\u00b7sen", "ge\u00b7n\u00fctzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Als Rom mit dem Fu\u00df auf den Nacken ihm trat?", "tokens": ["Als", "Rom", "mit", "dem", "Fu\u00df", "auf", "den", "Na\u00b7cken", "ihm", "trat", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPR", "ART", "NN", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Vor\u00fcber die Tage f\u00fcr friedliches Trachten,", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "die", "Ta\u00b7ge", "f\u00fcr", "fried\u00b7li\u00b7ches", "Trach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "F\u00fcr Denken und Dichten vor\u00fcber die Zeit:", "tokens": ["F\u00fcr", "Den\u00b7ken", "und", "Dich\u00b7ten", "vor\u00b7\u00fc\u00b7ber", "die", "Zeit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Jetzt sollst du dich g\u00fcrten zu br\u00fcllenden Schlachten,", "tokens": ["Jetzt", "sollst", "du", "dich", "g\u00fcr\u00b7ten", "zu", "br\u00fcl\u00b7len\u00b7den", "Schlach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF", "PTKZU", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.8": {"text": "F\u00fcr Freiheit und Leben zum grimmigen Streit:", "tokens": ["F\u00fcr", "Frei\u00b7heit", "und", "Le\u00b7ben", "zum", "grim\u00b7mi\u00b7gen", "Streit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.9": {"text": "Fort Becher und Liebe, du freudige Jugend,", "tokens": ["Fort", "Be\u00b7cher", "und", "Lie\u00b7be", ",", "du", "freu\u00b7di\u00b7ge", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "KON", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.10": {"text": "Jetzt ist der Ha\u00df die oberste Tugend:", "tokens": ["Jetzt", "ist", "der", "Ha\u00df", "die", "o\u00b7bers\u00b7te", "Tu\u00b7gend", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Ihr f\u00fchrtet den Griffel, den Mei\u00dfel genug,", "tokens": ["Ihr", "f\u00fchr\u00b7tet", "den", "Grif\u00b7fel", ",", "den", "Mei\u00b7\u00dfel", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "ADV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.12": {"text": "Legt nieder die Feder, den Hammer, den Pflug:", "tokens": ["Legt", "nie\u00b7der", "die", "Fe\u00b7der", ",", "den", "Ham\u00b7mer", ",", "den", "Pflug", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.13": {"te