{"dta.poem.12006": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "26.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gott gebe dir an dir ein stilles Wohlgefallen,", "tokens": ["Gott", "ge\u00b7be", "dir", "an", "dir", "ein", "stil\u00b7les", "Wohl\u00b7ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Ein innig freudiges in seiner Gnade Wallen.", "tokens": ["Ein", "in\u00b7nig", "freu\u00b7di\u00b7ges", "in", "sei\u00b7ner", "Gna\u00b7de", "Wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein heiliges Gef\u00fchl, da\u00df du ihm angeh\u00f6rest,", "tokens": ["Ein", "hei\u00b7li\u00b7ges", "Ge\u00b7f\u00fchl", ",", "da\u00df", "du", "ihm", "an\u00b7ge\u00b7h\u00f6\u00b7rest", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und seine Ordnungen die ewigen nicht st\u00f6rest.", "tokens": ["Und", "sei\u00b7ne", "Ord\u00b7nun\u00b7gen", "die", "e\u00b7wi\u00b7gen", "nicht", "st\u00f6\u00b7rest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "ADJA", "PTKNEG", "VVFIN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Ein hebendes Gef\u00fchl, da\u00df du auf rechten Wegen", "tokens": ["Ein", "he\u00b7ben\u00b7des", "Ge\u00b7f\u00fchl", ",", "da\u00df", "du", "auf", "rech\u00b7ten", "We\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit rechten Kr\u00e4ften strebst dem rechten Ziel entgegen.", "tokens": ["Mit", "rech\u00b7ten", "Kr\u00e4f\u00b7ten", "strebst", "dem", "rech\u00b7ten", "Ziel", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nicht Selbgef\u00e4lligkeit, sich andern \u00fcberhebend,", "tokens": ["Nicht", "Selb\u00b7ge\u00b7f\u00e4l\u00b7lig\u00b7keit", ",", "sich", "an\u00b7dern", "\u00fc\u00b7ber\u00b7he\u00b7bend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "PRF", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht Ungeselligkeit, in enger Dumpfheit strebend.", "tokens": ["Nicht", "Un\u00b7ge\u00b7sel\u00b7lig\u00b7keit", ",", "in", "en\u00b7ger", "Dum\u00b7pf\u00b7heit", "stre\u00b7bend", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-++-+-", "measure": "unknown.measure.septa"}}, "stanza.5": {"line.1": {"text": "Doch Selbgen\u00fcgsamkeit in deiner eignen Weise,", "tokens": ["Doch", "Selb\u00b7ge\u00b7n\u00fcg\u00b7sam\u00b7keit", "in", "dei\u00b7ner", "eig\u00b7nen", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Seelenf\u00fcgsamkeit in deinem Schicksalskreise.", "tokens": ["Und", "See\u00b7len\u00b7f\u00fcg\u00b7sam\u00b7keit", "in", "dei\u00b7nem", "Schick\u00b7sals\u00b7krei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Und Selbzufriedenheit, mit aller Welt in Frieden,", "tokens": ["Und", "Selb\u00b7zu\u00b7frie\u00b7den\u00b7heit", ",", "mit", "al\u00b7ler", "Welt", "in", "Frie\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "APPR", "PIAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weltabgeschiedenheit, von Gott nur ungeschieden.", "tokens": ["Welt\u00b7ab\u00b7ge\u00b7schie\u00b7den\u00b7heit", ",", "von", "Gott", "nur", "un\u00b7ge\u00b7schie\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}