{"dta.poem.3606": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Eine heilige Familie .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Tag war sch\u00f6n, ins Gr\u00fcne gehn,               ", "tokens": ["Der", "Tag", "war", "sch\u00f6n", ",", "ins", "Gr\u00fc\u00b7ne", "gehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Trieb an das lust'ge Wetter;", "tokens": ["Trieb", "an", "das", "lust'\u00b7ge", "Wet\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Feld geziert, vom Wind ber\u00fchrt,", "tokens": ["Das", "Feld", "ge\u00b7ziert", ",", "vom", "Wind", "be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Roth wie die Rosenbl\u00e4tter.", "tokens": ["Roth", "wie", "die", "Ro\u00b7sen\u00b7bl\u00e4t\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Maria rein, h\u00e4tt Sorg allein,", "tokens": ["Ma\u00b7ria", "rein", ",", "h\u00e4tt", "Sorg", "al\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "VAFIN", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihr Kindlein umzutragen;", "tokens": ["Ihr", "Kin\u00b7dlein", "um\u00b7zu\u00b7tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "M\u00f6cht ja von Haus, aus wohl hinaus,", "tokens": ["M\u00f6cht", "ja", "von", "Haus", ",", "aus", "wohl", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "NN", "$,", "APPR", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Soll doch die Mutter fragen:", "tokens": ["Soll", "doch", "die", "Mut\u00b7ter", "fra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Ob sie dies d\u00fcrfe wagen?", "tokens": ["Ob", "sie", "dies", "d\u00fcr\u00b7fe", "wa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}