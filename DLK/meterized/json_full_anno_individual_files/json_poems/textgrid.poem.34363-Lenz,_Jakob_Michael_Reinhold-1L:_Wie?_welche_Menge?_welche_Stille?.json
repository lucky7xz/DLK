{"textgrid.poem.34363": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie? welche Menge? welche Stille?", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie? welche Menge? welche Stille?", "tokens": ["Wie", "?", "wel\u00b7che", "Men\u00b7ge", "?", "wel\u00b7che", "Stil\u00b7le", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "$.", "PWAT", "NN", "$.", "PWAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als w\u00e4rens Geister. Welche Grille", "tokens": ["Als", "w\u00e4\u00b7rens", "Geis\u00b7ter", ".", "Wel\u00b7che", "Gril\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "$.", "PWAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bezaubert diese tausend K\u00f6pfe?", "tokens": ["Be\u00b7zau\u00b7bert", "die\u00b7se", "tau\u00b7send", "K\u00f6p\u00b7fe", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "CARD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich?", "tokens": ["Ich", "?"], "token_info": ["word", "punct"], "pos": ["PPER", "$."], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Mein Hamlet? Mein St\u00fcck!", "tokens": ["Mein", "Ham\u00b7let", "?", "Mein", "St\u00fcck", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Welch ein unerwartetes Gl\u00fcck!", "tokens": ["Welch", "ein", "un\u00b7er\u00b7war\u00b7te\u00b7tes", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJA", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Hamlet vor mir!", "tokens": ["Ham\u00b7let", "vor", "mir", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Gott! \u2013 Schafft dein Schicksal", "tokens": ["Gott", "!", "\u2013", "Schafft", "dein", "Schick\u00b7sal"], "token_info": ["word", "punct", "punct", "word", "word", "word"], "pos": ["NN", "$.", "$(", "NN", "PPOSAT", "NN"], "meter": "++-+-", "measure": "iambic.di"}, "line.9": {"text": "Menschen nach? Realisirt", "tokens": ["Men\u00b7schen", "nach", "?", "Re\u00b7a\u00b7li\u00b7sirt"], "token_info": ["word", "word", "punct", "word"], "pos": ["NN", "PTKVZ", "$.", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Was ich in unverge\u00dflichen Stunden", "tokens": ["Was", "ich", "in", "un\u00b7ver\u00b7ge\u00df\u00b7li\u00b7chen", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Durchgezittert, durchempfunden", "tokens": ["Durch\u00b7ge\u00b7zit\u00b7tert", ",", "dur\u00b7ch\u00b7em\u00b7pfun\u00b7den"], "token_info": ["word", "punct", "word"], "pos": ["VVPP", "$,", "VVINF"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.12": {"text": "In meiner Seele aufgef\u00fchrt?", "tokens": ["In", "mei\u00b7ner", "See\u00b7le", "auf\u00b7ge\u00b7f\u00fchrt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "O welch Herablassen! deinem Affen", "tokens": ["O", "welch", "Her\u00b7ab\u00b7las\u00b7sen", "!", "dei\u00b7nem", "Af\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "PIAT", "NN", "$.", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "W\u00fcrdigst du Vater! nachzuerschaffen. \u2013", "tokens": ["W\u00fcr\u00b7digst", "du", "Va\u00b7ter", "!", "nach\u00b7zu\u00b7er\u00b7schaf\u00b7fen", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "NN", "$.", "VVIZU", "$.", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Meine Shakespears! Ihr schenkt mich mir wiederum,", "tokens": ["Mei\u00b7ne", "Sha\u00b7ke\u00b7spe\u00b7ars", "!", "Ihr", "schenkt", "mich", "mir", "wie\u00b7de\u00b7rum", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PPER", "VVFIN", "PPER", "PPER", "ADV", "$,"], "meter": "+-+-+--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Liebes, liebes Publikum.", "tokens": ["Lie\u00b7bes", ",", "lie\u00b7bes", "Pub\u00b7li\u00b7kum", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Guckt nur! bis ihr seht was ich sah", "tokens": ["Guckt", "nur", "!", "bis", "ihr", "seht", "was", "ich", "sah"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$.", "KOUS", "PPER", "VVFIN", "PWS", "PPER", "VVFIN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Als die Offenbarung mir geschah.", "tokens": ["Als", "die", "Of\u00b7fen\u00b7ba\u00b7rung", "mir", "ge\u00b7schah", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Bis Euer Puls so fliegt, euer Leeben erhitzt", "tokens": ["Bis", "Eu\u00b7er", "Puls", "so", "fliegt", ",", "eu\u00b7er", "Lee\u00b7ben", "er\u00b7hitzt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVFIN", "$,", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "So das Augenlied schwingt, bis euer Auge blitzt", "tokens": ["So", "das", "Au\u00b7gen\u00b7lied", "schwingt", ",", "bis", "eu\u00b7er", "Au\u00b7ge", "blitzt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Voll unaussprechlicher Verlangen", "tokens": ["Voll", "un\u00b7aus\u00b7sprech\u00b7li\u00b7cher", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die sich Luft machen auf den Wangen.", "tokens": ["Die", "sich", "Luft", "ma\u00b7chen", "auf", "den", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-++-+-+-", "measure": "unknown.measure.penta"}, "line.9": {"text": "O ihr alle Shakespears an diesem Abend, alle", "tokens": ["O", "ihr", "al\u00b7le", "Sha\u00b7ke\u00b7spe\u00b7ars", "an", "die\u00b7sem", "A\u00b7bend", ",", "al\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NE", "PPOSAT", "PIAT", "NN", "APPR", "PDAT", "NN", "$,", "PIAT"], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Meine Kinder! meine Wiederhalle!", "tokens": ["Mei\u00b7ne", "Kin\u00b7der", "!", "mei\u00b7ne", "Wie\u00b7der\u00b7hal\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Bleibt nur den Abend so \u2013 darnach la\u00df ich euch lo\u00df,", "tokens": ["Bleibt", "nur", "den", "A\u00b7bend", "so", "\u2013", "dar\u00b7nach", "la\u00df", "ich", "euch", "lo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADV", "$(", "PAV", "VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Darnach werdt ihr wieder gewaltig und gro\u00df,", "tokens": ["Dar\u00b7nach", "werdt", "ihr", "wie\u00b7der", "ge\u00b7wal\u00b7tig", "und", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.13": {"text": "Seht hinaus \u00fcber mich, k\u00f6nnt wieder mich schreyen", "tokens": ["Seht", "hin\u00b7aus", "\u00fc\u00b7ber", "mich", ",", "k\u00f6nnt", "wie\u00b7der", "mich", "schre\u00b7yen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "$,", "VVFIN", "ADV", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "K\u00f6nnt mir ins Angesicht speyen", "tokens": ["K\u00f6nnt", "mir", "ins", "An\u00b7ge\u00b7sicht", "spe\u00b7yen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Critik, Galle, Zorn,", "tokens": ["Cri\u00b7tik", ",", "Gal\u00b7le", ",", "Zorn", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.16": {"text": "K\u00f6nnt, mich zu h\u00f6hnen", "tokens": ["K\u00f6nnt", ",", "mich", "zu", "h\u00f6h\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.17": {"text": "Mich kr\u00f6nen", "tokens": ["Mich", "kr\u00f6\u00b7nen"], "token_info": ["word", "word"], "pos": ["PPER", "VVINF"], "meter": "-+-", "measure": "amphibrach.single"}, "line.18": {"text": "Mit Dorn,", "tokens": ["Mit", "Dorn", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.19": {"text": "K\u00f6nnt ihr armen Ehrgeitzigen", "tokens": ["K\u00f6nnt", "ihr", "ar\u00b7men", "Ehr\u00b7geit\u00b7zi\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Meinethalben mich kreutzigen:", "tokens": ["Mei\u00b7net\u00b7hal\u00b7ben", "mich", "kreut\u00b7zi\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Hatte mein Gott, dessen Erdenklo\u00df", "tokens": ["Hat\u00b7te", "mein", "Gott", ",", "des\u00b7sen", "Er\u00b7den\u00b7klo\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "PRELAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.22": {"text": "Ich nur bin doch kein be\u00dfer Loo\u00df,", "tokens": ["Ich", "nur", "bin", "doch", "kein", "be\u00b7\u00dfer", "Loo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.23": {"text": "Hat euch doch ewig seelig gemacht", "tokens": ["Hat", "euch", "doch", "e\u00b7wig", "see\u00b7lig", "ge\u00b7macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "ADJD", "VVPP"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.24": {"text": "Da ich euch nur um zwey Stunden gebracht.", "tokens": ["Da", "ich", "euch", "nur", "um", "zwey", "Stun\u00b7den", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "APPR", "CARD", "NN", "VVPP", "$."], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}}, "stanza.3": {"line.1": {"text": "Bleibt die zwey Stunden nur so \u2013 liebe Ichs", "tokens": ["Bleibt", "die", "zwey", "Stun\u00b7den", "nur", "so", "\u2013", "lie\u00b7be", "Ichs"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "CARD", "NN", "ADV", "ADV", "$(", "VVFIN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Liebe Shakespears! \u2013 Gott! wie beseeligt mich's", "tokens": ["Lie\u00b7be", "Sha\u00b7ke\u00b7spe\u00b7ars", "!", "\u2013", "Gott", "!", "wie", "be\u00b7see\u00b7ligt", "mich's"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "NE", "$.", "$(", "NN", "$.", "PWAV", "VVPP", "NE"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Dis ", "tokens": ["Dis"], "token_info": ["word"], "pos": ["PDS"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Menschen mich mitgetheilt zu haben.", "tokens": ["Men\u00b7schen", "mich", "mit\u00b7ge\u00b7theilt", "zu", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "PTKZU", "VAINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Diese zwey Stunden nur \u2013 genug! \u2013", "tokens": ["Die\u00b7se", "zwey", "Stun\u00b7den", "nur", "\u2013", "ge\u00b7nug", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["PDAT", "CARD", "NN", "ADV", "$