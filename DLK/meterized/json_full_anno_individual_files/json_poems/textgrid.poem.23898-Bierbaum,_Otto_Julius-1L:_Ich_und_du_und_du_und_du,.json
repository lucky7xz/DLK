{"textgrid.poem.23898": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich und du und du und du,", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich und du und du und du,", "tokens": ["Ich", "und", "du", "und", "du", "und", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPER", "KON", "PPER", "KON", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Zwei mal zwei ist viere,", "tokens": ["Zwei", "mal", "zwei", "ist", "vie\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADV", "CARD", "VAFIN", "PIS", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Tragen Kr\u00e4nze auf dem Kopf,", "tokens": ["Tra\u00b7gen", "Kr\u00e4n\u00b7ze", "auf", "dem", "Kopf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kr\u00e4nze aus Papiere;", "tokens": ["Kr\u00e4n\u00b7ze", "aus", "Pa\u00b7pie\u00b7re", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Rechts herum und links herum,", "tokens": ["Rechts", "he\u00b7rum", "und", "links", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "KON", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "R\u00f6ck' und Z\u00f6pfe fliegen,", "tokens": ["R\u00f6ck'", "und", "Z\u00f6p\u00b7fe", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "troch