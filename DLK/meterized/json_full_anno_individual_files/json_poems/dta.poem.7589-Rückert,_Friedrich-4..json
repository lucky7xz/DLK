{"dta.poem.7589": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Thu deine Schuldigkeit, und la\u00df dir nur nicht bangen,", "tokens": ["Thu", "dei\u00b7ne", "Schul\u00b7dig\u00b7keit", ",", "und", "la\u00df", "dir", "nur", "nicht", "ban\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "KON", "VVIMP", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du wirst zu seiner Zeit daf\u00fcr den Lohn empfangen.", "tokens": ["Du", "wirst", "zu", "sei\u00b7ner", "Zeit", "da\u00b7f\u00fcr", "den", "Lohn", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "PAV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nimm dir nicht selbst den Lohn; sonst wird es dir mit Rechte", "tokens": ["Nimm", "dir", "nicht", "selbst", "den", "Lohn", ";", "sonst", "wird", "es", "dir", "mit", "Rech\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PTKNEG", "ADV", "ART", "NN", "$.", "ADV", "VAFIN", "PPER", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So gehn wie dort es gieng dem eigenm\u00e4cht'gen Knechte,", "tokens": ["So", "gehn", "wie", "dort", "es", "gieng", "dem", "ei\u00b7gen\u00b7m\u00e4cht'\u00b7gen", "Knech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "KOKOM", "ADV", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der brot- und arbeitslos zum Hause war gekommen", "tokens": ["Der", "brot", "und", "ar\u00b7beits\u00b7los", "zum", "Hau\u00b7se", "war", "ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "TRUNC", "KON", "ADJD", "APPRART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des reichen Herrn, und ward von ihm in Dienst genommen.", "tokens": ["Des", "rei\u00b7chen", "Herrn", ",", "und", "ward", "von", "ihm", "in", "Dienst", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "VAFIN", "APPR", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "\u201edir geb' ich f\u00fcr dies Jahr Ackerger\u00e4th und Samen;", "tokens": ["\u201e", "dir", "geb'", "ich", "f\u00fcr", "dies", "Jahr", "A\u00b7cker\u00b7ge\u00b7r\u00e4\u00b7th", "und", "Sa\u00b7men", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "APPR", "PDS", "NN", "NN", "KON", "NN", "$."], "meter": "-+--+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das Landgut vor der Stadt bestell' in meinem Namen.", "tokens": ["Das", "Land\u00b7gut", "vor", "der", "Stadt", "be\u00b7stell'", "in", "mei\u00b7nem", "Na\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und bring im Herbste nur mir den Ertrag davon,", "tokens": ["Und", "bring", "im", "Herbs\u00b7te", "nur", "mir", "den", "Er\u00b7trag", "da\u00b7von", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ADV", "PPER", "ART", "NN", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So geb' ich dir alsdann auch den verdienten Lohn.\u201c", "tokens": ["So", "geb'", "ich", "dir", "als\u00b7dann", "auch", "den", "ver\u00b7dien\u00b7ten", "Lohn", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "ADV", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-----+-+", "measure": "unknown.measure.tetra"}}, "stanza.6": {"line.1": {"text": "Da zog der Knecht aufs Land und ackert' und bestellte,", "tokens": ["Da", "zog", "der", "Knecht", "aufs", "Land", "und", "a\u00b7ckert'", "und", "be\u00b7stell\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sah die Ernte bald, die reicher Segen schwellte.", "tokens": ["Und", "sah", "die", "Ern\u00b7te", "bald", ",", "die", "rei\u00b7cher", "Se\u00b7gen", "schwell\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "$,", "PRELS", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Und als er aus dem Halm die K\u00f6rner nun gebracht;", "tokens": ["Und", "als", "er", "aus", "dem", "Halm", "die", "K\u00f6r\u00b7ner", "nun", "ge\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Eh er dem Herrn sie bringt, hat er sich so bedacht:", "tokens": ["Eh", "er", "dem", "Herrn", "sie", "bringt", ",", "hat", "er", "sich", "so", "be\u00b7dacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "PRF", "ADV", "VVPP", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.8": {"line.1": {"text": "\u201ewer wei\u00df, ob mir der Herr den vollen Lohn wird geben?", "tokens": ["\u201e", "wer", "wei\u00df", ",", "ob", "mir", "der", "Herr", "den", "vol\u00b7len", "Lohn", "wird", "ge\u00b7ben", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "ART", "ADJA", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu meiner Sicherheit behalt' ich etwas eben.", "tokens": ["Zu", "mei\u00b7ner", "Si\u00b7cher\u00b7heit", "be\u00b7halt'", "ich", "et\u00b7was", "e\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Hier am verborgnen Ort will ich ein Theil bewahren,", "tokens": ["Hier", "am", "ver\u00b7borg\u00b7nen", "Ort", "will", "ich", "ein", "Theil", "be\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bis die Gesinnung dort des Herrn ich hab' erfahren.\u201c", "tokens": ["Bis", "die", "Ge\u00b7sin\u00b7nung", "dort", "des", "Herrn", "ich", "hab'", "er\u00b7fah\u00b7ren", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ART", "NN", "PPER", "VAFIN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Der K\u00f6rner einen Theil vergr\u00e4bt er in der Grube,", "tokens": ["Der", "K\u00f6r\u00b7ner", "ei\u00b7nen", "Theil", "ver\u00b7gr\u00e4bt", "er", "in", "der", "Gru\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und bringt den \u00fcbrigen Ertrag dem Herrn der Hube.", "tokens": ["Und", "bringt", "den", "\u00fcb\u00b7ri\u00b7gen", "Er\u00b7trag", "dem", "Herrn", "der", "Hu\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Der Herr ist mit der Frucht des Jahres wohl zufrieden,", "tokens": ["Der", "Herr", "ist", "mit", "der", "Frucht", "des", "Jah\u00b7res", "wohl", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und hat dem Knecht mehr als verdienten Lohn beschieden.", "tokens": ["Und", "hat", "dem", "Knecht", "mehr", "als", "ver\u00b7dien\u00b7ten", "Lohn", "be\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "PIAT", "KOKOM", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Der Knecht hat kaum den Muth ins Antlitz ihm zu schaun", "tokens": ["Der", "Knecht", "hat", "kaum", "den", "Muth", "ins", "Ant\u00b7litz", "ihm", "zu", "schaun"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "APPRART", "NN", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df er zum guten Herrn nicht hatte mehr Vertraun.", "tokens": ["Da\u00df", "er", "zum", "gu\u00b7ten", "Herrn", "nicht", "hat\u00b7te", "mehr", "Ver\u00b7traun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "PTKNEG", "VAFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Er geht besch\u00e4mt und will gleich das Vergrabne holen,", "tokens": ["Er", "geht", "be\u00b7sch\u00e4mt", "und", "will", "gleich", "das", "Ver\u00b7grab\u00b7ne", "ho\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "VMFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch am verborgnen Ort hat es ein Dieb gestolen.", "tokens": ["Doch", "am", "ver\u00b7borg\u00b7nen", "Ort", "hat", "es", "ein", "Dieb", "ge\u00b7sto\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.14": {"line.1": {"text": "Und ganz betroffen tritt er vor den Herren wieder;", "tokens": ["Und", "ganz", "be\u00b7trof\u00b7fen", "tritt", "er", "vor", "den", "Her\u00b7ren", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der fragt: warum schl\u00e4gst du vor mir die Augen nieder?", "tokens": ["Der", "fragt", ":", "wa\u00b7rum", "schl\u00e4gst", "du", "vor", "mir", "die", "Au\u00b7gen", "nie\u00b7der", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$.", "PWAV", "VVFIN", "PPER", "APPR", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.15": {"line.1": {"text": "Er spricht: Wie d\u00fcrft' ich je zu dir sie mehr erheben?", "tokens": ["Er", "spricht", ":", "Wie", "d\u00fcrft'", "ich", "je", "zu", "dir", "sie", "mehr", "er\u00b7he\u00b7ben", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PWAV", "VMFIN", "PPER", "ADV", "APPR", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du hast mir \u00fcber den verdienten Lohn gegeben.", "tokens": ["Du", "hast", "mir", "\u00fc\u00b7ber", "den", "ver\u00b7dien\u00b7ten", "Lohn", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.16": {"line.1": {"text": "Ich aber habe mir mein Theil vorweg genommen;", "tokens": ["Ich", "a\u00b7ber", "ha\u00b7be", "mir", "mein", "Theil", "vor\u00b7weg", "ge\u00b7nom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PPER", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nun ich es bringen wollt', ist es abhanden kommen.", "tokens": ["Nun", "ich", "es", "brin\u00b7gen", "wollt'", ",", "ist", "es", "ab\u00b7han\u00b7den", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PPER", "VVINF", "VMFIN", "$,", "VAFIN", "PPER", "VVINF", "VVINF", "$."], "meter": "-+-+-++-+--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.17": {"line.1": {"text": "Der Herr sprach: besser ist's wer seine Schuld bekennt;", "tokens": ["Der", "Herr", "sprach", ":", "bes\u00b7ser", "ist's", "wer", "sei\u00b7ne", "Schuld", "be\u00b7kennt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ADJD", "VAFIN", "PWS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch weil du sie begiengst, sind wir hinfort getrennt.", "tokens": ["Doch", "weil", "du", "sie", "be\u00b7giengst", ",", "sind", "wir", "hin\u00b7fort", "ge\u00b7trennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.18": {"line.1": {"text": "Behalte deinen Lohn, und ich will dir nicht fluchen;", "tokens": ["Be\u00b7hal\u00b7te", "dei\u00b7nen", "Lohn", ",", "und", "ich", "will", "dir", "nicht", "flu\u00b7chen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "KON", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch mu\u00dft du nun dein Brot bei andern Th\u00fcren suchen.", "tokens": ["Doch", "mu\u00dft", "du", "nun", "dein", "Brot", "bei", "an\u00b7dern", "Th\u00fc\u00b7ren", "su\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}