{"dta.poem.10382": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ix.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Auf Fl\u00fcgeln des Gesanges,", "tokens": ["Auf", "Fl\u00fc\u00b7geln", "des", "Ge\u00b7san\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Herzliebchen trag' ich dich fort,", "tokens": ["Herz\u00b7lieb\u00b7chen", "trag'", "ich", "dich", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PRF", "PTKVZ", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Fort nach den Fluren des Ganges,", "tokens": ["Fort", "nach", "den", "Flu\u00b7ren", "des", "Gan\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Dort wei\u00df ich den sch\u00f6nsten Ort.", "tokens": ["Dort", "wei\u00df", "ich", "den", "sch\u00f6ns\u00b7ten", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Dort liegt ein rothbl\u00fchender Garten", "tokens": ["Dort", "liegt", "ein", "roth\u00b7bl\u00fc\u00b7hen\u00b7der", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Im stillen Mondenschein;", "tokens": ["Im", "stil\u00b7len", "Mon\u00b7den\u00b7schein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Lotosblumen erwarten", "tokens": ["Die", "Lo\u00b7tos\u00b7blu\u00b7men", "er\u00b7war\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ihr trautes Schwesterlein.", "tokens": ["Ihr", "trau\u00b7tes", "Schwes\u00b7ter\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Veilchen kichern und kosen,", "tokens": ["Die", "Veil\u00b7chen", "ki\u00b7chern", "und", "ko\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und schau'n nach den Sternen empor;", "tokens": ["Und", "schau'n", "nach", "den", "Ster\u00b7nen", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Heimlich erz\u00e4hlen die Rosen", "tokens": ["Heim\u00b7lich", "er\u00b7z\u00e4h\u00b7len", "die", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Sich duftende M\u00e4hrchen in's Ohr.", "tokens": ["Sich", "duf\u00b7ten\u00b7de", "M\u00e4hr\u00b7chen", "in's", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Es h\u00fcpfen herbei und lauschen", "tokens": ["Es", "h\u00fcp\u00b7fen", "her\u00b7bei", "und", "lau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die frommen, klugen Gazell'n;", "tokens": ["Die", "from\u00b7men", ",", "klu\u00b7gen", "Ga\u00b7zell'n", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und in der Ferne rauschen", "tokens": ["Und", "in", "der", "Fer\u00b7ne", "rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des heiligen Stromes Well'n.", "tokens": ["Des", "hei\u00b7li\u00b7gen", "Stro\u00b7mes", "Well'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["ART", "ADJA", "NN", "PIAT", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Dort wollen wir niedersinken", "tokens": ["Dort", "wol\u00b7len", "wir", "nie\u00b7der\u00b7sin\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Unter dem Palmenbaum,", "tokens": ["Un\u00b7ter", "dem", "Pal\u00b7men\u00b7baum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und Liebe und Ruhe trinken,", "tokens": ["Und", "Lie\u00b7be", "und", "Ru\u00b7he", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und tr\u00e4umen seligen Traum.", "tokens": ["Und", "tr\u00e4u\u00b7men", "se\u00b7li\u00b7gen", "Traum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}