{"dta.poem.11107": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf den grossen William in Engelland.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Europa schleust dich nicht mit deinen thaten ein,", "tokens": ["Eu\u00b7ro\u00b7pa", "schleust", "dich", "nicht", "mit", "dei\u00b7nen", "tha\u00b7ten", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie solte Niederland dir nicht zu niedrig seyn?", "tokens": ["Wie", "sol\u00b7te", "Nie\u00b7der\u00b7land", "dir", "nicht", "zu", "nied\u00b7rig", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PIS", "PPER", "PTKNEG", "PTKA", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "F\u00e4hrst du noch weiter fort, du held von dreyen erden!", "tokens": ["F\u00e4hrst", "du", "noch", "wei\u00b7ter", "fort", ",", "du", "held", "von", "drey\u00b7en", "er\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$,", "PPER", "VVFIN", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So wird dein Engeland dir auch zu enge werden.", "tokens": ["So", "wird", "dein", "En\u00b7ge\u00b7land", "dir", "auch", "zu", "en\u00b7ge", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "PPER", "ADV"