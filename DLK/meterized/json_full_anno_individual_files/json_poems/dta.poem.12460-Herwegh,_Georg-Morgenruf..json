{"dta.poem.12460": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Morgenruf.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Lerche war's, nicht die Nachtigall,", "tokens": ["Die", "Ler\u00b7che", "wa\u00b7r's", ",", "nicht", "die", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die eben am Himmel geschlagen:", "tokens": ["Die", "e\u00b7ben", "am", "Him\u00b7mel", "ge\u00b7schla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Schon schwingt er sich auf, der Sonnenball,", "tokens": ["Schon", "schwingt", "er", "sich", "auf", ",", "der", "Son\u00b7nen\u00b7ball", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKVZ", "$,", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Vom Winde des Morgens getragen.", "tokens": ["Vom", "Win\u00b7de", "des", "Mor\u00b7gens", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADV", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Der Tag, der Tag ist erwacht!", "tokens": ["Der", "Tag", ",", "der", "Tag", "ist", "er\u00b7wacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Die Nacht,", "tokens": ["Die", "Nacht", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Die Nacht soll blutig verenden. \u2014", "tokens": ["Die", "Nacht", "soll", "blu\u00b7tig", "ver\u00b7en\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VMFIN", "ADJD", "VVINF", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Heraus, wer an's ewige Licht noch glaubt!", "tokens": ["He\u00b7raus", ",", "wer", "an's", "e\u00b7wi\u00b7ge", "Licht", "noch", "glaubt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWS", "APPRART", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Ihr Schl\u00e4fer, die Rosen der Liebe vom Haubt,", "tokens": ["Ihr", "Schl\u00e4\u00b7fer", ",", "die", "Ro\u00b7sen", "der", "Lie\u00b7be", "vom", "Haubt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.10": {"text": "Und ein flammendes Schwert um die Lenden!", "tokens": ["Und", "ein", "flam\u00b7men\u00b7des", "Schwert", "um", "die", "Len\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.2": {"line.1": {"text": "Die Lerche war's, nicht die Nachtigall:", "tokens": ["Die", "Ler\u00b7che", "wa\u00b7r's", ",", "nicht", "die", "Nach\u00b7ti\u00b7gall", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erhebt euch vom Schlummer der S\u00fcnden!", "tokens": ["Er\u00b7hebt", "euch", "vom", "Schlum\u00b7mer", "der", "S\u00fcn\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Schon wollen die Feuer sich \u00fcberall,", "tokens": ["Schon", "wol\u00b7len", "die", "Feu\u00b7er", "sich", "\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PRF", "ADV", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die heiligen Feuer entz\u00fcnden.", "tokens": ["Die", "hei\u00b7li\u00b7gen", "Feu\u00b7er", "ent\u00b7z\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Frisch auf und die Waffen gefeit!", "tokens": ["Frisch", "auf", "und", "die", "Waf\u00b7fen", "ge\u00b7feit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "KON", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Der Streit,", "tokens": ["Der", "Streit", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Der Gottesstreit soll beginnen.", "tokens": ["Der", "Got\u00b7tes\u00b7streit", "soll", "be\u00b7gin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Hinweg aus des Liebchens rosigem Arm", "tokens": ["Hin\u00b7weg", "aus", "des", "Lieb\u00b7chens", "ro\u00b7si\u00b7gem", "Arm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Und hinein in der Feinde gepanzerten Schwarm", "tokens": ["Und", "hin\u00b7ein", "in", "der", "Fein\u00b7de", "ge\u00b7pan\u00b7zer\u00b7ten", "Schwarm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.10": {"text": "Und auf fliegenden Rossen vonhinnen!", "tokens": ["Und", "auf", "flie\u00b7gen\u00b7den", "Ros\u00b7sen", "von\u00b7hin\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.3": {"line.1": {"text": "Die Lerche war's, nicht die Nachtigall:", "tokens": ["Die", "Ler\u00b7che", "wa\u00b7r's", ",", "nicht", "die", "Nach\u00b7ti\u00b7gall", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Kein K\u00fcssen gilt es und Kosen,", "tokens": ["Kein", "K\u00fcs\u00b7sen", "gilt", "es", "und", "Ko\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie singt von nahendem Donnerhall,", "tokens": ["Sie", "singt", "von", "na\u00b7hen\u00b7dem", "Don\u00b7ner\u00b7hall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie singt von des Schlachtfelds Rosen,", "tokens": ["Sie", "singt", "von", "des", "Schlacht\u00b7felds", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Den Rosen, damit in Todeslust", "tokens": ["Den", "Ro\u00b7sen", ",", "da\u00b7mit", "in", "To\u00b7des\u00b7lust"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "APPR", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die Brust,", "tokens": ["Die", "Brust", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Die Brust der Helden sich schm\u00fccket.", "tokens": ["Die", "Brust", "der", "Hel\u00b7den", "sich", "schm\u00fc\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Drum auf und wohlan: bis frei die Welt,", "tokens": ["Drum", "auf", "und", "wo\u00b7hlan", ":", "bis", "frei", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PTKVZ", "KON", "ADV", "$.", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Sei der Himmel ein einig Kriegergezelt", "tokens": ["Sei", "der", "Him\u00b7mel", "ein", "ei\u00b7nig", "Krie\u00b7ger\u00b7ge\u00b7zelt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ART", "ADJD", "NN"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.10": {"text": "Und der Dolch der Rache gez\u00fccket!", "tokens": ["Und", "der", "Dolch", "der", "Ra\u00b7che", "ge\u00b7z\u00fc\u00b7cket", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Die Lerche war's, nicht die Nachtigall:", "tokens": ["Die", "Ler\u00b7che", "wa\u00b7r's", ",", "nicht", "die", "Nach\u00b7ti\u00b7gall", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So la\u00df, o Jugend, dein Tr\u00e4umen!", "tokens": ["So", "la\u00df", ",", "o", "Ju\u00b7gend", ",", "dein", "Tr\u00e4u\u00b7men", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "FM", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und wie von den Bergen mit Jubelschall", "tokens": ["Und", "wie", "von", "den", "Ber\u00b7gen", "mit", "Ju\u00b7bel\u00b7schall"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "ART", "NN", "APPR", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die muthigen Wasser entsch\u00e4umen,", "tokens": ["Die", "mut\u00b7hi\u00b7gen", "Was\u00b7ser", "ent\u00b7sch\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Und wie sie jagen in's tiefste Thal", "tokens": ["Und", "wie", "sie", "ja\u00b7gen", "in's", "tiefs\u00b7te", "Thal"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Den Strahl,", "tokens": ["Den", "Strahl", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Den silbernen Strahl durch's Gel\u00e4nde:", "tokens": ["Den", "sil\u00b7ber\u00b7nen", "Strahl", "durch's", "Ge\u00b7l\u00e4n\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "So gieb ihr dein Blut, so gieb ihr dein Wort,", "tokens": ["So", "gieb", "ihr", "dein", "Blut", ",", "so", "gieb", "ihr", "dein", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PPOSAT", "NN", "$,", "ADV", "VVIMP", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Da\u00df die Erde nicht ganz und gar verdorrt,", "tokens": ["Da\u00df", "die", "Er\u00b7de", "nicht", "ganz", "und", "gar", "ver\u00b7dorrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "ADV", "KON", "ADV", "VVPP", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.10": {"text": "So gieb ihr dein Herz und die H\u00e4nde!", "tokens": ["So", "gieb", "ihr", "dein", "Herz", "und", "die", "H\u00e4n\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PPOSAT", "NN", "KON", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "Die Lerche war's, nicht die Nachtigall:", "tokens": ["Die", "Ler\u00b7che", "wa\u00b7r's", ",", "nicht", "die", "Nach\u00b7ti\u00b7gall", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die kecke Gespielin der Wolke", "tokens": ["Die", "ke\u00b7cke", "Ge\u00b7spie\u00b7lin", "der", "Wol\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Fliegt jauchzend hinter dem Sonnenball,", "tokens": ["Fliegt", "jauch\u00b7zend", "hin\u00b7ter", "dem", "Son\u00b7nen\u00b7ball", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hoch \u00fcber dem staunenden Volke;", "tokens": ["Hoch", "\u00fc\u00b7ber", "dem", "stau\u00b7nen\u00b7den", "Vol\u00b7ke", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Und unter dem Scheffel bleibt auch nicht", "tokens": ["Und", "un\u00b7ter", "dem", "Schef\u00b7fel", "bleibt", "auch", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ADV", "PTKNEG"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Das Licht,", "tokens": ["Das", "Licht", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Das Licht der Freiheit verborgen;", "tokens": ["Das", "Licht", "der", "Frei\u00b7heit", "ver\u00b7bor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Viel tausend Herzen sind angefacht,", "tokens": ["Viel", "tau\u00b7send", "Her\u00b7zen", "sind", "an\u00b7ge\u00b7facht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Und preiset die Liebe die Sterne der Nacht:", "tokens": ["Und", "prei\u00b7set", "die", "Lie\u00b7be", "die", "Ster\u00b7ne", "der", "Nacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.10": {"text": "Die V\u00f6lker, sie preisen den Morgen.", "tokens": ["Die", "V\u00f6l\u00b7ker", ",", "sie", "prei\u00b7sen", "den", "Mor\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}