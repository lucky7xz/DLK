VFIN", "PPER", "$,", "PPER", "PTKZU", "VVINF", "$,", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Doch mit Winseln in die B\u00fcsche", "tokens": ["Doch", "mit", "Win\u00b7seln", "in", "die", "B\u00fc\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Sprang das Untier und entrann,", "tokens": ["Sprang", "das", "Un\u00b7tier", "und", "ent\u00b7rann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Und umsonst, es aufzufinden,", "tokens": ["Und", "um\u00b7sonst", ",", "es", "auf\u00b7zu\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PPER", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Sp\u00fcrt' er durch den ganzen Wald.", "tokens": ["Sp\u00fcrt'", "er", "durch", "den", "gan\u00b7zen", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Aber als er drauf nach Hause", "tokens": ["A\u00b7ber", "als", "er", "drauf", "nach", "Hau\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PAV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Kam in sp\u00e4ter Mitternacht,", "tokens": ["Kam", "in", "sp\u00e4\u00b7ter", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Fand er dort in Blute schwimmend", "tokens": ["Fand", "er", "dort", "in", "Blu\u00b7te", "schwim\u00b7mend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Auf dem Lager sein Gemahl,", "tokens": ["Auf", "dem", "La\u00b7ger", "sein", "Ge\u00b7mahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Wie sie wimmernd aus der Seite", "tokens": ["Wie", "sie", "wim\u00b7mernd", "aus", "der", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Einen scharfen Pfeil sich wand.", "tokens": ["Ei\u00b7nen", "schar\u00b7fen", "Pfeil", "sich", "wand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Schmerzlich schrie sie auf zum Himmel,", "tokens": ["Schmerz\u00b7lich", "schrie", "sie", "auf", "zum", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Als sie den Geliebten sah,", "tokens": ["Als", "sie", "den", "Ge\u00b7lieb\u00b7ten", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.31": {"text": "Schaute dann, die Lippen regend,", "tokens": ["Schau\u00b7te", "dann", ",", "die", "Lip\u00b7pen", "re\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Kummervollen Blicks ihn an,", "tokens": ["Kum\u00b7mer\u00b7vol\u00b7len", "Blicks", "ihn", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Doch bevor sie reden konnte,", "tokens": ["Doch", "be\u00b7vor", "sie", "re\u00b7den", "konn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.34": {"text": "War ihr Herz im Tod erstarrt.", "tokens": ["War", "ihr", "Herz", "im", "Tod", "er\u00b7starrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "Bei der Leiche stand der Ritter", "tokens": ["Bei", "der", "Lei\u00b7che", "stand", "der", "Rit\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Von Entsetzen \u00fcbermannt,", "tokens": ["Von", "Ent\u00b7set\u00b7zen", "\u00fc\u00b7berm\u00b7annt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.37": {"text": "Denn den eignen Pfeil erkannt' er,", "tokens": ["Denn", "den", "eig\u00b7nen", "Pfeil", "er\u00b7kannt'", "er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Der die Brust der Gattin traf,", "tokens": ["Der", "die", "Brust", "der", "Gat\u00b7tin", "traf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.39": {"text": "Und zerrissen unterm Bette", "tokens": ["Und", "zer\u00b7ris\u00b7sen", "un\u00b7term", "Bet\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVPP", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Lag ein blutig Wolfsgewand.", "tokens": ["Lag", "ein", "blu\u00b7tig", "Wolfs\u00b7ge\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJD", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}