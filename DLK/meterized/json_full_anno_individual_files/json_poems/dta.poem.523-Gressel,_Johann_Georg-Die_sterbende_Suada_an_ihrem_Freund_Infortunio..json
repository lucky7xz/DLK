{"dta.poem.523": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Die sterbende  Suada  an ihrem Freund  \n  Infortunio.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Liebster/ dessen Stern mit Ungl\u00fcck stets gew\u00fcttet/", "tokens": ["Mein", "Liebs\u00b7ter", "/", "des\u00b7sen", "Stern", "mit", "Un\u00b7gl\u00fcck", "stets", "ge\u00b7w\u00fct\u00b7tet", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PRELAT", "NN", "APPR", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "H\u00f6rt diesen letzten Schall mit tapffern Hertzen an:", "tokens": ["H\u00f6rt", "die\u00b7sen", "letz\u00b7ten", "Schall", "mit", "tapf\u00b7fern", "Hert\u00b7zen", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PDAT", "ADJA", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die treue ", "tokens": ["Die", "treu\u00b7e"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Die Zunge kaum dis Wort mit R\u00f6cheln sagen kan.", "tokens": ["Die", "Zun\u00b7ge", "kaum", "dis", "Wort", "mit", "R\u00f6\u00b7cheln", "sa\u00b7gen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PDS", "NN", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich scheide/ und die Lust/ die wir vor dem empfunden/", "tokens": ["Ich", "schei\u00b7de", "/", "und", "die", "Lust", "/", "die", "wir", "vor", "dem", "emp\u00b7fun\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KON", "ART", "NN", "$(", "PRELS", "PPER", "APPR", "ART", "ADJA", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Die wird zu Wermuth-Safft und Galle herber Pein/", "tokens": ["Die", "wird", "zu", "Wer\u00b7muth\u00b7\u00b7S\u00b7afft", "und", "Gal\u00b7le", "her\u00b7ber", "Pein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "NN", "KON", "NN", "ADJA", "NN", "$("], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Doch bl\u00fcht die Hoffnung noch vor eure Liebes-Wunden/", "tokens": ["Doch", "bl\u00fcht", "die", "Hoff\u00b7nung", "noch", "vor", "eu\u00b7re", "Lie\u00b7bes\u00b7Wun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wenn euch ", "tokens": ["Wenn", "euch"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "+-", "measure": "trochaic.single"}}}}}