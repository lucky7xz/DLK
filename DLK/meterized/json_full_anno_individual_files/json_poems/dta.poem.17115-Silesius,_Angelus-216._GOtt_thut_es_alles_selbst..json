{"dta.poem.17115": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "216.  GOtt thut es alles selbst.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Gott ist nur alles gar; Er stimmt die Seiten an/", "tokens": ["Gott", "ist", "nur", "al\u00b7les", "gar", ";", "Er", "stimmt", "die", "Sei\u00b7ten", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "PIS", "ADV", "$.", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er singt und spilt in un\u00df: wie hast dann du\u2019 s gethan?", "tokens": ["Er", "singt", "und", "spilt", "in", "un\u00df", ":", "wie", "hast", "dann", "du'", "s", "ge\u00b7than", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "NE", "$.", "PWAV", "VAFIN", "ADV", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}}}}