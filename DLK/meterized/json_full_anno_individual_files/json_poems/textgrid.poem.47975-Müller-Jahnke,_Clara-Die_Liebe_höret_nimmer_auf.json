{"textgrid.poem.47975": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Die Liebe h\u00f6ret nimmer auf", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verklungene Lieder, verbla\u00dftes Blau, \u2013", "tokens": ["Ver\u00b7klun\u00b7ge\u00b7ne", "Lie\u00b7der", ",", "ver\u00b7bla\u00df\u00b7tes", "Blau", ",", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "wie k\u00fchl der Wind und die Welt wie grau!", "tokens": ["wie", "k\u00fchl", "der", "Wind", "und", "die", "Welt", "wie", "grau", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "KON", "ART", "NN", "KOKOM", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Die letzte Rose am Hag verbl\u00fcht,", "tokens": ["Die", "letz\u00b7te", "Ro\u00b7se", "am", "Hag", "ver\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "ein Tr\u00e4nenregen vom Himmel spr\u00fcht.", "tokens": ["ein", "Tr\u00e4\u00b7nen\u00b7re\u00b7gen", "vom", "Him\u00b7mel", "spr\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "So schal und dunkel des Jahres Rest \u2013", "tokens": ["So", "schal", "und", "dun\u00b7kel", "des", "Jah\u00b7res", "Rest", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "ART", "NN", "NN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "die Glocken l\u00e4uten zum Totenfest.", "tokens": ["die", "Glo\u00b7cken", "l\u00e4u\u00b7ten", "zum", "To\u00b7ten\u00b7fest", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Der Mund, der schmeichelnd dich einst gek\u00fc\u00dft,", "tokens": ["Der", "Mund", ",", "der", "schmei\u00b7chelnd", "dich", "einst", "ge\u00b7k\u00fc\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "ward kalt und stumm, nun du elend bist \u2013", "tokens": ["ward", "kalt", "und", "stumm", ",", "nun", "du", "e\u00b7lend", "bist", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "$,", "ADV", "PPER", "ADJD", "VAFIN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "der Arm, der sch\u00fctzend dein Haupt umschlang,", "tokens": ["der", "Arm", ",", "der", "sch\u00fct\u00b7zend", "dein", "Haupt", "um\u00b7schlang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "er ruht im Grabe und modert lang, \u2013", "tokens": ["er", "ruht", "im", "Gra\u00b7be", "und", "mo\u00b7dert", "lang", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "KON", "VVFIN", "ADJD", "$,", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "und das Aug', das l\u00e4chelnd das deine traf,", "tokens": ["und", "das", "Aug'", ",", "das", "l\u00e4\u00b7chelnd", "das", "dei\u00b7ne", "traf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ADJD", "ART", "PPOSAT", "VVFIN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "nun schl\u00e4ft es den tiefen, den ewigen Schlaf. \u2013", "tokens": ["nun", "schl\u00e4ft", "es", "den", "tie\u00b7fen", ",", "den", "e\u00b7wi\u00b7gen", "Schlaf", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.7": {"line.1": {"text": "Und was dich freute, und all, was dein,", "tokens": ["Und", "was", "dich", "freu\u00b7te", ",", "und", "all", ",", "was", "dein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "KON", "PIAT", "$,", "PRELS", "PPOSAT", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "das sollt f\u00fcr immer verloren sein?!", "tokens": ["das", "sollt", "f\u00fcr", "im\u00b7mer", "ver\u00b7lo\u00b7ren", "sein", "?!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPR", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Was irdisch, wurde der Erde Raub;", "tokens": ["Was", "ir\u00b7disch", ",", "wur\u00b7de", "der", "Er\u00b7de", "Raub", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "$,", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "bekr\u00e4nze den H\u00fcgel, \u2013 den Staub zum Staub.", "tokens": ["be\u00b7kr\u00e4n\u00b7ze", "den", "H\u00fc\u00b7gel", ",", "\u2013", "den", "Staub", "zum", "Staub", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "$(", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.9": {"line.1": {"text": "Dann aber den tr\u00e4nenden Blick hinauf:", "tokens": ["Dann", "a\u00b7ber", "den", "tr\u00e4\u00b7nen\u00b7den", "Blick", "hin\u00b7auf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "\u00bbdie Liebe, sie h\u00f6ret nimmer auf!\u00ab", "tokens": ["\u00bb", "die", "Lie\u00b7be", ",", "sie", "h\u00f6\u00b7ret", "nim\u00b7mer", "auf", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "$,", "PPER", "VVFIN", "ADV", "PTKVZ", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "Wer hei\u00df geliebt und wer hoch gestrebt,", "tokens": ["Wer", "hei\u00df", "ge\u00b7liebt", "und", "wer", "hoch", "ge\u00b7strebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVPP", "KON", "PWS", "ADJD", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "der ist nicht begraben und tot, der lebt \u2013", "tokens": ["der", "ist", "nicht", "be\u00b7gra\u00b7ben", "und", "tot", ",", "der", "lebt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "VAFIN", "PTKNEG", "VVPP", "KON", "ADJD", "$,", "PRELS", "VVFIN", "$("], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.11": {"line.1": {"text": "Das Samenkorn, das wir der Erde vertraut,", "tokens": ["Das", "Sa\u00b7men\u00b7korn", ",", "das", "wir", "der", "Er\u00b7de", "ver\u00b7traut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "wird keimen, sobald der Himmel blaut,", "tokens": ["wird", "kei\u00b7men", ",", "so\u00b7bald", "der", "Him\u00b7mel", "blaut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVINF", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.12": {"line.1": {"text": "Und das Auge, das heut in Schmerzen weint,", "tokens": ["Und", "das", "Au\u00b7ge", ",", "das", "heut", "in", "Schmer\u00b7zen", "weint", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "wird l\u00e4cheln, wenn wieder die Sonne scheint.", "tokens": ["wird", "l\u00e4\u00b7cheln", ",", "wenn", "wie\u00b7der", "die", "Son\u00b7ne", "scheint", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVFIN", "$,", "KOUS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+---+-+", "measure": "zehnsilber"}}, "stanza.13": {"line.1": {"text": "O Tag der Toten, du Tr\u00e4nentag:", "tokens": ["O", "Tag", "der", "To\u00b7ten", ",", "du", "Tr\u00e4\u00b7nen\u00b7tag", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,", "PPER", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie tr\u00fcb der Himmel auch scheinen mag,", "tokens": ["Wie", "tr\u00fcb", "der", "Him\u00b7mel", "auch", "schei\u00b7nen", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.14": {"line.1": {"text": "wie tief auch H\u00fcgel und Tal verschneit:", "tokens": ["wie", "tief", "auch", "H\u00fc\u00b7gel", "und", "Tal", "ver\u00b7schneit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich glaub an die kommende Fr\u00fchlingszeit, \u2013", "tokens": ["Ich", "glaub", "an", "die", "kom\u00b7men\u00b7de", "Fr\u00fch\u00b7lings\u00b7zeit", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.15": {"line.1": {"text": "ich schlage das Auge zum Licht hinauf", "tokens": ["ich", "schla\u00b7ge", "das", "Au\u00b7ge", "zum", "Licht", "hin\u00b7auf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "PTKVZ"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "und wei\u00df: Die Liebe h\u00f6rt nimmer auf!", "tokens": ["und", "wei\u00df", ":", "Die", "Lie\u00b7be", "h\u00f6rt", "nim\u00b7mer", "auf", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ART", "NN", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.rela