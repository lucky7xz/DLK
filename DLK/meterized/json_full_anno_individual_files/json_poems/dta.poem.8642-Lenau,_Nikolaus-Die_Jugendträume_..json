{"dta.poem.8642": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Die Jugendtr\u00e4ume .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Der J\u00fcngling weilt in einem Bl\u00fcthengarten,", "tokens": ["Der", "J\u00fcng\u00b7ling", "weilt", "in", "ei\u00b7nem", "Bl\u00fc\u00b7then\u00b7gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und schaut mit Lust des Lebens Morgenroth;", "tokens": ["Und", "schaut", "mit", "Lust", "des", "Le\u00b7bens", "Mor\u00b7gen\u00b7roth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auf seinem Antlitz ruht ein sch\u00f6n Erwarten,", "tokens": ["Auf", "sei\u00b7nem", "Ant\u00b7litz", "ruht", "ein", "sch\u00f6n", "Er\u00b7war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Welt ist Himmel ihm, der Mensch ein Gott.", "tokens": ["Die", "Welt", "ist", "Him\u00b7mel", "ihm", ",", "der", "Mensch", "ein", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "PPER", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein Morgenl\u00fcftchen streut ihm duft'ge Rosen", "tokens": ["Ein", "Mor\u00b7gen\u00b7l\u00fcft\u00b7chen", "streut", "ihm", "duft'\u00b7ge", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit leisem Finger in das Lockenhaar;", "tokens": ["Mit", "lei\u00b7sem", "Fin\u00b7ger", "in", "das", "Lo\u00b7cken\u00b7haar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sein Haupt umflattert mit vertrautem Kosen", "tokens": ["Sein", "Haupt", "um\u00b7flat\u00b7tert", "mit", "ver\u00b7trau\u00b7tem", "Ko\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein bunt Gev\u00f6gel singend wunderbar.", "tokens": ["Ein", "bunt", "Ge\u00b7v\u00f6\u00b7gel", "sin\u00b7gend", "wun\u00b7der\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Seyd stille, stille, da\u00df die fl\u00fccht'gen G\u00e4ste", "tokens": ["Seyd", "stil\u00b7le", ",", "stil\u00b7le", ",", "da\u00df", "die", "fl\u00fccht'\u00b7gen", "G\u00e4s\u00b7te"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VAIMP", "VVFIN", "$,", "VVFIN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr nicht dem J\u00fcnglinge verscheucht; denn wi\u00dft:", "tokens": ["Ihr", "nicht", "dem", "J\u00fcng\u00b7lin\u00b7ge", "ver\u00b7scheucht", ";", "denn", "wi\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ART", "NN", "VVPP", "$.", "KON", "VVFIN", "$."], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Jugendtr\u00e4ume sind es, wohl das Beste,", "tokens": ["Die", "Ju\u00b7gend\u00b7tr\u00e4u\u00b7me", "sind", "es", ",", "wohl", "das", "Bes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was ihm f\u00fcr diese Welt beschieden ist.", "tokens": ["Was", "ihm", "f\u00fcr", "die\u00b7se", "Welt", "be\u00b7schie\u00b7den", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PDAT", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch, weh! nun naht mit eisern schwerem Gange", "tokens": ["Doch", ",", "weh", "!", "nun", "naht", "mit", "ei\u00b7sern", "schwe\u00b7rem", "Gan\u00b7ge"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "PTKVZ", "$.", "ADV", "VVFIN", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Wirklichkeit, und fort auf ewig flieh'n", "tokens": ["Die", "Wirk\u00b7lich\u00b7keit", ",", "und", "fort", "auf", "e\u00b7wig", "flieh'n"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "PTKVZ", "APPR", "ADJD", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die V\u00f6gel, und dem J\u00fcngling wird so bange,", "tokens": ["Die", "V\u00f6\u00b7gel", ",", "und", "dem", "J\u00fcng\u00b7ling", "wird", "so", "ban\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da er sie weiter sieht, und weiter zieh'n.", "tokens": ["Da", "er", "sie", "wei\u00b7ter", "sieht", ",", "und", "wei\u00b7ter", "zieh'", "n."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "abbreviation"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVFIN", "$,", "KON", "ADV", "VVFIN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}