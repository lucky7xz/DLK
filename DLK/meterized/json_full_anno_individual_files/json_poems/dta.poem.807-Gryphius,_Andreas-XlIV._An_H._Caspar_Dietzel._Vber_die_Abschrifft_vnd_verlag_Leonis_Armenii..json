{"dta.poem.807": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XlIV.  \n An H. Caspar Dietzel.  \n Vber die Abschrifft vnd verlag  Leonis Armenii.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Der bochverha\u00dfte F\u00fcrst/ den zwar die grosse Stadt ", "tokens": ["Der", "boch\u00b7ver\u00b7ha\u00df\u00b7te", "F\u00fcrst", "/", "den", "zwar", "die", "gros\u00b7se", "Stadt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "De\u00df ", "tokens": ["De\u00df"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}}, "stanza.2": {"line.1": {"text": "Erd/ Himmel/ Freund vnd Feind/ der durch geh\u00e4uffte wund\u1ebd", "tokens": ["Erd", "/", "Him\u00b7mel", "/", "Freund", "vnd", "Feind", "/", "der", "durch", "ge\u00b7h\u00e4uff\u00b7te", "wun\u00b7d\u1ebd"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$(", "NN", "$(", "NN", "KON", "NN", "$(", "ART", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von diesem Thron gest\u00fcrtzt/ den er mit macht betratt/", "tokens": ["Von", "die\u00b7sem", "Thron", "ge\u00b7st\u00fcrtzt", "/", "den", "er", "mit", "macht", "be\u00b7tratt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVPP", "$(", "PRELS", "PPER", "APPR", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der F\u00fcrst der sterbend lehrt/ wie bald das schnelle Radt", "tokens": ["Der", "F\u00fcrst", "der", "ster\u00b7bend", "lehrt", "/", "wie", "bald", "das", "schnel\u00b7le", "Radt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJD", "VVFIN", "$(", "KOKOM", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "De\u00df Gl\u00fccks/ werd\u2019 vmbgekehrt/ hat/ nun er gantzverschwun-", "tokens": ["De\u00df", "Gl\u00fccks", "/", "werd'", "vmb\u00b7ge\u00b7kehrt", "/", "hat", "/", "nun", "er", "gantz\u00b7ver\u00b7schwun"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$(", "VAFIN", "VVPP", "$(", "VAFIN", "$(", "ADV", "PPER", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "den ", "tokens": ["den"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Durch zuthun meiner faust/ sein Leben wider funden/", "tokens": ["Durch", "zu\u00b7thun", "mei\u00b7ner", "faust", "/", "sein", "Le\u00b7ben", "wi\u00b7der", "fun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "PTKVZ", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df jhm der M\u00f6rder Schwerdt/ so schnell verk\u00fcrtzet", "tokens": ["Da\u00df", "jhm", "der", "M\u00f6r\u00b7der", "Schwerdt", "/", "so", "schnell", "ver\u00b7k\u00fcrt\u00b7zet"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "NE", "$(", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "hatt. ", "tokens": ["hatt", "."], "token_info": ["word", "punct"], "pos": ["VAFIN", "$."], "meter": "+", "measure": "single.up"}}, "stanza.4": {"line.1": {"text": "Ich jrr\u2019/ Er fiel vorhin al\u00df Bizantz jhn entleibet/", "tokens": ["Ich", "jrr'", "/", "Er", "fiel", "vor\u00b7hin", "al\u00df", "Bi\u00b7zantz", "jhn", "ent\u00b7lei\u00b7bet", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$(", "PPER", "VVFIN", "ADV", "KOUS", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er f\u00e4llt nun durch die Hand/ die seinen Todt beschreibet", "tokens": ["Er", "f\u00e4llt", "nun", "durch", "die", "Hand", "/", "die", "sei\u00b7nen", "Todt", "be\u00b7schrei\u00b7bet"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$(", "ART", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er f\u00e4llt nun/ weil man nicht mein schreiben/ lesen kan.", "tokens": ["Er", "f\u00e4llt", "nun", "/", "weil", "man", "nicht", "mein", "schrei\u00b7ben", "/", "le\u00b7sen", "kan", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$(", "KOUS", "PIS", "PTKNEG", "PPOSAT", "VVINF", "$(", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Doch der/ der dreymal starb/ Mu\u00df dreyfach durch Euch leb\u1ebd/", "tokens": ["Doch", "der", "/", "der", "drey\u00b7mal", "starb", "/", "Mu\u00df", "drey\u00b7fach", "durch", "Euch", "le\u00b7b\u1ebd", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$(", "ART", "ADV", "VVFIN", "$(", "VMFIN", "ADV", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Herr Dietzel dessen Hand/ vnd druck jhm dis kan geben/", "tokens": ["Herr", "Diet\u00b7zel", "des\u00b7sen", "Hand", "/", "vnd", "druck", "jhm", "dis", "kan", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ART", "NN", "$(", "KON", "VVIMP", "PPER", "PDS", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was Bizantz/ mein gedicht; vnd schrifft jhm abgewan.", "tokens": ["Was", "Bi\u00b7zantz", "/", "mein", "ge\u00b7dicht", ";", "vnd", "schrifft", "jhm", "ab\u00b7ge\u00b7wan", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$(", "PPOSAT", "VVPP", "$.", "KON", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}