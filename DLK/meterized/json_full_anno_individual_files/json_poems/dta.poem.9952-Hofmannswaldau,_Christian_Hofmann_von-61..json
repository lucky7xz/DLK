{"dta.poem.9952": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "61.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da soll ich am besten seyn/ wenn ich auf die welt erst kommen/", "tokens": ["Da", "soll", "ich", "am", "bes\u00b7ten", "seyn", "/", "wenn", "ich", "auf", "die", "welt", "erst", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKA", "ADJD", "VAINF", "$(", "KOUS", "PPER", "APPR", "ART", "NN", "ADV", "VVINF", "$("], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Meine g\u00fcte die wird mir durch das alter weggenommen/", "tokens": ["Mei\u00b7ne", "g\u00fc\u00b7te", "die", "wird", "mir", "durch", "das", "al\u00b7ter", "weg\u00b7ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "VAFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Komm von weibern ohne mann/ pfleg auch kinder zu geb\u00e4hren/", "tokens": ["Komm", "von", "wei\u00b7bern", "oh\u00b7ne", "mann", "/", "pfleg", "auch", "kin\u00b7der", "zu", "ge\u00b7b\u00e4h\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "VVFIN", "APPR", "NN", "$(", "NE", "ADV", "NN", "PTKZU", "VVINF", "$("], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "So mir andre ziehen auff/ weil ich sie nicht kan era\u00e4hren.", "tokens": ["So", "mir", "and\u00b7re", "zie\u00b7hen", "auff", "/", "weil", "ich", "sie", "nicht", "kan", "er\u00b7a\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PIS", "VVFIN", "APPR", "$(", "KOUS", "PPER", "PPER", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}