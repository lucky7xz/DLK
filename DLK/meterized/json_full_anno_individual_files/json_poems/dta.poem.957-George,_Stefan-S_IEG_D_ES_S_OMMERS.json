{"dta.poem.957": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "S IEG   D ES   S OMMERS", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Die reichsten sch&#228;tze lernet frei verschwenden               ", "tokens": ["Die", "reichs\u00b7ten", "sch", "&#228;", "tze", "ler\u00b7net", "frei", "ver\u00b7schwen\u00b7den"], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJD", "$(", "NN", "VVFIN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie nach den langen strahlen auf verdorrte               ", "tokens": ["Wie", "nach", "den", "lan\u00b7gen", "strah\u00b7len", "auf", "ver\u00b7dorr\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gew&#228;chse sollet ihr am frohen orte               ", "tokens": ["Gew", "&#228;", "chse", "sol\u00b7let", "ihr", "am", "fro\u00b7hen", "or\u00b7te"], "token_info": ["word", "XML_entity", "word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "VMFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Den heissen gliedern milden regen spenden             ", "tokens": ["Den", "heis\u00b7sen", "glie\u00b7dern", "mil\u00b7den", "re\u00b7gen", "spen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Gedenkt vom sch\u00f6nsten pfl&#252;ckend was hier sprosset               ", "tokens": ["Ge\u00b7denkt", "vom", "sch\u00f6ns\u00b7ten", "pfl", "&#252;", "ckend", "was", "hier", "spros\u00b7set"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "word", "word"], "pos": ["VVPP", "APPRART", "ADJA", "NE", "$(", "VVPP", "PRELS", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn s&#252;ss und schw&#252;l die d&#228;mmrungssterne blicken               ", "tokens": ["Wenn", "s", "&#252;", "ss", "und", "schw", "&#252;", "l", "die", "d", "&#228;", "mm\u00b7rungs\u00b7ster\u00b7ne", "bli\u00b7cken"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "XML_entity", "word", "word", "word", "XML_entity", "word", "word"], "pos": ["KOUS", "PPER", "$(", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$(", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.3": {"text": "Wenn gl&#252;hn und dunkeln wechselnd euch bestricken               ", "tokens": ["Wenn", "gl", "&#252;", "hn", "und", "dun\u00b7keln", "wech\u00b7selnd", "euch", "be\u00b7stri\u00b7cken"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "$(", "VVINF", "KON", "ADJA", "ADJD", "PPER", "VVINF"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Dass ihr soviel verliehen ist genosset             ", "tokens": ["Dass", "ihr", "so\u00b7viel", "ver\u00b7lie\u00b7hen", "ist", "ge\u00b7nos\u00b7set"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und th\u00f6rig nennt als &#252;bel zu befahren               ", "tokens": ["Und", "th\u00f6\u00b7rig", "nennt", "als", "&#252;", "bel", "zu", "be\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "KOKOM", "$(", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dass ihr in euch schon ferne bilder k&#252;sstet               ", "tokens": ["Dass", "ihr", "in", "euch", "schon", "fer\u00b7ne", "bil\u00b7der", "k", "&#252;", "sstet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ADV", "ADJA", "NN", "NE", "$(", "VVFIN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Und dass ihr niemals zu vers\u00f6hnen w&#252;sstet               ", "tokens": ["Und", "dass", "ihr", "nie\u00b7mals", "zu", "ver\u00b7s\u00f6h\u00b7nen", "w", "&#252;", "sstet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "PTKZU", "VVINF", "NE", "$(", "VVFIN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Den kuss im traum empfangen und den wahren.             ", "tokens": ["Den", "kuss", "im", "traum", "emp\u00b7fan\u00b7gen", "und", "den", "wah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVPP", "KON", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}