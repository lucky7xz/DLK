{"textgrid.poem.52488": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "8. T\u00e4ndelei", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich ward zum Turtelt\u00e4ubchen", "tokens": ["Ich", "ward", "zum", "Tur\u00b7tel\u00b7t\u00e4ub\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im allersch\u00f6nsten Traum;", "tokens": ["Im", "al\u00b7ler\u00b7sch\u00f6ns\u00b7ten", "Traum", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sa\u00df bei meinem Weibchen,", "tokens": ["Und", "sa\u00df", "bei", "mei\u00b7nem", "Weib\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf einem gr\u00fcnen Baum:", "tokens": ["Auf", "ei\u00b7nem", "gr\u00fc\u00b7nen", "Baum", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Zwar Liebchen that sehr spr\u00f6de", "tokens": ["Zwar", "Lieb\u00b7chen", "that", "sehr", "spr\u00f6\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VVFIN", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und schien vor mir zu fliehn;", "tokens": ["Und", "schien", "vor", "mir", "zu", "fliehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Doch ich war nicht so bl\u00f6de,", "tokens": ["Doch", "ich", "war", "nicht", "so", "bl\u00f6\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PTKNEG", "ADV", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Als ich sonst, wachend, bin.", "tokens": ["Als", "ich", "sonst", ",", "wa\u00b7chend", ",", "bin", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "VVPP", "$,", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich gurrte meine Klagen", "tokens": ["Ich", "gurr\u00b7te", "mei\u00b7ne", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und trippelt' um sie her;", "tokens": ["Und", "trip\u00b7pelt'", "um", "sie", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie nickt' und schien zu sagen:", "tokens": ["Sie", "nickt'", "und", "schien", "zu", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Begehre nur noch mehr!", "tokens": ["Be\u00b7geh\u00b7re", "nur", "noch", "mehr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wir flogen girrend beide", "tokens": ["Wir", "flo\u00b7gen", "gir\u00b7rend", "bei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PIS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Aufs n\u00e4chste Halmendach:", "tokens": ["Aufs", "n\u00e4chs\u00b7te", "Hal\u00b7men\u00b7dach", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sie duckte sich: \u2013 O Freude!", "tokens": ["Sie", "duck\u00b7te", "sich", ":", "\u2013", "O", "Freu\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$.", "$(", "NE", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und ich \u2013 schnell ward ich wach.", "tokens": ["Und", "ich", "\u2013", "schnell", "ward", "ich", "wach", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$(", "ADJD", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich ward zum Turtelt\u00e4ubchen", "tokens": ["Ich", "ward", "zum", "Tur\u00b7tel\u00b7t\u00e4ub\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im allersch\u00f6nsten Traum;", "tokens": ["Im", "al\u00b7ler\u00b7sch\u00f6ns\u00b7ten", "Traum", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sa\u00df bei meinem Weibchen,", "tokens": ["Und", "sa\u00df", "bei", "mei\u00b7nem", "Weib\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf einem gr\u00fcnen Baum:", "tokens": ["Auf", "ei\u00b7nem", "gr\u00fc\u00b7nen", "Baum", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5"