{"dta.poem.11622": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Das lustige Wirthshaus.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "\u2014 Mein Vater, der hatte drei Krebse zum Schild,", "tokens": ["Mein", "Va\u00b7ter", ",", "der", "hat\u00b7te", "drei", "Kreb\u00b7se", "zum", "Schild", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "PRELS", "VAFIN", "CARD", "NN", "APPRART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Da sprachen die Leute nicht ein:", "tokens": ["Da", "spra\u00b7chen", "die", "Leu\u00b7te", "nicht", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Nun f\u00fchrt er den scheckigen Narren im Bild,", "tokens": ["Nun", "f\u00fchrt", "er", "den", "sche\u00b7cki\u00b7gen", "Nar\u00b7ren", "im", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+---+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Er selber trinkt aber den Wein.", "tokens": ["Er", "sel\u00b7ber", "trinkt", "a\u00b7ber", "den", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+---+", "measure": "unknown.measure.tri"}}}}}