{"textgrid.poem.47632": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es stritten miteinander", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es stritten miteinander", "tokens": ["Es", "strit\u00b7ten", "mi\u00b7tein\u00b7an\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Denker und ein Derwisch,", "tokens": ["Ein", "Den\u00b7ker", "und", "ein", "Der\u00b7wisch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und schm\u00e4hlich unterlegen", "tokens": ["Und", "schm\u00e4h\u00b7lich", "un\u00b7ter\u00b7le\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist bald genug der Derwisch;", "tokens": ["Ist", "bald", "ge\u00b7nug", "der", "Der\u00b7wisch", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Denn mi\u00dflich ist das Streiten", "tokens": ["Denn", "mi\u00df\u00b7lich", "ist", "das", "Strei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit der Philosophie.", "tokens": ["Mit", "der", "Phi\u00b7lo\u00b7so\u00b7phie", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Allein es hat der Derwisch", "tokens": ["Al\u00b7lein", "es", "hat", "der", "Der\u00b7wisch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VAFIN", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Er ballte seine F\u00e4uste", "tokens": ["Er", "ball\u00b7te", "sei\u00b7ne", "F\u00e4us\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Und pr\u00fcgelte den Denker,", "tokens": ["Und", "pr\u00fc\u00b7gel\u00b7te", "den", "Den\u00b7ker", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Da\u00df er um Gnade schrie.", "tokens": ["Da\u00df", "er", "um", "Gna\u00b7de", "schrie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Denn so wird aller Orten,", "tokens": ["Denn", "so", "wird", "al\u00b7ler", "Or\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PIAT", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.12": {"text": "Wird stets der Glaube fertig", "tokens": ["Wird", "stets", "der", "Glau\u00b7be", "fer\u00b7tig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Mit der Philosophie.", "tokens": ["Mit", "der", "Phi\u00b7lo\u00b7so\u00b7phie", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Denker ging, dem Kadi vorzuklagen,", "tokens": ["Der", "Den\u00b7ker", "ging", ",", "dem", "Ka\u00b7di", "vor\u00b7zu\u00b7kla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word",