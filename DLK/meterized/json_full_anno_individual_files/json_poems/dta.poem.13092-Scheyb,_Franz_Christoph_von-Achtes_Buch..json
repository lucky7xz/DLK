{"dta.poem.13092": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Achtes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Mahlerin vernahm fast mit Beleidigung", "tokens": ["Die", "Mah\u00b7le\u00b7rin", "ver\u00b7nahm", "fast", "mit", "Be\u00b7lei\u00b7di\u00b7gung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den schnellen Gegensaz, den sie zwar in sich schlung;", "tokens": ["Den", "schnel\u00b7len", "Ge\u00b7gen\u00b7saz", ",", "den", "sie", "zwar", "in", "sich", "schlung", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "APPR", "PRF", "ADJD", "$."], "meter": "-+-+-+-+++-+", "measure": "unknown.measure.septa"}, "line.3": {"text": "Hier aber trat sie vor, den Streit zu unterbrechen:", "tokens": ["Hier", "a\u00b7ber", "trat", "sie", "vor", ",", "den", "Streit", "zu", "un\u00b7ter\u00b7bre\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eerlaubet \u201e, sagte sie, mir auch ein Wort zu sprechen!", "tokens": ["\u201e", "er\u00b7lau\u00b7bet", "\u201e", ",", "sag\u00b7te", "sie", ",", "mir", "auch", "ein", "Wort", "zu", "spre\u00b7chen", "!"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$(", "$,", "VVFIN", "PPER", "$,", "PPER", "ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201eder K\u00fcnste Ziel beruht auf Wahrheit und auf Lust.", "tokens": ["\u201e", "der", "K\u00fcns\u00b7te", "Ziel", "be\u00b7ruht", "auf", "Wahr\u00b7heit", "und", "auf", "Lust", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "250\u201dDie baut auf eine Flutt, der dieses nicht bewu\u00dft.", "tokens": ["\"", "Die", "baut", "auf", "ei\u00b7ne", "Flutt", ",", "der", "die\u00b7ses", "nicht", "be\u00b7wu\u00dft", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "PDS", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201ewie weit erreichet ihr durch S\u00e4ulen und durch Mauern;", "tokens": ["\u201e", "wie", "weit", "er\u00b7rei\u00b7chet", "ihr", "durch", "S\u00e4u\u00b7len", "und", "durch", "Mau\u00b7ern", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "ADJD", "VVFIN", "PPER", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201ewann auch dieselbigen die Felsen \u00fcberdauern;", "tokens": ["\u201e", "wann", "auch", "die\u00b7sel\u00b7bi\u00b7gen", "die", "Fel\u00b7sen", "\u00fc\u00b7berd\u00b7au\u00b7ern", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "ADV", "PDAT", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201ewie weit erreichet ihr durch eure Kunst den Saz?", "tokens": ["\u201e", "wie", "weit", "er\u00b7rei\u00b7chet", "ihr", "durch", "eu\u00b7re", "Kunst", "den", "Saz", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "ADJD", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201enicht weit. So gebet mir und meiner Arbeit Plaz.", "tokens": ["\u201e", "nicht", "weit", ".", "So", "ge\u00b7bet", "mir", "und", "mei\u00b7ner", "Ar\u00b7beit", "Plaz", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKNEG", "ADJD", "$.", "ADV", "VVFIN", "PPER", "KON", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "255\u201dIch weise durch die Farb und durch des Pensels Spize,", "tokens": ["\"", "Ich", "wei\u00b7se", "durch", "die", "Farb", "und", "durch", "des", "Pen\u00b7sels", "Spi\u00b7ze", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "ART", "NN", "KON", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201eda\u00df ich die F\u00e4higkeit zu diesem Werck besize.", "tokens": ["\u201e", "da\u00df", "ich", "die", "F\u00e4\u00b7hig\u00b7keit", "zu", "die\u00b7sem", "Werck", "be\u00b7si\u00b7ze", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ART", "NN", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "\u201edie Wahrheit ist mein Ziel; die Luft begleitet mich;", "tokens": ["\u201e", "die", "Wahr\u00b7heit", "ist", "mein", "Ziel", ";", "die", "Luft", "be\u00b7glei\u00b7tet", "mich", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$.", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "\u201eso schm\u00fccket ihre Kraft fast jeden Pensel-Strich.", "tokens": ["\u201e", "so", "schm\u00fc\u00b7cket", "ih\u00b7re", "Kraft", "fast", "je\u00b7den", "Pen\u00b7sel\u00b7Strich", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPOSAT", "NN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "\u201ewas heisset Erz und Stein? was wird darein gegraben?", "tokens": ["\u201e", "was", "heis\u00b7set", "Erz", "und", "Stein", "?", "was", "wird", "da\u00b7rein", "ge\u00b7gra\u00b7ben", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "NN", "KON", "NN", "$.", "PWS", "VAFIN", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "260\u201dWas vor Lebhaftigkeit kann beydes in sich haben?", "tokens": ["\"", "Was", "vor", "Leb\u00b7haf\u00b7tig\u00b7keit", "kann", "bey\u00b7des", "in", "sich", "ha\u00b7ben", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "APPR", "NN", "VMFIN", "PIS", "APPR", "PRF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "\u201ef\u00fc\u00df\u2019, H\u00e4nde, Kleidungen, ein starres Gips-Gesicht,", "tokens": ["\u201e", "f\u00fc\u00df'", ",", "H\u00e4n\u00b7de", ",", "Klei\u00b7dun\u00b7gen", ",", "ein", "star\u00b7res", "Gips\u00b7Ge\u00b7sicht", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "NN", "$,", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-++--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "\u201edie\u00df ist, was euer Stahl in stummen Marmel sticht.", "tokens": ["\u201e", "die\u00df", "ist", ",", "was", "eu\u00b7er", "Stahl", "in", "stum\u00b7men", "Mar\u00b7mel", "sticht", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "$,", "PRELS", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "\u201ebelohnet das die M\u00fch? hingegen nach zu \u00e4ffen,", "tokens": ["\u201e", "be\u00b7loh\u00b7net", "das", "die", "M\u00fch", "?", "hin\u00b7ge\u00b7gen", "nach", "zu", "\u00e4f\u00b7fen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ART", "ART", "NN", "$.", "ADV", "APPR", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "\u201ewas die Natur erzeugt, und es genau zu treffen,", "tokens": ["\u201e", "was", "die", "Na\u00b7tur", "er\u00b7zeugt", ",", "und", "es", "ge\u00b7nau", "zu", "tref\u00b7fen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "ART", "NN", "VVPP", "$,", "KON", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "265\u201dIst meiner Farben Werck. Luft, Wolcken, Berg' und Thal,", "tokens": ["\"", "Ist", "mei\u00b7ner", "Far\u00b7ben", "Werck", ".", "Luft", ",", "Wol\u00b7cken", ",", "Ber\u00b7g'", "und", "Thal", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPOSAT", "NN", "NN", "$.", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.22": {"text": "\u201egeschichten, Schlachten, Thier und Mensch ist, was ich mahl.", "tokens": ["\u201e", "ge\u00b7schich\u00b7ten", ",", "Schlach\u00b7ten", ",", "Thier", "und", "Mensch", "ist", ",", "was", "ich", "mahl", "."], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "$,", "NN", "$,", "NN", "KON", "NN", "VAFIN", "$,", "PWS", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "\u201eich kann durch meine Kunst, Leib, Ansehn, Geist und Leben,", "tokens": ["\u201e", "ich", "kann", "durch", "mei\u00b7ne", "Kunst", ",", "Leib", ",", "An\u00b7sehn", ",", "Geist", "und", "Le\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "APPR", "PPOSAT", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "\u201eauch dem, der in der Welt nicht mehr zu sehn ist, geben.", "tokens": ["\u201e", "auch", "dem", ",", "der", "in", "der", "Welt", "nicht", "mehr", "zu", "sehn", "ist", ",", "ge\u00b7ben", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ADV", "ART", "$,", "PRELS", "APPR", "ART", "NN", "PTKNEG", "ADV", "PTKZU", "VVINF", "VAFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "\u201ezorn, Unbestand und Angst, Furcht, Hoffnung, Rach und Lieb,", "tokens": ["\u201e", "zorn", ",", "Un\u00b7be\u00b7stand", "und", "Angst", ",", "Furcht", ",", "Hoff\u00b7nung", ",", "Rach", "und", "Lieb", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "NN", "$,", "NN", "KON", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "270\u201dJa was der Tugenden und Leidenschaften Trieb", "tokens": ["\"", "Ja", "was", "der", "Tu\u00b7gen\u00b7den", "und", "Lei\u00b7den\u00b7schaf\u00b7ten", "Trieb"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PTKANT", "PRELS", "ART", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "\u201ein sich verbergen mag, das kann der Pensel zeigen.", "tokens": ["\u201e", "in", "sich", "ver\u00b7ber\u00b7gen", "mag", ",", "das", "kann", "der", "Pen\u00b7sel", "zei\u00b7gen", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PRF", "VVINF", "VMFIN", "$,", "PDS", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "\u201eist solche T\u00fcchtigkeit auch euern Griffeln eigen?", "tokens": ["\u201e", "ist", "sol\u00b7che", "T\u00fcch\u00b7tig\u00b7keit", "auch", "eu\u00b7ern", "Grif\u00b7feln", "ei\u00b7gen", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PIAT", "NN", "ADV", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "\u201ewas lehrt uns eine Maur, was sagt uns ein Palast?", "tokens": ["\u201e", "was", "lehrt", "uns", "ei\u00b7ne", "Maur", ",", "was", "sagt", "uns", "ein", "Pa\u00b7last", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "ART", "NN", "$,", "PWS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "\u201ewas ist ein Marmel-Kopf den man in Lorber fa\u00dft?", "tokens": ["\u201e", "was", "ist", "ein", "Mar\u00b7mel\u00b7Kopf", "den", "man", "in", "Lor\u00b7ber", "fa\u00dft", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VAFIN", "ART", "NN", "ART", "PIS", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "275\u201dSo findet keine sich zu diesem Ende besser;", "tokens": ["\"", "So", "fin\u00b7det", "kei\u00b7ne", "sich", "zu", "die\u00b7sem", "En\u00b7de", "bes\u00b7ser", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PIAT", "PRF", "APPR", "PDAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "\u201evon allen K\u00fcnsten ist die Macht gewi\u00df nicht gr\u00f6sser,", "tokens": ["\u201e", "von", "al\u00b7len", "K\u00fcns\u00b7ten", "ist", "die", "Macht", "ge\u00b7wi\u00df", "nicht", "gr\u00f6s\u00b7ser", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PIAT", "NN", "VAFIN", "ART", "NN", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "\u201eals die der Meinigen; mithin ist es mir leicht,", "tokens": ["\u201e", "als", "die", "der", "Mei\u00b7ni\u00b7gen", ";", "mi\u00b7thin", "ist", "es", "mir", "leicht", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "ART", "ART", "PPOSS", "$.", "ADV", "VAFIN", "PPER", "PRF", "ADJD", "$,"], "meter": "-+--+--++--+", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "\u201eda\u00df ich dasjenige, was diesem F\u00fcrsten gleicht,", "tokens": ["\u201e", "da\u00df", "ich", "das\u00b7je\u00b7ni\u00b7ge", ",", "was", "die\u00b7sem", "F\u00fcrs\u00b7ten", "gleicht", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "PDS", "$,", "PWS", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "\u201eauch in die sp\u00e4tste Zeit der Nachwelt \u00fcberseze;", "tokens": ["\u201e", "auch", "in", "die", "sp\u00e4ts\u00b7te", "Zeit", "der", "Nach\u00b7welt", "\u00fc\u00b7ber\u00b7se\u00b7ze", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "APPR", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "280\u201dSonst aber Licht und Grau so viel als Marmel sch\u00e4ze.", "tokens": ["\"", "Sonst", "a\u00b7ber", "Licht", "und", "Grau", "so", "viel", "als", "Mar\u00b7mel", "sch\u00e4\u00b7ze", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "NN", "KON", "NN", "ADV", "ADV", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}