{"textgrid.poem.63588": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tragt mir die Schale fort mit Walderdbeeren!", "tokens": ["Tragt", "mir", "die", "Scha\u00b7le", "fort", "mit", "Wal\u00b7derd\u00b7bee\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So schmerzlich s\u00fc\u00dfe Bilder wecken sie,", "tokens": ["So", "schmerz\u00b7lich", "s\u00fc\u00b7\u00dfe", "Bil\u00b7der", "we\u00b7cken", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df ich der Tr\u00e4nen kaum mich kann erwehren.", "tokens": ["Da\u00df", "ich", "der", "Tr\u00e4\u00b7nen", "kaum", "mich", "kann", "er\u00b7weh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sa\u00df nicht beim Nachtisch stets auf meinem Knie", "tokens": ["Sa\u00df", "nicht", "beim", "Nach\u00b7tisch", "stets", "auf", "mei\u00b7nem", "Knie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "APPRART", "NN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das liebe Kind, mit ungeduld'ger Bitte,", "tokens": ["Das", "lie\u00b7be", "Kind", ",", "mit", "un\u00b7ge\u00b7duld'\u00b7ger", "Bit\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Bis ich der Schmeichlerin den Teller lieh?", "tokens": ["Bis", "ich", "der", "Schmeich\u00b7le\u00b7rin", "den", "Tel\u00b7ler", "lieh", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und dann mit spitzen Fingern aus der Mitte", "tokens": ["Und", "dann", "mit", "spit\u00b7zen", "Fin\u00b7gern", "aus", "der", "Mit\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die sch\u00f6nsten Beeren lesend, immer zwei", "tokens": ["Die", "sch\u00f6ns\u00b7ten", "Bee\u00b7ren", "le\u00b7send", ",", "im\u00b7mer", "zwei"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,", "ADV", "CARD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "F\u00fcr sich erw\u00e4hlte sie, f\u00fcr mich die dritte.", "tokens": ["F\u00fcr", "sich", "er\u00b7w\u00e4hl\u00b7te", "sie", ",", "f\u00fcr", "mich", "die", "drit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "VVFIN", "PPER", "$,", "APPR", "PPER", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Oft zweifelt' ich bei mir, was r\u00f6ter sei,", "tokens": ["Oft", "zwei\u00b7felt'", "ich", "bei", "mir", ",", "was", "r\u00f6\u00b7ter", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "$,", "PRELS", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Die Waldfrucht oder meines Kindes Lippen;", "tokens": ["Die", "Wald\u00b7frucht", "o\u00b7der", "mei\u00b7nes", "Kin\u00b7des", "Lip\u00b7pen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Was s\u00fc\u00dfer, wu\u00dft' ich wohl. Das ist vorbei.", "tokens": ["Was", "s\u00fc\u00b7\u00dfer", ",", "wu\u00dft'", "ich", "wohl", ".", "Das", "ist", "vor\u00b7bei", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "$,", "VVFIN", "PPER", "ADV", "$.", "PDS", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Nie wirst du mehr aus meinem Glase nippen,", "tokens": ["Nie", "wirst", "du", "mehr", "aus", "mei\u00b7nem", "Gla\u00b7se", "nip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Nie mehr von einem Teller mit mir naschen,", "tokens": ["Nie", "mehr", "von", "ei\u00b7nem", "Tel\u00b7ler", "mit", "mir", "na\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Nie mehr, Bachstelzchen, auf dem Scho\u00df mir wippen.", "tokens": ["Nie", "mehr", ",", "Bach\u00b7stelz\u00b7chen", ",", "auf", "dem", "Scho\u00df", "mir", "wip\u00b7pen", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "NN", "$,", "APPR", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.16": {"text": "Von meiner Zunge nicht hinwegzuwaschen", "tokens": ["Von", "mei\u00b7ner", "Zun\u00b7ge", "nicht", "hin\u00b7weg\u00b7zu\u00b7wa\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Ist dieser bittre Schmack. Die S\u00fc\u00dfigkeit", "tokens": ["Ist", "die\u00b7ser", "bitt\u00b7re", "Schmack", ".", "Die", "S\u00fc\u00b7\u00dfig\u00b7keit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PDAT", "ADJA", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Der Welt wird mir im Mund zu Salz und Aschen.", "tokens": ["Der", "Welt", "wird", "mir", "im", "Mund", "zu", "Salz", "und", "A\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPRART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Denn wenn ein Mahl begann in Fr\u00f6hlichkeit,", "tokens": ["Denn", "wenn", "ein", "Mahl", "be\u00b7gann", "in", "Fr\u00f6h\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Zum Nachtisch schleicht ein kleiner Gast ins Zimmer", "tokens": ["Zum", "Nach\u00b7tisch", "schleicht", "ein", "klei\u00b7ner", "Gast", "ins", "Zim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Und stell