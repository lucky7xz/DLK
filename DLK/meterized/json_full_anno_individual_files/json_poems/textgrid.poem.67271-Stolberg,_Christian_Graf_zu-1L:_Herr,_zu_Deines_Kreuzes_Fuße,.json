{"textgrid.poem.67271": {"metadata": {"author": {"name": "Stolberg, Christian Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Herr, zu Deines Kreuzes Fu\u00dfe,", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr, zu Deines Kreuzes Fu\u00dfe,", "tokens": ["Herr", ",", "zu", "Dei\u00b7nes", "Kreu\u00b7zes", "Fu\u00b7\u00dfe", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Leg' ich flehend meine Schuld,", "tokens": ["Leg'", "ich", "fle\u00b7hend", "mei\u00b7ne", "Schuld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wollst auf meiner Z\u00e4hren Bu\u00dfe,", "tokens": ["Wollst", "auf", "mei\u00b7ner", "Z\u00e4h\u00b7ren", "Bu\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schaun Vergebung, schaun Geduld,", "tokens": ["Schaun", "Ver\u00b7ge\u00b7bung", ",", "schaun", "Ge\u00b7duld", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "St\u00e4rken meiner Ohnmacht Willen,", "tokens": ["St\u00e4r\u00b7ken", "mei\u00b7ner", "Ohn\u00b7macht", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der nicht ohne Dich vermag", "tokens": ["Der", "nicht", "oh\u00b7ne", "Dich", "ver\u00b7mag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PTKNEG", "APPR", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Seinen Vorsatz zu erf\u00fcllen,", "tokens": ["Sei\u00b7nen", "Vor\u00b7satz", "zu", "er\u00b7f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Den er, ach, so oft schon brach!", "tokens": ["Den", "er", ",", "ach", ",", "so", "oft", "schon", "brach", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "ITJ", "$,", "ADV", "ADV", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Tief, o tief bin ich gesunken,", "tokens": ["Tief", ",", "o", "tief", "bin", "ich", "ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "FM", "ADJD", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der ich, w\u00e4hnend hoch zu stehn,", "tokens": ["Der", "ich", ",", "w\u00e4h\u00b7nend", "hoch", "zu", "stehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "KOUS", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Durft', aus D\u00fcnkels-Bechern trunken,", "tokens": ["Durft'", ",", "aus", "D\u00fcn\u00b7kels\u00b7Be\u00b7chern", "trun\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stolz hinab auf Be\u00dfre sehn!", "tokens": ["Stolz", "hin\u00b7ab", "auf", "Be\u00df\u00b7re", "sehn", "!"], "token_info": ["word", "word", "word", "word", "w