{"dta.poem.20164": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Der Lieb-erfreute  \n  FJLAMON .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1666", "urn": "urn:nbn:de:kobv:b4-20424-1", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch-Historischer Lustgarten. Frankfurt (Main), 1666."}, "poem": {"stanza.1": {"line.1": {"text": "Ach Leiden/ Jammer/ Noth! ach weh der gro\u00dfen Schmertzen?", "tokens": ["Ach", "Lei\u00b7den", "/", "Jam\u00b7mer", "/", "Noth", "!", "ach", "weh", "der", "gro\u00b7\u00dfen", "Schmert\u00b7zen", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$(", "NN", "$(", "NN", "$.", "ITJ", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ach weh der gro\u00dfen Pem! so ich f\u00fchl\u2019 in dem Hertzen/", "tokens": ["Ach", "weh", "der", "gro\u00b7\u00dfen", "Pem", "!", "so", "ich", "f\u00fchl'", "in", "dem", "Hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "ART", "ADJA", "NN", "$.", "ADV", "PPER", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Hilf mir doch wiederumb/ Cupido starker Gott", "tokens": ["Hilf", "mir", "doch", "wie\u00b7de\u00b7rumb", "/", "Cu\u00b7pi\u00b7do", "star\u00b7ker", "Gott"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PPER", "ADV", "ADV", "$(", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du aller st\u00e4rkster du/ ach rette mich aus Noht:", "tokens": ["Du", "al\u00b7ler", "st\u00e4rks\u00b7ter", "du", "/", "ach", "ret\u00b7te", "mich", "aus", "Noht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "PPER", "$(", "ADV", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich bin dir nuterthan/ dir hab ich mich befohlen/", "tokens": ["Ich", "bin", "dir", "nu\u00b7ter\u00b7than", "/", "dir", "hab", "ich", "mich", "be\u00b7foh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "$(", "PPER", "VAFIN", "PPER", "PRF", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Hilf mir nur aus der Pein. Ich sag\u2019 es urverholen/", "tokens": ["Hilf", "mir", "nur", "aus", "der", "Pein", ".", "Ich", "sag'", "es", "ur\u00b7ver\u00b7ho\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "APPR", "ART", "NN", "$.", "PPER", "VVFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}