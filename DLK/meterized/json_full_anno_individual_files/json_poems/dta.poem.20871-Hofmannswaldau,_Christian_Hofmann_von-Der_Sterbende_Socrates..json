{"dta.poem.20871": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Sterbende  \n  Socrates.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Wer solche Kost hat wol verdeuet/", "tokens": ["Wer", "sol\u00b7che", "Kost", "hat", "wol", "ver\u00b7deu\u00b7et", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "VAFIN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der bleibt/ wann die Natur uns dreuet/", "tokens": ["Der", "bleibt", "/", "wann", "die", "Na\u00b7tur", "uns", "dreu\u00b7et", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$(", "PWAV", "ART", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Von Furcht des Todes unber\u00fchrt.", "tokens": ["Von", "Furcht", "des", "To\u00b7des", "un\u00b7be\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denckt nicht vom Winde zu verderben/", "tokens": ["Denckt", "nicht", "vom", "Win\u00b7de", "zu", "ver\u00b7der\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPRART", "NN", "PTKZU", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Er wird durch keine Angst gef\u00fchrt/", "tokens": ["Er", "wird", "durch", "kei\u00b7ne", "Angst", "ge\u00b7f\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df seine Seele k\u00f6nte sterben.", "tokens": ["Da\u00df", "sei\u00b7ne", "See\u00b7le", "k\u00f6n\u00b7te", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}