{"dta.poem.15701": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "34.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zur Angelegenheit des Herzens m\u00fc\u00dft ihr machen", "tokens": ["Zur", "An\u00b7ge\u00b7le\u00b7gen\u00b7heit", "des", "Her\u00b7zens", "m\u00fc\u00dft", "ihr", "ma\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Glauben, und ihn nicht einmischen ird'schen Sachen.", "tokens": ["Den", "Glau\u00b7ben", ",", "und", "ihn", "nicht", "ein\u00b7mi\u00b7schen", "ird'\u00b7schen", "Sa\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "PPER", "PTKNEG", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Im weltlichen Verkehr mu\u00df euch zusammenhalten", "tokens": ["Im", "welt\u00b7li\u00b7chen", "Ver\u00b7kehr", "mu\u00df", "euch", "zu\u00b7sam\u00b7men\u00b7hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein andres Band; das la\u00dft vom Glauben nicht zerspalten!", "tokens": ["Ein", "and\u00b7res", "Band", ";", "das", "la\u00dft", "vom", "Glau\u00b7ben", "nicht", "zer\u00b7spal\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PDS", "VVFIN", "APPRART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sonst hat des Himmels Wort euch um der Erde Theil", "tokens": ["Sonst", "hat", "des", "Him\u00b7mels", "Wort", "euch", "um", "der", "Er\u00b7de", "Theil"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gebracht; und fehlt es hier, so ist auch dort kein Heil.", "tokens": ["Ge\u00b7bracht", ";", "und", "fehlt", "es", "hier", ",", "so", "ist", "auch", "dort", "kein", "Heil", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$.", "KON", "VVFIN", "PPER", "ADV", "$,", "ADV", "VAFIN", "ADV", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}