{"textgrid.poem.67704": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Das menschliche Leben", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit D\u00e4mmernacht bedeckt auf w\u00fcstem, weitem Meer,", "tokens": ["Mit", "D\u00e4m\u00b7mer\u00b7nacht", "be\u00b7deckt", "auf", "w\u00fcs\u00b7tem", ",", "wei\u00b7tem", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ohn' Leitstern und Magnet, zieht dort ein schwimmend Heer;", "tokens": ["Ohn'", "Leits\u00b7tern", "und", "Mag\u00b7net", ",", "zieht", "dort", "ein", "schwim\u00b7mend", "Heer", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "VVFIN", "ADV", "ART", "ADJD", "NN", "$."], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sie rudern; Jeden treibt Gefahr und Trieb zum Ruder,", "tokens": ["Sie", "ru\u00b7dern", ";", "Je\u00b7den", "treibt", "Ge\u00b7fahr", "und", "Trieb", "zum", "Ru\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PIAT", "VVFIN", "NN", "KON", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und Niemand wei\u00df, wohin, und Jeder fr\u00e4gt den Bruder:", "tokens": ["Und", "Nie\u00b7mand", "wei\u00df", ",", "wo\u00b7hin", ",", "und", "Je\u00b7der", "fr\u00e4gt", "den", "Bru\u00b7der", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$,", "PWAV", "$,", "KON", "PIS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u00bbwohin?\u00ab Sie sehen All', wie weit ", "tokens": ["\u00bb", "wo\u00b7hin", "?", "\u00ab", "Sie", "se\u00b7hen", "All'", ",", "wie", "weit"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "PWAV", "$.", "$(", "PPER", "VVFIN", "PIS", "$,", "PWAV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch wohin Well' und Schlag und Meer sie Alle tr\u00e4gt \u2013", "tokens": ["Doch", "wo\u00b7hin", "Well'", "und", "Schlag", "und", "Meer", "sie", "Al\u00b7le", "tr\u00e4gt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NN", "KON", "NN", "KON", "NN", "PPER", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da blinden, stammeln sie! und Alle streben weiter.", "tokens": ["Da", "blin\u00b7den", ",", "stam\u00b7meln", "sie", "!", "und", "Al\u00b7le", "stre\u00b7ben", "wei\u00b7ter", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "VVFIN", "PPER", "$.", "KON", "PIS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der \u2013 leiten will er nicht und will auch keinen Leiter,", "tokens": ["Der", "\u2013", "lei\u00b7ten", "will", "er", "nicht", "und", "will", "auch", "kei\u00b7nen", "Lei\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "VVINF", "VMFIN", "PPER", "PTKNEG", "KON", "VMFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und rudert! \u2013 Heere dort, in Haufen ziehen die,", "tokens": ["Und", "ru\u00b7dert", "!", "\u2013", "Hee\u00b7re", "dort", ",", "in", "Hau\u00b7fen", "zie\u00b7hen", "die", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "NN", "ADV", "$,", "APPR", "NN", "VVFIN", "ART", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und Schreier ziehn voran, und Alle rufen sie:", "tokens": ["Und", "Schrei\u00b7er", "ziehn", "vo\u00b7ran", ",", "und", "Al\u00b7le", "ru\u00b7fen", "sie", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PTKVZ", "$,", "KON", "PIS", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u00bbmir nach! mir nach allein!\u00ab und klopfen wild und flammen", "tokens": ["\u00bb", "mir", "nach", "!", "mir", "nach", "al\u00b7lein", "!", "\u00ab", "und", "klop\u00b7fen", "wild", "und", "flam\u00b7men"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "PTKVZ", "$.", "PPER", "APPR", "ADV", "$.", "$(", "KON", "VVFIN", "ADJD", "KON", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Anma\u00dfend wild empor; jetzt st\u00fcrmen die zusammen", "tokens": ["An\u00b7ma\u00b7\u00dfend", "wild", "em\u00b7por", ";", "jetzt", "st\u00fcr\u00b7men", "die", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "PTKVZ", "$.", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und zwingen, fluchen, drohn und qu\u00e4len mit sich fort.", "tokens": ["Und", "zwin\u00b7gen", ",", "flu\u00b7chen", ",", "drohn", "und", "qu\u00e4\u00b7len", "mit", "sich", "fort", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "VVINF", "$,", "VVFIN", "KON", "VVFIN", "APPR", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Das arme Mitleid folgt und lindert hie und dort", "tokens": ["Das", "ar\u00b7me", "Mit\u00b7leid", "folgt", "und", "lin\u00b7dert", "hie", "und", "dort"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "ADV", "KON", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und tr\u00f6stet, wo es kann; die Schiffe kreuzen, zagen.", "tokens": ["Und", "tr\u00f6s\u00b7tet", ",", "wo", "es", "kann", ";", "die", "Schif\u00b7fe", "kreu\u00b7zen", ",", "za\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PPER", "VMFIN", "$.", "ART", "NN", "VVINF", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wohin hat Strudel dies, wo das der Strom verschlagen?", "tokens": ["Wo\u00b7hin", "hat", "Stru\u00b7del", "dies", ",", "wo", "das", "der", "Strom", "ver\u00b7schla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "NN", "PDS", "$,", "PWAV", "ART", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Auf Klippen? in den Grund? Untiefen? Sand und Strand?", "tokens": ["Auf", "Klip\u00b7pen", "?", "in", "den", "Grund", "?", "Un\u00b7tie\u00b7fen", "?", "Sand", "und", "Strand", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "APPR", "ART", "NN", "$.", "NN", "$.", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ach, der sie schiffen hie\u00df! \u2013 Trifft eines, keines Land? \u2013", "tokens": ["Ach", ",", "der", "sie", "schif\u00b7fen", "hie\u00df", "!", "\u2013", "Trifft", "ei\u00b7nes", ",", "kei\u00b7nes", "Land", "?", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ITJ", "$,", "PRELS", "PPER", "VVINF", "VVFIN", "$.", "$(", "NN", "ART", "$,", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Sei ruhig! Alle tr\u00e4gt das milde Meer schon weiter,", "tokens": ["Sei", "ru\u00b7hig", "!", "Al\u00b7le", "tr\u00e4gt", "das", "mil\u00b7de", "Meer", "schon", "wei\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$.", "PIS", "VVFIN", "ART", "ADJA", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Der keine Leiter hat, und alle blinde Leiter!", "tokens": ["Der", "kei\u00b7ne", "Lei\u00b7ter", "hat", ",", "und", "al\u00b7le", "blin\u00b7de", "Lei\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "$,", "KON", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wer schnell und ruhig treibt, wer \u00e4chzt und liegt im Sand:", "tokens": ["Wer", "schnell", "und", "ru\u00b7hig", "treibt", ",", "wer", "\u00e4chzt", "und", "liegt", "im", "Sand", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "KON", "ADJD", "VVFIN", "$,", "PWS", "VVFIN", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Sieh! dort lacht Ufer schon, sie kommen All' an Land.", "tokens": ["Sieh", "!", "dort", "lacht", "U\u00b7fer", "schon", ",", "sie", "kom\u00b7men", "All'", "an", "Land", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVFIN", "NN", "ADV", "$,", "PPER", "VVFIN", "PIS", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mit D\u00e4mmernacht bedeckt auf w\u00fcstem, weitem Meer,", "tokens": ["Mit", "D\u00e4m\u00b7mer\u00b7nacht", "be\u00b7deckt", "auf", "w\u00fcs\u00b7tem", ",", "wei\u00b7tem", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ohn' Leitstern und Magnet, zieht dort ein schwimmend Heer;", "tokens": ["Ohn'", "Leits\u00b7tern", "und", "Mag\u00b7net", ",", "zieht", "dort", "ein", "schwim\u00b7mend", "Heer", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "VVFIN", "ADV", "ART", "ADJD", "NN", "$."], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sie rudern; Jeden treibt Gefahr und Trieb zum Ruder,", "tokens": ["Sie", "ru\u00b7dern", ";", "Je\u00b7den", "treibt", "Ge\u00b7fahr", "und", "Trieb", "zum", "Ru\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PIAT", "VVFIN", "NN", "KON", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure":