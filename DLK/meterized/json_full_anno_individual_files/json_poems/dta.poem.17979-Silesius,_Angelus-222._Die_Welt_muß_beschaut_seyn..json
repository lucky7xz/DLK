{"dta.poem.17979": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "222.  Die Welt mu\u00df beschaut seyn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Kehr hin dein Angesicht/ und schau die eitle Welt/", "tokens": ["Kehr", "hin", "dein", "An\u00b7ge\u00b7sicht", "/", "und", "schau", "die", "eit\u00b7le", "Welt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "$(", "KON", "ADJD", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer sie nicht recht betracht/ der wird f\u00fcrwahr gef\u00e4llt", "tokens": ["Wer", "sie", "nicht", "recht", "be\u00b7tracht", "/", "der", "wird", "f\u00fcr\u00b7wahr", "ge\u00b7f\u00e4llt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "PTKNEG", "ADJD", "VVPP", "$(", "ART", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}