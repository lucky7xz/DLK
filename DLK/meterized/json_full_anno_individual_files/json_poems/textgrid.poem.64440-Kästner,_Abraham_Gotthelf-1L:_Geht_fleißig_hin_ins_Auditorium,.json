{"textgrid.poem.64440": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Geht flei\u00dfig hin ins Auditorium,", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geht flei\u00dfig hin ins Auditorium,", "tokens": ["Geht", "flei\u00b7\u00dfig", "hin", "ins", "Au\u00b7di\u00b7to\u00b7ri\u00b7um", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da seht ihr manches St\u00fcck, wo M\u00e4nner nur agiren.", "tokens": ["Da", "seht", "ihr", "man\u00b7ches", "St\u00fcck", ",", "wo", "M\u00e4n\u00b7ner", "nur", "a\u00b7gi\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$,", "PWAV", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gelehrt hei\u00dft man es disputiren,", "tokens": ["Ge\u00b7lehrt", "hei\u00dft", "man", "es", "dis\u00b7pu\u00b7ti\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PIS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und gar ein Specimen ediren;", "tokens": ["Und", "gar", "ein", "Spe\u00b7ci\u00b7men", "e\u00b7di\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.5": {"text": "Die Rollen lesen sie, doch nein, sie buchstabiren,", "tokens": ["Die", "Rol\u00b7len", "le\u00b7sen", "sie", ",", "doch", "nein", ",", "sie", "buch\u00b7sta\u00b7bi\u00b7ren", ","], "token_info": ["word", "word", "word",