{"dta.poem.18650": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Moganni Nameh .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Zwanzig Jahre liess ich gehn", "tokens": ["Zwan\u00b7zig", "Jah\u00b7re", "liess", "ich", "gehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und genoss was mir beschieden;", "tokens": ["Und", "ge\u00b7noss", "was", "mir", "be\u00b7schie\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PWS", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eine Reihe v\u00f6llig sch\u00f6n", "tokens": ["Ei\u00b7ne", "Rei\u00b7he", "v\u00f6l\u00b7lig", "sch\u00f6n"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie die Zeit der Barmekiden.", "tokens": ["Wie", "die", "Zeit", "der", "Bar\u00b7me\u00b7ki\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}