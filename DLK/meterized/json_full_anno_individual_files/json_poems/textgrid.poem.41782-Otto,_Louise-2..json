{"textgrid.poem.41782": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du edles N\u00fcrnberg bist wie eine Blume", "tokens": ["Du", "ed\u00b7les", "N\u00fcrn\u00b7berg", "bist", "wie", "ei\u00b7ne", "Blu\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "VAFIN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im deutschen Reich, so herrlich anzusehn,", "tokens": ["Im", "deut\u00b7schen", "Reich", ",", "so", "herr\u00b7lich", "an\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "ADV", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du bl\u00fchst Dir selbst und aller Zeit zum Ruhme,", "tokens": ["Du", "bl\u00fchst", "Dir", "selbst", "und", "al\u00b7ler", "Zeit", "zum", "Ruh\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "KON", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "L\u00e4\u00dft Deine Werke durch die Lande gehn!", "tokens": ["L\u00e4\u00dft", "Dei\u00b7ne", "Wer\u00b7ke", "durch", "die", "Lan\u00b7de", "gehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und Deine Zauber walten fort und fort", "tokens": ["Und", "Dei\u00b7ne", "Zau\u00b7ber", "wal\u00b7ten", "fort", "und", "fort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PTKVZ", "KON", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In Kunst und Wissenschaft, in Bild und Wort.", "tokens": ["In", "Kunst", "und", "Wis\u00b7sen\u00b7schaft", ",", "in", "Bild", "und", "Wort", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dahin zog es von je die edlen Geister,", "tokens": ["Da\u00b7hin", "zog", "es", "von", "je", "die", "ed\u00b7len", "Geis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die gern sich laben an des Lebens Glanz,", "tokens": ["Die", "gern", "sich", "la\u00b7ben", "an", "des", "Le\u00b7bens", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PRF", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Herrn und F\u00fcrsten und die gro\u00dfen Meister", "tokens": ["Die", "Herrn", "und", "F\u00fcrs\u00b7ten", "und", "die", "gro\u00b7\u00dfen", "Meis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von jeder Kunst in sch\u00f6n gebundnem Kranz,", "tokens": ["Von", "je\u00b7der", "Kunst", "in", "sch\u00f6n", "ge\u00b7bund\u00b7nem", "Kranz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dahin den Fl\u00fcchtling auch, dem auf dem Flo\u00df", "tokens": ["Da\u00b7hin", "den", "Fl\u00fccht\u00b7ling", "auch", ",", "dem", "auf", "dem", "Flo\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "ADV", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Einst rheinhinab die Welt sich erst erschlo\u00df.", "tokens": ["Einst", "rhein\u00b7hi\u00b7nab", "die", "Welt", "sich", "erst", "er\u00b7schlo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und aus dem J\u00fcngling ist ein Mann geworden,", "tokens": ["Und", "aus", "dem", "J\u00fcng\u00b7ling", "ist", "ein", "Mann", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "ART", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Konrad ", "tokens": ["Der", "Kon\u00b7rad"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Den Pickel in den Streit mit Feindeshorden,", "tokens": ["Den", "Pi\u00b7ckel", "in", "den", "Streit", "mit", "Fein\u00b7des\u00b7hor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie ihn gef\u00fchrt der alten Deutschen Hand,", "tokens": ["Wie", "ihn", "ge\u00b7f\u00fchrt", "der", "al\u00b7ten", "Deut\u00b7schen", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Den schwingt auch Konrad treu in seinen Schriften,", "tokens": ["Den", "schwingt", "auch", "Kon\u00b7rad", "treu", "in", "sei\u00b7nen", "Schrif\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "NE", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Des deutschen Namens Ehr' und Glanz zu stiften. \u2013", "tokens": ["Des", "deut\u00b7schen", "Na\u00b7mens", "Ehr'", "und", "Glanz", "zu", "stif\u00b7ten", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "KON", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Auf off'nem Markt ein wogendes Gedr\u00e4nge,", "tokens": ["Auf", "off'\u00b7nem", "Markt", "ein", "wo\u00b7gen\u00b7des", "Ge\u00b7dr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Um Kaiser Friedrich rings der F\u00fcrsten Glanz,", "tokens": ["Um", "Kai\u00b7ser", "Fried\u00b7rich", "rings", "der", "F\u00fcrs\u00b7ten", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "NE", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der freien Reichsstadt festliches Gepr\u00e4nge", "tokens": ["Der", "frei\u00b7en", "Reichs\u00b7stadt", "fest\u00b7li\u00b7ches", "Ge\u00b7pr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und eine Jungfrau mit dem Lorberkranz \u2013", "tokens": ["Und", "ei\u00b7ne", "Jung\u00b7frau", "mit", "dem", "Lor\u00b7ber\u00b7kranz", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein Wink des Kaisers \u2013 und die Dichterkrone", "tokens": ["Ein", "Wink", "des", "Kai\u00b7sers", "\u2013", "und", "die", "Dich\u00b7ter\u00b7kro\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$(", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Verleiht er so dem fl\u00fccht'gen Bauernsohne.", "tokens": ["Ver\u00b7leiht", "er", "so", "dem", "fl\u00fccht'\u00b7gen", "Bau\u00b7ern\u00b7soh\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wohl mag er denken jetzt der heim'schen Reben,", "tokens": ["Wohl", "mag", "er", "den\u00b7ken", "jetzt", "der", "heim'\u00b7schen", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die er verlie\u00df in ungest\u00fcmen Drang,", "tokens": ["Die", "er", "ver\u00b7lie\u00df", "in", "un\u00b7ge\u00b7st\u00fc\u00b7men", "Drang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Jetzt kann er edlern Ranken St\u00fctze geben,", "tokens": ["Jetzt", "kann", "er", "ed\u00b7lern", "Ran\u00b7ken", "St\u00fct\u00b7ze", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJA", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie er's ersehnt, jetzt str\u00f6met sein Gesang:", "tokens": ["Wie", "er's", "er\u00b7sehnt", ",", "jetzt", "str\u00f6\u00b7met", "sein", "Ge\u00b7sang", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVPP", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Gleich heim'schem Feuerwein durch Deutschlands Marken,", "tokens": ["Gleich", "heim'\u00b7schem", "Feu\u00b7er\u00b7wein", "durch", "Deutschlands", "Mar\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "NE", "NE", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Da\u00df deutsche Kunst und Art daran erstarken.", "tokens": ["Da\u00df", "deut\u00b7sche", "Kunst", "und", "Art", "da\u00b7ran", "er\u00b7star\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "KON", "NN", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Jetzt wirbt er \u00fcberall die Bundesbr\u00fcder", "tokens": ["Jetzt", "wirbt", "er", "\u00fc\u00b7be\u00b7rall", "die", "Bun\u00b7des\u00b7br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In Nord und S\u00fcd, zu wahren Deutschlands Ehr',", "tokens": ["In", "Nord", "und", "S\u00fcd", ",", "zu", "wah\u00b7ren", "Deutschlands", "Ehr'", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "$,", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Die Dichtkunst ihm ein Weinstock, sie die Glieder,", "tokens": ["Die", "Dicht\u00b7kunst", "ihm", "ein", "Wein\u00b7stock", ",", "sie", "die", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ART", "NN", "$,", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die er vereint wie Ranken um sich her.", "tokens": ["Die", "er", "ver\u00b7eint", "wie", "Ran\u00b7ken", "um", "sich", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "KOKOM", "NN", "APPR", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch ruhelos \u2013 wie damals auf dem Rhein \u2013", "tokens": ["Doch", "ru\u00b7he\u00b7los", "\u2013", "wie", "da\u00b7mals", "auf", "dem", "Rhein", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$(", "KOKOM", "ADV", "APPR", "ART", "NE", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "So mu\u00df hinfort sein ganzes Leben sein.", "tokens": ["So", "mu\u00df", "hin\u00b7fort", "sein", "gan\u00b7zes", "Le\u00b7ben", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PPOSAT", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Ein Dichterlos! ein sturmbewegtes Leben,", "tokens": ["Ein", "Dich\u00b7ter\u00b7los", "!", "ein", "sturm\u00b7be\u00b7weg\u00b7tes", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein prangend Grabmal am St. Stephansdom!", "tokens": ["Ein", "pran\u00b7gend", "Grab\u00b7mal", "am", "St.", "Ste\u00b7phans\u00b7dom", "!"], "token_info": ["word", "word", "word", "word", "abbreviation", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NE", "NE", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Doch ziemt es nicht drob Klage zu erheben,", "tokens": ["Doch", "ziemt", "es", "nicht", "drob", "Kla\u00b7ge", "zu", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ADV", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihn trug ein Flo\u00df auf einem deutschen Strom,", "tokens": ["Ihn", "trug", "ein", "Flo\u00df", "auf", "ei\u00b7nem", "deut\u00b7schen", "Strom", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Begeistrung war's die ihn dahin getragen: \u2013", "tokens": ["Be\u00b7geis\u00b7trung", "wa\u00b7r's", "die", "ihn", "da\u00b7hin", "ge\u00b7tra\u00b7gen", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VAFIN", "ART", "PPER", "PAV", "VVPP", "$.", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "Du edles N\u00fcrnberg bist wie eine Blume", "tokens": ["Du", "ed\u00b7les", "N\u00fcrn\u00b7berg", "bist", "wie", "ei\u00b7ne", "Blu\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "VAFIN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text