{"dta.poem.13502": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Ii .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wie k\u00fchl schweift sich's bei n\u00e4cht'ger Stunde,", "tokens": ["Wie", "k\u00fchl", "schweift", "sich's", "bei", "n\u00e4cht'\u00b7ger", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PIS", "APPR", "ADJA", "NN", "$,"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Die Zitter treulich in der Hand!", "tokens": ["Die", "Zit\u00b7ter", "treu\u00b7lich", "in", "der", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vom H\u00fcgel gr\u00fc\u00df ich in die Runde", "tokens": ["Vom", "H\u00fc\u00b7gel", "gr\u00fc\u00df", "ich", "in", "die", "Run\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Himmel und das stille Land.", "tokens": ["Den", "Him\u00b7mel", "und", "das", "stil\u00b7le", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie ist da alles so verwandelt,", "tokens": ["Wie", "ist", "da", "al\u00b7les", "so", "ver\u00b7wan\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo ich so fr\u00f6hlich war, im Thal.", "tokens": ["Wo", "ich", "so", "fr\u00f6h\u00b7lich", "war", ",", "im", "Thal", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "VAFIN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Wald wie still! der Mond nur wandelt", "tokens": ["Im", "Wald", "wie", "still", "!", "der", "Mond", "nur", "wan\u00b7delt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KOKOM", "ADJD", "$.", "ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nun durch den hohen Buchensaal.", "tokens": ["Nun", "durch", "den", "ho\u00b7hen", "Bu\u00b7chen\u00b7saal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Winzer Jauchzen ist verklungen", "tokens": ["Der", "Win\u00b7zer", "Jauch\u00b7zen", "ist", "ver\u00b7klun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und all der bunte Lebenslauf,", "tokens": ["Und", "all", "der", "bun\u00b7te", "Le\u00b7bens\u00b7lauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Str\u00f6me nur, im Thal geschlungen,", "tokens": ["Die", "Str\u00f6\u00b7me", "nur", ",", "im", "Thal", "ge\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie blicken manchmal silbern auf.", "tokens": ["Sie", "bli\u00b7cken", "manch\u00b7mal", "sil\u00b7bern", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und Nachtigallen wie aus Tr\u00e4umen", "tokens": ["Und", "Nach\u00b7ti\u00b7gal\u00b7len", "wie", "aus", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KOKOM", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erwachen oft mit s\u00fc\u00dfem Schall,", "tokens": ["Er\u00b7wa\u00b7chen", "oft", "mit", "s\u00fc\u00b7\u00dfem", "Schall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erinnernd r\u00fchrt sich in den B\u00e4umen,", "tokens": ["E\u00b7rin\u00b7nernd", "r\u00fchrt", "sich", "in", "den", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein heimlich Fl\u00fcstern \u00fcberall. \u2014", "tokens": ["Ein", "heim\u00b7lich", "Fl\u00fcs\u00b7tern", "\u00fc\u00b7be\u00b7rall", "."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJD", "NN", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Freude kann nicht gleich verklingen,", "tokens": ["Die", "Freu\u00b7de", "kann", "nicht", "gleich", "ver\u00b7klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und von des Tages Glanz und Lust", "tokens": ["Und", "von", "des", "Ta\u00b7ges", "Glanz", "und", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist so auch mir ein heimlich Singen", "tokens": ["Ist", "so", "auch", "mir", "ein", "heim\u00b7lich", "Sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADV", "PPER", "ART", "ADJD", "NN"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Geblieben in der tiefsten Brust.", "tokens": ["Ge\u00b7blie\u00b7ben", "in", "der", "tiefs\u00b7ten", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und fr\u00f6hlich greif ich in die Saiten,", "tokens": ["Und", "fr\u00f6h\u00b7lich", "greif", "ich", "in", "die", "Sai\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O M\u00e4dchen jenseits \u00fcber'm Flu\u00df,", "tokens": ["O", "M\u00e4d\u00b7chen", "jen\u00b7seits", "\u00fc\u00b7ber'm", "Flu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du lauschest wohl und h\u00f6rst's von weiten", "tokens": ["Du", "lau\u00b7schest", "wohl", "und", "h\u00f6r\u00b7st's", "von", "wei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "KON", "VVFIN", "APPR", "ADJA"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und kennst den S\u00e4nger an dem Gru\u00df!", "tokens": ["Und", "kennst", "den", "S\u00e4n\u00b7ger", "an", "dem", "Gru\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}