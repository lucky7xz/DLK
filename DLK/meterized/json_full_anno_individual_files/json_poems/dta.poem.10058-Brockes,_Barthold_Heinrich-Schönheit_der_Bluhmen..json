{"dta.poem.10058": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Sch\u00f6nheit der Bluhmen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In der holden Rosen-Zeit", "tokens": ["In", "der", "hol\u00b7den", "Ro\u00b7sen\u00b7Zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fcllten ungezehlte Arten", "tokens": ["F\u00fcll\u00b7ten", "un\u00b7ge\u00b7zehl\u00b7te", "Ar\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00f6ner Bluhmen meinen Garten", "tokens": ["Sch\u00f6\u00b7ner", "Bluh\u00b7men", "mei\u00b7nen", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit erw\u00fcnschter Lieblichkeit.", "tokens": ["Mit", "er\u00b7w\u00fcnschter", "Lieb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Aller Bluhmen klare Bl\u00e4tter,", "tokens": ["Al\u00b7ler", "Bluh\u00b7men", "kla\u00b7re", "Bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Von der Sonnen Gluth durchstrahlet,", "tokens": ["Von", "der", "Son\u00b7nen", "Gluth", "durch\u00b7strah\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Kamen, bey so heiterm Wetter,", "tokens": ["Ka\u00b7men", ",", "bey", "so", "hei\u00b7term", "Wet\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In gef\u00e4rbter Wunder-Zier,", "tokens": ["In", "ge\u00b7f\u00e4rb\u00b7ter", "Wun\u00b7der\u00b7Zier", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Mehr beflammet als bemahlet,", "tokens": ["Mehr", "be\u00b7flam\u00b7met", "als", "be\u00b7mah\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KOKOM", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Feurig mehr, als bunt, mir f\u00fcr.", "tokens": ["Feu\u00b7rig", "mehr", ",", "als", "bunt", ",", "mir", "f\u00fcr", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADV", "$,", "KOUS", "ADJD", "$,", "PPER", "APPR", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Auch das Kraut und Laub so gar,", "tokens": ["Auch", "das", "Kraut", "und", "Laub", "so", "gar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "NN", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da die Bl\u00e4tter zart und klar,", "tokens": ["Da", "die", "Bl\u00e4t\u00b7ter", "zart", "und", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Wann die Sonne durch sie schien,", "tokens": ["Wann", "die", "Son\u00b7ne", "durch", "sie", "schien", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zeigt\u2019 ein tausend-f\u00e4rbig gr\u00fcn:", "tokens": ["Zeigt'", "ein", "tau\u00b7sen\u00b7df\u00e4r\u00b7big", "gr\u00fcn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja die\u00df gr\u00fcn schien Wunder-sch\u00f6n", "tokens": ["Ja", "die\u00df", "gr\u00fcn", "schien", "Wun\u00b7der\u00b7sch\u00f6n"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKANT", "PDS", "ADJD", "VVFIN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Auch in gr\u00fcnem Feur zu stehn.", "tokens": ["Auch", "in", "gr\u00fc\u00b7nem", "Feur", "zu", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "In wie sch\u00f6nem Schimmer bl\u00fchte", "tokens": ["In", "wie", "sch\u00f6\u00b7nem", "Schim\u00b7mer", "bl\u00fch\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "KOKOM", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alles, was mein Auge sah!", "tokens": ["Al\u00b7les", ",", "was", "mein", "Au\u00b7ge", "sah", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In gef\u00e4rbten Flammen gl\u00fchte", "tokens": ["In", "ge\u00b7f\u00e4rb\u00b7ten", "Flam\u00b7men", "gl\u00fch\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der von Licht durchstrahlte Mah.", "tokens": ["Der", "von", "Licht", "durch\u00b7strahl\u00b7te", "Mah", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVFIN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Seine zarte Bl\u00e4tter werden", "tokens": ["Sei\u00b7ne", "zar\u00b7te", "Bl\u00e4t\u00b7ter", "wer\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VAINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In der That illuminirt.", "tokens": ["In", "der", "That", "il\u00b7lu\u00b7mi\u00b7nirt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sagt mir, was ist auf der Erden", "tokens": ["Sagt", "mir", ",", "was", "ist", "auf", "der", "Er\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PWS", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.8": {"text": "Herrlicher, als er, geziert?", "tokens": ["Herr\u00b7li\u00b7cher", ",", "als", "er", ",", "ge\u00b7ziert", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "$,", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Seht das funckeln! Seht die Menge!", "tokens": ["Seht", "das", "fun\u00b7ckeln", "!", "Seht", "die", "Men\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "VVINF", "$.", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seht der Liljen weissen Blitz!", "tokens": ["Seht", "der", "Lil\u00b7jen", "weis\u00b7sen", "Blitz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seht des Rosen-Stocks Gepr\u00e4nge!", "tokens": ["Seht", "des", "Ro\u00b7sen\u00b7Stocks", "Ge\u00b7pr\u00e4n\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Welche Feder, welcher Witz", "tokens": ["Wel\u00b7che", "Fe\u00b7der", ",", "wel\u00b7cher", "Witz"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PWAT", "NN", "$,", "PWAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wird der s\u00fcssen Farben brennen", "tokens": ["Wird", "der", "s\u00fcs\u00b7sen", "Far\u00b7ben", "bren\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "W\u00fcrdiglich beschreiben k\u00f6nnen?", "tokens": ["W\u00fcr\u00b7dig\u00b7lich", "be\u00b7schrei\u00b7ben", "k\u00f6n\u00b7nen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "VMINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sonderlich wann sanfte Winde", "tokens": ["Son\u00b7der\u00b7lich", "wann", "sanf\u00b7te", "Win\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Jhre bunte Pracht gelinde", "tokens": ["Ih\u00b7re", "bun\u00b7te", "Pracht", "ge\u00b7lin\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Lieblich durch einander treiben;", "tokens": ["Lieb\u00b7lich", "durch", "ein\u00b7an\u00b7der", "trei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Da der bunten Gluht bewegen", "tokens": ["Da", "der", "bun\u00b7ten", "Gluht", "be\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Und das glimmen bunter Kertzen", "tokens": ["Und", "das", "glim\u00b7men", "bun\u00b7ter", "Kert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VVFIN", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "In den angeflammten Hertzen", "tokens": ["In", "den", "an\u00b7ge\u00b7flamm\u00b7ten", "Hert\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Uns ein Anmuths-Feur erregen.", "tokens": ["Uns", "ein", "An\u00b7muths\u00b7Feur", "er\u00b7re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Unausdr\u00fccklich ist die Freude,", "tokens": ["Un\u00b7aus\u00b7dr\u00fcck\u00b7lich", "ist", "die", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Die so holde Augen-Weide", "tokens": ["Die", "so", "hol\u00b7de", "Au\u00b7gen\u00b7Wei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Dem in seine Seele dr\u00fcckt,", "tokens": ["Dem", "in", "sei\u00b7ne", "See\u00b7le", "dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Der sie, GOTT zum Ruhm, erblickt.", "tokens": ["Der", "sie", ",", "GoTT", "zum", "Ruhm", ",", "er\u00b7blickt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "$,", "NE", "APPRART", "NN", "$,", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}