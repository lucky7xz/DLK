{"textgrid.poem.67502": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "An Knebel", "genre": "verse", "period": "N.A.", "pub_year": 1779, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht auf gegl\u00e4ttetem gr\u00fcnen Papier, wo der zierliche Weichling,", "tokens": ["Nicht", "auf", "ge\u00b7gl\u00e4t\u00b7te\u00b7tem", "gr\u00fc\u00b7nen", "Pa\u00b7pier", ",", "wo", "der", "zier\u00b7li\u00b7che", "Weich\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ADJA", "ADJA", "NN", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wie auf smaragdener Au Blumen, die Worte pflanzt,", "tokens": ["Wie", "auf", "sma\u00b7rag\u00b7de\u00b7ner", "Au", "Blu\u00b7men", ",", "die", "Wor\u00b7te", "pflanzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+---+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sondern auf wei\u00dfem Grunde, wie solches den Frommen geziemet,", "tokens": ["Son\u00b7dern", "auf", "wei\u00b7\u00dfem", "Grun\u00b7de", ",", "wie", "sol\u00b7ches", "den", "From\u00b7men", "ge\u00b7zie\u00b7met", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,", "PWAV", "PIS", "ART", "NN", "VVPP", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Sag' ich f\u00fcr Deinen Properz Dir den elegischsten Dank.", "tokens": ["Sag'", "ich", "f\u00fcr", "Dei\u00b7nen", "Pro\u00b7perz", "Dir", "den", "e\u00b7le\u00b7gischs\u00b7ten", "Dank", "."], "token_info": ["word", "word", "word", 