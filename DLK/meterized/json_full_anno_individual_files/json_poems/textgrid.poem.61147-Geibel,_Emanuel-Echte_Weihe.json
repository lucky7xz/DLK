{"textgrid.poem.61147": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Echte Weihe", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kalt sind sie, kalt, und kalt ist ihr Gedicht;", "tokens": ["Kalt", "sind", "sie", ",", "kalt", ",", "und", "kalt", "ist", "ihr", "Ge\u00b7dicht", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "ADJD", "$,", "KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sie waren nie vom Hauch des Fr\u00fchlings trunken,", "tokens": ["Sie", "wa\u00b7ren", "nie", "vom", "Hauch", "des", "Fr\u00fch\u00b7lings", "trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPRART", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nie in des Gottes Melodie versunken,", "tokens": ["Nie", "in", "des", "Got\u00b7tes", "Me\u00b7lo\u00b7die", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der durch die heil'ge Nacht vernehmbar spricht", "tokens": ["Der", "durch", "die", "heil'\u00b7ge", "Nacht", "ver\u00b7nehm\u00b7bar", "spricht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Auch f\u00fchlen sie's, was ihrem Lied gebricht,", "tokens": ["Auch", "f\u00fch\u00b7len", "sie's", ",", "was", "ih\u00b7rem", "Lied", "ge\u00b7bricht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und lassen zum Ersatz der Lebensfunken", "tokens": ["Und", "las\u00b7sen", "zum", "Er\u00b7satz", "der", "Le\u00b7bens\u00b7fun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit Schmink' und Flittergold die Leiche prunken,", "tokens": ["Mit", "Schmink'", "und", "Flit\u00b7ter\u00b7gold", "die", "Lei\u00b7che", "prun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit eitlem Schimmer, der den Sinn besticht.", "tokens": ["Mit", "eit\u00b7lem", "Schim\u00b7mer", ",", "der", "den", "Sinn", "be\u00b7sticht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch wen der Geist beseelet, unerschrocken", "tokens": ["Doch", "wen", "der", "Geist", "be\u00b7see\u00b7let", ",", "un\u00b7er\u00b7schro\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PWS", "ART", "NN", "VVFIN", "$,", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verschm\u00e4hen mag er, was der Markt erhebt,", "tokens": ["Ver\u00b7schm\u00e4\u00b7hen", "mag", "er", ",", "was", "der", "Markt", "er\u00b7hebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und dennoch, singt er, bleibt kein Auge trocken.", "tokens": ["Und", "den\u00b7noch", ",", "singt", "er", ",", "bleibt", "kein", "Au\u00b7ge", "tro\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dem Gotte gleicht er, den der Aar umschwebt;", "tokens": ["Dem", "Got\u00b7te", "gleicht", "er", ",", "den", "der", "Aar", "um\u00b7schwebt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er sch\u00fcttelt leise nur die dunkeln Locken,", "tokens": ["Er", "sch\u00fct\u00b7telt", "lei\u00b7se", "nur", "die", "dun\u00b7keln", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und der Olymp und jedes Herz erbebt.", "tokens": ["Und", "der", "O\u00b7lymp", "und", "je\u00b7des", "Herz", "er\u00b7bebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "KON", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Kalt sind sie, kalt, und kalt ist ihr Gedicht;", "tokens": ["Kalt", "sind", "sie", ",", "kalt", ",", "und", "kalt", "ist", "ihr", "Ge\u00b7dicht", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "