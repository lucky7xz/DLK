{"textgrid.poem.55678": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Sturmflut", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gleichwie des Meeres Wogen dann und wann,", "tokens": ["Gleich\u00b7wie", "des", "Mee\u00b7res", "Wo\u00b7gen", "dann", "und", "wann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "ADV", "KON", "PWAV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den Damm zerrei\u00dfend, m\u00e4chtig \u00fcberfluten,", "tokens": ["Den", "Damm", "zer\u00b7rei\u00b7\u00dfend", ",", "m\u00e4ch\u00b7tig", "\u00fc\u00b7berf\u00b7lu\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erscheinen jedem Menschen auch Minuten", "tokens": ["Er\u00b7schei\u00b7nen", "je\u00b7dem", "Men\u00b7schen", "auch", "Mi\u00b7nu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PIAT", "NN", "ADV", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo er sein Elend nicht verschweigen kann.", "tokens": ["Wo", "er", "sein", "E\u00b7lend", "nicht", "ver\u00b7schwei\u00b7gen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sich selber \u00fcberst\u00fcrzend, naht es dann,", "tokens": ["Sich", "sel\u00b7ber", "\u00fc\u00b7bers\u00b7t\u00fcr\u00b7zend", ",", "naht", "es", "dann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVPP", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Aufwachen Qualen, die nur scheintot ruhten,", "tokens": ["Auf\u00b7wa\u00b7chen", "Qua\u00b7len", ",", "die", "nur", "schein\u00b7tot", "ruh\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vernarbte Wunden fangen an zu bluten,", "tokens": ["Ver\u00b7narb\u00b7te", "Wun\u00b7den", "fan\u00b7gen", "an", "zu", "blu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und es zerbricht des Schweigens starrer Bann.", "tokens": ["Und", "es", "zer\u00b7bricht", "des", "Schwei\u00b7gens", "star\u00b7rer", "Bann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und wie das Meer, gepeitscht, der St\u00fcrme Spiel,", "tokens": ["Und", "wie", "das", "Meer", ",", "ge\u00b7peitscht", ",", "der", "St\u00fcr\u00b7me", "Spiel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "$,", "VVPP", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Aufrast und tobt und um Erl\u00f6sung schreit,", "tokens": ["Auf\u00b7rast", "und", "tobt", "und", "um", "Er\u00b7l\u00f6\u00b7sung", "schreit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "KON", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So gellt der Mensch sein namenloses Leid", "tokens": ["So", "gellt", "der", "Mensch", "sein", "na\u00b7men\u00b7lo\u00b7ses", "Leid"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hinaus zur Welt, wenn nur der Bann erst fiel.", "tokens": ["Hin\u00b7aus", "zur", "Welt", ",", "wenn", "nur", "der", "Bann", "erst", "fiel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "KOUS", "ADV", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und rast und tobt und eher schweigt er nicht,", "tokens": ["Und", "rast", "und", "tobt", "und", "e\u00b7her", "schweigt", "er", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "KON", "ADV", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Als bis er todesmatt zusammenbricht.", "tokens": ["Als", "bis", "er", "to\u00b7des\u00b7matt", "zu\u00b7sam\u00b7men\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Gleichwie des Meeres Wogen dann und wann,", "tokens": ["Gleich\u00b7wie", "des", "Mee\u00b7res", "Wo\u00b7gen", "dann", "und", "wann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "ADV", "KON", "PWAV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den Damm zerrei\u00dfend, m\u00e4chtig \u00fcberfluten,", "tokens": ["Den", "Damm", "zer\u