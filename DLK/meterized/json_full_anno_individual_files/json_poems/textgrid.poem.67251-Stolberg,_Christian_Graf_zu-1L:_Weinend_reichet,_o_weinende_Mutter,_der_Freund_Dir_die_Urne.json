{"textgrid.poem.67251": {"metadata": {"author": {"name": "Stolberg, Christian Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Weinend reichet, o weinende Mutter, der Freund Dir die Urne", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weinend reichet, o weinende Mutter, der Freund Dir die Urne", "tokens": ["Wei\u00b7nend", "rei\u00b7chet", ",", "o", "wei\u00b7nen\u00b7de", "Mut\u00b7ter", ",", "der", "Freund", "Dir", "die", "Ur\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "$,", "FM", "ADJA", "NN", "$,", "ART", "NN", "PPER", "ART", "NN"], "meter": "+-+--+--+--++-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Deines Engels, in ihr, welkend, die Locke noch sch\u00f6n.", "tokens": ["Dei\u00b7nes", "En\u00b7gels", ",", "in", "ihr", ",", "wel\u00b7kend", ",", "die", "Lo\u00b7cke", "noch", "sch\u00f6n", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "PPER", "$,", "VVPP", "$,", "ART", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Der, wie die Stern' am Himmel, die Haar auf dem Haupte der Seinen", "tokens": ["Der", ",", "wie", "die", "Stern'", "am", "Him\u00b7mel", ",", "die", "Haar", "auf", "dem", "Haup\u00b7te", "der", "Sei\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "$,", "PWAV", "ART", "NN", "APPRART", "NN", "$,", "ART", "NN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Z\u00e4hlt, der gebot, und die Saat sank vor der Reife gem\u00e4ht.", "toke