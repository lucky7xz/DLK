-", "measure": "trochaic.tri"}, "line.8": {"text": "Schl\u00e4gt die Lust empor!", "tokens": ["Schl\u00e4gt", "die", "Lust", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Da\u00df der junge Sonnenball,", "tokens": ["Da\u00df", "der", "jun\u00b7ge", "Son\u00b7nen\u00b7ball", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Rollt er auf den H\u00fcgeln,", "tokens": ["Rollt", "er", "auf", "den", "H\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Sich im funkelnden Kristall", "tokens": ["Sich", "im", "fun\u00b7keln\u00b7den", "Kris\u00b7tall"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "APPRART", "ADJA", "NN"], "meter": "+-+--++", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Kl\u00e4rlich kann bespiegeln:", "tokens": ["Kl\u00e4r\u00b7lich", "kann", "be\u00b7spie\u00b7geln", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Halten wir entgegen", "tokens": ["Hal\u00b7ten", "wir", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPER", "PTKVZ"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Becher ihm und Glas!", "tokens": ["Be\u00b7cher", "ihm", "und", "Glas", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Flie\u00dfe, goldner Regen,", "tokens": ["Flie\u00b7\u00dfe", ",", "gold\u00b7ner", "Re\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Gl\u00fche, dunkles Na\u00df!", "tokens": ["Gl\u00fc\u00b7he", ",", "dunk\u00b7les", "Na\u00df", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Jungfrau! Geh und sieh mir nach", "tokens": ["Jung\u00b7frau", "!", "Geh", "und", "sieh", "mir", "nach"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "NE", "KON", "VVIMP", "PPER", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Rings in allen G\u00e4rten,", "tokens": ["Rings", "in", "al\u00b7len", "G\u00e4r\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ob die Rosen schon sind wach:", "tokens": ["Ob", "die", "Ro\u00b7sen", "schon", "sind", "wach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bring die tauverkl\u00e4rten!", "tokens": ["Bring", "die", "tau\u00b7ver\u00b7kl\u00e4r\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Rosen, Rosen bringe!", "tokens": ["Ro\u00b7sen", ",", "Ro\u00b7sen", "brin\u00b7ge", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Rosenduft soll wehn!", "tokens": ["Ro\u00b7sen\u00b7duft", "soll", "wehn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Wenn ich trink und singe,", "tokens": ["Wenn", "ich", "trink", "und", "sin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Mu\u00df ich Blumen sehn!", "tokens": ["Mu\u00df", "ich", "Blu\u00b7men", "sehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Horch! Der tiefe Amselschlag", "tokens": ["Horch", "!", "Der", "tie\u00b7fe", "Am\u00b7sel\u00b7schlag"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schallet aus den Gr\u00fcnden;", "tokens": ["Schal\u00b7let", "aus", "den", "Gr\u00fcn\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Treue W\u00e4chter soll der Tag", "tokens": ["Treu\u00b7e", "W\u00e4ch\u00b7ter", "soll", "der", "Tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NE", "VMFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Heiter in uns finden.", "tokens": ["Hei\u00b7ter", "in", "uns", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wer wird denn vermissen", "tokens": ["Wer", "wird", "denn", "ver\u00b7mis\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ADV", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Eine kurze Nacht,", "tokens": ["Ei\u00b7ne", "kur\u00b7ze", "Nacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Wenn sie sangbeflissen,", "tokens": ["Wenn", "sie", "sang\u00b7be\u00b7flis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Wacker durchgewacht?", "tokens": ["Wa\u00b7cker", "durch\u00b7ge\u00b7wacht", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Tief ist unsrer Freude Born,", "tokens": ["Tief", "ist", "uns\u00b7rer", "Freu\u00b7de", "Born", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "NE", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Tiefer als das Leiden,", "tokens": ["Tie\u00b7fer", "als", "das", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Doch es wacht der helle Zorn", "tokens": ["Doch", "es", "wacht", "der", "hel\u00b7le", "Zorn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gleich in ihnen beiden.", "tokens": ["Gleich", "in", "ih\u00b7nen", "bei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "PIAT", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Darum lasset rinnen", "tokens": ["Da\u00b7rum", "las\u00b7set", "rin\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["PAV", "VVFIN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Letztes Glas und Lied!",