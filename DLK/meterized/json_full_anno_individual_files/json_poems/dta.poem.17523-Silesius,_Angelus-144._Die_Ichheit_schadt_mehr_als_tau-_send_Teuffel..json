{"dta.poem.17523": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "144.  Die Ichheit schadt mehr als tau-  \n send Teuffel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Mensch h\u00fctte dich f\u00fcr dir. Wirstu mit dir beladen/", "tokens": ["Mensch", "h\u00fct\u00b7te", "dich", "f\u00fcr", "dir", ".", "Wirs\u00b7tu", "mit", "dir", "be\u00b7la\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPR", "PPER", "$.", "NN", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du wirst dir selber mehr als tausend Teuffel schaden.", "tokens": ["Du", "wirst", "dir", "sel\u00b7ber", "mehr", "als", "tau\u00b7send", "Teuf\u00b7fel", "scha\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PIAT", "KOKOM", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}