{"dta.poem.12924": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als Floris eine blume in ihren bu-  \n sen steckte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So gern ich auch die weichen h\u00fcgel k\u00fc\u00dfte,", "tokens": ["So", "gern", "ich", "auch", "die", "wei\u00b7chen", "h\u00fc\u00b7gel", "k\u00fc\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Floris stets vor mir versteckt;", "tokens": ["Die", "Flo\u00b7ris", "stets", "vor", "mir", "ver\u00b7steckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So hat die zarte furcht vor ihrem strengen zorne", "tokens": ["So", "hat", "die", "zar\u00b7te", "furcht", "vor", "ih\u00b7rem", "stren\u00b7gen", "zor\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mich gleichwol davon abgeschreckt.", "tokens": ["Mich", "gleich\u00b7wol", "da\u00b7von", "ab\u00b7ge\u00b7schreckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PAV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch wenn ich mich nur zu verwandeln w\u00fc\u00dfte!", "tokens": ["Doch", "wenn", "ich", "mich", "nur", "zu", "ver\u00b7wan\u00b7deln", "w\u00fc\u00df\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "ADV", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "K\u00f6nnt\u2019 ich nur eine blum\u2019, als wie Nareissus, seyn;", "tokens": ["K\u00f6nnt'", "ich", "nur", "ei\u00b7ne", "blum'", ",", "als", "wie", "Na\u00b7reis\u00b7sus", ",", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "$,", "KOUS", "KOKOM", "NE", "$,", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So tr\u00e4ffe mein verlangen dennoch ein:", "tokens": ["So", "tr\u00e4f\u00b7fe", "mein", "ver\u00b7lan\u00b7gen", "den\u00b7noch", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Denn Floris legte mich selbst zwischen ihre br\u00fcste.", "tokens": ["Denn", "Flo\u00b7ris", "leg\u00b7te", "mich", "selbst", "zwi\u00b7schen", "ih\u00b7re", "br\u00fcs\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}