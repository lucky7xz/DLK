{"dta.poem.21860": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "XvI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4r\u2019 ein Licht nicht au\u00dfgegangen/", "tokens": ["W\u00e4r'", "ein", "Licht", "nicht", "au\u00df\u00b7ge\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PTKNEG", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "w\u00e4r\u2019 ich/ Blinder/ nicht gefangen/", "tokens": ["w\u00e4r'", "ich", "/", "Blin\u00b7der", "/", "nicht", "ge\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$(", "NN", "$(", "PTKNEG", "ADJD", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nu verloschen ist das Licht/", "tokens": ["Nu", "ver\u00b7lo\u00b7schen", "ist", "das", "Licht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "hab\u2019 ich kein Gesichte nicht.", "tokens": ["hab'", "ich", "kein", "Ge\u00b7sich\u00b7te", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}