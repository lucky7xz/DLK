{"dta.poem.9344": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Auf eben da\u00dfelbe Jubelfest.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nun haben wier/ Gott lob! erlebt die zeit/", "tokens": ["Nun", "ha\u00b7ben", "wier", "/", "Gott", "lob", "!", "er\u00b7lebt", "die", "zeit", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "$(", "NN", "NN", "$.", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "da uns das fest der Druckerey nun wieder", "tokens": ["da", "uns", "das", "fest", "der", "Dru\u00b7cke\u00b7rey", "nun", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PDS", "VVFIN", "ART", "NN", "ADV", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "erschienen ist; stimmt an die sreuden-lieder!", "tokens": ["er\u00b7schie\u00b7nen", "ist", ";", "stimmt", "an", "die", "sreu\u00b7den\u00b7lie\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$.", "VVFIN", "APPR", "ART", "ADJA", "$."], "meter": "-+-+++-+-+-", "measure": "unknown.measure.hexa"}, "line.4": {"text": "wie bricht der tag der gnaden allbereit", "tokens": ["wie", "bricht", "der", "tag", "der", "gna\u00b7den", "all\u00b7be\u00b7reit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "voll glantz herein durch alle dunckelheit!", "tokens": ["voll", "glantz", "her\u00b7ein", "durch", "al\u00b7le", "dun\u00b7ckel\u00b7heit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "PTKVZ", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Pabst ligt nun durch diese Kunst darnieder/", "tokens": ["Der", "Pabst", "ligt", "nun", "durch", "die\u00b7se", "Kunst", "dar\u00b7nie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "PDAT", "NN", "PAV", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er ist gest\u00fcrtzt/ zerschlagen seine glieder/", "tokens": ["Er", "ist", "ge\u00b7st\u00fcrtzt", "/", "zer\u00b7schla\u00b7gen", "sei\u00b7ne", "glie\u00b7der", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$(", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "es ist nun klar und kundig weit und breit.", "tokens": ["es", "ist", "nun", "klar", "und", "kun\u00b7dig", "weit", "und", "breit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Di\u00df eben ists/ was dort Sibylla setzet/", "tokens": ["Di\u00df", "e\u00b7ben", "ists", "/", "was", "dort", "Si\u00b7byl\u00b7la", "set\u00b7zet", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VAFIN", "$(", "PWS", "ADV", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und propheceyt/ da\u00df des Verderbens-Sohn", "tokens": ["und", "pro\u00b7phe\u00b7ceyt", "/", "da\u00df", "des", "Ver\u00b7der\u00b7bens\u00b7Sohn"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NE", "$(", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "soll durch den flachs verliehren macht und krohn;", "tokens": ["soll", "durch", "den", "flachs", "ver\u00b7lieh\u00b7ren", "macht", "und", "krohn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "ADJA", "VVINF", "VVFIN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dis eben ists; was Babilon verletzet/", "tokens": ["Dis", "e\u00b7ben", "ists", ";", "was", "Ba\u00b7bi\u00b7lon", "ver\u00b7let\u00b7zet", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VAFIN", "$.", "PWS", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und niederwirfft; was vormahls Gutenberg", "tokens": ["und", "nie\u00b7der\u00b7wirfft", ";", "was", "vor\u00b7mahls", "Gu\u00b7ten\u00b7berg"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "PWS", "ADV", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und Faust erfandt/ das edle Drucker-werck.", "tokens": ["und", "Faust", "er\u00b7fandt", "/", "das", "ed\u00b7le", "Dru\u00b7cke\u00b7r\u00b7werck", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}}}}