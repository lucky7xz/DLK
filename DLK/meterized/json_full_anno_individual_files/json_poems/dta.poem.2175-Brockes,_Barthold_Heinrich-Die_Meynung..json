{"dta.poem.2175": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Meynung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Erw\u00e4gte man doch diesen Lehrsatz mit mehr Bedacht-", "tokens": ["Er\u00b7w\u00e4g\u00b7te", "man", "doch", "die\u00b7sen", "Lehr\u00b7satz", "mit", "mehr", "Be\u00b7dacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ADV", "PDAT", "NN", "APPR", "PIAT", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "samkeit und Flei\u00df!", "tokens": ["sam\u00b7keit", "und", "Flei\u00df", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ein Wesen, welches auf einmal so viel und auch so we-", "tokens": ["Ein", "We\u00b7sen", ",", "wel\u00b7ches", "auf", "ein\u00b7mal", "so", "viel", "und", "auch", "so", "we"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADV", "ADV", "ADV", "KON", "ADV", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "nig weis,", "tokens": ["nig", "weis", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Dem scheint sein wesentlicher Zustand nichts, als nur die-", "tokens": ["Dem", "scheint", "sein", "we\u00b7sent\u00b7li\u00b7cher", "Zu\u00b7stand", "nichts", ",", "als", "nur", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPOSAT", "ADJA", "NN", "PIS", "$,", "KOUS", "ADV", "TRUNC"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "ses, zu erlauben:", "tokens": ["ses", ",", "zu", "er\u00b7lau\u00b7ben", ":"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["FM.fr", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Jm Sinn- und Leiblichen zu meynen, im G\u00f6tt- und Geist-", "tokens": ["Jm", "Sinn", "und", "Leib\u00b7li\u00b7chen", "zu", "mey\u00b7nen", ",", "im", "G\u00f6t\u00b7t", "und", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "TRUNC", "KON", "NN", "PTKZU", "VVINF", "$,", "APPRART", "TRUNC", "KON", "TRUNC"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "lichen zu glauben.", "tokens": ["li\u00b7chen", "zu", "glau\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVINF", "PTKZU", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}