{"textgrid.poem.66792": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: H\u00f6re, Kind, und la\u00df dir sagen,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6re, Kind, und la\u00df dir sagen,", "tokens": ["H\u00f6\u00b7re", ",", "Kind", ",", "und", "la\u00df", "dir", "sa\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "KON", "VVIMP", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was zu dir die Mutter spricht:", "tokens": ["Was", "zu", "dir", "die", "Mut\u00b7ter", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Einen Namen sollst du tragen,", "tokens": ["Ei\u00b7nen", "Na\u00b7men", "sollst", "du", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einen Namen von Gewicht!", "tokens": ["Ei\u00b7nen", "Na\u00b7men", "von", "Ge\u00b7wicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Herr von Prittwitz-Prattwitz-Prottwitz", "tokens": ["Herr", "von", "Pritt\u00b7witz\u00b7Pratt\u00b7witz\u00b7Prott\u00b7witz"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Warb vertraut um deine Hand,", "tokens": ["Warb", "ver\u00b7traut", "um", "dei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dem die Kittwitz-Kattwitz-Kottwitz \u2013", "tokens": ["Dem", "die", "Kitt\u00b7witz\u00b7Katt\u00b7witz\u00b7Kott\u00b7witz", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "H\u00f6rst du? Kottwitz! \u2013 stammverwandt.", "tokens": ["H\u00f6rst", "du", "?", "Kott\u00b7witz", "!", "\u2013", "stamm\u00b7ver\u00b7wandt", "."], "token_info": ["word", "word", "punct", "word", "punct", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "NE", "$.", "$(", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Eva, Eva \u2013 la\u00df dich preisen,", "tokens": ["E\u00b7va", ",", "E\u00b7va", "\u2013", "la\u00df", "dich", "prei\u00b7sen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$(", "VVIMP", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zogst du doch ein gro\u00dfes Los,", "tokens": ["Zogst", "du", "doch", "ein", "gro\u00b7\u00dfes", "Los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In den allerh\u00f6chsten Kreisen", "tokens": ["In", "den", "al\u00b7ler\u00b7h\u00f6chs\u00b7ten", "Krei\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tr\u00e4gt man bald dich auf dem Scho\u00df.", "tokens": ["Tr\u00e4gt", "man", "bald", "dich", "auf", "dem", "Scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "PPER", "APPR", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Der Gesellschaft stolze Spitzen", "tokens": ["Der", "Ge\u00b7sell\u00b7schaft", "stol\u00b7ze", "Spit\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "K\u00fcssen \u2013 Evchen \u2013 dir die Hand,", "tokens": ["K\u00fcs\u00b7sen", "\u2013", "Ev\u00b7chen", "\u2013", "dir", "die", "Hand", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NE", "$(", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Deine Diamanten blitzen", "tokens": ["Dei\u00b7ne", "Di\u00b7a\u00b7man\u00b7ten", "blit\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Weit hinaus ins Vaterland.", "tokens": ["Weit", "hin\u00b7aus", "ins", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APZR", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Keiner fragt, was wir gewesen,", "tokens": ["Kei\u00b7ner", "fragt", ",", "was", "wir", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PRELS", "PPER", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn der Herr uns so erh\u00f6ht,", "tokens": ["Wenn", "der", "Herr", "uns", "so", "er\u00b7h\u00f6ht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df im Winkel hinterm Tresen", "tokens": ["Da\u00df", "im", "Win\u00b7kel", "hin\u00b7term", "Tre\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "T\u00fcten, T\u00fcten wir gedreht.", "tokens": ["T\u00fc\u00b7ten", ",", "T\u00fc\u00b7ten", "wir", "ge\u00b7dreht", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Keiner fragt, wie wir geworden,", "tokens": ["Kei\u00b7ner", "fragt", ",", "wie", "wir", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PWAV", "PPER", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Was wir, Gott sei Lob, nun sind,", "tokens": ["Was", "wir", ",", "Gott", "sei", "Lob", ",", "nun", "sind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "$,", "NN", "VAFIN", "NN", "$,", "ADV", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Vor dem Glanze unsrer Orden", "tokens": ["Vor", "dem", "Glan\u00b7ze", "uns\u00b7rer", "Or\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Werden alle Augen blind.", "tokens": ["Wer\u00b7den", "al\u00b7le", "Au\u00b7gen", "blind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Was verziehst du so dein M\u00e4ulchen?", "tokens": ["Was", "ver\u00b7ziehst", "du", "so", "dein", "M\u00e4ul\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df nicht jung mehr der Gemahl?", "tokens": ["Da\u00df", "nicht", "jung", "mehr", "der", "Ge\u00b7mahl", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ADJD", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, du bist ein kindlich Veilchen \u2013", "tokens": ["Ach", ",", "du", "bist", "ein", "kind\u00b7lich", "Veil\u00b7chen", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "ART", "ADJD", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df sein Witz ein wenig schal?", "tokens": ["Da\u00df", "sein", "Witz", "ein", "we\u00b7nig", "schal", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "PIS", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Geistreich str\u00f6mt's von allen Seiten", "tokens": ["Geist\u00b7reich", "str\u00f6mt's", "von", "al\u00b7len", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "F\u00fcr superbe Kost dir zu \u2013", "tokens": ["F\u00fcr", "su\u00b7per\u00b7be", "Kost", "dir", "zu", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "PTKZU", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Kleinigkeiten, Kleinigkeiten!", "tokens": ["Klei\u00b7nig\u00b7kei\u00b7ten", ",", "Klei\u00b7nig\u00b7kei\u00b7ten", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Welch ein dummes Ding bist du!", "tokens": ["Welch", "ein", "dum\u00b7mes", "Ding", "bist", "du", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJA", "NN", "VAFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "So, jetzt la\u00df ich dich alleine \u2013", "tokens": ["So", ",", "jetzt", "la\u00df", "ich", "dich", "al\u00b7lei\u00b7ne", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVIMP", "PPER", "PRF", "ADV", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Prottwitz bleibt nach dem Souper.", "tokens": ["Prott\u00b7witz", "bleibt", "nach", "dem", "Sou\u00b7per", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Du verstehst wohl, was ich meine?", "tokens": ["Du", "ver\u00b7stehst", "wohl", ",", "was", "ich", "mei\u00b7ne", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sag nur Ja \u2013 Noch mal: o je?!", "tokens": ["Sag", "nur", "Ja", "\u2013", "Noch", "mal", ":", "o", "je", "?!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKANT", "$(", "ADV", "ADV", "$.", "FM", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Seufzer sind hier \u00fcberfl\u00fcssig,", "tokens": ["Seuf\u00b7zer", "sind", "hier", "\u00fc\u00b7berf\u00b7l\u00fcs\u00b7sig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "La\u00df doch den Poetenkohl!", "tokens": ["La\u00df", "doch", "den", "Poe\u00b7ten\u00b7kohl", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.7": {"text": "Dein Papa und ich sind schl\u00fcssig,", "tokens": ["Dein", "Pa\u00b7pa", "und", "ich", "sind", "schl\u00fcs\u00b7sig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPER", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Das gen\u00fcgt dir. Lebe wohl!", "tokens": ["Das", "ge\u00b7n\u00fcgt", "dir", ".", "Le\u00b7be", "wohl", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$.", "VVIMP", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "H\u00f6re, Kind, und la\u00df dir sagen,", "tokens": ["H\u00f6\u00b7re", ",", "Kind", ",", "und", "la\u00df", "dir", "sa\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "KON", "VVIMP", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was zu dir die Mutter spricht:", "tokens": ["Was", "zu", "dir", "die", "Mut\u00b7ter", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Einen Namen sollst du tragen,", "tokens": ["Ei\u00b7nen", "Na\u00b7men", "sollst", "du", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einen Namen von Gewicht!", "tokens": ["Ei\u00b7nen", "Na\u00b7men", "von", "Ge\u00b7wicht", "!"], "tok