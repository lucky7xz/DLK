{"textgrid.poem.59607": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Scharnhorst der Ehrenbote", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wen erlest ihr f\u00fcr die gro\u00dfen Toten,", "tokens": ["Wen", "er\u00b7lest", "ihr", "f\u00fcr", "die", "gro\u00b7\u00dfen", "To\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die einst ritterlich f\u00fcrs deutsche Land", "tokens": ["Die", "einst", "rit\u00b7ter\u00b7lich", "f\u00fcrs", "deut\u00b7sche", "Land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJD", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ihre Brust dem Eisen boten?", "tokens": ["Ih\u00b7re", "Brust", "dem", "Ei\u00b7sen", "bo\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wen erlest ihr als den rechten Boten,", "tokens": ["Wen", "er\u00b7lest", "ihr", "als", "den", "rech\u00b7ten", "Bo\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "G\u00f6tter, f\u00fcr das Schattenland?", "tokens": ["G\u00f6t\u00b7ter", ",", "f\u00fcr", "das", "Schat\u00b7ten\u00b7land", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wer ist w\u00fcrdig, solche M\u00e4r zu bringen:", "tokens": ["Wer", "ist", "w\u00fcr\u00b7dig", ",", "sol\u00b7che", "M\u00e4r", "zu", "brin\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADJD", "$,", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "\u00bbaufgestanden sind die S\u00f6hne Teuts,", "tokens": ["\u00bb", "auf\u00b7ge\u00b7stan\u00b7den", "sind", "die", "S\u00f6h\u00b7ne", "Teuts", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "VAFIN", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Millionen Stimmen klingen:", "tokens": ["Mil\u00b7lion\u00b7en", "Stim\u00b7men", "klin\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auch der Donner klingt's des Streits.\u00ab", "tokens": ["Auch", "der", "Don\u00b7ner", "klingt's", "des", "Streits", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wer mag Hermann seine Rechte reichen", "tokens": ["Wer", "mag", "Her\u00b7mann", "sei\u00b7ne", "Rech\u00b7te", "rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "NE", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und der V\u00e4ter Angesichter schaun?", "tokens": ["Und", "der", "V\u00e4\u00b7ter", "An\u00b7ge\u00b7sich\u00b7ter", "schaun", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wahrlich keine von den bleichen", "tokens": ["Wahr\u00b7lich", "kei\u00b7ne", "von", "den", "blei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seelen, die vor jedem Sturmwind streichen,", "tokens": ["See\u00b7len", ",", "die", "vor", "je\u00b7dem", "Sturm\u00b7wind", "strei\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Die zermalmte schier das Graun.", "tokens": ["Die", "zer\u00b7malm\u00b7te", "schier", "das", "Graun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nur ein Held mag Helden Botschaft tragen,", "tokens": ["Nur", "ein", "Held", "mag", "Hel\u00b7den", "Bot\u00b7schaft", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VMFIN", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Darum mu\u00df Germaniens bester Mann,", "tokens": ["Da\u00b7rum", "mu\u00df", "Ger\u00b7ma\u00b7ni\u00b7ens", "bes\u00b7ter", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Scharnhorst mu\u00df die Botschaft tragen:", "tokens": ["Scharn\u00b7horst", "mu\u00df", "die", "Bot\u00b7schaft", "tra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Heil dir, edler Bote! Hohe Weihe", "tokens": ["Heil", "dir", ",", "ed\u00b7ler", "Bo\u00b7te", "!", "Ho\u00b7he", "Wei\u00b7he"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "PPER", "$,", "ADJA", "NN", "$.", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Gibt dein Gang dem deutschen Waffenspiel:", "tokens": ["Gibt", "dein", "Gang", "dem", "deut\u00b7schen", "Waf\u00b7fen\u00b7spiel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Jeder wird ein Held in Treue,", "tokens": ["Je\u00b7der", "wird", "ein", "Held", "in", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jeder wird f\u00fcrs Vaterland ein Leue,", "tokens": ["Je\u00b7der", "wird", "f\u00fcrs", "Va\u00b7ter\u00b7land", "ein", "Leu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Wann ein solcher blutig fiel.", "tokens": ["Wann", "ein", "sol\u00b7cher", "blu\u00b7tig", "fiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "PIAT", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Heil dir, edler Bote! M\u00e4nnerspiegel,", "tokens": ["Heil", "dir", ",", "ed\u00b7ler", "Bo\u00b7te", "!", "M\u00e4n\u00b7ner\u00b7spie\u00b7gel", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "$,", "ADJA", "NN", "$.", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Biedermann aus alter, deutscher Zeit!", "tokens": ["Bie\u00b7der\u00b7mann", "aus", "al\u00b7ter", ",", "deut\u00b7scher", "Zeit", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Herrlich gr\u00fcnt dein Grabesh\u00fcgel,", "tokens": ["Herr\u00b7lich", "gr\u00fcnt", "dein", "Gra\u00b7bes\u00b7h\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Ruhm schl\u00e4gt seine goldnen Fl\u00fcgel", "tokens": ["Und", "der", "Ruhm", "schl\u00e4gt", "sei\u00b7ne", "gold\u00b7nen", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Um ihn bis in Ewigkeit.", "tokens": ["Um", "ihn", "bis", "in", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und er steht uns wie ein heil'ges Zeichen,", "tokens": ["Und", "er", "steht", "uns", "wie", "ein", "heil'\u00b7ges", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wie ein hohes, festes G\u00f6tterpfand,", "tokens": ["Wie", "ein", "ho\u00b7hes", ",", "fes\u00b7tes", "G\u00f6t\u00b7ter\u00b7pfand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Da\u00df die Schande wird entweichen", "tokens": ["Da\u00df", "die", "Schan\u00b7de", "wird", "ent\u00b7wei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VAFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus dem Vaterlande gr\u00fcner Eichen,", "tokens": ["Aus", "dem", "Va\u00b7ter\u00b7lan\u00b7de", "gr\u00fc\u00b7ner", "Ei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Aus dem heil'gen deutschen Land.", "tokens": ["Aus", "dem", "heil'\u00b7gen", "deut\u00b7schen", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Wann einst fromme Herzen deutsch sich finden,", "tokens": ["Wann", "einst", "from\u00b7me", "Her\u00b7zen", "deutsch", "sich", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJA", "NN", "ADJD", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ohne Eide mit dem H\u00e4ndedruck", "tokens": ["Oh\u00b7ne", "Ei\u00b7de", "mit", "dem", "H\u00e4n\u00b7de\u00b7druck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Werden hier sie Treue binden,", "tokens": ["Wer\u00b7den", "hier", "sie", "Treu\u00b7e", "bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Br\u00e4uten, welche Hochzeitkr\u00e4nze winden,", "tokens": ["Br\u00e4u\u00b7ten", ",", "wel\u00b7che", "Hoch\u00b7zeit\u00b7kr\u00e4n\u00b7ze", "win\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Bl\u00fchet hier der Ehrenschmuck.", "tokens": ["Bl\u00fc\u00b7het", "hier", "der", "Eh\u00b7ren\u00b7schmuck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Wann sich M\u00e4nner n\u00e4chtlich still verschw\u00f6ren", "tokens": ["Wann", "sich", "M\u00e4n\u00b7ner", "n\u00e4cht\u00b7lich", "still", "ver\u00b7schw\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "NN", "ADV", "ADJD", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Gegen Lug und Vaterlandsverrat,", "tokens": ["Ge\u00b7gen", "Lug", "und", "Va\u00b7ter\u00b7lands\u00b7ver\u00b7rat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Gegen Gaukler, die bet\u00f6ren,", "tokens": ["Ge\u00b7gen", "Gauk\u00b7ler", ",", "die", "be\u00b7t\u00f6\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gegen Memmen, welche Knechtschaft lehren,", "tokens": ["Ge\u00b7gen", "Mem\u00b7men", ",", "wel\u00b7che", "Knecht\u00b7schaft", "leh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Hieher lenken sie den Pfad.", "tokens": ["Hie\u00b7her", "len\u00b7ken", "sie", "den", "Pfad", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Will der Vater seinen Sohn bewehren,", "tokens": ["Will", "der", "Va\u00b7ter", "sei\u00b7nen", "Sohn", "be\u00b7weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Hieher f\u00fchrt er ihn im Abendschein,", "tokens": ["Hie\u00b7her", "f\u00fchrt", "er", "ihn", "im", "A\u00b7bend\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Hei\u00dft ihn knien, hei\u00dft ihn schw\u00f6ren,", "tokens": ["Hei\u00dft", "ihn", "kni\u00b7en", ",", "hei\u00dft", "ihn", "schw\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Treu des Vaterlandes heil'gen Ehren,", "tokens": ["Treu", "des", "Va\u00b7ter\u00b7lan\u00b7des", "heil'\u00b7gen", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Treu bis in den Tod zu sein.", "tokens": ["Treu", "bis", "in", "den", "Tod", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "So bl\u00fcht Tugend aus der Tugend Samen", "tokens": ["So", "bl\u00fcht", "Tu\u00b7gend", "aus", "der", "Tu\u00b7gend", "Sa\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "APPR", "ART", "NN", "NN"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Herrlich durch die Zeiten ohne Ziel;", "tokens": ["Herr\u00b7lich", "durch", "die", "Zei\u00b7ten", "oh\u00b7ne", "Ziel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Buben zittern bei dem Namen,", "tokens": ["Bu\u00b7ben", "zit\u00b7tern", "bei", "dem", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Edle rufen ", "tokens": ["Ed\u00b7le", "ru\u00b7fen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "F\u00fcr das gl\u00e4ubigste Gef\u00fchl.", "tokens": ["F\u00fcr", "das", "gl\u00e4u\u00b7bigs\u00b7te", "Ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.12": {"line.1": {"text": "Wen erlest ihr f\u00fcr die gro\u00dfen Toten,", "tokens": ["Wen", "er\u00b7lest", "ihr", "f\u00fcr", "die", "gro\u00b7\u00dfen", "To\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die einst ritterlich f\u00fcrs deutsche Land", "tokens": ["Die", "einst", "rit\u00b7ter\u00b7lich", "f\u00fcrs", "deut\u00b7sche", "Land"], "token_info