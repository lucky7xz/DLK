{"dta.poem.590": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An das veraltete Frauen-Zimmer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Darf ich an eure Noht/ ihr lieben Kinder/ dencken?", "tokens": ["Darf", "ich", "an", "eu\u00b7re", "Noht", "/", "ihr", "lie\u00b7ben", "Kin\u00b7der", "/", "den\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN", "$(", "PPOSAT", "ADJA", "NN", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie ihr eur Ungel\u00fcck mit Thr\u00e4nen offt beklagt/", "tokens": ["Wie", "ihr", "eur", "Un\u00b7ge\u00b7l\u00fcck", "mit", "Thr\u00e4\u00b7nen", "offt", "be\u00b7klagt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "APPR", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So mu\u00df sich mein Gem\u00fcth zu tieffer Wehmuth leucken/", "tokens": ["So", "mu\u00df", "sich", "mein", "Ge\u00b7m\u00fcth", "zu", "tief\u00b7fer", "Weh\u00b7muth", "leu\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PRF", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Pflicht und Schuldigkeit den Frauens nichts versagt.", "tokens": ["Die", "Pflicht", "und", "Schul\u00b7dig\u00b7keit", "den", "Frau\u00b7ens", "nichts", "ver\u00b7sagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ART", "NN", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ach k\u00f6nnt ihr M\u00e4nner kriegen!", "tokens": ["Ach", "k\u00f6nnt", "ihr", "M\u00e4n\u00b7ner", "krie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nach eurem Wunsch und Sinn/", "tokens": ["Nach", "eu\u00b7rem", "Wunsch", "und", "Sinn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich g\u00f6nn euch das Vergn\u00fcgen", "tokens": ["Ich", "g\u00f6nn", "euch", "das", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wenn ich es nur nicht bin.", "tokens": ["Wenn", "ich", "es", "nur", "nicht", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "PTKNEG", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Jahre lauffen fort/ die Zeiten werden schlimmer/", "tokens": ["Die", "Jah\u00b7re", "lauf\u00b7fen", "fort", "/", "die", "Zei\u00b7ten", "wer\u00b7den", "schlim\u00b7mer", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$(", "ART", "NN", "VAFIN", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Freyers gibt es viel/ der Nehmer aber nicht/", "tokens": ["Der", "Frey\u00b7ers", "gibt", "es", "viel", "/", "der", "Neh\u00b7mer", "a\u00b7ber", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$(", "ART", "NN", "ADV", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verfluchtes Ungemach vor altes Frauen-Zimmer/", "tokens": ["Ver\u00b7fluch\u00b7tes", "Un\u00b7ge\u00b7mach", "vor", "al\u00b7tes", "Frau\u00b7en\u00b7Zim\u00b7mer", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem aus Verzweiffelung fast aller Trost gebricht.", "tokens": ["Dem", "aus", "Ver\u00b7zweif\u00b7fe\u00b7lung", "fast", "al\u00b7ler", "Trost", "ge\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NN", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Man schaut sie nur von ferne", "tokens": ["Man", "schaut", "sie", "nur", "von", "fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "APPR", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Als einen Jrwisch an.", "tokens": ["Als", "ei\u00b7nen", "Jr\u00b7wisch", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sie sind wie kleine Sterne/", "tokens": ["Sie", "sind", "wie", "klei\u00b7ne", "Ster\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die man kaum sehen kan.", "tokens": ["Die", "man", "kaum", "se\u00b7hen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "VVINF", "VMFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Zum \u00f6fftern sagt ihr zwar ihr seyd von achtzehn Jahren/", "tokens": ["Zum", "\u00f6ff\u00b7tern", "sagt", "ihr", "zwar", "ihr", "seyd", "von", "acht\u00b7zehn", "Jah\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "VVINF", "VVFIN", "PPER", "ADV", "PPER", "VAFIN", "APPR", "CARD", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da \u00fcber drey\u00dfig/ man euch an der Stirnen z\u00e4hlt/", "tokens": ["Da", "\u00fc\u00b7ber", "drey\u00b7\u00dfig", "/", "man", "euch", "an", "der", "Stir\u00b7nen", "z\u00e4hlt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "CARD", "$(", "PIS", "PPER", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ach dieses hilfft euch nicht/ es sind verlegne Waaren/", "tokens": ["Ach", "die\u00b7ses", "hilfft", "euch", "nicht", "/", "es", "sind", "ver\u00b7leg\u00b7ne", "Waa\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PDS", "VVFIN", "PPER", "PTKNEG", "$(", "PPER", "VAFIN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was hilfft ein sch\u00f6nes Kleid? wenn es am Ansehn f\u00e4hlt.", "tokens": ["Was", "hilfft", "ein", "sch\u00f6\u00b7nes", "Kleid", "?", "wenn", "es", "am", "An\u00b7sehn", "f\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "$.", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was jetzt die Leute kauffen/", "tokens": ["Was", "jetzt", "die", "Leu\u00b7te", "kauf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das mu\u00df was frisches seyn/", "tokens": ["Das", "mu\u00df", "was", "fri\u00b7sches", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PWS", "ADJA", "VAINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wer steht doch jemand lauffen", "tokens": ["Wer", "steht", "doch", "je\u00b7mand", "lauf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "PIS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nach halb verrauchten Wein?", "tokens": ["Nach", "halb", "ver\u00b7rauch\u00b7ten", "Wein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Die Sch\u00f6nheit will nicht mehr auf euren Wangen scheinen/", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "will", "nicht", "mehr", "auf", "eu\u00b7ren", "Wan\u00b7gen", "schei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Fr\u00fchling eurer Zeit ist leider gantz vorbey/", "tokens": ["Der", "Fr\u00fch\u00b7ling", "eu\u00b7rer", "Zeit", "ist", "lei\u00b7der", "gantz", "vor\u00b7bey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAFIN", "ADV", "ADV", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Tod sitzt in der Schoo\u00df/ das Alter in den Beinen/", "tokens": ["Der", "Tod", "sitzt", "in", "der", "Schoo\u00df", "/", "das", "Al\u00b7ter", "in", "den", "Bei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$(", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ach da\u00df ein Jubel-Jahr vor euch zu hoffen sey!", "tokens": ["Ach", "da\u00df", "ein", "Ju\u00b7bel\u00b7Jahr", "vor", "euch", "zu", "hof\u00b7fen", "sey", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "ART", "NN", "APPR", "PPER", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Drum seyd ihr \u00fcberladen/", "tokens": ["Drum", "seyd", "ihr", "\u00fc\u00b7berl\u00b7a\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit Senffzern f\u00fcr und f\u00fcr/", "tokens": ["Mit", "Senff\u00b7zern", "f\u00fcr", "und", "f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "KON", "APPR", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und sprecht: Vor unsern Schaden/", "tokens": ["Und", "sprecht", ":", "Vor", "un\u00b7sern", "Scha\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ist gar kein Pflaster hier.", "tokens": ["Ist", "gar", "kein", "Pflas\u00b7ter", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "So gehts/ wenn man zu sehr auf seine Jugend bauet/", "tokens": ["So", "gehts", "/", "wenn", "man", "zu", "sehr", "auf", "sei\u00b7ne", "Ju\u00b7gend", "bau\u00b7et", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "KOUS", "PIS", "APPR", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So gehts/ wenn man so offt im Freyen hat gew\u00e4hlt/", "tokens": ["So", "gehts", "/", "wenn", "man", "so", "offt", "im", "Frey\u00b7en", "hat", "ge\u00b7w\u00e4hlt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "KOUS", "PIS", "ADV", "ADV", "APPRART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein Sch\u00fctze/ der ihm stets zu treffen zugetrauet/", "tokens": ["Ein", "Sch\u00fct\u00b7ze", "/", "der", "ihm", "stets", "zu", "tref\u00b7fen", "zu\u00b7ge\u00b7trau\u00b7et", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "PPER", "ADV", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hat endlich doch zuletzt der Scheiben wol verf\u00e4hlt.", "tokens": ["Hat", "end\u00b7lich", "doch", "zu\u00b7letzt", "der", "Schei\u00b7ben", "wol", "ver\u00b7f\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADV", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jhr strebt nach hohen Dingen/", "tokens": ["Ihr", "strebt", "nach", "ho\u00b7hen", "Din\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Jhr wolt zu hoch hinan/", "tokens": ["Ihr", "wolt", "zu", "hoch", "hi\u00b7nan", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKA", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und fallt mit hohen Spr\u00fcngen", "tokens": ["Und", "fallt", "mit", "ho\u00b7hen", "Spr\u00fcn\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Merckt/ wie man st\u00fcrtzen kan.", "tokens": ["Merckt", "/", "wie", "man", "st\u00fcrt\u00b7zen", "kan", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "PWAV", "PIS", "VVINF", "VMFIN", "$."], "meter": "++-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Seyd ihr noch jung und sch\u00f6n/ auch artig von Gebehrden/", "tokens": ["Seyd", "ihr", "noch", "jung", "und", "sch\u00f6n", "/", "auch", "ar\u00b7tig", "von", "Ge\u00b7behr\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "ADV", "ADJD", "KON", "ADJD", "$(", "ADV", "ADJD", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wolt ihr da\u00df man euch verehret \u00fcberall", "tokens": ["So", "wolt", "ihr", "da\u00df", "man", "euch", "ver\u00b7eh\u00b7ret", "\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "KOUS", "PIS", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als Kronen unser Zeit/ als Engel dieser Erden/", "tokens": ["Als", "Kro\u00b7nen", "un\u00b7ser", "Zeit", "/", "als", "En\u00b7gel", "die\u00b7ser", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPOSAT", "NN", "$(", "KOUS", "NN", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Hoffarth ist allzeit der Bothe f\u00fcr dem Fall;", "tokens": ["Der", "Hof\u00b7farth", "ist", "all\u00b7zeit", "der", "Bo\u00b7the", "f\u00fcr", "dem", "Fall", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Rosen sind vergangen/", "tokens": ["Die", "Ro\u00b7sen", "sind", "ver\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Sommer ist vorbey/", "tokens": ["Der", "Som\u00b7mer", "ist", "vor\u00b7bey", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Jetzt zeigen eure Wangen/", "tokens": ["Jetzt", "zei\u00b7gen", "eu\u00b7re", "Wan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Da\u00df/ da der Winter sey.", "tokens": ["Da\u00df", "/", "da", "der", "Win\u00b7ter", "sey", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "KOUS", "ART", "NN", "VAFIN", "$."], "me