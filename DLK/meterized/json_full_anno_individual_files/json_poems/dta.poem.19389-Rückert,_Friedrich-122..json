{"dta.poem.19389": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "122.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bei einem Lehrer ist von Sch\u00fclern eine Gilde,", "tokens": ["Bei", "ei\u00b7nem", "Leh\u00b7rer", "ist", "von", "Sch\u00fc\u00b7lern", "ei\u00b7ne", "Gil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die unterweiset er in Gottesfurcht und Milde.", "tokens": ["Die", "un\u00b7ter\u00b7wei\u00b7set", "er", "in", "Got\u00b7tes\u00b7furcht", "und", "Mil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Er weist zu Gottesfurcht und Milde nur sie an,", "tokens": ["Er", "weist", "zu", "Got\u00b7tes\u00b7furcht", "und", "Mil\u00b7de", "nur", "sie", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "ADV", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch einer eilt voraus den andern auf der Bahn.", "tokens": ["Doch", "ei\u00b7ner", "eilt", "vo\u00b7raus", "den", "an\u00b7dern", "auf", "der", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADV", "ART", "ADJA", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Am allerj\u00fcngsten hat der Meister Wohlgefallen,", "tokens": ["Am", "al\u00b7ler\u00b7j\u00fcng\u00b7sten", "hat", "der", "Meis\u00b7ter", "Wohl\u00b7ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil er ihn sieht im Geist voran den andern wallen.", "tokens": ["Weil", "er", "ihn", "sieht", "im", "Geist", "vo\u00b7ran", "den", "an\u00b7dern", "wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "APPRART", "NN", "ADV", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die andern aber, die voran im Alter gehn,", "tokens": ["Die", "an\u00b7dern", "a\u00b7ber", ",", "die", "vo\u00b7ran", "im", "Al\u00b7ter", "gehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "$,", "PRELS", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie fragen sich, warum ihr Meister vorzieht den?", "tokens": ["Sie", "fra\u00b7gen", "sich", ",", "wa\u00b7rum", "ihr", "Meis\u00b7ter", "vor\u00b7zieht", "den", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "ART", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "Warum uns \u00e4ltern ihn, den j\u00fcngsten, ziehst du vor?", "tokens": ["Wa\u00b7rum", "uns", "\u00e4l\u00b7tern", "ihn", ",", "den", "j\u00fcng\u00b7sten", ",", "ziehst", "du", "vor", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "PPER", "$,", "ART", "ADJA", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er sprach: Ich sag' es euch, doch thut mir dis zuvor:", "tokens": ["Er", "sprach", ":", "Ich", "sag'", "es", "euch", ",", "doch", "thut", "mir", "dis", "zu\u00b7vor", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "PPER", "$,", "ADV", "VVFIN", "PPER", "PDS", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Von diesen V\u00f6gelein (er nahm sie aus dem Neste)", "tokens": ["Von", "die\u00b7sen", "V\u00f6\u00b7ge\u00b7lein", "(", "er", "nahm", "sie", "aus", "dem", "Nes\u00b7te", ")"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$(", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nehmt jeder eins zur Hand, und geht damit aufs beste", "tokens": ["Nehmt", "je\u00b7der", "eins", "zur", "Hand", ",", "und", "geht", "da\u00b7mit", "aufs", "bes\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "PIS", "APPRART", "NN", "$,", "KON", "VVFIN", "PAV", "APPRART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Hinaus an einen Ort, da wo euch sieht kein Blick;", "tokens": ["Hin\u00b7aus", "an", "ei\u00b7nen", "Ort", ",", "da", "wo", "euch", "sieht", "kein", "Blick", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "ADV", "PWAV", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erw\u00fcrgt die V\u00f6gel dort, und bringt sie her zur\u00fcck. \u2014", "tokens": ["Er\u00b7w\u00fcrgt", "die", "V\u00f6\u00b7gel", "dort", ",", "und", "bringt", "sie", "her", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$,", "KON", "VVFIN", "PPER", "ADV", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Sie gehn, und bringen dann die todten ohne Beben,", "tokens": ["Sie", "gehn", ",", "und", "brin\u00b7gen", "dann", "die", "tod\u00b7ten", "oh\u00b7ne", "Be\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "ADV", "ART", "ADJA", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als sollt', ein Wundermann, der Meister sie beleben.", "tokens": ["Als", "sollt'", ",", "ein", "Wun\u00b7der\u00b7mann", ",", "der", "Meis\u00b7ter", "sie", "be\u00b7le\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "$,", "ART", "NN", "$,", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Der j\u00fcngste aber bringt sein V\u00f6gelein lebendig;", "tokens": ["Der", "j\u00fcngs\u00b7te", "a\u00b7ber", "bringt", "sein", "V\u00f6\u00b7ge\u00b7lein", "le\u00b7ben\u00b7dig", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "VVFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was w\u00fcrgtest du es nicht? Er sprach darauf verst\u00e4ndig:", "tokens": ["Was", "w\u00fcrg\u00b7test", "du", "es", "nicht", "?", "Er", "sprach", "da\u00b7rauf", "ver\u00b7st\u00e4n\u00b7dig", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PPER", "PTKNEG", "$.", "PPER", "VVFIN", "PAV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Weil ich den Ort nicht fand, o Meister, welchen du", "tokens": ["Weil", "ich", "den", "Ort", "nicht", "fand", ",", "o", "Meis\u00b7ter", ",", "wel\u00b7chen", "du"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "$,", "FM", "NN", "$,", "PWAT", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mich suchen hie\u00dfest, da kein Blick mir s\u00e4he zu.", "tokens": ["Mich", "su\u00b7chen", "hie\u00b7\u00dfest", ",", "da", "kein", "Blick", "mir", "s\u00e4\u00b7he", "zu", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PIAT", "NN", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Ein Blick sieht \u00fcberall, er sieht aufs Leben nieder,", "tokens": ["Ein", "Blick", "sieht", "\u00fc\u00b7be\u00b7rall", ",", "er", "sieht", "aufs", "Le\u00b7ben", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie meins, des V\u00f6geleins; drum bring' ichs lebend wieder. \u2014", "tokens": ["Wie", "meins", ",", "des", "V\u00f6\u00b7ge\u00b7leins", ";", "drum", "bring'", "ichs", "le\u00b7bend", "wie\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PIS", "$,", "ART", "NN", "$.", "PAV", "VVFIN", "PIS", "ADJD", "ADV", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Der Meister sah sich um, die Sch\u00fcler waren stumm;", "tokens": ["Der", "Meis\u00b7ter", "sah", "sich", "um", ",", "die", "Sch\u00fc\u00b7ler", "wa\u00b7ren", "stumm", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKVZ", "$,", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den j\u00fcngsten zog er vor, nun wu\u00dften sie, warum. \u2014", "tokens": ["Den", "j\u00fcng\u00b7sten", "zog", "er", "vor", ",", "nun", "wu\u00df\u00b7ten", "sie", ",", "wa\u00b7rum", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "$,", "PWAV", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Die todten V\u00f6gelein setzt' er zur\u00fcck ins Nest,", "tokens": ["Die", "tod\u00b7ten", "V\u00f6\u00b7ge\u00b7lein", "setzt'", "er", "zu\u00b7r\u00fcck", "ins", "Nest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "APPRART", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ums lebende herum, und dr\u00fcckte sanft sie fest.", "tokens": ["Ums", "le\u00b7ben\u00b7de", "he\u00b7rum", ",", "und", "dr\u00fcck\u00b7te", "sanft", "sie", "fest", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "PTKVZ", "$,", "KON", "VVFIN", "ADJD", "PPER", "PTKVZ", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.14": {"line.1": {"text": "Vom Wunderhauch der Huld sind sie lebendig worden;", "tokens": ["Vom", "Wun\u00b7der\u00b7hauch", "der", "Huld", "sind", "sie", "le\u00b7ben\u00b7dig", "wor\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VAFIN", "PPER", "ADJD", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Beleben kann der Herr, doch soll der Mensch nicht morden.", "tokens": ["Be\u00b7le\u00b7ben", "kann", "der", "Herr", ",", "doch", "soll", "der", "Mensch", "nicht", "mor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVINF", "VMFIN", "ART", "NN", "$,", "ADV", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}