{"dta.poem.21544": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "S\u00fchne.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Liebe ist Thorheit; viel hab\u2019 ich erfahren,", "tokens": ["Lie\u00b7be", "ist", "Thor\u00b7heit", ";", "viel", "hab'", "ich", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$.", "ADV", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Es giebt kein Weib, das minnend ich ersehne . . . .", "tokens": ["Es", "giebt", "kein", "Weib", ",", "das", "min\u00b7nend", "ich", "er\u00b7seh\u00b7ne", ".", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "PRELS", "VVPP", "PPER", "VVFIN", "$.", "$.", "$.", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da kommst du auf den Wangen Thr\u00e4n\u2019 um Thr\u00e4ne,", "tokens": ["Da", "kommst", "du", "auf", "den", "Wan\u00b7gen", "Thr\u00e4n'", "um", "Thr\u00e4\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein reuig Weib zur\u00fcck zu meinen Laren.", "tokens": ["Ein", "reu\u00b7ig", "Weib", "zu\u00b7r\u00fcck", "zu", "mei\u00b7nen", "La\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "PTKVZ", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du kehrst zur\u00fcck \u2014 so kam vor tausend Jahren", "tokens": ["Du", "kehrst", "zu\u00b7r\u00fcck", "so", "kam", "vor", "tau\u00b7send", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$(", "ADV", "VVFIN", "APPR", "CARD", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zu Menelaus wohl die reuige Helene \u2014", "tokens": ["Zu", "Me\u00b7ne\u00b7laus", "wohl", "die", "reu\u00b7i\u00b7ge", "He\u00b7le\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und sinkest gleich der B\u00fc\u00df\u2019rin Magdalene", "tokens": ["Und", "sin\u00b7kest", "gleich", "der", "B\u00fc\u00df'\u00b7rin", "Mag\u00b7da\u00b7le\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu F\u00fc\u00dfen mir mit losgel\u00f6sten Haaren.", "tokens": ["Zu", "F\u00fc\u00b7\u00dfen", "mir", "mit", "los\u00b7ge\u00b7l\u00f6s\u00b7ten", "Haa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O Weib steh\u2019 auf, soviel du auch verschuldet,", "tokens": ["O", "Weib", "steh'", "auf", ",", "so\u00b7viel", "du", "auch", "ver\u00b7schul\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "PTKVZ", "$,", "KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Soviel dies arme Hirn um dich geduldet,", "tokens": ["So\u00b7viel", "dies", "ar\u00b7me", "Hirn", "um", "dich", "ge\u00b7dul\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PDS", "ADJA", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df ich auf\u2019s Neu\u2019 dir s\u00fc\u00dfe Liebe k\u00fcnde,", "tokens": ["Da\u00df", "ich", "auf's", "Neu'", "dir", "s\u00fc\u00b7\u00dfe", "Lie\u00b7be", "k\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PPER", "ADJA", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Steh\u2019 auf! La\u00df liebend dich auf\u2019s Neu\u2019 umfassen,", "tokens": ["Steh'", "auf", "!", "La\u00df", "lie\u00b7bend", "dich", "auf's", "Neu'", "um\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$.", "VVIMP", "ADJD", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vor deinem Blick zerschmilzt mein Zorn, mein Hassen,", "tokens": ["Vor", "dei\u00b7nem", "Blick", "zer\u00b7schmilzt", "mein", "Zorn", ",", "mein", "Has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und deine Sch\u00f6nheit s\u00fchnet deine S\u00fcnde.", "tokens": ["Und", "dei\u00b7ne", "Sch\u00f6n\u00b7heit", "s\u00fch\u00b7net", "dei\u00b7ne", "S\u00fcn\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}