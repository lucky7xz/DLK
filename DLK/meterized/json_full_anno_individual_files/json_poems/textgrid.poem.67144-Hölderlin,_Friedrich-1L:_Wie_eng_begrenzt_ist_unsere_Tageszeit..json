{"textgrid.poem.67144": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie eng begrenzt ist unsere Tageszeit.", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie eng begrenzt ist unsere Tageszeit.", "tokens": ["Wie", "eng", "be\u00b7grenzt", "ist", "un\u00b7se\u00b7re", "Ta\u00b7ges\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVPP", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Du warst und sahst und stauntest, schon Abend ists,", "tokens": ["Du", "warst", "und", "sahst", "und", "staun\u00b7test", ",", "schon", "A\u00b7bend", "ists", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KON", "VVFIN", "KON", "VVFIN", "$,", "ADV", "NN", "VAFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nun schlafe, wo unendlich ferne", "tokens": ["Nun", "schla\u00b7fe", ",", "wo", "un\u00b7end\u00b7lich", "fer\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "ADJD", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ziehen vor\u00fcber der V\u00f6lker Jahre.", "tokens": ["Zie\u00b7hen", "vor\u00b7\u00fc\u00b7ber", "der", "V\u00f6l\u00b7ker", "Jah\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Und mancher siehet \u00fcber die eigne Zeit,", "tokens": ["Und", "man\u00b7cher", "sie\u00b7het", "\u00fc\u00b7ber", "die", "eig\u00b7ne", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ihm zeigt ein Gott ins Freie, doch sehnend stehst", "tokens": ["Ihm", "zeigt", "ein", "Gott", "ins", "Frei\u00b7e", ",", "doch", "seh\u00b7nend", "stehst"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Am Ufer du, ein \u00c4rgernis den", "tokens": ["Am", "U\u00b7fer", "du", ",", "ein", "\u00c4r\u00b7ger\u00b7nis", "den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "PPER", "$,", "ART", "NN", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Deinen, ein Schatten, und liebst sie nimmer,", "tokens": ["Dei\u00b7nen", ",", "ein", "Schat\u00b7ten", ",", "und", "liebst", "sie", "nim\u00b7mer", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "$,", "ART", "NN", "$,", "KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Und jene, die du nennst, die Verhei\u00dfenen,", "tokens": ["Und", "je\u00b7ne", ",", "die", "du", "nennst", ",", "die", "Ver\u00b7hei\u00b7\u00dfe\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PDS", "$,", "PRELS", "PPER", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wo sind die Neuen, da\u00df du an Freundeshand", "tokens": ["Wo", "sind", "die", "Neu\u00b7en", ",", "da\u00df", "du", "an", "Freun\u00b7des\u00b7hand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$,", "KOUS", "PPER", "APPR", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Erwarmst, wo nahn sie, da\u00df du einmal,", "tokens": ["Er\u00b7warmst", ",", "wo", "nahn", "sie", ",", "da\u00df", "du", "ein\u00b7mal", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Einsame Rede, vernehmlich seiest?", "tokens": ["Ein\u00b7sa\u00b7me", "Re\u00b7de", ",", "ver\u00b7nehm\u00b7lich", "sei\u00b7est", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "VAFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Klanglos ists, armer Mann, in der Halle dir,", "tokens": ["Klang\u00b7los", "ists", ",", "ar\u00b7mer", "Mann", ",", "in", "der", "Hal\u00b7le", "dir", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,", "ADJA", "NN", "$,", "APPR", "ART", "NN", "PPER", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Und gleich den Unbegrabenen, irrest du", "tokens": ["Und", "gleich", "den", "Un\u00b7be\u00b7gra\u00b7be\u00b7nen", ",", "ir\u00b7rest", "du"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "$,", "VVFIN", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Unst\u00e4t und suchest Ruh und niemand", "tokens": ["Un\u00b7st\u00e4t", "und", "su\u00b7chest", "Ruh", "und", "nie\u00b7mand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "VVFIN", "NN", "KON", "PIS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wei\u00df den beschiedenen Weg zu weisen.", "tokens": ["Wei\u00df", "den", "be\u00b7schie\u00b7de\u00b7nen", "Weg", "zu", "wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Sei denn zufrieden! der Baum entw\u00e4chst", "tokens": ["Sei", "denn", "zu\u00b7frie\u00b7den", "!", "der", "Baum", "ent\u00b7w\u00e4chst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADJD", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dem heimatlichen Boden, aber es sinken ihm", "tokens": ["Dem", "hei\u00b7mat\u00b7li\u00b7chen", "Bo\u00b7den", ",", "a\u00b7ber", "es", "sin\u00b7ken", "ihm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die liebenden, die jugendlichen", "tokens": ["Die", "lie\u00b7ben\u00b7den", ",", "die", "ju\u00b7gend\u00b7li\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Arme, und trauernd neigt er sein Haupt.", "tokens": ["Ar\u00b7me", ",", "und", "trau\u00b7ernd", "neigt", "er", "sein", "Haupt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.6": {"line.1": {"text": "Des Lebens \u00dcberflu\u00df, das Unendliche,", "tokens": ["Des", "Le\u00b7bens", "\u00dc\u00b7berf\u00b7lu\u00df", ",", "das", "Un\u00b7end\u00b7li\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das um ihn und d\u00e4mmert, er fa\u00dft es nie.", "tokens": ["Das", "um", "ihn", "und", "d\u00e4m\u00b7mert", ",", "er", "fa\u00dft", "es", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPER", "KON", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Doch lebts in ihm und gegenw\u00e4rtig,", "tokens": ["Doch", "lebts", "in", "ihm", "und", "ge\u00b7gen\u00b7w\u00e4r\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "W\u00e4rmend und wirkend, die Frucht entquillt ihm.", "tokens": ["W\u00e4r\u00b7mend", "und", "wir\u00b7kend", ",", "die", "Frucht", "ent\u00b7quillt", "ihm", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Du hast gelebt! auch dir, auch dir", "tokens": ["Du", "hast", "ge\u00b7lebt", "!", "auch", "dir", ",", "auch", "dir"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "$.", "ADV", "PPER", "$,", "ADV", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erfreuet die ferne Sonne dein Haupt,", "tokens": ["Er\u00b7freu\u00b7et", "die", "fer\u00b7ne", "Son\u00b7ne", "dein", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und Strahlen aus der sch\u00f6nern Zeit. Es", "tokens": ["Und", "Strah\u00b7len", "aus", "der", "sch\u00f6\u00b7nern", "Zeit", ".", "Es"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "NN", "APPR", "ART", "ADJA", "NN", "$.", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Haben die Boten dein Herz gefunden.", "tokens": ["Ha\u00b7ben", "die", "Bo\u00b7ten", "dein", "Herz", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Vernommen hast du sie, verstanden die Sprache der Fremdlinge,", "tokens": ["Ver\u00b7nom\u00b7men", "hast", "du", "sie", ",", "ver\u00b7stan\u00b7den", "die", "Spra\u00b7che", "der", "Fremd\u00b7lin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "PPER", "$,", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Gedeutet ihre Seele! Dem Sehnenden war", "tokens": ["Ge\u00b7deu\u00b7tet", "ih\u00b7re", "See\u00b7le", "!", "Dem", "Seh\u00b7nen\u00b7den", "war"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "ART", "NN", "VAFIN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Wink genug, und Winke sind", "tokens": ["Der", "Wink", "ge\u00b7nug", ",", "und", "Win\u00b7ke", "sind"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "KON", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von alters her die Sprache der G\u00f6tter.", "tokens": ["Von", "al\u00b7ters", "her", "die", "Spra\u00b7che", "der", "G\u00f6t\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Und wunderbar, als h\u00e4tte von Anbeginn", "tokens": ["Und", "wun\u00b7der\u00b7bar", ",", "als", "h\u00e4t\u00b7te", "von", "An\u00b7be\u00b7ginn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "KOKOM", "VAFIN", "APPR", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des Menschen Geist das Werden und Wirken all,", "tokens": ["Des", "Men\u00b7schen", "Geist", "das", "Wer\u00b7den", "und", "Wir\u00b7ken", "all", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ART", "NN", "KON", "NN", "PIAT", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Des Lebens Weise schon erfahren,", "tokens": ["Des", "Le\u00b7bens", "Wei\u00b7se", "schon", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Kennt er im ersten Zeichen Vollendetes schon,", "tokens": ["Kennt", "er", "im", "ers\u00b7ten", "Zei\u00b7chen", "Voll\u00b7en\u00b7de\u00b7tes", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "NN", "ADV", "$,"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Und fliegt, der k\u00fchne Geist, wie Adler den", "tokens": ["Und", "fliegt", ",", "der", "k\u00fch\u00b7ne", "Geist", ",", "wie", "Ad\u00b7ler", "den"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "ART", "ADJA", "NN", "$,", "PWAV", "NN", "ART"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gewittern, weissagend seinen", "tokens": ["Ge\u00b7wit\u00b7tern", ",", "weis\u00b7sa\u00b7gend", "sei\u00b7nen"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPOSAT"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Kommenden G\u00f6ttern voraus,", "tokens": ["Kom\u00b7men\u00b7den", "G\u00f6t\u00b7tern", "vo\u00b7raus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.11": {"line.1": {"text": "Wie eng begrenzt ist unsere Tageszeit.", "tokens": ["Wie", "eng", "be\u00b7grenzt", "ist", "un\u00b7se\u00b7re", "Ta\u00b7ges\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVPP", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Du warst und sahst und stauntest, schon Abend ists,", "tokens": ["Du", "warst", "und", "sahst", "und", "staun\u00b7test", ",", "schon", "A\u00b7bend", "ists", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KON", "VVFIN", "KON", "VVFIN", "$,", "ADV", "NN", "VAFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nun schlafe, wo unendlich ferne", "tokens": ["Nun", "schla\u00b7fe", ",", "wo", "un\u00b7end\u00b7lich", "fer\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "ADJD", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ziehen vor\u00fcber der V\u00f6lker Jahre.", "tokens": ["Zie\u00b7hen", "vor\u00b7\u00fc\u00b7ber", "der", "V\u00f6l\u00b7ker", "Jah\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.12": {"line.1": {"text": "Und mancher siehet \u00fcber die eigne Zeit,", "tokens": ["Und", "man\u00b7cher", "sie\u00b7het", "\u00fc\u00b7ber", "die", "eig\u00b7ne", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ihm zeigt ein Gott ins Freie, doch sehnend stehst", "tokens": ["Ihm", "zeigt", "ein", "Gott", "ins", "Frei\u00b7e", ",", "doch", "seh\u00b7nend", "stehst"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Am Ufer du, ein \u00c4rgernis den", "tokens": ["Am", "U\u00b7fer", "du", ",", "ein", "\u00c4r\u00b7ger\u00b7nis", "den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "PPER", "$,", "ART", "NN", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Deinen, ein Schatten, und liebst sie nimmer,", "tokens": ["Dei\u00b7nen", ",", "ein", "Schat\u00b7ten", ",", "und", "liebst", "sie", "nim\u00b7mer", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "$,", "ART", "NN", "$,", "KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.13": {"line.1": {"text": "Und jene, die du nennst, die Verhei\u00dfenen,", "tokens": ["Und", "je\u00b7ne", ",", "die", "du", "nennst", ",", "die", "Ver\u00b7hei\u00b7\u00dfe\u00b7nen", ","], "token_info"