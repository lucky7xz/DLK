{"dta.poem.6986": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Vereinigung und dem Unter-  \n scheid der Seelen und des C\u00f6rpers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jm groben Jrrthums Dunst, worin der Geist sich sencket,", "tokens": ["Jm", "gro\u00b7ben", "Jrr\u00b7thums", "Dunst", ",", "wo\u00b7rin", "der", "Geist", "sich", "sen\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "NN", "$,", "PWAV", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verwirrt er sich doch mit dem C\u00f6rper nicht.", "tokens": ["Ver\u00b7wirrt", "er", "sich", "doch", "mit", "dem", "C\u00f6r\u00b7per", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "APPR", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Begreifft ein Mensch, wie sehr er sich den Kopff zerbricht,", "tokens": ["Be\u00b7greifft", "ein", "Mensch", ",", "wie", "sehr", "er", "sich", "den", "Kopff", "zer\u00b7bricht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PWAV", "ADV", "PPER", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie doch ein St\u00fcckchen Stoff, erweget und gedencket?", "tokens": ["Wie", "doch", "ein", "St\u00fcck\u00b7chen", "Stoff", ",", "er\u00b7we\u00b7get", "und", "ge\u00b7den\u00b7cket", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "NN", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie d\u00fcnne man es gleich gemacht, wie zart, wie klein,", "tokens": ["Wie", "d\u00fcn\u00b7ne", "man", "es", "gleich", "ge\u00b7macht", ",", "wie", "zart", ",", "wie", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PIS", "PPER", "ADV", "VVPP", "$,", "PWAV", "ADJD", "$,", "PWAV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Kan es dadurch zum Ha\u00df zur Liebe f\u00e4hig seyn?", "tokens": ["Kan", "es", "da\u00b7durch", "zum", "Ha\u00df", "zur", "Lie\u00b7be", "f\u00e4\u00b7hig", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PAV", "APPRART", "NN", "APPRART", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es sey leicht, rund und spitz, er k\u00f6nn\u2019 auch alles trennen,", "tokens": ["Es", "sey", "leicht", ",", "rund", "und", "spitz", ",", "er", "k\u00f6nn'", "auch", "al\u00b7les", "tren\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,", "PPER", "VMFIN", "ADV", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Es mag, so viel man will, unf\u00fchlbar seyn und fl\u00fcchtig;", "tokens": ["Es", "mag", ",", "so", "viel", "man", "will", ",", "un\u00b7f\u00fchl\u00b7bar", "seyn", "und", "fl\u00fcch\u00b7tig", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "ADV", "ADV", "PIS", "VMFIN", "$,", "ADJD", "VAINF", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So mu\u00df man doch zuletzt bekennen,", "tokens": ["So", "mu\u00df", "man", "doch", "zu\u00b7letzt", "be\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Es werde nimmermehr zum Dencken t\u00fcchtig,", "tokens": ["Es", "wer\u00b7de", "nim\u00b7mer\u00b7mehr", "zum", "Den\u00b7cken", "t\u00fcch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Man k\u00f6nn\u2019 es nimmermehr mit Recht vern\u00fcnfftig nennen.", "tokens": ["Man", "k\u00f6nn'", "es", "nim\u00b7mer\u00b7mehr", "mit", "Recht", "ver\u00b7n\u00fcnff\u00b7tig", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "ADV", "APPR", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}