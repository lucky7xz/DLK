{"textgrid.poem.33650": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Lied", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn etwas in dir leise spricht,", "tokens": ["Wenn", "et\u00b7was", "in", "dir", "lei\u00b7se", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df dir mein Herz ergeben,", "tokens": ["Da\u00df", "dir", "mein", "Herz", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So zweifle, Holde, nicht,", "tokens": ["So", "zweif\u00b7le", ",", "Hol\u00b7de", ",", "nicht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "NN", "$,", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Du leuchtest in mein Leben!", "tokens": ["Du", "leuch\u00b7test", "in", "mein", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Doch nie wirst du von mir begehrt;", "tokens": ["Doch", "nie", "wirst", "du", "von", "mir", "be\u00b7gehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo sch\u00f6n're Sterne funkeln,", "tokens": ["Wo", "sch\u00f6n'\u00b7re", "Ster\u00b7ne", "fun\u00b7keln", ","], "token_info": ["word", "word", 