{"textgrid.poem.55345": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "4", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie das erbaut war, wie's im Frieden lag,", "tokens": ["Wie", "das", "er\u00b7baut", "war", ",", "wie's", "im", "Frie\u00b7den", "lag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "VVPP", "VAFIN", "$,", "VVFIN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es kommt vielleicht vom Altertum zu Tag:", "tokens": ["Es", "kommt", "viel\u00b7leicht", "vom", "Al\u00b7ter\u00b7tum", "zu", "Tag", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denn vieles wirkte, hielt am sel'gen Flei\u00df,", "tokens": ["Denn", "vie\u00b7les", "wirk\u00b7te", ",", "hielt", "am", "sel'\u00b7gen", "Flei\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$,", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wovon die Welt noch keine Silbe wei\u00df.", "tokens": ["Wo\u00b7von", "die", "Welt", "noch", "kei\u00b7ne", "Sil\u00b7be", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Tempel steht, dem h\u00f6chsten Sinn geweiht,", "tokens": ["Der", "Tem\u00b7pel", "steht", ",", "dem", "h\u00f6chs\u00b7ten", "Sinn", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Auf Felsengrund in hehrer Einsamkeit.", "tokens": ["Auf", "Fel\u00b7sen\u00b7grund", "in", "heh\u00b7rer", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Daneben wohnt die fromme Pilgerschar,", "tokens": ["Da\u00b7ne\u00b7ben", "wohnt", "die", "from\u00b7me", "Pil\u00b7ger\u00b7schar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Sie wechseln, gehend, kommend, Jahr f\u00fcr Jahr.", "tokens": ["Sie", "wech\u00b7seln", ",", "ge\u00b7hend", ",", "kom\u00b7mend", ",", "Jahr", "f\u00fcr", "Jahr", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVPP", "$,", "ADJD", "$,", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "So ruhig harrt ein wallendes Geschlecht,", "tokens": ["So", "ru\u00b7hig", "harrt", "ein", "wal\u00b7len\u00b7des", "Ge\u00b7schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Gesch\u00fctzt durch Mauern, mehr durch Licht und Recht,", "tokens": ["Ge\u00b7sch\u00fctzt", "durch", "Mau\u00b7ern", ",", "mehr", "durch", "Licht", "und", "Recht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "$,", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und wer sich dort sein Probejahr befand,", "tokens": ["Und", "wer", "sich", "dort", "sein", "Pro\u00b7be\u00b7jahr", "be\u00b7fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PRF", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Hat in der Welt gar einen eignen Stand;", "tokens": ["Hat", "in", "der", "Welt", "gar", "ei\u00b7nen", "eig\u00b7nen", "Stand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Wir hofften selbst uns im Asyl zu gr\u00fcnden.", "tokens": ["Wir", "hoff\u00b7ten", "selbst", "uns", "im", "A\u00b7syl", "zu", "gr\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Wer Buchten kennt, Erdzungen, wird es finden.", "tokens": ["Wer", "Buch\u00b7ten", "kennt", ",", "Erd\u00b7zun\u00b7gen", ",", "wird", "es", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "$,", "NN", "$,", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Der Abend war un\u00fcbertrefflich sch\u00f6n,", "tokens": ["Der", "A\u00b7bend", "war", "un\u00b7\u00fc\u00b7bert\u00b7reff\u00b7lich", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Ach, wollte Gott, ein K\u00f