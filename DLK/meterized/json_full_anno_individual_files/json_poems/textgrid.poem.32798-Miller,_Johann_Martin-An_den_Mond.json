{"textgrid.poem.32798": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "An den Mond", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heiliger, keuscher Mond!", "tokens": ["Hei\u00b7li\u00b7ger", ",", "keu\u00b7scher", "Mond", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Sieh herab auf meine Leiden!", "tokens": ["Sieh", "her\u00b7ab", "auf", "mei\u00b7ne", "Lei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Habe Mitleid, und erbarm dich meiner!", "tokens": ["Ha\u00b7be", "Mit\u00b7leid", ",", "und", "er\u00b7barm", "dich", "mei\u00b7ner", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "KON", "ADJD", "PPER", "PPOSAT", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Weinend und totenbleich", "tokens": ["Wei\u00b7nend", "und", "to\u00b7ten\u00b7bleich"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KON", "ADJD"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Seh' ich dich, du Kind des Himmels,", "tokens": ["Seh'", "ich", "dich", ",", "du", "Kind", "des", "Him\u00b7mels", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "PPER", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ringe meine H\u00e4nd', und schmacht' in Jammer.", "tokens": ["Rin\u00b7ge", "mei\u00b7ne", "H\u00e4nd'", ",", "und", "schmacht'", "in", "Jam\u00b7mer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Heiliger, keuscher Mond!", "tokens": ["Hei\u00b7li\u00b7ger", ",", "keu\u00b7scher", "Mond", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Ach, ich lieb', ich lieb' ein M\u00e4dchen,", "tokens": ["Ach", ",", "ich", "lieb'", ",", "ich", "lieb'", "ein", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sie wei\u00df es nicht, da\u00df ich sie liebe!", "tokens": ["Und", "sie", "wei\u00df", "es", "nicht", ",", "da\u00df", "ich", "sie", "lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Heilig und keusch, wie du,", "tokens": ["Hei\u00b7lig", "und", "keusch", ",", "wie", "du", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "KON", "ADJD", "$,", "PWAV", "PPER", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Brennt ihr meine ganze Seele,", "tokens": ["Brennt", "ihr", "mei\u00b7ne", "gan\u00b7ze", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle Heilige und Engel wissen's!", "tokens": ["Al\u00b7le", "Hei\u00b7li\u00b7ge", "und", "En\u00b7gel", "wis\u00b7sen's", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "KON", "NE", "NE", "$."], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Aber Sie wei\u00df es nicht! \u2013", "tokens": ["A\u00b7ber", "Sie", "wei\u00df", "es", "nicht", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKNEG", "$.", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Gott im Himmel, la\u00df mich sterben,", "tokens": ["Gott", "im", "Him\u00b7mel", ",", "la\u00df", "mich", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "VVIMP", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn du nicht f\u00fcr mich den Engel schufest!", "tokens": ["Wenn", "du", "nicht", "f\u00fcr", "mich", "den", "En\u00b7gel", "schu\u00b7fest", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Heiliger, keuscher Mond!", "tokens": ["Hei\u00b7li\u00b7ger", ",", "keu\u00b7scher", "Mond", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Sieh herab auf meine Leiden!", "tokens": ["Sieh", "her\u00b7ab", "auf", "mei\u00b7ne", "Lei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Habe Mitleid, und erbarm dich meiner!", "tokens": [