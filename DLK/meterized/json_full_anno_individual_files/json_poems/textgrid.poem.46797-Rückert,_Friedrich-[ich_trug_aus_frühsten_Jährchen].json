er": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "So hoff' ich, da\u00df die Knaben,", "tokens": ["So", "hoff'", "ich", ",", "da\u00df", "die", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die ein Geschwisterpaar", "tokens": ["Die", "ein", "Ge\u00b7schwis\u00b7ter\u00b7paar"], "token_info": ["word", "word", "word"], "pos": ["ART", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Jetzt achtlos sehn begraben,", "tokens": ["Jetzt", "acht\u00b7los", "sehn", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch einst es nehmen wahr.", "tokens": ["Doch", "einst", "es", "neh\u00b7men", "wahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "VVINF", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Und wenn's von selbst nicht strahlet", "tokens": ["Und", "wenn's", "von", "selbst", "nicht", "strah\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "APPR", "ADV", "PTKNEG", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von selbst nicht ihnen t\u00f6nt,", "tokens": ["Von", "selbst", "nicht", "ih\u00b7nen", "t\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PTKNEG", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So sehn sie's dort gemalet", "tokens": ["So", "sehn", "sie's", "dort", "ge\u00b7ma\u00b7let"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und hier im Lied versch\u00f6nt.", "tokens": ["Und", "hier", "im", "Lied", "ver\u00b7sch\u00f6nt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}