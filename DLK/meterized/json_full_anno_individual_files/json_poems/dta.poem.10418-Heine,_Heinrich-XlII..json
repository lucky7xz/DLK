{"dta.poem.10418": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XlII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Mir tr\u00e4umte von einem K\u00f6nigskind',", "tokens": ["Mir", "tr\u00e4um\u00b7te", "von", "ei\u00b7nem", "K\u00f6\u00b7nigs\u00b7kind'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit nassen, blassen Wangen;", "tokens": ["Mit", "nas\u00b7sen", ",", "blas\u00b7sen", "Wan\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir sa\u00dfen unter der gr\u00fcnen Lind',", "tokens": ["Wir", "sa\u00b7\u00dfen", "un\u00b7ter", "der", "gr\u00fc\u00b7nen", "Lind'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und hielten uns liebumfangen.", "tokens": ["Und", "hiel\u00b7ten", "uns", "lieb\u00b7um\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "\u201eich will nicht deines Vaters Thron,", "tokens": ["\u201e", "ich", "will", "nicht", "dei\u00b7nes", "Va\u00b7ters", "Thron", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "PTKNEG", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich will nicht sein Scepter von Golde,", "tokens": ["Ich", "will", "nicht", "sein", "Scep\u00b7ter", "von", "Gol\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "PPOSAT", "NN", "APPR", "NE", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ich will nicht seine demantene Kron',", "tokens": ["Ich", "will", "nicht", "sei\u00b7ne", "de\u00b7man\u00b7te\u00b7ne", "Kron'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich will dich selber, du Holde!\u201c", "tokens": ["Ich", "will", "dich", "sel\u00b7ber", ",", "du", "Hol\u00b7de", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "$,", "PPER", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Das kann nicht seyn, sprach sie zu mir,", "tokens": ["Das", "kann", "nicht", "seyn", ",", "sprach", "sie", "zu", "mir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PTKNEG", "VAINF", "$,", "VVFIN", "PPER", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich liege ja im Grabe,", "tokens": ["Ich", "lie\u00b7ge", "ja", "im", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und nur des Nachts komm' ich zu dir,", "tokens": ["Und", "nur", "des", "Nachts", "komm'", "ich", "zu", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADV", "VVFIN", "PPER", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weil ich so lieb dich habe.", "tokens": ["Weil", "ich", "so", "lieb", "dich", "ha\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "PPER", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}