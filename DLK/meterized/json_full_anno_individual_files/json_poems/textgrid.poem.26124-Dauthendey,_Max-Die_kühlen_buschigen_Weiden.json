{"textgrid.poem.26124": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Die k\u00fchlen buschigen Weiden", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es stehen wieder die k\u00fchlen, buschigen Weiden", "tokens": ["Es", "ste\u00b7hen", "wie\u00b7der", "die", "k\u00fch\u00b7len", ",", "bu\u00b7schi\u00b7gen", "Wei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Am Inselufer, wo sie zum Wasser hinf\u00fchlen", "tokens": ["Am", "In\u00b7se\u00b7lu\u00b7fer", ",", "wo", "sie", "zum", "Was\u00b7ser", "hin\u00b7f\u00fch\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PWAV", "PPER", "APPRART", "NN", "VVINF"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und spiegeln sich wieder den Sommer lang;", "tokens": ["Und", "spie\u00b7geln", "sich", "wie\u00b7der", "den", "Som\u00b7mer", "lang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Gr\u00fc\u00dfen den Flu\u00df auf seinem t\u00e4glichen Gang,", "tokens": ["Gr\u00fc\u00b7\u00dfen", "den", "Flu\u00df", "auf", "sei\u00b7nem", "t\u00e4g\u00b7li\u00b7chen", "Gang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Lassen sich die Welle um den Wurzelfu\u00df sp\u00fclen,", "tokens": ["Las\u00b7sen", "sich", "die", "Wel\u00b7le", "um", "den", "Wur\u00b7zel\u00b7fu\u00df", "sp\u00fc\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Zischen mit den Bl\u00e4ttern noch nachts voll Genu\u00df", "tokens": ["Zi\u00b7schen", "mit", "den", "Bl\u00e4t\u00b7tern", "noch", "nachts", "voll", "Ge\u00b7nu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "ADV", "ADV", "ADJD", "NN"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Und lassen sich gern von den vier Winden durchw\u00fchlen.", "tokens": ["Und", "las\u00b7sen", "sich", "gern", "von", "den", "vier", "Win\u00b7den", "durch\u00b7w\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "APPR", "ART", "CARD", "NN", "VVINF", "$."], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Alle die Weidenbl\u00e4tter voll silbriger Spiegel sind", "tokens": ["Al\u00b7le", "die", "Wei\u00b7den\u00b7bl\u00e4t\u00b7ter", "voll", "silb\u00b7ri\u00b7ger", "Spie\u00b7gel", "sind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "ART", "NN", "ADJD", "ADJA", "NN", "VAFIN"], "meter": "+--+-+--+--+-+", "measure": "iambic.hexa.invert"}, "line.9": {"text": "Und werden wie die Sehnenden auch nachts nicht blind.", "tokens": ["Und", "wer\u00b7den", "wie", "die", "Seh\u00b7nen\u00b7den", "auch", "nachts", "nicht", "blind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "KOKOM", "ART", "NN", "ADV", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic