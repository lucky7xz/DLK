{"textgrid.poem.57821": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Matrosenlied", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heute wollen wir ein Liedlein singen,", "tokens": ["Heu\u00b7te", "wol\u00b7len", "wir", "ein", "Lied\u00b7lein", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Trinken wollen wir den k\u00fchlen Wein,", "tokens": ["Trin\u00b7ken", "wol\u00b7len", "wir", "den", "k\u00fch\u00b7len", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und die Gl\u00e4ser sollen dazu klingen,", "tokens": ["Und", "die", "Gl\u00e4\u00b7ser", "sol\u00b7len", "da\u00b7zu", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "PAV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Denn es mu\u00df, es mu\u00df geschieden sein;", "tokens": ["Denn", "es", "mu\u00df", ",", "es", "mu\u00df", "ge\u00b7schie\u00b7den", "sein", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "$,", "PPER", "VMFIN", "VVPP", "VAINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Gib mir deine Hand,", "tokens": ["Gib", "mir", "dei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Deine wei\u00dfe Hand,", "tokens": ["Dei\u00b7ne", "wei\u00b7\u00dfe", "Hand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Leb wohl, mein Schatz, leb wohl,", "tokens": ["Leb", "wohl", ",", "mein", "Schatz", ",", "leb", "wohl", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPOSAT", "NN", "$,", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Denn wir fahren gegen Engelland.", "tokens": ["Denn", "wir", "fah\u00b7ren", "ge\u00b7gen", "En\u00b7gel\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Unsre Flagge und die wehet auf dem Maste,", "tokens": ["Uns\u00b7re", "Flag\u00b7ge", "und", "die", "we\u00b7het", "auf", "dem", "Mas\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "ART", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Sie verk\u00fcndet unsres Reiches Macht,", "tokens": ["Sie", "ver\u00b7k\u00fcn\u00b7det", "uns\u00b7res", "Rei\u00b7ches", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Denn wir wollen es nicht l\u00e4nger leiden,", "tokens": ["Denn", "wir", "wol\u00b7len", "es", "nicht", "l\u00e4n\u00b7ger", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Da\u00df der Englis