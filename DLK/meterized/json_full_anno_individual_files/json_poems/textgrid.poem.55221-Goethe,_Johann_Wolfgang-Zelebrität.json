s": ["Da\u00df", "er", "gar", "weit", "im", "Bil\u00b7de", "prangt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Kupferstich, Holzschnitt tun sich eilen,", "tokens": ["Kup\u00b7fer\u00b7stich", ",", "Holz\u00b7schnitt", "tun", "sich", "ei\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "NN", "VVFIN", "PRF", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Ihn allen Welten mitzuteilen;", "tokens": ["Ihn", "al\u00b7len", "Wel\u00b7ten", "mit\u00b7zu\u00b7tei\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und jede Gestalt wird wohl empfangen,", "tokens": ["Und", "je\u00b7de", "Ge\u00b7stalt", "wird", "wohl", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Tut sie mit seinem Namen prangen:", "tokens": ["Tut", "sie", "mit", "sei\u00b7nem", "Na\u00b7men", "pran\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Wie es denn auch dem Herren Christ", "tokens": ["Wie", "es", "denn", "auch", "dem", "Her\u00b7ren", "Christ"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Nicht ein Haar besser geworden ist.", "tokens": ["Nicht", "ein", "Ha