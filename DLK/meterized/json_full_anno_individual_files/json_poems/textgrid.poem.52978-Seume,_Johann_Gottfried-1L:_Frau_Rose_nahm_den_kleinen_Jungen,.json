{"textgrid.poem.52978": {"metadata": {"author": {"name": "Seume, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Frau Rose nahm den kleinen Jungen,", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frau Rose nahm den kleinen Jungen,", "tokens": ["Frau", "Ro\u00b7se", "nahm", "den", "klei\u00b7nen", "Jun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der jubelnd um sie hergesprungen,", "tokens": ["Der", "ju\u00b7belnd", "um", "sie", "her\u00b7ge\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit m\u00fctterlichem Wohlbehagen,", "tokens": ["Mit", "m\u00fct\u00b7ter\u00b7li\u00b7chem", "Wohl\u00b7be\u00b7ha\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Um ihn mit Trommel, Flint' und Wagen", "tokens": ["Um", "ihn", "mit", "Trom\u00b7mel", ",", "Flint'", "und", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUI", "PPER", "APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Zu T\u00f6ffeln, ihrem Mann, zu tragen.", "tokens": ["Zu", "T\u00f6f\u00b7feln", ",", "ih\u00b7rem", "Mann", ",", "zu", "tra\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PPOSAT", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sieh sieh doch, T\u00f6ffel, sprach Frau Rose,", "tokens": ["Sieh", "sieh", "doch", ",", "T\u00f6f\u00b7fel", ",", "sprach", "Frau", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$,", "NN", "$,", "VVFIN", "NN", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wie rasch er ist, wie flink und lose;", "tokens": ["Wie", "rasch", "er", "ist", ",", "wie", "flink", "und", "lo\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VAFIN", "$,", "PWAV", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Schau nur einmahl dem kleinen Wichte", "tokens": ["Schau", "nur", "ein\u00b7mahl", "dem", "klei\u00b7nen", "Wich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Auf jeden Zug im Angesichte;", "tokens": ["Auf", "je\u00b7den", "Zug", "im", "An\u00b7ge\u00b7sich\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Er ist von Kopf bis zu den Sohlen", "tokens": ["Er", "ist", "von", "Kopf", "bis", "zu", "den", "Soh\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "APPR", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Im Ebenbild mir abgestohlen:", "tokens": ["Im", "E\u00b7ben\u00b7bild", "mir", "ab\u00b7ge\u00b7stoh\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"te