{"dta.poem.9959": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "68.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich mich will h\u00f6ren lassen/ zittert alles unter mir/", "tokens": ["Wenn", "ich", "mich", "will", "h\u00f6\u00b7ren", "las\u00b7sen", "/", "zit\u00b7tert", "al\u00b7les", "un\u00b7ter", "mir", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VMFIN", "VVINF", "VVINF", "$(", "VVFIN", "PIS", "APPR", "PPER", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Ob ich schon nicht hoch gewachse\u0303/ grosse macht ich bey mir f\u00fchr/", "tokens": ["Ob", "ich", "schon", "nicht", "hoch", "ge\u00b7wachs\u1ebd", "/", "gros\u00b7se", "macht", "ich", "bey", "mir", "f\u00fchr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "ADJD", "VAPP", "$(", "ADJA", "VVFIN", "PPER", "APPR", "PPER", "VVFIN", "$("], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Ist scho\u0303 meine herkunfft schlecht/ ist es mir drum keine schande/", "tokens": ["Ist", "sch\u00f5", "mei\u00b7ne", "her\u00b7kunfft", "schlecht", "/", "ist", "es", "mir", "drum", "kei\u00b7ne", "schan\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PPOSAT", "NN", "VVFIN", "$(", "VAFIN", "PPER", "PRF", "PAV", "PIAT", "ADJA", "$("], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Weil ich m\u00fcntze schlagen darff/ wie der gr\u00f6ste f\u00fcrst im lande.", "tokens": ["Weil", "ich", "m\u00fcnt\u00b7ze", "schla\u00b7gen", "darff", "/", "wie", "der", "gr\u00f6s\u00b7te", "f\u00fcrst", "im", "lan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "VVINF", "VMFIN", "$(", "KOKOM", "ART", "ADJA", "ADV", "APPRART", "ADJA", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}}}}}