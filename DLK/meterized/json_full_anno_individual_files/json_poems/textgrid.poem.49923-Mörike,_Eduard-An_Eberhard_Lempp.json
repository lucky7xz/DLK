{"textgrid.poem.49923": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "An Eberhard Lempp", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kennst du der Furien schlimmste, Freund? Ich hoffe, nein!", "tokens": ["Kennst", "du", "der", "Fu\u00b7ri\u00b7en", "schlimms\u00b7te", ",", "Freund", "?", "Ich", "hof\u00b7fe", ",", "nein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVFIN", "$,", "NN", "$.", "PPER", "VVFIN", "$,", "PTKANT", "$."], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Kein Dichter, nicht der alten, noch der neuen Zeit,", "tokens": ["Kein", "Dich\u00b7ter", ",", "nicht", "der", "al\u00b7ten", ",", "noch", "der", "neu\u00b7en", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PTKNEG", "ART", "ADJA", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kein Mythograph hat sie zu nennen je gewagt;", "tokens": ["Kein", "My\u00b7tho\u00b7graph", "hat", "sie", "zu", "nen\u00b7nen", "je", "ge\u00b7wagt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "PTKZU", "VVINF", "ADV", "VVPP", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ich selber, bange vor der leise h\u00f6renden,", "tokens": ["Ich", "sel\u00b7ber", ",", "ban\u00b7ge", "vor", "der", "lei\u00b7se", "h\u00f6\u00b7ren\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ADV", "APPR", "ART", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.5": {"text": "Tu es nur heimlich: Agrypnia hei\u00dfet sie.", "tokens": ["Tu", "es", "nur", "heim\u00b7lich", ":", "Ag\u00b7ryp\u00b7nia", "hei\u00b7\u00dfet", "sie", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "ADJD", "$.", "NE", "VVFIN", "PPER", "$."], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Ach, als ich jung war, deuchte sie mir sch\u00f6n zu sein,", "tokens": ["Ach", ",", "als", "ich", "jung", "war", ",", "deuch\u00b7te", "sie", "mir", "sch\u00f6n", "zu", "sein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,", "VVFIN", "PPER", "PPER", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Pi\u00ebrische Jungfrau, oder ihnen nah verwandt;", "tokens": ["Pi\u00ebri\u00b7sche", "Jung\u00b7frau", ",", "o\u00b7der", "ih\u00b7nen", "nah", "ver\u00b7wandt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.8": {"text": "Vielleicht auch ist sie's, aber weh dem, der sie ruft!", "tokens": ["Viel\u00b7leicht", "auch", "ist", "sie's", ",", "a\u00b7ber", "weh", "dem", ",", "der", "sie", "ruft", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "NE", "$,", "ADV", "ADV", "ART", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Denn der Gesundheit Farbe saugt ihr hei\u00dfer Blick", "tokens": ["Denn", "der", "Ge\u00b7sund\u00b7heit", "Far\u00b7be", "saugt", "ihr", "hei\u00b7\u00dfer", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dem J\u00fcngling von den Wangen, und verzehrt den Mann.", "tokens": ["Dem", "J\u00fcng\u00b7ling", "von", "den", "Wan\u00b7gen", ",", "und", "ver\u00b7zehrt", "den", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "An meinem Bette sitzt sie manche Mitternacht,", "tokens": ["An", "mei\u00b7nem", "Bet\u00b7te", "sitzt", "sie", "man\u00b7che", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Gleich einer Buhlerin, der man \u00fcberdr\u00fcssig ist.", "tokens": ["Gleich", "ei\u00b7ner", "Buh\u00b7le\u00b7rin", ",", "der", "man", "\u00fc\u00b7berd\u00b7r\u00fcs\u00b7sig", "ist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "PIS", "ADJD", "VAFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Den R\u00fccken ihr zukehrend blinz ich seufzend nur,", "tokens": ["Den", "R\u00fc\u00b7cken", "ihr", "zu\u00b7keh\u00b7rend", "blinz", "ich", "seuf\u00b7zend", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADJD", "VVFIN", "PPER", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, 