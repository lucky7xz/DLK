{"dta.poem.5341": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Sonnen-Lehre.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Quell des Lichts und aller Wonne!\n\u201dLebens Ursprung! helle Sonne!", "tokens": ["Quell", "des", "Lichts", "und", "al\u00b7ler", "Won\u00b7ne", "!", "\"", "Le\u00b7bens", "Ur\u00b7sprung", "!", "hel\u00b7le", "Son\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "PIAT", "NN", "$.", "$(", "NN", "NN", "$.", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "\u201edu l\u00e4\u00df\u2019t alles, was auf Erden,", "tokens": ["\u201e", "du", "l\u00e4\u00df't", "al\u00b7les", ",", "was", "auf", "Er\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PIS", "$,", "PRELS", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u201ewarm und licht und fruchtbar werden;", "tokens": ["\u201e", "warm", "und", "licht", "und", "frucht\u00b7bar", "wer\u00b7den", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "KON", "NN", "KON", "ADJD", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u201esollte denn dein Lebens-Schein", "tokens": ["\u201e", "soll\u00b7te", "denn", "dein", "Le\u00b7bens\u00b7Schein"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "VMFIN", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "\u201enicht die wahre GOttheit ", "tokens": ["\u201e", "nicht", "die", "wah\u00b7re", "Got\u00b7theit"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "PTKNEG", "ART", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Es w\u00fcrd\u2019 ein Perser so zu sprechen", "tokens": ["Es", "w\u00fcrd'", "ein", "Per\u00b7ser", "so", "zu", "spre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich nicht entbrechen.", "tokens": ["Sich", "nicht", "ent\u00b7bre\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Allein die Sonne selbst, indem sie uns verl\u00e4\u00dft,", "tokens": ["Al\u00b7lein", "die", "Son\u00b7ne", "selbst", ",", "in\u00b7dem", "sie", "uns", "ver\u00b7l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zeigt, da\u00df sie solchen Ruhm nicht k\u00f6nn\u2019 ertragen.", "tokens": ["Zeigt", ",", "da\u00df", "sie", "sol\u00b7chen", "Ruhm", "nicht", "k\u00f6nn'", "er\u00b7tra\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "PIAT", "NN", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sie setzet diese Wahrheit fest,", "tokens": ["Sie", "set\u00b7zet", "die\u00b7se", "Wahr\u00b7heit", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ja scheinet, wenn sie weicht, uns gleichsam die\u00df zu sagen:", "tokens": ["Ja", "schei\u00b7net", ",", "wenn", "sie", "weicht", ",", "uns", "gleich\u00b7sam", "die\u00df", "zu", "sa\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,", "PPER", "ADJD", "PDS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "La\u00df, liebster Mensch, doch meine Pracht und Zier,", "tokens": ["La\u00df", ",", "liebs\u00b7ter", "Mensch", ",", "doch", "mei\u00b7ne", "Pracht", "und", "Zier", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "ADJA", "NN", "$,", "KON", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "So wie dein leiblich Auge, dir", "tokens": ["So", "wie", "dein", "leib\u00b7lich", "Au\u00b7ge", ",", "dir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "KOKOM", "PPOSAT", "ADJD", "NN", "$,", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Nicht auch dein Seelen Auge blenden!", "tokens": ["Nicht", "auch", "dein", "See\u00b7len", "Au\u00b7ge", "blen\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "W\u00e4r\u2019 ich ein GOtt; wie k\u00f6nnte sich", "tokens": ["W\u00e4r'", "ich", "ein", "Gott", ";", "wie", "k\u00f6nn\u00b7te", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "$.", "PWAV", "VMFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Mein Glantz entfernen, und ich mich,", "tokens": ["Mein", "Glantz", "ent\u00b7fer\u00b7nen", ",", "und", "ich", "mich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,", "KON", "PPER", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Gehemmt von etwas, weg- und abw\u00e4rts wenden,", "tokens": ["Ge\u00b7hemmt", "von", "et\u00b7was", ",", "weg", "und", "ab\u00b7w\u00e4rts", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIS", "$,", "TRUNC", "KON", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Allein, wilt du die GOttheit sehn,", "tokens": ["Al\u00b7lein", ",", "wilt", "du", "die", "Got\u00b7theit", "sehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Und seine Gr\u00f6sse kennen lernen;", "tokens": ["Und", "sei\u00b7ne", "Gr\u00f6s\u00b7se", "ken\u00b7nen", "ler\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "So kann es zwar durch mich geschehn,", "tokens": ["So", "kann", "es", "zwar", "durch", "mich", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Doch mu\u00df ich mich so dann entfernen.", "tokens": ["Doch", "mu\u00df", "ich", "mich", "so", "dann", "ent\u00b7fer\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PRF", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Jhr alle k\u00f6nnt durch mich, in seinen Creaturen,", "tokens": ["Ihr", "al\u00b7le", "k\u00f6nnt", "durch", "mich", ",", "in", "sei\u00b7nen", "Crea\u00b7tu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVFIN", "APPR", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Vom grossen Sch\u00f6pfer, helle Spuren;", "tokens": ["Vom", "gros\u00b7sen", "Sch\u00f6p\u00b7fer", ",", "hel\u00b7le", "Spu\u00b7ren", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Doch ohne mich von ihm noch gr\u00f6ssre, sehn.", "tokens": ["Doch", "oh\u00b7ne", "mich", "von", "ihm", "noch", "gr\u00f6ss\u00b7re", ",", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "KOUI", "PRF", "APPR", "PPER", "ADV", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "So bald durch eurer Erde drehn,", "tokens": ["So", "bald", "durch", "eu\u00b7rer", "Er\u00b7de", "drehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Jhr mich verliert, und ich f\u00fcr euch verschwunden;", "tokens": ["Ihr", "mich", "ver\u00b7liert", ",", "und", "ich", "f\u00fcr", "euch", "ver\u00b7schwun\u00b7den", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "VVFIN", "$,", "KON", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So wird ins Luft-Raums tieffem Meer", "tokens": ["So", "wird", "ins", "Luft\u00b7Raums", "tief\u00b7fem", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein unzuzehlend Sonnen-Heer,", "tokens": ["Ein", "un\u00b7zu\u00b7zeh\u00b7lend", "Son\u00b7nen\u00b7Heer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An meiner statt, von euch gefunden,", "tokens": ["An", "mei\u00b7ner", "statt", ",", "von", "euch", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von welchen ihr, sollt\u2019 ich mit meinem Schein", "tokens": ["Von", "wel\u00b7chen", "ihr", ",", "sollt'", "ich", "mit", "mei\u00b7nem", "Schein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "PPOSAT", "$,", "VMFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Bey euch stets gegenw\u00e4rtig seyn,", "tokens": ["Bey", "euch", "stets", "ge\u00b7gen\u00b7w\u00e4r\u00b7tig", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ADJD", "VAINF", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Und immer bey euch leuchten m\u00fcssen,", "tokens": ["Und", "im\u00b7mer", "bey", "euch", "leuch\u00b7ten", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPER", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Nicht das Geringste w\u00fcrdet wissen.", "tokens": ["Nicht", "das", "Ge\u00b7rings\u00b7te", "w\u00fcr\u00b7det", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Hier schwindet meiner GOttheit Glantz,", "tokens": ["Hier", "schwin\u00b7det", "mei\u00b7ner", "Got\u00b7theit", "Glantz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Bey Millionen Sonnen gantz.", "tokens": ["Bey", "Mil\u00b7lion\u00b7en", "Son\u00b7nen", "gantz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Die wahre GOttheit zeiget sich", "tokens": ["Die", "wah\u00b7re", "Got\u00b7theit", "zei\u00b7get", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "In dieser Schaar hingegen sichtbarlich.", "tokens": ["In", "die\u00b7ser", "Schaar", "hin\u00b7ge\u00b7gen", "sicht\u00b7bar\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "In keinen w\u00fcrdigern Jdeen,", "tokens": ["In", "kei\u00b7nen", "w\u00fcr\u00b7di\u00b7gern", "Jdeen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.13": {"text": "Kann euer Geist ein Bild der wahren GOttheit sehen,", "tokens": ["Kann", "eu\u00b7er", "Geist", "ein", "Bild", "der", "wah\u00b7ren", "Got\u00b7theit", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Als in dem ew\u2019gen Geist, der eine solche Zahl,", "tokens": ["Als", "in", "dem", "ew'\u00b7gen", "Geist", ",", "der", "ei\u00b7ne", "sol\u00b7che", "Zahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "$,", "PRELS", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die nicht zu zehlen ist, von Sonnen schaft, regieret", "tokens": ["Die", "nicht", "zu", "zeh\u00b7len", "ist", ",", "von", "Son\u00b7nen", "schaft", ",", "re\u00b7gie\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "PTKNEG", "PTKZU", "VVINF", "VAFIN", "$,", "APPR", "NN", "ADJD", "$,", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und noch ein gr\u00f6sser Heer von Welten um sie f\u00fchret.", "tokens": ["Und", "noch", "ein", "gr\u00f6s\u00b7ser", "Heer", "von", "Wel\u00b7ten", "um", "sie", "f\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "APPR", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "So deucht mich da\u00df, zu ihres Sch\u00f6pfers Ehre,", "tokens": ["So", "deucht", "mich", "da\u00df", ",", "zu", "ih\u00b7res", "Sch\u00f6p\u00b7fers", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOUS", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Ich, wenn sie untergeht, die Sonne reden h\u00f6re.", "tokens": ["Ich", ",", "wenn", "sie", "un\u00b7ter\u00b7geht", ",", "die", "Son\u00b7ne", "re\u00b7den", "h\u00f6\u00b7re", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUS", "PPER", "VVFIN", "$,", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ach, la\u00dft uns ihre Red\u2019 uns ins Ged\u00e4chtnis pr\u00e4gen,", "tokens": ["Ach", ",", "la\u00dft", "uns", "ih\u00b7re", "Red'", "uns", "ins", "Ge\u00b7d\u00e4cht\u00b7nis", "pr\u00e4\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVIMP", "PPER", "PPOSAT", "NN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und, wenn wir in der Nacht der Sterne Schaaren sehn,", "tokens": ["Und", ",", "wenn", "wir", "in", "der", "Nacht", "der", "Ster\u00b7ne", "Schaa\u00b7ren", "sehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Den HErrn der Schaaren doch erh\u00f6hn,", "tokens": ["Den", "Herrn", "der", "Schaa\u00b7ren", "doch", "er\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Und, wenn wir unsern Blick ins Himmels Tieffe sencken,", "tokens": ["Und", ",", "wenn", "wir", "un\u00b7sern", "Blick", "ins", "Him\u00b7mels", "Tief\u00b7fe", "sen\u00b7cken"