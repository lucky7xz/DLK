{"textgrid.poem.55392": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "14", "genre": "verse", "period": "N.A.", "pub_year": 1820, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu der Apfelverk\u00e4uferin", "tokens": ["Zu", "der", "Ap\u00b7fel\u00b7ver\u00b7k\u00e4u\u00b7fe\u00b7rin"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Kamen Kinder gelaufen,", "tokens": ["Ka\u00b7men", "Kin\u00b7der", "ge\u00b7lau\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "VVPP", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Alle wollten kaufen;", "tokens": ["Al\u00b7le", "woll\u00b7ten", "kau\u00b7fen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Mit munterm Sinn", "tokens": ["Mit", "mun\u00b7term", "Sinn"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Griffen sie aus dem Haufen,", "tokens": ["Grif\u00b7fen", "sie", "aus", "dem", "Hau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Beschauten mit Verlangen", "tokens": ["Be\u00b7schau\u00b7ten", "mit", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Nah und n\u00e4her rotb\u00e4ckige Wangen \u2013", "tokens": ["Nah", "und", "n\u00e4\u00b7her", "rot\u00b7b\u00e4\u00b7cki\u00b7ge", "Wan\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "po