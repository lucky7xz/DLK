{"dta.poem.191": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als sie sagte ihre Br\u00fcste w\u00e4ren nicht  \n h\u00fcbsch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Jocaste deine Brust ist \u00fcberaus galant,", "tokens": ["Jo\u00b7cas\u00b7te", "dei\u00b7ne", "Brust", "ist", "\u00fc\u00b7be\u00b7raus", "ga\u00b7lant", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie/ kan ein Busen wol von Br\u00fcsten h\u00fcbscher seyn?", "tokens": ["Wie", "/", "kan", "ein", "Bu\u00b7sen", "wol", "von", "Br\u00fcs\u00b7ten", "h\u00fcb\u00b7scher", "seyn", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$(", "VMFIN", "ART", "NN", "ADV", "APPR", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Athen schwellet auf den weichen ", "tokens": ["Der", "A\u00b7then", "schwel\u00b7let", "auf", "den", "wei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der runde Circul f\u00fcllt die drauf gesenckte Hand.", "tokens": ["Der", "run\u00b7de", "Cir\u00b7cul", "f\u00fcllt", "die", "drauf", "ge\u00b7senck\u00b7te", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "PAV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es macht der warme Schnee die gr\u00f6ste Lust bekand/", "tokens": ["Es", "macht", "der", "war\u00b7me", "Schnee", "die", "gr\u00f6s\u00b7te", "Lust", "be\u00b7kand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die beste Zierde ist/ da\u00df sie ein wenig klein/", "tokens": ["Die", "bes\u00b7te", "Zier\u00b7de", "ist", "/", "da\u00df", "sie", "ein", "we\u00b7nig", "klein", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$(", "KOUS", "PPER", "ART", "PIS", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch bl\u00e4\u00dft sie Feur und Reitz den l\u00fcstern Geistern ein.", "tokens": ["Doch", "bl\u00e4\u00dft", "sie", "Feur", "und", "Reitz", "den", "l\u00fcs\u00b7tern", "Geis\u00b7tern", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "KON", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Jhr Milch-Meer das entz\u00fcndt durch einen sanfften Brand.", "tokens": ["Ihr", "Milch\u00b7Meer", "das", "ent\u00b7z\u00fcndt", "durch", "ei\u00b7nen", "sanff\u00b7ten", "Brand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-++--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Von den Rubinen trifft ein s\u00fcsser ", "tokens": ["Von", "den", "Ru\u00b7bi\u00b7nen", "trifft", "ein", "s\u00fcs\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die Mildigkeit und Gunst zu vollen Wachsthum streun", "tokens": ["Die", "Mil\u00b7dig\u00b7keit", "und", "Gunst", "zu", "vol\u00b7len", "Wach\u00b7sthum", "streun"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Den Saamen s\u00fcsser Lust auf das beliebte Land.", "tokens": ["Den", "Saa\u00b7men", "s\u00fcs\u00b7ser", "Lust", "auf", "das", "be\u00b7lieb\u00b7te", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Jhr Br\u00fcste quillt hervor das bester Zuckerkand/", "tokens": ["Ihr", "Br\u00fcs\u00b7te", "quillt", "her\u00b7vor", "das", "bes\u00b7ter", "Zu\u00b7cker\u00b7kand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Man mu\u00df euch Hand und Mund zu einen Alkar weyhn", "tokens": ["Man", "mu\u00df", "euch", "Hand", "und", "Mund", "zu", "ei\u00b7nen", "Al\u00b7kar", "weyhn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "PPER", "NN", "KON", "NN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und behten g\u00f6ttlich an den weissen Wunder-Schein.", "tokens": ["Und", "beh\u00b7ten", "g\u00f6tt\u00b7lich", "an", "den", "weis\u00b7sen", "Wun\u00b7der\u00b7Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}