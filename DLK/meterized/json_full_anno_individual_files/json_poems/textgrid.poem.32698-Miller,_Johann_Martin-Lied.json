{"textgrid.poem.32698": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "Lied", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein sch\u00f6ner, junger Rittersmann", "tokens": ["Ein", "sch\u00f6\u00b7ner", ",", "jun\u00b7ger", "Rit\u00b7ters\u00b7mann"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schleicht mir den ganzen Tag,", "tokens": ["Schleicht", "mir", "den", "gan\u00b7zen", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vom allerersten Morgen an", "tokens": ["Vom", "al\u00b7le\u00b7rers\u00b7ten", "Mor\u00b7gen", "an"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bis an den Abend, nach.", "tokens": ["Bis", "an", "den", "A\u00b7bend", ",", "nach", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "$,", "APPR", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich aber meid' ihn f\u00fcr und f\u00fcr", "tokens": ["Ich", "a\u00b7ber", "meid'", "ihn", "f\u00fcr", "und", "f\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "APPR", "KON", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und flieh' ihn \u00fcberall,", "tokens": ["Und", "flieh'", "ihn", "\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weil es mit vielem Ernste mir", "tokens": ["Weil", "es", "mit", "vie\u00b7lem", "Erns\u00b7te", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PIS", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Mutter anbefahl.", "tokens": ["Die", "Mut\u00b7ter", "an\u00b7be\u00b7fahl", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch thut es mir im Herzen leid,", "tokens": ["Doch", "thut", "es", "mir", "im", "Her\u00b7zen", "leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ich ihn meiden soll,", "tokens": ["Da\u00df", "ich", "ihn", "mei\u00b7den", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denn sein Gesicht voll Freundlichkeit", "tokens": ["Denn", "sein", "Ge\u00b7sicht", "voll", "Freund\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gef\u00e4llt mir gar zu wohl.", "tokens": ["Ge\u00b7f\u00e4llt", "mir", "gar", "zu", "wohl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Heut sprach er viel von Angst und Not,", "tokens": ["Heut", "sprach", "er", "viel", "von", "Angst", "und", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zuletzt vom Sterben gar,", "tokens": ["Zu\u00b7letzt", "vom", "Ster\u00b7ben", "gar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ward dabei so rot, so rot,", "tokens": ["Und", "ward", "da\u00b7bei", "so", "rot", ",", "so", "rot", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PAV", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als kaum der Himmel war.", "tokens": ["Als", "kaum", "der", "Him\u00b7mel", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich konnt' ihm wahrlich nicht entfliehn;", "tokens": ["Ich", "konnt'", "ihm", "wahr\u00b7lich", "nicht", "ent\u00b7fliehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Denn weinend bat er mich,", "tokens": ["Denn", "wei\u00b7nend", "bat", "er", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und weinend setzt' ich neben ihn", "tokens": ["Und", "wei\u00b7nend", "setzt'", "ich", "ne\u00b7ben", "ihn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aufs Blumenlager mich.", "tokens": ["Aufs", "Blu\u00b7men\u00b7la\u00b7ger", "mich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Den Mund, so sehr ich's ihm verbot,", "tokens": ["Den", "Mund", ",", "so", "sehr", "ich's", "ihm", "ver\u00b7bot", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADV", "PIS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat er mir so gek\u00fc\u00dft,", "tokens": ["Hat", "er", "mir", "so", "ge\u00b7k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df er noch itzo feuerrot", "tokens": ["Da\u00df", "er", "noch", "it\u00b7zo", "feu\u00b7er\u00b7rot"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von seinen K\u00fcssen ist.", "tokens": ["Von", "sei\u00b7nen", "K\u00fcs\u00b7sen", "ist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Die ganze St\u00e4tte, wo ich sa\u00df,", "tokens": ["Die", "gan\u00b7ze", "St\u00e4t\u00b7te", ",", "wo", "ich", "sa\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bedeckt' er mir mit Moos,", "tokens": ["Be\u00b7deckt'", "er", "mir", "mit", "Moos", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und streute Blumen aus dem Gras", "tokens": ["Und", "streu\u00b7te", "Blu\u00b7men", "aus", "dem", "Gras"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir freundlich in den Scho\u00df.", "tokens": ["Mir", "freund\u00b7lich", "in", "den", "Scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Man sieht, ich f\u00fcrchte, noch die Spur", "tokens": ["Man", "sieht", ",", "ich", "f\u00fcrch\u00b7te", ",", "noch", "die", "Spur"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "PPER", "VVFIN", "$,", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von unsrer Lagerstatt.", "tokens": ["Von", "uns\u00b7rer", "La\u00b7ger\u00b7statt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O guter Himmel! wenn man nur", "tokens": ["O", "gu\u00b7ter", "Him\u00b7mel", "!", "wenn", "man", "nur"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$.", "KOUS", "PIS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Uns nicht belauschet hat!", "tokens": ["Uns", "nicht", "be\u00b7lau\u00b7schet", "hat", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Doch war kein Mensch im ganzen Thal,", "tokens": ["Doch", "war", "kein", "Mensch", "im", "gan\u00b7zen", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIAT", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und dunkel war der Hain;", "tokens": ["Und", "dun\u00b7kel", "war", "der", "Hain", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und die geliebte Nachtigall", "tokens": ["Und", "die", "ge\u00b7lieb\u00b7te", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wird doch verschwiegen sein?", "tokens": ["Wird", "doch", "ver\u00b7schwie\u00b7gen", "sein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Ein sch\u00f6ner, junger Rittersmann", "tokens": ["Ein", "sch\u00f6\u00b7ner", ",", "jun\u00b7ger", "Rit\u00b7ters\u00b7mann"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schleicht mir den ganzen Tag,", "tokens": ["Schleicht", "mir", "den", "gan\u00b7zen", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vom allerersten Morgen an", "tokens": ["Vom", "al\u00b7le\u00b7rers\u00b7ten", "Mor\u00b7gen", "an"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bis an den Abend, nach.", "tokens": ["Bis", "an", "den", "A\u00b7bend", ",", "nach", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": 