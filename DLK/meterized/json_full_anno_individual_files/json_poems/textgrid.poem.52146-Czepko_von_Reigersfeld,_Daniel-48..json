{"textgrid.poem.52146": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "48.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Frommer Ferdinand, wie wirst du hintergangen,", "tokens": ["O", "From\u00b7mer", "Fer\u00b7di\u00b7nand", ",", "wie", "wirst", "du", "hin\u00b7ter\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$,", "PWAV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die so viel Geld und Gut vor ihre Treu empfangen,", "tokens": ["Die", "so", "viel", "Geld", "und", "Gut", "vor", "ih\u00b7re", "Treu", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "NN", "KON", "ADJD", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verehren itzt den Belt, der ist ihr bestes Theil,", "tokens": ["Ver\u00b7eh\u00b7ren", "itzt", "den", "Belt", ",", "der", "ist", "ihr", "bes\u00b7tes", "Theil", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,", "PRELS", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem tragen sie die Leut' und deine Renten feil.", "tokens": ["Dem", "tra\u00b7gen", "sie", "die", "Leut'", "und", "dei\u00b7ne", "Ren\u00b7ten", "feil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "NN", "KON", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was keiner vorgesucht, dein Oesterreich zu sch\u00fctzen,", "tokens": ["Was", "kei\u00b7ner", "vor\u00b7ge\u00b7sucht", ",", "dein", "O\u00b7es\u00b7ter\u00b7reich", "zu", "sch\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVPP", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Das kehrt man aus, das kratzt man aus den letzten Ritzen:", "tokens": ["Das", "kehrt", "man", "aus", ",", "das", "kratzt", "man", "aus", "den", "letz\u00b7ten", "Rit\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "PTKVZ", "$,", "PDS", "VVFIN", "PIS", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie wollen Ehr und Blut beym Schweden setzen zu,", "tokens": ["Sie", "wol\u00b7len", "Ehr", "und", "Blut", "beym", "Schwe\u00b7den", "set\u00b7zen", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "KON", "NN", "APPRART", "NE", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und bringen Haus und Hoff, ja Stadt und Land zur Ruh.", "tokens": ["Und", "brin\u00b7gen", "Haus", "und", "Hoff", ",", "ja", "Stadt", "und", "Land", "zur", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "VVFIN", "$,", "ADV", "NN", "KON", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was unsern Held betrifft, so wil ich das begr\u00fcnden,", "tokens":