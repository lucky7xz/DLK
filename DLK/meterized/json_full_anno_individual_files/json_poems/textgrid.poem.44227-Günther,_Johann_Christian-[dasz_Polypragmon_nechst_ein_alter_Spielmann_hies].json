{"textgrid.poem.44227": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "[dasz Polypragmon nechst ein alter Spielmann hies]", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dasz Polypragmon nechst ein alter Spielmann hies,", "tokens": ["Dasz", "Po\u00b7ly\u00b7prag\u00b7mon", "nechst", "ein", "al\u00b7ter", "Spiel\u00b7mann", "hies", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als er der Mode nach sich vor dem Grafen wies,", "tokens": ["Als", "er", "der", "Mo\u00b7de", "nach", "sich", "vor", "dem", "Gra\u00b7fen", "wies", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bewundern viel, nicht ich. Warum? Weil seine Gaben", "tokens": ["Be\u00b7wun\u00b7dern", "viel", ",", "nicht", "ich", ".", "Wa\u00b7rum", "?", "Weil", "sei\u00b7ne", "Ga\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADV", "$,", "PTKNEG", "PPER", "$.", "PWAV", "$.", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den Titul mehr mit Recht als blos vom Irrthum haben.", "tokens": ["Den", "Ti\u00b7tul", "mehr", "mit", "Recht", "als", "blos", "vom", "Irr\u00b7thum", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "KOKOM", "ADV", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Denn ob ihm gleich der Tod und Gottes Langmuthstreu", "tokens": ["Denn", "ob", "ihm", "gleich", "der", "Tod", "und", "Got\u00b7tes", "Lang\u00b7muths\u00b7treu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN", "KON", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Viel Zeit zur Bu\u00dfe l\u00e4st, so bleibt er dennoch heuer", "tokens": ["Viel", "Zeit", "zur", "Bu\u00b7\u00dfe", "l\u00e4st", ",", "so", "bleibt", "er", "den\u00b7noch", "heu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPRART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So gut als \u00fcbers Jahr an Geiz, Pedanterey,", "tokens": ["So", "gut", "als", "\u00fc\u00b7bers", "Jahr", "an", "Geiz", ",", "Pe\u00b7dan\u00b7te\u00b7rey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "APPRART", "NN", "APPR", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "List, Falschheit, Stolz und Zanck auf seiner alten Leyer.", "tokens": ["List", ",", "Falschheit", ",", "Stolz", "und", "Zanck", "auf", "sei\u00b7ner", "al\u00b7ten", "Le\u00b7yer", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NN", "KON", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}}, "stanza.2": {"line.1": {"text": "Dasz Polypragmon nechst ein alter Spielmann hies,", "tokens": ["Dasz", "Po\u00b7ly\u00b7prag\u00b