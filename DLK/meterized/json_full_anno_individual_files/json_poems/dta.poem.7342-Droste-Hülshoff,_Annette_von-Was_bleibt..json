{"dta.poem.7342": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Was bleibt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Seh' ich ein Kind zur Weihnachtsfrist,", "tokens": ["Seh'", "ich", "ein", "Kind", "zur", "Weih\u00b7nachts\u00b7frist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein rosig Kind mit Taubenaugen,", "tokens": ["Ein", "ro\u00b7sig", "Kind", "mit", "Tau\u00b7ben\u00b7au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Kunde von dem kleinen Christ", "tokens": ["Die", "Kun\u00b7de", "von", "dem", "klei\u00b7nen", "Christ"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Begierig aus den Lippen saugen,", "tokens": ["Be\u00b7gie\u00b7rig", "aus", "den", "Lip\u00b7pen", "sau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Aufhorchen, wenn es rauscht im Tann,", "tokens": ["Auf\u00b7hor\u00b7chen", ",", "wenn", "es", "rauscht", "im", "Tann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ob drau\u00dfen schon sein Pferdchen schnaube:", "tokens": ["Ob", "drau\u00b7\u00dfen", "schon", "sein", "Pferd\u00b7chen", "schnau\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "\u201eo Unschuld, Unschuld,\u201c denk ich dann,", "tokens": ["\u201e", "o", "Un\u00b7schuld", ",", "Un\u00b7schuld", ",", "\u201c", "denk", "ich", "dann", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "$,", "NN", "$,", "$(", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Du zarte, scheue, fl\u00fcchtge Taube!", "tokens": ["Du", "zar\u00b7te", ",", "scheu\u00b7e", ",", "fl\u00fccht\u00b7ge", "Tau\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und als die Wolke kaum verzog,", "tokens": ["Und", "als", "die", "Wol\u00b7ke", "kaum", "ver\u00b7zog", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Studenten klirrten durch die Stra\u00dfen,", "tokens": ["Stu\u00b7den\u00b7ten", "klirr\u00b7ten", "durch", "die", "Stra\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und: \u00bb", "tokens": ["Und", ":", "\u00bb"], "token_info": ["word", "punct", "punct"], "pos": ["KON", "$.", "$("], "meter": "+", "measure": "single.up"}, "line.4": {"text": "So keck und fr\u00f6hlich sonder Ma\u00dfen;", "tokens": ["So", "keck", "und", "fr\u00f6h\u00b7lich", "son\u00b7der", "Ma\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie schaarten sich wie eine Macht,", "tokens": ["Sie", "schaar\u00b7ten", "sich", "wie", "ei\u00b7ne", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die gegen den Kolo\u00df sich b\u00e4ume:", "tokens": ["Die", "ge\u00b7gen", "den", "Ko\u00b7lo\u00df", "sich", "b\u00e4u\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "\u201eo Hoffnung\u201c, hab' ich da gedacht,", "tokens": ["\u201e", "o", "Hoff\u00b7nung", "\u201c", ",", "hab'", "ich", "da", "ge\u00b7dacht", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "$(", "$,", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "\u201ewie bald zerrinnen Tr\u00e4um' und Sch\u00e4ume!\u201c", "tokens": ["\u201e", "wie", "bald", "zer\u00b7rin\u00b7nen", "Tr\u00e4um'", "und", "Sch\u00e4u\u00b7me", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "ADV", "ADJA", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und ihnen nach ein Reiter stampft,", "tokens": ["Und", "ih\u00b7nen", "nach", "ein", "Rei\u00b7ter", "stampft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geschm\u00fcckt mit Kreuz und Epaulette,", "tokens": ["Ge\u00b7schm\u00fcckt", "mit", "Kreuz", "und", "E\u00b7pau\u00b7let\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den Tzacko l\u00fcftet er, es dampft", "tokens": ["Den", "Tzac\u00b7ko", "l\u00fcf\u00b7tet", "er", ",", "es", "dampft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NE", "VVFIN", "PPER", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Oefen seines Scheitels Gl\u00e4tte;", "tokens": ["Wie", "O\u00b7e\u00b7fen", "sei\u00b7nes", "Schei\u00b7tels", "Gl\u00e4t\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "K\u00fchn war der Blick, der Arm noch stramm,", "tokens": ["K\u00fchn", "war", "der", "Blick", ",", "der", "Arm", "noch", "stramm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch droben schwebt' der Zeitenrabe:", "tokens": ["Doch", "dro\u00b7ben", "schwebt'", "der", "Zei\u00b7ten\u00b7ra\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da schien mir Kraft ein Meeresdamm,", "tokens": ["Da", "schien", "mir", "Kraft", "ein", "Mee\u00b7res\u00b7damm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den jeder Pulsschlag untergrabe.", "tokens": ["Den", "je\u00b7der", "Puls\u00b7schlag", "un\u00b7ter\u00b7gra\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und wieder durch die Gasse zog", "tokens": ["Und", "wie\u00b7der", "durch", "die", "Gas\u00b7se", "zog"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Studentenhauf, und vor dem Hause", "tokens": ["Stu\u00b7den\u00b7ten\u00b7hauf", ",", "und", "vor", "dem", "Hau\u00b7se"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Des Rektors dreimal \u201ehurrah hoch!\u201c", "tokens": ["Des", "Rek\u00b7tors", "drei\u00b7mal", "\u201e", "hur\u00b7rah", "hoch", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADV", "$(", "VVFIN", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wieder \u201ehoch!\u201c \u2014 aus seiner Klause,", "tokens": ["Und", "wie\u00b7der", "\u201e", "hoch", "!", "\u201c", "aus", "sei\u00b7ner", "Klau\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "punct", "punct", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$(", "ADJD", "$.", "$(", "$(", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "In Zipfelm\u00fctze und Flanell,", "tokens": ["In", "Zip\u00b7fel\u00b7m\u00fct\u00b7ze", "und", "Fla\u00b7nell", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Schemen nickt am Fensterbogen.", "tokens": ["Ein", "Sche\u00b7men", "nickt", "am", "Fens\u00b7ter\u00b7bo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "\u201eha\u201c, dacht ich, \u201eRuhm, du Mordgesell,", "tokens": ["\u201e", "ha", "\u201c", ",", "dacht", "ich", ",", "\u201e", "Ruhm", ",", "du", "Mord\u00b7ge\u00b7sell", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ITJ", "$(", "$,", "VVFIN", "PPER", "$,", "$(", "NN", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "K\u00f6mmst nur als Leichenhuhn geflogen!\u201c", "tokens": ["K\u00f6mmst", "nur", "als", "Lei\u00b7chen\u00b7huhn", "ge\u00b7flo\u00b7gen", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADV", "KOUS", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "An meine Wange haucht' es dicht,", "tokens": ["An", "mei\u00b7ne", "Wan\u00b7ge", "haucht'", "es", "dicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wie das Haupt ich seitw\u00e4rts regte,", "tokens": ["Und", "wie", "das", "Haupt", "ich", "seit\u00b7w\u00e4rts", "reg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da sah ich in das Angesicht", "tokens": ["Da", "sah", "ich", "in", "das", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Frau, die meine Kindheit pflegte,", "tokens": ["Der", "Frau", ",", "die", "mei\u00b7ne", "Kind\u00b7heit", "pfleg\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dies Antlitz wo Erinnerung", "tokens": ["Dies", "Ant\u00b7litz", "wo", "E\u00b7rin\u00b7ne\u00b7rung"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "NN", "PWAV", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.6": {"text": "Und werthe Gegenwart sich paaren:", "tokens": ["Und", "wert\u00b7he", "Ge\u00b7gen\u00b7wart", "sich", "paa\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "\u201eo Liebe\u201c, dacht ich, \u201eewig jung,", "tokens": ["\u201e", "o", "Lie\u00b7be", "\u201c", ",", "dacht", "ich", ",", "\u201e", "e\u00b7wig", "jung", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "$(", "$,", "VVFIN", "PPER", "$,", "$(", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und ewig frisch bei grauen Haaren!\u201c", "tokens": ["Und", "e\u00b7wig", "frisch", "bei", "grau\u00b7en", "Haa\u00b7ren", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "ADJD", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}