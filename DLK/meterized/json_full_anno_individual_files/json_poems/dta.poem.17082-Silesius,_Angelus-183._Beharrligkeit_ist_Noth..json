{"dta.poem.17082": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "183.  Beharrligkeit ist Noth.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Das gr\u00f6ste das ein Mensch bedarff zur seeligkeit/", "tokens": ["Das", "gr\u00f6s\u00b7te", "das", "ein", "Mensch", "be\u00b7darff", "zur", "see\u00b7lig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ART", "ART", "NN", "ADJD", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "(wo er im gutten steht) ist die beharrligkeit.", "tokens": ["(", "wo", "er", "im", "gut\u00b7ten", "steht", ")", "ist", "die", "be\u00b7harr\u00b7lig\u00b7keit", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PPER", "APPRART", "ADJA", "VVFIN", "$(", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}