{"dta.poem.14852": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die stille Gemeine.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.71", "af:0.28"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Von Bretagne's H\u00fcgeln, die das Meer", "tokens": ["Von", "Bre\u00b7ta\u00b7gne's", "H\u00fc\u00b7geln", ",", "die", "das", "Meer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bl\u00fchend hell ums\u00e4umen,", "tokens": ["Bl\u00fc\u00b7hend", "hell", "um\u00b7s\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Schaute ein Kirchlein trostreich her", "tokens": ["Schau\u00b7te", "ein", "Kirch\u00b7lein", "trost\u00b7reich", "her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ADJD", "PTKVZ"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Zwischen uralten B\u00e4umen.", "tokens": ["Zwi\u00b7schen", "ur\u00b7al\u00b7ten", "B\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}