{"textgrid.poem.47394": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich st\u00fcrze meinen Wogenschwall", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich st\u00fcrze meinen Wogenschwall", "tokens": ["Ich", "st\u00fcr\u00b7ze", "mei\u00b7nen", "Wo\u00b7gen\u00b7schwall"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch grause Felsenkl\u00fcfte,", "tokens": ["Durch", "grau\u00b7se", "Fel\u00b7sen\u00b7kl\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und meines Ganges Donnerhall", "tokens": ["Und", "mei\u00b7nes", "Gan\u00b7ges", "Don\u00b7ner\u00b7hall"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vernehmen alle L\u00fcfte.", "tokens": ["Ver\u00b7neh\u00b7men", "al\u00b7le", "L\u00fcf\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df ich ein wilder Gie\u00dfbach bin", "tokens": ["Da\u00df", "ich", "ein", "wil\u00b7der", "Gie\u00df\u00b7bach", "bin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und hege keinen sanften Sinn,", "tokens": ["Und", "he\u00b7ge", "kei\u00b7nen", "sanf\u00b7ten", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das mag ein jeder wissen.", "tokens": ["Das", "mag", "ein", "je\u00b7der", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "In Fesseln thu' ich nimmer gut", "tokens": ["In", "Fes\u00b7seln", "thu'", "ich", "nim\u00b7mer", "gut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und dulde keine Banden;", "tokens": ["Und", "dul\u00b7de", "kei\u00b7ne", "Ban\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wer begegnet meinem Mut,", "tokens": ["Und", "wer", "be\u00b7geg\u00b7net", "mei\u00b7nem", "Mut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der wird vor ihm zu schanden;", "tokens": ["Der", "wird", "vor", "ihm", "zu", "schan\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und wer sich mir entgegenstemmt,", "tokens": ["Und", "wer", "sich", "mir", "ent\u00b7ge\u00b7gens\u00b7temmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PRF", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mich in der stolzen Freiheit hemmt", "tokens": ["Mich", "in", "der", "stol\u00b7zen", "Frei\u00b7heit", "hemmt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er mu\u00df den Frevel b\u00fc\u00dfen.", "tokens": ["Er", "mu\u00df", "den", "Fre\u00b7vel", "b\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Den Felsen, der in meiner Bahn", "tokens": ["Den", "Fel\u00b7sen", ",", "der", "in", "mei\u00b7ner", "Bahn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich keck entgegen t\u00fcrmet,", "tokens": ["Sich", "keck", "ent\u00b7ge\u00b7gen", "t\u00fcr\u00b7met", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Fass' ich mit starken Armen an,", "tokens": ["Fass'", "ich", "mit", "star\u00b7ken", "Ar\u00b7men", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er wird hinabgest\u00fcrmet;", "tokens": ["Er", "wird", "hin\u00b7ab\u00b7ge\u00b7st\u00fcr\u00b7met", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das Bl\u00fcmlein aber, das geb\u00fcckt", "tokens": ["Das", "Bl\u00fcm\u00b7lein", "a\u00b7ber", ",", "das", "ge\u00b7b\u00fcckt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auf meine Strudel niederblickt,", "tokens": ["Auf", "mei\u00b7ne", "Stru\u00b7del", "nie\u00b7der\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Darf ohne Scheu mich k\u00fcssen.", "tokens": ["Darf", "oh\u00b7ne", "Scheu", "mich", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und d\u00e4mpfen wollt' ich meinen Mut,", "tokens": ["Und", "d\u00e4mp\u00b7fen", "wollt'", "ich", "mei\u00b7nen", "Mut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn ich ein Quellchen f\u00e4nde,", "tokens": ["Wenn", "ich", "ein", "Quell\u00b7chen", "f\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das willig seine klare Flut", "tokens": ["Das", "wil\u00b7lig", "sei\u00b7ne", "kla\u00b7re", "Flut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ADJD", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit meinem Strom verb\u00e4nde:", "tokens": ["Mit", "mei\u00b7nem", "Strom", "ver\u00b7b\u00e4n\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich g\u00e4be meinen wilden Sinn", "tokens": ["Ich", "g\u00e4\u00b7be", "mei\u00b7nen", "wil\u00b7den", "Sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Geduldig ihr in Fesseln hin", "tokens": ["Ge\u00b7dul\u00b7dig", "ihr", "in", "Fes\u00b7seln", "hin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "PPER", "APPR", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und lernte sanft zu murmeln.", "tokens": ["Und", "lern\u00b7te", "sanft", "zu", "mur\u00b7meln", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und wenn du denn das Quellchen bist,", "tokens": ["Und", "wenn", "du", "denn", "das", "Quell\u00b7chen", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wenn ich dir gefalle,", "tokens": ["Und", "wenn", "ich", "dir", "ge\u00b7fal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So sei mir froh als Braut gegr\u00fc\u00dft,", "tokens": ["So", "sei", "mir", "froh", "als", "Braut", "ge\u00b7gr\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KOKOM", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So zeuch in meine Halle;", "tokens": ["So", "zeuch", "in", "mei\u00b7ne", "Hal\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mein Haus ist hochgew\u00f6lbt und k\u00fchl,", "tokens": ["Mein", "Haus", "ist", "hoch\u00b7ge\u00b7w\u00f6lbt", "und", "k\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "La\u00df uns der Minne s\u00fc\u00dfes Spiel", "tokens": ["La\u00df", "uns", "der", "Min\u00b7ne", "s\u00fc\u00b7\u00dfes", "Spiel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In seinem Schatten spielen.", "tokens": ["In", "sei\u00b7nem", "Schat\u00b7ten", "spie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und wenn der wilde Schaum dich schreckt", "tokens": ["Und", "wenn", "der", "wil\u00b7de", "Schaum", "dich", "schreckt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf meines Reiches Fl\u00e4che,", "tokens": ["Auf", "mei\u00b7nes", "Rei\u00b7ches", "Fl\u00e4\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sieh nur herein, der Schaum bedeckt", "tokens": ["Sieh", "nur", "her\u00b7ein", ",", "der", "Schaum", "be\u00b7deckt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADV", "PTKVZ", "$,", "ART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kristallne Wasserb\u00e4che.", "tokens": ["Kris\u00b7tall\u00b7ne", "Was\u00b7ser\u00b7b\u00e4\u00b7che", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Rein komm' ich aus der Mutter Scho\u00df,", "tokens": ["Rein", "komm'", "ich", "aus", "der", "Mut\u00b7ter", "Scho\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und keine tr\u00fcbe Lache flo\u00df", "tokens": ["Und", "kei\u00b7ne", "tr\u00fc\u00b7be", "La\u00b7che", "flo\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit meinem Strom zusammen.", "tokens": ["Mit", "mei\u00b7nem", "Strom", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "So komm und la\u00df mein Brausen dir,", "tokens": ["So", "komm", "und", "la\u00df", "mein", "Brau\u00b7sen", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVIMP", "PPOSAT", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie ich dich liebe, sagen.", "tokens": ["Wie", "ich", "dich", "lie\u00b7be", ",", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Komm, Braut, o komm, und la\u00df von mir", "tokens": ["Komm", ",", "Braut", ",", "o", "komm", ",", "und", "la\u00df", "von", "mir"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "NN", "$,", "FM", "VVFIN", "$,", "KON", "VVIMP", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In meinem Arm dich tragen,", "tokens": ["In", "mei\u00b7nem", "Arm", "dich", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Von meiner stolzen Manneskraft", "tokens": ["Von", "mei\u00b7ner", "stol\u00b7zen", "Man\u00b7nes\u00b7kraft"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In hohem Lauf dahingerafft", "tokens": ["In", "ho\u00b7hem", "Lauf", "da\u00b7hin\u00b7ge\u00b7rafft"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zum Bett des Ozeanes.", "tokens": ["Zum", "Bett", "des", "O\u00b7zea\u00b7nes", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.8": {"line.1": {"text": "Ich st\u00fcrze meinen Wogenschwall", "tokens": ["Ich", "st\u00fcr\u00b7ze", "mei\u00b7nen", "Wo\u00b7gen\u00b7schwall"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch grause Felsenkl\u00fcfte,", "tokens": ["Durch", "grau\u00b7se", "Fel\u00b7sen\u00b7kl\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und meines Ganges Donnerhall", "tokens": ["Und", "mei\u00b7nes", "Gan\u00b7ges", "Don\u00b7ner\u00b7hall"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vernehmen alle L\u00fcfte.", "tokens": ["Ver\u00b7neh\u00b7men", "al\u00b7le", "L\u00fcf\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "po