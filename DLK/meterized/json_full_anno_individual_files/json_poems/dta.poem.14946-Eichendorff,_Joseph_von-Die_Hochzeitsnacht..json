{"dta.poem.14946": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Hochzeitsnacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Du siehst so still und wilde,", "tokens": ["Du", "siehst", "so", "still", "und", "wil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "KON", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So bleich wird Dein Gesicht,", "tokens": ["So", "bleich", "wird", "Dein", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mir graut vor Deinem Bilde \u2014", "tokens": ["Mir", "graut", "vor", "Dei\u00b7nem", "Bil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du bist mein Br\u00e4ut'gam nicht!\u201c \u2014", "tokens": ["Du", "bist", "mein", "Br\u00e4ut'\u00b7gam", "nicht", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NE", "PTKNEG", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}