{"textgrid.poem.43527": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Frieden ruht auf Berg und Thal,", "tokens": ["Der", "Frie\u00b7den", "ruht", "auf", "Berg", "und", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Welt will schlafen allzumal.", "tokens": ["Die", "Welt", "will", "schla\u00b7fen", "all\u00b7zu\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Sternlein blickt zum Fenster 'nein,", "tokens": ["Ein", "Stern\u00b7lein", "blickt", "zum", "Fens\u00b7ter", "'n\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "NE", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und fragt: schl\u00e4ft Kindlein noch nicht ein?", "tokens": ["Und", "fragt", ":", "schl\u00e4ft", "Kin\u00b7dlein", "noch", "nicht", "ein", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "VVFIN", "NN", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbmein Kind ist gut und folget gern,", "tokens": ["\u00bb", "mein", "Kind", "ist", "gut", "und", "fol\u00b7get", "gern", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "ADJD", "KON", "VVFIN", "ADV", "$,"], "m