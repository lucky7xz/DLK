{"dta.poem.13184": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z ehndes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "An einer Seite war ein hoch erhabner Chor,", "tokens": ["An", "ei\u00b7ner", "Sei\u00b7te", "war", "ein", "hoch", "er\u00b7hab\u00b7ner", "Chor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "10Von diesem klungen stets verschiedne Stimmen vor.", "tokens": ["die\u00b7sem", "klun\u00b7gen", "stets", "ver\u00b7schied\u00b7ne", "Stim\u00b7men", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "ADV", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Der Sayten s\u00fcsser Thon, der Klang vermischter Fl\u00f6ten,", "tokens": ["Der", "Say\u00b7ten", "s\u00fcs\u00b7ser", "Thon", ",", "der", "Klang", "ver\u00b7mischter", "Fl\u00f6\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Der Paucken Murr-Gesumm, der Schall der Feld-Trompeten,", "tokens": ["Der", "Pau\u00b7cken", "Murr\u00b7Ge\u00b7summ", ",", "der", "Schall", "der", "Feld\u00b7Trom\u00b7pe\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und was die Lust und Kunst zum Reiz des Ohrs erfand,", "tokens": ["Und", "was", "die", "Lust", "und", "Kunst", "zum", "Reiz", "des", "Ohrs", "er\u00b7fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "KON", "NN", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "War, was von Anfang gleich auf diesem Chor entstand.", "tokens": ["War", ",", "was", "von", "An\u00b7fang", "gleich", "auf", "die\u00b7sem", "Chor", "ent\u00b7stand", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PRELS", "APPR", "NN", "ADV", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "15Das Herz empfande sich in munterem Frolocken,", "tokens": ["Herz", "emp\u00b7fan\u00b7de", "sich", "in", "mun\u00b7te\u00b7rem", "Fro\u00b7lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.8": {"text": "Dann alles f\u00fcgte sich, zu Freuden anzulocken.", "tokens": ["Dann", "al\u00b7les", "f\u00fcg\u00b7te", "sich", ",", "zu", "Freu\u00b7den", "an\u00b7zu\u00b7lo\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PRF", "$,", "APPR", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}