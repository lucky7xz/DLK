{"dta.poem.21402": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XXII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Neuen essen Schweinefleisch,", "tokens": ["Die", "Neu\u00b7en", "es\u00b7sen", "Schwei\u00b7ne\u00b7fleisch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zeigen sich widersetzig,", "tokens": ["Zei\u00b7gen", "sich", "wi\u00b7der\u00b7set\u00b7zig", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Sind Demokraten; die Alten sind", "tokens": ["Sind", "De\u00b7mo\u00b7kra\u00b7ten", ";", "die", "Al\u00b7ten", "sind"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "NN", "$.", "ART", "NN", "VAFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Vielmehr aristokr\u00e4tzig.", "tokens": ["Viel\u00b7mehr", "a\u00b7ris\u00b7to\u00b7kr\u00e4t\u00b7zig", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich liebe die Alten, ich liebe die Neu\u2019n \u2013", "tokens": ["Ich", "lie\u00b7be", "die", "Al\u00b7ten", ",", "ich", "lie\u00b7be", "die", "Neu'n", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Doch schw\u00f6r\u2019 ich, beim ewigen Gotte,", "tokens": ["Doch", "schw\u00f6r'", "ich", ",", "beim", "e\u00b7wi\u00b7gen", "Got\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ich liebe gewisse Fischchen noch mehr,", "tokens": ["Ich", "lie\u00b7be", "ge\u00b7wis\u00b7se", "Fischchen", "noch", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "ADV", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Man hei\u00dft sie ger\u00e4ucherte Sprotte.", "tokens": ["Man", "hei\u00dft", "sie", "ge\u00b7r\u00e4u\u00b7cher\u00b7te", "Sprot\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}