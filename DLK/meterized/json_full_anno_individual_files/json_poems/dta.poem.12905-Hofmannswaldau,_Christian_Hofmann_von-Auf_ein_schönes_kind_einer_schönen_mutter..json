{"dta.poem.12905": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf ein sch\u00f6nes kind einer sch\u00f6nen  \n mutter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zart und schnee-wei\u00dfes kind! das an der Daphne br\u00fcsten,", "tokens": ["Zart", "und", "schnee\u00b7wei\u00b7\u00dfes", "kind", "!", "das", "an", "der", "Daph\u00b7ne", "br\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJA", "NN", "$.", "ART", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-++-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "Nach denen so viel h\u00e4nd\u2019 und m\u00fcnd\u2019 umsonst gel\u00fcsten,", "tokens": ["Nach", "de\u00b7nen", "so", "viel", "h\u00e4nd'", "und", "m\u00fcnd'", "um\u00b7sonst", "ge\u00b7l\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADV", "ADV", "ADJD", "KON", "VMFIN", "ADV", "VVPP", "$,"], "meter": "-+--++-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Als eine muntre bien\u2019 an s\u00fc\u00dfen rosen h\u00e4ngt,", "tokens": ["Als", "ei\u00b7ne", "mun\u00b7tre", "bi\u00b7en'", "an", "s\u00fc\u00b7\u00dfen", "ro\u00b7sen", "h\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und sich an statt der milch, mit nectar-s\u00e4fften tr\u00e4nckt;", "tokens": ["Und", "sich", "an", "statt", "der", "milch", ",", "mit", "nec\u00b7ta\u00b7rs\u00e4ff\u00b7ten", "tr\u00e4nckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "APPR", "ART", "NN", "$,", "APPR", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mein auge kan in dir nichts menschliches erkennen.", "tokens": ["Mein", "au\u00b7ge", "kan", "in", "dir", "nichts", "menschli\u00b7ches", "er\u00b7ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "APPR", "PPER", "PIS", "PIS", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Weil du nun g\u00f6ttlich bist, wie soll ich dich denn nennen?", "tokens": ["Weil", "du", "nun", "g\u00f6tt\u00b7lich", "bist", ",", "wie", "soll", "ich", "dich", "denn", "nen\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$,", "PWAV", "VMFIN", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Zwar wenn ich Daphnen seh\u2019 so k\u00f6mmt mir Venus ein,", "tokens": ["Zwar", "wenn", "ich", "Daph\u00b7nen", "seh'", "so", "k\u00f6mmt", "mir", "Ve\u00b7nus", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "NN", "VVFIN", "ADV", "VVFIN", "PPER", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Drum wirst du, als ihr sohn, gewi\u00df Cupido seyn.", "tokens": ["Drum", "wirst", "du", ",", "als", "ihr", "sohn", ",", "ge\u00b7wi\u00df", "Cu\u00b7pi\u00b7do", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "$,", "ADV", "NE", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}