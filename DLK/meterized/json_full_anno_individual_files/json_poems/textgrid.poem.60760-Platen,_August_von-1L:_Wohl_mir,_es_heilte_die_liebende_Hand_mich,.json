{"textgrid.poem.60760": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wohl mir, es heilte die liebende Hand mich,", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl mir, es heilte die liebende Hand mich,", "tokens": ["Wohl", "mir", ",", "es", "heil\u00b7te", "die", "lie\u00b7ben\u00b7de", "Hand", "mich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die mit balsamischem Blatte verband mich;", "tokens": ["Die", "mit", "bal\u00b7sa\u00b7mi\u00b7schem", "Blat\u00b7te", "ver\u00b7band", "mich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Als mich in Flammen umdrohte Verzweiflung,", "tokens": ["Als", "mich", "in", "Flam\u00b7men", "um\u00b7droh\u00b7te", "Ver\u00b7zwei\u00b7flung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Deckte des Glaubens asbesten Gewand mich;", "tokens": ["Deck\u00b7te", "des", "Glau\u00b7bens", "as\u00b7bes\u00b7ten", "Ge\u00b7wand", "mich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "PPER", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "Irrend durchstrich ich das waldige Dickicht,", "tokens": ["Ir\u00b7rend", "durch\u00b7strich", "ich", "das", "wal\u00b7di\u00b7ge", "Di\u00b7ckicht", ","], "to