{"dta.poem.20486": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "An Melinden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich rede nur mit steinen.", "tokens": ["Ich", "re\u00b7de", "nur", "mit", "stei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dein stoltzes ohre h\u00f6rt mich nicht/", "tokens": ["Dein", "stolt\u00b7zes", "oh\u00b7re", "h\u00f6rt", "mich", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "PTKNEG", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und deiner augen feurig licht", "tokens": ["Und", "dei\u00b7ner", "au\u00b7gen", "feu\u00b7rig", "licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Will mir nur ewig grausam scheinen.", "tokens": ["Will", "mir", "nur", "e\u00b7wig", "grau\u00b7sam", "schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Kan denn mein nasses thr\u00e4nen-meer", "tokens": ["Kan", "denn", "mein", "nas\u00b7ses", "thr\u00e4\u00b7nen\u00b7meer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht deines zornes glut abwaschen?", "tokens": ["Nicht", "dei\u00b7nes", "zor\u00b7nes", "glut", "ab\u00b7wa\u00b7schen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So siehe/ wie ich mich verzehr!", "tokens": ["So", "sie\u00b7he", "/", "wie", "ich", "mich", "ver\u00b7zehr", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "$(", "PWAV", "PPER", "PRF", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dein heisser augen-plitz verbrennet mich zu aschen.", "tokens": ["Dein", "heis\u00b7ser", "au\u00b7gen\u00b7plitz", "ver\u00b7bren\u00b7net", "mich", "zu", "asc\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was zwinget dich/ Melinde/", "tokens": ["Was", "zwin\u00b7get", "dich", "/", "Me\u00b7lin\u00b7de", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$(", "NE", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So grausam gegen mir zu seyn?", "tokens": ["So", "grau\u00b7sam", "ge\u00b7gen", "mir", "zu", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPER", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verdopple doch nicht meine pein/", "tokens": ["Ver\u00b7dopp\u00b7le", "doch", "nicht", "mei\u00b7ne", "pein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKNEG", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weil ich ohn dem genug empfinde;", "tokens": ["Weil", "ich", "ohn", "dem", "ge\u00b7nug", "emp\u00b7fin\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du straffest mich ja gar zu sehr.", "tokens": ["Du", "straf\u00b7fest", "mich", "ja", "gar", "zu", "sehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "PTKA", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Verdienet denn ein treues lieben", "tokens": ["Ver\u00b7die\u00b7net", "denn", "ein", "treu\u00b7es", "lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Bey dir schon kein erbarmni\u00df mehr/", "tokens": ["Bey", "dir", "schon", "kein", "er\u00b7barm\u00b7ni\u00df", "mehr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "PIAT", "NN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wo hast du/ sch\u00f6nste/ denn die feinde hingeschrieben?", "tokens": ["Wo", "hast", "du", "/", "sch\u00f6ns\u00b7te", "/", "denn", "die", "fein\u00b7de", "hin\u00b7ge\u00b7schrie\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$(", "VVFIN", "$(", "KON", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was wilst du ferner haben?", "tokens": ["Was", "wilst", "du", "fer\u00b7ner", "ha\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Begehrst du doch mein blut zu lohn?", "tokens": ["Be\u00b7gehrst", "du", "doch", "mein", "blut", "zu", "lohn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn meine seele hast du schon/", "tokens": ["Denn", "mei\u00b7ne", "see\u00b7le", "hast", "du", "schon", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPER", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So nimm es; du will ichs vergraben.", "tokens": ["So", "nimm", "es", ";", "du", "will", "ichs", "ver\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "$.", "PPER", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wird hierdurch deine lust gestillt/", "tokens": ["Wird", "hier\u00b7durch", "dei\u00b7ne", "lust", "ge\u00b7stillt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PAV", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So will ich mich noch sterbend freuen/", "tokens": ["So", "will", "ich", "mich", "noch", "ster\u00b7bend", "freu\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und also mach ichs wie du wilt;", "tokens": ["Und", "al\u00b7so", "mach", "ichs", "wie", "du", "wilt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "KOKOM", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Doch wo du menschlich bist/ so wird mein tod dich reuen.", "tokens": ["Doch", "wo", "du", "menschlich", "bist", "/", "so", "wird", "mein", "tod", "dich", "reu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADJD", "VAFIN", "$(", "ADV", "VAFIN", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}}}}