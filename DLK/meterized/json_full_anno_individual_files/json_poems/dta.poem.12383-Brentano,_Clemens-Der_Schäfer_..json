{"dta.poem.12383": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Der Sch\u00e4fer .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mond des Himmels treib zur Weide               ", "tokens": ["Mond", "des", "Him\u00b7mels", "treib", "zur", "Wei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deine Sch\u00e4flein g\u00fclden gelb,", "tokens": ["Dei\u00b7ne", "Sch\u00e4f\u00b7lein", "g\u00fcl\u00b7den", "gelb", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf gew\u00f6lbter blauer Heide", "tokens": ["Auf", "ge\u00b7w\u00f6lb\u00b7ter", "blau\u00b7er", "Hei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00df die Sterne walten selbst,", "tokens": ["La\u00df", "die", "Ster\u00b7ne", "wal\u00b7ten", "selbst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich noch neulich so th\u00e4t reden,", "tokens": ["Ich", "noch", "neu\u00b7lich", "so", "th\u00e4t", "re\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADV", "ADJD", "VVINF", "$,"], "meter": "--+--+--", "measure": "anapaest.di.plus"}, "line.6": {"text": "Da zu Nacht ein schwacher Hirt,", "tokens": ["Da", "zu", "Nacht", "ein", "schwa\u00b7cher", "Hirt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Aller Wegen, Steg und Pf\u00e4den,", "tokens": ["Al\u00b7ler", "We\u00b7gen", ",", "Steg", "und", "Pf\u00e4\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sucht ein Sch\u00e4flein mit Begierd.", "tokens": ["Sucht", "ein", "Sch\u00e4f\u00b7lein", "mit", "Be\u00b7gierd", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und der Mond h\u00f6rt' was ich sagte,", "tokens": ["Und", "der", "Mond", "h\u00f6rt'", "was", "ich", "sag\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PWS", "PPER", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Nahm ein lind gestimmtes Rohr:", "tokens": ["Nahm", "ein", "lind", "ge\u00b7stimm\u00b7tes", "Rohr", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das er blasend z\u00e4rtlich nagte,", "tokens": ["Das", "er", "bla\u00b7send", "z\u00e4rt\u00b7lich", "nag\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Spielte seinen Sternen vor.", "tokens": ["Spiel\u00b7te", "sei\u00b7nen", "Ster\u00b7nen", "vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Auf ihr Sch\u00e4flein, auf zur Heiden", "tokens": ["Auf", "ihr", "Sch\u00e4f\u00b7lein", ",", "auf", "zur", "Hei\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weidet reines Himmelblau,", "tokens": ["Wei\u00b7det", "rei\u00b7nes", "Him\u00b7mel\u00b7blau", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df nachher, wenn wir hier scheiden,", "tokens": ["Da\u00df", "nach\u00b7her", ",", "wenn", "wir", "hier", "schei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "$,", "KOUS", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von euch flie\u00dft der Morgenthau.", "tokens": ["Von", "euch", "flie\u00dft", "der", "Mor\u00b7gent\u00b7hau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach wer aber dort im Garten", "tokens": ["Ach", "wer", "a\u00b7ber", "dort", "im", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "PWS", "ADV", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liegt mit seinem Hirtenstab!", "tokens": ["Liegt", "mit", "sei\u00b7nem", "Hir\u00b7ten\u00b7stab", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer will seiner dorten warten!", "tokens": ["Wer", "will", "sei\u00b7ner", "dor\u00b7ten", "war\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPOSAT", "ADV", "VVINF", "$."], "meter": "++--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Schaut ihr Sternlein, schaut herab,", "tokens": ["Schaut", "ihr", "Stern\u00b7lein", ",", "schaut", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Haltet, haltet, ich nicht fehle:", "tokens": ["Hal\u00b7tet", ",", "hal\u00b7tet", ",", "ich", "nicht", "feh\u00b7le", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Es ist Daphnis wohl bekannt.", "tokens": ["Es", "ist", "Daph\u00b7nis", "wohl", "be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Eja, Daphnis, mir erz\u00e4hle,", "tokens": ["E\u00b7ja", ",", "Daph\u00b7nis", ",", "mir", "er\u00b7z\u00e4h\u00b7le", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Daphnis, was will dieser Stand?", "tokens": ["Daph\u00b7nis", ",", "was", "will", "die\u00b7ser", "Stand", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "VMFIN", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Weidet meine Sch\u00e4flein, weidet!", "tokens": ["Wei\u00b7det", "mei\u00b7ne", "Sch\u00e4f\u00b7lein", ",", "wei\u00b7det", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich mit ihm noch reden mu\u00df,", "tokens": ["Ich", "mit", "ihm", "noch", "re\u00b7den", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Weidet meine Sterne, weidet!", "tokens": ["Wei\u00b7det", "mei\u00b7ne", "Ster\u00b7ne", ",", "wei\u00b7det", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Daphnis liegt in harter Bu\u00df,", "tokens": ["Daph\u00b7nis", "liegt", "in", "har\u00b7ter", "Bu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Daphnis thu' die Lippen r\u00fchren,", "tokens": ["Daph\u00b7nis", "thu'", "die", "Lip\u00b7pen", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eja, nicht verbleibe stumm,", "tokens": ["E\u00b7ja", ",", "nicht", "ver\u00b7blei\u00b7be", "stumm", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PTKNEG", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Daphnis, la\u00df dich dannen f\u00fchren,", "tokens": ["Daph\u00b7nis", ",", "la\u00df", "dich", "dan\u00b7nen", "f\u00fch\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVIMP", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Eja, nicht verbleibe stumm.", "tokens": ["E\u00b7ja", ",", "nicht", "ver\u00b7blei\u00b7be", "stumm", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PTKNEG", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Weidet meine Sch\u00e4flein, weidet,", "tokens": ["Wei\u00b7det", "mei\u00b7ne", "Sch\u00e4f\u00b7lein", ",", "wei\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Daphnis liegt in Aengsten gro\u00df,", "tokens": ["Daph\u00b7nis", "liegt", "in", "A\u00b7engs\u00b7ten", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NE", "ADJD", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Daphnis Pein und Marter leidet,", "tokens": ["Daph\u00b7nis", "Pein", "und", "Mar\u00b7ter", "lei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wollt', er l\u00e4g im Mutterschos!", "tokens": ["Wollt'", ",", "er", "l\u00e4g", "im", "Mut\u00b7ter\u00b7schos", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Er dem Felsen liegt in Armen,", "tokens": ["Er", "dem", "Fel\u00b7sen", "liegt", "in", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Liegt auf harten Steinen blo\u00df:", "tokens": ["Liegt", "auf", "har\u00b7ten", "Stei\u00b7nen", "blo\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ach dort wird er nie erwarmen!", "tokens": ["Ach", "dort", "wird", "er", "nie", "er\u00b7war\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "F\u00fcrcht, da\u00df er sein Haupt zersto\u00df.", "tokens": ["F\u00fcrcht", ",", "da\u00df", "er", "sein", "Haupt", "zer\u00b7sto\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Weidet meine Sch\u00e4flein, weidet,", "tokens": ["Wei\u00b7det", "mei\u00b7ne", "Sch\u00e4f\u00b7lein", ",", "wei\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Daphnis spaltet mir das Herz:", "tokens": ["Daph\u00b7nis", "spal\u00b7tet", "mir", "das", "Herz", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer mag haben ihn beleidet?", "tokens": ["Wer", "mag", "ha\u00b7ben", "ihn", "be\u00b7lei\u00b7det", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "VAFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weinen m\u00f6chten Stein und Erz;", "tokens": ["Wei\u00b7nen", "m\u00f6ch\u00b7ten", "Stein", "und", "Erz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Kalter Wind, halt ein die Fl\u00fcgel,", "tokens": ["Kal\u00b7ter", "Wind", ",", "halt", "ein", "die", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "ART", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "R\u00fchre nicht das kranke Blut,", "tokens": ["R\u00fch\u00b7re", "nicht", "das", "kran\u00b7ke", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Meide jenen Berg und H\u00fcgel,", "tokens": ["Mei\u00b7de", "je\u00b7nen", "Berg", "und", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PDAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Daphnis liegt ohn Schuh und Hut.", "tokens": ["Daph\u00b7nis", "liegt", "ohn", "Schuh", "und", "Hut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Weidet meine Sch\u00e4flein, weidet,", "tokens": ["Wei\u00b7det", "mei\u00b7ne", "Sch\u00e4f\u00b7lein", ",", "wei\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Daphnis leidet Angst und Noth,", "tokens": ["Daph\u00b7nis", "lei\u00b7det", "Angst", "und", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": 