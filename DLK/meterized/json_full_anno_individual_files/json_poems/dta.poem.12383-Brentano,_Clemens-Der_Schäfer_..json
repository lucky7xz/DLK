ra"}, "line.2": {"text": "Alles, alles ist umsonst,", "tokens": ["Al\u00b7les", ",", "al\u00b7les", "ist", "um\u00b7sonst", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PIS", "VAFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Er doch allen Trost vermeidet,", "tokens": ["Er", "doch", "al\u00b7len", "Trost", "ver\u00b7mei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sang und Becher bleibt umsonst.", "tokens": ["Sang", "und", "Be\u00b7cher", "bleibt", "um\u00b7sonst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "O du frommer Knab von oben,", "tokens": ["O", "du", "from\u00b7mer", "Knab", "von", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "APPR", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Du nur mehrest ihm die Pein,", "tokens": ["Du", "nur", "meh\u00b7rest", "ihm", "die", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch ich deine Treu mu\u00df loben,", "tokens": ["Doch", "ich", "dei\u00b7ne", "Treu", "mu\u00df", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Gott! dirs mu\u00df geklaget seyn.", "tokens": ["Gott", "!", "dirs", "mu\u00df", "ge\u00b7kla\u00b7get", "seyn", "."], "token_info": ["word", "punct", "w