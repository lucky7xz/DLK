: {"text": "D\u00fcrft vielleicht wohl morgen schreien:", "tokens": ["D\u00fcrft", "viel\u00b7leicht", "wohl", "mor\u00b7gen", "schrei\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ach wie sehr mich d\u00fcrstet nun!", "tokens": ["Ach", "wie", "sehr", "mich", "d\u00fcrs\u00b7tet", "nun", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ADV", "PPER", "VMFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Weidet meine Sch\u00e4flein, weidet,", "tokens": ["Wei\u00b7det", "mei\u00b7ne", "Sch\u00e4f\u00b7lein", ",", "wei\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Daphnis bleibet schmerzenvoll,", "tokens": ["Daph\u00b7nis", "blei\u00b7bet", "schmer\u00b7zen\u00b7voll", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich befehle euch entkleidet,", "tokens": ["Ich", "be\u00b7feh\u00b7le", "euch", "ent\u00b7klei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Reisset aus die g\u00fcldne Woll,", "tokens": ["Reis\u00b7set", "aus", "die", "g\u00fcld\u00b7ne", "Woll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nur euch kleidet pur in Kohlen,", "tokens": ["Nur", "euch", "klei\u00b7det", "pur", "in", "Koh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Pur in lauter schwarz Gewand,", "tokens": ["Pur", "in", "lau\u00b7ter", "schwarz", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Von dem Scheitel auf die Sohlen", "tokens": ["Von", "dem", "Schei\u00b7tel", "auf", "die", "Soh\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Euch geb\u00fchret solcher Stand.", "tokens": ["Euch", "ge\u00b7b\u00fch\u00b7ret", "sol\u00b7cher", "Stand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Weidet meine Sch\u00e4flein,", "tokens": ["Wei\u00b7det", "mei\u00b7ne", "Sch\u00e4f\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Daphnis f\u00fchret starkes Leid,", "tokens": ["Daph\u00b7nis", "f\u00fch\u00b7ret", "star\u00b7kes", "Leid", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist vom Vater hoch vereidet,", "tokens": ["Ist", "vom", "Va\u00b7ter", "hoch", "ver\u00b7ei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hoch, mit wohl bedachtem Eid,", "tokens": ["Hoch", ",", "mit", "wohl", "be\u00b7dach\u00b7tem", "Eid", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Er doch wollte wieder bringen,", "tokens": ["Er", "doch", "woll\u00b7te", "wie\u00b7der", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ein verloren Sch\u00e4flein sein,", "tokens": ["Ein", "ver\u00b7lo\u00b7ren", "Sch\u00e4f\u00b7lein", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ach wenn sollte das mi\u00dflingen,", "tokens": ["Ach", "wenn", "soll\u00b7te", "das", "mi\u00df\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "VMFIN", "PDS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Er ja st\u00fcrb f\u00fcr lauter Pein.", "tokens": ["Er", "ja", "st\u00fcrb", "f\u00fcr", "lau\u00b7ter", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Weidet meine Sch\u00e4flein, weidet,", "tokens": ["Wei\u00b7det", "mei\u00b7ne", "Sch\u00e4f\u00b7lein", ",", "wei\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Daphnis wird verfolget stark.", "tokens": ["Daph\u00b7nis", "wird", "ver\u00b7fol\u00b7get", "stark", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "B\u00f6s Gesinde ihn beneidet,", "tokens": ["B\u00f6s", "Ge\u00b7sin\u00b7de", "ihn", "be\u00b7nei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Trachtet ihm nach Blut und Mark.", "tokens": ["Trach\u00b7tet", "ihm", "nach", "Blut", "und", "Mark", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "O was dorten, was f\u00fcr Stangen,", "tokens": ["O", "was", "dor\u00b7ten", ",", "was", "f\u00fcr", "Stan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "ADV", "$,", "PRELS", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wehr und Waffen nehm ich wahr!", "tokens": ["Wehr", "und", "Waf\u00b7fen", "nehm", "ich", "wahr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "O vielleicht will man ihn fangen,", "tokens": ["O", "viel\u00b7leicht", "will", "man", "ihn", "fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VMFIN", "PIS", "PPER", "VVFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "Wahrlich, wahrlich, ist Gefahr!", "tokens": ["Wahr\u00b7lich", ",", "wahr\u00b7lich", ",", "ist", "Ge\u00b7fahr", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "$,", "VAFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "Weidet meine Sch\u00e4flein, weidet,", "tokens": ["Wei\u00b7det", "mei\u00b7ne", "Sch\u00e4f\u00b7lein", ",", "wei\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sprechen wollte bleicher Moud,", "tokens": ["Spre\u00b7chen", "woll\u00b7te", "blei\u00b7cher", "Moud", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ja nicht weidet, sondern scheidet,", "tokens": ["Ja", "nicht", "wei\u00b7det", ",", "son\u00b7dern", "schei\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "PTKNEG", "VVFIN", "$,", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Er da sprach, und wollte gehn,", "tokens": ["Er", "da", "sprach", ",", "und", "woll\u00b7te", "gehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$,", "KON", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Scheidet, scheidet, meine Schaaren,", "tokens": ["Schei\u00b7det", ",", "schei\u00b7det", ",", "mei\u00b7ne", "Schaa\u00b7ren", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kann vor Leid nicht schauen zu,", "tokens": ["Kann", "vor", "Leid", "nicht", "schau\u00b7en", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "PTKNEG", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dich nun wolle Gott bewahren,", "tokens": ["Dich", "nun", "wol\u00b7le", "Gott", "be\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Daphnis wer kann bleiben nun!", "tokens": ["Daph\u00b7nis", "wer", "kann", "blei\u00b7ben", "nun", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWS", "VMFIN", "VVINF", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.19": {"line.1": {"text": "Drauf Ade der Mond wollt spielen,", "tokens": ["Drauf", "A\u00b7de", "der", "Mond", "wollt", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da zersprang das matte Rohr:", "tokens": ["Da", "zer\u00b7sprang", "das", "mat\u00b7te", "Rohr", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Augentropfen ihm entfielen,", "tokens": ["Au\u00b7gen\u00b7trop\u00b7fen", "ihm", "ent\u00b7fie\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00fcllte sich in Trauerflor.", "tokens": ["H\u00fcll\u00b7te", "sich", "in", "Trau\u00b7er\u00b7flor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und weil eben dazumahlen,", "tokens": ["Und", "weil", "e\u00b7ben", "da\u00b7zu\u00b7mah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "PAV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Er trat an in vollem Schein,", "tokens": ["Er", "trat", "an", "in", "vol\u00b7lem", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Gleich vertauschet er die Strahlen,", "tokens": ["Gleich", "ver\u00b7tau\u00b7schet", "er", "die", "Strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Vollen Schein, gen volle Pein.", "tokens": ["Vol\u00b7len", "Schein", ",", "gen", "vol\u00b7le", "Pein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.20": {"line.1": {"text": "Auch die Sterne weinen, kamen", "tokens": ["Auch", "die", "Ster\u00b7ne", "wei\u00b7nen", ",", "ka\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ART", "NN", "VVINF", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gossen ab all ihren Schein,", "tokens": ["Gos\u00b7sen", "ab", "all", "ih\u00b7ren", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schein und Thr\u00e4nen flossen sammen,", "tokens": ["Schein", "und", "Thr\u00e4\u00b7nen", "flos\u00b7sen", "sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Reihn zum blauen Feld hinein,", "tokens": ["Reihn", "zum", "blau\u00b7en", "Feld", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Machten eine wei\u00dfe Stra\u00dfen,", "tokens": ["Mach\u00b7ten", "ei\u00b7ne", "wei\u00b7\u00dfe", "Stra\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "So noch heut man sp\u00fcren mag:", "tokens": ["So", "noch", "heut", "man", "sp\u00fc\u00b7ren", "mag", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PIS", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dann der Milchweg hinterlassen,", "tokens": ["Dann", "der", "Milch\u00b7weg", "hin\u00b7ter\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ist der sch\u00f6nsten Thr\u00e4nen Bach.", "tokens": ["Ist", "der", "sch\u00f6ns\u00b7ten", "Thr\u00e4\u00b7nen", "Bach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}