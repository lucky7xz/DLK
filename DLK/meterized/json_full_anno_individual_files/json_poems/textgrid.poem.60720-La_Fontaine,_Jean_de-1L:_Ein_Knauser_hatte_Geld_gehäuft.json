{"textgrid.poem.60720": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Knauser hatte Geld geh\u00e4uft", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Knauser hatte Geld geh\u00e4uft", "tokens": ["Ein", "Knau\u00b7ser", "hat\u00b7te", "Geld", "ge\u00b7h\u00e4uft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wu\u00dfte nicht, wohin damit.", "tokens": ["Und", "wu\u00df\u00b7te", "nicht", ",", "wo\u00b7hin", "da\u00b7mit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "PWAV", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Geiz, der immer gleichen Schritt", "tokens": ["Der", "Geiz", ",", "der", "im\u00b7mer", "glei\u00b7chen", "Schritt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Dummheit l\u00e4uft,", "tokens": ["Mit", "Dumm\u00b7heit", "l\u00e4uft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Macht ihm die Wahl eines Verwahrers schwer.", "tokens": ["Macht", "ihm", "die", "Wahl", "ei\u00b7nes", "Ver\u00b7wa\u00b7hrers", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ART", "NN", "ADJD", "$."], "meter": "+--++--+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Er suchte einen; \u00bbdenn,\u00ab so sagte er", "tokens": ["Er", "such\u00b7te", "ei\u00b7nen", ";", "\u00bb", "denn", ",", "\u00ab", "so", "sag\u00b7te", "er"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "$.", "$(", "ADV", "$,", "$(", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Bei sich, \u00bbder Gegenstand verf\u00fchrt;", "tokens": ["Bei", "sich", ",", "\u00bb", "der", "Ge\u00b7gen\u00b7stand", "ver\u00b7f\u00fchrt", ";"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "$,", "$(", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sofern mein Schatz bei mir zu Hause blieb,", "tokens": ["So\u00b7fern", "mein", "Schatz", "bei", "mir", "zu", "Hau\u00b7se", "blieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "So blieb er dort nicht unber\u00fchrt,", "tokens": ["So", "blieb", "er", "dort", "nicht", "un\u00b7be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ich w\u00fcrde selbst vielleicht an ihm zum Dieb.\u00ab", "tokens": ["Ich", "w\u00fcr\u00b7de", "selbst", "viel\u00b7leicht", "an", "ihm", "zum", "Dieb", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "APPR", "PPER", "APPRART", "NN", "$.", "$("], "meter": "-+----+--+", "measure": "iambic.tri.chol"}, "line.11": {"text": "\u2013 Zum Dieb? Bestiehlt sich, wer genie\u00dft?", "tokens": ["\u2013", "Zum", "Dieb", "?", "Be\u00b7stiehlt", "sich", ",", "wer", "ge\u00b7nie\u00dft", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "APPRART", "NN", "$.", "VVFIN", "PRF", "$,", "PWS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Mein Freund, mir tut dein Irrtum leid.", "tokens": ["Mein", "Freund", ",", "mir", "tut", "dein", "Irr\u00b7tum", "leid", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Lern dies von mir: Gut ist nicht gut,", "tokens": ["Lern", "dies", "von", "mir", ":", "Gut", "ist", "nicht", "gut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "APPR", "PPER", "$.", "ADJD", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.14": {"text": "Vielmehr ein \u00dcbel, wenn man es verschlie\u00dft.", "tokens": ["Viel\u00b7mehr", "ein", "\u00dc\u00b7bel", ",", "wenn", "man", "es", "ver\u00b7schlie\u00dft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "KOUS", "PIS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Willst du's verwahren f\u00fcr die Greisenzeit,", "tokens": ["Willst", "du's", "ver\u00b7wah\u00b7ren", "f\u00fcr", "die", "Grei\u00b7sen\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Wo jeder Wunsch nach Lust und Freude ruht?", "tokens": ["Wo", "je\u00b7der", "Wunsch", "nach", "Lust", "und", "Freu\u00b7de", "ruht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Dein Sorgen nimmt den Wert dem Geld,", "tokens": ["Dein", "Sor\u00b7gen", "nimmt", "den", "Wert", "dem", "Geld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Das man f\u00fcr gar notwendig h\u00e4lt. \u2013", "tokens": ["Das", "man", "f\u00fcr", "gar", "not\u00b7wen\u00b7dig", "h\u00e4lt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "PIS", "APPR", "ADV", "ADJD", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Gewi\u00df, er h\u00e4tte manchen Mann gefunden,", "tokens": ["Ge\u00b7wi\u00df", ",", "er", "h\u00e4t\u00b7te", "man\u00b7chen", "Mann", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Der seiner Sorgenlast ihn gern entbunden,", "tokens": ["Der", "sei\u00b7ner", "Sor\u00b7gen\u00b7last", "ihn", "gern", "ent\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Er zog jedoch die Erde vor", "tokens": ["Er", "zog", "je\u00b7doch", "die", "Er\u00b7de", "vor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Und den Gevatter ins Vertrauen;", "tokens": ["Und", "den", "Ge\u00b7vat\u00b7ter", "ins", "Ver\u00b7trau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Der half ihm seinen Schatz vergraben.", "tokens": ["Der", "half", "ihm", "sei\u00b7nen", "Schatz", "ver\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Nach kurzem wollte unser Tor", "tokens": ["Nach", "kur\u00b7zem", "woll\u00b7te", "un\u00b7ser", "Tor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Sein Geld sich wieder mal beschauen.", "tokens": ["Sein", "Geld", "sich", "wie\u00b7der", "mal", "be\u00b7schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Was mag er wohl gefunden haben?", "tokens": ["Was", "mag", "er", "wohl", "ge\u00b7fun\u00b7den", "ha\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Ein leeres Grab! Jawohl! Was macht", "tokens": ["Ein", "lee\u00b7res", "Grab", "!", "Ja\u00b7wohl", "!", "Was", "macht"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "XY", "$.", "PWS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Der \u00c4rmste nun? Er hegt Verdacht", "tokens": ["Der", "\u00c4rms\u00b7te", "nun", "?", "Er", "hegt", "Ver\u00b7dacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$.", "PPER", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Auf den Gevatter \u2013 und mit Recht,", "tokens": ["Auf", "den", "Ge\u00b7vat\u00b7ter", "\u2013", "und", "mit", "Recht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$(", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Doch er verbirgt ihn gar nicht schlecht", "tokens": ["Doch", "er", "ver\u00b7birgt", "ihn", "gar", "nicht", "schlecht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Und eilt zum Dieb und sagt: \u00bbZur Nacht", "tokens": ["Und", "eilt", "zum", "Dieb", "und", "sagt", ":", "\u00bb", "Zur", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "KON", "VVFIN", "$.", "$(", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Halt dich bereit; denn denke dir, ich fand", "tokens": ["Halt", "dich", "be\u00b7reit", ";", "denn", "den\u00b7ke", "dir", ",", "ich", "fand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "PPER", "ADJD", "$.", "KON", "VVFIN", "PPER", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Noch einige Heller, die wir meinem Schatz", "tokens": ["Noch", "ei\u00b7ni\u00b7ge", "Hel\u00b7ler", ",", "die", "wir", "mei\u00b7nem", "Schatz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "$,", "PRELS", "PPER", "PPOSAT", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "Beif\u00fcgen wollen.\u00ab \u2013 Was zu tun war, stand", "tokens": ["Bei\u00b7f\u00fc\u00b7gen", "wol\u00b7len", ".", "\u00ab", "\u2013", "Was", "zu", "tun", "war", ",", "stand"], "token_info": ["word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "VMFIN", "$.", "$(", "$(", "PWS", "PTKZU", "VVINF", "VAFIN", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Alsbald bei dem Vertrauten fest:", "tokens": ["Als\u00b7bald", "bei", "dem", "Ver\u00b7trau\u00b7ten", "fest", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.36": {"text": "Er trug die Beute schnell zum alten Platz", "tokens": ["Er", "trug", "die", "Beu\u00b7te", "schnell", "zum", "al\u00b7ten", "Platz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Um sie nachher mitsamt dem kleinen Rest", "tokens": ["Um", "sie", "nach\u00b7her", "mit\u00b7samt", "dem", "klei\u00b7nen", "Rest"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PPER", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Wiederzunehmen. Diesmal aber war", "tokens": ["Wie\u00b7der\u00b7zu\u00b7neh\u00b7men", ".", "Dies\u00b7mal", "a\u00b7ber", "war"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "ADV", "ADV", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Der andre schlauer. All sein Gut", "tokens": ["Der", "and\u00b7re", "schlau\u00b7er", ".", "All", "sein", "Gut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "li