{"dta.poem.11249": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An Clelien.  \n C. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man sagt: Das gl\u00fccke soll' auf einer kugel stehn;", "tokens": ["Man", "sagt", ":", "Das", "gl\u00fc\u00b7cke", "soll'", "auf", "ei\u00b7ner", "ku\u00b7gel", "stehn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$.", "PDS", "VVFIN", "PIAT", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Drum la\u00df dir, Clelia! mein leid zu hertzen gehn!", "tokens": ["Drum", "la\u00df", "dir", ",", "Cle\u00b7lia", "!", "mein", "leid", "zu", "hert\u00b7zen", "gehn", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "NE", "$.", "PPOSAT", "ADJD", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+----+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Und wenn ich meine hand zu deinen br\u00fcsten schicke,", "tokens": ["Und", "wenn", "ich", "mei\u00b7ne", "hand", "zu", "dei\u00b7nen", "br\u00fcs\u00b7ten", "schi\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So denck\u2019, ich suche da f\u00fcr mich ein doppelt gl\u00fccke.", "tokens": ["So", "denck", ",", "ich", "su\u00b7che", "da", "f\u00fcr", "mich", "ein", "dop\u00b7pelt", "gl\u00fc\u00b7cke", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "PPER", "VVFIN", "ADV", "APPR", "PPER", "ART", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}