{"dta.poem.1179": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die bitter-s\u00fcsse Dulcinde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Kind/ deine Freundligkeit                         ", "tokens": ["Kind", "/", "dei\u00b7ne", "Freund\u00b7lig\u00b7keit"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$(", "PPOSAT", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Kan Freud und Lust erwecken/", "tokens": ["Kan", "Freud", "und", "Lust", "er\u00b7we\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo Trauren/ Sorg und Leyd", "tokens": ["Wo", "Trau\u00b7ren", "/", "Sorg", "und", "Leyd"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "NN", "$(", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Im innern Hertzen stecken:", "tokens": ["Im", "in\u00b7nern", "Hert\u00b7zen", "ste\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Man sieht auff deinen Wangen", "tokens": ["Man", "sieht", "auff", "dei\u00b7nen", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Narci\u00df\u2019 und Rose prangen.", "tokens": ["Na\u00b7rci\u00df'", "und", "Ro\u00b7se", "pran\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Doch will ich was darvon", "tokens": ["Doch", "will", "ich", "was", "dar\u00b7von"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "PIS", "PAV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mit s\u00fcssem Zwange brechen/", "tokens": ["Mit", "s\u00fcs\u00b7sem", "Zwan\u00b7ge", "bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So pfleget mich zum Lohn", "tokens": ["So", "pfle\u00b7get", "mich", "zum", "Lohn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ein scharffer Dorn zu stechen.", "tokens": ["Ein", "scharf\u00b7fer", "Dorn", "zu", "ste\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich darff nicht frey bekennen", "tokens": ["Ich", "darff", "nicht", "frey", "be\u00b7ken\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie Hertz und Seele brennen.", "tokens": ["Wie", "Hertz", "und", "See\u00b7le", "bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wilt du mit gutem Recht", "tokens": ["Wilt", "du", "mit", "gu\u00b7tem", "Recht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dulcindens Nahmen f\u00fchren/", "tokens": ["Dul\u00b7cin\u00b7dens", "Nah\u00b7men", "f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVINF", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "La\u00df deinen treuen Knecht", "tokens": ["La\u00df", "dei\u00b7nen", "treu\u00b7en", "Knecht"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Genad und Gunst versp\u00fcren.", "tokens": ["Ge\u00b7nad", "und", "Gunst", "ver\u00b7sp\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Den Honig auff dem Munde", "tokens": ["Den", "Ho\u00b7nig", "auff", "dem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Verderbt die Gall im Grunde.", "tokens": ["Ver\u00b7derbt", "die", "Gall", "im", "Grun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}