{"textgrid.poem.55855": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Herbsttag", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr: es ist Zeit. Der Sommer war sehr gro\u00df.", "tokens": ["Herr", ":", "es", "ist", "Zeit", ".", "Der", "Som\u00b7mer", "war", "sehr", "gro\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VAFIN", "NN", "$.", "ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Leg deinen Schatten auf die Sonnenuhren,", "tokens": ["Leg", "dei\u00b7nen", "Schat\u00b7ten", "auf", "die", "Son\u00b7ne\u00b7nuh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und auf den Fluren la\u00df die Winde los.", "tokens": ["und", "auf", "den", "Flu\u00b7ren", "la\u00df", "die", "Win\u00b7de", "los", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Befiehl den letzten Fr\u00fcchten voll zu sein;", "tokens": ["Be\u00b7fiehl", "den", "letz\u00b7ten", "Fr\u00fcch\u00b7ten", "voll", "zu", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "gieb ihnen noch zwei s\u00fcdlichere Tage,", "tokens": ["gieb", "ih\u00b7nen", "noch", "zwei", "s\u00fcd\u00b7li\u00b7che\u00b7re", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "CARD", "ADJA", "NN", "$,"], "meter": "-+--++-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "dr\u00e4nge sie zur Vollendung hin und jage", "tokens": ["dr\u00e4n\u00b7ge", "sie", "zur", "Vol\u00b7len\u00b7dung", "hin", "und", "ja\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "KON", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "die letzte S\u00fc\u00dfe in den schweren Wein.", "tokens": ["die", "letz\u00b7te", "S\u00fc\u00b7\u00dfe", "in", "den", "schwe\u00b7ren", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wer jetzt kein Haus hat, baut sich keines mehr.", "tokens": ["Wer", "jetzt", "kein", "Haus", "hat", ",", "baut", "sich", "kei\u00b7nes", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PIAT", "NN", "VAFIN", "$,", "VVFIN", "PRF", "PIS", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wer jetzt allein ist, wird es lange bleiben,", "tokens": ["Wer", "jetzt", "al\u00b7lein", "ist", ",", "wird", "es", "lan\u00b7ge", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "VAFIN", "$,", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "wird wachen, lesen, lange Briefe schreiben", "tokens": ["wird", "wa\u00b7chen", ",", "le\u00b7sen", ",", "lan\u00b7ge", "Brie\u00b7fe", "schrei\u00b7ben"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "VVINF", "$,", "VVINF", "$,", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und wird in den Alleen hin und her", "tokens": ["und", "wird", "in", "den", "Al\u00b7leen", "hin", "und", "her"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "ART", "NN", "PTKVZ", "KON", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "unruhig wandern, wenn die Bl\u00e4tter treiben.", "tokens": ["un\u00b7ru\u00b7hig", "wan\u00b7dern", ",", "wenn", "die", "Bl\u00e4t\u00b7ter", "trei\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "KOUS", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "Herr: es ist Zeit. Der Sommer war sehr gro\u00df.", "tokens": ["Herr", ":", "es", "ist", "Zeit", ".", "Der", "Som\u00b7mer", "war", "sehr", "gro\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VAFIN", "NN", "$.", "ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Leg deinen Schatten auf die Sonnenuhren,", "tokens": ["Leg", "dei\u00b7nen", "Schat\u00b7ten", "auf", "die", "Son\u00b7ne\u00b7nuh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und auf den Fluren la\u00df die Winde los.", "tokens": ["und", "auf", "den", "Flu\u00b7ren", "la\u00df", "die", "Win\u00b7de", "los", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART