{"textgrid.poem.38008": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Einquartierung", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jackele guck zum Fenster n'aus,", "tokens": ["Ja\u00b7cke\u00b7le", "guck", "zum", "Fens\u00b7ter", "n'\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Moin i h\u00f6r \u00e4in Drommen,", "tokens": ["Moin", "i", "h\u00f6r", "\u00e4\u00b7in", "Drom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Annele gang beschleui\u00df das Haus,", "tokens": ["An\u00b7ne\u00b7le", "gang", "be\u00b7schleu\u00b7i\u00df", "das", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Glaub Soldaten kommen,", "tokens": ["Glaub", "Sol\u00b7da\u00b7ten", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Sind gau g'wi\u00df Husaren,", "tokens": ["Sind", "gau", "g'\u00b7wi\u00df", "Hu\u00b7sa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "ADV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Lueg obs kannst erfahren,", "tokens": ["Lu\u00b7eg", "obs", "kannst", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Noin sie sind nit so anthaun,", "tokens": ["Noin", "sie", "sind", "nit", "so", "an\u00b7thaun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VAFIN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.8": {"text": "Potz i wai\u00df und kenn sie schaun.", "tokens": ["Potz", "i", "wai\u00df", "und", "kenn", "sie", "schaun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "KON", "VVFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ei pfui Teuffen, wie sehns drein:", "tokens": ["Ei", "pfui", "Teuf\u00b7fen", ",", "wie", "sehns", "drein", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,", "PWAV", "PIS", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "I kan nit gnug gucken,", "tokens": ["I", "kan", "nit", "gnug", "gu\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["XY", "VMFIN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Werden wol Tralpatschen sein,", "tokens": ["Wer\u00b7den", "wol", "Tral\u00b7pat\u00b7schen", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "VAINF", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Was haunds auf den Rucken?", "tokens": ["Was", "haunds", "auf", "den", "Ru\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Sieht als wie ein Pr\u00fcgel,", "tokens": ["Sieht", "als", "wie", "ein", "Pr\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOUS", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "B\u00e4rt haunds wie die Igel,", "tokens": ["B\u00e4rt", "haunds", "wie", "die", "I\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Hosen wie die Schweizer an,", "tokens": ["Ho\u00b7sen", "wie", "die", "Schwei\u00b7zer", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Helf is Gott! mein lieber Mann.", "tokens": ["Helf", "is", "Gott", "!", "mein", "lie\u00b7ber", "Mann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "FM", "NN", "$.", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Was kommen denn dort f\u00fcr ruff?", "tokens": ["Was", "kom\u00b7men", "denn", "dort", "f\u00fcr", "ruff", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ADV", "APPR", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Horch wie sie schau murren:", "tokens": ["Horch", "wie", "sie", "schau", "mur\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "PPER", "ADJD", "VVINF", "$."], "meter": "+--++-", "measure": "dactylic.init"}, "line.3": {"text": "Marrei gang und mach mir uff,", "tokens": ["Mar\u00b7rei", "gang", "und", "mach", "mir", "uff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "I glaub es seind Panduren.", "tokens": ["I", "glaub", "es", "seind", "Pan\u00b7du\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "VVFIN", "PPER", "VAFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Was mu\u00df i gau kochen,", "tokens": ["Was", "mu\u00df", "i", "gau", "ko\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "NE", "NE", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Da\u00df nit mit mir pochen,", "tokens": ["Da\u00df", "nit", "mit", "mir", "po\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "APPR", "PPER", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Kn\u00f6pfle und ein d\u00fcrre Wurst.", "tokens": ["Kn\u00f6pf\u00b7le", "und", "ein", "d\u00fcr\u00b7re", "Wurst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Mann gang, frag ob sie's nit durst?", "tokens": ["Mann", "gang", ",", "frag", "ob", "sie's", "nit", "durst", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "VVFIN", "KOUS", "PPER", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Weib was denkst, lo\u00df mi ungkeit,", "tokens": ["Weib", "was", "denkst", ",", "lo\u00df", "mi", "ung\u00b7keit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PWS", "VVFIN", "$,", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schweig du alter Fetzen,", "tokens": ["Schweig", "du", "al\u00b7ter", "Fet\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wai\u00df schau, was es sind f\u00fcr Leut,", "tokens": ["Wai\u00df", "schau", ",", "was", "es", "sind", "f\u00fcr", "Leut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PWS", "PPER", "VAFIN", "APPR", "NN", "$,"], "meter": "++--+-+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Darf nit mit sie schw\u00e4tzen,", "tokens": ["Darf", "nit", "mit", "sie", "schw\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "APPR", "PPER", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Kann sie nit verstande,", "tokens": ["Kann", "sie", "nit", "ver\u00b7stan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Kunt'n mi haue zu schande,", "tokens": ["Kunt'n", "mi", "hau\u00b7e", "zu", "schan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "APPR", "ADJA", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Mi und di und au den Bue,", "tokens": ["Mi", "und", "di", "und", "au", "den", "Bue", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "KON", "NE", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und h\u00e4tt no den Spott darzue.", "tokens": ["Und", "h\u00e4tt", "no", "den", "Spott", "dar\u00b7zue", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NE", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ei so schlag der Plunder drein,", "tokens": ["Ei", "so", "schlag", "der", "Plun\u00b7der", "drein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Was sind das f\u00fcr Gsellen:", "tokens": ["Was", "sind", "das", "f\u00fcr", "Gsel\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PDS", "APPR", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Marrei gang und hoi du Wein;", "tokens": ["Mar\u00b7rei", "gang", "und", "hoi", "du", "Wein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "VVFIN", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Annele thu aufstellen,", "tokens": ["An\u00b7ne\u00b7le", "thu", "auf\u00b7stel\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "VVIZU", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Thu fein tapfer tenn\u00e4,", "tokens": ["Thu", "fein", "tap\u00b7fer", "ten\u00b7n\u00e4", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADJD", "FM", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Lo\u00df Kraut nit verbrenn\u00e4,", "tokens": ["Lo\u00df", "Kraut", "nit", "ver\u00b7bren\u00b7n\u00e4", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Schnid a s\u00e4lle d\u00fcrre Speck,", "tokens": ["Schnid", "a", "s\u00e4l\u00b7le", "d\u00fcr\u00b7re", "Speck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Gost dah\u00e4r wie oine Schneck.", "tokens": ["Gost", "da\u00b7h\u00e4r", "wie", "o\u00b7i\u00b7ne", "Schneck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PAV", "KOKOM", "NE", "NE", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.6": {"line.1": {"text": "Weib i gang gau \u00fcber Feld,", "tokens": ["Weib", "i", "gang", "gau", "\u00fc\u00b7ber", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df sie mi nit seh\u00e4,", "tokens": ["Da\u00df", "sie", "mi", "nit", "se\u00b7h\u00e4", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn sie w\u00f6lle hau no Geld,", "tokens": ["Wenn", "sie", "w\u00f6l\u00b7le", "hau", "no", "Geld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "NE", "NE", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dort im Trog thut steh\u00e4,", "tokens": ["Dort", "im", "Trog", "thut", "ste\u00b7h\u00e4", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Anderthalbe Gulde,", "tokens": ["An\u00b7der\u00b7thal\u00b7be", "Gul\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Und dem Wirt bleibs schulde,", "tokens": ["Und", "dem", "Wirt", "bleibs", "schul\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NE", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Will ihm geben Korn daf\u00fcr,", "tokens": ["Will", "ihm", "ge\u00b7ben", "Korn", "da\u00b7f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "NN", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Hol der Velte das Quartier.", "tokens": ["Hol", "der", "Vel\u00b7te", "das", "Quar\u00b7tier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.7": {"line.1": {"text": "Jackele guck zum Fenster n'aus,", "tokens": ["Ja\u00b7cke\u00b7le", "guck", "zum", "Fens\u00b7ter", "n'\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Moin i h\u00f6r \u00e4in Drommen,", "tokens": ["Moin", "i", "h\u00f6r", "\u00e4\u00b7in", "Drom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Annele gang beschleui\u00df das Haus,", "tokens": ["An\u00b7ne\u00b7le", "gang", "be\u00b7schleu\u00b7i\u00df", "das", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Glaub Soldaten kommen,", "tokens": ["Glaub", "Sol\u00b7da\u00b7ten", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Sind gau g'wi\u00df Husaren,", "tokens": ["Sind", "gau", "g'\u00b7wi\u00df", "Hu\u00b7sa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "ADV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Lueg obs kannst erfahren,", "tokens": ["Lu\u00b7eg", "obs", "kannst", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Noin sie sind nit so anthaun,", "tokens": ["Noin", "sie", "sind", "nit", "so", "an\u00b7thaun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VAFIN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.8": {"text": "Potz i wai\u00df und kenn sie schaun.", "tokens": ["Potz", "i", "wai\u00df", "und", "kenn", "sie", "schaun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "KON", "VVFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Ei pfui Teuffen, wie sehns drein:", "tokens": ["Ei", "pfui", "Teuf\u00b7fen", ",", "wie", "sehns", "drein", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,", "PWAV", "PIS", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "I kan nit gnug gucken,", "tokens": ["I", "kan", "nit", "gnug", "gu\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["XY", "VMFIN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Werden wol Tralpatschen sein,", "tokens": ["Wer\u00b7den", "wol", "Tral\u00b7pat\u00b7schen", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "VAINF", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Was haunds auf den Rucken?", "tokens": ["Was", "haunds", "auf", "den", "Ru\u00b