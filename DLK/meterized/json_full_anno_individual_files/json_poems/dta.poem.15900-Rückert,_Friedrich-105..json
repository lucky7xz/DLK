{"dta.poem.15900": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "105.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn den Gealterten es freut, sich selber jung", "tokens": ["Wenn", "den", "Ge\u00b7al\u00b7ter\u00b7ten", "es", "freut", ",", "sich", "sel\u00b7ber", "jung"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "$,", "PRF", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Spiegel anzuschaun der R\u00fcckerinnerung;", "tokens": ["Im", "Spie\u00b7gel", "an\u00b7zu\u00b7schaun", "der", "R\u00fc\u00b7cke\u00b7rin\u00b7ne\u00b7rung", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVIZU", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So kann sich deinem Gl\u00fcck, o Dichter, keins vergleichen,", "tokens": ["So", "kann", "sich", "dei\u00b7nem", "Gl\u00fcck", ",", "o", "Dich\u00b7ter", ",", "keins", "ver\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PRF", "PPOSAT", "NN", "$,", "FM", "NN", "$,", "PIAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da deine Lieder dir so helle Spiegel reichen.", "tokens": ["Da", "dei\u00b7ne", "Lie\u00b7der", "dir", "so", "hel\u00b7le", "Spie\u00b7gel", "rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nun siehst du, da\u00df du nicht hast M\u00fch' und Zeit verloren,", "tokens": ["Nun", "siehst", "du", ",", "da\u00df", "du", "nicht", "hast", "M\u00fch'", "und", "Zeit", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PTKNEG", "VAFIN", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und da\u00df die Himmelskunst dir hielt, was sie geschworen.", "tokens": ["Und", "da\u00df", "die", "Him\u00b7mels\u00b7kunst", "dir", "hielt", ",", "was", "sie", "ge\u00b7schwo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wie wenig auch die Welt von diesen Liedern h\u00e4lt,", "tokens": ["Wie", "we\u00b7nig", "auch", "die", "Welt", "von", "die\u00b7sen", "Lie\u00b7dern", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "ART", "NN", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verewigt hast du dich darin und deine Welt.", "tokens": ["Ve\u00b7re\u00b7wigt", "hast", "du", "dich", "da\u00b7rin", "und", "dei\u00b7ne", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "PRF", "PAV", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}