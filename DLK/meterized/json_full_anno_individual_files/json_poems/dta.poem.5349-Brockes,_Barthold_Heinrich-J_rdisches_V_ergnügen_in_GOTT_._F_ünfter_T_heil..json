{"dta.poem.5349": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Indem mein Geist auf diese Weise dencket,", "tokens": ["In\u00b7dem", "mein", "Geist", "auf", "die\u00b7se", "Wei\u00b7se", "den\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Augen auf der K\u00e4fer-Schwarm gesencket,", "tokens": ["Die", "Au\u00b7gen", "auf", "der", "K\u00e4\u00b7fer\u00b7Schwarm", "ge\u00b7sen\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Seh ich wie einer schnell sich aufwerts hebt,", "tokens": ["Seh", "ich", "wie", "ei\u00b7ner", "schnell", "sich", "auf\u00b7werts", "hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "ART", "ADJD", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und mit geschwindem Flug in reinen L\u00fcften schwebt.", "tokens": ["Und", "mit", "ge\u00b7schwin\u00b7dem", "Flug", "in", "rei\u00b7nen", "L\u00fcf\u00b7ten", "schwebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ach dacht ich m\u00f6chte die\u00df Chrysander sehen!", "tokens": ["Ach", "dacht", "ich", "m\u00f6ch\u00b7te", "die\u00df", "Chry\u00b7san\u00b7der", "se\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "VMFIN", "PDS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und, durch den Wurm ger\u00fchret, in sich gehen,", "tokens": ["Und", ",", "durch", "den", "Wurm", "ge\u00b7r\u00fch\u00b7ret", ",", "in", "sich", "ge\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ART", "NN", "VVPP", "$,", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und aus dem Koth sich so, wie er, erh\u00f6hn,", "tokens": ["Und", "aus", "dem", "Koth", "sich", "so", ",", "wie", "er", ",", "er\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PRF", "ADV", "$,", "PWAV", "PPER", "$,", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und schauen wie die Welt, des Sch\u00f6pfers Werck, so", "tokens": ["Und", "schau\u00b7en", "wie", "die", "Welt", ",", "des", "Sch\u00f6p\u00b7fers", "Werck", ",", "so"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "KOKOM", "ART", "NN", "$,", "ART", "NN", "NN", "$,", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "sch\u00f6n!", "tokens": ["sch\u00f6n", "!"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+", "measure": "single.up"}, "line.10": {"text": "Allein ich f\u00fcrchte sehr, er l\u00e4\u00dft den K\u00e4fer fliegen,", "tokens": ["Al\u00b7lein", "ich", "f\u00fcrch\u00b7te", "sehr", ",", "er", "l\u00e4\u00dft", "den", "K\u00e4\u00b7fer", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und bleibet, vor wie nach, in seinem Unrath liegen.", "tokens": ["Und", "blei\u00b7bet", ",", "vor", "wie", "nach", ",", "in", "sei\u00b7nem", "Un\u00b7rath", "lie\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PTKVZ", "KOKOM", "APPR", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}