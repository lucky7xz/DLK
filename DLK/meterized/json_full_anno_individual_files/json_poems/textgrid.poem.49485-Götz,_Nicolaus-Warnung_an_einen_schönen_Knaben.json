{"textgrid.poem.49485": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "Warnung an einen sch\u00f6nen Knaben", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Holdseelig Kind, du meine werthe Freude,", "tokens": ["Hold\u00b7see\u00b7lig", "Kind", ",", "du", "mei\u00b7ne", "wert\u00b7he", "Freu\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Anmuthig, wie der West,", "tokens": ["An\u00b7mut\u00b7hig", ",", "wie", "der", "West", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+----+", "measure": "dactylic.init"}, "line.3": {"text": "Rein, wie ein Lamm, das auf der Fr\u00fchlingsweide", "tokens": ["Rein", ",", "wie", "ein", "Lamm", ",", "das", "auf", "der", "Fr\u00fch\u00b7lings\u00b7wei\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Am Bach sich s\u00e4ugen l\u00e4st.", "tokens": ["Am", "Bach", "sich", "s\u00e4u\u00b7gen", "l\u00e4st", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dies goldne Haar, da\u00df sich itzt kurtzgekrollet", "tokens": ["Dies", "gold\u00b7ne", "Haar", ",", "da\u00df", "sich", "itzt", "kurtz\u00b7ge\u00b7krol\u00b7let"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "ADJA", "NN", "$,", "KOUS", "PRF", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Um deine Schl\u00e4fe kr\u00fcmmt,", "tokens": ["Um", "dei\u00b7ne", "Schl\u00e4\u00b7fe", "kr\u00fcmmt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn es einst braun in langen Locken rollet,", "tokens": ["Wenn", "es", "einst", "braun", "in", "lan\u00b7gen", "Lo\u00b7cken", "rol\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und auf den Schultern schwimmt;", "tokens": ["Und", "auf", "den", "Schul\u00b7tern", "schwimmt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wenn Hebens Hand mit einem zarten Schatten", "tokens": ["Wenn", "He\u00b7bens", "Hand", "mit", "ei\u00b7nem", "zar\u00b7ten", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein rundes Kinn bekr\u00f6nt,", "tokens": ["Dein", "run\u00b7des", "Kinn", "be\u00b7kr\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sich dein Geist nach freyen Blumenmatten,", "tokens": ["Und", "sich", "dein", "Geist", "nach", "frey\u00b7en", "Blu\u00b7men\u00b7mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und ofnen Feldern sehnt;", "tokens": ["Und", "of\u00b7nen", "Fel\u00b7dern", "sehnt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wenn einst dein Leib in holder schlanker L\u00e4nge", "tokens": ["Wenn", "einst", "dein", "Leib", "in", "hol\u00b7der", "schlan\u00b7ker", "L\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zur M\u00e4nnergr\u00f6se steigt,", "tokens": ["Zur", "M\u00e4n\u00b7ner\u00b7gr\u00f6\u00b7se", "steigt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie Cedern thun, die in ber\u00fchmter Menge", "tokens": ["Wie", "Ce\u00b7dern", "thun", ",", "die", "in", "be\u00b7r\u00fchm\u00b7ter", "Men\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "VVINF", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der heilge Hermon zeugt:", "tokens": ["Der", "heil\u00b7ge", "Her\u00b7mon", "zeugt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Alsdann, o Sohn, fleuch, gleich dem Strahl der Blitze,", "tokens": ["Als\u00b7dann", ",", "o", "Sohn", ",", "fleuch", ",", "gleich", "dem", "Strahl", "der", "Blit\u00b7ze", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "FM", "NN", "$,", "VVFIN", "$,", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den angenehmen Strand,", "tokens": ["Den", "an\u00b7ge\u00b7neh\u00b7men", "Strand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo neben dir auf einem Rasensitze", "tokens": ["Wo", "ne\u00b7ben", "dir", "auf", "ei\u00b7nem", "Ra\u00b7sen\u00b7sit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dich Doris sch\u00f6n genannt,", "tokens": ["Dich", "Do\u00b7ris", "sch\u00f6n", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wo sie gesagt, da\u00df dir der Weinstock bl\u00fchet,", "tokens": ["Wo", "sie", "ge\u00b7sagt", ",", "da\u00df", "dir", "der", "Wein\u00b7stock", "bl\u00fc\u00b7het", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und auf dem Blumenfeld,", "tokens": ["Und", "auf", "dem", "Blu\u00b7men\u00b7feld", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Um dich zu sehn, der klare Quell verziehet,", "tokens": ["Um", "dich", "zu", "sehn", ",", "der", "kla\u00b7re", "Quell", "ver\u00b7zie\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "PTKZU", "VVINF", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und froh die Ufer schwellt,", "tokens": ["Und", "froh", "die", "U\u00b7fer", "schwellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Da\u00df nur f\u00fcr dich die k\u00fchlende Melone", "tokens": ["Da\u00df", "nur", "f\u00fcr", "dich", "die", "k\u00fch\u00b7len\u00b7de", "Me\u00b7lo\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Am Sonnenstrale reift,", "tokens": ["Am", "Son\u00b7nen\u00b7stra\u00b7le", "reift", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und nur f\u00fcr dich der Lentz, des Jahres Krone,", "tokens": ["Und", "nur", "f\u00fcr", "dich", "der", "Lentz", ",", "des", "Jah\u00b7res", "Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPER", "ART", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Rosenw\u00e4ldgen streift.", "tokens": ["Im", "Ro\u00b7sen\u00b7w\u00e4ld\u00b7gen", "streift", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Sohn, wenn sie einst, indem sie Blumen pfl\u00fccket,", "tokens": ["Sohn", ",", "wenn", "sie", "einst", ",", "in\u00b7dem", "sie", "Blu\u00b7men", "pfl\u00fc\u00b7cket", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ADV", "$,", "KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Die Stengel nach dir schmeist,", "tokens": ["Die", "Sten\u00b7gel", "nach", "dir", "schmeist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie? oder doch die steifen Stengel knicket,", "tokens": ["Wie", "?", "o\u00b7der", "doch", "die", "stei\u00b7fen", "Sten\u00b7gel", "kni\u00b7cket", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "KON", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und dir sich spr\u00f6d entreist,", "tokens": ["Und", "dir", "sich", "spr\u00f6d", "en\u00b7treist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und wie ein Reh in junge Myrtenhecken", "tokens": ["Und", "wie", "ein", "Reh", "in", "jun\u00b7ge", "Myr\u00b7ten\u00b7he\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht ohne Schalkheit flieht,", "tokens": ["Nicht", "oh\u00b7ne", "Schalk\u00b7heit", "flieht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vor ihrem Freund sich sch\u00fcchtern zu verstecken,", "tokens": ["Vor", "ih\u00b7rem", "Freund", "sich", "sch\u00fcch\u00b7tern", "zu", "ver\u00b7ste\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "VVINF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch so, da\u00df er sie sieht:", "tokens": ["Doch", "so", ",", "da\u00df", "er", "sie", "sieht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "So folg ihr nicht; sie leitet deine Jugend,", "tokens": ["So", "folg", "ihr", "nicht", ";", "sie", "lei\u00b7tet", "dei\u00b7ne", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf Pfade voller Blut.", "tokens": ["Auf", "Pfa\u00b7de", "vol\u00b7ler", "Blut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ach! folge nur der ewigsch\u00f6nen Tugend;", "tokens": ["Ach", "!", "fol\u00b7ge", "nur", "der", "e\u00b7wig\u00b7sch\u00f6\u00b7nen", "Tu\u00b7gend", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die ist das h\u00f6chste Gut.", "tokens": ["Die", "ist", "das", "h\u00f6chs\u00b7te", "Gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Die wird dich auch im Tode nicht verlassen;", "tokens": ["Die", "wird", "dich", "auch", "im", "To\u00b7de", "nicht", "ver\u00b7las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos"