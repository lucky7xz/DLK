{"textgrid.poem.61308": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Abschied von Sankt Goar", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie flog im Land des Rheines", "tokens": ["Wie", "flog", "im", "Land", "des", "Rhei\u00b7nes"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPRART", "NN", "ART", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So rasch die Sommerszeit!", "tokens": ["So", "rasch", "die", "Som\u00b7mers\u00b7zeit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schon dunkelt blauen Scheines", "tokens": ["Schon", "dun\u00b7kelt", "blau\u00b7en", "Schei\u00b7nes"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Traube weit und breit;", "tokens": ["Die", "Trau\u00b7be", "weit", "und", "breit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es f\u00e4rbt das Laub sich gelber,", "tokens": ["Es", "f\u00e4rbt", "das", "Laub", "sich", "gel\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Kranich zieht dahin;", "tokens": ["Der", "Kra\u00b7nich", "zieht", "da\u00b7hin", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mit zieh' ich, weil ich selber", "tokens": ["Mit", "zieh'", "ich", ",", "weil", "ich", "sel\u00b7ber"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein Wandervogel bin.", "tokens": ["Ein", "Wan\u00b7der\u00b7vo\u00b7gel", "bin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Fahr wohl, von Walnu\u00dfb\u00e4umen", "tokens": ["Fahr", "wohl", ",", "von", "Wal\u00b7nu\u00df\u00b7b\u00e4u\u00b7men"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "$,", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Umrauscht, mein Sankt Goar!", "tokens": ["Um\u00b7rauscht", ",", "mein", "Sankt", "Go\u00b7ar", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "VVFIN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das war ein s\u00fc\u00dfes Tr\u00e4umen", "tokens": ["Das", "war", "ein", "s\u00fc\u00b7\u00dfes", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In deinem Scho\u00df f\u00fcrwahr.", "tokens": ["In", "dei\u00b7nem", "Scho\u00df", "f\u00fcr\u00b7wahr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie oft im Tal der Grindel", "tokens": ["Wie", "oft", "im", "Tal", "der", "Grin\u00b7del"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ward mir die Lust Gesang,", "tokens": ["Ward", "mir", "die", "Lust", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wenn die kristallne Spindel", "tokens": ["Wenn", "die", "kris\u00b7tall\u00b7ne", "Spin\u00b7del"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "--++-+-", "measure": "anapaest.init"}, "line.8": {"text": "Der Wasserfei erklang!", "tokens": ["Der", "Was\u00b7ser\u00b7fei", "er\u00b7klang", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Fahr wohl, du Lei der Lore", "tokens": ["Fahr", "wohl", ",", "du", "Lei", "der", "Lo\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "PPER", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An wilder Strudel Schwall!", "tokens": ["An", "wil\u00b7der", "Stru\u00b7del", "Schwall", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Noch t\u00f6nt in meinem Ohre", "tokens": ["Noch", "t\u00f6nt", "in", "mei\u00b7nem", "Oh\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ged\u00e4mpft dein Klagehall;", "tokens": ["Ge\u00b7d\u00e4mpft", "dein", "Kla\u00b7ge\u00b7hall", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er rief mir tief im Sinne", "tokens": ["Er", "rief", "mir", "tief", "im", "Sin\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die d\u00fcstre Sage wach", "tokens": ["Die", "d\u00fcst\u00b7re", "Sa\u00b7ge", "wach"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Vom Herzen, das die Minne", "tokens": ["Vom", "Her\u00b7zen", ",", "das", "die", "Min\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit ihrer Falschheit brach.", "tokens": ["Mit", "ih\u00b7rer", "Falschheit", "brach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Ihr T\u00fcrm' und Burgen droben,", "tokens": ["Ihr", "T\u00fcrm'", "und", "Bur\u00b7gen", "dro\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich gr\u00fc\u00df' euch tausendmal;", "tokens": ["Ich", "gr\u00fc\u00df'", "euch", "tau\u00b7send\u00b7mal", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von eurem Gr\u00fcn umwoben", "tokens": ["Von", "eu\u00b7rem", "Gr\u00fcn", "um\u00b7wo\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie schaut' ich gern zu Tal!", "tokens": ["Wie", "schaut'", "ich", "gern", "zu", "Tal", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich sah mit trunknem Geiste", "tokens": ["Ich", "sah", "mit", "trunk\u00b7nem", "Geis\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Sonne dort vergl\u00fchn,", "tokens": ["Die", "Son\u00b7ne", "dort", "ver\u00b7gl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und mein Gedanke kreiste", "tokens": ["Und", "mein", "Ge\u00b7dan\u00b7ke", "kreis\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wie euer Falk so k\u00fchn.", "tokens": ["Wie", "eu\u00b7er", "Falk", "so", "k\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Fahrt wohl, ihr sonnigen Weiler,", "tokens": ["Fahrt", "wohl", ",", "ihr", "son\u00b7ni\u00b7gen", "Wei\u00b7ler", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Mein Bacharach so traut,", "tokens": ["Mein", "Bac\u00b7ha\u00b7rach", "so", "traut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo um Sankt Werners Pfeiler", "tokens": ["Wo", "um", "Sankt", "Wer\u00b7ners", "Pfei\u00b7ler"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "VVFIN", "NE", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Voll Glanz der Himmel blaut;", "tokens": ["Voll", "Glanz", "der", "Him\u00b7mel", "blaut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und Kaub voll rosiger Dirnen", "tokens": ["Und", "Kaub", "voll", "ro\u00b7si\u00b7ger", "Dir\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "ADJD", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und Wesel gr\u00fcn von Wein;", "tokens": ["Und", "We\u00b7sel", "gr\u00fcn", "von", "Wein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich denk' an euern Firnen", "tokens": ["Ich", "denk'", "an", "eu\u00b7ern", "Fir\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "F\u00fcrwahr noch weit vom Rhein.", "tokens": ["F\u00fcr\u00b7wahr", "noch", "weit", "vom", "Rhein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "APPRART", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und du fahr wohl, mein Dichter,", "tokens": ["Und", "du", "fahr", "wohl", ",", "mein", "Dich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du Mann so jugendgr\u00fcn,", "tokens": ["Du", "Mann", "so", "ju\u00b7gend\u00b7gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und mag dir immer lichter", "tokens": ["Und", "mag", "dir", "im\u00b7mer", "lich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Herz von Liedern bl\u00fchn!", "tokens": ["Das", "Herz", "von", "Lie\u00b7dern", "bl\u00fchn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wohl s\u00e4nge dir Besseres gerne,", "tokens": ["Wohl", "s\u00e4n\u00b7ge", "dir", "Bes\u00b7se\u00b7res", "ger\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "ADV", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Der dieses sang und schrieb;", "tokens": ["Der", "die\u00b7ses", "sang", "und", "schrieb", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDS", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Doch sei's \u2013 und halt auch ferne", "tokens": ["Doch", "sei's", "\u2013", "und", "halt", "auch", "fer\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NE", "$(", "KON", "VVFIN", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wie hier am Rhein ihn lieb!", "tokens": ["Wie", "hier", "am", "Rhein", "ihn", "lieb", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPRART", "NE", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wie flog im Land des Rheines", "tokens": ["Wie", "flog", "im", "Land", "des", "Rhei\u00b7nes"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPRART", "NN", "ART", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So rasch di