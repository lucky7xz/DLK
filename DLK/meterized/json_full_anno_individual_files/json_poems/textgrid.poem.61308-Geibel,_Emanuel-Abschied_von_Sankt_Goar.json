os": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Fahr wohl, du Lei der Lore", "tokens": ["Fahr", "wohl", ",", "du", "Lei", "der", "Lo\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "PPER", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An wilder Strudel Schwall!", "tokens": ["An", "wil\u00b7der", "Stru\u00b7del", "Schwall", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Noch t\u00f6nt in meinem Ohre", "tokens": ["Noch", "t\u00f6nt", "in", "mei\u00b7nem", "Oh\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ged\u00e4mpft dein Klagehall;", "tokens": ["Ge\u00b7d\u00e4mpft", "dein", "Kla\u00b7ge\u00b7hall", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er rief mir tief im Sinne", "tokens": ["Er", "rief", "mir", "tief", "im", "Sin\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die d\u00fcstre Sage wach", "tokens": ["Die", "d\u00fcst\u00b7re", "Sa\u00b7ge", "wach"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Vom Herzen, das die Minne", "tokens": ["Vom", "Her\u00b7zen", ",", "das", "die", "Min\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit