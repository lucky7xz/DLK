 "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Die Masken von den n\u00fcchternen Gesichtern,", "tokens": ["Die", "Mas\u00b7ken", "von", "den", "n\u00fcch\u00b7ter\u00b7nen", "Ge\u00b7sich\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Wo bliebe dann der Phrasen Flitterputz?", "tokens": ["Wo", "blie\u00b7be", "dann", "der", "Phra\u00b7sen", "Flit\u00b7ter\u00b7putz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Gut, da\u00df uns die Geschiednen nicht erscheinen;", "tokens": ["Gut", ",", "da\u00df", "uns", "die", "Ge\u00b7schied\u00b7nen", "nicht", "er\u00b7schei\u00b7nen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.14": {"text": "Auch ihnen wohl, da\u00df sie genie\u00dfen k\u00f6nnen", "tokens": ["Auch", "ih\u00b7nen", "wohl", ",", "da\u00df", "sie", "ge\u00b7nie\u00b7\u00dfen", "k\u00f6n\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "ADV", "$,", "KOUS", "PPER", "VVINF", "VMINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Die Ruhe, die wir ihnen gerne g\u00f6nnen,", "tokens": ["Die", "Ru\u00b7he", ",", "die", "wir", "ih\u00b7nen", "ger\u00b7ne", "g\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Und da\u00d