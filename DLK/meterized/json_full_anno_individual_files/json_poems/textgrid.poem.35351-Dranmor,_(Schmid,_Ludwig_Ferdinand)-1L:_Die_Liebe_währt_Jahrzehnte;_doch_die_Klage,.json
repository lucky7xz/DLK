{"textgrid.poem.35351": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Liebe w\u00e4hrt Jahrzehnte; doch die Klage,", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Liebe w\u00e4hrt Jahrzehnte; doch die Klage,", "tokens": ["Die", "Lie\u00b7be", "w\u00e4hrt", "Jahr\u00b7zehn\u00b7te", ";", "doch", "die", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$.", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht jene nur den Formen angemess'ne,", "tokens": ["Nicht", "je\u00b7ne", "nur", "den", "For\u00b7men", "an\u00b7ge\u00b7mess'\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PDS", "ADV", "ART", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Oft auch der Gram um niemals ganz vergess'ne,", "tokens": ["Oft", "auch", "der", "Gram", "um", "nie\u00b7mals", "ganz", "ver\u00b7ge\u00b7ss'\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "APPR", "ADV", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Vermindert leider sich mit jedem Tage.", "tokens": ["Ver\u00b7min\u00b7dert", "lei\u00b7der", "sich", "mit", "je\u00b7dem", "Ta\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PRF", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ach, unsre Selbstsucht spottet mancher L\u00fccken,", "tokens": ["Ach", ",", "uns\u00b7re", "Selbst\u00b7sucht", "spot\u00b7tet", "man\u00