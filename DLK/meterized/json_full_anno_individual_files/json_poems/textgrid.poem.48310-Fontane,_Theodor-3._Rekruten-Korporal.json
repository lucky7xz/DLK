{"textgrid.poem.48310": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "3. Rekruten-Korporal", "genre": "verse", "period": "N.A.", "pub_year": 1888, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In W\u00fcrzburg, bei den Bisch\u00f6flichen,", "tokens": ["In", "W\u00fcrz\u00b7burg", ",", "bei", "den", "Bi\u00b7sch\u00f6f\u00b7li\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sind ihm schon sieben Jahre verstrichen;", "tokens": ["Sind", "ihm", "schon", "sie\u00b7ben", "Jah\u00b7re", "ver\u00b7stri\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Seiner Potsdamer Tage, manch liebes Mal", "tokens": ["Sei\u00b7ner", "Pots\u00b7da\u00b7mer", "Ta\u00b7ge", ",", "manch", "lie\u00b7bes", "Mal"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PIAT", "ADJA", "NN"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Denkt der alte Korporal.", "tokens": ["Denkt", "der", "al\u00b7te", "Kor\u00b7po\u00b7ral", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Auf dem Platze, hart an der W\u00fcrzburger Br\u00fcck',", "tokens": ["Auf", "dem", "Plat\u00b7ze", ",", "hart", "an", "der", "W\u00fcrz\u00b7bur\u00b7ger", "Br\u00fcck", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADJD", "APPR", "