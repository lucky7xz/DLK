{"dta.poem.19118": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "C.  Auf vier theure Mitglieder unsrer Ge-  \n meine, so in der Christ-Woche auf den  \n Hutberg kommen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "L\u00e4mmer Christi, weinet nicht, oder weinet ihr vor Freuden,", "tokens": ["L\u00e4m\u00b7mer", "Chris\u00b7ti", ",", "wei\u00b7net", "nicht", ",", "o\u00b7der", "wei\u00b7net", "ihr", "vor", "Freu\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "VVFIN", "PTKNEG", "$,", "KON", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Da\u00df von eurer Heerde schon neuntzig mit dem Lamme weyden?", "tokens": ["Da\u00df", "von", "eu\u00b7rer", "Heer\u00b7de", "schon", "ne\u00b7unt\u00b7zig", "mit", "dem", "Lam\u00b7me", "wey\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "ADV", "CARD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Ubel angewandte Z\u00e4hren, die man der Verwesung zollt!", "tokens": ["U\u00b7bel", "an\u00b7ge\u00b7wand\u00b7te", "Z\u00e4h\u00b7ren", ",", "die", "man", "der", "Ver\u00b7we\u00b7sung", "zollt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,", "PRELS", "PIS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-++--+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.4": {"text": "Wem sind \u00fcber Hoffnungs-Saaten Thr\u00e4nen auf sein Feld ge-", "tokens": ["Wem", "sind", "\u00fc\u00b7ber", "Hoff\u00b7nungs\u00b7Saa\u00b7ten", "Thr\u00e4\u00b7nen", "auf", "sein", "Feld", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "APPR", "NN", "NN", "APPR", "PPOSAT", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.5": {"text": "Aber, was beweget mich unsre Br\u00fcder anzuschreyen:", "tokens": ["A\u00b7ber", ",", "was", "be\u00b7we\u00b7get", "mich", "uns\u00b7re", "Br\u00fc\u00b7der", "an\u00b7zu\u00b7schre\u00b7yen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWS", "VVFIN", "PPER", "PPOSAT", "NN", "VVIZU", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Da\u00df sie ihr gesegnet Korn ohne nasse Augen streuen.", "tokens": ["Da\u00df", "sie", "ihr", "ge\u00b7seg\u00b7net", "Korn", "oh\u00b7ne", "nas\u00b7se", "Au\u00b7gen", "streu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVPP", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Halten sie nicht dieses ", "tokens": ["Hal\u00b7ten", "sie", "nicht", "die\u00b7ses"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "PTKNEG", "PDAT"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Und hingegen das ", "tokens": ["Und", "hin\u00b7ge\u00b7gen", "das"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "ART"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Warlich! wer nur Herrnhut kennt, diese hingewagte H\u00fctte,", "tokens": ["War\u00b7lich", "!", "wer", "nur", "Herrn\u00b7hut", "kennt", ",", "die\u00b7se", "hin\u00b7ge\u00b7wag\u00b7te", "H\u00fct\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PWS", "ADV", "NN", "VVFIN", "$,", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Und giebt achtung auf das Volck, und auf alle seine Tritte;", "tokens": ["Und", "giebt", "ach\u00b7tung", "auf", "das", "Volck", ",", "und", "auf", "al\u00b7le", "sei\u00b7ne", "Trit\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "APPR", "ART", "NN", "$,", "KON", "APPR", "PIAT", "PPOSAT", "NN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.11": {"text": "Der wird, (hat er offne Augen,) ohne grosse M\u00fche sehn,", "tokens": ["Der", "wird", ",", "(", "hat", "er", "off\u00b7ne", "Au\u00b7gen", ",", ")", "oh\u00b7ne", "gros\u00b7se", "M\u00fc\u00b7he", "sehn", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "$(", "VAFIN", "PPER", "ADJA", "NN", "$,", "$(", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Da\u00df uns mit der Heim-Beruffung eine Gnade kan geschehn.", "tokens": ["Da\u00df", "uns", "mit", "der", "Heim\u00b7Be\u00b7ruf\u00b7fung", "ei\u00b7ne", "Gna\u00b7de", "kan", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "+---+-+-+-+-+-+", "measure": "dactylic.init"}, "line.13": {"text": "Gehe hin, du Volck des HErrn, und verschleu\u00df dich vor dem", "tokens": ["Ge\u00b7he", "hin", ",", "du", "Volck", "des", "Herrn", ",", "und", "ver\u00b7schleu\u00df", "dich", "vor", "dem"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "PTKVZ", "$,", "PPER", "NN", "ART", "NN", "$,", "KON", "VVFIN", "PRF", "APPR", "ART"], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "(da\u00df man S\u00fcnde sehen mu\u00df,) in die lustge ", "tokens": ["(", "da\u00df", "man", "S\u00fcn\u00b7de", "se\u00b7hen", "mu\u00df", ",", ")", "in", "die", "lust\u00b7ge"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["$(", "KOUS", "PIS", "NN", "VVINF", "VMFIN", "$,", "$(", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.15": {"text": "Warte, bi\u00df der Wiederbringer von dem stoltzen Bogen rufft:", "tokens": ["War\u00b7te", ",", "bi\u00df", "der", "Wie\u00b7der\u00b7brin\u00b7ger", "von", "dem", "stolt\u00b7zen", "Bo\u00b7gen", "rufft", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "NN", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.16": {"text": "Judith! G\u00fcrge! Paul! Rosine! kommt ihr Tauben aus\nder Klufft!", "tokens": ["Ju\u00b7dith", "!", "G\u00fcr\u00b7ge", "!", "Paul", "!", "Ro\u00b7si\u00b7ne", "!", "kommt", "ihr", "Tau\u00b7ben", "aus", "der", "Klufft", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "$.", "NE", "$.", "NE", "$.", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.17": {"text": "Aber, wo gerath ich hin, unbefleckte Friedens-Geister!", "tokens": ["A\u00b7ber", ",", "wo", "ge\u00b7rath", "ich", "hin", ",", "un\u00b7be\u00b7fleck\u00b7te", "Frie\u00b7dens\u00b7Geis\u00b7ter", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "VVFIN", "PPER", "PTKVZ", "$,", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.18": {"text": "La\u00dft die morschen H\u00fctten ruhn, \u00fcbergebt sie ihrem Meister,", "tokens": ["La\u00dft", "die", "mor\u00b7schen", "H\u00fct\u00b7ten", "ruhn", ",", "\u00fc\u00b7ber\u00b7gebt", "sie", "ih\u00b7rem", "Meis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "VVINF", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "--+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Unser Freund ist unser Schmeltzer, wer vertraute dem nicht gern?", "tokens": ["Un\u00b7ser", "Freund", "ist", "un\u00b7ser", "Sch\u00b7melt\u00b7zer", ",", "wer", "ver\u00b7trau\u00b7te", "dem", "nicht", "gern", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$,", "PWS", "VVFIN", "ART", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+--+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.20": {"text": "Lernt: ", "tokens": ["Lernt", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.21": {"text": "Aber ehe wir uns noch, Auserwehlte! g\u00e4ntzlich scheiden,", "tokens": ["A\u00b7ber", "e\u00b7he", "wir", "uns", "noch", ",", "Au\u00b7ser\u00b7wehl\u00b7te", "!", "g\u00e4ntz\u00b7lich", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "ADV", "$,", "NN", "$.", "ADJD", "VVINF", "$,"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.22": {"text": "Jhr dahin zieht in die Ruh, wir zur\u00fcck gehn in die Leiden,", "tokens": ["Ihr", "da\u00b7hin", "zieht", "in", "die", "Ruh", ",", "wir", "zu\u00b7r\u00fcck", "gehn", "in", "die", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PAV", "VVFIN", "APPR", "ART", "NN", "$,", "PPER", "PTKVZ", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+--+--+-+-+-", "measure": "elegiambus"}, "line.23": {"text": "Ehe wir die Ancker lichten, und verlassen euren Port;", "tokens": ["E\u00b7he", "wir", "die", "An\u00b7cker", "lich\u00b7ten", ",", "und", "ver\u00b7las\u00b7sen", "eu\u00b7ren", "Port", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJA", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.24": {"text": "Lasset uns ein Angedencken, gebet uns ein Losung Wort:", "tokens": ["Las\u00b7set", "uns", "ein", "An\u00b7ge\u00b7den\u00b7cken", ",", "ge\u00b7bet", "uns", "ein", "Lo\u00b7sung", "Wort", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}