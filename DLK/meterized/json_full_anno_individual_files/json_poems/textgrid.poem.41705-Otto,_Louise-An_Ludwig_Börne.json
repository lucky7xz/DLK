{"textgrid.poem.41705": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "An Ludwig B\u00f6rne", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war oft Brauch in alten frommen Zeiten", "tokens": ["Es", "war", "oft", "Brauch", "in", "al\u00b7ten", "from\u00b7men", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "NN", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df eine heilge Lampe ward entz\u00fcndet", "tokens": ["Da\u00df", "ei\u00b7ne", "heil\u00b7ge", "Lam\u00b7pe", "ward", "ent\u00b7z\u00fcn\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf ein geliebtes Grab ihr Licht zu breiten,", "tokens": ["Auf", "ein", "ge\u00b7lieb\u00b7tes", "Grab", "ihr", "Licht", "zu", "brei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Liebeslicht das nimmermehr entschwindet", "tokens": ["Ein", "Lie\u00b7bes\u00b7licht", "das", "nim\u00b7mer\u00b7mehr", "ent\u00b7schwin\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mit seiner Wehmut sanftem Silberscheine.", "tokens": ["Mit", "sei\u00b7ner", "Weh\u00b7mut", "sanf\u00b7tem", "Sil\u00b7ber\u00b7schei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "F\u00fcrwahr! ich m\u00f6chte gern den Brauch erneuen", "tokens": ["F\u00fcr\u00b7wahr", "!", "ich", "m\u00f6ch\u00b7te", "gern", "den", "Brauch", "er\u00b7neu\u00b7en"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "PPER", "VMFIN", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und Liebesschimmer auf ein Grab verstreuen,", "tokens": ["Und", "Lie\u00b7bes\u00b7schim\u00b7mer", "auf", "ein", "Grab", "ver\u00b7streu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die Lampe hing so gern ich auf das Deine! \u2013", "tokens": ["Die", "Lam\u00b7pe", "hing", "so", "gern", "ich", "auf", "das", "Dei\u00b7ne", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "PPER", "APPR", "ART", "PPOSAT", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Als mir zuerst die Kunde war gekommen:", "tokens": ["Als", "mir", "zu\u00b7erst", "die", "Kun\u00b7de", "war", "ge\u00b7kom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbach, unser ", "tokens": ["\u00bb", "ach", ",", "un\u00b7ser"], "token_info": ["punct", "word", "punct", "word"], "pos": ["$(", "ITJ", "$,", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Hat unsren treusten K\u00e4mpfer aufgenommen?\u00ab \u2013", "tokens": ["Hat", "un\u00b7sren", "treus\u00b7ten", "K\u00e4mp\u00b7fer", "auf\u00b7ge\u00b7nom\u00b7men", "?", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "VVPP", "$.", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da kannte ich noch nicht den gro\u00dfen Toten;", "tokens": ["Da", "kann\u00b7te", "ich", "noch", "nicht", "den", "gro\u00b7\u00dfen", "To\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sah nur der Lieder Leichenfackeln blinken,", "tokens": ["Sah", "nur", "der", "Lie\u00b7der", "Lei\u00b7chen\u00b7fa\u00b7ckeln", "blin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die hinter Deinem Sarge hergetragen,", "tokens": ["Die", "hin\u00b7ter", "Dei\u00b7nem", "Sar\u00b7ge", "her\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Sah Deiner J\u00fcnger Thr\u00e4nen niedersinken \u2013", "tokens": ["Sah", "Dei\u00b7ner", "J\u00fcn\u00b7ger", "Thr\u00e4\u00b7nen", "nie\u00b7der\u00b7sin\u00b7ken", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und lie\u00df mir Deines Lebens K\u00e4mpfe sagen.", "tokens": ["Und", "lie\u00df", "mir", "Dei\u00b7nes", "Le\u00b7bens", "K\u00e4mp\u00b7fe", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nun lauscht ich selber der Prophetenstimme,", "tokens": ["Nun", "lauscht", "ich", "sel\u00b7ber", "der", "Pro\u00b7phe\u00b7ten\u00b7stim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die f\u00fcr die Freiheit alles Volk entflammte,", "tokens": ["Die", "f\u00fcr", "die", "Frei\u00b7heit", "al\u00b7les", "Volk", "ent\u00b7flamm\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die, bald vernichtend, Deines Hasses Grimme", "tokens": ["Die", ",", "bald", "ver\u00b7nich\u00b7tend", ",", "Dei\u00b7nes", "Has\u00b7ses", "Grim\u00b7me"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "$,", "ADV", "VVPP", "$,", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bald Deiner Liebe f\u00fcr das Volk entstammte.", "tokens": ["Bald", "Dei\u00b7ner", "Lie\u00b7be", "f\u00fcr", "das", "Volk", "ent\u00b7stamm\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da pre\u00dft die Seele Sehnsucht mir zusammen,", "tokens": ["Da", "pre\u00dft", "die", "See\u00b7le", "Sehn\u00b7sucht", "mir", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein lindernd \u00d6l f\u00fchl da ich in mir flie\u00dfen,", "tokens": ["Ein", "lin\u00b7dernd", "\u00d6l", "f\u00fchl", "da", "ich", "in", "mir", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "KOUS", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "In eine goldne Lampe m\u00f6cht ich's gie\u00dfen", "tokens": ["In", "ei\u00b7ne", "gold\u00b7ne", "Lam\u00b7pe", "m\u00f6cht", "ich's", "gie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VMFIN", "PIS", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Von Deinem Grabe durch die Welt zu flammen.", "tokens": ["Von", "Dei\u00b7nem", "Gra\u00b7be", "durch", "die", "Welt", "zu", "flam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Des \u00d6les Balsam, den ich so empfangen,", "tokens": ["Des", "\u00d6\u00b7les", "Bal\u00b7sam", ",", "den", "ich", "so", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es ist das Lied mit seinem hellen Dochte,", "tokens": ["Es", "ist", "das", "Lied", "mit", "sei\u00b7nem", "hel\u00b7len", "Doch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dem Freiheitsstreben und dem Kraftverlangen,", "tokens": ["Dem", "Frei\u00b7heits\u00b7stre\u00b7ben", "und", "dem", "Kraft\u00b7ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das ich nur Dir, nur Dir verdanken mochte!", "tokens": ["Das", "ich", "nur", "Dir", ",", "nur", "Dir", "ver\u00b7dan\u00b7ken", "moch\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "PPER", "$,", "ADV", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich bin ein Weib \u2013 doch wirst Du nicht verachten", "tokens": ["Ich", "bin", "ein", "Weib", "\u2013", "doch", "wirst", "Du", "nicht", "ver\u00b7ach\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$(", "ADV", "VAFIN", "PPER", "PTKNEG", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mein Streben, nicht mein Lieben und mein Singen!", "tokens": ["Mein", "Stre\u00b7ben", ",", "nicht", "mein", "Lie\u00b7ben", "und", "mein", "Sin\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PTKNEG", "PPOSAT", "ADJA", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ich bin ein Kind \u2013 kann keine Schwerter schwingen,", "tokens": ["Ich", "bin", "ein", "Kind", "\u2013", "kann", "kei\u00b7ne", "Schwer\u00b7ter", "schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$(", "VMFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Den Brand nicht werfen, wo die V\u00f6lker nachten.", "tokens": ["Den", "Brand", "nicht", "wer\u00b7fen", ",", "wo", "die", "V\u00f6l\u00b7ker", "nach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVINF", "$,", "PWAV", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Doch ist's ein weiblich, kindliches Gesch\u00e4fte", "tokens": ["Doch", "ist's", "ein", "weib\u00b7lich", ",", "kind\u00b7li\u00b7ches", "Ge\u00b7sch\u00e4f\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VAFIN", "ART", "ADJD", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Treue Lampe sorgsam fortzupflegen.", "tokens": ["Der", "Treu\u00b7e", "Lam\u00b7pe", "sorg\u00b7sam", "fort\u00b7zu\u00b7pfle\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das heischt nur Wachsamkeit nicht M\u00e4nnerkr\u00e4fte", "tokens": ["Das", "heischt", "nur", "Wach\u00b7sam\u00b7keit", "nicht", "M\u00e4n\u00b7ner\u00b7kr\u00e4f\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "NN", "PTKNEG", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und giebt im Dunkeln doch des Lichtes Segen,", "tokens": ["Und", "giebt", "im", "Dun\u00b7keln", "doch", "des", "Lich\u00b7tes", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und w\u00e4r es nur ein bleicher Silberschimmer:", "tokens": ["Und", "w\u00e4r", "es", "nur", "ein", "blei\u00b7cher", "Sil\u00b7ber\u00b7schim\u00b7mer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "'s ist besser doch als ganz im Finstern weilen.", "tokens": ["'s", "ist", "bes\u00b7ser", "doch", "als", "ganz", "im", "Fins\u00b7tern", "wei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ADV", "KOKOM", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.7": {"text": "Das \u00d6l der Liebe brennt \u2013 doch kann's auch heilen:", "tokens": ["Das", "\u00d6l", "der", "Lie\u00b7be", "brennt", "\u2013", "doch", "kann's", "auch", "hei\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$(", "ADV", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Es war oft Brauch in alten frommen Zeiten", "tokens": ["Es", "war", "oft", "Brauch", "in", "al\u00b7ten", "from\u00b7men", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "NN", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df eine heilge Lampe ward entz\u00fcndet", "tokens": ["Da\u00df", "ei\u00b7ne", "heil\u00b7ge", "Lam\u00b7pe", "ward", "ent\u00b7z\u00fcn\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf ein geliebtes Grab ihr Licht zu breiten,", "tokens": ["Auf", "ein", "ge\u00b7lieb\u00b7tes", "Grab", "ihr", "Licht", "zu", "bre