{"textgrid.poem.41614": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Sonne \u00fcberzog ein Schleier. Wie ihr Strahl,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sonne \u00fcberzog ein Schleier. Wie ihr Strahl,", "tokens": ["Die", "Son\u00b7ne", "\u00fc\u00b7ber\u00b7zog", "ein", "Schlei\u00b7er", ".", "Wie", "ihr", "Strahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$.", "PWAV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O meines Lebens Mond, h\u00fcll dich in warme Schatten;", "tokens": ["O", "mei\u00b7nes", "Le\u00b7bens", "Mond", ",", "h\u00fcll", "dich", "in", "war\u00b7me", "Schat\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "NN", "$,", "ADJD", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Umw\u00f6lk dich oder schlaf! Sei stumm, und im Ermatten", "tokens": ["Um\u00b7w\u00f6lk", "dich", "o\u00b7der", "schlaf", "!", "Sei", "stumm", ",", "und", "im", "Er\u00b7mat\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "KON", "VVFIN", "$.", "VAIMP", "ADJD", "$,", "KON", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vergeh und sinke in der Leere n\u00e4chtig Tal!", "tokens": ["Ver\u00b7geh", "und", "sin\u00b7ke", "in", "der", "Lee\u00b7re", "n\u00e4ch\u00b7tig", "Tal", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "V