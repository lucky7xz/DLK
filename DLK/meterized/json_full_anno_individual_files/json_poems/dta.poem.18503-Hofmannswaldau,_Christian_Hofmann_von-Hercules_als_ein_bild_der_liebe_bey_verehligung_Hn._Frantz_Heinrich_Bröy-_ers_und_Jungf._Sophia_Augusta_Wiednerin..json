{"dta.poem.18503": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Hercules als ein bild der liebe/ bey  \n verehligung Hn. Frantz Heinrich Br\u00f6y-  \n ers/ und Jungf. Sophia  \n Augusta Wiednerin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gleich heute/ werthes paar/ ist Hereulis sein tag/", "tokens": ["Gleich", "heu\u00b7te", "/", "wert\u00b7hes", "paar", "/", "ist", "Her\u00b7eu\u00b7lis", "sein", "tag", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$(", "PWS", "PIAT", "$(", "VAFIN", "NE", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der zwar im heydenthum der st\u00e4rckste held gewesen;", "tokens": ["Der", "zwar", "im", "hey\u00b7den\u00b7thum", "der", "st\u00e4rcks\u00b7te", "held", "ge\u00b7we\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "ART", "ADJA", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch weil den tichtern nichts verwehret werden mag/", "tokens": ["Doch", "weil", "den", "tich\u00b7tern", "nichts", "ver\u00b7weh\u00b7ret", "wer\u00b7den", "mag", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PIS", "VVPP", "VAINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hab ich ihn als ein bild von eurer lieb erlesen.", "tokens": ["Hab", "ich", "ihn", "als", "ein", "bild", "von", "eu\u00b7rer", "lieb", "er\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "KOUS", "ART", "NN", "APPR", "PPOSAT", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nicht denckt/ verliebten zwey/ da\u00df dieser wilde mann", "tokens": ["Nicht", "denckt", "/", "ver\u00b7lieb\u00b7ten", "zwey", "/", "da\u00df", "die\u00b7ser", "wil\u00b7de", "mann"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "$(", "VVFIN", "CARD", "$(", "KOUS", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mit seiner keule hier vor euer antlitz steiget/", "tokens": ["Mit", "sei\u00b7ner", "keu\u00b7le", "hier", "vor", "eu\u00b7er", "ant\u00b7litz", "stei\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und da\u00df er ihm die haut von l\u00f6wen angethan/", "tokens": ["Und", "da\u00df", "er", "ihm", "die", "haut", "von", "l\u00f6\u00b7wen", "an\u00b7ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "ART", "NN", "APPR", "NE", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wie ihn ein sch\u00f6ner brunn in unserm Leipzig ", "tokens": ["Wie", "ihn", "ein", "sch\u00f6\u00b7ner", "brunn", "in", "un\u00b7serm", "Leip\u00b7zig"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Nein! schauet ihn vielmehr/ wie er Omphalen liebt/", "tokens": ["Nein", "!", "schau\u00b7et", "ihn", "viel\u00b7mehr", "/", "wie", "er", "Om\u00b7pha\u00b7len", "liebt", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "VVFIN", "PPER", "ADV", "$(", "PWAV", "PPER", "NN", "VVFIN", "$("], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Wie diese D\u00e4lila den Simson gantz gewinnet/", "tokens": ["Wie", "die\u00b7se", "D\u00e4\u00b7li\u00b7la", "den", "Sim\u00b7son", "gantz", "ge\u00b7win\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "ART", "NE", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wie er auf ihr gehei\u00df die waffen von sich giebt/", "tokens": ["Wie", "er", "auf", "ihr", "ge\u00b7hei\u00df", "die", "waf\u00b7fen", "von", "sich", "giebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPOSAT", "NN", "ART", "NN", "APPR", "PRF", "VVFIN", "$("], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.12": {"text": "Die spindel aber nimmt/ und an dem rocken spinnet.", "tokens": ["Die", "spin\u00b7del", "a\u00b7ber", "nimmt", "/", "und", "an", "dem", "ro\u00b7cken", "spin\u00b7net", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "VVFIN", "$(", "KON", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Di\u00df lehret allbereit/ da\u00df lieben m\u00e4chtig sey/", "tokens": ["Di\u00df", "leh\u00b7ret", "all\u00b7be\u00b7reit", "/", "da\u00df", "lie\u00b7ben", "m\u00e4ch\u00b7tig", "sey", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "$(", "KOUS", "ADJA", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und da\u00df die Venus selbst den krieges-gott besieget/", "tokens": ["Und", "da\u00df", "die", "Ve\u00b7nus", "selbst", "den", "krie\u00b7ge\u00b7sgott", "be\u00b7sie\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ja/ dieses spricht auch euch von schuld und fehler frey/", "tokens": ["Ja", "/", "die\u00b7ses", "spricht", "auch", "euch", "von", "schuld", "und", "feh\u00b7ler", "frey", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "PDS", "VVFIN", "ADV", "PPER", "APPR", "ADJD", "KON", "ADJD", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wenn ihr als sclaven heut in liebes-fesseln lieget.", "tokens": ["Wenn", "ihr", "als", "scla\u00b7ven", "heut", "in", "lie\u00b7bes\u00b7fes\u00b7seln", "lie\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOKOM", "VVFIN", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Er/ werther br\u00e4utigam/ ist ja kein krieges-mann/", "tokens": ["Er", "/", "wert\u00b7her", "br\u00e4u\u00b7ti\u00b7gam", "/", "ist", "ja", "kein", "krie\u00b7ge\u00b7smann", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "PWS", "VVFIN", "$(", "VAFIN", "ADV", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und seine braut ist nicht vom heer der Amazonen/", "tokens": ["Und", "sei\u00b7ne", "braut", "ist", "nicht", "vom", "heer", "der", "A\u00b7maz\u00b7o\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "VVFIN", "VAFIN", "PTKNEG", "APPRART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Was wunder/ da\u00df ihr arm nicht widerstehen kan/", "tokens": ["Was", "wun\u00b7der", "/", "da\u00df", "ihr", "arm", "nicht", "wi\u00b7der\u00b7ste\u00b7hen", "kan", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$(", "KOUS", "PPER", "ADJD", "PTKNEG", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und da\u00df sich beyder brust von liebe l\u00e4st bewohnen?", "tokens": ["Und", "da\u00df", "sich", "bey\u00b7der", "brust", "von", "lie\u00b7be", "l\u00e4st", "be\u00b7woh\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PRF", "PIAT", "NN", "APPR", "VVFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die t\u00f6chter Hesperi besassen einen platz/", "tokens": ["Die", "t\u00f6ch\u00b7ter", "Hes\u00b7pe\u00b7ri", "be\u00b7sas\u00b7sen", "ei\u00b7nen", "platz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Auf dem die b\u00e4ume nichts als g\u00f6ldne \u00e4pffel trugen:", "tokens": ["Auf", "dem", "die", "b\u00e4u\u00b7me", "nichts", "als", "g\u00f6ld\u00b7ne", "\u00e4pf\u00b7fel", "tru\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "PIS", "KOKOM", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die h\u00e4nde Hereulis bekamen diesen schatz/", "tokens": ["Die", "h\u00e4n\u00b7de", "Her\u00b7eu\u00b7lis", "be\u00b7ka\u00b7men", "die\u00b7sen", "schatz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Da sie den drachen erst/ der ihn bewacht/ erschlugen:", "tokens": ["Da", "sie", "den", "dra\u00b7chen", "erst", "/", "der", "ihn", "be\u00b7wacht", "/", "er\u00b7schlu\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "ADV", "$(", "PRELS", "PPER", "VVPP", "$(", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Mich d\u00fcnckt/ da\u00df einsamkeit sich einem drachen gleicht/", "tokens": ["Mich", "d\u00fcnckt", "/", "da\u00df", "ein\u00b7sam\u00b7keit", "sich", "ei\u00b7nem", "dra\u00b7chen", "gleicht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "NN", "PRF", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Der unverehlichte stets zu begleiten pfleget/", "tokens": ["Der", "un\u00b7ve\u00b7reh\u00b7lich\u00b7te", "stets", "zu", "be\u00b7glei\u00b7ten", "pfle\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Und der auch eher nicht von ihrer seite weicht/", "tokens": ["Und", "der", "auch", "e\u00b7her", "nicht", "von", "ih\u00b7rer", "sei\u00b7te", "weicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "ADV", "PTKNEG", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Bi\u00df keusche liebe ihn durch ihre macht erleget.", "tokens": ["Bi\u00df", "keu\u00b7sche", "lie\u00b7be", "ihn", "durch", "ih\u00b7re", "macht", "er\u00b7le\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVFIN", "PPER", "APPR", "PPOSAT", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Verlobten/ heute schl\u00fcst sich euer einsam seyn/", "tokens": ["Ver\u00b7lob\u00b7ten", "/", "heu\u00b7te", "schl\u00fcst", "sich", "eu\u00b7er", "ein\u00b7sam", "seyn", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ADV", "VVFIN", "PRF", "PPOSAT", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und lieb und treue will euch be\u00dfre zeit versprechen.", "tokens": ["Und", "lieb", "und", "treu\u00b7e", "will", "euch", "be\u00df\u00b7re", "zeit", "ver\u00b7spre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJA", "VMFIN", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Die ehe f\u00fchret euch ins paradie\u00df hinein/", "tokens": ["Die", "e\u00b7he", "f\u00fch\u00b7ret", "euch", "ins", "pa\u00b7ra\u00b7die\u00df", "hin\u00b7ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Wo die vergn\u00fcgung heist nur g\u00f6ldne \u00e4pffel brechen.", "tokens": ["Wo", "die", "ver\u00b7gn\u00fc\u00b7gung", "heist", "nur", "g\u00f6ld\u00b7ne", "\u00e4pf\u00b7fel", "bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Da\u00df bey dem alterthum in Rom und zu Athen", "tokens": ["Da\u00df", "bey", "dem", "al\u00b7ter\u00b7thum", "in", "Rom", "und", "zu", "A\u00b7then"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "APPR", "NE", "KON", "APPR", "NE"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "Die ringer nackt und blo\u00df auff schau-ger\u00fcsten stunden/", "tokens": ["Die", "rin\u00b7ger", "nackt", "und", "blo\u00df", "auff", "schau\u00b7ge\u00b7r\u00fcs\u00b7ten", "stun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJD", "KON", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Und da\u00df sie so entbl\u00f6st zum kampffe musten gehn/", "tokens": ["Und", "da\u00df", "sie", "so", "ent\u00b7bl\u00f6st", "zum", "kampf\u00b7fe", "mus\u00b7ten", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVFIN", "APPRART", "VVFIN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Hat gleichfalls Hercules zum denckmahl ihm erfunden.", "tokens": ["Hat", "gleich\u00b7falls", "Her\u00b7cu\u00b7les", "zum", "denck\u00b7mahl", "ihm", "er\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "APPRART", "ADV", "PPER", "VVPP", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.37": {"text": "Die liebe will auch euch/ ihr angenehmes paar/", "tokens": ["Die", "lie\u00b7be", "will", "auch", "euch", "/", "ihr", "an\u00b7ge\u00b7neh\u00b7mes", "paar", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VMFIN", "ADV", "PPER", "$(", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "An diesem abend noch ein gleiches spiel verg\u00f6nnen/", "tokens": ["An", "die\u00b7sem", "a\u00b7bend", "noch", "ein", "glei\u00b7ches", "spiel", "ver\u00b7g\u00f6n\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADV", "ADV", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Sie leget allen schertz zu euren diensten dar/", "tokens": ["Sie", "le\u00b7get", "al\u00b7len", "schertz", "zu", "eu\u00b7ren", "diens\u00b7ten", "dar", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Jhr werdet unbekleidt zusammen ringen k\u00f6nnen.", "tokens": ["Ihr", "wer\u00b7det", "un\u00b7be\u00b7kleidt", "zu\u00b7sam\u00b7men", "rin\u00b7gen", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ADV", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Wohlan/ man w\u00fcnscht euch viel. Ich w\u00fcndsche nur inde\u00df", "tokens": ["Wo\u00b7hlan", "/", "man", "w\u00fcnscht", "euch", "viel", ".", "Ich", "w\u00fcnd\u00b7sche", "