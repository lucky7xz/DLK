{"dta.poem.12606": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Erster Theil.  \n Von den Tisch-Liedern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905199128", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O w\u00fcrd\u2019 ich eine sch\u00f6ne Leyer", "tokens": ["O", "w\u00fcrd'", "ich", "ei\u00b7ne", "sch\u00f6\u00b7ne", "Le\u00b7yer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Von weissem Elfenbein,", "tokens": ["Von", "weis\u00b7sem", "El\u00b7fen\u00b7bein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und k\u00f6nnt\u2019 ich denn durch sch\u00f6ner Kinder Hand", "tokens": ["Und", "k\u00f6nnt'", "ich", "denn", "durch", "sch\u00f6\u00b7ner", "Kin\u00b7der", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zum Bacchus-Tanz getragen seyn!", "tokens": ["Zum", "Bac\u00b7chus\u00b7Tanz", "ge\u00b7tra\u00b7gen", "seyn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O w\u00fcrd\u2019 ich Gold, das noch kein Feuer", "tokens": ["O", "w\u00fcrd'", "ich", "Gold", ",", "das", "noch", "kein", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "NN", "$,", "PRELS", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Versehrt und durchgebrannt,", "tokens": ["Ver\u00b7sehrt", "und", "durch\u00b7ge\u00b7brannt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und n\u00e4hm\u2019 ein tugendhaftes Weib", "tokens": ["Und", "n\u00e4hm'", "ein", "tu\u00b7gend\u00b7haf\u00b7tes", "Weib"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mich denn an ihren sch\u00f6nen Leib!", "tokens": ["Mich", "denn", "an", "ih\u00b7ren", "sch\u00f6\u00b7nen", "Leib", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Lebe, trinke, liebe, l\u00e4rme,", "tokens": ["Le\u00b7be", ",", "trin\u00b7ke", ",", "lie\u00b7be", ",", "l\u00e4r\u00b7me", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVIMP", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kr\u00e4nze dich mit mir!", "tokens": ["Kr\u00e4n\u00b7ze", "dich", "mit", "mir", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Schw\u00e4rme mit mir, wenn ich schw\u00e4rme;", "tokens": ["Schw\u00e4r\u00b7me", "mit", "mir", ",", "wenn", "ich", "schw\u00e4r\u00b7me", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich bin wieder klug mit dir.", "tokens": ["Ich", "bin", "wie\u00b7der", "klug", "mit", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Auf! Cotonis, schenk mir ein,", "tokens": ["Auf", "!", "Co\u00b7to\u00b7nis", ",", "schenk", "mir", "ein", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schenk mir ein, und h\u00f6re,", "tokens": ["Schenk", "mir", "ein", ",", "und", "h\u00f6\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "La\u00df dir diese Lehre", "tokens": ["La\u00df", "dir", "die\u00b7se", "Leh\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PDAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Heut von mir gesaget seyn:", "tokens": ["Heut", "von", "mir", "ge\u00b7sa\u00b7get", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Man mu\u00df das Getr\u00e4nk der Reben", "tokens": ["Man", "mu\u00df", "das", "Ge\u00b7tr\u00e4nk", "der", "Re\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "ART", "NN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Allen braven Leuten geben.", "tokens": ["Al\u00b7len", "bra\u00b7ven", "Leu\u00b7ten", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}