{"dta.poem.19237": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "84.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Den Wald erf\u00fcllte laut der L\u00f6we mit Gebr\u00fclle,", "tokens": ["Den", "Wald", "er\u00b7f\u00fcll\u00b7te", "laut", "der", "L\u00f6\u00b7we", "mit", "Ge\u00b7br\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NE", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df mit dem blut'gen Raub er seinen Rachen f\u00fclle;", "tokens": ["Da\u00df", "mit", "dem", "blut'\u00b7gen", "Raub", "er", "sei\u00b7nen", "Ra\u00b7chen", "f\u00fcl\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Als unterm Rasen leis' ein Ameisl\u00f6wchen sprach:", "tokens": ["Als", "un\u00b7term", "Ra\u00b7sen", "leis'", "ein", "A\u00b7meis\u00b7l\u00f6w\u00b7chen", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was jagt so ungest\u00fcm dem Wild der Wilde nach?", "tokens": ["Was", "jagt", "so", "un\u00b7ge\u00b7st\u00fcm", "dem", "Wild", "der", "Wil\u00b7de", "nach", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ADJD", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich sitze still im Sand, um ruhig zu verspeisen", "tokens": ["Ich", "sit\u00b7ze", "still", "im", "Sand", ",", "um", "ru\u00b7hig", "zu", "ver\u00b7spei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "NN", "$,", "KOUI", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die durch den Trichter mir herrollenden Ameisen;", "tokens": ["Die", "durch", "den", "Trich\u00b7ter", "mir", "her\u00b7rol\u00b7len\u00b7den", "A\u00b7mei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und von dem magersten Ameischen werd' ich satt,", "tokens": ["Und", "von", "dem", "ma\u00b7gers\u00b7ten", "A\u00b7mei\u00b7schen", "werd'", "ich", "satt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-++--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wie er vom fettsten Reh, wenn ers erjaget hat.", "tokens": ["Wie", "er", "vom", "fetts\u00b7ten", "Reh", ",", "wenn", "ers", "er\u00b7ja\u00b7get", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "ADJA", "NN", "$,", "KOUS", "PIS", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Mag er nun gr\u00f6\u00dfern Raub und blutigern zerrei\u00dfen,", "tokens": ["Mag", "er", "nun", "gr\u00f6\u00b7\u00dfern", "Raub", "und", "blu\u00b7ti\u00b7gern", "zer\u00b7rei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJA", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was liegt daran, wenn wir doch beide L\u00f6wen hei\u00dfen?", "tokens": ["Was", "liegt", "da\u00b7ran", ",", "wenn", "wir", "doch", "bei\u00b7de", "L\u00f6\u00b7wen", "hei\u00b7\u00dfen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PAV", "$,", "KOUS", "PPER", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}