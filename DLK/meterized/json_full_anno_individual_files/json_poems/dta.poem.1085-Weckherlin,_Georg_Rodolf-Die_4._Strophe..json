{"dta.poem.1085": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Die 4. Strophe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1618", "urn": "urn:nbn:de:kobv:b4-20628-9", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wje oft hat dich dein Vatterland/", "tokens": ["Wie", "oft", "hat", "dich", "dein", "Vat\u00b7ter\u00b7land", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VAFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wan du von meine\u0303 grossen Printzen", "tokens": ["Wan", "du", "von", "mein\u1ebd", "gros\u00b7sen", "Print\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In vil ferr-ligende Provintzen", "tokens": ["In", "vil", "ferr\u00b7li\u00b7gen\u00b7de", "Pro\u00b7vint\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "W\u00fcrdiglich warest au\u00dfgesant/", "tokens": ["W\u00fcr\u00b7dig\u00b7lich", "wa\u00b7rest", "au\u00df\u00b7ge\u00b7sant", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zumahl voll trawrigkeit vnd fraid", "tokens": ["Zu\u00b7mahl", "voll", "traw\u00b7rig\u00b7keit", "vnd", "fraid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit hail vnd gl\u00fcckw\u00fcnschung geehret?", "tokens": ["Mit", "hail", "vnd", "gl\u00fcck\u00b7w\u00fcn\u00b7schung", "ge\u00b7eh\u00b7ret", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wissend wie dein abwesen sein laid/", "tokens": ["Wis\u00b7send", "wie", "dein", "ab\u00b7we\u00b7sen", "sein", "laid", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "PPOSAT", "VVFIN", "PPOSAT", "NN", "$("], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Vnd auch seinen nutzen vermehret:", "tokens": ["Vnd", "auch", "sei\u00b7nen", "nut\u00b7zen", "ver\u00b7meh\u00b7ret", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Dieweil deine wolredenheit", "tokens": ["Die\u00b7weil", "dei\u00b7ne", "wol\u00b7re\u00b7den\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Kan an dich lieblich ohn bem\u00fchen", "tokens": ["Kan", "an", "dich", "lieb\u00b7lich", "ohn", "be\u00b7m\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "PPER", "ADJD", "APPR", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Der gr\u00f6sten Monarchen hochheit/", "tokens": ["Der", "gr\u00f6s\u00b7ten", "Mon\u00b7ar\u00b7chen", "hoch\u00b7heit", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$("], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.12": {"text": "Vnd die Sehl durch die ohren ziehen.", "tokens": ["Vnd", "die", "Sehl", "durch", "die", "oh\u00b7ren", "zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}}}}