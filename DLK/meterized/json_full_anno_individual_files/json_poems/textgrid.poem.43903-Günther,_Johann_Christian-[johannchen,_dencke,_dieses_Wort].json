 auch vorbey.", "tokens": ["Ein", "b\u00f6\u00b7ser", "Tag", "geht", "auch", "vor\u00b7bey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Ich weis, der Gott, dem mein Gebeth", "tokens": ["Ich", "weis", ",", "der", "Gott", ",", "dem", "mein", "Ge\u00b7beth"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PTKVZ", "$,", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Um Mitternacht das Opfer bringet,", "tokens": ["Um", "Mit\u00b7ter\u00b7nacht", "das", "Op\u00b7fer", "brin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Erh\u00f6rt, was meine Demuth fleht,", "tokens": ["Er\u00b7h\u00f6rt", ",", "was", "mei\u00b7ne", "De\u00b7muth", "fleht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die oft mit der Verzweiflung ringet;", "tokens": ["Die", "oft", "mit", "der", "Ver\u00b7zwei\u00b7flung", "rin\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich weis, da\u00df er, hab ich gefehlt,", "tokens": ["Ich", "weis", ",", "da\u00df", "er", ",", "hab", "ich", "ge\u00b7fehlt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "KOUS", "PPER", "$,", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Die Thr\u00e4nen nicht vergebens zehlt.", "tokens": ["Die", "Thr\u00e4\u00b7nen", "nicht", "ver\u00b7ge\u00b7bens", "zehlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}