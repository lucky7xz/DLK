{"textgrid.poem.43903": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "[johannchen, dencke, dieses Wort]", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Johannchen, dencke, dieses Wort", "tokens": ["Jo\u00b7hann\u00b7chen", ",", "den\u00b7cke", ",", "die\u00b7ses", "Wort"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "$,", "VVFIN", "$,", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geht aus der Tiefe meines Herzens:", "tokens": ["Geht", "aus", "der", "Tie\u00b7fe", "mei\u00b7nes", "Her\u00b7zens", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ach seze deine Freundschaft fort,", "tokens": ["Ach", "se\u00b7ze", "dei\u00b7ne", "Freund\u00b7schaft", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Entbinde mich des harten Schmerzens,", "tokens": ["Ent\u00b7bin\u00b7de", "mich", "des", "har\u00b7ten", "Schmer\u00b7zens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der mich um derentwillen plagt,", "tokens": ["Der", "mich", "um", "de\u00b7rent\u00b7wil\u00b7len", "plagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PDS", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Die mir ihr Herze zugesagt.", "tokens": ["Die", "mir", "ihr", "Her\u00b7ze", "zu\u00b7ge\u00b7sagt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach sorge vor mein liebstes Kind;", "tokens": ["Ach", "sor\u00b7ge", "vor", "mein", "liebs\u00b7tes", "Kind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich weis, sie wird es dir noch dancken.", "tokens": ["Ich", "weis", ",", "sie", "wird", "es", "dir", "noch", "dan\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "PPER", "VAFIN", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So wahr mir jezt das Auge rinnt,", "tokens": ["So", "wahr", "mir", "jezt", "das", "Au\u00b7ge", "rinnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So wahr werd ich und sie nicht wancken.", "tokens": ["So", "wahr", "werd", "ich", "und", "sie", "nicht", "wan\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "KON", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Gedencke, sag ich noch einmahl,", "tokens": ["Ge\u00b7den\u00b7cke", ",", "sag", "ich", "noch", "ein\u00b7mahl", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der alten Lust, der neuen Qual.", "tokens": ["Der", "al\u00b7ten", "Lust", ",", "der", "neu\u00b7en", "Qual", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich gr\u00fcnde mich auf deine Gunst", "tokens": ["Ich", "gr\u00fcn\u00b7de", "mich", "auf", "dei\u00b7ne", "Gunst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und traue deiner reinen G\u00fcte", "tokens": ["Und", "trau\u00b7e", "dei\u00b7ner", "rei\u00b7nen", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "(es ist gewis kein falscher Dunst)", "tokens": ["(", "es", "ist", "ge\u00b7wis", "kein", "fal\u00b7scher", "Dunst", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und seh dein redliches Gem\u00fcthe;", "tokens": ["Und", "seh", "dein", "red\u00b7li\u00b7ches", "Ge\u00b7m\u00fc\u00b7the", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Las mich, ach m\u00f6cht es bald geschehn,", "tokens": ["Las", "mich", ",", "ach", "m\u00f6cht", "es", "bald", "ge\u00b7schehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "ADV", "VMFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von dir doch einen Trostbrief sehn.", "tokens": ["Von", "dir", "doch", "ei\u00b7nen", "Trost\u00b7brief", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du meines Herzens halber Theil,", "tokens": ["Du", "mei\u00b7nes", "Her\u00b7zens", "hal\u00b7ber", "Theil", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "APPO", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Kind, mein Schaz, mein Heil, mein Leben,", "tokens": ["Mein", "Kind", ",", "mein", "Schaz", ",", "mein", "Heil", ",", "mein", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wirst gleichfalls mir in aller Eil", "tokens": ["Wirst", "gleich\u00b7falls", "mir", "in", "al\u00b7ler", "Eil"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Pflaster vor die Wunde geben;", "tokens": ["Ein", "Pflas\u00b7ter", "vor", "die", "Wun\u00b7de", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Bleib fromm und redlich, halt getreu \u2013", "tokens": ["Bleib", "fromm", "und", "red\u00b7lich", ",", "halt", "ge\u00b7treu", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "$,", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein b\u00f6ser Tag geht auch vorbey.", "tokens": ["Ein", "b\u00f6\u00b7ser", "Tag", "geht", "auch", "vor\u00b7bey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich weis, der Gott, dem mein Gebeth", "tokens": ["Ich", "weis", ",", "der", "Gott", ",", "dem", "mein", "Ge\u00b7beth"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PTKVZ", "$,", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Um Mitternacht das Opfer bringet,", "tokens": ["Um", "Mit\u00b7ter\u00b7nacht", "das", "Op\u00b7fer", "brin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Erh\u00f6rt, was meine Demuth fleht,", "tokens": ["Er\u00b7h\u00f6rt", ",", "was", "mei\u00b7ne", "De\u00b7muth", "fleht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die oft mit der Verzweiflung ringet;", "tokens": ["Die", "oft", "mit", "der", "Ver\u00b7zwei\u00b7flung", "rin\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich weis, da\u00df er, hab ich gefehlt,", "tokens": ["Ich", "weis", ",", "da\u00df", "er", ",", "hab", "ich", "ge\u00b7fehlt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "KOUS", "PPER", "$,", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}