{"dta.poem.6590": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den durchsichtigen und nicht durch-  \n sichtigen Dingen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was andre C\u00f6rper nun betrifft, durch die der Schein", "tokens": ["Was", "and\u00b7re", "C\u00f6r\u00b7per", "nun", "be\u00b7tr\u00b7ifft", ",", "durch", "die", "der", "Schein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "ADJA", "NN", "ADV", "VVFIN", "$,", "APPR", "PRELS", "ART", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und Licht-Strahl aufgefangen seyn;", "tokens": ["Und", "Licht\u00b7Strahl", "auf\u00b7ge\u00b7fan\u00b7gen", "seyn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So haben die ein\u2019 Art von Oeffnungen in sich", "tokens": ["So", "ha\u00b7ben", "die", "ein'", "Art", "von", "Oeff\u00b7nun\u00b7gen", "in", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ART", "NN", "APPR", "NN", "APPR", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die, wie die andere, nicht in geraden Strich\u2019", "tokens": ["Die", ",", "wie", "die", "an\u00b7de\u00b7re", ",", "nicht", "in", "ge\u00b7ra\u00b7den", "Strich'"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "$,", "PWAV", "ART", "ADJA", "$,", "PTKNEG", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Durch ihre C\u00f6rper gehen,", "tokens": ["Durch", "ih\u00b7re", "C\u00f6r\u00b7per", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nein, durch verwirrte G\u00e4ng\u2019 den Strahlen wiederstehen:", "tokens": ["Nein", ",", "durch", "ver\u00b7wirr\u00b7te", "G\u00e4ng'", "den", "Strah\u00b7len", "wie\u00b7ders\u00b7te\u00b7hen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "APPR", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Verwicklungen, die sich unordentlich verbinden,", "tokens": ["Ver\u00b7wick\u00b7lun\u00b7gen", ",", "die", "sich", "un\u00b7or\u00b7dent\u00b7lich", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PRF", "ADJD", "VVINF", "$,"], "meter": "-++-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Wodurch die Strahlen denn sich aufgehalten finden.", "tokens": ["Wo\u00b7durch", "die", "Strah\u00b7len", "denn", "sich", "auf\u00b7ge\u00b7hal\u00b7ten", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "KON", "PRF", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}