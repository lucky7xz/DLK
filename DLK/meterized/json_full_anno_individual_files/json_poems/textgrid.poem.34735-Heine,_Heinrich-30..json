{"textgrid.poem.34735": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "30.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In meiner Erinnrung erbl\u00fchen", "tokens": ["In", "mei\u00b7ner", "E\u00b7rinn\u00b7rung", "er\u00b7bl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die Bilder, die l\u00e4ngst verwittert \u2013", "tokens": ["Die", "Bil\u00b7der", ",", "die", "l\u00e4ngst", "ver\u00b7wit\u00b7tert", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVPP", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Was ist in deiner Stimme,", "tokens": ["Was", "ist", "in", "dei\u00b7ner", "Stim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das mich so tief ersch\u00fcttert?", "tokens": ["Das", "mich", "so", "tief", "er\u00b7sch\u00fct\u00b7tert", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sag nicht, da\u00df du mich liebst!", "tokens": ["Sag", "nicht", ",", "da\u00df", "du", "mich", "liebst", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "KOUS", "PPER", "PRF", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ich wei\u00df, das Sch\u00f6nste auf Erden,", "tokens": ["Ich", "wei\u00df", ",", "das", "Sch\u00f6ns\u00b7te", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der Fr\u00fchling und die Liebe,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "und", "die", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es mu\u00df zuschanden werden.", "tokens": ["Es", "mu\u00df", "zu\u00b7schan\u00b7den", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sag nicht, da\u00df du mich liebst!", "tokens": ["Sag", "nicht", ",", "da\u00df", "du", "mich", "liebst", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "KOUS", "PPER", "PRF", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und k\u00fcsse nur und schweige,", "tokens": ["Und", "k\u00fcs\u00b7se", "nur", "und", "schwei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "KON", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und l\u00e4chle, wenn ich dir morgen", "tokens": ["Und", "l\u00e4ch\u00b7le", ",", "wenn", "ich", "dir", "mor\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "PPER", "ADV"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die welken Rosen zeige.", "tokens": ["Die", "wel\u00b7ken", "Ro\u00b7sen", "zei\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.