{"textgrid.poem.55179": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Ph\u00f6bos und Hermes", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Delos' ernster Beherrscher und Majas Sohn, der gewandte,", "tokens": ["De\u00b7los'", "erns\u00b7ter", "Be\u00b7herr\u00b7scher", "und", "Ma\u00b7jas", "Sohn", ",", "der", "ge\u00b7wand\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "KON", "NE", "NN", "$,", "ART", "ADJA", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Rechteten heftig, es w\u00fcnscht' jeder den herrlichen Preis.", "tokens": ["Rech\u00b7te\u00b7ten", "hef\u00b7tig", ",", "es", "w\u00fcnscht'", "je\u00b7der", "den", "herr\u00b7li\u00b7chen", "Preis", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "PPER", "VVFIN", "PIS", "ART", "ADJA", "NN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Hermes verlangte die Leier, die Leier verlangt' auch Apollon,", "tokens": ["Her\u00b7mes", "ver\u00b7lang\u00b7te", "die", "Lei\u00b7er", ",", "die", "Lei\u00b7er", "ver\u00b7langt'", "auch", "A\u00b7pol\u00b7lon", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "ADV", "NE", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Doch vergeblich erf\u00fcllt Hoffnung den beiden das Herz;", "tokens": ["Doch", "ver\u00b7geb\u00b7lich", "er\u00b7f\u00fcllt", "Hoff\u00b7nung", "den", "bei\u00b7den", "das", "Herz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "NN", "ART", "PIAT", "ART", "NN", "$."], "meter": "--+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Denn rasch dr\u00e4nget sich Ares heran, gewaltsam entscheidend,", "tokens": ["Denn", "rasch", "dr\u00e4n\u00b7get", "sich", "A\u00b7res", "he\u00b7ran", ",", "ge\u00b7walt\u00b7sam", "ent\u00b7schei\u00b7dend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PRF", "NE", "PTKVZ", "$,", "ADJD", "VVPP", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Schl\u00e4gt das goldene Spiel wild mit dem Eisen entzwei.", "tokens": ["Schl\u00e4gt", "das", "gol\u00b7de\u00b7ne", "Spiel", "wild", "mit", "dem", "Ei\u00b7sen", "ent\u00b7zwei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Hermes lacht unm\u00e4\u00dfig, der schadenfrohe; doch Ph\u00f6bos", "tokens": ["Her\u00b7mes", "lacht", "un\u00b7m\u00e4\u00b7\u00dfig", ",", "der", "scha\u00b7den\u00b7fro\u00b7he", ";", "doch", "Ph\u00f6\u00b7bos"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "ADJD", "$,", "ART", "ADJA", "$.", "ADV", "NN"], "meter": "+--+---+-+-+-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "Und den Musen ergreift inniger Schmerz das Gem\u00fct.", "tokens": ["Und", "den", "Mu\u00b7sen", "er\u00b7greift", "in\u00b7ni\u00b7ger", "Schmerz", "das", "Ge\u00b7m\u00fct", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "NN", "ART", "NN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Delos' ern