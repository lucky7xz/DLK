{"dta.poem.6134": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Schwere und der Leichte,  \n von der Fluht und Ebbe  \n des Meeres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Um eine Stunde mu\u00df die Fluht versp\u00e4tet seyn.", "tokens": ["Um", "ei\u00b7ne", "Stun\u00b7de", "mu\u00df", "die", "Fluht", "ver\u00b7sp\u00e4\u00b7tet", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VMFIN", "ART", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn mit des Mondes Lauff trifft sie stets \u00fcberein.", "tokens": ["Denn", "mit", "des", "Mon\u00b7des", "Lauff", "trifft", "sie", "stets", "\u00fc\u00b7be\u00b7re\u00b7in", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "---+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Derselbe geht von Osten dreyzehn Grad", "tokens": ["Der\u00b7sel\u00b7be", "geht", "von", "Os\u00b7ten", "drey\u00b7zehn", "Grad"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "NN", "CARD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In einem Tage fort,", "tokens": ["In", "ei\u00b7nem", "Ta\u00b7ge", "fort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und wenn die Erde nun den Lauff vollf\u00fchret hat;", "tokens": ["Und", "wenn", "die", "Er\u00b7de", "nun", "den", "Lauff", "voll\u00b7f\u00fch\u00b7ret", "hat", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mu\u00df sie noch weiter an den Ort,", "tokens": ["Mu\u00df", "sie", "noch", "wei\u00b7ter", "an", "den", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wo neben ihm der Wirbel enger wird.", "tokens": ["Wo", "ne\u00b7ben", "ihm", "der", "Wir\u00b7bel", "en\u00b7ger", "wird", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PPER", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Wodurch sodann der Unterscheid entsteht,", "tokens": ["Wo\u00b7durch", "so\u00b7dann", "der", "Un\u00b7ter\u00b7scheid", "ent\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Da\u00df eine Stunde stets die Fluht znr\u00fccke geht.", "tokens": ["Da\u00df", "ei\u00b7ne", "Stun\u00b7de", "stets", "die", "Fluht", "znr\u00fc\u00b7cke", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ART", "NN", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}}}}