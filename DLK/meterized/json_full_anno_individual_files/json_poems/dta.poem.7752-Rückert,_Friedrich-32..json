{"dta.poem.7752": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "32.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nord\u00f6stlich im Gebirg liegt eine feste Stadt,", "tokens": ["Nord\u00b7\u00f6st\u00b7lich", "im", "Ge\u00b7birg", "liegt", "ei\u00b7ne", "fes\u00b7te", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Worin ein eignes Volk sich angesiedelt hat.", "tokens": ["Wo\u00b7rin", "ein", "eig\u00b7nes", "Volk", "sich", "an\u00b7ge\u00b7sie\u00b7delt", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "PRF", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die glauben, da\u00df ein Heil zuk\u00fcnftig sei den Frommen,", "tokens": ["Die", "glau\u00b7ben", ",", "da\u00df", "ein", "Heil", "zu\u00b7k\u00fcnf\u00b7tig", "sei", "den", "From\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "KOUS", "ART", "NN", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und hoffen jeden Tag, der Heiland werde kommen.", "tokens": ["Und", "hof\u00b7fen", "je\u00b7den", "Tag", ",", "der", "Hei\u00b7land", "wer\u00b7de", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$,", "ART", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Beim ersten Morgenstral besteigen sie das Ro\u00df,", "tokens": ["Beim", "ers\u00b7ten", "Mor\u00b7gen\u00b7stral", "be\u00b7stei\u00b7gen", "sie", "das", "Ro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In vollem Waffenschmuck, und reiten aus dem Schlo\u00df.", "tokens": ["In", "vol\u00b7lem", "Waf\u00b7fen\u00b7schmuck", ",", "und", "rei\u00b7ten", "aus", "dem", "Schlo\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Entgegen reiten sie dem Kommenden mit Prangen,", "tokens": ["Ent\u00b7ge\u00b7gen", "rei\u00b7ten", "sie", "dem", "Kom\u00b7men\u00b7den", "mit", "Pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "APPR", "NN", "$,"], "meter": "+--+-+-+---+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Alsob sie seines Nahns Eilboten schon empfangen,", "tokens": ["Al\u00b7sob", "sie", "sei\u00b7nes", "Nahns", "Eil\u00b7bo\u00b7ten", "schon", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Alsob auf heute sei die Ankunft angesagt.", "tokens": ["Al\u00b7sob", "auf", "heu\u00b7te", "sei", "die", "An\u00b7kunft", "an\u00b7ge\u00b7sagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Und wenn nun, ohne da\u00df er kommt, die Sonne tagt,", "tokens": ["Und", "wenn", "nun", ",", "oh\u00b7ne", "da\u00df", "er", "kommt", ",", "die", "Son\u00b7ne", "tagt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "$,", "KOUI", "KOUS", "PPER", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "So reiten sie zur\u00fcck, mit Trauer in den Mienen,", "tokens": ["So", "rei\u00b7ten", "sie", "zu\u00b7r\u00fcck", ",", "mit", "Trau\u00b7er", "in", "den", "Mie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Klag' im Mund: Er ist heut wieder nicht erschienen.", "tokens": ["Und", "Klag'", "im", "Mund", ":", "Er", "ist", "heut", "wie\u00b7der", "nicht", "er\u00b7schie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPRART", "NN", "$.", "PPER", "VAFIN", "ADV", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}