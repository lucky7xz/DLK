{"dta.poem.8859": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XxXVIII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Und nicht dem Fl\u00f6tenton, dem Ton der Zinke folgt!", "tokens": ["Und", "nicht", "dem", "Fl\u00f6\u00b7ten\u00b7ton", ",", "dem", "Ton", "der", "Zin\u00b7ke", "folgt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer, ohne Tanz und Scherz, der alternden Vernunft,", "tokens": ["Wer", ",", "oh\u00b7ne", "Tanz", "und", "Scherz", ",", "der", "al\u00b7tern\u00b7den", "Ver\u00b7nunft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "KOUI", "NN", "KON", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wohin auch schleiche sie, wohin sie hinke, folgt!", "tokens": ["Wo\u00b7hin", "auch", "schlei\u00b7che", "sie", ",", "wo\u00b7hin", "sie", "hin\u00b7ke", ",", "folgt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kurz ist der Lenz, es ging das Veilchen keusch voran,", "tokens": ["Kurz", "ist", "der", "Lenz", ",", "es", "ging", "das", "Veil\u00b7chen", "keusch", "vo\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Rose, die sich malt mit eitler Schminke, folgt:", "tokens": ["Die", "Ro\u00b7se", ",", "die", "sich", "malt", "mit", "eit\u00b7ler", "Schmin\u00b7ke", ",", "folgt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "VVFIN", "APPR", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Kurz ist das Gl\u00fcck, da stets der Freude die Gefahr,", "tokens": ["Kurz", "ist", "das", "Gl\u00fcck", ",", "da", "stets", "der", "Freu\u00b7de", "die", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "KOUS", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So wie dem rechten Fu\u00df sogleich der linke folgt;", "tokens": ["So", "wie", "dem", "rech\u00b7ten", "Fu\u00df", "sog\u00b7leich", "der", "lin\u00b7ke", "folgt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "ADJA", "NN", "ADV", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Doch naht auch selbst ein Tag, der wahre Gunst verleiht,", "tokens": ["Doch", "naht", "auch", "selbst", "ein", "Tag", ",", "der", "wah\u00b7re", "Gunst", "ver\u00b7leiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ART", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Tr\u00e4ge bleibt zur\u00fcck, und nur der Flinke folgt.", "tokens": ["Der", "Tr\u00e4\u00b7ge", "bleibt", "zu\u00b7r\u00fcck", ",", "und", "nur", "der", "Flin\u00b7ke", "folgt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "KON", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}