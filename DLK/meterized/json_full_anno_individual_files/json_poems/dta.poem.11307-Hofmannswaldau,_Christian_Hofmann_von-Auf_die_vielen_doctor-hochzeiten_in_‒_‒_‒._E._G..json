{"dta.poem.11307": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf die vielen doctor-hochzeiten in \u2012 \u2012 \u2012.  \n  E. G.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df nicht, was die m\u00e4gdgen hier bet\u00e4ubt,", "tokens": ["Ich", "wei\u00df", "nicht", ",", "was", "die", "m\u00e4gd\u00b7gen", "hier", "be\u00b7t\u00e4ubt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PRELS", "ART", "ADJA", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df sie den doctorn so an mantel wachsen.", "tokens": ["Da\u00df", "sie", "den", "doc\u00b7torn", "so", "an", "man\u00b7tel", "wach\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADV", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Des zeugs ist ja genung in Sachsen,", "tokens": ["Des", "zeugs", "ist", "ja", "ge\u00b7nung", "in", "Sach\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "APPR", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da alle st\u00e4dtgen solcher m\u00e4nner voll.", "tokens": ["Da", "al\u00b7le", "st\u00e4dt\u00b7gen", "sol\u00b7cher", "m\u00e4n\u00b7ner", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "PIAT", "ADJA", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich d\u00e4cht\u2019, ein kauff- und handels-mann", "tokens": ["Ich", "d\u00e4cht'", ",", "ein", "kauff", "und", "han\u00b7dels\u00b7mann"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ART", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der st\u00fcnde mit den goldnen spitzen", "tokens": ["Der", "st\u00fcn\u00b7de", "mit", "den", "gold\u00b7nen", "spit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Den tummen schachteln besser an:", "tokens": ["Den", "tum\u00b7men", "schach\u00b7teln", "bes\u00b7ser", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Doch nein, ihr anschlag ist nicht toll;", "tokens": ["Doch", "nein", ",", "ihr", "an\u00b7schlag", "ist", "nicht", "toll", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$,", "PPOSAT", "NN", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wer wolt\u2019 umsonst so lang in \u2012 \u2012 sitzen?", "tokens": ["Wer", "wolt'", "um\u00b7sonst", "so", "lang", "in", "\u2012", "\u2012", "sit\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ADV", "ADJD", "APPR", "$(", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Das sind allhier die besten gaben;", "tokens": ["Das", "sind", "all\u00b7hier", "die", "bes\u00b7ten", "ga\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Wer auf dem unverstande lebt,", "tokens": ["Wer", "auf", "dem", "un\u00b7ver\u00b7stan\u00b7de", "lebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Will freylich gern auch einen ", "tokens": ["Will", "frey\u00b7lich", "gern", "auch", "ei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ADV", "ADV", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Nur eines f\u00e4llt mir ein:", "tokens": ["Nur", "ei\u00b7nes", "f\u00e4llt", "mir", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Jhr ", "tokens": ["Ihr"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.15": {"text": "Denn wo man es wird l\u00e4nger treiben,", "tokens": ["Denn", "wo", "man", "es", "wird", "l\u00e4n\u00b7ger", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIS", "PPER", "VAFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "So kan kein burschen-m\u00e4gdgen \u00fcbrig bleiben.", "tokens": ["So", "kan", "kein", "bur\u00b7schen\u00b7m\u00e4gd\u00b7gen", "\u00fcb\u00b7rig", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIAT", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}