{"dta.poem.6784": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von Spiegeln und Fern-  \n Gl\u00e4sern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn unsre Augen nun, vom SCHOEPFFER zugericht,", "tokens": ["Wenn", "uns\u00b7re", "Au\u00b7gen", "nun", ",", "vom", "ScHOE\u00b7PF\u00b7FER", "zu\u00b7ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Nur Instrumente sind f\u00fcr das Gesicht;", "tokens": ["Nur", "Inst\u00b7ru\u00b7men\u00b7te", "sind", "f\u00fcr", "das", "Ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So sind die Fern-und Spiegel-Gl\u00e4ser auch", "tokens": ["So", "sind", "die", "Fern\u00b7\u00b7und", "Spie\u00b7gel\u00b7Gl\u00e4\u00b7ser", "auch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nicht minder eine Art von Augen,", "tokens": ["Nicht", "min\u00b7der", "ei\u00b7ne", "Art", "von", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ja die der Unsrigen Gebrauch", "tokens": ["Ja", "die", "der", "Uns\u00b7ri\u00b7gen", "Ge\u00b7brauch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKANT", "PRELS", "ART", "ADJA", "NN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Noch gleichsam zu verbessern taugen,", "tokens": ["Noch", "gleich\u00b7sam", "zu", "ver\u00b7bes\u00b7sern", "tau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die Menschen-Kunst nach dem Modell gemacht,", "tokens": ["Die", "Men\u00b7schen\u00b7Kunst", "nach", "dem", "Mo\u00b7dell", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Das GOTTES Wunder-Hand hervorgebracht.", "tokens": ["Das", "GoT\u00b7TES", "Wun\u00b7der\u00b7Hand", "her\u00b7vor\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}