{"dta.poem.8352": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Gedanken bei Betrachtung ei-  \n nes Wetterhahns.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198782", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Wetterhahn wird stets vom Wind\nherum gedrehet,", "tokens": ["Der", "Wet\u00b7ter\u00b7hahn", "wird", "stets", "vom", "Wind", "he\u00b7rum", "ge\u00b7dre\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPRART", "NN", "APZR", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er richtet sich darnach; so wie der-", "tokens": ["Er", "rich\u00b7tet", "sich", "dar\u00b7nach", ";", "so", "wie", "der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "PAV", "$.", "ADV", "KOKOM", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sich \u00e4ndert, hie herf\u00e4hrt, aus Ost", "tokens": ["Sich", "\u00e4n\u00b7dert", ",", "hie", "her\u00b7f\u00e4hrt", ",", "aus", "Ost"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PRF", "VVFIN", "$,", "ADV", "VVPP", "$,", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus S\u00fcden oder Nord mit seinen Fl\u00fcgeln sau\u00dft;", "tokens": ["Aus", "S\u00fc\u00b7den", "o\u00b7der", "Nord", "mit", "sei\u00b7nen", "Fl\u00fc\u00b7geln", "sau\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NE", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So schwinget er sich auch; die Fahne wird be-", "tokens": ["So", "schwin\u00b7get", "er", "sich", "auch", ";", "die", "Fah\u00b7ne", "wird", "be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "$.", "ART", "NN", "VAFIN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nachdem der Windebraus kommt in der Lufft ge-", "tokens": ["Nach\u00b7dem", "der", "Win\u00b7de\u00b7braus", "kommt", "in", "der", "Lufft", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Das ist ein rechtes Bild der Unbest\u00e4ndigkeit,", "tokens": ["Das", "ist", "ein", "rech\u00b7tes", "Bild", "der", "Un\u00b7be\u00b7st\u00e4n\u00b7dig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Von einer Wechselhaft- und Aendrungs-vollen Zeit.", "tokens": ["Von", "ei\u00b7ner", "Wech\u00b7sel\u00b7haft", "und", "A\u00b7en\u00b7drungs\u00b7vol\u00b7len", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "TRUNC", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Die Menschen gleichen auch mit ihren Sin, Ge-", "tokens": ["Die", "Men\u00b7schen", "glei\u00b7chen", "auch", "mit", "ih\u00b7ren", "Sin", ",", "Ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Den H\u00e4hnen auf dem Thurm die hin und wieder-", "tokens": ["Den", "H\u00e4h\u00b7nen", "auf", "dem", "Thurm", "die", "hin", "und", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ART", "PTKVZ", "KON", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Bl\u00e4sst sie ein Gl\u00fckkeswind auf dieser Erden an,", "tokens": ["Bl\u00e4sst", "sie", "ein", "Gl\u00fck\u00b7kes\u00b7wind", "auf", "die\u00b7ser", "Er\u00b7den", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "PDAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So sieht man wie gar leicht sie solcher \u00e4ndern kan,", "tokens": ["So", "sieht", "man", "wie", "gar", "leicht", "sie", "sol\u00b7cher", "\u00e4n\u00b7dern", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "KOKOM", "ADV", "ADJD", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und kommt ein rauher Nord des Schiksahls herge-", "tokens": ["Und", "kommt", "ein", "rau\u00b7her", "Nord", "des", "Schik\u00b7sahls", "her\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "So wird auch ihr Gem\u00fct von solchen gleich bewo-", "tokens": ["So", "wird", "auch", "ihr", "Ge\u00b7m\u00fct", "von", "sol\u00b7chen", "gleich", "be\u00b7wo"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "NN", "APPR", "PIAT", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Der Vorsaz \u00e4ndert sich, und ihr erw\u00e4hltes Ziel,", "tokens": ["Der", "Vor\u00b7saz", "\u00e4n\u00b7dert", "sich", ",", "und", "ihr", "er\u00b7w\u00e4hl\u00b7tes", "Ziel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$,", "KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Was ihnen kurz vorher, als richtig woll gefiel", "tokens": ["Was", "ih\u00b7nen", "kurz", "vor\u00b7her", ",", "als", "rich\u00b7tig", "woll", "ge\u00b7fiel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADJD", "ADV", "$,", "KOUS", "ADJD", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wird Augenbliks verr\u00fckt. So wie sich dreht die", "tokens": ["Wird", "Au\u00b7gen\u00b7bliks", "ver\u00b7r\u00fckt", ".", "So", "wie", "sich", "dreht", "die"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "VVPP", "$.", "ADV", "KOKOM", "PRF", "VVFIN", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Nach jedem Zug der Luft, an einen Wetterhahne;", "tokens": ["Nach", "je\u00b7dem", "Zug", "der", "Luft", ",", "an", "ei\u00b7nen", "Wet\u00b7ter\u00b7hah\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So \u00e4ndert sich der Mensch. Die Meinungen der", "tokens": ["So", "\u00e4n\u00b7dert", "sich", "der", "Mensch", ".", "Die", "Mei\u00b7nun\u00b7gen", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$.", "ART", "NN", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Bestimmen was ihm woll, und was ihn mis-", "tokens": ["Be\u00b7stim\u00b7men", "was", "ihm", "woll", ",", "und", "was", "ihn", "mis"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PWS", "PPER", "ADV", "$,", "KON", "PWS", "PPER", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Und das soll Klugheit seyn sich nach der Welt be-", "tokens": ["Und", "das", "soll", "Klug\u00b7heit", "seyn", "sich", "nach", "der", "Welt", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VMFIN", "NN", "PPOSAT", "PRF", "APPR", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Und nach dem Lauf der Zeit den Gegenstand an-", "tokens": ["Und", "nach", "dem", "Lauf", "der", "Zeit", "den", "Ge\u00b7gen\u00b7stand", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.23": {"text": "Allein wenn dieses wahr; so w\u00e4r der Wetterhan,", "tokens": ["Al\u00b7lein", "wenn", "die\u00b7ses", "wahr", ";", "so", "w\u00e4r", "der", "Wet\u00b7ter\u00b7han", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PDAT", "ADJD", "$.", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Viel kl\u00fcger anzusehn, als selbst der kl\u00fcgste Mann.", "tokens": ["Viel", "kl\u00fc\u00b7ger", "an\u00b7zu\u00b7sehn", ",", "als", "selbst", "der", "kl\u00fcgs\u00b7te", "Mann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVIZU", "$,", "KOUS", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}