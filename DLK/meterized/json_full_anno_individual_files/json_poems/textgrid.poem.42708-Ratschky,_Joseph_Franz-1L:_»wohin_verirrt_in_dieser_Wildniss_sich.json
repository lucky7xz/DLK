{"textgrid.poem.42708": {"metadata": {"author": {"name": "Ratschky, Joseph Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbwohin verirrt in dieser Wildniss sich", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwohin verirrt in dieser Wildniss sich", "tokens": ["\u00bb", "wo\u00b7hin", "ver\u00b7irrt", "in", "die\u00b7ser", "Wild\u00b7niss", "sich"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PWAV", "VVPP", "APPR", "PDAT", "NN", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mein wunder Fuss durch dornige Gestr\u00e4uche?", "tokens": ["Mein", "wun\u00b7der", "Fuss", "durch", "dor\u00b7ni\u00b7ge", "Ge\u00b7str\u00e4u\u00b7che", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein schauerndes Gef\u00fchl durchstr\u00f6met mich;", "tokens": ["Ein", "schau\u00b7ern\u00b7des", "Ge\u00b7f\u00fchl", "durch\u00b7str\u00f6\u00b7met", "mich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Denn schwermuthsvoll steigt rings um mich der Eiche", "tokens": ["Denn", "schwer\u00b7muths\u00b7voll", "steigt", "rings", "um", "mich", "der", "Ei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ADV", "APPR", "PRF", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Bemooster Stamm aus dem Geb\u00fcsch hervor,", "tokens": ["Be\u00b7moos\u00b7ter", "Stamm", "aus", "dem", "Ge\u00b7b\u00fcsch", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und w\u00f6lbt ein Dach von dunklem Laub empor.", "tokens": ["Und", "w\u00f6lbt", "ein", "Dach", "von", "dunk\u00b7lem", "Laub", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sey mir gegr\u00fcsst, o stiller Zufluchtsort", "tokens": ["Sey", "mir", "ge\u00b7gr\u00fcs\u00b7st", ",", "o", "stil\u00b7ler", "Zu\u00b7fluch\u00b7tsort"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "FM", "ADJA", "NN"], "meter": "+---+-+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Des d\u00fcstern Grams, wo nichts sich regt, als Bienen,", "tokens": ["Des", "d\u00fcs\u00b7tern", "Grams", ",", "wo", "nichts", "sich", "regt", ",", "als", "Bie\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "PIS", "PRF", "VVFIN", "$,", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die, aufgebl\u00e4ht von Honig, sumsend dort", "tokens": ["Die", ",", "auf\u00b7ge\u00b7bl\u00e4ht", "von", "Ho\u00b7nig", ",", "sum\u00b7send", "dort"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "$,", "VVPP", "APPR", "NN", "$,", "ADJD", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Buche nahn, und ein im Lenz mit ihnen", "tokens": ["Der", "Bu\u00b7che", "nahn", ",", "und", "ein", "im", "Lenz", "mit", "ih\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "$,", "KON", "ART", "APPRART", "NN", "APPR", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Erzeugter West, den dieser Hain erzog,", "tokens": ["Er\u00b7zeug\u00b7ter", "West", ",", "den", "die\u00b7ser", "Hain", "er\u00b7zog", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PRELS", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und der noch nie um sch\u00f6ne Busen flog!", "tokens": ["Und", "der", "noch", "nie", "um", "sch\u00f6\u00b7ne", "Bu\u00b7sen", "flog", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Hier, wo das Laub kein Sonnenstrahl durchdringt,", "tokens": ["Hier", ",", "wo", "das", "Laub", "kein", "Son\u00b7nen\u00b7strahl", "durch\u00b7dringt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo um und um mit dichtverwebten Netzen", "tokens": ["Wo", "um", "und", "um", "mit", "dicht\u00b7ver\u00b7web\u00b7ten", "Net\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "KON", "APPR", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Epheu fest den hohlen Stamm umschlingt,", "tokens": ["Der", "E\u00b7pheu", "fest", "den", "hoh\u00b7len", "Stamm", "um\u00b7schlingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hier will ich mich auf welke Bl\u00e4tter setzen:", "tokens": ["Hier", "will", "ich", "mich", "auf", "wel\u00b7ke", "Bl\u00e4t\u00b7ter", "set\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "APPR", "PWAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch nein, dort w\u00e4lzt durch wildverschlungnes Gr\u00fcn", "tokens": ["Doch", "nein", ",", "dort", "w\u00e4lzt", "durch", "wild\u00b7ver\u00b7schlung\u00b7nes", "Gr\u00fcn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKANT", "$,", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und Wurzeln sich ein rascher Bergquell hin.", "tokens": ["Und", "Wur\u00b7zeln", "sich", "ein", "ra\u00b7scher", "Berg\u00b7quell", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PRF", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--++-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Er wird vielleicht zu \u00f6dern W\u00fcsteneyn", "tokens": ["Er", "wird", "viel\u00b7leicht", "zu", "\u00f6\u00b7dern", "W\u00fcs\u00b7te\u00b7neyn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mein F\u00fchrer seyn: drum folg' ich seinen Wellen ...", "tokens": ["Mein", "F\u00fch\u00b7rer", "seyn", ":", "drum", "fol\u00b7g'", "ich", "sei\u00b7nen", "Wel\u00b7len", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAINF", "$.", "PAV", "VVFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ha! welch ein Glanz bebt pl\u00f6tzlich durch den Hain!", "tokens": ["Ha", "!", "welch", "ein", "Glanz", "bebt", "pl\u00f6tz\u00b7lich", "durch", "den", "Hain", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAT", "ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Sieh! hier beginnt das Laub sich aufzuhellen,", "tokens": ["Sieh", "!", "hier", "be\u00b7ginnt", "das", "Laub", "sich", "auf\u00b7zu\u00b7hel\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVFIN", "ART", "NN", "PRF", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und staunend blickt von dieses Felsens Saum", "tokens": ["Und", "stau\u00b7nend", "blickt", "von", "die\u00b7ses", "Fel\u00b7sens", "Saum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "PDAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mein Auge tief in eines Thales Raum.", "tokens": ["Mein", "Au\u00b7ge", "tief", "in", "ei\u00b7nes", "Tha\u00b7les", "Raum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Hier, wo der Bach hoch von der steilen Wand", "tokens": ["Hier", ",", "wo", "der", "Bach", "hoch", "von", "der", "stei\u00b7len", "Wand"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Mit dumpfem Laut, wie ferne Donner t\u00f6nen,", "tokens": ["Mit", "dum\u00b7pfem", "Laut", ",", "wie", "fer\u00b7ne", "Don\u00b7ner", "t\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PWAV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sich st\u00e4ubend st\u00fcrzt, hier an des Abgrunds Rand", "tokens": ["Sich", "st\u00e4u\u00b7bend", "st\u00fcrzt", ",", "hier", "an", "des", "Ab\u00b7grunds", "Rand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PRF", "ADJD", "VVFIN", "$,", "ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Will ich mich hin an diese Klippe lehnen,", "tokens": ["Will", "ich", "mich", "hin", "an", "die\u00b7se", "Klip\u00b7pe", "leh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-----+-+-", "measure": "dactylic.init"}, "line.5": {"text": "Die (wie das Haar auf Timons Stirne wild", "tokens": ["Die", "(", "wie", "das", "Haar", "auf", "Ti\u00b7mons", "Stir\u00b7ne", "wild"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "$(", "KOKOM", "ART", "NN", "APPR", "NE", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Herniederh\u00e4ngt) ein d\u00fcrrer Strauch umh\u00fcllt.", "tokens": ["Her\u00b7nie\u00b7der\u00b7h\u00e4ngt", ")", "ein", "d\u00fcr\u00b7rer", "Strauch", "um\u00b7h\u00fcllt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Sey meinem Gram willkommen, \u00f6der Wald!", "tokens": ["Sey", "mei\u00b7nem", "Gram", "will\u00b7kom\u00b7men", ",", "\u00f6\u00b7der", "Wald", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dich w\u00e4hl' ich mir zum Zeugen meiner Klagen:", "tokens": ["Dich", "w\u00e4hl'", "ich", "mir", "zum", "Zeu\u00b7gen", "mei\u00b7ner", "Kla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Fern von der Welt im dunkeln Aufenthalt", "tokens": ["Fern", "von", "der", "Welt", "im", "dun\u00b7keln", "Auf\u00b7ent\u00b7halt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.4": {"text": "Des scheuen Wilds will ich der Lieb' entsagen.", "tokens": ["Des", "scheu\u00b7en", "Wilds", "will", "ich", "der", "Lieb'", "ent\u00b7sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Leb' ewig wohl, o Amor! mein Elpin", "tokens": ["Leb'", "e\u00b7wig", "wohl", ",", "o", "A\u00b7mor", "!", "mein", "El\u00b7pin"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "ADJD", "ADV", "$,", "FM", "FM", "$.", "PPOSAT", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Liebt mich nicht mehr: ach! Doris fesselt ihn.\u00ab", "tokens": ["Liebt", "mich", "nicht", "mehr", ":", "ach", "!", "Do\u00b7ris", "fes\u00b7selt", "ihn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "$.", "XY", "$.", "NE", "VVFIN", "PPER", "$.", "$("], "meter": "+--+---+-+", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "So sang, versenkt in tiefe Traurigkeit,", "tokens": ["So", "sang", ",", "ver\u00b7senkt", "in", "tie\u00b7fe", "Trau\u00b7rig\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Indess ein Schwarm gelinder Abendwinde", "tokens": ["In\u00b7dess", "ein", "Schwarm", "ge\u00b7lin\u00b7der", "Ab\u00b7end\u00b7win\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Allm\u00e4hlich schon ihr nymphenhaftes Kleid", "tokens": ["All\u00b7m\u00e4h\u00b7lich", "schon", "ihr", "nym\u00b7phen\u00b7haf\u00b7tes", "Kleid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Umflatterte, die reitzende Melinde,", "tokens": ["Um\u00b7flat\u00b7ter\u00b7te", ",", "die", "reit\u00b7zen\u00b7de", "Me\u00b7lin\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Als ihr Elpin, der heimlich sie belauscht,", "tokens": ["Als", "ihr", "El\u00b7pin", ",", "der", "heim\u00b7lich", "sie", "be\u00b7lauscht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "$,", "PRELS", "ADJD", "PPER", "VVPP", "$,"], "meter": "-++--+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Dicht hinter ihr aus dem Geb\u00fcsche rauscht.", "tokens": ["Dicht", "hin\u00b7ter", "ihr", "aus", "dem", "Ge\u00b7b\u00fc\u00b7sche", "rauscht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Ein liebend Paar vers\u00f6hnt sich leicht. Zwar dreht", "tokens": ["Ein", "lie\u00b7bend", "Paar", "ver\u00b7s\u00f6hnt", "sich", "leicht", ".", "Zwar", "dreht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJD", "NN", "VVFIN", "PRF", "ADJD", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Melinde sich, als sie Elpinen siehet,", "tokens": ["Me\u00b7lin\u00b7de", "sich", ",", "als", "sie", "El\u00b7pi\u00b7nen", "sie\u00b7het", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PRF", "$,", "KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hinweg, und flieht, doch wie im Blumenbeet", "tokens": ["Hin\u00b7weg", ",", "und", "flieht", ",", "doch", "wie", "im", "Blu\u00b7men\u00b7beet"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KON", "VVFIN", "$,", "ADV", "KOKOM", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vor Zephyrs Kuss die junge Rose fliehet,", "tokens": ["Vor", "Ze\u00b7phyrs", "Kuss", "die", "jun\u00b7ge", "Ro\u00b7se", "flie\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die, wenn sie kaum von ihm sich weggeneigt,", "tokens": ["Die", ",", "wenn", "sie", "kaum", "von", "ihm", "sich", "weg\u00b7ge\u00b7neigt", ","], "token