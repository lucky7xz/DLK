ter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der meine Seele lenkte!", "tokens": ["Der", "mei\u00b7ne", "See\u00b7le", "lenk\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Und ewig m\u00fcss' in Dankgef\u00fchl", "tokens": ["Und", "e\u00b7wig", "m\u00fcss'", "in", "Dank\u00b7ge\u00b7f\u00fchl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VMFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich unser Herz ergie\u00dfen!", "tokens": ["Sich", "un\u00b7ser", "Herz", "er\u00b7gie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und sanft, wie dieses Saitenspiel,", "tokens": ["Und", "sanft", ",", "wie", "die\u00b7ses", "Sai\u00b7ten\u00b7spiel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Leben uns verflie\u00dfen!", "tokens": ["Das", "Le\u00b7ben", "uns", "ver\u00b7flie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}