{"dta.poem.3865": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "An einen abgebrannten Freund.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Langer!", "tokens": ["Mein", "Lan\u00b7ger", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "k\u00f6nt ich doch mit Blut und Thr\u00e4nen", "tokens": ["k\u00f6nt", "ich", "doch", "mit", "Blut", "und", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "schreiben,", "tokens": ["schrei\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "O! d\u00fcrft ich dir nur itzt die Antwort schuldig bleiben!", "tokens": ["O", "!", "d\u00fcrft", "ich", "dir", "nur", "itzt", "die", "Ant\u00b7wort", "schul\u00b7dig", "blei\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VMFIN", "PPER", "PPER", "ADV", "ADV", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "O Freund! mein bester Freund! ach bist du abge-", "tokens": ["O", "Freund", "!", "mein", "bes\u00b7ter", "Freund", "!", "ach", "bist", "du", "ab\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "PPOSAT", "ADJA", "NN", "$.", "XY", "VAFIN", "PPER", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "brannt!", "tokens": ["brannt", "!"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Du Freund Nathanael, in meinem Vaterland!", "tokens": ["Du", "Freund", "Na\u00b7tha\u00b7nael", ",", "in", "mei\u00b7nem", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "NE", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Mu\u00df nun das Deinige durch eine Gluth verderben,", "tokens": ["Mu\u00df", "nun", "das", "Dei\u00b7ni\u00b7ge", "durch", "ei\u00b7ne", "Gluth", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "PPOSS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Es kostet dich viel Schwei\u00df dasselbe zu erwerben,", "tokens": ["Es", "kos\u00b7tet", "dich", "viel", "Schwei\u00df", "das\u00b7sel\u00b7be", "zu", "er\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "PDAT", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und du verliehrest es durch Feuer, Rauch u. Dampff,", "tokens": ["Und", "du", "ver\u00b7lieh\u00b7rest", "es", "durch", "Feu\u00b7er", ",", "Rauch", "u.", "Dampff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "abbreviation", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "NN", "$,", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.11": {"text": "Das heist in Ungl\u00fcck seyn, das ist ein schwerer", "tokens": ["Das", "heist", "in", "Un\u00b7gl\u00fcck", "seyn", ",", "das", "ist", "ein", "schwe\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "NN", "VAINF", "$,", "PDS", "VAFIN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Kampff!", "tokens": ["Kampff", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.13": {"text": "Dich hat der HErr betr\u00fcbt, dich, deinen Schatz", "tokens": ["Dich", "hat", "der", "Herr", "be\u00b7tr\u00fcbt", ",", "dich", ",", "dei\u00b7nen", "Schatz"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,", "PPER", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "und Kinder;", "tokens": ["und", "Kin\u00b7der", ";"], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "Vielleicht alleine ihr wart grob\u2019 und freche S\u00fcnder?", "tokens": ["Viel\u00b7leicht", "al\u00b7lei\u00b7ne", "ihr", "wart", "grob'", "und", "fre\u00b7che", "S\u00fcn\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "ADJD", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "O! nein, gerechter GOtt! was hat di\u00df Schaaf", "tokens": ["O", "!", "nein", ",", "ge\u00b7rech\u00b7ter", "Gott", "!", "was", "hat", "di\u00df", "Schaaf"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "PTKANT", "$,", "ADJD", "NN", "$.", "PWS", "VAFIN", "PDS", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "gethan?", "tokens": ["ge\u00b7than", "?"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.18": {"text": "Ein jeder legte mit das Holtz zum Feuer an.", "tokens": ["Ein", "je\u00b7der", "leg\u00b7te", "mit", "das", "Holtz", "zum", "Feu\u00b7er", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "APPR", "ART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wer kennt nicht euer Thun, den fr\u00f6mmsten Lebens-", "tokens": ["Wer", "kennt", "nicht", "eu\u00b7er", "Thun", ",", "den", "fr\u00f6mms\u00b7ten", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$,", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Wandel,", "tokens": ["Wan\u00b7del", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.21": {"text": "Den gantz besondren Flei\u00df so im Gewerb als Handel?", "tokens": ["Den", "gantz", "be\u00b7sond\u00b7ren", "Flei\u00df", "so", "im", "Ge\u00b7werb", "als", "Han\u00b7del", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "ADV", "APPRART", "NN", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Jhr lebt in stillster Ruh, ihr wisset keinen Feind,", "tokens": ["Ihr", "lebt", "in", "stills\u00b7ter", "Ruh", ",", "ihr", "wis\u00b7set", "kei\u00b7nen", "Feind", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Denn euer ", "tokens": ["Denn", "eu\u00b7er"], "token_info": ["word", "word"], "pos": ["KON", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.24": {"text": "zum Freund.", "tokens": ["zum", "Freund", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.25": {"text": "Die Vorsicht, so euch st\u00fcrtzt, wird euch auch wie-", "tokens": ["Die", "Vor\u00b7sicht", ",", "so", "euch", "st\u00fcrtzt", ",", "wird", "euch", "auch", "wie"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "ADV", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "der heben,", "tokens": ["der", "he\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.27": {"text": "Die wird euch den Verlust bald doppelt wieder", "tokens": ["Die", "wird", "euch", "den", "Ver\u00b7lust", "bald", "dop\u00b7pelt", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "ART", "NN", "ADV", "ADJD", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "geben.", "tokens": ["ge\u00b7ben", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.29": {"text": "Seht Hiobs Beyspiel an; nach vielem Ungemach", "tokens": ["Seht", "Hiobs", "Bey\u00b7spiel", "an", ";", "nach", "vie\u00b7lem", "Un\u00b7ge\u00b7mach"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "NE", "NN", "PTKVZ", "$.", "APPR", "PIS", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.30": {"text": "Ersetzt euch GOttes Huld das alles tausendfach.", "tokens": ["Er\u00b7setzt", "euch", "Got\u00b7tes", "Huld", "das", "al\u00b7les", "tau\u00b7send\u00b7fach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "NN", "ART", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Es bricht sein Vater-Hertz. Sein gn\u00e4diges Er-", "tokens": ["Es", "bricht", "sein", "Va\u00b7ter\u00b7Hertz", ".", "Sein", "gn\u00e4\u00b7di\u00b7ges", "Er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$.", "PPOSAT", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.32": {"text": "barmen", "tokens": ["bar\u00b7men"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.33": {"text": "Erh\u00f6rt auch euch gewi\u00df, ihr abgebrannten Armen.", "tokens": ["Er\u00b7h\u00f6rt", "auch", "euch", "ge\u00b7wi\u00df", ",", "ihr", "ab\u00b7ge\u00b7brann\u00b7ten", "Ar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Nach ausgestandner Noth bl\u00fcht euer Wohlergehn,", "tokens": ["Nach", "aus\u00b7ge\u00b7stand\u00b7ner", "Noth", "bl\u00fcht", "eu\u00b7er", "Woh\u00b7ler\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Ich selbst will di\u00df Gebet zum Thron des H\u00f6chsten", "tokens": ["Ich", "selbst", "will", "di\u00df", "Ge\u00b7bet", "zum", "Thron", "des", "H\u00f6chs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VMFIN", "PDS", "NN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "flehn.", "tokens": ["flehn", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+", "measure": "single.up"}, "line.37": {"text": "Lebt wohl! mein Auge thr\u00e4nt, ich werde mich be-", "tokens": ["Lebt", "wohl", "!", "mein", "Au\u00b7ge", "thr\u00e4nt", ",", "ich", "wer\u00b7de", "mich", "be"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$.", "PPOSAT", "NN", "VVFIN", "$,", "PPER", "VAFIN", "PPER", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "streben", "tokens": ["stre\u00b7ben"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.39": {"text": "Stets euer Freund zu seyn, zu sterben und zu leben.", "tokens": ["Stets", "eu\u00b7er", "Freund", "zu", "seyn", ",", "zu", "ster\u00b7ben", "und", "zu", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PTKZU", "VAINF", "$,", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}