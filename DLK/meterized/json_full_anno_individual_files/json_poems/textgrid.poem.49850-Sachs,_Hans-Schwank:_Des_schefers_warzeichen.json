{"textgrid.poem.49850": {"metadata": {"author": {"name": "Sachs, Hans", "birth": "N.A.", "death": "N.A."}, "title": "Schwank: Des schefers warzeichen", "genre": "verse", "period": "N.A.", "pub_year": 1568, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vor jarn in Meinz ein kremer sa\u00df,", "tokens": ["Vor", "jarn", "in", "Meinz", "ein", "kre\u00b7mer", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NE", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "der Hans Appol genennet was.", "tokens": ["der", "Hans", "Ap\u00b7pol", "ge\u00b7nen\u00b7net", "was", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NE", "VVFIN", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "als er zur mess gen Frankfurt wolt", "tokens": ["als", "er", "zur", "mess", "gen", "Frank\u00b7furt", "wolt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "APPR", "NE", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und im sein kram einkaufen solt,", "tokens": ["und", "im", "sein", "kram", "ein\u00b7kau\u00b7fen", "solt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "PPOSAT", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "da baten sein nachbauren in,", "tokens": ["da", "ba\u00b7ten", "sein", "nach\u00b7bau\u00b7ren", "in", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "wann er k\u00f6m in die mess dorthin,", "tokens": ["wann", "er", "k\u00f6m", "in", "die", "mess", "dor\u00b7thin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NE", "APPR", "ART", "NN", "ADV", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "das er in solt kaufen dermas", "tokens": ["das", "er", "in", "solt", "kau\u00b7fen", "der\u00b7mas"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRELS", "PPER", "APPR", "VMFIN", "VVINF", "PDS"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "eim dises und dem andern das,", "tokens": ["eim", "di\u00b7ses", "und", "dem", "an\u00b7dern", "das", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "KON", "ART", "ADJA", "PDS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "dienstlich zu irer kremerei,", "tokens": ["dienst\u00b7lich", "zu", "i\u00b7rer", "kre\u00b7me\u00b7rei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.10": {"text": "von war und gattung mancherlei.", "tokens": ["von", "war", "und", "gat\u00b7tung", "man\u00b7cher\u00b7lei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VAFIN", "KON", "NN", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "er sprach: das als ich gern tun wil,", "tokens": ["er", "sprach", ":", "das", "als", "ich", "gern", "tun", "wil", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PDS", "KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "wiewol ich hab zu schaffen vil", "tokens": ["wie\u00b7wol", "ich", "hab", "zu", "schaf\u00b7fen", "vil"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VAFIN", "PTKZU", "VVINF", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "selb mit meim handel in der mess;", "tokens": ["selb", "mit", "meim", "han\u00b7del", "in", "der", "mess", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "das ich nit etlich st\u00fcck verge\u00df,", "tokens": ["das", "ich", "nit", "et\u00b7lich", "st\u00fcck", "ver\u00b7ge\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PPER", "PTKNEG", "ADJD", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "schreib mir iedr ein denkzettel an,", "tokens": ["schreib", "mir", "iedr", "ein", "denk\u00b7zet\u00b7tel", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.16": {"text": "was ich im sol einkaufen tan.", "tokens": ["was", "ich", "im", "sol", "ein\u00b7kau\u00b7fen", "tan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "VMFIN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "als man im vil denkzettel bracht,", "tokens": ["als", "man", "im", "vil", "denk\u00b7zet\u00b7tel", "bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPRART", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.18": {"text": "doch keinr keins gelts darin gedacht,", "tokens": ["doch", "keinr", "keins", "gelts", "da\u00b7rin", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "PIAT", "NN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "bi\u00df ein schefer im ganzen haufen", "tokens": ["bi\u00df", "ein", "sche\u00b7fer", "im", "gan\u00b7zen", "hau\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "bat, er solt im ein sackpfeif kaufen.", "tokens": ["bat", ",", "er", "solt", "im", "ein", "sack\u00b7pfeif", "kau\u00b7fen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VMFIN", "APPRART", "ART", "ADJD", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.21": {"text": "dem gab er einen jochimstaler,", "tokens": ["dem", "gab", "er", "ei\u00b7nen", "jo\u00b7chims\u00b7ta\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "das er wer der sackpfeif ein zaler,", "tokens": ["das", "er", "wer", "der", "sack\u00b7pfeif", "ein", "za\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PPER", "PWS", "ART", "ADJD", "ART", "ADJA", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.23": {"text": "auf das er in Frankfurter mess", "tokens": ["auf", "das", "er", "in", "Frank\u00b7fur\u00b7ter", "mess"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "APPR", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "seiner sackpfeifen nit verge\u00df.", "tokens": ["sei\u00b7ner", "sack\u00b7pfei\u00b7fen", "nit", "ver\u00b7ge\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.25": {"text": "als nun Hans Appol auf die fart", "tokens": ["als", "nun", "Hans", "Ap\u00b7pol", "auf", "die", "fart"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "NE", "NE", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "hin auf die mess bereitet wart,", "tokens": ["hin", "auf", "die", "mess", "be\u00b7rei\u00b7tet", "wart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NE", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "das er zu schif, doch nit allein,", "tokens": ["das", "er", "zu", "schif", ",", "doch", "nit", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRELS", "PPER", "PTKA", "ADJD", "$,", "ADV", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "fur hin gen Frankfurt auf dem Mein,", "tokens": ["fur", "hin", "gen", "Frank\u00b7furt", "auf", "dem", "Mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "als sie ans laut nun kamen dar,", "tokens": ["als", "sie", "ans", "laut", "nun", "ka\u00b7men", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJD", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "da lud Hans Appol aus sein war,", "tokens": ["da", "lud", "Hans", "Ap\u00b7pol", "aus", "sein", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "NE", "APPR", "VAINF", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "legt die denkzettel auf ein haufen,", "tokens": ["legt", "die", "denk\u00b7zet\u00b7tel", "auf", "ein", "hau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.32": {"text": "was er ein iedem ein solt kaufen;", "tokens": ["was", "er", "ein", "ie\u00b7dem", "ein", "solt", "kau\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "PIAT", "ART", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "da kam ins schif ein starker wint", "tokens": ["da", "kam", "ins", "schif", "ein", "star\u00b7ker", "wint"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "VVFIN", "ART", "ADJA", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "und warf die denkzettel geschwint", "tokens": ["und", "warf", "die", "denk\u00b7zet\u00b7tel", "ge\u00b7schwint"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "dahin aus dem schif in den Mein.", "tokens": ["da\u00b7hin", "aus", "dem", "schif", "in", "den", "Mein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "ART", "ADJD", "APPR", "ART", "PPOSAT", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.36": {"text": "keiner blib da im schif, allein", "tokens": ["kei\u00b7ner", "blib", "da", "im", "schif", ",", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PIS", "VVFIN", "ADV", "APPRART", "VVFIN", "$,", "ADV"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.37": {"text": "der, drauf der jochimstaler lag,", "tokens": ["der", ",", "drauf", "der", "jo\u00b7chims\u00b7ta\u00b7ler", "lag", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "der den denkzettel bschweren pflag;", "tokens": ["der", "den", "denk\u00b7zet\u00b7tel", "bschwe\u00b7ren", "pflag", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.39": {"text": "darumb die sackpfeif kaufen was,", "tokens": ["da\u00b7rumb", "die", "sack\u00b7pfeif", "kau\u00b7fen", "was", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJD", "VVINF", "PWS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "die andern zettel er verga\u00df,", "tokens": ["die", "an\u00b7dern", "zet\u00b7tel", "er", "ver\u00b7ga\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "die warn all von dem wint ertrunken", "tokens": ["die", "warn", "all", "von", "dem", "wint", "er\u00b7trun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PIAT", "APPR", "ART", "VVFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "und in dem Mein zu grunt gesunken,", "tokens": ["und", "in", "dem", "Mein", "zu", "grunt", "ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "PPOSAT", "PTKA", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "das der sturmwint hat hin zerstreut;", "tokens": ["das", "der", "sturm\u00b7wint", "hat", "hin", "zer\u00b7streut", ";"], "token_info