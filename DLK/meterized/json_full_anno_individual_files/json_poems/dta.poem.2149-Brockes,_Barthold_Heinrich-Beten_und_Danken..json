{"dta.poem.2149": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Beten und Danken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Welch eine gottheitw\u00fcrd'ge Liebe wird in der Ord-", "tokens": ["Welch", "ei\u00b7ne", "gott\u00b7heit\u00b7w\u00fcrd'\u00b7ge", "Lie\u00b7be", "wird", "in", "der", "Ord"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ART", "ADJA", "NN", "VAFIN", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-++-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "nung nicht gefunden,", "tokens": ["nung", "nicht", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Da Gott allhier mit unsrer Lust und Anmuth seinen", "tokens": ["Da", "Gott", "all\u00b7hier", "mit", "uns\u00b7rer", "Lust", "und", "An\u00b7muth", "sei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ADV", "APPR", "PPOSAT", "NN", "KON", "NN", "PPOSAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ruhm verbunden;", "tokens": ["Ruhm", "ver\u00b7bun\u00b7den", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Denn dadurch, da\u00df ihr seine Werke mit Lust und mit", "tokens": ["Denn", "da\u00b7durch", ",", "da\u00df", "ihr", "sei\u00b7ne", "Wer\u00b7ke", "mit", "Lust", "und", "mit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "$,", "KOUS", "PPER", "PPOSAT", "NN", "APPR", "NN", "KON", "APPR"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Vergn\u00fcgen seht,", "tokens": ["Ver\u00b7gn\u00fc\u00b7gen", "seht", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Wird seine Weisheit, Lieb und Macht allein geehrt, allein", "tokens": ["Wird", "sei\u00b7ne", "Weis\u00b7heit", ",", "Lieb", "und", "Macht", "al\u00b7lein", "ge\u00b7ehrt", ",", "al\u00b7lein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "NN", "KON", "NN", "ADV", "VVPP", "$,", "ADV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "erh\u00f6ht.", "tokens": ["er\u00b7h\u00f6ht", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Die\u00df ist der wahre Gottesdienst, da Gott so sehr, da\u00df", "tokens": ["Die\u00df", "ist", "der", "wah\u00b7re", "Got\u00b7tes\u00b7dienst", ",", "da", "Gott", "so", "sehr", ",", "da\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,", "KOUS", "NN", "ADV", "ADV", "$,", "KOUS"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "man ihn ehret,", "tokens": ["man", "ihn", "eh\u00b7ret", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "PPER", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Nicht (weil er unsers Ruhms nicht braucht) als unsrer", "tokens": ["Nicht", "(", "weil", "er", "un\u00b7sers", "Ruhms", "nicht", "braucht", ")", "als", "uns\u00b7rer"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "$(", "KOUS", "PPER", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$(", "KOUS", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Seelen Lust begehret.", "tokens": ["See\u00b7len", "Lust", "be\u00b7ge\u00b7hret", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.13": {"text": "Erkennet denn die gro\u00dfe Wahrheit! Hierinn bestehen ihre", "tokens": ["Er\u00b7ken\u00b7net", "denn", "die", "gro\u00b7\u00dfe", "Wahr\u00b7heit", "!", "Hie\u00b7rinn", "be\u00b7ste\u00b7hen", "ih\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$.", "ADV", "VVFIN", "PPOSAT"], "meter": "-+-+-+-+-+--+-+-", "measure": "iambic.septa.relaxed"}, "line.14": {"text": "Schranken:", "tokens": ["Schran\u00b7ken", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "Durch Beten dienen wir uns selbst, der Gottheit dient", "tokens": ["Durch", "Be\u00b7ten", "die\u00b7nen", "wir", "uns", "selbst", ",", "der", "Got\u00b7theit", "dient"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PRF", "ADV", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "man blo\u00df durch Danken.", "tokens": ["man", "blo\u00df", "durch", "Dan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "APPR", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}