{"dta.poem.23644": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Englische Geduld in der englischen Krankheit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sehnsucht hatte mich getrieben", "tokens": ["Sehn\u00b7sucht", "hat\u00b7te", "mich", "ge\u00b7trie\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach dem Lande meiner Lieben.", "tokens": ["Nach", "dem", "Lan\u00b7de", "mei\u00b7ner", "Lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber, armes Heimathland,", "tokens": ["A\u00b7ber", ",", "ar\u00b7mes", "Hei\u00b7mat\u00b7hland", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Habe dich fast nicht wiedererkannt.", "tokens": ["Ha\u00b7be", "dich", "fast", "nicht", "wie\u00b7der\u00b7er\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Lange schon liegst du danieder,", "tokens": ["Lan\u00b7ge", "schon", "liegst", "du", "da\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PAV", "$,"], "meter": "+-+++-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Und wie zuckt's dir durch die Glieder!", "tokens": ["Und", "wie", "zuckt's", "dir", "durch", "die", "Glie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gro\u00df ist Gottes Gnad' und Huld,", "tokens": ["Gro\u00df", "ist", "Got\u00b7tes", "Gnad'", "und", "Huld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber noch gr\u00f6\u00dfer deine Geduld.", "tokens": ["A\u00b7ber", "noch", "gr\u00f6\u00b7\u00dfer", "dei\u00b7ne", "Ge\u00b7duld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "PPOSAT", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Nie wirst du den Schmerz verwinden", "tokens": ["Nie", "wirst", "du", "den", "Schmerz", "ver\u00b7win\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und auch nie den Arzt wohl finden:", "tokens": ["Und", "auch", "nie", "den", "Arzt", "wohl", "fin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine Qual, wer heilt sie, wer?", "tokens": ["Dei\u00b7ne", "Qual", ",", "wer", "heilt", "sie", ",", "wer", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWS", "VVFIN", "PPER", "$,", "PWS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Englische Krankheit \u2014 heilet man schwer.", "tokens": ["Eng\u00b7li\u00b7sche", "Krank\u00b7heit", "hei\u00b7let", "man", "schwer", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "VVFIN", "PIS", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}