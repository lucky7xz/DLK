tes", "Ver\u00b7nich\u00b7tungs\u00b7kampf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "VMFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "An dunkler Stoffe Widerstreben, wie lange noch?", "tokens": ["An", "dunk\u00b7ler", "Stof\u00b7fe", "Wi\u00b7der\u00b7stre\u00b7ben", ",", "wie", "lan\u00b7ge", "noch", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,", "PWAV", "ADV", "ADV", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Wie lange willst du deiner Schranken, beschr\u00e4nkter Geist,", "tokens": ["Wie", "lan\u00b7ge", "willst", "du", "dei\u00b7ner", "Schran\u00b7ken", ",", "be\u00b7schr\u00e4nk\u00b7ter", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "PPOSAT", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Ohnm\u00e4cht'gen Drangs dich \u00fcberheben, wie lange noch?", "tokens": ["Ohn\u00b7m\u00e4cht'\u00b7gen", "Drangs", "dich", "\u00fc\u00b7ber\u00b7he\u00b7ben", ",", "wie", "lan\u00b7ge", "noch", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVINF", "$,", "PWAV", "ADV", "ADV", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Sich senkt vor dir der Vorhang tiefer, jemehr du hebst,", "tokens": ["Sich", "senkt", "vor", "dir", "der", "Vor\u00b7hang", "tie\u00b7fer", ",", "je\u00b7mehr", "du", "hebst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "PPER", "ART", "NN", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+