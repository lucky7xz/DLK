{"dta.poem.7665": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "37.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wieviel Abwechslung ist im kleinsten Raum zu haben!", "tokens": ["Wie\u00b7viel", "Ab\u00b7wechs\u00b7lung", "ist", "im", "kleins\u00b7ten", "Raum", "zu", "ha\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "APPRART", "ADJA", "NN", "PTKZU", "VAINF", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Dich kann ein t\u00e4glicher Spaziergang immer laben.", "tokens": ["Dich", "kann", "ein", "t\u00e4g\u00b7li\u00b7cher", "Spa\u00b7zier\u00b7gang", "im\u00b7mer", "la\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sei auch die Stunde gleich, und gleich des Weges Richte,", "tokens": ["Sei", "auch", "die", "Stun\u00b7de", "gleich", ",", "und", "gleich", "des", "We\u00b7ges", "Rich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADV", "$,", "KON", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch jede Jahreszeit erscheint in anderm Lichte.", "tokens": ["Doch", "je\u00b7de", "Jah\u00b7res\u00b7zeit", "er\u00b7scheint", "in", "an\u00b7derm", "Lich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und willst du ab vom Weg nur wenig Schritte gehn,", "tokens": ["Und", "willst", "du", "ab", "vom", "Weg", "nur", "we\u00b7nig", "Schrit\u00b7te", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKVZ", "APPRART", "NN", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wirst du Bekanntes neu von neuer Seite sehn.", "tokens": ["Wirst", "du", "Be\u00b7kann\u00b7tes", "neu", "von", "neu\u00b7er", "Sei\u00b7te", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "ADJD", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}