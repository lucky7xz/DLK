eter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.28": {"line.1": {"text": "Edler Mann, ich habe viel erfahren,", "tokens": ["Ed\u00b7ler", "Mann", ",", "ich", "ha\u00b7be", "viel", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VAFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Unaussprechlich viel;", "tokens": ["Un\u00b7aus\u00b7sprech\u00b7lich", "viel", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Mancher Harm in neun durch\u00e4chzten Jahren", "tokens": ["Man\u00b7cher", "Harm", "in", "neun", "durc\u00b7h\u00e4chz\u00b7ten", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPR", "CARD", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Quetschte mein Gef\u00fchl;", "tokens": ["Quetschte", "mein", "Ge\u00b7f\u00fchl", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.29": {"line.1": {"text": "Aber als du kamst, da kam die G\u00fcte,", "tokens": ["A\u00b7ber", "als", "du", "kamst", ",", "da", "kam", "die", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und die hei\u00dfe Wunde im Gem\u00fcthe", "tokens": ["Und", "die", "hei\u00b7\u00dfe", "Wun\u00b7de", "im", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "F\u00fchlt' ich kaum noch mehr.", "tokens": ["F\u00fchlt'", "ich", "kaum", "noch", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.30": {"line.1": {"text": "Ach, so nimm das Opfer meiner Lieder,", "tokens": ["Ach", ",", "so", "nimm", "das", "Op\u00b7fer", "mei\u00b7ner", "Lie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVIMP", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Das gen Himmel steigt;", "tokens": ["Das", "gen", "Him\u00b7mel", "steigt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Doch die Thr\u00e4nen st\u00fcrzen str\u00f6mend nieder,", "tokens": ["Doch", "die", "Thr\u00e4\u00b7nen", "st\u00fcr\u00b7zen", "str\u00f6\u00b7mend", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und die Muse schweigt.", "tokens": ["Und", "die", "Mu\u00b7se", "schweigt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}