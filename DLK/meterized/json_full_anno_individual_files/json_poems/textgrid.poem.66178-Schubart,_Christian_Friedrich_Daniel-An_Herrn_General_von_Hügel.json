{"textgrid.poem.66178": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "An Herrn General von H\u00fcgel", "genre": "verse", "period": "N.A.", "pub_year": 1785, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht der Feldherrnstab,", "tokens": ["Nicht", "der", "Feld\u00b7herrns\u00b7tab", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Nicht des Bergbeherrschers Schimmerw\u00fcrde,", "tokens": ["Nicht", "des", "Berg\u00b7be\u00b7herr\u00b7schers", "Schim\u00b7mer\u00b7w\u00fcr\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Die dein F\u00fcrst dir gab;", "tokens": ["Die", "dein", "F\u00fcrst", "dir", "gab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Nicht der Muth des r\u00fcstigen Soldaten,", "tokens": ["Nicht", "der", "Muth", "des", "r\u00fcs\u00b7ti\u00b7gen", "Sol\u00b7da\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Nicht die Th\u00e4tigkeit,", "tokens": ["Nicht", "die", "Th\u00e4\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Die zu k\u00fcnft'gen, lorbeerreichen Thaten", "tokens": ["Die", "zu", "k\u00fcnft'\u00b7gen", ",", "lor\u00b7beer\u00b7rei\u00b7chen", "Tha\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PTKZU", "VVINF", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Deine Krieger weiht;", "tokens": ["Dei\u00b7ne", "Krie\u00b7ger", "weiht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Sei am Tage, da das Licht dich k\u00fc\u00dfte,", "tokens": ["Sei", "am", "Ta\u00b7ge", ",", "da", "das", "Licht", "dich", "k\u00fc\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Inhalt vom Gesang;", "tokens": ["In\u00b7halt", "vom", "Ge\u00b7sang", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Wenn ich dich als Krieger loben m\u00fc\u00dfte,", "tokens": ["Wenn", "ich", "dich", "als", "Krie\u00b7ger", "lo\u00b7ben", "m\u00fc\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "KOUS", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "S\u00e4ng' ich nur aus Zwang.", "tokens": ["S\u00e4ng'", "ich", "nur", "aus", "Zwang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Aber frei, wie B\u00e4che sich vom Felsen,", "tokens": ["A\u00b7ber", "frei", ",", "wie", "B\u00e4\u00b7che", "sich", "vom", "Fel\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "NN", "PRF", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Voll, wie Str\u00f6me sich", "tokens": ["Voll", ",", "wie", "Str\u00f6\u00b7me", "sich"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "PWAV", "NN", "PRF"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Von Gebirgen in die Th\u00e4ler w\u00e4lzen,", "tokens": ["Von", "Ge\u00b7bir\u00b7gen", "in", "die", "Th\u00e4\u00b7ler", "w\u00e4l\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sing' ich, ", "tokens": ["Sing'", "ich", ","], "token_info": ["word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.5": {"line.1": {"text": "Dich, den Edlen, der in frommer Stille", "tokens": ["Dich", ",", "den", "Ed\u00b7len", ",", "der", "in", "from\u00b7mer", "Stil\u00b7le"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Oft im Glutgebet", "tokens": ["Oft", "im", "Glut\u00b7ge\u00b7bet"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPRART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Mit des Christenherzens ganzer F\u00fclle", "tokens": ["Mit", "des", "Chris\u00b7ten\u00b7her\u00b7zens", "gan\u00b7zer", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "F\u00fcr die Menschen fleht;", "tokens": ["F\u00fcr", "die", "Men\u00b7schen", "fleht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Dich, der mit dem hohen Kriegermuthe", "tokens": ["Dich", ",", "der", "mit", "dem", "ho\u00b7hen", "Krie\u00b7ger\u00b7mu\u00b7the"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "So viel Huld vereint,", "tokens": ["So", "viel", "Huld", "ver\u00b7eint", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df er selbst beim Schwunge seiner Ruthe", "tokens": ["Da\u00df", "er", "selbst", "beim", "Schwun\u00b7ge", "sei\u00b7ner", "Ru\u00b7the"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Aus Erbarmen weint;", "tokens": ["Aus", "Er\u00b7bar\u00b7men", "weint", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Dich, den Sanften, dem vom Munde selten", "tokens": ["Dich", ",", "den", "Sanf\u00b7ten", ",", "dem", "vom", "Mun\u00b7de", "sel\u00b7ten"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Donnersprach' entwischt,", "tokens": ["Don\u00b7ner\u00b7sprach'", "ent\u00b7wischt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und wenn ihn der Frevler zwingt zu schelten,", "tokens": ["Und", "wenn", "ihn", "der", "Frev\u00b7ler", "zwingt", "zu", "schel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Gnade drunter mischt.", "tokens": ["Gna\u00b7de", "drun\u00b7ter", "mischt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PAV", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Dich, den treuen, dich, den liebevollen", "tokens": ["Dich", ",", "den", "treu\u00b7en", ",", "dich", ",", "den", "lie\u00b7be\u00b7vol\u00b7len"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "$,", "ART", "ADJA", "$,", "PPER", "$,", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Z\u00e4rtlichen Gemahl,", "tokens": ["Z\u00e4rt\u00b7li\u00b7chen", "Ge\u00b7mahl", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Dem der Gattin Wonnethr\u00e4nen rollen,", "tokens": ["Dem", "der", "Gat\u00b7tin", "Won\u00b7net\u00b7hr\u00e4\u00b7nen", "rol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Thr\u00e4nen ohne Zahl;", "tokens": ["Thr\u00e4\u00b7nen", "oh\u00b7ne", "Zahl", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Dem die Kinder froh entgegen fliegen!", "tokens": ["Dem", "die", "Kin\u00b7der", "froh", "ent\u00b7ge\u00b7gen", "flie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "ADJD", "PTKVZ", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ihres Lebens Ruh',", "tokens": ["Ih\u00b7res", "Le\u00b7bens", "Ruh'", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ihre St\u00fctz' und Vorbild, ihr Vergn\u00fcgen,", "tokens": ["Ih\u00b7re", "St\u00fctz'", "und", "Vor\u00b7bild", ",", "ihr", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Bist als Vater du.", "tokens": ["Bist", "als", "Va\u00b7ter", "du", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "NN", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "Dich, den nie die grinsende Schikane", "tokens": ["Dich", ",", "den", "nie", "die", "grin\u00b7sen\u00b7de", "Schi\u00b7ka\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+--+--", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Nah' am Throne schreckt,", "tokens": ["Nah'", "am", "Thro\u00b7ne", "schreckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Weil den Freund der Tugend Gottes Fahne", "tokens": ["Weil", "den", "Freund", "der", "Tu\u00b7gend", "Got\u00b7tes", "Fah\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sch\u00fctzet und bedeckt.", "tokens": ["Sch\u00fct\u00b7zet", "und", "be\u00b7deckt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.11": {"line.1": {"text": "Dich, der voller Mitleid sich zum Armen", "tokens": ["Dich", ",", "der", "vol\u00b7ler", "Mit\u00b7leid", "sich", "zum", "Ar\u00b7men"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "ART", "ADJA", "NN", "PRF", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sanft herunter neigt,", "tokens": ["Sanft", "her\u00b7un\u00b7ter", "neigt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APZR", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und dem Gram sein Auge voll Erbarmen", "tokens": ["Und", "dem", "Gram", "sein", "Au\u00b7ge", "voll", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN", "ADJD", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Auch im Kerker zeigt.", "tokens": ["Auch", "im", "Ker\u00b7ker", "zeigt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.12": {"line.1": {"text": "Bin ich nicht verlassen und gefangen", "tokens": ["Bin", "ich", "nicht", "ver\u00b7las\u00b7sen", "und", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PTKNEG", "VVINF", "KON", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Hier im Kerkergrab?", "tokens": ["Hier", "im", "Ker\u00b7ker\u00b7grab", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Trocknest du nicht von erbleichten Wangen", "tokens": ["Trock\u00b7nest", "du", "nicht", "von", "er\u00b7bleich\u00b7ten", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Mir die Z\u00e4hren ab?", "tokens": ["Mir", "die", "Z\u00e4h\u00b7ren", "ab", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.13": {"line.1": {"text": "Edler Mann, ich habe viel erfahren,", "tokens": ["Ed\u00b7ler", "Mann", ",", "ich", "ha\u00b7be", "viel", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VAFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Unaussprechlich viel;", "tokens": ["Un\u00b7aus\u00b7sprech\u00b7lich", "viel", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Mancher Harm in neun durch\u00e4chzten Jahren", "tokens": ["Man\u00b7cher", "Harm", "in", "neun", "durc\u00b7h\u00e4chz\u00b7ten", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPR", "CARD", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Quetschte mein Gef\u00fchl;", "tokens": ["Quetschte", "mein", "Ge\u00b7f\u00fchl", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.14": {"line.1": {"text": "Aber als du kamst, da kam die G\u00fcte,", "tokens": ["A\u00b7ber", "als", "du", "kamst", ",", "da", "kam", "die", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und die hei\u00dfe Wunde im Gem\u00fcthe", "tokens": ["Und", "die", "hei\u00b7\u00dfe", "Wun\u00b7de", "im", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "F\u00fchlt' ich kaum noch mehr.", "tokens": ["F\u00fchlt'", "ich", "kaum", "noch", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.15": {"line.1": {"text": "Ach, so nimm das Opfer meiner Lieder,", "tokens": ["Ach", ",", "so", "nimm", "das", "Op\u00b7fer", "mei\u00b7ner", "Lie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVIMP", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Das gen Himmel steigt;", "tokens": ["Das", "gen", "Him\u00b7mel", "steigt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Doch die Thr\u00e4nen st\u00fcrzen str\u00f6mend nieder,", "tokens": ["Doch", "die", "Thr\u00e4\u00b7nen", "st\u00fcr\u00b7zen", "str\u00f6\u00b7mend", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und die Muse schweigt.", "tokens": ["Und", "die", "Mu\u00b7se", "schweigt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.16": {"line.1": {"text": "Nicht der Feldherrnstab,", "tokens": ["Nicht", "der", "Feld\