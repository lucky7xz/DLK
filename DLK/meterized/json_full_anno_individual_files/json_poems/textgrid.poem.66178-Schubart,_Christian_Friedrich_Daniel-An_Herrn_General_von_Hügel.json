,", "ihr", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Bist als Vater du.", "tokens": ["Bist", "als", "Va\u00b7ter", "du", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "NN", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.25": {"line.1": {"text": "Dich, den nie die grinsende Schikane", "tokens": ["Dich", ",", "den", "nie", "die", "grin\u00b7sen\u00b7de", "Schi\u00b7ka\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+--+--", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Nah' am Throne schreckt,", "tokens": ["Nah'", "am", "Thro\u00b7ne", "schreckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Weil den Freund der Tugend Gottes Fahne", "tokens": ["Weil", "den", "Freund", "der", "Tu\u00b7gend", "Got\u00b7tes", "Fah\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sch\u00fctzet und bedeckt.", "tokens": ["Sch\u00fct\u00b7zet", "und", "be\u00b7deckt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.26": {"line.1": {"text": "Dich, der voller Mitleid sich zum Armen", "tokens": ["Dich", ",", "der", "vol\u00b7ler", "Mit\u00b7leid", "sich", "zum", "Ar\u00b7men"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "ART", "ADJA", "NN", "PRF", "APPRAR