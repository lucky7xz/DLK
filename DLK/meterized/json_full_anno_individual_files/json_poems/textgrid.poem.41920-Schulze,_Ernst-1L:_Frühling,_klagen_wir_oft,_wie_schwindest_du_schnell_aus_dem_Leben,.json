{"textgrid.poem.41920": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Fr\u00fchling, klagen wir oft, wie schwindest du schnell aus dem Leben,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00fchling, klagen wir oft, wie schwindest du schnell aus dem Leben,", "tokens": ["Fr\u00fch\u00b7ling", ",", "kla\u00b7gen", "wir", "oft", ",", "wie", "schwin\u00b7dest", "du", "schnell", "aus", "dem", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "ADV", "$,", "PWAV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Und wie schwindet mit dir, was du geschaffen, dahin!", "tokens": ["Und", "wie", "schwin\u00b7det", "mit", "dir", ",", "was", "du", "ge\u00b7schaf\u00b7fen", ",", "da\u00b7hin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "APPR", "PPER", "$,", "PWS", "PPER", "VVPP", "$,", "PAV", "$."], "meter": "--+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "W\u00fcnsche verliehst du uns nur und hochaufstrebende Hoffnung;", "tokens": ["W\u00fcn\u00b7sche", "ver\u00b7liehst", "du", "uns", "nur", "und", "hoch\u00b7auf\u00b7stre\u00b7ben\u00b7de", "Hoff\u00b7nung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PRF", "ADV", "KON", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Aber das k\u00e4ltere Herz spottet der kindischen Lust,", "tokens": ["A\u00b7ber", "das", "k\u00e4l\u00b7te\u00b7re", "Herz", "spot\u00b7tet", "der", "kin\u00b7di\u00b7schen", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Und was fr\u00f6hlich und frey sich erg\u00f6tzt in beweglicher Willk\u00fchr,", "tokens": ["Und", "was", "fr\u00f6h\u00b7lich", "und", "frey", "sich", "er\u00b7g\u00f6tzt", "in", "be\u00b7weg\u00b7li\u00b7cher", "Will\u00b7k\u00fchr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADJD", "KON", "ADJD", "PRF", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+--+--+-+-+", "measure": "anapaest.tetra.plus"}, "line.6": {"text": "Neigt in den Banden der Pflicht welkend das bl\u00fchende Haupt. \u2013", "tokens": ["Neigt", "in", "den", "Ban\u00b7den", "der", "Pflicht", "wel\u00b7kend", "das", "bl\u00fc\u00b7hen\u00b7de", "Haupt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "ADJD", "ART", "ADJA", "NN", "$.", "$("], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Th\u00f6richt scheint mir der Wahn, der am fliehenden Bild der Erscheinung", "tokens": ["Th\u00f6\u00b7richt", "scheint", "mir", "der", "Wahn", ",", "der", "am", "flie\u00b7hen\u00b7den", "Bild", "der", "Er\u00b7schei\u00b7nung"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "APPRART", "ADJA", "NN", "ART", "NN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.8": {"text": "H\u00e4ngt und den waltenden Geist, der sie beseelte, vergi\u00dft.", "tokens": ["H\u00e4ngt", "und", "den", "wal\u00b7ten\u00b7den", "Geist", ",", "der", "sie", "be\u00b7seel\u00b7te", ",", "ver\u00b7gi\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "KON", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.9": {"text": "Einmal nur rinnt fl\u00fcchtig das Gold in erzitternden Wellen,", "tokens": ["Ein\u00b7mal", "nur", "rinnt", "fl\u00fcch\u00b7tig", "das", "Gold", "in", "er\u00b7zit\u00b7tern\u00b7den", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ADJD", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+---+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Lieblich, mit farbigem Glanz, spielen die Flammen umher;", "tokens": ["Lieb\u00b7lich", ",", "mit", "far\u00b7bi\u00b7gem", "Glanz", ",", "spie\u00b7len", "die", "Flam\u00b7men", "um\u00b7her", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "ADJA", "NN", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.11": {"text": "Doch dann ruhet es lauter und fest in gediegener Klarheit,", "tokens": ["Doch", "dann", "ru\u00b7het", "es", "lau\u00b7ter", "und", "fest", "in", "ge\u00b7die\u00b7ge\u00b7ner", "Klar\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.12": {"text": "Und in sichrer Gestalt bindet und lenkt es die Welt.", "tokens": ["Und", "in", "sich\u00b7rer", "Ge\u00b7stalt", "bin\u00b7det", "und", "lenkt", "es", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.13": {"text": "Kraft und Liebe bewegen den Lenz; doch schwindet die Kraft denn,", "tokens": ["Kraft", "und", "Lie\u00b7be", "be\u00b7we\u00b7gen", "den", "Lenz", ";", "doch", "schwin\u00b7det", "die", "Kraft", "denn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ART", "NN", "$.", "ADV", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.14": {"text": "Schwindet die Liebe dahin mit der verg\u00e4nglichen Lust?", "tokens": ["Schwin\u00b7det", "die", "Lie\u00b7be", "da\u00b7hin", "mit", "der", "ver\u00b7g\u00e4ng\u00b7li\u00b7chen", "Lust", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PAV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.15": {"text": "Mag auch die Bl\u00fcthe verwehn, ", "tokens": ["Mag", "auch", "die", "Bl\u00fc\u00b7the", "ver\u00b7wehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.16": {"text": "Auf dem verborgenen Heerd ewig die schaffende Gluth,", "tokens": ["Auf", "dem", "ver\u00b7bor\u00b7ge\u00b7nen", "Heerd", "e\u00b7wig", "die", "schaf\u00b7fen\u00b7de", "Gluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+---+--+--+", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Schm\u00fccken im schw\u00fcleren Sommer das Feld mit der goldenen Garbe,", "tokens": ["Schm\u00fc\u00b7cken", "im", "schw\u00fc\u00b7le\u00b7ren", "Som\u00b7mer", "das", "Feld", "mit", "der", "gol\u00b7de\u00b7nen", "Gar\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.18": {"text": "Ziehn um den herbstlichen Baum \u00fcppige Reben umher,", "tokens": ["Ziehn", "um", "den", "herbst\u00b7li\u00b7chen", "Baum", "\u00fcp\u00b7pi\u00b7ge", "Re\u00b7ben", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.19": {"text": "Und wenn z\u00f6gernd der Winter genaht, dann pflegen sie freundlich,", "tokens": ["Und", "wenn", "z\u00f6\u00b7gernd", "der", "Win\u00b7ter", "ge\u00b7naht", ",", "dann", "pfle\u00b7gen", "sie", "freund\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADJD", "ART", "NN", "VVPP", "$,", "ADV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Bis sich das eisige Band l\u00f6st, den unsterblichen Lenz.", "tokens": ["Bis", "sich", "das", "ei\u00b7si\u00b7ge", "Band", "l\u00f6st", ",", "den", "uns\u00b7terb\u00b7li\u00b7chen", "Lenz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "ADJA", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "La\u00df denn schwinden die Bilder der Lust und der fr\u00f6hlichen Jugend", "tokens": ["La\u00df", "denn", "schwin\u00b7den", "die", "Bil\u00b7der", "der", "Lust", "und", "der", "fr\u00f6h\u00b7li\u00b7chen", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "KON", "ART", "ADJA", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.22": {"text": "Gr\u00fcnende Kr\u00e4nze, die einst heiter das Haupt dir geschm\u00fcckt;", "tokens": ["Gr\u00fc\u00b7nen\u00b7de", "Kr\u00e4n\u00b7ze", ",", "die", "einst", "hei\u00b7ter", "das", "Haupt", "dir", "ge\u00b7schm\u00fcckt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "ADV", "ADJD", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.23": {"text": "Welk auch freuen die Kr\u00e4nze dich noch, und die holden Gestalten", "tokens": ["Welk", "auch", "freu\u00b7en", "die", "Kr\u00e4n\u00b7ze", "dich", "noch", ",", "und", "die", "hol\u00b7den", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VVFIN", "ART", "NN", "PPER", "ADV", "$,", "KON", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.24": {"text": "Bl\u00fchen in friedlicher Brust stiller und sinniger fort.", "tokens": ["Bl\u00fc\u00b7hen", "in", "fried\u00b7li\u00b7cher", "Brust", "stil\u00b7ler", "und", "sin\u00b7ni\u00b7ger", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "ADJA", "KON", "ADJD", "PTKVZ", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.25": {"text": "Denn nicht scheidet das Leben von uns, wir scheiden vom Leben;", "tokens": ["Denn", "nicht", "schei\u00b7det", "das", "Le\u00b7ben", "von", "uns", ",", "wir", "schei\u00b7den", "vom", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VVFIN", "ART", "NN", "APPR", "PPER", "$,", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.26": {"text": "Nicht durch den Gott, es erlischt nur durch den Priester die Gluth.", "tokens": ["Nicht", "durch", "den", "Gott", ",", "es", "er\u00b7lischt", "nur", "durch", "den", "Pries\u00b7ter", "die", "Gluth", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "$,", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "Herrliches ward dir verliehn, und herrlicher gabst du es wieder;", "tokens": ["Herr\u00b7li\u00b7ches", "ward", "dir", "ver\u00b7liehn", ",", "und", "herr\u00b7li\u00b7cher", "gabst", "du", "es", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "VVINF", "$,", "KON", "ADJA", "VVFIN", "PPER", "PPER", "ADV", "$."], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.28": {"text": "Schwand auch Manches, es bleibt, weil du es bildetest, dein.", "tokens": ["Schwand", "auch", "Man\u00b7ches", ",", "es", "bleibt", ",", "weil", "du", "es", "bil\u00b7de\u00b7test", ",", "dein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ADV", "PIS", "$,", "PPER", "VVFIN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,", "PPOSAT", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.29": {"text": "Hegst du doch stets in dem Herzen die Kraft, die das Sch\u00f6ne geboren,", "tokens": ["Hegst", "du", "doch", "stets", "in", "dem", "Her\u00b7zen", "die", "Kraft", ",", "die", "das", "Sch\u00f6\u00b7ne", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.30": {"text": "Hegst du die Liebe doch stets, welche das Sch\u00f6ne beseelt.", "tokens": ["Hegst", "du", "die", "Lie\u00b7be", "doch", "stets", ",", "wel\u00b7che", "das", "Sch\u00f6\u00b7ne", "be\u00b7seelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "ADV", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Fr\u00fchling, klagen wir oft, wie schwindest du schnell aus dem Leben,", "tokens": ["Fr\u00fch\u00b7ling", ",", "kla\u00b7gen", "wir", "oft", ",", "wie", "schwin\u00b7dest", "du", "schnell", "aus", "dem", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "ADV", "$,", "PWAV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Und wie schwindet mit dir, was du geschaffen, dahin!", "tokens": ["Und", "wie", "schwin\u00b7det", "mit", "dir", ",", "was", "du", "ge\u00b7schaf\u00b7fen", ",", "da\u00b7hin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "APPR", "PPER", "$,", "PWS", "PPER", "VVPP", "$,", "PAV", "$."], "meter": "--+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "W\u00fcnsche verliehst du uns nur und hochaufstrebende Hoffnung;", "